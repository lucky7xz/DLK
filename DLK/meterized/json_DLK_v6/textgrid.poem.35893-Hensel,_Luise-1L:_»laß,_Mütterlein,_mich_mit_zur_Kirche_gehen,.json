{"textgrid.poem.35893": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbla\u00df, M\u00fctterlein, mich mit zur Kirche gehen,", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbla\u00df, M\u00fctterlein, mich mit zur Kirche gehen,", "tokens": ["\u00bb", "la\u00df", ",", "M\u00fct\u00b7ter\u00b7lein", ",", "mich", "mit", "zur", "Kir\u00b7che", "ge\u00b7hen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "NN", "$,", "PRF", "APPR", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will auch ganz artig sein und geben Acht.\u00ab", "tokens": ["Will", "auch", "ganz", "ar\u00b7tig", "sein", "und", "ge\u00b7ben", "Acht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJD", "VAINF", "KON", "VVFIN", "CARD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbkind, bist zu klein und kannst noch nicht verstehen,", "tokens": ["\u00bb", "kind", ",", "bist", "zu", "klein", "und", "kannst", "noch", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "VAFIN", "PTKA", "ADJD", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was Vater da den ", "tokens": ["Was", "Va\u00b7ter", "da", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "NN", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u00bblieb M\u00fctterlein, so will an Gott ich denken,", "tokens": ["\u00bb", "lieb", "M\u00fct\u00b7ter\u00b7lein", ",", "so", "will", "an", "Gott", "ich", "den\u00b7ken", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "ADV", "VMFIN", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und da\u00df Er einst ein Kind geworden ist,", "tokens": ["Und", "da\u00df", "Er", "einst", "ein", "Kind", "ge\u00b7wor\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und will daf\u00fcr Ihm auch mein Herzlein schenken", "tokens": ["Und", "will", "da\u00b7f\u00fcr", "Ihm", "auch", "mein", "Herz\u00b7lein", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PAV", "PPER", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und beten an den lieben heil'gen Christ.\u00ab", "tokens": ["Und", "be\u00b7ten", "an", "den", "lie\u00b7ben", "heil'\u00b7gen", "Christ", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und M\u00fctterlein erh\u00f6rt des Kindes Bitten,", "tokens": ["Und", "M\u00fct\u00b7ter\u00b7lein", "er\u00b7h\u00f6rt", "des", "Kin\u00b7des", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00fcllt schnell es ein in's warme Winterkleid,", "tokens": ["H\u00fcllt", "schnell", "es", "ein", "in's", "war\u00b7me", "Win\u00b7ter\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und als sie in die Kirche nun geschritten,", "tokens": ["Und", "als", "sie", "in", "die", "Kir\u00b7che", "nun", "ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da wird das kleine Herz so gro\u00df, so weit.", "tokens": ["Da", "wird", "das", "klei\u00b7ne", "Herz", "so", "gro\u00df", ",", "so", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie h\u00f6ret Singen, h\u00f6ret Orgelklingen,", "tokens": ["Sie", "h\u00f6\u00b7ret", "Sin\u00b7gen", ",", "h\u00f6\u00b7ret", "Or\u00b7gel\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df ihren Lobsang gleich auch bringen dar:", "tokens": ["Mu\u00df", "ih\u00b7ren", "Lob\u00b7sang", "gleich", "auch", "brin\u00b7gen", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "PTKVZ", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "\u00bbgott! Amen!\u00ab hebt sie freudig an zu singen,", "tokens": ["\u00bb", "gott", "!", "A\u00b7men", "!", "\u00ab", "hebt", "sie", "freu\u00b7dig", "an", "zu", "sin\u00b7gen", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbgott! Amen!\u00ab t\u00f6nt ihr Stimmlein hell und klar.", "tokens": ["\u00bb", "gott", "!", "A\u00b7men", "!", "\u00ab", "t\u00f6nt", "ihr", "Stimm\u00b7lein", "hell", "und", "klar", "."], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "NN", "$.", "$(", "VVFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch als nun schon verstummte die Gemeine", "tokens": ["Doch", "als", "nun", "schon", "ver\u00b7stumm\u00b7te", "die", "Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ernst der Vater auf der Kanzel stand:", "tokens": ["Und", "ernst", "der", "Va\u00b7ter", "auf", "der", "Kan\u00b7zel", "stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbgott! Amen!\u00ab singt noch immerfort die Kleine,", "tokens": ["\u00bb", "gott", "!", "A\u00b7men", "!", "\u00ab", "singt", "noch", "im\u00b7mer\u00b7fort", "die", "Klei\u00b7ne", ","], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "NN", "$.", "$(", "VVFIN", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Thr\u00e4nen zitternd, mit gefalt'ner Hand.", "tokens": ["In", "Thr\u00e4\u00b7nen", "zit\u00b7ternd", ",", "mit", "ge\u00b7falt'\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbschweig', Kind! dein Sang wird sonst den Vater st\u00f6ren,\u00ab", "tokens": ["\u00bb", "schweig'", ",", "Kind", "!", "dein", "Sang", "wird", "sonst", "den", "Va\u00b7ter", "st\u00f6\u00b7ren", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spricht leis' die Mutter, zeigt zur Kanzel hin;", "tokens": ["Spricht", "leis'", "die", "Mut\u00b7ter", ",", "zeigt", "zur", "Kan\u00b7zel", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbdie Leute wollen nun die Predigt h\u00f6ren.\u00ab", "tokens": ["\u00bb", "die", "Leu\u00b7te", "wol\u00b7len", "nun", "die", "Pre\u00b7digt", "h\u00f6\u00b7ren", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da schweigt das Kind mit tief bewegtem Sinn. \u2013", "tokens": ["Da", "schweigt", "das", "Kind", "mit", "tief", "be\u00b7weg\u00b7tem", "Sinn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wohl manche Weisen hat's seitdem gesungen,", "tokens": ["Wohl", "man\u00b7che", "Wei\u00b7sen", "hat's", "seit\u00b7dem", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Schmerz durchbebt, von Freude hell durchgl\u00fcht,", "tokens": ["Von", "Schmerz", "durch\u00b7bebt", ",", "von", "Freu\u00b7de", "hell", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch keine hat je lieblicher geklungen,", "tokens": ["Doch", "kei\u00b7ne", "hat", "je", "lieb\u00b7li\u00b7cher", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als jenes erste Gott gegeb'ne Lied.", "tokens": ["Als", "je\u00b7nes", "ers\u00b7te", "Gott", "ge\u00b7ge\u00b7b'\u00b7ne", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Gott \u2013 Amen! sprach ihr Herz mit tiefem Beben,", "tokens": ["Gott", "\u2013", "A\u00b7men", "!", "sprach", "ihr", "Herz", "mit", "tie\u00b7fem", "Be\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$.", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als sie der Kirche Heiligthum erkannt,", "tokens": ["Als", "sie", "der", "Kir\u00b7che", "Hei\u00b7lig\u00b7thum", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott \u2013 Amen! als sie willig hingegeben,", "tokens": ["Gott", "\u2013", "A\u00b7men", "!", "als", "sie", "wil\u00b7lig", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$.", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was zwischen ihr und Seiner Kirche stand.", "tokens": ["Was", "zwi\u00b7schen", "ihr", "und", "Sei\u00b7ner", "Kir\u00b7che", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Gott! Amen! wissen wir denn mehr zu sagen", "tokens": ["Gott", "!", "A\u00b7men", "!", "wis\u00b7sen", "wir", "denn", "mehr", "zu", "sa\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit aller Weisheit, aller Wissenschaft?", "tokens": ["Mit", "al\u00b7ler", "Weis\u00b7heit", ",", "al\u00b7ler", "Wis\u00b7sen\u00b7schaft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott \u2013 Amen! soll in meinem Herzen schlagen,", "tokens": ["Gott", "\u2013", "A\u00b7men", "!", "soll", "in", "mei\u00b7nem", "Her\u00b7zen", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$.", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott \u2013 Amen! sei mein Wissen, meine Kraft.", "tokens": ["Gott", "\u2013", "A\u00b7men", "!", "sei", "mein", "Wis\u00b7sen", ",", "mei\u00b7ne", "Kraft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$.", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Gott! Amen! sei in jener ernsten Stunde", "tokens": ["Gott", "!", "A\u00b7men", "!", "sei", "in", "je\u00b7ner", "erns\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein letztes Wort, mein stilles Schwanenlied.", "tokens": ["Mein", "letz\u00b7tes", "Wort", ",", "mein", "stil\u00b7les", "Schwa\u00b7nen\u00b7lied", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott \u2013 Amen \u2013 sag' ich mit ergebnem Munde,", "tokens": ["Gott", "\u2013", "A\u00b7men", "\u2013", "sag'", "ich", "mit", "er\u00b7geb\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn bang die Seele vor dem Richter kniet.", "tokens": ["Wenn", "bang", "die", "See\u00b7le", "vor", "dem", "Rich\u00b7ter", "kniet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Gott \u2013 Amen! \u2013 leidend noch in jener Flamme,", "tokens": ["Gott", "\u2013", "A\u00b7men", "!", "\u2013", "lei\u00b7dend", "noch", "in", "je\u00b7ner", "Flam\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$.", "$(", "ADJD", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die reinigend von Schlacken mich befreit,", "tokens": ["Die", "rei\u00b7ni\u00b7gend", "von", "Schla\u00b7cken", "mich", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gott! Amen! jauchz' ich selig dann dem Lamme", "tokens": ["Gott", "!", "A\u00b7men", "!", "jauch\u00b7z'", "ich", "se\u00b7lig", "dann", "dem", "Lam\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Durch aller Himmel sel'ge Ewigkeit.", "tokens": ["Durch", "al\u00b7ler", "Him\u00b7mel", "sel'\u00b7ge", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gott! Amen!", "tokens": ["Gott", "!", "A\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}