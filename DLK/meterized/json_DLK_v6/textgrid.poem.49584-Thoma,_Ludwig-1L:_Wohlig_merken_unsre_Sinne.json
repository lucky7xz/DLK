{"textgrid.poem.49584": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlig merken unsre Sinne", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlig merken unsre Sinne", "tokens": ["Woh\u00b7lig", "mer\u00b7ken", "uns\u00b7re", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun den Fr\u00fchling allgemach,", "tokens": ["Nun", "den", "Fr\u00fch\u00b7ling", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn es trauft aus jeder Rinne,", "tokens": ["Denn", "es", "trauft", "aus", "je\u00b7der", "Rin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es tropft von jedem Dach.", "tokens": ["Und", "es", "tropft", "von", "je\u00b7dem", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Leise regt sich im Theater", "tokens": ["Lei\u00b7se", "regt", "sich", "im", "The\u00b7a\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Welt ein Liebeston;", "tokens": ["Die\u00b7ser", "Welt", "ein", "Lie\u00b7be\u00b7ston", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00e4chtens schreien viele Kater,", "tokens": ["N\u00e4ch\u00b7tens", "schrei\u00b7en", "vie\u00b7le", "Ka\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Hase rammelt schon.", "tokens": ["Und", "der", "Ha\u00b7se", "ram\u00b7melt", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So auch uns ergreift die Glieder", "tokens": ["So", "auch", "uns", "er\u00b7greift", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wundersame Lebenskraft;", "tokens": ["Wun\u00b7der\u00b7sa\u00b7me", "Le\u00b7bens\u00b7kraft", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst solide Seifensieder", "tokens": ["Selbst", "so\u00b7li\u00b7de", "Sei\u00b7fen\u00b7sie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlen ihren Knospensaft.", "tokens": ["F\u00fch\u00b7len", "ih\u00b7ren", "Knos\u00b7pen\u00b7saft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Treibet das Gesch\u00e4ft der Paarung!", "tokens": ["Trei\u00b7bet", "das", "Ge\u00b7sch\u00e4ft", "der", "Paa\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lasset der Natur den Lauf!", "tokens": ["Las\u00b7set", "der", "Na\u00b7tur", "den", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Denn ihr wisset aus Erfahrung,", "tokens": ["Denn", "ihr", "wis\u00b7set", "aus", "Er\u00b7fah\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einmal h\u00f6rt es leider auf.", "tokens": ["Ein\u00b7mal", "h\u00f6rt", "es", "lei\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}