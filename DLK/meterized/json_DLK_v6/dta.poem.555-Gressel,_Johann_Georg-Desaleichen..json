{"dta.poem.555": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Desaleichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es tr\u00e4ufft des Himmels-Gunst nun lauter Balsam nieder/", "tokens": ["Es", "tr\u00e4ufft", "des", "Him\u00b7mels\u00b7Gunst", "nun", "lau\u00b7ter", "Bal\u00b7sam", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der str\u00f6hmt mit vollem Gl\u00fcck auf deine Scheitel zu/", "tokens": ["Der", "str\u00f6hmt", "mit", "vol\u00b7lem", "Gl\u00fcck", "auf", "dei\u00b7ne", "Schei\u00b7tel", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach langen St\u00fcrmen findt ein sanffter Blick sich wieder/", "tokens": ["Nach", "lan\u00b7gen", "St\u00fcr\u00b7men", "findt", "ein", "sanff\u00b7ter", "Blick", "sich", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADV", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und auf die Unlust-Last vergn\u00fcgte Seelen Ruh.", "tokens": ["Und", "auf", "die", "Un\u00b7lust\u00b7Last", "ver\u00b7gn\u00fcg\u00b7te", "See\u00b7len", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Wollust will anjetzt/ mein Freund/ dich truncken machen/", "tokens": ["Die", "Wol\u00b7lust", "will", "an\u00b7jetzt", "/", "mein", "Freund", "/", "dich", "trun\u00b7cken", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "$(", "PPOSAT", "NN", "$(", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie l\u00e4sset ihre Lust dir zum Vergn\u00fcgen aus/", "tokens": ["Sie", "l\u00e4s\u00b7set", "ih\u00b7re", "Lust", "dir", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Selbst das Verh\u00e4ngni\u00df mu\u00df ob deinen Gl\u00fccke lachen/", "tokens": ["Selbst", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "mu\u00df", "ob", "dei\u00b7nen", "Gl\u00fc\u00b7cke", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es f\u00fchret dich zuletzt in das beliebte Haus.", "tokens": ["Es", "f\u00fch\u00b7ret", "dich", "zu\u00b7letzt", "in", "das", "be\u00b7lieb\u00b7te", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So kr\u00f6nt Best\u00e4ndigkeit nach ausgestandnen Proben", "tokens": ["So", "kr\u00f6nt", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "nach", "aus\u00b7ge\u00b7stand\u00b7nen", "Pro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dennoch dein wehrtes Haupt mit einem Myrthen-Krantz/", "tokens": ["Den\u00b7noch", "dein", "wehr\u00b7tes", "Haupt", "mit", "ei\u00b7nem", "Myr\u00b7then\u00b7Krantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die erste Liebe bleibt/ ob gleich die Neider toben/", "tokens": ["Die", "ers\u00b7te", "Lie\u00b7be", "bleibt", "/", "ob", "gleich", "die", "Nei\u00b7der", "to\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In der Zerreissung stehn/ und im Zertrennen gantz.", "tokens": ["In", "der", "Zer\u00b7reis\u00b7sung", "stehn", "/", "und", "im", "Zer\u00b7tren\u00b7nen", "gantz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$(", "KON", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bellandra will ihr Wort/ ihr reines Wort/ erf\u00fcllen/", "tokens": ["Bel\u00b7land\u00b7ra", "will", "ihr", "Wort", "/", "ihr", "rei\u00b7nes", "Wort", "/", "er\u00b7f\u00fcl\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie will in kurtzer Zeit dein bestes Labsahl seyn/", "tokens": ["Sie", "will", "in", "kurt\u00b7zer", "Zeit", "dein", "bes\u00b7tes", "Lab\u00b7sahl", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es soll dein heisser Geist den Liebes-Hunger stillen/", "tokens": ["Es", "soll", "dein", "heis\u00b7ser", "Geist", "den", "Lie\u00b7bes\u00b7Hun\u00b7ger", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es nimmt ihr ", "tokens": ["Es", "nimmt", "ihr"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Vergn\u00fcgung will mit Lust um deine Scheitel spielen/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gung", "will", "mit", "Lust", "um", "dei\u00b7ne", "Schei\u00b7tel", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Liebste labet dich mit ihrer Zucker-Brust/", "tokens": ["Die", "Liebs\u00b7te", "la\u00b7bet", "dich", "mit", "ih\u00b7rer", "Zu\u00b7cke\u00b7rBrust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Corallen werden dir die heissen Lippen k\u00fchlen/", "tokens": ["Co\u00b7ral\u00b7len", "wer\u00b7den", "dir", "die", "heis\u00b7sen", "Lip\u00b7pen", "k\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du wirst beladen seyn mit angenehmer Lust.", "tokens": ["Du", "wirst", "be\u00b7la\u00b7den", "seyn", "mit", "an\u00b7ge\u00b7neh\u00b7mer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "VAINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das traurige Gestirn/ die d\u00fcsteren ", "tokens": ["Das", "trau\u00b7ri\u00b7ge", "Ge\u00b7stirn", "/", "die", "d\u00fcs\u00b7te\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Die dir in dein er Lieb\u2019 Angst/ Noht und Schmertz erweckten", "tokens": ["Die", "dir", "in", "dein", "er", "Lieb'", "Angst", "/", "Noht", "und", "Schmertz", "er\u00b7weck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "PPER", "NN", "NN", "$(", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wird nun ein frohes Licht des klaren Himmels seyn.", "tokens": ["Wird", "nun", "ein", "fro\u00b7hes", "Licht", "des", "kla\u00b7ren", "Him\u00b7mels", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du wirst dein Haupt mit Lust wie ", "tokens": ["Du", "wirst", "dein", "Haupt", "mit", "Lust", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Dis frendige Gestirn vermehret deine Ruh/", "tokens": ["Dis", "fren\u00b7di\u00b7ge", "Ge\u00b7stirn", "ver\u00b7meh\u00b7ret", "dei\u00b7ne", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Kein St\u00fcrmen kan den Schein des Freuden-Lichts vernichten/", "tokens": ["Kein", "St\u00fcr\u00b7men", "kan", "den", "Schein", "des", "Freu\u00b7den\u00b7Lichts", "ver\u00b7nich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es wirfft dir unverhofft die sch\u00f6nsten Strahlen zu.", "tokens": ["Es", "wirfft", "dir", "un\u00b7ver\u00b7hofft", "die", "sch\u00f6ns\u00b7ten", "Strah\u00b7len", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nun kanst du nach der Nacht und ihren Finsternissen", "tokens": ["Nun", "kanst", "du", "nach", "der", "Nacht", "und", "ih\u00b7ren", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Den sch\u00f6nen Morgen-Stern in vollem Glantze sehn.", "tokens": ["Den", "sch\u00f6\u00b7nen", "Mor\u00b7gen\u00b7S\u00b7tern", "in", "vol\u00b7lem", "Glant\u00b7ze", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dein Engels-Kind wird dich recht zu Vergn\u00fcgen wissen/", "tokens": ["Dein", "En\u00b7gels\u00b7Kind", "wird", "dich", "recht", "zu", "Ver\u00b7gn\u00fc\u00b7gen", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dein Schmertze ist nun fort/ dein Klagen ist geschehn.", "tokens": ["Dein", "Schmert\u00b7ze", "ist", "nun", "fort", "/", "dein", "Kla\u00b7gen", "ist", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKVZ", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}