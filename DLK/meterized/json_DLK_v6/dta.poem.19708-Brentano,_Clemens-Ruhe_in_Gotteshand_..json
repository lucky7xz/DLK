{"dta.poem.19708": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Ruhe in Gotteshand .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie des Noah T\u00e4ubelein               ", "tokens": ["Gleich", "wie", "des", "Noah", "T\u00e4u\u00b7be\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NE", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihr F\u00fc\u00dflein nicht wollt sudeln ein,", "tokens": ["Ihr", "F\u00fc\u00df\u00b7lein", "nicht", "wollt", "su\u00b7deln", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Letten dieser Welt;", "tokens": ["Im", "Let\u00b7ten", "die\u00b7ser", "Welt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie floh dem Patriarchen zu,", "tokens": ["Sie", "floh", "dem", "Pat\u00b7ri\u00b7ar\u00b7chen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In seiner Hand da fand sie Ruh,", "tokens": ["In", "sei\u00b7ner", "Hand", "da", "fand", "sie", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst nirgends in dem Feld,", "tokens": ["Sonst", "nir\u00b7gends", "in", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Also in diesem S\u00fcnden-Land,", "tokens": ["Al\u00b7so", "in", "die\u00b7sem", "S\u00fcn\u00b7den\u00b7Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Maria stund in Gottes Hand,", "tokens": ["Ma\u00b7ria", "stund", "in", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Der Feind erjagt sie nicht,", "tokens": ["Der", "Feind", "er\u00b7jagt", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ihr Leib und Seel wohl angef\u00fchrt,", "tokens": ["Ihr", "Leib", "und", "Seel", "wohl", "an\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Zum B\u00f6sen keine Neigung sp\u00fcrt,", "tokens": ["Zum", "B\u00f6\u00b7sen", "kei\u00b7ne", "Nei\u00b7gung", "sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sieh an ihr Angesicht.", "tokens": ["Sieh", "an", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}