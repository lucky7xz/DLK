{"dta.poem.7079": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wunder, so auf dieser Erden,", "tokens": ["Die", "Wun\u00b7der", ",", "so", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch GOTTES Allmachts-Wort geschehn,", "tokens": ["Durch", "GoT\u00b7TES", "All\u00b7machts\u00b7Wort", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird er erstaunend \u00fcbersehn.", "tokens": ["Wird", "er", "er\u00b7stau\u00b7nend", "\u00fc\u00b7ber\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wird sodann gar leichtlich fassen", "tokens": ["Er", "wird", "so\u00b7dann", "gar", "leicht\u00b7lich", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wunder, anf der Berge H\u00f6h\u2019,", "tokens": ["Die", "Wun\u00b7der", ",", "anf", "der", "Ber\u00b7ge", "H\u00f6h'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wunder in der tieffen See,", "tokens": ["Die", "Wun\u00b7der", "in", "der", "tief\u00b7fen", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf welche Art von der Materie", "tokens": ["Auf", "wel\u00b7che", "Art", "von", "der", "Ma\u00b7te\u00b7rie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Die Theilchen an einander hangen,", "tokens": ["Die", "Theil\u00b7chen", "an", "ein\u00b7an\u00b7der", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Davon wird er die Nachricht bald erlangen.", "tokens": ["Da\u00b7von", "wird", "er", "die", "Nach\u00b7richt", "bald", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Es wird sich das sodann von ihm begreiffen lassen,", "tokens": ["Es", "wird", "sich", "das", "so\u00b7dann", "von", "ihm", "be\u00b7greif\u00b7fen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PDS", "ADV", "APPR", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was alle Weisen hier verwirrt,", "tokens": ["Was", "al\u00b7le", "Wei\u00b7sen", "hier", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Worinn der kl\u00fcgste Geist geirrt.", "tokens": ["Wo\u00b7rinn", "der", "kl\u00fcgs\u00b7te", "Geist", "ge\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es wird ihm die Natur, die Ursach und der Grund,", "tokens": ["Es", "wird", "ihm", "die", "Na\u00b7tur", ",", "die", "Ur\u00b7sach", "und", "der", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Schwehr\u2019 und der Bewegung kund.", "tokens": ["Der", "Schwehr'", "und", "der", "Be\u00b7we\u00b7gung", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Von den Erscheinungen der w\u00fcrckenden Natur", "tokens": ["Von", "den", "Er\u00b7schei\u00b7nun\u00b7gen", "der", "w\u00fcr\u00b7cken\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Kommt er auch gleichfalls auf die Spur:", "tokens": ["Kommt", "er", "auch", "gleich\u00b7falls", "auf", "die", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Es wird ihm deutlich seyn und klar,", "tokens": ["Es", "wird", "ihm", "deut\u00b7lich", "seyn", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAINF", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Durch welche Mischungen die Mannigfaltigkeit", "tokens": ["Durch", "wel\u00b7che", "Misc\u00b7hun\u00b7gen", "die", "Man\u00b7nig\u00b7fal\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ART", "NN"], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Der Dinge, die so wunderbar", "tokens": ["Der", "Din\u00b7ge", ",", "die", "so", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und so verschiedlich sind, entstehn;", "tokens": ["Und", "so", "ver\u00b7schied\u00b7lich", "sind", ",", "ent\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df obgleich allezeit", "tokens": ["Da\u00df", "ob\u00b7gleich", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "KOUS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Die C\u00f6rper, durch best\u00e4ndigs reiben,", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "durch", "be\u00b7st\u00e4n\u00b7digs", "rei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Stets einen Abgang sehn, den sie ersetzen", "tokens": ["Stets", "ei\u00b7nen", "Ab\u00b7gang", "sehn", ",", "den", "sie", "er\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Durch andern Stoff, ohn dies ihr Wesen zu verletzen;", "tokens": ["Durch", "an\u00b7dern", "Stoff", ",", "ohn", "dies", "ihr", "We\u00b7sen", "zu", "ver\u00b7let\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUI", "PDS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch stets dasjenige, was sie gewesen, bleiben.", "tokens": ["Doch", "stets", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "sie", "ge\u00b7we\u00b7sen", ",", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PDS", "$,", "PRELS", "PPER", "VAPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Er kennt sodann der Thiere Seelen,", "tokens": ["Er", "kennt", "so\u00b7dann", "der", "Thie\u00b7re", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ob, und wie weit sie c\u00f6rperlich.", "tokens": ["Ob", ",", "und", "wie", "weit", "sie", "c\u00f6r\u00b7per\u00b7lich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KON", "PWAV", "ADJD", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Es kann f\u00fcr sein\u2019 Erkenntni\u00df sich", "tokens": ["Es", "kann", "f\u00fcr", "sein'", "Er\u00b7kennt\u00b7ni\u00df", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Das Allerkleinste nicht verheelen.", "tokens": ["Das", "Al\u00b7ler\u00b7kleins\u00b7te", "nicht", "ver\u00b7hee\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Es wird der Geist sodann erkennen,", "tokens": ["Es", "wird", "der", "Geist", "so\u00b7dann", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Auf welche wunderbare Weise", "tokens": ["Auf", "wel\u00b7che", "wun\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Am Himmel so viel tausend Creise", "tokens": ["Am", "Him\u00b7mel", "so", "viel", "tau\u00b7send", "Crei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Best\u00e4ndig gl\u00e4ntzen, strahlen brennen,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "gl\u00e4nt\u00b7zen", ",", "strah\u00b7len", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "In so verschiednem Licht und Schein,", "tokens": ["In", "so", "ver\u00b7schied\u00b7nem", "Licht", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "In solcher Schimmer-reichen Pracht.", "tokens": ["In", "sol\u00b7cher", "Schim\u00b7mer\u00b7rei\u00b7chen", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Es wird ihm unverholen seyn,", "tokens": ["Es", "wird", "ihm", "un\u00b7ver\u00b7ho\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Durch welche St\u00e4rcke, Krafft und Macht,", "tokens": ["Durch", "wel\u00b7che", "St\u00e4r\u00b7cke", ",", "Krafft", "und", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.38": {"text": "Ein solch unz\u00e4hlbar Heer, von Sonnen und von Erden,", "tokens": ["Ein", "solch", "un\u00b7z\u00e4hl\u00b7bar", "Heer", ",", "von", "Son\u00b7nen", "und", "von", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Jm Creise stets getrieben werden.", "tokens": ["Jm", "Crei\u00b7se", "stets", "ge\u00b7trie\u00b7ben", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}