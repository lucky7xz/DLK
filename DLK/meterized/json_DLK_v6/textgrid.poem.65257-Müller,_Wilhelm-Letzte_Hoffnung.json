{"textgrid.poem.65257": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Letzte Hoffnung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier und da ist an den B\u00e4umen", "tokens": ["Hier", "und", "da", "ist", "an", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Noch ein buntes Blatt zu sehn,", "tokens": ["Noch", "ein", "bun\u00b7tes", "Blatt", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich bleibe vor den B\u00e4umen", "tokens": ["Und", "ich", "blei\u00b7be", "vor", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oftmals in Gedanken stehn.", "tokens": ["Oft\u00b7mals", "in", "Ge\u00b7dan\u00b7ken", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaue nach dem einen Blatte,", "tokens": ["Schau\u00b7e", "nach", "dem", "ei\u00b7nen", "Blat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4nge meine Hoffnung dran;", "tokens": ["H\u00e4n\u00b7ge", "mei\u00b7ne", "Hoff\u00b7nung", "dran", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spielt der Wind mit meinem Blatte,", "tokens": ["Spielt", "der", "Wind", "mit", "mei\u00b7nem", "Blat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zittr' ich, was ich zittern kann.", "tokens": ["Zit\u00b7tr'", "ich", ",", "was", "ich", "zit\u00b7tern", "kann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, und f\u00e4llt das Blatt zu Boden,", "tokens": ["Ach", ",", "und", "f\u00e4llt", "das", "Blatt", "zu", "Bo\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt mit ihm die Hoffnung ab,", "tokens": ["F\u00e4llt", "mit", "ihm", "die", "Hoff\u00b7nung", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fall' ich selber mit zu Boden,", "tokens": ["Fall'", "ich", "sel\u00b7ber", "mit", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wein' auf meiner Hoffnung Grab.", "tokens": ["Wein'", "auf", "mei\u00b7ner", "Hoff\u00b7nung", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}