{"dta.poem.11735": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr n\u00e4rr'schen Dichter, die ihr scheltet die Natur,", "tokens": ["Ihr", "n\u00e4rr'\u00b7schen", "Dich\u00b7ter", ",", "die", "ihr", "schel\u00b7tet", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sie zu schelten nehmt aus ihr die Bilder nur!", "tokens": ["Und", "sie", "zu", "schel\u00b7ten", "nehmt", "aus", "ihr", "die", "Bil\u00b7der", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn Musen sonst aus L\u00e4rm die Einsamkeit gesucht,", "tokens": ["Wenn", "Mu\u00b7sen", "sonst", "aus", "L\u00e4rm", "die", "Ein\u00b7sam\u00b7keit", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nehmt ihr vom Land zur Stadt die umgekehrte Flucht;", "tokens": ["Nehmt", "ihr", "vom", "Land", "zur", "Stadt", "die", "um\u00b7ge\u00b7kehr\u00b7te", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "H\u00e4ngt um die Poesie des Staates Flitterstaat,", "tokens": ["H\u00e4ngt", "um", "die", "Poe\u00b7sie", "des", "Staa\u00b7tes", "Flit\u00b7ter\u00b7staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Statt jener Unschuld, die im Paradies auftrat.", "tokens": ["Statt", "je\u00b7ner", "Un\u00b7schuld", ",", "die", "im", "Pa\u00b7ra\u00b7dies", "auf\u00b7trat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Seht dort nur hin, wo l\u00e4ngst schon steht das Ideal,", "tokens": ["Seht", "dort", "nur", "hin", ",", "wo", "l\u00e4ngst", "schon", "steht", "das", "I\u00b7deal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$,", "PWAV", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ihr hier bauen wollt; sprecht: wo ist Lust? wo Qual?", "tokens": ["Das", "ihr", "hier", "bau\u00b7en", "wollt", ";", "sprecht", ":", "wo", "ist", "Lust", "?", "wo", "Qual", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVINF", "VMFIN", "$.", "VVFIN", "$.", "PWAV", "VAFIN", "NN", "$.", "PWAV", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Ist hier die Wiese kahl? ist hier der Bach nur schmal?", "tokens": ["Ist", "hier", "die", "Wie\u00b7se", "kahl", "?", "ist", "hier", "der", "Bach", "nur", "schmal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$.", "VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie gl\u00e4nzen doch, sei's nun von Fr\u00fch- von Abendstral.", "tokens": ["Sie", "gl\u00e4n\u00b7zen", "doch", ",", "sei's", "nun", "von", "Fr\u00fch", "von", "A\u00b7bend\u00b7stral", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VAFIN", "ADV", "APPR", "TRUNC", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenns hier ist kahl und schmal, so ists dort schal und fahl,", "tokens": ["Wenns", "hier", "ist", "kahl", "und", "schmal", ",", "so", "ists", "dort", "schal", "und", "fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$,", "ADV", "VAFIN", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort wo ihr jetzt noch seht nur h\u00f6chstes Ideal.", "tokens": ["Dort", "wo", "ihr", "jetzt", "noch", "seht", "nur", "h\u00f6chs\u00b7tes", "I\u00b7deal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Geht hin zur Stadt im Sumpf, zur Stadt im Kohlendampf,", "tokens": ["Geht", "hin", "zur", "Stadt", "im", "Sumpf", ",", "zur", "Stadt", "im", "Koh\u00b7len\u00b7dampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und k\u00e4mpft f\u00fcr Erdenheil, f\u00fcr Erdlicht euern Kampf!", "tokens": ["Und", "k\u00e4mpft", "f\u00fcr", "Er\u00b7den\u00b7heil", ",", "f\u00fcr", "Erd\u00b7licht", "eu\u00b7ern", "Kampf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Hier la\u00dft die heitre Lust f\u00fcr Weltheil, Gottlicht k\u00e4mpfen;", "tokens": ["Hier", "la\u00dft", "die", "heit\u00b7re", "Lust", "f\u00fcr", "Welt\u00b7heil", ",", "Gott\u00b7licht", "k\u00e4mp\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Heiterkeit sollt ihr mit Koth und Dampf nicht d\u00e4mpfen.", "tokens": ["Die", "Hei\u00b7ter\u00b7keit", "sollt", "ihr", "mit", "Koth", "und", "Dampf", "nicht", "d\u00e4mp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}