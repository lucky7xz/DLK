{"textgrid.poem.49479": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "[die Stunde schl\u00e4gt, dein Ende kam]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stunde schl\u00e4gt, dein Ende kam,", "tokens": ["Die", "Stun\u00b7de", "schl\u00e4gt", ",", "dein", "En\u00b7de", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schwerste Leid von allen Leiden,", "tokens": ["Das", "schwers\u00b7te", "Leid", "von", "al\u00b7len", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die tiefste Nacht, der tiefste Gram, \u2013", "tokens": ["Die", "tiefs\u00b7te", "Nacht", ",", "der", "tiefs\u00b7te", "Gram", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fast meine selber ich dahinzuscheiden.", "tokens": ["Fast", "mei\u00b7ne", "sel\u00b7ber", "ich", "da\u00b7hin\u00b7zu\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, all die Glut dahingerafft,", "tokens": ["Ach", ",", "all", "die", "Glut", "da\u00b7hin\u00b7ge\u00b7rafft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerrissen alle zarten Bande!", "tokens": ["Zer\u00b7ris\u00b7sen", "al\u00b7le", "zar\u00b7ten", "Ban\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Heiterkeit, die sch\u00f6ne Kraft,", "tokens": ["Die", "Hei\u00b7ter\u00b7keit", ",", "die", "sch\u00f6\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Wahnsinn, so viel Glanz verscharrt im Sande!", "tokens": ["O", "Wahn\u00b7sinn", ",", "so", "viel", "Glanz", "ver\u00b7scharrt", "im", "San\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Trug! Ja das Nichts ist gut f\u00fcr mich,", "tokens": ["Trug", "!", "Ja", "das", "Nichts", "ist", "gut", "f\u00fcr", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKANT", "ART", "PIS", "VAFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr dies verkehrte, schwache Wesen,", "tokens": ["F\u00fcr", "dies", "ver\u00b7kehr\u00b7te", ",", "schwa\u00b7che", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDS", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da will ich ruhn \u2013 jedoch f\u00fcr dich ...", "tokens": ["Da", "will", "ich", "ruhn", "\u2013", "je\u00b7doch", "f\u00fcr", "dich", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$(", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrwahr, du bist aus anderm Ton gewesen.", "tokens": ["F\u00fcr\u00b7wahr", ",", "du", "bist", "aus", "an\u00b7derm", "Ton", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In mir haust Finsternis, gepaart", "tokens": ["In", "mir", "haust", "Fins\u00b7ter\u00b7nis", ",", "ge\u00b7paart"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "VVFIN", "NE", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Schweigen nach des Sturmes Toben,", "tokens": ["Mit", "Schwei\u00b7gen", "nach", "des", "Stur\u00b7mes", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch dich entr\u00fcckt die Himmelsfahrt", "tokens": ["Doch", "dich", "ent\u00b7r\u00fcckt", "die", "Him\u00b7mels\u00b7fahrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Frauen, die der Liebe Ruhm erhoben.", "tokens": ["Der", "Frau\u00b7en", ",", "die", "der", "Lie\u00b7be", "Ruhm", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Denn in der Nacht, in die man geht,", "tokens": ["Denn", "in", "der", "Nacht", ",", "in", "die", "man", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird dich dein Reiz im Kranz der Sch\u00f6nen,", "tokens": ["Wird", "dich", "dein", "Reiz", "im", "Kranz", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu denen Liebe je gefleht,", "tokens": ["Zu", "de\u00b7nen", "Lie\u00b7be", "je", "ge\u00b7fleht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor allen Liebenden und Jungfraun kr\u00f6nen.", "tokens": ["Vor", "al\u00b7len", "Lie\u00b7ben\u00b7den", "und", "Jung\u00b7fraun", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und letzte Gabe deinem Treun", "tokens": ["Und", "letz\u00b7te", "Ga\u00b7be", "dei\u00b7nem", "Treun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begl\u00e4nzt den Ort, wo ich begraben,", "tokens": ["Be\u00b7gl\u00e4nzt", "den", "Ort", ",", "wo", "ich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie stolzer, Feldherrn Leichenstein,", "tokens": ["Wie", "stol\u00b7zer", ",", "Feld\u00b7herrn", "Lei\u00b7chen\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ruhm vor allen dich geliebt zu haben.", "tokens": ["Der", "Ruhm", "vor", "al\u00b7len", "dich", "ge\u00b7liebt", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "PPER", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}