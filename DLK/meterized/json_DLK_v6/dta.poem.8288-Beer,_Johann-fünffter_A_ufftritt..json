{"dta.poem.8288": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "f\u00fcnffter  A ufftritt.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Das Lieben ist wie eine Fischerey/", "tokens": ["Das", "Lie\u00b7ben", "ist", "wie", "ei\u00b7ne", "Fi\u00b7sche\u00b7rey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier angelt eins/ dort streicht es mit den Netzen/", "tokens": ["Hier", "an\u00b7gelt", "eins", "/", "dort", "streicht", "es", "mit", "den", "Net\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch der verhoffte Fang", "tokens": ["Doch", "der", "ver\u00b7hoff\u00b7te", "Fang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Ist offters nicht drey Pfenn\u2019ge werth zu sch\u00e4tzen:", "tokens": ["Ist", "off\u00b7ters", "nicht", "drey", "Pfenn'\u00b7ge", "werth", "zu", "sch\u00e4t\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "CARD", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es zieht/ es m\u00fcht sich Tag und Nacht", "tokens": ["Es", "zieht", "/", "es", "m\u00fcht", "sich", "Tag", "und", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "PRF", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenns den Zug am Rand gebracht/", "tokens": ["Und", "wenns", "den", "Zug", "am", "Rand", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist kaum darinn ein Stichling \u00fcberley/", "tokens": ["Ist", "kaum", "da\u00b7rinn", "ein", "Stich\u00b7ling", "\u00fc\u00b7ber\u00b7ley", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das Lieben ist wie eine Fischerey.", "tokens": ["Das", "Lie\u00b7ben", "ist", "wie", "ei\u00b7ne", "Fi\u00b7sche\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}