{"textgrid.poem.41385": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der sch\u00f6ne Kopf, an ***", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, ja, es reizt auch mich das bl\u00fchende Gesicht,", "tokens": ["Ja", ",", "ja", ",", "es", "reizt", "auch", "mich", "das", "bl\u00fc\u00b7hen\u00b7de", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch ich empfinde selbst die Kraft von diesen Blicken.", "tokens": ["Auch", "ich", "emp\u00b7fin\u00b7de", "selbst", "die", "Kraft", "von", "die\u00b7sen", "Bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mund, das Auge kann entz\u00fccken,", "tokens": ["Der", "Mund", ",", "das", "Au\u00b7ge", "kann", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wer verehrt den vollen Busen nicht,", "tokens": ["Und", "wer", "ver\u00b7ehrt", "den", "vol\u00b7len", "Bu\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der alles das an Liebreiz \u00fcbersteiget,", "tokens": ["Der", "al\u00b7les", "das", "an", "Lieb\u00b7reiz", "\u00fc\u00b7bers\u00b7tei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was Paris je gesehn, und Venus je gezeiget?", "tokens": ["Was", "Pa\u00b7ris", "je", "ge\u00b7sehn", ",", "und", "Ve\u00b7nus", "je", "ge\u00b7zei\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "VVPP", "$,", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch Phryne schwatzt, und scherzt. Mein erster Trieb wird kalt.", "tokens": ["Doch", "Phry\u00b7ne", "schwatzt", ",", "und", "scherzt", ".", "Mein", "ers\u00b7ter", "Trieb", "wird", "kalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "KON", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr l\u00e4cherlicher Witz, ihr unertr\u00e4glich Scherzen", "tokens": ["Ihr", "l\u00e4\u00b7cher\u00b7li\u00b7cher", "Witz", ",", "ihr", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verliert die schon gefangnen Herzen:", "tokens": ["Ver\u00b7liert", "die", "schon", "ge\u00b7fang\u00b7nen", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich merke kaum die t\u00e4uschende Gestalt.", "tokens": ["Ich", "mer\u00b7ke", "kaum", "die", "t\u00e4u\u00b7schen\u00b7de", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.5": {"text": "Es wird ihr Sieg bef\u00f6rdert, und gest\u00f6ret,", "tokens": ["Es", "wird", "ihr", "Sieg", "be\u00b7f\u00f6r\u00b7dert", ",", "und", "ge\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So oft man sie erblickt, so oft man sie geh\u00f6ret.", "tokens": ["So", "oft", "man", "sie", "er\u00b7blickt", ",", "so", "oft", "man", "sie", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "VVPP", "$,", "ADV", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Freund, dir ist gewi\u00df Aesopus noch bekannt,", "tokens": ["Mein", "Freund", ",", "dir", "ist", "ge\u00b7wi\u00df", "A\u00b7e\u00b7so\u00b7pus", "noch", "be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Der kl\u00fcgste Phrygier, der uns vom Fuchs erz\u00e4hlet,", "tokens": ["Der", "kl\u00fcgs\u00b7te", "Phry\u00b7gier", ",", "der", "uns", "vom", "Fuchs", "er\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NE", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df er ein Bild, dem nichts gefehlet,", "tokens": ["Da\u00df", "er", "ein", "Bild", ",", "dem", "nichts", "ge\u00b7feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den sch\u00f6nsten Kopf bei einem K\u00fcnstler fand.", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Kopf", "bei", "ei\u00b7nem", "K\u00fcnst\u00b7ler", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er rief: Wie sch\u00f6n ist Auge, Mund und Stirne!", "tokens": ["Er", "rief", ":", "Wie", "sch\u00f6n", "ist", "Au\u00b7ge", ",", "Mund", "und", "Stir\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "ADJD", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bewundernswerther Kopf, ach h\u00e4ttest du Gehirne!", "tokens": ["Be\u00b7wun\u00b7derns\u00b7wert\u00b7her", "Kopf", ",", "ach", "h\u00e4t\u00b7test", "du", "Ge\u00b7hir\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "XY", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}