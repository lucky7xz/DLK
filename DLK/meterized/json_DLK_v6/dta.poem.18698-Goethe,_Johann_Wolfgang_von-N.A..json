{"dta.poem.18698": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Behandelt die Frauen mit Nachsicht!", "tokens": ["Be\u00b7han\u00b7delt", "die", "Frau\u00b7en", "mit", "Nach\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Aus krummer Rippe ward sie erschaffen,", "tokens": ["Aus", "krum\u00b7mer", "Rip\u00b7pe", "ward", "sie", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gott konnte sie nicht ganz grade machen.", "tokens": ["Gott", "konn\u00b7te", "sie", "nicht", "ganz", "gra\u00b7de", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Willst du sie biegen, sie bricht.", "tokens": ["Willst", "du", "sie", "bie\u00b7gen", ",", "sie", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "L\u00e4sst du sie ruhig, sie wird noch kr\u00fcmmer,", "tokens": ["L\u00e4sst", "du", "sie", "ru\u00b7hig", ",", "sie", "wird", "noch", "kr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Du guter Adam, was ist denn schlimmer? \u2014", "tokens": ["Du", "gu\u00b7ter", "A\u00b7dam", ",", "was", "ist", "denn", "schlim\u00b7mer", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWS", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Behandelt die Frauen mit Nachsicht:", "tokens": ["Be\u00b7han\u00b7delt", "die", "Frau\u00b7en", "mit", "Nach\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Es ist nicht gut dass euch eine Rippe bricht.", "tokens": ["Es", "ist", "nicht", "gut", "dass", "euch", "ei\u00b7ne", "Rip\u00b7pe", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}