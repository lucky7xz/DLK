{"textgrid.poem.42701": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der beleibte tonnenschwere", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der beleibte tonnenschwere", "tokens": ["Der", "be\u00b7leib\u00b7te", "ton\u00b7nen\u00b7schwe\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dorfvikar Spiridion", "tokens": ["Dorf\u00b7vi\u00b7kar", "Spi\u00b7ri\u00b7di\u00b7on"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Fragte bey der Christenlehre", "tokens": ["Frag\u00b7te", "bey", "der", "Chris\u00b7ten\u00b7leh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Veiten einst, ob Gottes Sohn", "tokens": ["Vei\u00b7ten", "einst", ",", "ob", "Got\u00b7tes", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "KOUS", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleichfalls Gott sey, wie der Vater.", "tokens": ["Gleich\u00b7falls", "Gott", "sey", ",", "wie", "der", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nein, sprach Veit, der nicht, Herr Pater!", "tokens": ["Nein", ",", "sprach", "Veit", ",", "der", "nicht", ",", "Herr", "Pa\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "NE", "$,", "PRELS", "PTKNEG", "$,", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie? rief, vor Entsetzen bleich,", "tokens": ["Wie", "?", "rief", ",", "vor", "Ent\u00b7set\u00b7zen", "bleich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Vikar, ey! wer, zum Plunder!", "tokens": ["Der", "Vi\u00b7kar", ",", "ey", "!", "wer", ",", "zum", "Plun\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "PWS", "$,", "APPRART", "NN", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Lehrte dich solch Zeug? Kein Wunder", "tokens": ["Lehr\u00b7te", "dich", "solch", "Zeug", "?", "Kein", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$.", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "W\u00e4r' es, Gott im Himmelreich", "tokens": ["W\u00e4r'", "es", ",", "Gott", "im", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "L\u00e4hmte spornstreichs dir die Zunge.", "tokens": ["L\u00e4hm\u00b7te", "sporn\u00b7streichs", "dir", "die", "Zun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sachte, sachte! sprach der Junge,", "tokens": ["Sach\u00b7te", ",", "sach\u00b7te", "!", "sprach", "der", "Jun\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Macht nur kein so wild Gesicht!", "tokens": ["Macht", "nur", "kein", "so", "wild", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "ADV", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Noch bis jetzt ist er es nicht:", "tokens": ["Noch", "bis", "jetzt", "ist", "er", "es", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Doch sollt' einst der Vater sterben,", "tokens": ["Doch", "sollt'", "einst", "der", "Va\u00b7ter", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Dann vermuth' ich, Herr Kaplan,", "tokens": ["Dann", "ver\u00b7muth'", "ich", ",", "Herr", "Ka\u00b7plan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Dass es ihm als n\u00e4chstem Erben", "tokens": ["Dass", "es", "ihm", "als", "n\u00e4chs\u00b7tem", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ganz gewiss nicht fehlen kann.", "tokens": ["Ganz", "ge\u00b7wiss", "nicht", "feh\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}