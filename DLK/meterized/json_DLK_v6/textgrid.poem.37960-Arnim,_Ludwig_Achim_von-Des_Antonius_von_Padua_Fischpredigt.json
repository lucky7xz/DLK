{"textgrid.poem.37960": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Des Antonius von Padua Fischpredigt", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Antonius zur Predig", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "zur", "Pre\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kirche findt ledig,", "tokens": ["Die", "Kir\u00b7che", "findt", "le\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er geht zu den Fl\u00fcssen,", "tokens": ["Er", "geht", "zu", "den", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und predigt den Fischen;", "tokens": ["Und", "pre\u00b7digt", "den", "Fi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie schlagn mit den Schw\u00e4nzen,", "tokens": ["Sie", "schlagn", "mit", "den", "Schw\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Im Sonnenschein gl\u00e4nzen.", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Karpfen mit Rogen", "tokens": ["Die", "Karp\u00b7fen", "mit", "Ro\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sind all hieher zogen,", "tokens": ["Sind", "all", "hie\u00b7her", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PAV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Haben d' M\u00e4uler aufrissen,", "tokens": ["Ha\u00b7ben", "d'", "M\u00e4u\u00b7ler", "auf\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "VVPP", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sich Zuh\u00f6rens beflissen:", "tokens": ["Sich", "Zu\u00b7h\u00f6\u00b7rens", "be\u00b7flis\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Kein Predig niemalen", "tokens": ["Kein", "Pre\u00b7dig", "nie\u00b7ma\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Karpfen so gfallen.", "tokens": ["Den", "Karp\u00b7fen", "so", "gfal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Spitzgoschete Hechte,", "tokens": ["Spitz\u00b7go\u00b7sche\u00b7te", "Hech\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die immerzu fechten,", "tokens": ["Die", "im\u00b7mer\u00b7zu", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sind eilend herschwommen", "tokens": ["Sind", "ei\u00b7lend", "her\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu h\u00f6ren den Frommen:", "tokens": ["Zu", "h\u00f6\u00b7ren", "den", "From\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kein Predig niemalen", "tokens": ["Kein", "Pre\u00b7dig", "nie\u00b7ma\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Hechten so gfallen.", "tokens": ["Den", "Hech\u00b7ten", "so", "gfal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Auch jene Phantasten", "tokens": ["Auch", "je\u00b7ne", "Phan\u00b7tas\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PDAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So immer beym Fasten,", "tokens": ["So", "im\u00b7mer", "beym", "Fas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Stockfisch ich meine", "tokens": ["Die", "Stock\u00b7fisch", "ich", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PPOSAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zur Predig erscheinen.", "tokens": ["Zur", "Pre\u00b7dig", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kein Predig niemalen", "tokens": ["Kein", "Pre\u00b7dig", "nie\u00b7ma\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Stockfisch so gfallen.", "tokens": ["Den", "Stock\u00b7fisch", "so", "gfal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Gut Aalen und Hausen", "tokens": ["Gut", "Aa\u00b7len", "und", "Hau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Vornehme schmausen,", "tokens": ["Die", "Vor\u00b7neh\u00b7me", "schmau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die selber sich bequemen,", "tokens": ["Die", "sel\u00b7ber", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Predig vernehmen:", "tokens": ["Die", "Pre\u00b7dig", "ver\u00b7neh\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kein Predig niemalen", "tokens": ["Kein", "Pre\u00b7dig", "nie\u00b7ma\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Aalen so gfallen.", "tokens": ["Den", "Aa\u00b7len", "so", "gfal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Auch Krebsen, Schildkroten,", "tokens": ["Auch", "Kreb\u00b7sen", ",", "Schild\u00b7kro\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sonst langsame Boten,", "tokens": ["Sonst", "lang\u00b7sa\u00b7me", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Steigen eilend vom Grund,", "tokens": ["Stei\u00b7gen", "ei\u00b7lend", "vom", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Zu h\u00f6ren diesen Mund:", "tokens": ["Zu", "h\u00f6\u00b7ren", "die\u00b7sen", "Mund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Predig niemalen", "tokens": ["Kein", "Pre\u00b7dig", "nie\u00b7ma\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Den Krebsen so gfallen.", "tokens": ["Den", "Kreb\u00b7sen", "so", "gfal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Fisch gro\u00dfe, Fisch kleine,", "tokens": ["Fisch", "gro\u00b7\u00dfe", ",", "Fisch", "klei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "ADJD", "ADJA", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Vornehm' und gemeine", "tokens": ["Vor\u00b7nehm'", "und", "ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Erheben die K\u00f6pfe", "tokens": ["Er\u00b7he\u00b7ben", "die", "K\u00f6p\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie verst\u00e4ndge Gesch\u00f6pfe:", "tokens": ["Wie", "ver\u00b7st\u00e4nd\u00b7ge", "Ge\u00b7sch\u00f6p\u00b7fe", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Auf Gottes Begehren", "tokens": ["Auf", "Got\u00b7tes", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Antonium anh\u00f6ren.", "tokens": ["An\u00b7to\u00b7ni\u00b7um", "an\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Predigt geendet,", "tokens": ["Die", "Pre\u00b7digt", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein jedes sich wendet,", "tokens": ["Ein", "je\u00b7des", "sich", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PRF", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Hechte bleiben Diebe,", "tokens": ["Die", "Hech\u00b7te", "blei\u00b7ben", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Aale viel lieben.", "tokens": ["Die", "Aa\u00b7le", "viel", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Predig hat gfallen,", "tokens": ["Die", "Pre\u00b7dig", "hat", "gfal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie bleiben wie alle.", "tokens": ["Sie", "blei\u00b7ben", "wie", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KOKOM", "PIAT", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Die Krebs gehn zur\u00fccke,", "tokens": ["Die", "Krebs", "gehn", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Stockfisch bleiben dicke,", "tokens": ["Die", "Stock\u00b7fisch", "blei\u00b7ben", "di\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Karpfen viel fressen,", "tokens": ["Die", "Karp\u00b7fen", "viel", "fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Predig vergessen.", "tokens": ["Die", "Pre\u00b7dig", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Predig hat gfallen,", "tokens": ["Die", "Pre\u00b7dig", "hat", "gfal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie bleiben wie alle.", "tokens": ["Sie", "blei\u00b7ben", "wie", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KOKOM", "PIAT", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}