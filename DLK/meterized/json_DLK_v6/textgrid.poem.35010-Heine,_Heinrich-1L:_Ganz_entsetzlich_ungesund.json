{"textgrid.poem.35010": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz entsetzlich ungesund", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz entsetzlich ungesund", "tokens": ["Ganz", "ent\u00b7setz\u00b7lich", "un\u00b7ge\u00b7sund"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Erde, und zugrund',", "tokens": ["Ist", "die", "Er\u00b7de", ",", "und", "zu\u00b7grund'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, zugrund' mu\u00df alles gehn,", "tokens": ["Ja", ",", "zu\u00b7grund'", "mu\u00df", "al\u00b7les", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was hienieden gro\u00df und sch\u00f6n.", "tokens": ["Was", "hien\u00b7ie\u00b7den", "gro\u00df", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind es alten Wahns Phantasmen,", "tokens": ["Sind", "es", "al\u00b7ten", "Wahns", "Phan\u00b7tas\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dem Boden als Miasmen", "tokens": ["Die", "dem", "Bo\u00b7den", "als", "Mi\u00b7as\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stumm entsteigen und die L\u00fcfte", "tokens": ["Stumm", "ent\u00b7stei\u00b7gen", "und", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schw\u00e4ngern mit dem argen Gifte?", "tokens": ["Schw\u00e4n\u00b7gern", "mit", "dem", "ar\u00b7gen", "Gif\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Holde Frauenblumen, welche", "tokens": ["Hol\u00b7de", "Frau\u00b7en\u00b7blu\u00b7men", ",", "wel\u00b7che"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaum erschlossen ihre Kelche", "tokens": ["Kaum", "er\u00b7schlos\u00b7sen", "ih\u00b7re", "Kel\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den geliebten Sonnenk\u00fcssen,", "tokens": ["Den", "ge\u00b7lieb\u00b7ten", "Son\u00b7nen\u00b7k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat der Tod schon fortgerissen.", "tokens": ["Hat", "der", "Tod", "schon", "fort\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Helden, trabend hoch zu Ro\u00df,", "tokens": ["Hel\u00b7den", ",", "tra\u00b7bend", "hoch", "zu", "Ro\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trifft unsichtbar das Gescho\u00df;", "tokens": ["Trifft", "un\u00b7sicht\u00b7bar", "das", "Ge\u00b7scho\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und die Kr\u00f6ten sich beeifern,", "tokens": ["Und", "die", "Kr\u00f6\u00b7ten", "sich", "be\u00b7ei\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Lorbeer zu begeifern.", "tokens": ["Ih\u00b7ren", "Lor\u00b7beer", "zu", "be\u00b7gei\u00b7fern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was noch gestern stolz gelodert,", "tokens": ["Was", "noch", "ge\u00b7stern", "stolz", "ge\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ist heute schon vermodert;", "tokens": ["Das", "ist", "heu\u00b7te", "schon", "ver\u00b7mo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Seine Leier mit Verdru\u00df", "tokens": ["Sei\u00b7ne", "Lei\u00b7er", "mit", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bricht entzwei der Genius.", "tokens": ["Bricht", "ent\u00b7zwei", "der", "Ge\u00b7nius", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "O wie klug sind doch die Sterne!", "tokens": ["O", "wie", "klug", "sind", "doch", "die", "Ster\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halten sich in sichrer Ferne", "tokens": ["Hal\u00b7ten", "sich", "in", "sich\u00b7rer", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem b\u00f6sen Erdenrund,", "tokens": ["Von", "dem", "b\u00f6\u00b7sen", "Er\u00b7den\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das so t\u00f6dlich ungesund.", "tokens": ["Das", "so", "t\u00f6d\u00b7lich", "un\u00b7ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kluge Sterne wollen nicht", "tokens": ["Klu\u00b7ge", "Ster\u00b7ne", "wol\u00b7len", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leben, Ruhe, Himmelslicht", "tokens": ["Le\u00b7ben", ",", "Ru\u00b7he", ",", "Him\u00b7mels\u00b7licht"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier einb\u00fc\u00dfen, hier auf Erden,", "tokens": ["Hier", "ein\u00b7b\u00fc\u00b7\u00dfen", ",", "hier", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mit uns elendig werden \u2013", "tokens": ["Und", "mit", "uns", "e\u00b7len\u00b7dig", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJD", "VAINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.8": {"line.1": {"text": "Wollen nicht mit uns versinken", "tokens": ["Wol\u00b7len", "nicht", "mit", "uns", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Twieten, welche stinken,", "tokens": ["In", "den", "Twie\u00b7ten", ",", "wel\u00b7che", "stin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Mist, wo W\u00fcrmer kriechen,", "tokens": ["In", "dem", "Mist", ",", "wo", "W\u00fcr\u00b7mer", "krie\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche auch nicht lieblich riechen \u2013", "tokens": ["Wel\u00b7che", "auch", "nicht", "lieb\u00b7lich", "rie\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wollen immer ferne bleiben", "tokens": ["Wol\u00b7len", "im\u00b7mer", "fer\u00b7ne", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vom fatalen Erdentreiben,", "tokens": ["Vom", "fa\u00b7ta\u00b7len", "Er\u00b7den\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Kl\u00fcngel und Geruddel,", "tokens": ["Von", "dem", "Kl\u00fcn\u00b7gel", "und", "Ge\u00b7rud\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem Erdenkuddelmuddel.", "tokens": ["Von", "dem", "Er\u00b7den\u00b7kud\u00b7del\u00b7mud\u00b7del", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mitleidsvoll aus ihrer H\u00f6he", "tokens": ["Mit\u00b7le\u00b7ids\u00b7voll", "aus", "ih\u00b7rer", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schaun sie oft auf unser Wehe;", "tokens": ["Schaun", "sie", "oft", "auf", "un\u00b7ser", "We\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine goldne Tr\u00e4ne f\u00e4llt", "tokens": ["Ei\u00b7ne", "gold\u00b7ne", "Tr\u00e4\u00b7ne", "f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann herab auf diese Welt.", "tokens": ["Dann", "her\u00b7ab", "auf", "die\u00b7se", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}