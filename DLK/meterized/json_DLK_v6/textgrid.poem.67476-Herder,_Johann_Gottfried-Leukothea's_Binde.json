{"textgrid.poem.67476": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Leukothea's Binde", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lerne die Lehren der Schule; doch, gleich der Leukothea Binde,", "tokens": ["Ler\u00b7ne", "die", "Leh\u00b7ren", "der", "Schu\u00b7le", ";", "doch", ",", "gleich", "der", "Leu\u00b7ko\u00b7thea", "Bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "ADV", "$,", "ADV", "ART", "NE", "NN", "$,"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.2": {"text": "Bist Du am Ufer, so wirf sie in die Wellen zur\u00fcck!", "tokens": ["Bist", "Du", "am", "U\u00b7fer", ",", "so", "wirf", "sie", "in", "die", "Wel\u00b7len", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}