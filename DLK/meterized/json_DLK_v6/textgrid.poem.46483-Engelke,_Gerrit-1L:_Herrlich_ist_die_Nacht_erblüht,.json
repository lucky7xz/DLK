{"textgrid.poem.46483": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herrlich ist die Nacht erbl\u00fcht,", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herrlich ist die Nacht erbl\u00fcht,", "tokens": ["Herr\u00b7lich", "ist", "die", "Nacht", "er\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von jedem Blinkstern spr\u00fcht", "tokens": ["Von", "je\u00b7dem", "Blinks\u00b7tern", "spr\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Himmelstropfen \u2013", "tokens": ["Ein", "Him\u00b7mel\u00b7strop\u00b7fen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die dunkelschwere Schweigestadt", "tokens": ["Die", "dun\u00b7kel\u00b7schwe\u00b7re", "Schwei\u00b7ge\u00b7stadt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4ft friedlich, tagessatt,", "tokens": ["Schl\u00e4ft", "fried\u00b7lich", ",", "ta\u00b7ges\u00b7satt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Unter Himmelstropfen \u2013", "tokens": ["Un\u00b7ter", "Him\u00b7mel\u00b7strop\u00b7fen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die ganze Stadt ist \u00fcberregnet,", "tokens": ["Die", "gan\u00b7ze", "Stadt", "ist", "\u00fc\u00b7ber\u00b7reg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Licht das alle Schl\u00e4fer segnet,", "tokens": ["Vom", "Licht", "das", "al\u00b7le", "Schl\u00e4\u00b7fer", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Diese Nacht.", "tokens": ["Die\u00b7se", "Nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}