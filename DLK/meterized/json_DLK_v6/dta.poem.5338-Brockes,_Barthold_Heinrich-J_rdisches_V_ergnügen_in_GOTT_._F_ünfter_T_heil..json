{"dta.poem.5338": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Vertrauen st\u00e4rcker wird; kann man klar daraus er-", "tokens": ["Das", "Ver\u00b7trau\u00b7en", "st\u00e4r\u00b7cker", "wird", ";", "kann", "man", "klar", "da\u00b7raus", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$.", "VMFIN", "PIS", "ADJD", "PAV", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sehen,", "tokens": ["se\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df ein lauter GOttes Dienst n\u00fctzlich, gut und n\u00f6htig", "tokens": ["Da\u00df", "ein", "lau\u00b7ter", "Got\u00b7tes", "Dienst", "n\u00fctz\u00b7lich", ",", "gut", "und", "n\u00f6h\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "PIAT", "NN", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "sey,", "tokens": ["sey", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ja nicht dann nur, wann man eintzeln, sondern auch mit", "tokens": ["Ja", "nicht", "dann", "nur", ",", "wann", "man", "eint\u00b7zeln", ",", "son\u00b7dern", "auch", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PTKNEG", "ADV", "ADV", "$,", "PWAV", "PIS", "VVINF", "$,", "KON", "ADV", "APPR"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "andern, singt", "tokens": ["an\u00b7dern", ",", "singt"], "token_info": ["word", "punct", "word"], "pos": ["PIS", "$,", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und, in starck vereinten Ch\u00f6ren, wol gestimmte Lieder bringt.", "tokens": ["Und", ",", "in", "starck", "ver\u00b7ein\u00b7ten", "Ch\u00f6\u00b7ren", ",", "wol", "ge\u00b7stimm\u00b7te", "Lie\u00b7der", "bringt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJD", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Einer solchen Harmonie Wirckung, Anmuth, Kraft und", "tokens": ["Ei\u00b7ner", "sol\u00b7chen", "Har\u00b7mo\u00b7nie", "Wir\u00b7ckung", ",", "An\u00b7muth", ",", "Kraft", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIS", "PIAT", "NN", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.9": {"text": "Macht,", "tokens": ["Macht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Da wir durch Gewohnheit taub, wird zwar leider nicht", "tokens": ["Da", "wir", "durch", "Ge\u00b7wohn\u00b7heit", "taub", ",", "wird", "zwar", "lei\u00b7der", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "$,", "VAFIN", "ADV", "ADV", "PTKNEG"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "geacht;", "tokens": ["ge\u00b7acht", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Aber la\u00dft uns einen fragen, der in einer langen Zeit", "tokens": ["A\u00b7ber", "la\u00dft", "uns", "ei\u00b7nen", "fra\u00b7gen", ",", "der", "in", "ei\u00b7ner", "lan\u00b7gen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PIS", "VVINF", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Und in vielen Jahren nicht der Ges\u00e4nge Lieblichkeit", "tokens": ["Und", "in", "vie\u00b7len", "Jah\u00b7ren", "nicht", "der", "Ge\u00b7s\u00e4n\u00b7ge", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "PTKNEG", "ART", "NN", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "In den Kirchen angeh\u00f6rt. Er wird gantz gewi\u00df gestehn,", "tokens": ["In", "den", "Kir\u00b7chen", "an\u00b7ge\u00b7h\u00f6rt", ".", "Er", "wird", "gantz", "ge\u00b7wi\u00df", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Da\u00df f\u00fcr Lust er kaum gef\u00fchlet, wie ihm eigentlich geschehn.", "tokens": ["Da\u00df", "f\u00fcr", "Lust", "er", "kaum", "ge\u00b7f\u00fch\u00b7let", ",", "wie", "ihm", "ei\u00b7gent\u00b7lich", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "ADV", "VVPP", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "La\u00dft uns denn doch k\u00fcnftig hin Stimm\u2019 und Singen h\u00f6her", "tokens": ["La\u00dft", "uns", "denn", "doch", "k\u00fcnf\u00b7tig", "hin", "Stimm'", "und", "Sin\u00b7gen", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "ADV", "NN", "KON", "NN", "ADJD"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "achten,", "tokens": ["ach\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Und es nicht nur als ein Wunder, auch als ein Geschenck", "tokens": ["Und", "es", "nicht", "nur", "als", "ein", "Wun\u00b7der", ",", "auch", "als", "ein", "Ge\u00b7schenck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "KOUS", "ART", "NN", "$,", "ADV", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "betrachten!", "tokens": ["be\u00b7trach\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "La\u00dft uns unserm Sch\u00f6pfer dancken, da\u00df er uns in diesem", "tokens": ["La\u00dft", "uns", "un\u00b7serm", "Sch\u00f6p\u00b7fer", "dan\u00b7cken", ",", "da\u00df", "er", "uns", "in", "die\u00b7sem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,", "KOUS", "PPER", "PRF", "APPR", "PDAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "Leben,", "tokens": ["Le\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Jhm zur Ehr und uns zur Lust, Stimm\u2019 und Harmonie", "tokens": ["Jhm", "zur", "Ehr", "und", "uns", "zur", "Lust", ",", "Stimm'", "und", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "KON", "PPER", "APPRART", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "gegeben,", "tokens": ["ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Auch zu seinem Ruhm sie \u00f6fters zu gebrauchen, uns bestreben.", "tokens": ["Auch", "zu", "sei\u00b7nem", "Ruhm", "sie", "\u00f6f\u00b7ters", "zu", "ge\u00b7brau\u00b7chen", ",", "uns", "be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}