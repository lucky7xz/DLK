{"textgrid.poem.63576": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[trennt euch zuweilen]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trennt euch zuweilen,", "tokens": ["Trennt", "euch", "zu\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ihr gl\u00fccklich Liebenden!", "tokens": ["Ihr", "gl\u00fcck\u00b7lich", "Lie\u00b7ben\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Ach, nur die Ferne", "tokens": ["Ach", ",", "nur", "die", "Fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gl\u00fcht Seel' und Seele", "tokens": ["Gl\u00fcht", "Seel'", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Magisch zusammen;", "tokens": ["Ma\u00b7gisch", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ach, nur die Sehnsucht", "tokens": ["Ach", ",", "nur", "die", "Sehn\u00b7sucht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Verm\u00e4hlt euch ganz!", "tokens": ["Ver\u00b7m\u00e4hlt", "euch", "ganz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "S\u00fc\u00df ist das Haben", "tokens": ["S\u00fc\u00df", "ist", "das", "Ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Arm in Armen,", "tokens": ["Arm", "in", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "S\u00fc\u00df sind die Gaben,", "tokens": ["S\u00fc\u00df", "sind", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die lebenswarmen,", "tokens": ["Die", "le\u00b7bens\u00b7war\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Des geselligen", "tokens": ["Des", "ge\u00b7sel\u00b7li\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Augenblicks.", "tokens": ["Au\u00b7gen\u00b7blicks", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wie reife Trauben,", "tokens": ["Wie", "rei\u00b7fe", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Des Gartens Zierde", "tokens": ["Des", "Gar\u00b7tens", "Zier\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In sonnigen Lauben,", "tokens": ["In", "son\u00b7ni\u00b7gen", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die voll Begierde", "tokens": ["Die", "voll", "Be\u00b7gier\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.5": {"text": "Wir pfl\u00fccken und naschen,", "tokens": ["Wir", "pfl\u00fc\u00b7cken", "und", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durstig des raschen,", "tokens": ["Durs\u00b7tig", "des", "ra\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Trunkenen Gl\u00fccks.", "tokens": ["Trun\u00b7ke\u00b7nen", "Gl\u00fccks", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Doch gleich dem Weine,", "tokens": ["Doch", "gleich", "dem", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der aus der Kelter", "tokens": ["Der", "aus", "der", "Kel\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Tr\u00fcbe geflossen,", "tokens": ["Tr\u00fc\u00b7be", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lange von dunkeln", "tokens": ["Lan\u00b7ge", "von", "dun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Reifen umschlossen,", "tokens": ["Rei\u00b7fen", "um\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bis er mit Funkeln", "tokens": ["Bis", "er", "mit", "Fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Im Becher gl\u00fcht:", "tokens": ["Im", "Be\u00b7cher", "gl\u00fcht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "So kann nur Liebe", "tokens": ["So", "kann", "nur", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Das Mark durchgl\u00fchen,", "tokens": ["Das", "Mark", "durch\u00b7gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die ausgereift ist", "tokens": ["Die", "aus\u00b7ge\u00b7reift", "ist"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "In Sehnsuchtsm\u00fchen,", "tokens": ["In", "Sehn\u00b7suchts\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Fern und alleine,", "tokens": ["Fern", "und", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bis ihr die Blume,", "tokens": ["Bis", "ihr", "die", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die duftig reine,", "tokens": ["Die", "duf\u00b7tig", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Dauernd erbl\u00fcht.", "tokens": ["Dau\u00b7ernd", "er\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Trennt euch zuweilen,", "tokens": ["Trennt", "euch", "zu\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ihr gl\u00fccklich Liebenden!", "tokens": ["Ihr", "gl\u00fcck\u00b7lich", "Lie\u00b7ben\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Besser, es trennen", "tokens": ["Bes\u00b7ser", ",", "es", "tren\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Euch weite Meilen,", "tokens": ["Euch", "wei\u00b7te", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Als der N\u00e4he", "tokens": ["Als", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Treiben und Jagen,", "tokens": ["Trei\u00b7ben", "und", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wo Herz dem Herzen", "tokens": ["Wo", "Herz", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Mu\u00df ferne schlagen", "tokens": ["Mu\u00df", "fer\u00b7ne", "schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Und Blicke scherzen", "tokens": ["Und", "Bli\u00b7cke", "scher\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "In fremdem Glanz.", "tokens": ["In", "frem\u00b7dem", "Glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ach, nur die Ferne", "tokens": ["Ach", ",", "nur", "die", "Fer\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gl\u00fcht Seel' und Seele", "tokens": ["Gl\u00fcht", "Seel'", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Magisch zusammen;", "tokens": ["Ma\u00b7gisch", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ach, nur die Sehnsucht", "tokens": ["Ach", ",", "nur", "die", "Sehn\u00b7sucht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Verm\u00e4hlt euch ganz!", "tokens": ["Ver\u00b7m\u00e4hlt", "euch", "ganz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}