{"textgrid.poem.25623": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine verbuhlte Gastwirthin", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn doch die Reisende das Klagen unterlie\u00dfen,", "tokens": ["Wenn", "doch", "die", "Rei\u00b7sen\u00b7de", "das", "Kla\u00b7gen", "un\u00b7ter\u00b7lie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unsre Wirthin hier, sie schnelle; denn mich d\u00fcnkt,", "tokens": ["Da\u00df", "uns\u00b7re", "Wirt\u00b7hin", "hier", ",", "sie", "schnel\u00b7le", ";", "denn", "mich", "d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df manche G\u00e4ste mehr von ihr genie\u00dfen,", "tokens": ["Da\u00df", "man\u00b7che", "G\u00e4s\u00b7te", "mehr", "von", "ihr", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als sie in Rechnung bringt.", "tokens": ["Als", "sie", "in", "Rech\u00b7nung", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}