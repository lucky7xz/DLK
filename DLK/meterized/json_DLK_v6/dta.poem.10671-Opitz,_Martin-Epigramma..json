{"dta.poem.10671": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich dir/ Delia/ ein Schreiben zugeschickt/", "tokens": ["Als", "ich", "dir", "/", "De\u00b7lia", "/", "ein", "Schrei\u00b7ben", "zu\u00b7ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$(", "NE", "$(", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Darau\u00df du meine Lieb vnd grosse Gunst erkennet/", "tokens": ["Dar\u00b7au\u00df", "du", "mei\u00b7ne", "Lieb", "vnd", "gros\u00b7se", "Gunst", "er\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hastu es vnverschuldt vnfreundlich angeblickt/", "tokens": ["Has\u00b7tu", "es", "vn\u00b7ver\u00b7schuldt", "vn\u00b7freund\u00b7lich", "an\u00b7ge\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd in des Fewers Gluth au\u00df grossem Zorn verbrennet/", "tokens": ["Vnd", "in", "des", "Fe\u00b7wers", "Gluth", "au\u00df", "gros\u00b7sem", "Zorn", "ver\u00b7bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wunder ich mich nicht/ weil du mir feind gewesen/", "tokens": ["Doch", "wun\u00b7der", "ich", "mich", "nicht", "/", "weil", "du", "mir", "feind", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "PRF", "PTKNEG", "$(", "KOUS", "PPER", "PPER", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ich durch meine Wort dir solchen Grimm erweckt/", "tokens": ["Da\u00df", "ich", "durch", "mei\u00b7ne", "Wort", "dir", "sol\u00b7chen", "Grimm", "er\u00b7weckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "PIAT", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Di\u00df wundert mich viel mehr/ weil du den Briess gelesen/", "tokens": ["Di\u00df", "wun\u00b7dert", "mich", "viel", "mehr", "/", "weil", "du", "den", "Briess", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das deiner Augen glantz jhn nicht hat angesteckt.", "tokens": ["Das", "dei\u00b7ner", "Au\u00b7gen", "glantz", "jhn", "nicht", "hat", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}