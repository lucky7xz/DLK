{"dta.poem.18710": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Uebermacht, Ihr k\u00f6nnt es sp\u00fcren,", "tokens": ["Ue\u00b7ber\u00b7macht", ",", "Ihr", "k\u00f6nnt", "es", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nicht aus der Welt zu bannen;", "tokens": ["Ist", "nicht", "aus", "der", "Welt", "zu", "ban\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir gef\u00e4llt zu conversiren", "tokens": ["Mir", "ge\u00b7f\u00e4llt", "zu", "con\u00b7ver\u00b7si\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVPP", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Gescheiten, mit Tyrannen.", "tokens": ["Mit", "Ge\u00b7schei\u00b7ten", ",", "mit", "Ty\u00b7ran\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da die dummen Eingeengten", "tokens": ["Da", "die", "dum\u00b7men", "Ein\u00b7ge\u00b7eng\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immerfort am st\u00e4rksten pochten.", "tokens": ["Im\u00b7mer\u00b7fort", "am", "st\u00e4rks\u00b7ten", "poch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Halben, die Beschr\u00e4nkten", "tokens": ["Und", "die", "Hal\u00b7ben", ",", "die", "Be\u00b7schr\u00e4nk\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar zu gern uns unterjochten;", "tokens": ["Gar", "zu", "gern", "uns", "un\u00b7ter\u00b7joch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hab\u2019 ich mich f\u00fcr frey erkl\u00e4ret,", "tokens": ["Hab'", "ich", "mich", "f\u00fcr", "frey", "er\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Narren, von den Weisen,", "tokens": ["Von", "den", "Nar\u00b7ren", ",", "von", "den", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese bleiben ungest\u00f6ret,", "tokens": ["Die\u00b7se", "blei\u00b7ben", "un\u00b7ge\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene m\u00f6chten sich zerreissen.", "tokens": ["Je\u00b7ne", "m\u00f6ch\u00b7ten", "sich", "zer\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denken in Gewalt und Liebe", "tokens": ["Den\u00b7ken", "in", "Ge\u00b7walt", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fcssten wir zuletzt uns gatten,", "tokens": ["M\u00fcss\u00b7ten", "wir", "zu\u00b7letzt", "uns", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Machen mir die Sonne tr\u00fcbe", "tokens": ["Ma\u00b7chen", "mir", "die", "Son\u00b7ne", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erhitzen mir den Schatten.", "tokens": ["Und", "er\u00b7hit\u00b7zen", "mir", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hafis auch und Ulrich Hutten", "tokens": ["Ha\u00b7fis", "auch", "und", "Ul\u00b7rich", "Hut\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "KON", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mussten ganz bestimmt sich r\u00fcsten", "tokens": ["Muss\u00b7ten", "ganz", "be\u00b7stimmt", "sich", "r\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "VVFIN", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen braun\u2019 und blaue Kutten;", "tokens": ["Ge\u00b7gen", "braun'", "und", "blau\u00b7e", "Kut\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Meine gehn wie andre Christen.", "tokens": ["Mei\u00b7ne", "gehn", "wie", "and\u00b7re", "Chris\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201eaber nenn\u2019 uns doch die Feinde!\u201c", "tokens": ["\u201e", "a\u00b7ber", "nenn'", "uns", "doch", "die", "Fein\u00b7de", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemand soll sie unterscheiden:", "tokens": ["Nie\u00b7mand", "soll", "sie", "un\u00b7ter\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ich hab\u2019 in der Gemeinde", "tokens": ["Denn", "ich", "hab'", "in", "der", "Ge\u00b7mein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon genug daran zu leiden.", "tokens": ["Schon", "ge\u00b7nug", "da\u00b7ran", "zu", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}