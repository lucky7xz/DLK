{"textgrid.poem.52238": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Hohes Wasser", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hallo, nun dr\u00fccke sich, wer zagt!", "tokens": ["Hal\u00b7lo", ",", "nun", "dr\u00fc\u00b7cke", "sich", ",", "wer", "zagt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PRF", "$,", "PWS", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Austritt der Rheinstrom mit Gebrause,", "tokens": ["Aus\u00b7tritt", "der", "Rhein\u00b7strom", "mit", "Ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schie\u00dft in die Gassen ungefragt", "tokens": ["Schie\u00dft", "in", "die", "Gas\u00b7sen", "un\u00b7ge\u00b7fragt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht sich breit vor jedem Hause!", "tokens": ["Und", "macht", "sich", "breit", "vor", "je\u00b7dem", "Hau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Pocht an die T\u00fcren, st\u00fcrmt den Herd \u2013", "tokens": ["Pocht", "an", "die", "T\u00fc\u00b7ren", ",", "st\u00fcrmt", "den", "Herd", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da hilft kein D\u00e4mmen und kein Stauen!", "tokens": ["Da", "hilft", "kein", "D\u00e4m\u00b7men", "und", "kein", "Stau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er will dem St\u00e4dtchen, das er n\u00e4hrt,", "tokens": ["Er", "will", "dem", "St\u00e4dt\u00b7chen", ",", "das", "er", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch einmal in die Stunden schauen!", "tokens": ["Auch", "ein\u00b7mal", "in", "die", "Stun\u00b7den", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die braune Bergwand allerw\u00e4rts", "tokens": ["Die", "brau\u00b7ne", "Berg\u00b7wand", "al\u00b7ler\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schickt ihm ihr dunkelgelb Gerinnsel:", "tokens": ["Schickt", "ihm", "ihr", "dun\u00b7kel\u00b7gelb", "Ge\u00b7rinn\u00b7sel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, tritt ans Fenster, liebes Herz \u2013", "tokens": ["Komm", ",", "tritt", "ans", "Fens\u00b7ter", ",", "lie\u00b7bes", "Herz", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh, unser Haus auch ward zur Insel!", "tokens": ["Sieh", ",", "un\u00b7ser", "Haus", "auch", "ward", "zur", "In\u00b7sel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "ADV", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch guten Muts! Ob hier und dort", "tokens": ["Doch", "gu\u00b7ten", "Muts", "!", "Ob", "hier", "und", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "KOUS", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Flut auch auf die Treppen springe:", "tokens": ["Die", "Flut", "auch", "auf", "die", "Trep\u00b7pen", "sprin\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu hoch am Fels doch liegt der Ort,", "tokens": ["Zu", "hoch", "am", "Fels", "doch", "liegt", "der", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als da\u00df es uns ans Leben ginge!", "tokens": ["Als", "da\u00df", "es", "uns", "ans", "Le\u00b7ben", "gin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, an der Mauer dort das Merk:", "tokens": ["Sieh", ",", "an", "der", "Mau\u00b7er", "dort", "das", "Merk", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht, Lieb, du kannst den Strich gewahren?", "tokens": ["Nicht", ",", "Lieb", ",", "du", "kannst", "den", "Strich", "ge\u00b7wah\u00b7ren", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dort hemmte sein Zerst\u00f6rungswerk", "tokens": ["Dort", "hemm\u00b7te", "sein", "Zer\u00b7st\u00f6\u00b7rungs\u00b7werk"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der alte Rhein vor sechzig Jahren!", "tokens": ["Der", "al\u00b7te", "Rhein", "vor", "sech\u00b7zig", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da, wahrlich, \u00fcbt' er strengern Brauch,", "tokens": ["Da", ",", "wahr\u00b7lich", ",", "\u00fcbt'", "er", "stren\u00b7gern", "Brauch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie hoch der Schaum auch diesmal fliege!", "tokens": ["Wie", "hoch", "der", "Schaum", "auch", "dies\u00b7mal", "flie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da ri\u00df er meine Mutter auch", "tokens": ["Da", "ri\u00df", "er", "mei\u00b7ne", "Mut\u00b7ter", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit sich als Kind in ihrer Wiege!", "tokens": ["Mit", "sich", "als", "Kind", "in", "ih\u00b7rer", "Wie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KOUS", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch da sogar, sieh nur den Strich,", "tokens": ["Doch", "da", "so\u00b7gar", ",", "sieh", "nur", "den", "Strich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "VVIMP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blieb unser Stand hier ungef\u00e4hrdet!", "tokens": ["Blieb", "un\u00b7ser", "Stand", "hier", "un\u00b7ge\u00b7f\u00e4hr\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Drum auf, lieb Herz, und fasse dich,", "tokens": ["Drum", "auf", ",", "lieb", "Herz", ",", "und", "fas\u00b7se", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "ADJD", "NN", "$,", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie auch die Schneeflut sich geb\u00e4rdet!", "tokens": ["Wie", "auch", "die", "Schnee\u00b7flut", "sich", "ge\u00b7b\u00e4r\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum guten Muts! Gib mir die Hand!", "tokens": ["Drum", "gu\u00b7ten", "Muts", "!", "Gib", "mir", "die", "Hand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$.", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Glaub' mir, der Strom wird uns verschonen!", "tokens": ["Glaub'", "mir", ",", "der", "Strom", "wird", "uns", "ver\u00b7scho\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gott sch\u00fctze nur das Niederland,", "tokens": ["Gott", "sch\u00fct\u00b7ze", "nur", "das", "Nie\u00b7der\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und die in seiner Fl\u00e4che wohnen!", "tokens": ["Und", "die", "in", "sei\u00b7ner", "Fl\u00e4\u00b7che", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du stimmst mir bei, du bist getrost!", "tokens": ["Du", "stimmst", "mir", "bei", ",", "du", "bist", "ge\u00b7trost", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch \u2013 aufs neue siehst du tr\u00fcbe!", "tokens": ["Und", "doch", "\u2013", "aufs", "neu\u00b7e", "siehst", "du", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPRART", "ADJA", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht mehr die Flut, die uns umtost \u2013", "tokens": ["Nicht", "mehr", "die", "Flut", ",", "die", "uns", "um\u00b7tost", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df, was sonst dich \u00e4ngstigt, Liebe!", "tokens": ["Ich", "wei\u00df", ",", "was", "sonst", "dich", "\u00e4ngs\u00b7tigt", ",", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dir ahnt, da\u00df eine andreFlut", "tokens": ["Dir", "ahnt", ",", "da\u00df", "ei\u00b7ne", "and\u00b7re", "Flut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald unsre Herdstatt \u00fcberschwemme \u2013", "tokens": ["Bald", "uns\u00b7re", "Herds\u00b7tatt", "\u00fc\u00b7bersc\u00b7hwem\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich selber ja mit dreistem Mut", "tokens": ["Ich", "sel\u00b7ber", "ja", "mit", "dreis\u00b7tem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00d6ffn' ihr die Schleusen und die D\u00e4mme!", "tokens": ["\u00d6ffn'", "ihr", "die", "Schleu\u00b7sen", "und", "die", "D\u00e4m\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das offne Wort, das k\u00fchn und frei", "tokens": ["Das", "off\u00b7ne", "Wort", ",", "das", "k\u00fchn", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufriefe gern zu offnen Taten;", "tokens": ["Auf\u00b7rie\u00b7fe", "gern", "zu", "off\u00b7nen", "Ta\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das ehrlich z\u00fcrnt und ohne Scheu \u2013", "tokens": ["Das", "ehr\u00b7lich", "z\u00fcrnt", "und", "oh\u00b7ne", "Scheu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sticht sie durch mit keckem Spaten.", "tokens": ["Das", "sticht", "sie", "durch", "mit", "ke\u00b7ckem", "Spa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das gibt Gewalt dem breiten Strahl,", "tokens": ["Das", "gibt", "Ge\u00b7walt", "dem", "brei\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus diesen liebgewordnen R\u00e4umen,", "tokens": ["Aus", "die\u00b7sen", "lieb\u00b7ge\u00b7word\u00b7nen", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Aus diesem ganzen pr\u00e4cht'gen Tal", "tokens": ["Aus", "die\u00b7sem", "gan\u00b7zen", "pr\u00e4cht'\u00b7gen", "Tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf und von dannen uns zu sch\u00e4umen!", "tokens": ["Auf", "und", "von", "dan\u00b7nen", "uns", "zu", "sch\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wohin? \u2013 noch wei\u00df es Gott allein \u2013", "tokens": ["Wo\u00b7hin", "?", "\u2013", "noch", "wei\u00df", "es", "Gott", "al\u00b7lein", "\u2013"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "$(", "ADV", "VVFIN", "PPER", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch bin ich freudig und ergeben!", "tokens": ["Doch", "bin", "ich", "freu\u00b7dig", "und", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und du auch, Liebe, sollst es sein:", "tokens": ["Und", "du", "auch", ",", "Lie\u00b7be", ",", "sollst", "es", "sein", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "NN", "$,", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch solche Springflut h\u00f6rt zum Leben!", "tokens": ["Auch", "sol\u00b7che",