etra"}, "line.2": {"text": "Und lasse k\u00fchl die Welle branden!", "tokens": ["Und", "las\u00b7se", "k\u00fchl", "die", "Wel\u00b7le", "bran\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob hier und dort ein Strick auch rei\u00dft \u2013", "tokens": ["Ob", "hier", "und", "dort", "ein", "Strick", "auch", "rei\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADV", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir werden landen und nicht stranden!", "tokens": ["Wir", "wer\u00b7den", "lan\u00b7den", "und", "nicht", "stran\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hell offen liegt vor uns die Welt,", "tokens": ["Hell", "of\u00b7fen", "liegt", "vor", "uns", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin gerecht in vielen S\u00e4tteln:", "tokens": ["Ich", "bin", "ge\u00b7recht", "in", "vie\u00b7len", "S\u00e4t\u00b7teln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Solange Faust und Sch\u00e4del h\u00e4lt,", "tokens": ["So\u00b7lan\u00b7ge", "Faust", "und", "Sch\u00e4\u00b7del", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NE", "VVFIN", "$,"], 