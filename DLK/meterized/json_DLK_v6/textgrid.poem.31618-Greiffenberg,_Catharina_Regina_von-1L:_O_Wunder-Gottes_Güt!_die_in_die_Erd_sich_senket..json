{"textgrid.poem.31618": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Wunder-Gottes G\u00fct! die in die Erd sich senket.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Wunder-Gottes G\u00fct! die in die Erd sich senket.", "tokens": ["O", "Wun\u00b7der\u00b7Got\u00b7tes", "G\u00fct", "!", "die", "in", "die", "Erd", "sich", "sen\u00b7ket", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ART", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie gr\u00fcnt und prangt hervor / in Nahrung-reicher Art.", "tokens": ["Sie", "gr\u00fcnt", "und", "prangt", "her\u00b7vor", "/", "in", "Nah\u00b7rung\u00b7rei\u00b7cher", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Allmacht hat mit ihr sich in die Erd gepaart:", "tokens": ["die", "All\u00b7macht", "hat", "mit", "ihr", "sich", "in", "die", "Erd", "ge\u00b7paart", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "aus deren W\u00fcrkung Gott / uns diese Gaben schenket.", "tokens": ["aus", "de\u00b7ren", "W\u00fcr\u00b7kung", "Gott", "/", "uns", "die\u00b7se", "Ga\u00b7ben", "schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "$(", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "bey iedem Sichelschnitt / ists billig / da\u00df man denket", "tokens": ["bey", "ie\u00b7dem", "Si\u00b7chel\u00b7schnitt", "/", "ists", "bil\u00b7lig", "/", "da\u00df", "man", "den\u00b7ket"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$(", "VAFIN", "ADJD", "$(", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "an Gottes Gnaden M\u00e4ng' und Lob zum wundern schaart.", "tokens": ["an", "Got\u00b7tes", "Gna\u00b7den", "M\u00e4ng'", "und", "Lob", "zum", "wun\u00b7dern", "schaart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "NN", "KON", "NN", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wenig ja den Dank / als er den Segen / spaart!", "tokens": ["So", "we\u00b7nig", "ja", "den", "Dank", "/", "als", "er", "den", "Se\u00b7gen", "/", "spaart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "sein Gnaden-Herz sich ganz auf uns zu giessen lenket.", "tokens": ["sein", "Gna\u00b7den\u00b7Herz", "sich", "ganz", "auf", "uns", "zu", "gies\u00b7sen", "len\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ein schallends Ehren-Lob soll aus den Halmen gehn /", "tokens": ["Ein", "schal\u00b7lends", "Eh\u00b7ren\u00b7Lob", "soll", "aus", "den", "Hal\u00b7men", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "weil seiner Ehren voll die Erd' / und was sie tr\u00e4get.", "tokens": ["weil", "sei\u00b7ner", "Eh\u00b7ren", "voll", "die", "Erd'", "/", "und", "was", "sie", "tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ART", "NN", "$(", "KON", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Am Lebens Mastbaum soll der Lobes-Segel stehn:", "tokens": ["Am", "Le\u00b7bens", "Mast\u00b7baum", "soll", "der", "Lo\u00b7bes\u00b7Se\u00b7gel", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Freuden-Seufzer-Wind ihn lieblich s\u00fc\u00df beweget.", "tokens": ["Der", "Freu\u00b7den\u00b7Seuf\u00b7zer\u00b7Wind", "ihn", "lieb\u00b7lich", "s\u00fc\u00df", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So sammlet Gottes Lieb / durch diese Erdenfr\u00fccht:", "tokens": ["So", "samm\u00b7let", "Got\u00b7tes", "Lieb", "/", "durch", "die\u00b7se", "Er\u00b7den\u00b7fr\u00fccht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$(", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und sch\u00fcttet daf\u00fcr aus / sein Lieb-und Lob-Ger\u00fccht!", "tokens": ["und", "sch\u00fct\u00b7tet", "da\u00b7f\u00fcr", "aus", "/", "sein", "Lie\u00b7b\u00b7und", "Lob\u00b7Ge\u00b7r\u00fccht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PTKVZ", "$(", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}}}}