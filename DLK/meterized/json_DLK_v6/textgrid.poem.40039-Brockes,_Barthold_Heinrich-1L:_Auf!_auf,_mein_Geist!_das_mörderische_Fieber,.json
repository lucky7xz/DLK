{"textgrid.poem.40039": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf! auf, mein Geist! das m\u00f6rderische Fieber,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! auf, mein Geist! das m\u00f6rderische Fieber,", "tokens": ["Auf", "!", "auf", ",", "mein", "Geist", "!", "das", "m\u00f6r\u00b7de\u00b7ri\u00b7sche", "Fie\u00b7ber", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das deinen Leib sehr Gefoltert und geplagt,", "tokens": ["Das", "dei\u00b7nen", "Leib", "sehr", "Ge\u00b7fol\u00b7tert", "und", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das dir so Blut als Hertz, recht wie ein Wurm, genagt,", "tokens": ["Das", "dir", "so", "Blut", "als", "Hertz", ",", "recht", "wie", "ein", "Wurm", ",", "ge\u00b7nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "NN", "KOUS", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist, durch des H\u00f6chsten Huld, vor\u00fcber,", "tokens": ["Ist", ",", "durch", "des", "H\u00f6chs\u00b7ten", "Huld", ",", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und der Gesundheit Sonnen-Schein", "tokens": ["Und", "der", "Ge\u00b7sund\u00b7heit", "Son\u00b7nen\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vertreibt den wilden Frost, der Marck und Bein ersch\u00fcttert,", "tokens": ["Ver\u00b7treibt", "den", "wil\u00b7den", "Frost", ",", "der", "Marck", "und", "Bein", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wodurch dein gantzer Leib, in grosser Pein, gezittert.", "tokens": ["Wo\u00b7durch", "dein", "gant\u00b7zer", "Leib", ",", "in", "gros\u00b7ser", "Pein", ",", "ge\u00b7zit\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Um Deine Gnade nun, o Gott! recht zu ermessen,", "tokens": ["Um", "Dei\u00b7ne", "Gna\u00b7de", "nun", ",", "o", "Gott", "!", "recht", "zu", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "$,", "FM", "NN", "$.", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der vergang'nen Noth so bald nicht zu vergessen;", "tokens": ["Und", "der", "ver\u00b7gang'\u00b7nen", "Noth", "so", "bald", "nicht", "zu", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So leite mir (da\u00df ich der Kranckheit Jammer-Stand", "tokens": ["So", "lei\u00b7te", "mir", "(", "da\u00df", "ich", "der", "Kran\u00b7ck\u00b7heit", "Jam\u00b7mer\u00b7Stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und der Gesunheit Schatz recht bilde) selbst die Hand!", "tokens": ["Und", "der", "Ge\u00b7sun\u00b7heit", "Schatz", "recht", "bil\u00b7de", ")", "selbst", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADJD", "VVFIN", "$(", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mich deucht', ich f\u00fchlte schon des Lebens Tacht verlodern;", "tokens": ["Mich", "deucht'", ",", "ich", "f\u00fchl\u00b7te", "schon", "des", "Le\u00b7bens", "Tacht", "ver\u00b7lo\u00b7dern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich deucht', ich s\u00e4h mein Fleisch schon schwinden und vermodern.", "tokens": ["Mich", "deucht'", ",", "ich", "s\u00e4h", "mein", "Fleisch", "schon", "schwin\u00b7den", "und", "ver\u00b7mo\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald klemmt' ein frostig Eis, und dr\u00fcckt' ein bitter Schmertz", "tokens": ["Bald", "klemmt'", "ein", "fros\u00b7tig", "Eis", ",", "und", "dr\u00fcckt'", "ein", "bit\u00b7ter", "Schmertz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fleisch, Sehnen, Hirn und Marck. Das sehr bedr\u00e4ngte Hertz", "tokens": ["Fleisch", ",", "Seh\u00b7nen", ",", "Hirn", "und", "Marck", ".", "Das", "sehr", "be\u00b7dr\u00e4ng\u00b7te", "Hertz"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$.", "PDS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schlug \u00e4ngstlich, klopft' und pochte,", "tokens": ["Schlug", "\u00e4ngst\u00b7lich", ",", "klopft'", "und", "poch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Genagt, erhitzt, gepresst von unsichtbarer Gluth.", "tokens": ["Ge\u00b7nagt", ",", "er\u00b7hitzt", ",", "ge\u00b7presst", "von", "un\u00b7sicht\u00b7ba\u00b7rer", "Gluth", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das gantz verwirrte Blut", "tokens": ["Das", "gantz", "ver\u00b7wirr\u00b7te", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Folg, tobte, sch\u00e4umt' und kochte.", "tokens": ["Folg", ",", "tob\u00b7te", ",", "sch\u00e4umt'", "und", "koch\u00b7te", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie ein verschlossen Na\u00df, das auf dem Feuer stehet,", "tokens": ["Wie", "ein", "ver\u00b7schlos\u00b7sen", "Na\u00df", ",", "das", "auf", "dem", "Feu\u00b7er", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald auf- bald niederw\u00e4rts voll Unruh braust und wallt:", "tokens": ["Bald", "auf", "bald", "nie\u00b7der\u00b7w\u00e4rts", "voll", "Un\u00b7ruh", "braust", "und", "wallt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "ADV", "ADV", "ADJD", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie ein verwehter Staub sich wirbelt, circkelt, drehet,", "tokens": ["Wie", "ein", "ver\u00b7weh\u00b7ter", "Staub", "sich", "wir\u00b7belt", ",", "cir\u00b7ckelt", ",", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In der bewegten Luft ohn' allen Aufenthalt;", "tokens": ["In", "der", "be\u00b7weg\u00b7ten", "Luft", "ohn'", "al\u00b7len", "Auf\u00b7ent\u00b7halt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wanckten Sinn und Geist, erb\u00e4rmlich umgetrieben.", "tokens": ["So", "wanck\u00b7ten", "Sinn", "und", "Geist", ",", "er\u00b7b\u00e4rm\u00b7lich", "um\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Essens-Lust war fort, vom Schlaf war keine Spur:", "tokens": ["Die", "Es\u00b7sens\u00b7Lust", "war", "fort", ",", "vom", "Schlaf", "war", "kei\u00b7ne", "Spur", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,", "APPRART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur Dust, in welchem doch fast wider die Natur", "tokens": ["Nur", "Dust", ",", "in", "wel\u00b7chem", "doch", "fast", "wi\u00b7der", "die", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "APPR", "PRELS", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein banger Eckel steckt, verzehrte meine Kr\u00e4fte;", "tokens": ["Ein", "ban\u00b7ger", "E\u00b7ckel", "steckt", ",", "ver\u00b7zehr\u00b7te", "mei\u00b7ne", "Kr\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die starre Zunge lechtzt' und klebt' am d\u00fcrren Gaum:", "tokens": ["Die", "star\u00b7re", "Zun\u00b7ge", "lechtzt'", "und", "klebt'", "am", "d\u00fcr\u00b7ren", "Gaum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es war die Fl\u00fcssigkeit der s\u00fcssen Lebens-S\u00e4fte", "tokens": ["Es", "war", "die", "Fl\u00fcs\u00b7sig\u00b7keit", "der", "s\u00fcs\u00b7sen", "Le\u00b7bens\u00b7S\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verd\u00fcnstet und verkocht: Ein schleimigt-z\u00e4her Schaum", "tokens": ["Ver\u00b7d\u00fcns\u00b7tet", "und", "ver\u00b7kocht", ":", "Ein", "schlei\u00b7mig\u00b7tz\u00e4\u00b7her", "Schaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Band oft die Lippen fest; das Hertze wollte brechen;", "tokens": ["Band", "oft", "die", "Lip\u00b7pen", "fest", ";", "das", "Hert\u00b7ze", "woll\u00b7te", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$.", "PDS", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Oft schien sich's zu bem\u00fchn,", "tokens": ["Oft", "schien", "sich's", "zu", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Der Marter mit Gewalt sich selber zu entziehn,", "tokens": ["Der", "Mar\u00b7ter", "mit", "Ge\u00b7walt", "sich", "sel\u00b7ber", "zu", "ent\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zu steigen aus der Brust. Es ist nicht auszusprechen,", "tokens": ["Zu", "stei\u00b7gen", "aus", "der", "Brust", ".", "Es", "ist", "nicht", "aus\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie manche Leidenschaft des Leibes Qual noch h\u00e4uft.", "tokens": ["Wie", "man\u00b7che", "Lei\u00b7den\u00b7schaft", "des", "Lei\u00b7bes", "Qual", "noch", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bald ward der Sinnen Schiff durch Hoffnung aufgehoben,", "tokens": ["Bald", "ward", "der", "Sin\u00b7nen", "Schiff", "durch", "Hoff\u00b7nung", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bald ins Verzweiflungs-Meer versencket und ers\u00e4uft", "tokens": ["Bald", "ins", "Ver\u00b7zweif\u00b7lungs\u00b7Meer", "ver\u00b7sen\u00b7cket", "und", "er\u00b7s\u00e4uft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch Schrecken, Furcht und Angst. Wie, wenn die Wellen toben,", "tokens": ["Durch", "Schre\u00b7cken", ",", "Furcht", "und", "Angst", ".", "Wie", ",", "wenn", "die", "Wel\u00b7len", "to\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$.", "PWAV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn ihr besch\u00e4umter Grimm ein schwaches Schiff best\u00fcrmt,", "tokens": ["Wenn", "ihr", "be\u00b7sch\u00e4um\u00b7ter", "Grimm", "ein", "schwa\u00b7ches", "Schiff", "be\u00b7st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NE", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn er mit falbem Schwall sich immer h\u00f6her th\u00fcrmt,", "tokens": ["Wenn", "er", "mit", "fal\u00b7bem", "Schwall", "sich", "im\u00b7mer", "h\u00f6\u00b7her", "th\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und dann im Augenblick, da Mast und Ruder bricht,", "tokens": ["Und", "dann", "im", "Au\u00b7gen\u00b7blick", ",", "da", "Mast", "und", "Ru\u00b7der", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Schiffer in der Fern' ein helles Licht", "tokens": ["Der", "Schif\u00b7fer", "in", "der", "Fern'", "ein", "hel\u00b7les", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Gantz unverhofft erblickt; das aber pl\u00f6tzlich schwindet,", "tokens": ["Gantz", "un\u00b7ver\u00b7hofft", "er\u00b7blickt", ";", "das", "a\u00b7ber", "pl\u00f6tz\u00b7lich", "schwin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "PDS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er seine Noth fast mehr noch, als vorhin, empfindet;", "tokens": ["Er", "sei\u00b7ne", "Noth", "fast", "mehr", "noch", ",", "als", "vor\u00b7hin", ",", "emp\u00b7fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADV", "ADV", "$,", "KOUS", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "So ward auch ich, durchs Licht verhoffter Besserung,", "tokens": ["So", "ward", "auch", "ich", ",", "durchs", "Licht", "ver\u00b7hoff\u00b7ter", "Bes\u00b7se\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das schnell verschwand, get\u00e4uscht. Verschiedene Gedancken", "tokens": ["Das", "schnell", "ver\u00b7schwand", ",", "ge\u00b7t\u00e4uscht", ".", "Ver\u00b7schie\u00b7de\u00b7ne", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "VVPP", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Erquickten theils mein Hertz; theils rissen sie die Schrancken", "tokens": ["Er\u00b7quick\u00b7ten", "theils", "mein", "Hertz", ";", "theils", "ris\u00b7sen", "sie", "die", "Schran\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Gduld'ger Hoffnug ein:", "tokens": ["Gduld'\u00b7ger", "Hoff\u00b7nug", "ein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.30": {"text": "Bald kr\u00e4nckt mich der Verlust des neuen Ehren-Standes,", "tokens": ["Bald", "kr\u00e4nckt", "mich", "der", "Ver\u00b7lust", "des", "neu\u00b7en", "Eh\u00b7ren\u00b7Stan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den ich mir, zu dem Dienst und Nutz des Vaterlandes,", "tokens": ["Den", "ich", "mir", ",", "zu", "dem", "Dienst", "und", "Nutz", "des", "Va\u00b7ter\u00b7lan\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "$,", "APPR", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Von Gott geschenckt geglaubt: Mein frommes Eh-Gemahl,", "tokens": ["Von", "Gott", "ge\u00b7schenckt", "ge\u00b7glaubt", ":", "Mein", "from\u00b7mes", "Eh\u00b7Ge\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VVPP", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "F\u00fcnf sch\u00f6ne Kinderchen vermehrten meine Plagen;", "tokens": ["F\u00fcnf", "sch\u00f6\u00b7ne", "Kin\u00b7der\u00b7chen", "ver\u00b7mehr\u00b7ten", "mei\u00b7ne", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Doch kann ich hievon mit Wahrheits-Grunde sagen,", "tokens": ["Doch", "kann", "ich", "hie\u00b7von", "mit", "Wahr\u00b7heits\u00b7Grun\u00b7de", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PAV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.35": {"text": "Da\u00df alles dieses mir weit mindern Gram gemacht,", "tokens": ["Da\u00df", "al\u00b7les", "die\u00b7ses", "mir", "weit", "min\u00b7dern", "Gram", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDAT", "PPER", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Als mancher glauben wird, und als ich selbst gedacht.", "tokens": ["Als", "man\u00b7cher", "glau\u00b7ben", "wird", ",", "und", "als", "ich", "selbst", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VAFIN", "$,", "KON", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die\u00df aber, wie ich es gar wohl erkennet habe,", "tokens": ["Die\u00df", "a\u00b7ber", ",", "wie", "ich", "es", "gar", "wohl", "er\u00b7ken\u00b7net", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PWAV", "PPER", "PPER", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "War ein besond'res Gl\u00fcck und Gottes Gabe,", "tokens": ["War", "ein", "be\u00b7son\u00b7d'\u00b7res", "Gl\u00fcck", "und", "Got\u00b7tes", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Da\u00df ich Gut, Haus und Hof, Weib, Kinder, Ehr' und Leben", "tokens": ["Da\u00df", "ich", "Gut", ",", "Haus", "und", "Hof", ",", "Weib", ",", "Kin\u00b7der", ",", "Ehr'", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Allein als G\u00fcter nahm, die Gott allein gegeben,", "tokens": ["Al\u00b7lein", "als", "G\u00fc\u00b7ter", "nahm", ",", "die", "Gott", "al\u00b7lein", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und die best\u00e4ndig Sein;", "tokens": ["Und", "die", "be\u00b7st\u00e4n\u00b7dig", "Sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Da\u00df Er allein der Herr; da\u00df sie mir nur geliehen;", "tokens": ["Da\u00df", "Er", "al\u00b7lein", "der", "Herr", ";", "da\u00df", "sie", "mir", "nur", "ge\u00b7lie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$.", "KOUS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da\u00df ER gar wohl befugt, sie wieder zu entziehen.", "tokens": ["Da\u00df", "Er", "gar", "wohl", "be\u00b7fugt", ",", "sie", "wie\u00b7der", "zu", "ent\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und galub' ich, da\u00df die beste Artzeney", "tokens": ["Und", "ga\u00b7lub'", "ich", ",", "da\u00df", "die", "bes\u00b7te", "Art\u00b7ze\u00b7ney"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.45": {"text": "Mir die Gelassenheit gewesen sey.", "tokens": ["Mir", "die", "Ge\u00b7las\u00b7sen\u00b7heit", "ge\u00b7we\u00b7sen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAPP", "VAFIN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.5": {"line.1": {"text": "Itzt aber, da ich nun, durch Gottes Huld, genesen,", "tokens": ["Itzt", "a\u00b7ber", ",", "da", "ich", "nun", ",", "durch", "Got\u00b7tes", "Huld", ",", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "$,", "APPR", "NN", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich nun wieder bin, was zuvor gewesen,", "tokens": ["Da", "ich", "nun", "wie\u00b7der", "bin", ",", "was", "zu\u00b7vor", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,", "PRELS", "ADV", "VAPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da aller Schmertz dahin, als ein leichter Schaum;", "tokens": ["Da", "al\u00b7ler", "Schmertz", "da\u00b7hin", ",", "als", "ein", "leich\u00b7ter", "Schaum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PAV", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ist mir, ob wacht' ich auf aus einem schweren Traum,", "tokens": ["Ist", "mir", ",", "ob", "wacht'", "ich", "auf", "aus", "ei\u00b7nem", "schwe\u00b7ren", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der uns, bey dunckler Nacht, mit schwartzen Larven plaget,", "tokens": ["Der", "uns", ",", "bey", "dunck\u00b7ler", "Nacht", ",", "mit", "schwart\u00b7zen", "Lar\u00b7ven", "pla\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Schreck-Gespensten droht; der aber, wenn es taget,", "tokens": ["Mit", "Schreck\u00b7Ge\u00