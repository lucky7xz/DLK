{"dta.poem.9191": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n Marindgen das Hertzen-Diebgen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Marindgen/ wer hat mir mein hertze gestohlen/", "tokens": ["Ma\u00b7rind\u00b7gen", "/", "wer", "hat", "mir", "mein", "hert\u00b7ze", "ge\u00b7stoh\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hast du es etwan selbst gethan?", "tokens": ["Hast", "du", "es", "et\u00b7wan", "selbst", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df mir dasselbe nicht wieder zu holen", "tokens": ["Ich", "wei\u00df", "mir", "das\u00b7sel\u00b7be", "nicht", "wie\u00b7der", "zu", "ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "PTKNEG", "ADV", "PTKZU", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du diebgen gieb dich immer an/", "tokens": ["Du", "dieb\u00b7gen", "gieb", "dich", "im\u00b7mer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mache deinen diebstal kund/", "tokens": ["Und", "ma\u00b7che", "dei\u00b7nen", "diebs\u00b7tal", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So hab ich mein hertze/ so bin ich gesund.", "tokens": ["So", "hab", "ich", "mein", "hert\u00b7ze", "/", "so", "bin", "ich", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "2. Marindgen/ ich suche mein hertze vergebens/", "tokens": ["Ma\u00b7rind\u00b7gen", "/", "ich", "su\u00b7che", "mein", "hert\u00b7ze", "ver\u00b7ge\u00b7bens", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wann ich bey andern leuten bin/", "tokens": ["Wann", "ich", "bey", "an\u00b7dern", "leu\u00b7ten", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du zierde der hertzen/ du krone des lebens", "tokens": ["Du", "zier\u00b7de", "der", "hert\u00b7zen", "/", "du", "kro\u00b7ne", "des", "le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wo steckst du mir das leben hin?", "tokens": ["Wo", "steckst", "du", "mir", "das", "le\u00b7ben", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast es doch gewi\u00df bey dir/", "tokens": ["Du", "hast", "es", "doch", "ge\u00b7wi\u00df", "bey", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach lange den heimlichen diebstahl herf\u00fcr.", "tokens": ["Ach", "lan\u00b7ge", "den", "heim\u00b7li\u00b7chen", "dieb\u00b7stahl", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "3. Marindgen/ du hast mir die helffte genommen/", "tokens": ["Ma\u00b7rind\u00b7gen", "/", "du", "hast", "mir", "die", "helff\u00b7te", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was nutzt mir nun die todte last?", "tokens": ["Was", "nutzt", "mir", "nun", "die", "tod\u00b7te", "last", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beliebe nur immer noch einmahl zu kommen/", "tokens": ["Be\u00b7lie\u00b7be", "nur", "im\u00b7mer", "noch", "ein\u00b7mahl", "zu", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bi\u00df du mich gantz gestohlen hast/", "tokens": ["Bi\u00df", "du", "mich", "gantz", "ge\u00b7stoh\u00b7len", "hast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schleu\u00df mich zu deinem hertzen ein/", "tokens": ["Schleu\u00df", "mich", "zu", "dei\u00b7nem", "hert\u00b7zen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So werd ich vollkommen befriediget seyn.", "tokens": ["So", "werd", "ich", "voll\u00b7kom\u00b7men", "be\u00b7frie\u00b7di\u00b7get", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "4. Mein hertze/ mein hertze Marindgen/ ich sterbe/", "tokens": ["Mein", "hert\u00b7ze", "/", "mein", "hert\u00b7ze", "Ma\u00b7rind\u00b7gen", "/", "ich", "ster\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$(", "PPER", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wo du mich ferner so betr\u00fcbst/", "tokens": ["Wo", "du", "mich", "fer\u00b7ner", "so", "be\u00b7tr\u00fcbst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und/ eh ich in meiner bedr\u00e4ngni\u00df verderbe/", "tokens": ["Und", "/", "eh", "ich", "in", "mei\u00b7ner", "be\u00b7dr\u00e4ng\u00b7ni\u00df", "ver\u00b7der\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "----+--+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mir nicht das hertze wieder gibst/", "tokens": ["Mir", "nicht", "das", "hert\u00b7ze", "wie\u00b7der", "gibst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum komm/ eh sich der schwache geist", "tokens": ["Drum", "komm", "/", "eh", "sich", "der", "schwa\u00b7che", "geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$(", "KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Au\u00df meinem verst\u00fcmmelten c\u00f6rper entreist.", "tokens": ["Au\u00df", "mei\u00b7nem", "ver\u00b7st\u00fcm\u00b7mel\u00b7ten", "c\u00f6r\u00b7per", "en\u00b7treist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}}}}}