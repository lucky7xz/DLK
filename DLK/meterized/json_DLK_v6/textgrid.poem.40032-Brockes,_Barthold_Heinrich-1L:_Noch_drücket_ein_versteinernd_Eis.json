{"textgrid.poem.40032": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch dr\u00fccket ein versteinernd Eis", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch dr\u00fccket ein versteinernd Eis", "tokens": ["Noch", "dr\u00fc\u00b7cket", "ein", "ver\u00b7stei\u00b7nernd", "Eis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die starre Fluth, die harten Felder;", "tokens": ["Die", "star\u00b7re", "Fluth", ",", "die", "har\u00b7ten", "Fel\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es sind annoch bereift und wei\u00df", "tokens": ["Es", "sind", "an\u00b7noch", "be\u00b7reift", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wipfel der beschneiten W\u00e4lder;", "tokens": ["Die", "Wip\u00b7fel", "der", "be\u00b7schnei\u00b7ten", "W\u00e4l\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Himmel deckt annoch ein falbes Dunckel-Grau;", "tokens": ["Den", "Him\u00b7mel", "deckt", "an\u00b7noch", "ein", "fal\u00b7bes", "Dun\u00b7ckel\u00b7Grau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die L\u00fcfte sind noch scharf und rauh,", "tokens": ["Die", "L\u00fcf\u00b7te", "sind", "noch", "scharf", "und", "rauh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und du, fr\u00fchzeitigs Bl\u00fchmchen, dringest", "tokens": ["Und", "du", ",", "fr\u00fch\u00b7zei\u00b7tigs", "Bl\u00fch\u00b7mchen", ",", "drin\u00b7gest"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schon aus dem harten Staub' hervor?", "tokens": ["Schon", "aus", "dem", "har\u00b7ten", "Staub'", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich, den du fast samt dir verj\u00fcngest,", "tokens": ["Ich", ",", "den", "du", "fast", "samt", "dir", "ver\u00b7j\u00fcn\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Bewundre deinen fr\u00fchen Flor.", "tokens": ["Be\u00b7wund\u00b7re", "dei\u00b7nen", "fr\u00fc\u00b7hen", "Flor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du kannst und willt nicht l\u00e4nger mehr verweilen;", "tokens": ["Du", "kannst", "und", "willt", "nicht", "l\u00e4n\u00b7ger", "mehr", "ver\u00b7wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Man sieht dein zartes Laub und deine sch\u00f6ne Bluhme,", "tokens": ["Man", "sieht", "dein", "zar\u00b7tes", "Laub", "und", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Bluh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu deines grossen Sch\u00f6pfers Ruhme,", "tokens": ["Zu", "dei\u00b7nes", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Aus noch beeister Erden eilen.", "tokens": ["Aus", "noch", "be\u00b7eis\u00b7ter", "Er\u00b7den", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach da\u00df dein starcke Trieb, zu Gottes Ehr',", "tokens": ["Ach", "da\u00df", "dein", "star\u00b7cke", "Trieb", ",", "zu", "Got\u00b7tes", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch mir ein reitzend Beyspiel w\u00e4r'!", "tokens": ["Auch", "mir", "ein", "reit\u00b7zend", "Bey\u00b7spiel", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach m\u00f6chte doch dein Laub auch mir zur Folge dienen!", "tokens": ["Ach", "m\u00f6ch\u00b7te", "doch", "dein", "Laub", "auch", "mir", "zur", "Fol\u00b7ge", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "M\u00f6cht' auch in mir ein reger Vorsatz gr\u00fcnen!", "tokens": ["M\u00f6cht'", "auch", "in", "mir", "ein", "re\u00b7ger", "Vor\u00b7satz", "gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ach m\u00f6cht' auch ich so fr\u00fch auf Gott allein", "tokens": ["Ach", "m\u00f6cht'", "auch", "ich", "so", "fr\u00fch", "auf", "Gott", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "PPER", "ADV", "ADJD", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der gr\u00fcnen Hoffnung Ancker gr\u00fcnden!", "tokens": ["Der", "gr\u00fc\u00b7nen", "Hoff\u00b7nung", "An\u00b7cker", "gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So w\u00fcrd' auch ich den warmen Sonnen-Schein", "tokens": ["So", "w\u00fcrd'", "auch", "ich", "den", "war\u00b7men", "Son\u00b7nen\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von Seiner Gnade fr\u00fche finden!", "tokens": ["Von", "Sei\u00b7ner", "Gna\u00b7de", "fr\u00fc\u00b7he", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dein holdes Himmel-blaues Kleid,", "tokens": ["Dein", "hol\u00b7des", "Him\u00b7mel\u00b7blau\u00b7es", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Womit der Sch\u00f6pfer dich so sch\u00f6n", "tokens": ["Wo\u00b7mit", "der", "Sch\u00f6p\u00b7fer", "dich", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit solcher Pracht geschm\u00fcckt, mit solcher Lieblichkeit,", "tokens": ["Mit", "sol\u00b7cher", "Pracht", "ge\u00b7schm\u00fcckt", ",", "mit", "sol\u00b7cher", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Erinnert, mich den Himmel anzusehn,", "tokens": ["E\u00b7rin\u00b7nert", ",", "mich", "den", "Him\u00b7mel", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRF", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Erinnert mich, den Sinn vom Ird'schen abzulencken,", "tokens": ["E\u00b7rin\u00b7nert", "mich", ",", "den", "Sinn", "vom", "Ird'\u00b7schen", "ab\u00b7zu\u00b7len\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und an des Himmels Herrn und Sch\u00f6pfer zu gedencken.", "tokens": ["Und", "an", "des", "Him\u00b7mels", "Herrn", "und", "Sch\u00f6p\u00b7fer", "zu", "ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Sternen-f\u00f6rmige Figur,", "tokens": ["Die", "Ster\u00b7nen\u00b7f\u00f6r\u00b7mi\u00b7ge", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ich an dein Bluhmen sehe,", "tokens": ["So", "ich", "an", "dein", "Bluh\u00b7men", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die leitet mich noch ferner auf die Spur", "tokens": ["Die", "lei\u00b7tet", "mich", "noch", "fer\u00b7ner", "auf", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu jener hell-gestirnen H\u00f6he.", "tokens": ["Zu", "je\u00b7ner", "hell\u00b7ge\u00b7stir\u00b7nen", "H\u00f6\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein lieblicher Geruch erf\u00fcllt mir Hirn und Brust", "tokens": ["Dein", "lieb\u00b7li\u00b7cher", "Ge\u00b7ruch", "er\u00b7f\u00fcllt", "mir", "Hirn", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Balsam- d\u00fcnstenden Vergn\u00fcgungs-schwangern Geistern,", "tokens": ["Mit", "Bal\u00b7sam", "d\u00fcns\u00b7ten\u00b7den", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7schwan\u00b7gern", "Geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die, durch recht unverhoffte Lust,", "tokens": ["Die", ",", "durch", "recht", "un\u00b7ver\u00b7hoff\u00b7te", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich fast der Seele selbst mit s\u00fcsser Macht bemeistern,", "tokens": ["Sich", "fast", "der", "See\u00b7le", "selbst", "mit", "s\u00fcs\u00b7ser", "Macht", "be\u00b7meis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als welche schier im Anmuths-Meer versincket,", "tokens": ["Als", "wel\u00b7che", "schier", "im", "An\u00b7muths\u00b7Meer", "ver\u00b7sin\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn sie, recht wie berauscht durch des Geruches Kraft,", "tokens": ["Wenn", "sie", ",", "recht", "wie", "be\u00b7rauscht", "durch", "des", "Ge\u00b7ru\u00b7ches", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "KOKOM", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.11": {"text": "Den s\u00e4urlich-s\u00fcssen zarten Saft", "tokens": ["Den", "s\u00e4ur\u00b7lich\u00b7s\u00fcs\u00b7sen", "zar\u00b7ten", "Saft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Aus deiner frischen Bl\u00fcht' Sapphirnen Kelchen trincket.", "tokens": ["Aus", "dei\u00b7ner", "fri\u00b7schen", "Bl\u00fcht'", "Sap\u00b7phir\u00b7nen", "Kel\u00b7chen", "trin\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Woraus, indem sie unterw\u00e4rts gekehrt,", "tokens": ["Wo\u00b7raus", ",", "in\u00b7dem", "sie", "un\u00b7ter\u00b7w\u00e4rts", "ge\u00b7kehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Der trockne Saft sich stets ergiesset,", "tokens": ["Der", "trock\u00b7ne", "Saft", "sich", "stets", "er\u00b7gies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und, sonder, da\u00df sie ausgeleert,", "tokens": ["Und", ",", "son\u00b7der", ",", "da\u00df", "sie", "aus\u00b7ge\u00b7leert", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zu unsrer Lust best\u00e4ndig fliesset.", "tokens": ["Zu", "uns\u00b7rer", "Lust", "be\u00b7st\u00e4n\u00b7dig", "flies\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die allers\u00fcssesten Tockayer-Reben", "tokens": ["Die", "al\u00b7ler\u00b7s\u00fcs\u00b7ses\u00b7ten", "To\u00b7ckayer\u00b7\u00b7Re\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Verm\u00f6gen nicht, dergleichen Kraft und Lust", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "nicht", ",", "derg\u00b7lei\u00b7chen", "Kraft", "und", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Dem d\u00fcrren Gaum' und unsrer matten Brust,", "tokens": ["Dem", "d\u00fcr\u00b7ren", "Gaum'", "und", "uns\u00b7rer", "mat\u00b7ten", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Durch ihren Nectar-Saft, zu geben,", "tokens": ["Durch", "ih\u00b7ren", "Nec\u00b7ta\u00b7rSaft", ",", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Als dein gew\u00fcrtzter Dunst, mit Balsam angemischt,", "tokens": ["Als", "dein", "ge\u00b7w\u00fcrtz\u00b7ter", "Dunst", ",", "mit", "Bal\u00b7sam", "an\u00b7ge\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "A