{"textgrid.poem.67754": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn schwarz Gew\u00f6lk das Firmament verh\u00fcllet,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn schwarz Gew\u00f6lk das Firmament verh\u00fcllet,", "tokens": ["Wenn", "schwarz", "Ge\u00b7w\u00f6lk", "das", "Fir\u00b7ma\u00b7ment", "ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So strahlt kein Stern, die Welt ist Nacht.", "tokens": ["So", "strahlt", "kein", "Stern", ",", "die", "Welt", "ist", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Sturm die Wolken rollt und br\u00fcllet,", "tokens": ["Wenn", "Sturm", "die", "Wol\u00b7ken", "rollt", "und", "br\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird das krystallne Meer von Wolken tr\u00fcb gemacht.", "tokens": ["Wird", "das", "krys\u00b7tall\u00b7ne", "Meer", "von", "Wol\u00b7ken", "tr\u00fcb", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn sich ein Strom von hohen Bergen st\u00fcrzt,", "tokens": ["Wenn", "sich", "ein", "Strom", "von", "ho\u00b7hen", "Ber\u00b7gen", "st\u00fcrzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So wird sein Lauf vom Felsst\u00fcck oft verk\u00fcrzt.", "tokens": ["So", "wird", "sein", "Lauf", "vom", "Fels\u00b7st\u00fcck", "oft", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und willst Du klar die helle Wahrheit sehen,", "tokens": ["Und", "willst", "Du", "klar", "die", "hel\u00b7le", "Wahr\u00b7heit", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So treibe von Dir Furcht und Schmerz!", "tokens": ["So", "trei\u00b7be", "von", "Dir", "Furcht", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Willst Du den rechten Fu\u00dfsteig gehen,", "tokens": ["Willst", "Du", "den", "rech\u00b7ten", "Fu\u00df\u00b7steig", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fessle Wollust nicht Dein Herz!", "tokens": ["So", "fess\u00b7le", "Wol\u00b7lust", "nicht", "Dein", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Schmerz und Wollust F\u00fcrsten sind,", "tokens": ["Wo", "Schmerz", "und", "Wol\u00b7lust", "F\u00fcrs\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Dein Gem\u00fcth ein Sklav und blind.", "tokens": ["Ist", "Dein", "Ge\u00b7m\u00fcth", "ein", "Sklav", "und", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}