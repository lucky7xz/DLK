{"textgrid.poem.28633": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ietzt ist die g\u00f6ldne Zeit; wer ietzt kein Gold nicht hat,", "tokens": ["Ietzt", "ist", "die", "g\u00f6ld\u00b7ne", "Zeit", ";", "wer", "ietzt", "kein", "Gold", "nicht", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "PWS", "ADV", "PIAT", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat keine gute Zeit und ist ihm auch kein Rath.", "tokens": ["Hat", "kei\u00b7ne", "gu\u00b7te", "Zeit", "und", "ist", "ihm", "auch", "kein", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "KON", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}