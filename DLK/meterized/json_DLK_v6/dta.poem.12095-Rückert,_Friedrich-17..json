{"dta.poem.12095": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was du im t\u00e4glichen Hinleben leicht vergissest,", "tokens": ["Was", "du", "im", "t\u00e4g\u00b7li\u00b7chen", "Hin\u00b7le\u00b7ben", "leicht", "ver\u00b7gis\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht vergissest, doch nach W\u00fcrden nicht ermissest,", "tokens": ["Wo", "nicht", "ver\u00b7gis\u00b7sest", ",", "doch", "nach", "W\u00fcr\u00b7den", "nicht", "er\u00b7mis\u00b7sest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "VVFIN", "$,", "ADV", "APPR", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fcck der H\u00e4uslichkeit, der Deinen Lieb' und Treue;", "tokens": ["Das", "Gl\u00fcck", "der", "H\u00e4us\u00b7lich\u00b7keit", ",", "der", "Dei\u00b7nen", "Lieb'", "und", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geh auf die Reise nur, so f\u00fchlest du's aufs neue:", "tokens": ["Geh", "auf", "die", "Rei\u00b7se", "nur", ",", "so", "f\u00fch\u00b7lest", "du's", "aufs", "neu\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "$,", "ADV", "VVFIN", "PIS", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn dir vom Hause kommt ein Brief und Kunde giebt,", "tokens": ["Wenn", "dir", "vom", "Hau\u00b7se", "kommt", "ein", "Brief", "und", "Kun\u00b7de", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df alles ist gesund, und dich ins Ferne liebt;", "tokens": ["Da\u00df", "al\u00b7les", "ist", "ge\u00b7sund", ",", "und", "dich", "ins", "Fer\u00b7ne", "liebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "ADJD", "$,", "KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein solcher Gru\u00df, wieviel des Gro\u00dfen du und Sch\u00f6nen", "tokens": ["Ein", "sol\u00b7cher", "Gru\u00df", ",", "wie\u00b7viel", "des", "Gro\u00b7\u00dfen", "du", "und", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Magst drau\u00dfen sehn, wird erst mit innrer Lust es kr\u00f6nen.", "tokens": ["Magst", "drau\u00b7\u00dfen", "sehn", ",", "wird", "erst", "mit", "inn\u00b7rer", "Lust", "es", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,", "VAFIN", "ADV", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}