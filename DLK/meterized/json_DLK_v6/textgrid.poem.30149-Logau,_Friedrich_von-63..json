{"textgrid.poem.30149": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cynthia wil ihren Mann, wann sie stirbt, der Chloris geben;", "tokens": ["Cyn\u00b7thia", "wil", "ih\u00b7ren", "Mann", ",", "wann", "sie", "stirbt", ",", "der", "Chlo\u00b7ris", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ART", "NE", "VVINF", "$."], "meter": "---+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Chloris wil die Erbschafft nicht weiter und zuvor erheben,", "tokens": ["Chlo\u00b7ris", "wil", "die", "Erb\u00b7schafft", "nicht", "wei\u00b7ter", "und", "zu\u00b7vor", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Bi\u00df ein Fund-Register da, (seht mir an den klugen Rath!)", "tokens": ["Bi\u00df", "ein", "Fun\u00b7dRe\u00b7gis\u00b7ter", "da", ",", "(", "seht", "mir", "an", "den", "klu\u00b7gen", "Rath", "!", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "$(", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Bi\u00df zuvor sie sey gewi\u00df, was f\u00fcr Krafft die Erbschafft hat.", "tokens": ["Bi\u00df", "zu\u00b7vor", "sie", "sey", "ge\u00b7wi\u00df", ",", "was", "f\u00fcr", "Krafft", "die", "Erb\u00b7schafft", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "VAFIN", "ADV", "$,", "PRELS", "APPR", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}