{"textgrid.poem.43321": {"metadata": {"author": {"name": "Gerstenberg, Heinrich Wilhelm von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kleine Rose, glaube mir,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleine Rose, glaube mir,", "tokens": ["Du", "klei\u00b7ne", "Ro\u00b7se", ",", "glau\u00b7be", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sollst Lucindens Busen schm\u00fccken.", "tokens": ["Du", "sollst", "Lu\u00b7cin\u00b7dens", "Bu\u00b7sen", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich selber will dich ihr", "tokens": ["Ich", "sel\u00b7ber", "will", "dich", "ihr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Itzt auf den vollen Busen dr\u00fccken.", "tokens": ["Itzt", "auf", "den", "vol\u00b7len", "Bu\u00b7sen", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann sag ich: \u00bbM\u00e4dchen, k\u00fcsse mich,", "tokens": ["Dann", "sag", "ich", ":", "\u00bb", "M\u00e4d\u00b7chen", ",", "k\u00fcs\u00b7se", "mich", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh, die hat Flora dir geweihet.", "tokens": ["Sieh", ",", "die", "hat", "Flo\u00b7ra", "dir", "ge\u00b7wei\u00b7het", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh, wie die Rose sich", "tokens": ["Sieh", ",", "wie", "die", "Ro\u00b7se", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schon \u00fcber ihre Stelle freuet.\u00ab", "tokens": ["Schon", "\u00fc\u00b7ber", "ih\u00b7re", "Stel\u00b7le", "freu\u00b7et", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch untersteht ein J\u00fcngling sich", "tokens": ["Doch", "un\u00b7ter\u00b7steht", "ein", "J\u00fcng\u00b7ling", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich von dem Busen abzubrechen:", "tokens": ["Dich", "von", "dem", "Bu\u00b7sen", "ab\u00b7zu\u00b7bre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann, Rose, r\u00e4che mich,", "tokens": ["Dann", ",", "Ro\u00b7se", ",", "r\u00e4\u00b7che", "mich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann mu\u00dft du ihn gewaltsam stechen.", "tokens": ["Dann", "mu\u00dft", "du", "ihn", "ge\u00b7walt\u00b7sam", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wenn in meines M\u00e4dchens Brust", "tokens": ["Doch", "wenn", "in", "mei\u00b7nes", "M\u00e4d\u00b7chens", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach mir sich zarte W\u00fcnsche regen \u2013", "tokens": ["Nach", "mir", "sich", "zar\u00b7te", "W\u00fcn\u00b7sche", "re\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJA", "NN", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O die geliebte Brust!", "tokens": ["O", "die", "ge\u00b7lieb\u00b7te", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann hauch ihr s\u00fc\u00dfern Duft entgegen.", "tokens": ["Dann", "hauch", "ihr", "s\u00fc\u00b7\u00dfern", "Duft", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}