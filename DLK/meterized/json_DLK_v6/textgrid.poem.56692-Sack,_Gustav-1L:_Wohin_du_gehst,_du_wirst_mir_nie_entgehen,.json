{"textgrid.poem.56692": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin du gehst, du wirst mir nie entgehen,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin du gehst, du wirst mir nie entgehen,", "tokens": ["Wo\u00b7hin", "du", "gehst", ",", "du", "wirst", "mir", "nie", "ent\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "denn meiner Sehnsucht feine Witterung", "tokens": ["denn", "mei\u00b7ner", "Sehn\u00b7sucht", "fei\u00b7ne", "Wit\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "wird schneller, als du glaubst, den k\u00fchlen Sprung", "tokens": ["wird", "schnel\u00b7ler", ",", "als", "du", "glaubst", ",", "den", "k\u00fch\u00b7len", "Sprung"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in das verf\u00fchrerische Land verstehen,", "tokens": ["in", "das", "ver\u00b7f\u00fch\u00b7re\u00b7ri\u00b7sche", "Land", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "in dessen ewig glatten Schattenseen", "tokens": ["in", "des\u00b7sen", "e\u00b7wig", "glat\u00b7ten", "Schat\u00b7ten\u00b7se\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "du dich vor mir geborgen w\u00e4hntest \u2013 jung", "tokens": ["du", "dich", "vor", "mir", "ge\u00b7bor\u00b7gen", "w\u00e4hn\u00b7test", "\u2013", "jung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PRF", "APPR", "PPER", "VVPP", "VAFIN", "$(", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und mittaghei\u00df wird die Erinnerung", "tokens": ["und", "mit\u00b7tag\u00b7hei\u00df", "wird", "die", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "an deine Liebe brausend dich umwehen", "tokens": ["an", "dei\u00b7ne", "Lie\u00b7be", "brau\u00b7send", "dich", "um\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "und wenn du aufwachst, siehst du mich, der dich", "tokens": ["und", "wenn", "du", "auf\u00b7wachst", ",", "siehst", "du", "mich", ",", "der", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit blanken Armen an das Ufer zieht", "tokens": ["mit", "blan\u00b7ken", "Ar\u00b7men", "an", "das", "U\u00b7fer", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und dir mit einem Ku\u00df, dem wehen Stich", "tokens": ["und", "dir", "mit", "ei\u00b7nem", "Ku\u00df", ",", "dem", "we\u00b7hen", "Stich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "der gl\u00fchen Lanze gleich, das Zauberlied", "tokens": ["der", "gl\u00fc\u00b7hen", "Lan\u00b7ze", "gleich", ",", "das", "Zau\u00b7berl\u00b7ied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "einhaucht: uns schwanden l\u00e4ngst schon Raum und Zeit", "tokens": ["ein\u00b7haucht", ":", "uns", "schwan\u00b7den", "l\u00e4ngst", "schon", "Raum", "und", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$.", "PPER", "VVFIN", "ADV", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "was fl\u00fcchtest du dich in die Ewigkeit?", "tokens": ["was", "fl\u00fcch\u00b7test", "du", "dich", "in", "die", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}