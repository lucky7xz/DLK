{"dta.poem.5337": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar auf Menschen Stimm\u2019 und T\u00f6ne, lautes Ruffen, und", "tokens": ["Gar", "auf", "Men\u00b7schen", "Stimm'", "und", "T\u00f6\u00b7ne", ",", "lau\u00b7tes", "Ruf\u00b7fen", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "NN", "NN", "KON", "NN", "$,", "ADJA", "NN", "$,", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Geschrey,", "tokens": ["Ge\u00b7schrey", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Doch insonderheit aufs Beten, welches laut von uns ge-", "tokens": ["Doch", "in\u00b7son\u00b7der\u00b7heit", "aufs", "Be\u00b7ten", ",", "wel\u00b7ches", "laut", "von", "uns", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "schicht,", "tokens": ["schicht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wenn uns etwann H\u00fclffe n\u00f6htig, wenn uns etwas hier", "tokens": ["Wenn", "uns", "et\u00b7wann", "H\u00fclf\u00b7fe", "n\u00f6h\u00b7tig", ",", "wenn", "uns", "et\u00b7was", "hier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "gebricht.", "tokens": ["ge\u00b7bricht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Da\u00df nun die\u00df in Ansehn GOttes unn\u00fctz, \u00fcberfl\u00fc\u00dfig sey,", "tokens": ["Da\u00df", "nun", "die\u00df", "in", "An\u00b7sehn", "Got\u00b7tes", "un\u00b7n\u00fctz", ",", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7\u00dfig", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "APPR", "NN", "NN", "ADJD", "$,", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Meint ich damahls; glaub\u2019s auch noch. Weil dem Sch\u00f6pf-", "tokens": ["Meint", "ich", "da\u00b7mahls", ";", "glaub's", "auch", "noch", ".", "Weil", "dem", "Sch\u00f6pf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "VVFIN", "ADV", "ADV", "$.", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "fer, was uns fehlet,", "tokens": ["fer", ",", "was", "uns", "feh\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Was uns n\u00fctzlich, was uns n\u00f6thig, uns erfreut, und", "tokens": ["Was", "uns", "n\u00fctz\u00b7lich", ",", "was", "uns", "n\u00f6\u00b7thig", ",", "uns", "er\u00b7freut", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "ADJD", "$,", "PRELS", "PPER", "ADJD", "$,", "PPER", "VVFIN", "$,", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "was uns qv\u00e4let", "tokens": ["was", "uns", "qv\u00e4\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Besser als uns selbst bekannt. Er auch minder nicht, nicht", "tokens": ["Bes\u00b7ser", "als", "uns", "selbst", "be\u00b7kannt", ".", "Er", "auch", "min\u00b7der", "nicht", ",", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "KOKOM", "PPER", "ADV", "ADJD", "$.", "PPER", "ADV", "ADV", "PTKNEG", "$,", "PTKNEG"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "mehr,", "tokens": ["mehr", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Durchs Geschrey, beweget wird. Wenn jedoch dadurch", "tokens": ["Durchs", "Ge\u00b7schrey", ",", "be\u00b7we\u00b7get", "wird", ".", "Wenn", "je\u00b7doch", "da\u00b7durch"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "VVFIN", "VAFIN", "$.", "KOUS", "ADV", "PAV"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.15": {"text": "nicht nur", "tokens": ["nicht", "nur"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Andre Menschen, sondern auch noch wol manche Creatur,", "tokens": ["And\u00b7re", "Men\u00b7schen", ",", "son\u00b7dern", "auch", "noch", "wol", "man\u00b7che", "Crea\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADV", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.17": {"text": "Engel oder andre Geister, dem Allm\u00e4chtigen zur Ehr\u2019,", "tokens": ["En\u00b7gel", "o\u00b7der", "and\u00b7re", "Geis\u00b7ter", ",", "dem", "All\u00b7m\u00e4ch\u00b7ti\u00b7gen", "zur", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.18": {"text": "(von der \u00e4usserlichen Andacht, auch aufs innere zu schliessen,", "tokens": ["(", "von", "der", "\u00e4us\u00b7ser\u00b7li\u00b7chen", "An\u00b7dacht", ",", "auch", "aufs", "in\u00b7ne\u00b7re", "zu", "schlies\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "APPRART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Und wie etwann wir zu weilen, durch der Nachtigalleu", "tokens": ["Und", "wie", "et\u00b7wann", "wir", "zu", "wei\u00b7len", ",", "durch", "der", "Nach\u00b7ti\u00b7gal\u00b7leu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PPER", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.20": {"text": "singen", "tokens": ["sin\u00b7gen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Uns ger\u00fchret sehn) dem Sch\u00f6pfer auch ein Lob-Lied mit-", "tokens": ["Uns", "ge\u00b7r\u00fch\u00b7ret", "sehn", ")", "dem", "Sch\u00f6p\u00b7fer", "auch", "ein", "Lob\u00b7Lied", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "VVINF", "$(", "ART", "NN", "ADV", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "zubringen", "tokens": ["zu\u00b7brin\u00b7gen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Angetrieben werden k\u00f6nnen: ja so gar die Eigenschaft", "tokens": ["An\u00b7ge\u00b7trie\u00b7ben", "wer\u00b7den", "k\u00f6n\u00b7nen", ":", "ja", "so", "gar", "die", "Ei\u00b7gen\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAINF", "VMFIN", "$.", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Unsers menschlichen Gem\u00fcths diese w\u00fcrcklich scheint zu", "tokens": ["Un\u00b7sers", "menschli\u00b7chen", "Ge\u00b7m\u00fcths", "die\u00b7se", "w\u00fcrck\u00b7lich", "scheint", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PDAT", "ADJD", "VVFIN", "APPR"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "seyn;", "tokens": ["seyn", ";"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Da\u00df auch selber, wenn wir Beten, selbst-gesprochner W\u00f6r-", "tokens": ["Da\u00df", "auch", "sel\u00b7ber", ",", "wenn", "wir", "Be\u00b7ten", ",", "selbst\u00b7ge\u00b7sproch\u00b7ner", "W\u00f6r"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "$,", "KOUS", "PPER", "NN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.27": {"text": "ter-Kraft,", "tokens": ["ter\u00b7Kraft", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.28": {"text": "Sonderlich wenns laut geschehn, w\u00fcrcklich sich so weit er-", "tokens": ["Son\u00b7der\u00b7lich", "wenns", "laut", "ge\u00b7schehn", ",", "w\u00fcrck\u00b7lich", "sich", "so", "weit", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADJD", "VVPP", "$,", "ADJD", "PRF", "ADV", "ADJD", "TRUNC"], "meter": "+---+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "strecket,", "tokens": ["stre\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Da\u00df die Andacht noch vermehrt, da\u00df der Geist dadurch", "tokens": ["Da\u00df", "die", "An\u00b7dacht", "noch", "ver\u00b7mehrt", ",", "da\u00df", "der", "Geist", "da\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "PAV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "erwecket", "tokens": ["er\u00b7we\u00b7cket"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}