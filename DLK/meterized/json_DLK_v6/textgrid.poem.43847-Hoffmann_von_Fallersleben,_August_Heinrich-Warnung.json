{"textgrid.poem.43847": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was streicht der Kater seinen Bart?", "tokens": ["Was", "streicht", "der", "Ka\u00b7ter", "sei\u00b7nen", "Bart", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mag das sein?", "tokens": ["Was", "mag", "das", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Viel Fremde kehren heut am Tag", "tokens": ["Viel", "Frem\u00b7de", "keh\u00b7ren", "heut", "am", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei uns noch ein.", "tokens": ["Bei", "uns", "noch", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wenn sich der Kater putzt,", "tokens": ["Wenn", "sich", "der", "Ka\u00b7ter", "putzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann kommen G\u00e4st' ins Haus.", "tokens": ["Dann", "kom\u00b7men", "G\u00e4st'", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und habt ihr denn wohl Kaffee auch", "tokens": ["Und", "habt", "ihr", "denn", "wohl", "Kaf\u00b7fee", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Genug gemacht?", "tokens": ["Ge\u00b7nug", "ge\u00b7macht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "An Zwieback und an Kuchen auch", "tokens": ["An", "Zwie\u00b7back", "und", "an", "Ku\u00b7chen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorher gedacht?", "tokens": ["Vor\u00b7her", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wenn sich der Kater putzt,", "tokens": ["Wenn", "sich", "der", "Ka\u00b7ter", "putzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann kommen G\u00e4st' ins Haus.", "tokens": ["Dann", "kom\u00b7men", "G\u00e4st'", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sonntagskleider ziehet an", "tokens": ["Die", "Sonn\u00b7tags\u00b7klei\u00b7der", "zie\u00b7het", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschwind, geschwind!", "tokens": ["Ge\u00b7schwind", ",", "ge\u00b7schwind", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da seht einmal, im Hofe schon", "tokens": ["Da", "seht", "ein\u00b7mal", ",", "im", "Ho\u00b7fe", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fremden sind.", "tokens": ["Die", "Frem\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wenn sich der Kater putzt,", "tokens": ["Wenn", "sich", "der", "Ka\u00b7ter", "putzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann kommen G\u00e4st' ins Haus.", "tokens": ["Dann", "kom\u00b7men", "G\u00e4st'", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Kater, sei ein ander Mal", "tokens": ["O", "Ka\u00b7ter", ",", "sei", "ein", "an\u00b7der", "Mal"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr auf der Hut!", "tokens": ["Mehr", "auf", "der", "Hut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Streichst k\u00fcnftig du so sp\u00e4t den Bart,", "tokens": ["Streichst", "k\u00fcnf\u00b7tig", "du", "so", "sp\u00e4t", "den", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geht's dir nicht gut!", "tokens": ["Geht's", "dir", "nicht", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wenn sich der Kater putzt,", "tokens": ["Wenn", "sich", "der", "Ka\u00b7ter", "putzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dann kommen G\u00e4st' ins Haus.", "tokens": ["Dann", "kom\u00b7men", "G\u00e4st'", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}