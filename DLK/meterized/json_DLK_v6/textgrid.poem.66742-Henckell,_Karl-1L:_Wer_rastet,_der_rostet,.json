{"textgrid.poem.66742": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer rastet, der rostet,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer rastet, der rostet,", "tokens": ["Wer", "ras\u00b7tet", ",", "der", "ros\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wie lieb' ich das Wort!", "tokens": ["Wie", "lieb'", "ich", "das", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es treibt durch die Weiten", "tokens": ["Es", "treibt", "durch", "die", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Des Lebens mich fort.", "tokens": ["Des", "Le\u00b7bens", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Nicht kann mir gen\u00fcgen,", "tokens": ["Nicht", "kann", "mir", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was je ich erfuhr,", "tokens": ["Was", "je", "ich", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Zu ferneren Fl\u00fcgen", "tokens": ["Zu", "fer\u00b7ne\u00b7ren", "Fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Erwittr' ich die Spur.", "tokens": ["Er\u00b7wit\u00b7tr'", "ich", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ihr habt mich im Kampfe", "tokens": ["Ihr", "habt", "mich", "im", "Kamp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Der Tage gesehn,", "tokens": ["Der", "Ta\u00b7ge", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Es kl\u00e4rt sich vom Dampfe,", "tokens": ["Es", "kl\u00e4rt", "sich", "vom", "Damp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Staubwolken verwehn.", "tokens": ["Staub\u00b7wol\u00b7ken", "ver\u00b7wehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Streitlieder verschallen", "tokens": ["Streit\u00b7lie\u00b7der", "ver\u00b7schal\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.14": {"text": "Der st\u00fcrmischen Zeit,", "tokens": ["Der", "st\u00fcr\u00b7mi\u00b7schen", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Zornwaffen zerfallen,", "tokens": ["Zorn\u00b7waf\u00b7fen", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.16": {"text": "Der Walstatt geweiht.", "tokens": ["Der", "Wals\u00b7tatt", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Doch drinnen die Welle", "tokens": ["Doch", "drin\u00b7nen", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Des Blutes, sie kreist", "tokens": ["Des", "Blu\u00b7tes", ",", "sie", "kreist"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Von Zelle zu Zelle,", "tokens": ["Von", "Zel\u00b7le", "zu", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Mit Wachstum gespeist.", "tokens": ["Mit", "Wachs\u00b7tum", "ge\u00b7speist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Die Schale zum Plunder,", "tokens": ["Die", "Scha\u00b7le", "zum", "Plun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Die Kernfrucht zum Licht:", "tokens": ["Die", "Kern\u00b7frucht", "zum", "Licht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.23": {"text": "O Leben, du Wunder,", "tokens": ["O", "Le\u00b7ben", ",", "du", "Wun\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Ich lasse dich nicht!", "tokens": ["Ich", "las\u00b7se", "dich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Du segnest denn heute", "tokens": ["Du", "seg\u00b7nest", "denn", "heu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Mich m\u00e4chtig wie je,", "tokens": ["Mich", "m\u00e4ch\u00b7tig", "wie", "je", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "Da\u00df meine erneute", "tokens": ["Da\u00df", "mei\u00b7ne", "er\u00b7neu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Geb\u00e4rung gescheh.", "tokens": ["Ge\u00b7b\u00e4\u00b7rung", "ge\u00b7scheh", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Es soll mich erf\u00fcllen", "tokens": ["Es", "soll", "mich", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Mit suchender Lust,", "tokens": ["Mit", "su\u00b7chen\u00b7der", "Lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.31": {"text": "Mein Wesen enth\u00fcllen,", "tokens": ["Mein", "We\u00b7sen", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.32": {"text": "Wie kaum ich's gewu\u00dft.", "tokens": ["Wie", "kaum", "ich's", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "VVPP", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.33": {"text": "Es soll mich durchgl\u00fchen", "tokens": ["Es", "soll", "mich", "durch\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "+---+-", "measure": "dactylic.init"}, "line.34": {"text": "Mit sehnendem Brand,", "tokens": ["Mit", "seh\u00b7nen\u00b7dem", "Brand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Mein Herz will erbl\u00fchen", "tokens": ["Mein", "Herz", "will", "er\u00b7bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "F\u00fcrs heilige Land ...", "tokens": ["F\u00fcrs", "hei\u00b7li\u00b7ge", "Land", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Das immer den Tiefen", "tokens": ["Das", "im\u00b7mer", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.38": {"text": "Enttaucht \u00fcber Nacht,", "tokens": ["Ent\u00b7taucht", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "Wenn Stimmen drum riefen", "tokens": ["Wenn", "Stim\u00b7men", "drum", "rie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PAV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Aus innerster Macht. \u2013", "tokens": ["Aus", "in\u00b7ners\u00b7ter", "Macht", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.41": {"text": "Wer wagt, der gewinnt,", "tokens": ["Wer", "wagt", ",", "der", "ge\u00b7winnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.42": {"text": "Wie lieb' ich das Wort!", "tokens": ["Wie", "lieb'", "ich", "das", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.43": {"text": "Es treibt durch die Weiten", "tokens": ["Es", "treibt", "durch", "die", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.44": {"text": "Der Seele mich fort.", "tokens": ["Der", "See\u00b7le", "mich", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}