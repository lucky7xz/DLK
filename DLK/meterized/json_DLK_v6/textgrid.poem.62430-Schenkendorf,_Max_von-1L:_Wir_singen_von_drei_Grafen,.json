{"textgrid.poem.62430": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir singen von drei Grafen,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir singen von drei Grafen,", "tokens": ["Wir", "sin\u00b7gen", "von", "drei", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die unterm Rasen schlafen,", "tokens": ["Die", "un\u00b7term", "Ra\u00b7sen", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So lust- und liebevoll;", "tokens": ["So", "lust", "und", "lie\u00b7be\u00b7voll", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du mu\u00dft nun sanfter klingen,", "tokens": ["Du", "mu\u00dft", "nun", "sanf\u00b7ter", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Lied! wir alle bringen", "tokens": ["O", "Lied", "!", "wir", "al\u00b7le", "brin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Br\u00fcdern dieser Thr\u00e4ne Zoll.", "tokens": ["Den", "Br\u00fc\u00b7dern", "die\u00b7ser", "Thr\u00e4\u00b7ne", "Zoll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war dem ", "tokens": ["Es", "war", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Als ob sich Kr\u00e4nze w\u00f6ben", "tokens": ["Als", "ob", "sich", "Kr\u00e4n\u00b7ze", "w\u00f6\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PRF", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Maien f\u00fcr sein Haupt;", "tokens": ["Im", "Mai\u00b7en", "f\u00fcr", "sein", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es waren Todeskr\u00e4nze \u2013", "tokens": ["Es", "wa\u00b7ren", "To\u00b7des\u00b7kr\u00e4n\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O weh dem falschen Lenze,", "tokens": ["O", "weh", "dem", "fal\u00b7schen", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der uns den liebsten Freund geraubt.", "tokens": ["Der", "uns", "den", "liebs\u00b7ten", "Freund", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er dachte noch im Sinken", "tokens": ["Er", "dach\u00b7te", "noch", "im", "Sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Einen, deren Winken", "tokens": ["Der", "Ei\u00b7nen", ",", "de\u00b7ren", "Win\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Busen z\u00e4rtlich schlug,", "tokens": ["Sein", "Bu\u00b7sen", "z\u00e4rt\u00b7lich", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der holden Frau der Schmerzen,", "tokens": ["Der", "hol\u00b7den", "Frau", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die unterm keuschen Herzen", "tokens": ["Die", "un\u00b7term", "keu\u00b7schen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein edles Kind des Helden trug.", "tokens": ["Ein", "ed\u00b7les", "Kind", "des", "Hel\u00b7den", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Wittwe, schau nach oben,", "tokens": ["O", "Witt\u00b7we", ",", "schau", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Thr\u00e4nen Gott zu loben,", "tokens": ["In", "Thr\u00e4\u00b7nen", "Gott", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du schwerbetr\u00fcbte Frau!", "tokens": ["Du", "schwer\u00b7be\u00b7tr\u00fcb\u00b7te", "Frau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein Liebling steht gekleidet,", "tokens": ["Dein", "Lieb\u00b7ling", "steht", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo Christ die Schafe weidet,", "tokens": ["Wo", "Christ", "die", "Scha\u00b7fe", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch jetzt in Wei\u00df und Himmelblau.", "tokens": ["Noch", "jetzt", "in", "Wei\u00df", "und", "Him\u00b7mel\u00b7blau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus altem S\u00e4ngerstamme", "tokens": ["Aus", "al\u00b7tem", "S\u00e4n\u00b7ger\u00b7stam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein J\u00fcngling, der die Flamme", "tokens": ["Ein", "J\u00fcng\u00b7ling", ",", "der", "die", "Flam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verbarg in stillem Sinn \u2013", "tokens": ["Ver\u00b7barg", "in", "stil\u00b7lem", "Sinn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihn trug als Himmelsbeute", "tokens": ["Ihn", "trug", "als", "Him\u00b7mels\u00b7beu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Engel aus dem Streite", "tokens": ["Ein", "En\u00b7gel", "aus", "dem", "Strei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu seinem Ahnherrn ", "tokens": ["Zu", "sei\u00b7nem", "Ahn\u00b7herrn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Es hatten beide Ritter", "tokens": ["Es", "hat\u00b7ten", "bei\u00b7de", "Rit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Pinsel und die Zither", "tokens": ["Den", "Pin\u00b7sel", "und", "die", "Zi\u00b7ther"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In fr\u00fcher Zeit gef\u00fchrt.", "tokens": ["In", "fr\u00fc\u00b7her", "Zeit", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bis j\u00fcngst ihr tapfres Herze", "tokens": ["Bis", "j\u00fcngst", "ihr", "tapf\u00b7res", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Klang von Stahl und Erze", "tokens": ["Der", "Klang", "von", "Stahl", "und", "Er\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Freiheits-Morgengru\u00df ber\u00fchrt.", "tokens": ["Wie", "Frei\u00b7heits\u00b7Mor\u00b7gen\u00b7gru\u00df", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wen meinen noch die Glocken?", "tokens": ["Wen", "mei\u00b7nen", "noch", "die", "Glo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich mit den krausen Locken,", "tokens": ["Dich", "mit", "den", "krau\u00b7sen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich mit dem schlichten Muth,", "tokens": ["Dich", "mit", "dem", "schlich\u00b7ten", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von altem Frankenadel,", "tokens": ["Von", "al\u00b7tem", "Fran\u00b7ke\u00b7na\u00b7del", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dich ohne Furcht und Tadel", "tokens": ["Dich", "oh\u00b7ne", "Furcht", "und", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}}, "stanza.8": {"line.1": {"text": "In Schlachten so verwegen,", "tokens": ["In", "Schlach\u00b7ten", "so", "ver\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So treu im Krankenpflegen,", "tokens": ["So", "treu", "im", "Kran\u00b7ken\u00b7pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Ritter vom Spital. \u2013", "tokens": ["Ein", "Rit\u00b7ter", "vom", "Spi\u00b7tal", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O heiliges Verm\u00e4chtni\u00df", "tokens": ["O", "hei\u00b7li\u00b7ges", "Ver\u00b7m\u00e4cht\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Freunde, dein Ged\u00e4chtni\u00df", "tokens": ["Dem", "Freun\u00b7de", ",", "dein", "Ge\u00b7d\u00e4cht\u00b7ni\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu preisen in der Jahre Zahl.", "tokens": ["Zu", "prei\u00b7sen", "in", "der", "Jah\u00b7re", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Karwinden und Podangen,", "tokens": ["Kar\u00b7win\u00b7den", "und", "Po\u00b7dan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Lied und Saiten klangen,", "tokens": ["Wo", "Lied", "und", "Sai\u00b7ten", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im sch\u00f6nen Oberland,", "tokens": ["Im", "sch\u00f6\u00b7nen", "O\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nun steht ihr \u00f6d' und schaurig,", "tokens": ["Nun", "steht", "ihr", "\u00f6d'", "und", "schau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun tr\u00e4nkest du so traurig,", "tokens": ["Nun", "tr\u00e4n\u00b7kest", "du", "so", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Passarge, deinen Blumenstrand.", "tokens": ["Pas\u00b7sar\u00b7ge", ",", "dei\u00b7nen", "Blu\u00b7men\u00b7strand", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch Heiden m\u00f6gen klagen,", "tokens": ["Doch", "Hei\u00b7den", "m\u00f6\u00b7gen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir Christen sehn es tagen", "tokens": ["Wir", "Chris\u00b7ten", "sehn", "es", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus Dunkel und aus Blut;", "tokens": ["Aus", "Dun\u00b7kel", "und", "aus", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Eifer w\u00e4chst uns allen,", "tokens": ["Der", "Ei\u00b7fer", "w\u00e4chst", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn solche Opfer fallen", "tokens": ["Wenn", "sol\u00b7che", "Op\u00b7fer", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr unsrer V\u00e4ter h\u00f6chstes Gut.", "tokens": ["F\u00fcr", "uns\u00b7rer", "V\u00e4\u00b7ter", "h\u00f6chs\u00b7tes", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So m\u00f6gt ihr ruhig schlafen,", "tokens": ["So", "m\u00f6gt", "ihr", "ru\u00b7hig", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr lieben deutschen Grafen,", "tokens": ["Ihr", "lie\u00b7ben", "deut\u00b7schen", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis an den j\u00fcngsten Tag.", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir wollen eurer denken,", "tokens": ["Wir", "wol\u00b7len", "eu\u00b7rer", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Euch manchen Becher schenken", "tokens": ["Euch", "man\u00b7chen", "Be\u00b7cher", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bei Freiheits-Mahl und Festgelag.", "tokens": ["Bei", "Frei\u00b7heits\u00b7Mahl", "und", "Fest\u00b7ge\u00b7lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}