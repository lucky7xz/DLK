{"textgrid.poem.44914": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Ged\u00e4chtnisbuch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Menschen Dasein, alt wie jung,", "tokens": ["Des", "Men\u00b7schen", "Da\u00b7sein", ",", "alt", "wie", "jung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lebt zwischen Hoffnung und Erinnerung.", "tokens": ["Lebt", "zwi\u00b7schen", "Hoff\u00b7nung", "und", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jung, sieht dem Wunsch er alle Pfade offen,", "tokens": ["Jung", ",", "sieht", "dem", "Wunsch", "er", "al\u00b7le", "Pfa\u00b7de", "of\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "PPER", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und alt, erinnert er sich eben an sein Hoffen.", "tokens": ["Und", "alt", ",", "e\u00b7rin\u00b7nert", "er", "sich", "e\u00b7ben", "an", "sein", "Hof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}