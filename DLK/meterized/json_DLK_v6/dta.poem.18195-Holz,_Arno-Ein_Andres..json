{"dta.poem.18195": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Ein Andres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ins letzte Stockwerk einer Miethskaserne;", "tokens": ["Ins", "letz\u00b7te", "Stock\u00b7werk", "ei\u00b7ner", "Mieths\u00b7ka\u00b7ser\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier h\u00e4lt der Nordwind sich am liebsten auf", "tokens": ["Hier", "h\u00e4lt", "der", "Nord\u00b7wind", "sich", "am", "liebs\u00b7ten", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PTKA", "ADJD", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und durch das Dachwerk schaun des Himmels Sterne.", "tokens": ["Und", "durch", "das", "Dach\u00b7werk", "schaun", "des", "Him\u00b7mels", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sie ersp\u00e4hn, o, es ist grad genug,", "tokens": ["Was", "sie", "er\u00b7sp\u00e4hn", ",", "o", ",", "es", "ist", "grad", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "FM", "$,", "PPER", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Um mit dem Elend br\u00fcderlich zu weinen:", "tokens": ["Um", "mit", "dem", "E\u00b7lend", "br\u00fc\u00b7der\u00b7lich", "zu", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein St\u00fcckchen Schwarzbrod und ein Wasserkrug,", "tokens": ["Ein", "St\u00fcck\u00b7chen", "Schwarz\u00b7brod", "und", "ein", "Was\u00b7ser\u00b7krug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Werktisch und ein Schemel mit drei Beinen.", "tokens": ["Ein", "Werk\u00b7tisch", "und", "ein", "Sche\u00b7mel", "mit", "drei", "Bei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Fenster ist vernagelt durch ein Brett", "tokens": ["Das", "Fens\u00b7ter", "ist", "ver\u00b7na\u00b7gelt", "durch", "ein", "Brett"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und doch durchpfeift der Wind es hin und wieder", "tokens": ["Und", "doch", "durch\u00b7pfeift", "der", "Wind", "es", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dort auf jenem strohgestopften Bett", "tokens": ["Und", "dort", "auf", "je\u00b7nem", "stroh\u00b7ge\u00b7stopf\u00b7ten", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Liegt fieberkrank ein junges Weib darnieder.", "tokens": ["Liegt", "fie\u00b7ber\u00b7krank", "ein", "jun\u00b7ges", "Weib", "dar\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drei kleine Kinder stehn um sie herum,", "tokens": ["Drei", "klei\u00b7ne", "Kin\u00b7der", "stehn", "um", "sie", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die stieren Blicks an ihren Z\u00fcgen hangen,", "tokens": ["Die", "stie\u00b7ren", "Blicks", "an", "ih\u00b7ren", "Z\u00fc\u00b7gen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Vor vielem Weinen ward ihr M\u00fcndlein stumm", "tokens": ["Vor", "vie\u00b7lem", "Wei\u00b7nen", "ward", "ihr", "M\u00fcnd\u00b7lein", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "VAFIN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und keine Thr\u00e4ne mehr netzt ihre Wangen.", "tokens": ["Und", "kei\u00b7ne", "Thr\u00e4\u00b7ne", "mehr", "netzt", "ih\u00b7re", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein St\u00fcmpfchen Talglicht giebt nur tr\u00fcben Schein,", "tokens": ["Ein", "St\u00fcmpf\u00b7chen", "Talg\u00b7licht", "giebt", "nur", "tr\u00fc\u00b7ben", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch horch, es klopft, was mag das nur bedeuten?", "tokens": ["Doch", "horch", ",", "es", "klopft", ",", "was", "mag", "das", "nur", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PPER", "VVFIN", "$,", "PWS", "VMFIN", "PDS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es klopft und durch die Th\u00fcr tritt nun herein", "tokens": ["Es", "klopft", "und", "durch", "die", "Th\u00fcr", "tritt", "nun", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein junger Herr, gef\u00fchrt von Nachbarsleuten.", "tokens": ["Ein", "jun\u00b7ger", "Herr", ",", "ge\u00b7f\u00fchrt", "von", "Nach\u00b7bars\u00b7leu\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Armenhilfsarzt ist's aus dem Revier,", "tokens": ["Der", "Ar\u00b7men\u00b7hilfs\u00b7arzt", "ist's", "aus", "dem", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den sie geholt aus Mitleid mit der Kranken,", "tokens": ["Den", "sie", "ge\u00b7holt", "aus", "Mit\u00b7leid", "mit", "der", "Kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Inde\u00df ihr Mann bei Branntwein oder Bier", "tokens": ["In\u00b7de\u00df", "ihr", "Mann", "bei", "Brannt\u00b7wein", "o\u00b7der", "Bier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich selbst bet\u00e4ubt und seine Wuthgedanken.", "tokens": ["Sich", "selbst", "be\u00b7t\u00e4ubt", "und", "sei\u00b7ne", "Wuth\u00b7ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der junge Doctor aber nimmt das Licht", "tokens": ["Der", "jun\u00b7ge", "Doc\u00b7tor", "a\u00b7ber", "nimmt", "das", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und tritt mit ihm ans Bett des armen Weibes,", "tokens": ["Und", "tritt", "mit", "ihm", "ans", "Bett", "des", "ar\u00b7men", "Wei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch gelb wie Wachs und spitz ist ihr Gesicht", "tokens": ["Doch", "gelb", "wie", "Wachs", "und", "spitz", "ist", "ihr", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NN", "KON", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kalt und starr die Glieder ihres Leibes.", "tokens": ["Und", "kalt", "und", "starr", "die", "Glie\u00b7der", "ih\u00b7res", "Lei\u00b7bes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da schluchzt sein Herz, inde\u00df das Licht verkohlt,", "tokens": ["Da", "schluchzt", "sein", "Herz", ",", "in\u00b7de\u00df", "das", "Licht", "ver\u00b7kohlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von nie gekannter Wehmuth \u00fcberschlichen:", "tokens": ["Von", "nie", "ge\u00b7kann\u00b7ter", "Weh\u00b7muth", "\u00fc\u00b7berschli\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Weint, Kinder, weint! ich bin zu sp\u00e4t geholt,", "tokens": ["Weint", ",", "Kin\u00b7der", ",", "weint", "!", "ich", "bin", "zu", "sp\u00e4t", "ge\u00b7holt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn eure Mutter ist bereits \u2014 verblichen!", "tokens": ["Denn", "eu\u00b7re", "Mut\u00b7ter", "ist", "be\u00b7reits", "ver\u00b7bli\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}