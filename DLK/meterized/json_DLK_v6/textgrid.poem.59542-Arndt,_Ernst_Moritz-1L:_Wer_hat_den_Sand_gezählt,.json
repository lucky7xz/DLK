{"textgrid.poem.59542": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer hat den Sand gez\u00e4hlt,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat den Sand gez\u00e4hlt,", "tokens": ["Wer", "hat", "den", "Sand", "ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Welcher im Wasser haust?", "tokens": ["Wel\u00b7cher", "im", "Was\u00b7ser", "haust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wem hat kein Blatt gefehlt,", "tokens": ["Wem", "hat", "kein", "Blatt", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wann der November braust?", "tokens": ["Wann", "der", "No\u00b7vem\u00b7ber", "braust", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wei\u00df im Januar", "tokens": ["Wer", "wei\u00df", "im", "Ja\u00b7nu\u00b7ar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wieviel der Flocken wehn,", "tokens": ["Wie\u00b7viel", "der", "Flo\u00b7cken", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie viele auf ein Haar", "tokens": ["Wie", "vie\u00b7le", "auf", "ein", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Tropfen aufs Weltmeer gehn?", "tokens": ["Trop\u00b7fen", "aufs", "Welt\u00b7meer", "gehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wer mi\u00dft den Ozean,", "tokens": ["Wer", "mi\u00dft", "den", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo er am tiefsten flie\u00dft?", "tokens": ["Wo", "er", "am", "tiefs\u00b7ten", "flie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer mag die Strahlen fahn,", "tokens": ["Wer", "mag", "die", "Strah\u00b7len", "fahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Welche die Sonne schie\u00dft?", "tokens": ["Wel\u00b7che", "die", "Son\u00b7ne", "schie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wer holt das Lichtgespann", "tokens": ["Wer", "holt", "das", "Licht\u00b7ge\u00b7spann"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Fliegender Blitze ein?", "tokens": ["Flie\u00b7gen\u00b7der", "Blit\u00b7ze", "ein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Nenne den Wundermann!", "tokens": ["Nen\u00b7ne", "den", "Wun\u00b7der\u00b7mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Keiner mag gr\u00f6\u00dfer sein.", "tokens": ["Kei\u00b7ner", "mag", "gr\u00f6\u00b7\u00dfer", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Gott ist der Ohnezahl,", "tokens": ["Gott", "ist", "der", "Oh\u00b7ne\u00b7zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vor dem die Zahl vergeht,", "tokens": ["Vor", "dem", "die", "Zahl", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der durch den Sternensaal", "tokens": ["Der", "durch", "den", "Ster\u00b7nen\u00b7saal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sonnen wie Flocken weht;", "tokens": ["Son\u00b7nen", "wie", "Flo\u00b7cken", "weht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Gott ist der \u00dcberall,", "tokens": ["Gott", "ist", "der", "\u00dc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Gott ist der Ohnegrund,", "tokens": ["Gott", "ist", "der", "Oh\u00b7ne\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schneller als Licht und Schall,", "tokens": ["Schnel\u00b7ler", "als", "Licht", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Tiefer als Meeresgrund.", "tokens": ["Tie\u00b7fer", "als", "Mee\u00b7res\u00b7grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sandk\u00f6rner z\u00e4hlest du,", "tokens": ["Sand\u00b7k\u00f6r\u00b7ner", "z\u00e4h\u00b7lest", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nimmer die Freundlichkeit,", "tokens": ["Nim\u00b7mer", "die", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Weltmeere missest du,", "tokens": ["Welt\u00b7mee\u00b7re", "mis\u00b7sest", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nie die Barmherzigkeit,", "tokens": ["Nie", "die", "Barm\u00b7her\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sonnenstrahl holst du ein,", "tokens": ["Son\u00b7nen\u00b7strahl", "holst", "du", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Nimmer die Liebe doch,", "tokens": ["Nim\u00b7mer", "die", "Lie\u00b7be", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Womit sein Gnadenschein", "tokens": ["Wo\u00b7mit", "sein", "Gna\u00b7den\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "S\u00fcndern entgegenflog.", "tokens": ["S\u00fcn\u00b7dern", "ent\u00b7ge\u00b7gen\u00b7flog", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}