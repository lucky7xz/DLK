{"textgrid.poem.36905": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Um Ergebung in den g\u00f6ttlichen Willen", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Herr, mein Gott! durch den ich bin und lebe,", "tokens": ["O", "Herr", ",", "mein", "Gott", "!", "durch", "den", "ich", "bin", "und", "le\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$.", "APPR", "PRELS", "PPER", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gib, da\u00df ich mich in deinen Rat ergebe;", "tokens": ["Gib", ",", "da\u00df", "ich", "mich", "in", "dei\u00b7nen", "Rat", "er\u00b7ge\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df ewig deinen Willen mein,", "tokens": ["La\u00df", "e\u00b7wig", "dei\u00b7nen", "Wil\u00b7len", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PPOSAT", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was du tust, mir teuer sein!", "tokens": ["Und", "was", "du", "tust", ",", "mir", "teu\u00b7er", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, du regierst, bist Weisheit, Lieb und St\u00e4rke.", "tokens": ["Du", ",", "du", "re\u00b7gierst", ",", "bist", "Weis\u00b7heit", ",", "Lieb", "und", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, Herr, erbarmst dich aller deiner Werke.", "tokens": ["Du", ",", "Herr", ",", "er\u00b7barmst", "dich", "al\u00b7ler", "dei\u00b7ner", "Wer\u00b7ke", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was zag ich einen Augenblick?", "tokens": ["Was", "zag", "ich", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist mein Gott, und willst mein Gl\u00fcck.", "tokens": ["Du", "bist", "mein", "Gott", ",", "und", "willst", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "VMFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von Ewigkeit hast du mein Los entschieden.", "tokens": ["Von", "E\u00b7wig\u00b7keit", "hast", "du", "mein", "Los", "ent\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was du bestimmt, das dient zu meinem Frieden.", "tokens": ["Was", "du", "be\u00b7stimmt", ",", "das", "dient", "zu", "mei\u00b7nem", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wogst mein Gl\u00fcck, du wogst mein Leid,", "tokens": ["Du", "wogst", "mein", "Gl\u00fcck", ",", "du", "wogst", "mein", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was du schickst, ist Seligkeit.", "tokens": ["Und", "was", "du", "schickst", ",", "ist", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gef\u00e4llt es dir: so m\u00fcsse keine Plage", "tokens": ["Ge\u00b7f\u00e4llt", "es", "dir", ":", "so", "m\u00fcs\u00b7se", "kei\u00b7ne", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$.", "ADV", "VMFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich zu mir nahn; gib mir zufriedne Tage.", "tokens": ["Sich", "zu", "mir", "nahn", ";", "gib", "mir", "zu\u00b7fried\u00b7ne", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "ADJA", "$.", "VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein verwehrt's mein ewig Heil:", "tokens": ["Al\u00b7lein", "ver\u00b7wehrt's", "mein", "e\u00b7wig", "Heil", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bleibe nur dein Trost mein Teil.", "tokens": ["So", "blei\u00b7be", "nur", "dein", "Trost", "mein", "Teil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du gibst aus Huld uns dieser Erde Freuden;", "tokens": ["Du", "gibst", "aus", "Huld", "uns", "die\u00b7ser", "Er\u00b7de", "Freu\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus gleicher Huld verh\u00e4ngst du unsre Leiden.", "tokens": ["Aus", "glei\u00b7cher", "Huld", "ver\u00b7h\u00e4ngst", "du", "uns\u00b7re", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist nur mein Weh nicht meine Schuld:", "tokens": ["Ist", "nur", "mein", "Weh", "nicht", "mei\u00b7ne", "Schuld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So zag ich nicht. Du gibst Geduld.", "tokens": ["So", "zag", "ich", "nicht", ".", "Du", "gibst", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Soll ich ein Gl\u00fcck, das du mir gabst, verlieren,", "tokens": ["Soll", "ich", "ein", "Gl\u00fcck", ",", "das", "du", "mir", "gabst", ",", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und willst du, Gott! mich rauhe Wege f\u00fchren:", "tokens": ["Und", "willst", "du", ",", "Gott", "!", "mich", "rau\u00b7he", "We\u00b7ge", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "NN", "$.", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wirst du, denn du h\u00f6rst mein Flehn,", "tokens": ["So", "wirst", "du", ",", "denn", "du", "h\u00f6rst", "mein", "Flehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir dennoch eine H\u00fclf ersehn.", "tokens": ["Mir", "den\u00b7noch", "ei\u00b7ne", "H\u00fclf", "er\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vielleicht mu\u00df ich nach wenig Tagen sterben.", "tokens": ["Viel\u00b7leicht", "mu\u00df", "ich", "nach", "we\u00b7nig", "Ta\u00b7gen", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herr, wie du willst! Soll ich den Himmel erben,", "tokens": ["Herr", ",", "wie", "du", "willst", "!", "Soll", "ich", "den", "Him\u00b7mel", "er\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VMFIN", "$.", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dieser ist im Glauben mein,", "tokens": ["Und", "die\u00b7ser", "ist", "im", "Glau\u00b7ben", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kann der Tod mir schrecklich sein?", "tokens": ["Wie", "kann", "der", "Tod", "mir", "schreck\u00b7lich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}