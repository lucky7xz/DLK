{"textgrid.poem.58719": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Glaubenswaffen mu\u00df man wohl gewappnet stehn,", "tokens": ["Mit", "Glau\u00b7bens\u00b7waf\u00b7fen", "mu\u00df", "man", "wohl", "ge\u00b7wapp\u00b7net", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "ADV", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will man dem Feind entgegengehn;", "tokens": ["Will", "man", "dem", "Feind", "ent\u00b7ge\u00b7gen\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommt, nur dem K\u00f6nig nahe bleibet,", "tokens": ["Kommt", ",", "nur", "dem", "K\u00f6\u00b7nig", "na\u00b7he", "blei\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihn mit einem Blick vertreibet!", "tokens": ["Der", "ihn", "mit", "ei\u00b7nem", "Blick", "ver\u00b7trei\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies kurze Leben ist die rechte Kampfzeit nur,", "tokens": ["Dies", "kur\u00b7ze", "Le\u00b7ben", "ist", "die", "rech\u00b7te", "Kampf\u00b7zeit", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besiegt den Teufel, die Natur!", "tokens": ["Be\u00b7siegt", "den", "Teu\u00b7fel", ",", "die", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nachl\u00e4ssige sind bald geschlagen,", "tokens": ["Nach\u00b7l\u00e4s\u00b7si\u00b7ge", "sind", "bald", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer will dies schrecklich' Ungl\u00fcck tragen?", "tokens": ["Wer", "will", "dies", "schreck\u00b7lich'", "Un\u00b7gl\u00fcck", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn man dem Feinde folgt, kann man nicht Gottes sein,", "tokens": ["Wenn", "man", "dem", "Fein\u00b7de", "folgt", ",", "kann", "man", "nicht", "Got\u00b7tes", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "VMFIN", "PIS", "PTKNEG", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das bilde dir nicht m\u00f6glich ein!", "tokens": ["Das", "bil\u00b7de", "dir", "nicht", "m\u00f6g\u00b7lich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist, wer Gott will halb anhangen,", "tokens": ["Es", "ist", ",", "wer", "Gott", "will", "halb", "an\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unf\u00e4hig, Gnaden zu erlangen.", "tokens": ["Un\u00b7f\u00e4\u00b7hig", ",", "Gna\u00b7den", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie Jesus wandelte, so gehn wir nach und nach", "tokens": ["Wie", "Je\u00b7sus", "wan\u00b7del\u00b7te", ",", "so", "gehn", "wir", "nach", "und", "nach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "KON", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und folgen auf Kalvaria;", "tokens": ["Und", "fol\u00b7gen", "auf", "Kal\u00b7va\u00b7ria", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sonst kann man keinen Sold empfangen", "tokens": ["Sonst", "kann", "man", "kei\u00b7nen", "Sold", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch mit ihm im Triumphe prangen.", "tokens": ["Noch", "mit", "ihm", "im", "Tri\u00b7um\u00b7phe", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Anbetungsw\u00fcrdiger, mein Hauptmann, geh voran,", "tokens": ["An\u00b7be\u00b7tungs\u00b7w\u00fcr\u00b7di\u00b7ger", ",", "mein", "Haupt\u00b7mann", ",", "geh", "vo\u00b7ran", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich folge deiner Kreuzesfahn'!", "tokens": ["Ich", "fol\u00b7ge", "dei\u00b7ner", "Kreu\u00b7zes\u00b7fahn'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer unter deiner Aufsicht k\u00e4mpfet,", "tokens": ["Wer", "un\u00b7ter", "dei\u00b7ner", "Auf\u00b7sicht", "k\u00e4mp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheut keinen Ernst, die Feinde d\u00e4mpfet.", "tokens": ["Scheut", "kei\u00b7nen", "Ernst", ",", "die", "Fein\u00b7de", "d\u00e4mp\u00b7fet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}