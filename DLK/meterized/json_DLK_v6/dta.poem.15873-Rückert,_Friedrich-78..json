{"dta.poem.15873": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Gott l\u00e4\u00dft man sich viel, l\u00e4\u00dft alles sich gefallen;", "tokens": ["Von", "Gott", "l\u00e4\u00dft", "man", "sich", "viel", ",", "l\u00e4\u00dft", "al\u00b7les", "sich", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PRF", "ADV", "$,", "VVFIN", "PIS", "PRF", "VVPP", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Warum? man denkt, er machts am besten doch von allen.", "tokens": ["Wa\u00b7rum", "?", "man", "denkt", ",", "er", "machts", "am", "bes\u00b7ten", "doch", "von", "al\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PIS", "VVFIN", "$,", "PPER", "VVFIN", "PTKA", "ADJD", "ADV", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dis schlechte Wetter, Sohn, wer wei\u00df wozu's ist gut?", "tokens": ["Dis", "schlech\u00b7te", "Wet\u00b7ter", ",", "Sohn", ",", "wer", "wei\u00df", "wo\u00b7zu's", "ist", "gut", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "NN", "$,", "PWS", "VVFIN", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir nicht! Gott hats gemacht, und wei\u00df wol was er thut.", "tokens": ["Wir", "nicht", "!", "Gott", "hats", "ge\u00b7macht", ",", "und", "wei\u00df", "wol", "was", "er", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "NN", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "ADV", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "H\u00e4tt' es ein Mensch gemacht, dem w\u00e4r' es schlimm ergangen,", "tokens": ["H\u00e4tt'", "es", "ein", "Mensch", "ge\u00b7macht", ",", "dem", "w\u00e4r'", "es", "schlimm", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVPP", "$,", "PRELS", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch der im Himmel ist nicht h\u00f6her zu belangen.", "tokens": ["Doch", "der", "im", "Him\u00b7mel", "ist", "nicht", "h\u00f6\u00b7her", "zu", "be\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}