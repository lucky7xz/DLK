{"textgrid.poem.63346": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf auf Cuno", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.71", "nl:0.14", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cuno steigt in die Arena.", "tokens": ["Cu\u00b7no", "steigt", "in", "die", "A\u00b7re\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mensch, wie er, so kann es keena.", "tokens": ["Mensch", ",", "wie", "er", ",", "so", "kann", "es", "ke\u00b7e\u00b7na", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "$,", "ADV", "VMFIN", "PPER", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Cuno wird das Tau schon ziehn.", "tokens": ["Cu\u00b7no", "wird", "das", "Tau", "schon", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er drehts Ding nicht \u2013", "tokens": ["Er", "drehts", "Ding", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "'s Dreht ihn.", "tokens": ["'s", "Dreht", "ihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Cuno stemmt mit Pappgewichten.", "tokens": ["Cu\u00b7no", "stemmt", "mit", "Papp\u00b7ge\u00b7wich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Cuno wird die Zwietracht schlichten.", "tokens": ["Cu\u00b7no", "wird", "die", "Zwiet\u00b7racht", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht die Sache noch so schief:", "tokens": ["Geht", "die", "Sa\u00b7che", "noch", "so", "schief", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Cuno ist und bleibt passiv.", "tokens": ["Cu\u00b7no", "ist", "und", "bleibt", "pas\u00b7siv", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Steigt der Dollar in die Puppen:", "tokens": ["Steigt", "der", "Dol\u00b7lar", "in", "die", "Pup\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Cuno'n kann das nicht verschnuppen.", "tokens": ["Cu\u00b7no'n", "kann", "das", "nicht", "ver\u00b7schnup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PDS", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er verschenkt zum Schleuderpreise", "tokens": ["Er", "ver\u00b7schenkt", "zum", "Schleu\u00b7der\u00b7prei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pfund und Dollar scheffelweise.", "tokens": ["Pfund", "und", "Dol\u00b7lar", "schef\u00b7fel\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Cuno, das ist unser Mann.", "tokens": ["Cu\u00b7no", ",", "das", "ist", "un\u00b7ser", "Mann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Cuno regt den Spartrieb an.", "tokens": ["Cu\u00b7no", "regt", "den", "Spar\u00b7trieb", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Arbeit wird ihr Lohn:", "tokens": ["Je\u00b7der", "Ar\u00b7beit", "wird", "ihr", "Lohn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Mark gleich 'ner Million.", "tokens": ["Ei\u00b7ne", "Mark", "gleich", "'ner", "Mil\u00b7li\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Steuernstundung, Markkredite:", "tokens": ["Steu\u00b7ern\u00b7stun\u00b7dung", ",", "Mark\u00b7kre\u00b7di\u00b7te", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles f\u00fcr des Volks Elite.", "tokens": ["Al\u00b7les", "f\u00fcr", "des", "Volks", "E\u00b7li\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stinnes singt von steiler H\u00f6h' in den Alpen:", "tokens": ["Stin\u00b7nes", "singt", "von", "stei\u00b7ler", "H\u00f6h'", "in", "den", "Al\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Safe qui peut.", "tokens": ["Sa\u00b7fe", "qui", "peut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Cuno pirscht auf Nietzsches F\u00e4hrte:", "tokens": ["Cu\u00b7no", "pirscht", "auf", "Nietz\u00b7sches", "F\u00e4hr\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unterwertung aller Werte.", "tokens": ["Un\u00b7ter\u00b7wer\u00b7tung", "al\u00b7ler", "Wer\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Cuno sagt aus Karten wahr.", "tokens": ["Cu\u00b7no", "sagt", "aus", "Kar\u00b7ten", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was er nicht zahlt, zahlt er bar.", "tokens": ["Was", "er", "nicht", "zahlt", ",", "zahlt", "er", "bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Cuno spielt f\u00fcr uns Patience", "tokens": ["Cu\u00b7no", "spielt", "f\u00fcr", "uns", "Pa\u00b7ti\u00b7en\u00b7ce"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPER", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Mit Kanonen, Gas und Tanks.", "tokens": ["Mit", "Ka\u00b7no\u00b7nen", ",", "Gas", "und", "Tanks", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Treibts Poincar\u00e9 idiotisch:", "tokens": ["Treibts", "Poin\u00b7ca\u00b7r\u00e9", "i\u00b7dio\u00b7tisch", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Cuno schafft es mehr auf gotisch.", "tokens": ["Cu\u00b7no", "schafft", "es", "mehr", "auf", "go\u00b7tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Cuno ist f\u00fcr Alles gut,", "tokens": ["Cu\u00b7no", "ist", "f\u00fcr", "Al\u00b7les", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Cuno hebt gesunknen Mut,", "tokens": ["Cu\u00b7no", "hebt", "ge\u00b7sun\u00b7knen", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Senkt die Mark von Etsch bis Belt", "tokens": ["Senkt", "die", "Mark", "von", "Etsch", "bis", "Belt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Alles in der Welt.", "tokens": ["Un\u00b7ter", "Al\u00b7les", "in", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Steigt ins Walhall deutscher Geister", "tokens": ["Steigt", "ins", "Wal\u00b7hall", "deut\u00b7scher", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Cuno jetzt, der W\u00e4hrungsmeister \u2013", "tokens": ["Cu\u00b7no", "jetzt", ",", "der", "W\u00e4h\u00b7rungs\u00b7meis\u00b7ter", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "$("], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "La\u00dft's nicht zur Verzweiflung treiben:", "tokens": ["La\u00dft's", "nicht", "zur", "Ver\u00b7zwei\u00b7flung", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Helfferich, er wird uns bleiben!", "tokens": ["Helf\u00b7fe\u00b7rich", ",", "er", "wird", "uns", "blei\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}