{"textgrid.poem.56820": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Nebel ums Haus", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Dunstgew\u00f6lb, wie ich noch keines sah!", "tokens": ["Ein", "Dunst\u00b7ge\u00b7w\u00f6lb", ",", "wie", "ich", "noch", "kei\u00b7nes", "sah", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durchbleicht von au\u00dfen von des Vollmonds Schein!", "tokens": ["Durch\u00b7bleicht", "von", "au\u00b7\u00dfen", "von", "des", "Voll\u00b7monds", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf kleiner Insel d\u00fcnk' ich mich allein \u2013", "tokens": ["Auf", "klei\u00b7ner", "In\u00b7sel", "d\u00fcnk'", "ich", "mich", "al\u00b7lein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bin ich Napoleon auf St. Helena? ...", "tokens": ["bin", "ich", "Na\u00b7po\u00b7le\u00b7on", "auf", "St.", "He\u00b7le\u00b7na", "?", "..."], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NE", "APPR", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der nahe Bach gibt lauter Brandung Ton ...", "tokens": ["Der", "na\u00b7he", "Bach", "gibt", "lau\u00b7ter", "Bran\u00b7dung", "Ton", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durchs Tannicht schimmert's hell \u2013 wie meilenweit ...", "tokens": ["Durchs", "Tan\u00b7nicht", "schim\u00b7mert's", "hell", "\u2013", "wie", "mei\u00b7len\u00b7weit", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$(", "PWAV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich br\u00fct' in ungeheure Einsamkeit ...", "tokens": ["Ich", "br\u00fct'", "in", "un\u00b7ge\u00b7heu\u00b7re", "Ein\u00b7sam\u00b7keit", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nach Englands K\u00fcste kehrt Bellerophon.", "tokens": ["Nach", "En\u00b7glands", "K\u00fcs\u00b7te", "kehrt", "Bel\u00b7le\u00b7ro\u00b7phon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}