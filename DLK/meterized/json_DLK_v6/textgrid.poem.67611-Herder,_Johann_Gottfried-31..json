{"textgrid.poem.67611": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach K\u00f6rnern scharrt' der Hahn", "tokens": ["Nach", "K\u00f6r\u00b7nern", "scharrt'", "der", "Hahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und fand ein Perlein. Sah es an:", "tokens": ["Und", "fand", "ein", "Per\u00b7lein", ".", "Sah", "es", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbwas soll ich nun mit Dir?", "tokens": ["\u00bb", "was", "soll", "ich", "nun", "mit", "Dir", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Tr\u00e4berkorn w\u00e4r' n\u00fctzer mir!\u00ab", "tokens": ["Ein", "Tr\u00e4\u00b7ber\u00b7korn", "w\u00e4r'", "n\u00fct\u00b7zer", "mir", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbein trefflich Buch! dem Autor Heil und Gru\u00df!", "tokens": ["\u00bb", "ein", "treff\u00b7lich", "Buch", "!", "dem", "Au\u00b7tor", "Heil", "und", "Gru\u00df", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "$.", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch \u2013 leider, da\u00df ich's sagen mu\u00df! \u2013", "tokens": ["Doch", "\u2013", "lei\u00b7der", ",", "da\u00df", "ich's", "sa\u00b7gen", "mu\u00df", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "ADV", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr uns Factoren,", "tokens": ["F\u00fcr", "uns", "Fac\u00b7to\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Des heil'gen R\u00f6merreichs Papier-Imperatoren", "tokens": ["Des", "heil'\u00b7gen", "R\u00f6\u00b7mer\u00b7reichs", "Pa\u00b7pier\u00b7Im\u00b7pe\u00b7ra\u00b7to\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von wenigem Genu\u00df.", "tokens": ["Von", "we\u00b7ni\u00b7gem", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein L\u00e4ufer der! \u2013", "tokens": ["Ein", "L\u00e4u\u00b7fer", "der", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "$.", "$("], "meter": "-+--", "measure": "dactylic.init"}, "line.7": {"text": "Schreib' Er mir einen H\u00fcbner, Herr!", "tokens": ["Schreib'", "Er", "mir", "ei\u00b7nen", "H\u00fcb\u00b7ner", ",", "Herr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Muzelius! ein kritisches Journal!\u00ab", "tokens": ["Mu\u00b7ze\u00b7lius", "!", "ein", "kri\u00b7ti\u00b7sches", "Jour\u00b7nal", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sie sangen All' in hoher Zahl:", "tokens": ["Sie", "san\u00b7gen", "All'", "in", "ho\u00b7her", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbmuzelius! Muzelius!", "tokens": ["\u00bb", "mu\u00b7ze\u00b7lius", "!", "Mu\u00b7ze\u00b7lius", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "FM.la", "$.", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Journal! Journal! Journal!\u00ab", "tokens": ["Jour\u00b7nal", "!", "Jour\u00b7nal", "!", "Jour\u00b7nal", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}