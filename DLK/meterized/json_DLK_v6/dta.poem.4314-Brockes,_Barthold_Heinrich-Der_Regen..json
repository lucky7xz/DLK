{"dta.poem.4314": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Regen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann aber ein getriebner Luft-Strohm zwo Wolken an", "tokens": ["Wann", "a\u00b7ber", "ein", "ge\u00b7trieb\u00b7ner", "Luft\u00b7Strohm", "zwo", "Wol\u00b7ken", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "CARD", "NN", "PTKVZ"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "einander dr\u00e4ngt;", "tokens": ["ein\u00b7an\u00b7der", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So werden viele von den Bl\u00e4sgen, durch diesen starken", "tokens": ["So", "wer\u00b7den", "vie\u00b7le", "von", "den", "Bl\u00e4s\u00b7gen", ",", "durch", "die\u00b7sen", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "ART", "NN", "$,", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drang, zersprengt.", "tokens": ["Drang", ",", "zer\u00b7sprengt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das Wasser, woraus sie bestunden, flie\u00dft \u00fcberall und", "tokens": ["Das", "Was\u00b7ser", ",", "wo\u00b7raus", "sie", "be\u00b7stun\u00b7den", ",", "flie\u00dft", "\u00fc\u00b7be\u00b7rall", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,", "VVFIN", "ADV", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "sammlet sich", "tokens": ["samm\u00b7let", "sich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "In kleinen oder grossen Tropfen, nachdem der Wind un-", "tokens": ["In", "klei\u00b7nen", "o\u00b7der", "gros\u00b7sen", "Trop\u00b7fen", ",", "nach\u00b7dem", "der", "Wind", "un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "ordentlich", "tokens": ["or\u00b7dent\u00b7lich"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+--", "measure": "dactylic.init"}, "line.9": {"text": "Sie abwerts st\u00fcrzt und durch sie dringet. Sie werden", "tokens": ["Sie", "ab\u00b7werts", "st\u00fcrzt", "und", "durch", "sie", "drin\u00b7get", ".", "Sie", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "KON", "APPR", "PPER", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "mehr und mehr vereint,", "tokens": ["mehr", "und", "mehr", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und gr\u00f6sser, wenn sie lange fallen, wie wir dieselbigen", "tokens": ["Und", "gr\u00f6s\u00b7ser", ",", "wenn", "sie", "lan\u00b7ge", "fal\u00b7len", ",", "wie", "wir", "die\u00b7sel\u00b7bi\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,", "PWAV", "PPER", "PDS"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "hingegen,", "tokens": ["hin\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Wenn Wolken nah und niedrig h\u00e4ngen, zuweilen unge-", "tokens": ["Wenn", "Wol\u00b7ken", "nah", "und", "nied\u00b7rig", "h\u00e4n\u00b7gen", ",", "zu\u00b7wei\u00b7len", "un\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "KON", "ADJD", "VVINF", "$,", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "mein verkleint,", "tokens": ["mein", "ver\u00b7kleint", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Und \u00f6fters fast wie Staub befinden. Je h\u00f6her Wolk, je", "tokens": ["Und", "\u00f6f\u00b7ters", "fast", "wie", "Staub", "be\u00b7fin\u00b7den", ".", "Je", "h\u00f6\u00b7her", "Wolk", ",", "je"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ADV", "KOKOM", "NN", "VVFIN", "$.", "ADV", "ADJD", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+--++", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "gr\u00f6sser Regen.", "tokens": ["gr\u00f6s\u00b7ser", "Re\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}