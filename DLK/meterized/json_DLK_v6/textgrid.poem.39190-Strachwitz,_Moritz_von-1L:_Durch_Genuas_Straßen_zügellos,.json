{"textgrid.poem.39190": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch Genuas Stra\u00dfen z\u00fcgellos,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Genuas Stra\u00dfen z\u00fcgellos,", "tokens": ["Durch", "Ge\u00b7nu\u00b7as", "Stra\u00b7\u00dfen", "z\u00fc\u00b7gel\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da tummelt sich das Verderben,", "tokens": ["Da", "tum\u00b7melt", "sich", "das", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Schwert ist blos, die Rache gro\u00df:", "tokens": ["Das", "Schwert", "ist", "blos", ",", "die", "Ra\u00b7che", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Doria, der mu\u00df sterben!", "tokens": ["Der", "Do\u00b7ria", ",", "der", "mu\u00df", "ster\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie st\u00fcrmen heran, sie suchen den Greis,", "tokens": ["Sie", "st\u00fcr\u00b7men", "he\u00b7ran", ",", "sie", "su\u00b7chen", "den", "Greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie k\u00f6nnen zu ihm nicht dringen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "zu", "ihm", "nicht", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den Alten schirmt ein Zauberkreis,", "tokens": ["Den", "Al\u00b7ten", "schirmt", "ein", "Zau\u00b7ber\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Kreis von deutschen Klingen.", "tokens": ["Ein", "Kreis", "von", "deut\u00b7schen", "Klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der eherne Kn\u00e4ul, er schreitet stumm", "tokens": ["Der", "e\u00b7her\u00b7ne", "Kn\u00e4ul", ",", "er", "schrei\u00b7tet", "stumm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im schwerhindr\u00f6hnenden Takte;", "tokens": ["Im", "schwer\u00b7hind\u00b7r\u00f6h\u00b7nen\u00b7den", "Tak\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Um des Herzogs Brust, als Wall rundum,", "tokens": ["Um", "des", "Her\u00b7zogs", "Brust", ",", "als", "Wall", "run\u00b7dum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,", "KOUS", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da leuchtet das Schwert, das nackte.", "tokens": ["Da", "leuch\u00b7tet", "das", "Schwert", ",", "das", "nack\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Es stutzt die italische Mordbegier", "tokens": ["Es", "stutzt", "die", "i\u00b7ta\u00b7li\u00b7sche", "Mord\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Im wildesten Rachetriebe;", "tokens": ["Im", "wil\u00b7des\u00b7ten", "Ra\u00b7che\u00b7trie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Die Frage schallt: Was gibt es hier?", "tokens": ["Die", "Fra\u00b7ge", "schallt", ":", "Was", "gibt", "es", "hier", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Antwort: Deutsche Hiebe!", "tokens": ["Die", "Ant\u00b7wort", ":", "Deut\u00b7sche", "Hie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So standen die Deutschen ehrenhaft", "tokens": ["So", "stan\u00b7den", "die", "Deut\u00b7schen", "eh\u00b7ren\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr andere im Gefechte,", "tokens": ["F\u00fcr", "an\u00b7de\u00b7re", "im", "Ge\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So focht die deutsche B\u00e4renkraft", "tokens": ["So", "focht", "die", "deut\u00b7sche", "B\u00e4\u00b7ren\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr fremder Herren Rechte.", "tokens": ["F\u00fcr", "frem\u00b7der", "Her\u00b7ren", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch zupften sie Dir am eignen Gewand,", "tokens": ["Doch", "zupf\u00b7ten", "sie", "Dir", "am", "eig\u00b7nen", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die fremden Diebeskrallen,", "tokens": ["Die", "frem\u00b7den", "Die\u00b7bes\u00b7kral\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Vaterland, mein Vaterland,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "mein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da lie\u00dfest Du Dir's gefallen!", "tokens": ["Da", "lie\u00b7\u00dfest", "Du", "Dir's", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mein Vaterland, lieb' Vaterland,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "lieb'", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Dich die Welschen dr\u00e4ngen,", "tokens": ["Wenn", "Dich", "die", "Wel\u00b7schen", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn des Reu\u00dfen freche Hand", "tokens": ["Und", "wenn", "des", "Reu\u00b7\u00dfen", "fre\u00b7che", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dir Deinen Brei will mengen;", "tokens": ["Dir", "Dei\u00b7nen", "Brei", "will", "men\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann auf die Finger unverzagt", "tokens": ["Dann", "auf", "die", "Fin\u00b7ger", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Klopf' ihn Dir selbst zu Liebe,", "tokens": ["Klopf'", "ihn", "Dir", "selbst", "zu", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wenn er schreiend: Was gibt's denn? fragt,", "tokens": ["Und", "wenn", "er", "schrei\u00b7end", ":", "Was", "gibt's", "denn", "?", "fragt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "$.", "PWS", "VVFIN", "ADV", "$.", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "So sage: Deutsche Hiebe!", "tokens": ["So", "sa\u00b7ge", ":", "Deut\u00b7sche", "Hie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}