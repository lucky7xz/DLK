{"textgrid.poem.67592": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein junger Sklave war zuerst auf wilder See", "tokens": ["Ein", "jun\u00b7ger", "Skla\u00b7ve", "war", "zu\u00b7erst", "auf", "wil\u00b7der", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schrie und bebt' und wimmerte.", "tokens": ["Und", "schrie", "und", "bebt'", "und", "wim\u00b7mer\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsteckt,\u00ab sprach der K\u00f6nig, \u00bbihn ins nasse Wellenhaus", "tokens": ["\u00bb", "steckt", ",", "\u00ab", "sprach", "der", "K\u00f6\u00b7nig", ",", "\u00bb", "ihn", "ins", "nas\u00b7se", "Wel\u00b7len\u00b7haus"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zieht ihn schnell heraus!\u00ab", "tokens": ["Und", "zieht", "ihn", "schnell", "he\u00b7raus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie thaten's. Stracks war all sein Wimmern aus;", "tokens": ["Sie", "tha\u00b7ten'", "s.", "Stracks", "war", "all", "sein", "Wim\u00b7mern", "aus", ";"], "token_info": ["word", "word", "abbreviation", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VAFIN", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sehr wohl bekam ihm nun sein trocknes Bretterhaus.", "tokens": ["Sehr", "wohl", "be\u00b7kam", "ihm", "nun", "sein", "trock\u00b7nes", "Bret\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer nie war krank,", "tokens": ["Wer", "nie", "war", "krank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wei\u00df kaum f\u00fcr sein Gesundsein Dank.", "tokens": ["Wei\u00df", "kaum", "f\u00fcr", "sein", "Ge\u00b7sun\u00b7dsein", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Hungrigen ist Wermuth s\u00fc\u00df;", "tokens": ["Dem", "Hung\u00b7ri\u00b7gen", "ist", "Wer\u00b7muth", "s\u00fc\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der H\u00f6lle d\u00fcnkt die Erde Paradies;", "tokens": ["Der", "H\u00f6l\u00b7le", "d\u00fcnkt", "die", "Er\u00b7de", "Pa\u00b7ra\u00b7dies", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Himmel d\u00fcnkt die Erde H\u00f6lle;", "tokens": ["Dem", "Him\u00b7mel", "d\u00fcnkt", "die", "Er\u00b7de", "H\u00f6l\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Satten wird der Honig Ekels Quelle.", "tokens": ["Dem", "Sat\u00b7ten", "wird", "der", "Ho\u00b7nig", "E\u00b7kels", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}