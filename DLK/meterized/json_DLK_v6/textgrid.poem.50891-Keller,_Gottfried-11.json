{"textgrid.poem.50891": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "11", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich an deiner Fr\u00fchlingsbrust zwiefachem Himmel geruht,", "tokens": ["Als", "ich", "an", "dei\u00b7ner", "Fr\u00fch\u00b7lings\u00b7brust", "zwie\u00b7fa\u00b7chem", "Him\u00b7mel", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.2": {"text": "In k\u00f6niglicher Ruhe stolz hinwogte unser Blut:", "tokens": ["In", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Ru\u00b7he", "stolz", "hin\u00b7wog\u00b7te", "un\u00b7ser", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und schaute in den Hesperus mit frohem, stillem Mut.", "tokens": ["Und", "schau\u00b7te", "in", "den", "Hes\u00b7pe\u00b7rus", "mit", "fro\u00b7hem", ",", "stil\u00b7lem", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NE", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Dann dr\u00fcckte m\u00fcd die Augen ich an deinem Busen zu,", "tokens": ["Dann", "dr\u00fcck\u00b7te", "m\u00fcd", "die", "Au\u00b7gen", "ich", "an", "dei\u00b7nem", "Bu\u00b7sen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Doch immerfort sah ich den Stern in seiner sch\u00f6nen Glut.", "tokens": ["Doch", "im\u00b7mer\u00b7fort", "sah", "ich", "den", "Stern", "in", "sei\u00b7ner", "sch\u00f6\u00b7nen", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Er ging in deinem Herzen auf, wie es der Widerschein", "tokens": ["Er", "ging", "in", "dei\u00b7nem", "Her\u00b7zen", "auf", ",", "wie", "es", "der", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Lunas in einem spiegelnden und tiefen Brunnen tut.", "tokens": ["Lu\u00b7nas", "in", "ei\u00b7nem", "spie\u00b7geln\u00b7den", "und", "tie\u00b7fen", "Brun\u00b7nen", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}