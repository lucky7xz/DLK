{"dta.poem.6227": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns auf gleiche Art, dergleichen Licht zu kriegen,", "tokens": ["Lasst", "uns", "auf", "glei\u00b7che", "Art", ",", "derg\u00b7lei\u00b7chen", "Licht", "zu", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "PIS", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anitzt beflissen seyn, um gleichfalls zu verstehn", "tokens": ["A\u00b7nitzt", "be\u00b7flis\u00b7sen", "seyn", ",", "um", "gleich\u00b7falls", "zu", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAINF", "$,", "KOUI", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Dinge, die uns hier vor Augen liegen,", "tokens": ["Die", "Din\u00b7ge", ",", "die", "uns", "hier", "vor", "Au\u00b7gen", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um auch dieselben zu empfinden,", "tokens": ["Um", "auch", "die\u00b7sel\u00b7ben", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PDAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ihre Z\u00fcg\u2019, ihr Wesen zu ergr\u00fcnden.", "tokens": ["Und", "ih\u00b7re", "Z\u00fcg'", ",", "ihr", "We\u00b7sen", "zu", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}