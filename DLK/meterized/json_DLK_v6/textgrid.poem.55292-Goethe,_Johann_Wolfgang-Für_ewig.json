{"textgrid.poem.55292": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr ewig", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denn was der Mensch in seinen Erdeschranken", "tokens": ["Denn", "was", "der", "Mensch", "in", "sei\u00b7nen", "Er\u00b7de\u00b7schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von hohem Gl\u00fcck mit G\u00f6tternamen nennt,", "tokens": ["Von", "ho\u00b7hem", "Gl\u00fcck", "mit", "G\u00f6t\u00b7ter\u00b7na\u00b7men", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Harmonie der Treue, die kein Wanken,", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "der", "Treu\u00b7e", ",", "die", "kein", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freundschaft, die nicht Zweifelsorge kennt;", "tokens": ["Der", "Freund\u00b7schaft", ",", "die", "nicht", "Zwei\u00b7fel\u00b7sor\u00b7ge", "kennt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Licht, das Weisen nur zu einsamen Gedanken,", "tokens": ["Das", "Licht", ",", "das", "Wei\u00b7sen", "nur", "zu", "ein\u00b7sa\u00b7men", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Dichtern nur in sch\u00f6nen Bildern brennt,", "tokens": ["Das", "Dich\u00b7tern", "nur", "in", "sch\u00f6\u00b7nen", "Bil\u00b7dern", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das hatt ich all in meinen besten Stunden", "tokens": ["Das", "hatt", "ich", "all", "in", "mei\u00b7nen", "bes\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PIAT", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In ihr entdeckt und es f\u00fcr mich gefunden.", "tokens": ["In", "ihr", "ent\u00b7deckt", "und", "es", "f\u00fcr", "mich", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVPP", "KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}