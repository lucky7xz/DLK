{"textgrid.poem.35317": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ich bin und was ich habe,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich bin und was ich habe,", "tokens": ["Was", "ich", "bin", "und", "was", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebste! dank ich dir allein;", "tokens": ["Liebs\u00b7te", "!", "dank", "ich", "dir", "al\u00b7lein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne dich wo w\u00fcrd' ich sein?", "tokens": ["Oh\u00b7ne", "dich", "wo", "w\u00fcrd'", "ich", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PWAV", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nirgends als im kalten Grabe.", "tokens": ["Nir\u00b7gends", "als", "im", "kal\u00b7ten", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch du hast mich gut gepflegt", "tokens": ["Doch", "du", "hast", "mich", "gut", "ge\u00b7pflegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und in St\u00fcrmen und Gefahren", "tokens": ["Und", "in", "St\u00fcr\u00b7men", "und", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Tr\u00f6stend, schon seit manchen Jahren,", "tokens": ["Tr\u00f6s\u00b7tend", ",", "schon", "seit", "man\u00b7chen", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Dich an meine Brust gelegt.", "tokens": ["Dich", "an", "mei\u00b7ne", "Brust", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "B\u00f6sen Kummer, schwarze Grillen,", "tokens": ["B\u00f6\u00b7sen", "Kum\u00b7mer", ",", "schwar\u00b7ze", "Gril\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weggeplaudert hast du sie.", "tokens": ["Weg\u00b7ge\u00b7plau\u00b7dert", "hast", "du", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich betr\u00fcben mocht' ich nie;", "tokens": ["Dich", "be\u00b7tr\u00fc\u00b7ben", "mocht'", "ich", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer that ich deinen Willen,", "tokens": ["Im\u00b7mer", "that", "ich", "dei\u00b7nen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wie wenig meiner Art", "tokens": ["Und", "wie", "we\u00b7nig", "mei\u00b7ner", "Art"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kinderlaunen sonst behagen,", "tokens": ["Kin\u00b7der\u00b7lau\u00b7nen", "sonst", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dir verzeih' ich alle Plagen,", "tokens": ["Dir", "ver\u00b7zeih'", "ich", "al\u00b7le", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die selbst du mir nicht erspart.", "tokens": ["Die", "selbst", "du", "mir", "nicht", "er\u00b7spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es kommt mir sehr zu statten,", "tokens": ["Und", "es", "kommt", "mir", "sehr", "zu", "stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du keine Dame bist", "tokens": ["Da\u00df", "du", "kei\u00b7ne", "Da\u00b7me", "bist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Voller Trug und Hinterlist,", "tokens": ["Vol\u00b7ler", "Trug", "und", "Hin\u00b7ter\u00b7list", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Keiner Schwiegermutter Schatten,", "tokens": ["Kei\u00b7ner", "Schwie\u00b7ger\u00b7mut\u00b7ter", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Puppe, steif und stolz,", "tokens": ["Kei\u00b7ne", "Pup\u00b7pe", ",", "steif", "und", "stolz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich verwundert und betroffen,", "tokens": ["Gleich", "ver\u00b7wun\u00b7dert", "und", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Angethan mit schweren Stoffen,", "tokens": ["An\u00b7ge\u00b7than", "mit", "schwe\u00b7ren", "Stof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und darunter leichtes Holz.", "tokens": ["Und", "da\u00b7run\u00b7ter", "leich\u00b7tes", "Holz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Nein! Von wohlgeratnem Gusse", "tokens": ["Nein", "!", "Von", "wohl\u00b7ge\u00b7rat\u00b7nem", "Gus\u00b7se"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du, fein und zierlich zwar,", "tokens": ["Bist", "du", ",", "fein", "und", "zier\u00b7lich", "zwar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich's liebe, ganz und gar;", "tokens": ["Wie", "ich's", "lie\u00b7be", ",", "ganz", "und", "gar", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber von des Ungl\u00fccks Kusse", "tokens": ["A\u00b7ber", "von", "des", "Un\u00b7gl\u00fccks", "Kus\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blieben deine Lippen bleich.", "tokens": ["Blie\u00b7ben", "dei\u00b7ne", "Lip\u00b7pen", "bleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deshalb nenn' ich dich die Meine,", "tokens": ["Des\u00b7halb", "nenn'", "ich", "dich", "die", "Mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dein Herz, du arme Kleine,", "tokens": ["Und", "dein", "Herz", ",", "du", "ar\u00b7me", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Macht mich unerme\u00dflich reich.", "tokens": ["Macht", "mich", "un\u00b7er\u00b7me\u00df\u00b7lich", "reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zitternd kamst du hergeflogen,", "tokens": ["Zit\u00b7ternd", "kamst", "du", "her\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Klausner hielt dich fest;", "tokens": ["Und", "der", "Klaus\u00b7ner", "hielt", "dich", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seines Strebens ganzen Rest", "tokens": ["Sei\u00b7nes", "Stre\u00b7bens", "gan\u00b7zen", "Rest"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat dein L\u00e4cheln aufgewogen;", "tokens": ["Hat", "dein", "L\u00e4\u00b7cheln", "auf\u00b7ge\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kind und Gattin bist du mir \u2013", "tokens": ["Kind", "und", "Gat\u00b7tin", "bist", "du", "mir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle Dichter schw\u00e4rmen gerne;", "tokens": ["Al\u00b7le", "Dich\u00b7ter", "schw\u00e4r\u00b7men", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber selbst aus dunkler Ferne", "tokens": ["A\u00b7ber", "selbst", "aus", "dunk\u00b7ler", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kam ich wieder heim zu dir.", "tokens": ["Kam", "ich", "wie\u00b7der", "heim", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00e4tt' ich jene Riesenfeder,", "tokens": ["H\u00e4tt'", "ich", "je\u00b7ne", "Rie\u00b7sen\u00b7fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Aetnas Schlund getaucht,", "tokens": ["In", "des", "A\u00b7et\u00b7nas", "Schlund", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ein andrer schon gebraucht,", "tokens": ["Die", "ein", "an\u00b7drer", "schon", "ge\u00b7braucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lesen m\u00fc\u00dfte bald ein jeder,", "tokens": ["Le\u00b7sen", "m\u00fc\u00df\u00b7te", "bald", "ein", "je\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "ART", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich mit gewalt'ger Hand", "tokens": ["Was", "ich", "mit", "ge\u00b7walt'\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An die Himmelsdecke schriebe", "tokens": ["An", "die", "Him\u00b7mels\u00b7de\u00b7cke", "schrie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der zu Ehren, die ich liebe,", "tokens": ["Der", "zu", "Eh\u00b7ren", ",", "die", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deren Herz das meine fand.", "tokens": ["De\u00b7ren", "Herz", "das", "mei\u00b7ne", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch wie konnt' ich daran denken,", "tokens": ["Doch", "wie", "konnt'", "ich", "da\u00b7ran", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich, ein sonst vern\u00fcnft'ger Mann,", "tokens": ["Ich", ",", "ein", "sonst", "ver\u00b7n\u00fcnft'\u00b7ger", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Was sie gar nicht lesen kann,", "tokens": ["Was", "sie", "gar", "nicht", "le\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutsche Verse ihr zu schenken?", "tokens": ["Deut\u00b7sche", "Ver\u00b7se", "ihr", "zu", "schen\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! ich habe jederzeit", "tokens": ["Ach", "!", "ich", "ha\u00b7be", "je\u00b7der\u00b7zeit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das nur niederschreiben wollen,", "tokens": ["Das", "nur", "nie\u00b7der\u00b7schrei\u00b7ben", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was dem Herzen mir entquollen,", "tokens": ["Was", "dem", "Her\u00b7zen", "mir", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht der Dichtereitelkeit.", "tokens": ["Nicht", "der", "Dich\u00b7te\u00b7rei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df denn die Poetengabe", "tokens": ["La\u00df", "denn", "die", "Poe\u00b7ten\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Diesmal dir willkommen sein;", "tokens": ["Dies\u00b7mal", "dir", "will\u00b7kom\u00b7men", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich hier f\u00fcr dich allein", "tokens": ["Was", "ich", "hier", "f\u00fcr", "dich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcchtern eingeschaltet habe,", "tokens": ["Sch\u00fcch\u00b7tern", "ein\u00b7ge\u00b7schal\u00b7tet", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist nur deshalb ein Gedicht,", "tokens": ["Ist", "nur", "des\u00b7halb", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Weil ich nicht genug gepriesen", "tokens": ["Weil", "ich", "nicht", "ge\u00b7nug", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was du Liebes mir erwiesen,", "tokens": ["Was", "du", "Lie\u00b7bes", "mir", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber das \u2013 das glaubst du nicht.", "tokens": ["A\u00b7ber", "das", "\u2013", "das", "glaubst", "du", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}