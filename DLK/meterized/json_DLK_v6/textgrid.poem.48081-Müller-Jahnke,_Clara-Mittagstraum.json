{"textgrid.poem.48081": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Mittagstraum", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sengend \u00fcber den Feldern", "tokens": ["Sen\u00b7gend", "\u00fc\u00b7ber", "den", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "br\u00fctet die Juliglut,", "tokens": ["br\u00fc\u00b7tet", "die", "Ju\u00b7lig\u00b7lut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "der Weizen reift im Brande,", "tokens": ["der", "Wei\u00b7zen", "reift", "im", "Bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und alles Leben ruht.", "tokens": ["und", "al\u00b7les", "Le\u00b7ben", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Grashalm wogt im Winde,", "tokens": ["Kein", "Gras\u00b7halm", "wogt", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "kein Vogel singt im Baum;", "tokens": ["kein", "Vo\u00b7gel", "singt", "im", "Baum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "durch meine Seele flutet", "tokens": ["durch", "mei\u00b7ne", "See\u00b7le", "flu\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein goldiger Mittagstraum.", "tokens": ["ein", "gol\u00b7di\u00b7ger", "Mit\u00b7tags\u00b7traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Tief in zitternde Aehren", "tokens": ["Tief", "in", "zit\u00b7tern\u00b7de", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "bin ich der Welt entflohn,", "tokens": ["bin", "ich", "der", "Welt", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "gro\u00dfblumig mir zu H\u00e4upten", "tokens": ["gro\u00df\u00b7blu\u00b7mig", "mir", "zu", "H\u00e4up\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "bl\u00fcht der rote Mohn . . .", "tokens": ["bl\u00fcht", "der", "ro\u00b7te", "Mohn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Er kr\u00e4nzt mir Haar und Stirne", "tokens": ["Er", "kr\u00e4nzt", "mir", "Haar", "und", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit flammendem Geschmeid, \u2013 \u2013", "tokens": ["mit", "flam\u00b7men\u00b7dem", "Ge\u00b7schmeid", ",", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "all meine W\u00fcnsche reifen", "tokens": ["all", "mei\u00b7ne", "W\u00fcn\u00b7sche", "rei\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der gro\u00dfen Erntezeit.", "tokens": ["der", "gro\u00b7\u00dfen", "Ern\u00b7te\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}