{"dta.poem.3318": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Baron von C**.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Du, der des Adels Glanz mit schim\u0303erndem Verstande,", "tokens": ["Du", ",", "der", "des", "A\u00b7dels", "Glanz", "mit", "schim\u0303ern\u00b7dem", "Ver\u00b7stan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Musen und Geschmack vereint,", "tokens": ["Mit", "Mu\u00b7sen", "und", "Ge\u00b7schmack", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entreisse dich, o C**! edler Freund!", "tokens": ["E\u00b7ntreis\u00b7se", "dich", ",", "o", "C", "*", "*", "!", "ed\u00b7ler", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "XY", "XY", "XY", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Pleisse liederreichem Strande.", "tokens": ["Der", "Pleis\u00b7se", "lie\u00b7der\u00b7rei\u00b7chem", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In jener hohen Burg, wo Ephe\u00fc an den Mauern", "tokens": ["In", "je\u00b7ner", "ho\u00b7hen", "Burg", ",", "wo", "E\u00b7phe\u00fc", "an", "den", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PWAV", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein dauernd Gr\u00fcn dir aufbewahrt,", "tokens": ["Sein", "dau\u00b7ernd", "Gr\u00fcn", "dir", "auf\u00b7be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erwarten dich nur Freuden \u00e4chter Art,", "tokens": ["Er\u00b7war\u00b7ten", "dich", "nur", "Freu\u00b7den", "\u00e4ch\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die nie vergr\u00fcnen, immer dauern.", "tokens": ["Die", "nie", "ver\u00b7gr\u00fc\u00b7nen", ",", "im\u00b7mer", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier mahle die Natur, die nun, vom Lenz umkr\u00e4nzet,", "tokens": ["Hier", "mah\u00b7le", "die", "Na\u00b7tur", ",", "die", "nun", ",", "vom", "Lenz", "um\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In iedem Auftritt hier entz\u00fcckt,", "tokens": ["In", "ie\u00b7dem", "Auf\u00b7tritt", "hier", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ungeschminkt, nur landhaft aufgeschm\u00fcckt,", "tokens": ["Und", "un\u00b7ge\u00b7schminkt", ",", "nur", "land\u00b7haft", "auf\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch in verschiednem Schmucke, gl\u00e4nzet.", "tokens": ["Doch", "in", "ver\u00b7schied\u00b7nem", "Schmu\u00b7cke", ",", "gl\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Welch liebliches Gemisch von sonnenreichen H\u00f6hen", "tokens": ["Welch", "lieb\u00b7li\u00b7ches", "Ge\u00b7misch", "von", "son\u00b7nen\u00b7rei\u00b7chen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rauhbeb\u00fcschter Th\u00e4ler Nacht,", "tokens": ["Und", "rauh\u00b7be\u00b7b\u00fcschter", "Th\u00e4\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und gr\u00fcner Saat und junger Bluhmen Pracht", "tokens": ["Und", "gr\u00fc\u00b7ner", "Saat", "und", "jun\u00b7ger", "Bluh\u00b7men", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und B\u00e4chen und bestrahlten Seen!", "tokens": ["Und", "B\u00e4\u00b7chen", "und", "be\u00b7strahl\u00b7ten", "Seen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Aug ist unbeschr\u00e4nkt, die freyen Blicke fliegen", "tokens": ["Das", "Aug", "ist", "un\u00b7be\u00b7schr\u00e4nkt", ",", "die", "frey\u00b7en", "Bli\u00b7cke", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoch \u00fcber furchtbarn W\u00e4ldern hin,", "tokens": ["Hoch", "\u00fc\u00b7ber", "furcht\u00b7barn", "W\u00e4l\u00b7dern", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehn erstaunt, mit angespanntem Sinn,", "tokens": ["Und", "sehn", "er\u00b7staunt", ",", "mit", "an\u00b7ge\u00b7spann\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch zwanzig St\u00e4dte duftig liegen.", "tokens": ["Noch", "zwan\u00b7zig", "St\u00e4d\u00b7te", "duf\u00b7tig", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O Aufenthalt der Lust f\u00fcr unverw\u00f6hnte Weisen!", "tokens": ["O", "Auf\u00b7ent\u00b7halt", "der", "Lust", "f\u00fcr", "un\u00b7ver\u00b7w\u00f6hn\u00b7te", "Wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Musen liebster Aufenthalt,", "tokens": ["Der", "Mu\u00b7sen", "liebs\u00b7ter", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo aus der Flur der Lerchen Lied erschallt,", "tokens": ["Wo", "aus", "der", "Flur", "der", "Ler\u00b7chen", "Lied", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die ihre Sch\u00f6pfung fr\u00f6hlig preisen!", "tokens": ["Die", "ih\u00b7re", "Sch\u00f6p\u00b7fung", "fr\u00f6h\u00b7lig", "prei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die g\u00fctige Natur verlangt nicht unsre Plagen:", "tokens": ["Die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", "ver\u00b7langt", "nicht", "uns\u00b7re", "Pla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O ruhten wir an ihrer Brust,", "tokens": ["O", "ruh\u00b7ten", "wir", "an", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und liessen ihr die Wahl der bessern Lust:", "tokens": ["Und", "lies\u00b7sen", "ihr", "die", "Wahl", "der", "bes\u00b7sern", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie heiter fl\u00f6ssen unsre Tage!", "tokens": ["Wie", "hei\u00b7ter", "fl\u00f6s\u00b7sen", "uns\u00b7re", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Freude, welche sie mit milder Hand bereitet,", "tokens": ["Die", "Freu\u00b7de", ",", "wel\u00b7che", "sie", "mit", "mil\u00b7der", "Hand", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Reizt ungekauft, erm\u00fcdet nicht,", "tokens": ["Reizt", "un\u00b7ge\u00b7kauft", ",", "er\u00b7m\u00fc\u00b7det", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist ruhig, rein, sanft, wie das Morgenlicht,", "tokens": ["Ist", "ru\u00b7hig", ",", "rein", ",", "sanft", ",", "wie", "das", "Mor\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das \u00fcber frische Rosen gleitet.", "tokens": ["Das", "\u00fc\u00b7ber", "fri\u00b7sche", "Ro\u00b7sen", "glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Quellen wahrer Lust stehn allen Menschen offen:", "tokens": ["Die", "Quel\u00b7len", "wah\u00b7rer", "Lust", "stehn", "al\u00b7len", "Men\u00b7schen", "of\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergn\u00fcgungen der Phantasie,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gun\u00b7gen", "der", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Euch kaufen wir mit unvergoltner M\u00fch:", "tokens": ["Euch", "kau\u00b7fen", "wir", "mit", "un\u00b7ver\u00b7golt\u00b7ner", "M\u00fch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ta\u00fcscht ihr unser schmachtend Hoffen!", "tokens": ["Wie", "ta\u00fcscht", "ihr", "un\u00b7ser", "schmach\u00b7tend", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Pracht, Hoheit, Ruhm, die ihr vom Wahn geschm\u00fc-", "tokens": ["Pracht", ",", "Ho\u00b7heit", ",", "Ruhm", ",", "die", "ihr", "vom", "Wahn", "ge\u00b7schm\u00fc"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "TRUNC"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "cket,", "tokens": ["cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Den Sterblichen so blendend gleisst!", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", "so", "blen\u00b7dend", "gleisst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr s\u00e4ttigt nicht, weil ihr mit Rauche speist;", "tokens": ["Ihr", "s\u00e4t\u00b7tigt", "nicht", ",", "weil", "ihr", "mit", "Rau\u00b7che", "speist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und flieht, indem ihr uns entz\u00fccket.", "tokens": ["Und", "flieht", ",", "in\u00b7dem", "ihr", "uns", "ent\u00b7z\u00fc\u00b7cket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}