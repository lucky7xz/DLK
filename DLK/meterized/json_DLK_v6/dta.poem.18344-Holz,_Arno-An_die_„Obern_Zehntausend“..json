{"dta.poem.18344": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An die \u201eObern Zehntausend\u201c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Abgrund zu, drin uns die Zeit verschlingt,", "tokens": ["Dem", "Ab\u00b7grund", "zu", ",", "drin", "uns", "die", "Zeit", "ver\u00b7schlingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und ihr seid immer noch nicht abgeplundert,", "tokens": ["Und", "ihr", "seid", "im\u00b7mer", "noch", "nicht", "ab\u00b7ge\u00b7plun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht hinter die ", "tokens": ["Nicht", "hin\u00b7ter", "die"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wollt euch nicht l\u00e4nger freventlich vermessen,", "tokens": ["Wollt", "euch", "nicht", "l\u00e4n\u00b7ger", "fre\u00b7vent\u00b7lich", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Denn euer Lebensnerv ist abgestumpft,", "tokens": ["Denn", "eu\u00b7er", "Le\u00b7bens\u00b7nerv", "ist", "ab\u00b7ge\u00b7stumpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn eure Kronen sind von Rost zerfressen", "tokens": ["Denn", "eu\u00b7re", "Kro\u00b7nen", "sind", "von", "Rost", "zer\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eure Stammbaumw\u00e4lder sind versumpft!", "tokens": ["Und", "eu\u00b7re", "Stamm\u00b7baum\u00b7w\u00e4l\u00b7der", "sind", "ver\u00b7sumpft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Schon webt die Sonne ihm den Glorienschein,", "tokens": ["Schon", "webt", "die", "Son\u00b7ne", "ihm", "den", "Glo\u00b7ri\u00b7en\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und glaubt: Es wird kein veilchenblauer Werther,", "tokens": ["Und", "glaubt", ":", "Es", "wird", "kein", "veil\u00b7chen\u00b7blau\u00b7er", "Wert\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es wird ein blutiger Messias sein!", "tokens": ["Es", "wird", "ein", "blu\u00b7ti\u00b7ger", "Mes\u00b7si\u00b7as", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}