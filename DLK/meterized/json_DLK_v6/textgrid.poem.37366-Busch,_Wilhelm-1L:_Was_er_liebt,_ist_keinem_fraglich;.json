{"textgrid.poem.37366": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was er liebt, ist keinem fraglich;", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was er liebt, ist keinem fraglich;", "tokens": ["Was", "er", "liebt", ",", "ist", "kei\u00b7nem", "frag\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Triumphierend und behaglich", "tokens": ["Tri\u00b7um\u00b7phie\u00b7rend", "und", "be\u00b7hag\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmt es seine Seele ein", "tokens": ["Nimmt", "es", "sei\u00b7ne", "See\u00b7le", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und befiehlt: So soll es sein.", "tokens": ["Und", "be\u00b7fiehlt", ":", "So", "soll", "es", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Suche nie, wo dies geschehen,", "tokens": ["Su\u00b7che", "nie", ",", "wo", "dies", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Widersprechend vorzugehen,", "tokens": ["Wi\u00b7der\u00b7spre\u00b7chend", "vor\u00b7zu\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sintemalen im Gem\u00fct", "tokens": ["Sin\u00b7te\u00b7ma\u00b7len", "im", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon die h\u00f6chste Macht entschied.", "tokens": ["Schon", "die", "h\u00f6chs\u00b7te", "Macht", "ent\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ungest\u00f6rt in ihren Lauben", "tokens": ["Un\u00b7ge\u00b7st\u00f6rt", "in", "ih\u00b7ren", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df die Liebe, la\u00df den Glauben,", "tokens": ["La\u00df", "die", "Lie\u00b7be", ",", "la\u00df", "den", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der, wenn man es recht ermi\u00dft,", "tokens": ["Der", ",", "wenn", "man", "es", "recht", "er\u00b7mi\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch nur lauter Liebe ist.", "tokens": ["Auch", "nur", "lau\u00b7ter", "Lie\u00b7be", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}