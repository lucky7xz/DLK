{"textgrid.poem.42353": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hatte einen Traum von Einsamkeit.", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte einen Traum von Einsamkeit.", "tokens": ["Ich", "hat\u00b7te", "ei\u00b7nen", "Traum", "von", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Menschen hatten alle mich verstossen", "tokens": ["Die", "Men\u00b7schen", "hat\u00b7ten", "al\u00b7le", "mich", "ver\u00b7stos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und zitternd floh ich durch die Dunkelheit.", "tokens": ["Und", "zit\u00b7ternd", "floh", "ich", "durch", "die", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Obdach, keine H\u00fctte weit und breit,", "tokens": ["Kein", "Ob\u00b7dach", ",", "kei\u00b7ne", "H\u00fct\u00b7te", "weit", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kein Leben ausser meinem in dem grossen", "tokens": ["Kein", "Le\u00b7ben", "aus\u00b7ser", "mei\u00b7nem", "in", "dem", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Irrgang der Flucht und der Verlassenheit.", "tokens": ["Irr\u00b7gang", "der", "Flucht", "und", "der", "Ver\u00b7las\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die Nacht war tief und wetterstrahlbedroht.", "tokens": ["Die", "Nacht", "war", "tief", "und", "wet\u00b7ter\u00b7strahl\u00b7be\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Firmament wie angeschmiedet lagen", "tokens": ["Am", "Fir\u00b7ma\u00b7ment", "wie", "an\u00b7ge\u00b7schmie\u00b7det", "la\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KOKOM", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wolken schwarzums\u00e4umt. Der Mond glomm rot.", "tokens": ["Die", "Wol\u00b7ken", "schwar\u00b7zu\u00b7ms\u00e4umt", ".", "Der", "Mond", "glomm", "rot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Jammern kam mich an in meiner Not,", "tokens": ["Ein", "Jam\u00b7mern", "kam", "mich", "an", "in", "mei\u00b7ner", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie auf den Meeren die Verlornen klagen,", "tokens": ["Wie", "auf", "den", "Mee\u00b7ren", "die", "Ver\u00b7lor\u00b7nen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn ohne Halt und Wehr versinkt ihr Boot.", "tokens": ["Wenn", "oh\u00b7ne", "Halt", "und", "Wehr", "ver\u00b7sinkt", "ihr", "Boot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In meine lauten Klagen durch die Nacht", "tokens": ["In", "mei\u00b7ne", "lau\u00b7ten", "Kla\u00b7gen", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erbrauste der Posaunenschall der Rufer", "tokens": ["Er\u00b7braus\u00b7te", "der", "Po\u00b7sau\u00b7nen\u00b7schall", "der", "Ru\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Heer der ewigen Gewittermacht.", "tokens": ["Im", "Heer", "der", "e\u00b7wi\u00b7gen", "Ge\u00b7wit\u00b7ter\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und eine Flamme sah ich angefacht", "tokens": ["Und", "ei\u00b7ne", "Flam\u00b7me", "sah", "ich", "an\u00b7ge\u00b7facht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00dcber den dunklen Fernen ohne Ufer \u2013", "tokens": ["\u00dc\u00b7ber", "den", "dunk\u00b7len", "Fer\u00b7nen", "oh\u00b7ne", "U\u00b7fer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und bin in Graun und Gottesfurcht erwacht.", "tokens": ["Und", "bin", "in", "Graun", "und", "Got\u00b7tes\u00b7furcht", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}