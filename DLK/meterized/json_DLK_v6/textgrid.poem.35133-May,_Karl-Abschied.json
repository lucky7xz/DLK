{"textgrid.poem.35133": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade, ade! Ich ziehe von dir fort,", "tokens": ["A\u00b7de", ",", "a\u00b7de", "!", "Ich", "zie\u00b7he", "von", "dir", "fort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kenn nicht das Ziel, kenn weder Zeit noch Ort.", "tokens": ["Kenn", "nicht", "das", "Ziel", ",", "kenn", "we\u00b7der", "Zeit", "noch", "Ort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "$,", "VVFIN", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Auge weint; es thut das Herz mir weh,", "tokens": ["Das", "Au\u00b7ge", "weint", ";", "es", "thut", "das", "Herz", "mir", "weh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch zag ich nicht. Ade, ade, ade!", "tokens": ["Doch", "zag", "ich", "nicht", ".", "A\u00b7de", ",", "a\u00b7de", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "$.", "NN", "$,", "VVFIN", "$,", "ADV", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ade, ade! Ich ziehe von dir fort", "tokens": ["A\u00b7de", ",", "a\u00b7de", "!", "Ich", "zie\u00b7he", "von", "dir", "fort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und nehm den Glauben mit als meinen Hort.", "tokens": ["Und", "nehm", "den", "Glau\u00b7ben", "mit", "als", "mei\u00b7nen", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er k\u00fcndet mir, indem ich von dir geh,", "tokens": ["Er", "k\u00fcn\u00b7det", "mir", ",", "in\u00b7dem", "ich", "von", "dir", "geh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wiedersehn. Ade, ade, ade!", "tokens": ["Ein", "Wie\u00b7der\u00b7sehn", ".", "A\u00b7de", ",", "a\u00b7de", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "$,", "VVFIN", "$,", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ade, ade! Ich ziehe von dir fort", "tokens": ["A\u00b7de", ",", "a\u00b7de", "!", "Ich", "zie\u00b7he", "von", "dir", "fort"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "$.", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und sage dir ein liebes, sch\u00f6nes Wort:", "tokens": ["Und", "sa\u00b7ge", "dir", "ein", "lie\u00b7bes", ",", "sch\u00f6\u00b7nes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn ich auch nicht an deiner Seite steh,", "tokens": ["Wenn", "ich", "auch", "nicht", "an", "dei\u00b7ner", "Sei\u00b7te", "steh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es sch\u00fctzt dich Gott. Ade, ade, ade!", "tokens": ["Es", "sch\u00fctzt", "dich", "Gott", ".", "A\u00b7de", ",", "a\u00b7de", ",", "a\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$.", "NN", "$,", "VVFIN", "$,", "ADV", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}