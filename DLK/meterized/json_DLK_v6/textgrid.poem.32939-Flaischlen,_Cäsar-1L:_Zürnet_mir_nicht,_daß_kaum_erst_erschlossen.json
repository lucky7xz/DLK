{"textgrid.poem.32939": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Z\u00fcrnet mir nicht, da\u00df kaum erst erschlossen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z\u00fcrnet mir nicht, da\u00df kaum erst erschlossen", "tokens": ["Z\u00fcr\u00b7net", "mir", "nicht", ",", "da\u00df", "kaum", "erst", "er\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV", "ADV", "VVPP"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "ich euch schon pfl\u00fccke, zu ernstem Scherz ...", "tokens": ["ich", "euch", "schon", "pfl\u00fc\u00b7cke", ",", "zu", "erns\u00b7tem", "Scherz", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gr\u00fc\u00dft eure Schwester mir, duftende Rosen,", "tokens": ["Gr\u00fc\u00dft", "eu\u00b7re", "Schwes\u00b7ter", "mir", ",", "duf\u00b7ten\u00b7de", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "eure Schwester vom fr\u00fchen M\u00e4rz!", "tokens": ["eu\u00b7re", "Schwes\u00b7ter", "vom", "fr\u00fc\u00b7hen", "M\u00e4rz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Leise schon zittert's wie Herbst durch die L\u00fcfte ...", "tokens": ["Lei\u00b7se", "schon", "zit\u00b7tert's", "wie", "Herbst", "durch", "die", "L\u00fcf\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "KOKOM", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "euer Bl\u00fchen, wie lang wohl noch w\u00e4hrt's?!", "tokens": ["eu\u00b7er", "Bl\u00fc\u00b7hen", ",", "wie", "lang", "wohl", "noch", "w\u00e4hrt's", "?!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD", "ADV", "ADV", "NE", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gr\u00fc\u00dft eure Schwester mir, duftende Rosen,", "tokens": ["Gr\u00fc\u00dft", "eu\u00b7re", "Schwes\u00b7ter", "mir", ",", "duf\u00b7ten\u00b7de", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "eure Schwester vom fr\u00fchen M\u00e4rz!", "tokens": ["eu\u00b7re", "Schwes\u00b7ter", "vom", "fr\u00fc\u00b7hen", "M\u00e4rz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Statt in Herbstn\u00e4chten einsam zu welken ...", "tokens": ["Statt", "in", "Herbst\u00b7n\u00e4ch\u00b7ten", "ein\u00b7sam", "zu", "wel\u00b7ken", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "st\u00fcrbt ihr nicht lieber vergl\u00fchend am Herz", "tokens": ["st\u00fcrbt", "ihr", "nicht", "lie\u00b7ber", "ver\u00b7gl\u00fc\u00b7hend", "am", "Herz"], "token_info": ["word", "