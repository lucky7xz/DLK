{"textgrid.poem.57139": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[es martert dich]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es martert dich,", "tokens": ["Es", "mar\u00b7tert", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "da\u00df wir Menschen gleich fraglich sind,", "tokens": ["da\u00df", "wir", "Men\u00b7schen", "gleich", "frag\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "ob wir lachen oder weinen.", "tokens": ["ob", "wir", "la\u00b7chen", "o\u00b7der", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Freund, du mu\u00dft beiseite gehn,", "tokens": ["Mein", "Freund", ",", "du", "mu\u00dft", "bei\u00b7sei\u00b7te", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "nur selten einem ins Antlitz sehn,", "tokens": ["nur", "sel\u00b7ten", "ei\u00b7nem", "ins", "Ant\u00b7litz", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "sonst wirst du uns stets verneinen.", "tokens": ["sonst", "wirst", "du", "uns", "stets", "ver\u00b7nei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}