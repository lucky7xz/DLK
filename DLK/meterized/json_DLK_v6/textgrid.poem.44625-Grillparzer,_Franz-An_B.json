{"textgrid.poem.44625": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An B", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schrieb Verse gegen dich,", "tokens": ["Ich", "schrieb", "Ver\u00b7se", "ge\u00b7gen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "so sprichst du \u00e4rmster der Poeten,", "tokens": ["so", "sprichst", "du", "\u00e4rms\u00b7ter", "der", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "das hie\u00dfe, Gott beh\u00fcte mich,", "tokens": ["das", "hie\u00b7\u00dfe", ",", "Gott", "be\u00b7h\u00fc\u00b7te", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mit goldnen Kugeln Spatzen t\u00f6ten.", "tokens": ["mit", "gold\u00b7nen", "Ku\u00b7geln", "Spat\u00b7zen", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}