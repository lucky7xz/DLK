{"dta.poem.1190": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die wei\u00dfe Fillis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst die bunten Tulpen weisen ", "tokens": ["Lasst", "die", "bun\u00b7ten", "Tul\u00b7pen", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer hohen Farbe Zier/", "tokens": ["Ih\u00b7rer", "ho\u00b7hen", "Far\u00b7be", "Zier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasst die edle Rose preisen/", "tokens": ["Lasst", "die", "ed\u00b7le", "Ro\u00b7se", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeig Narei\u00df und N\u00e4geln f\u00fcr:", "tokens": ["Zeig", "Nar\u00b7ei\u00df", "und", "N\u00e4\u00b7geln", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liljen/ die bey Fillis stehn/", "tokens": ["Lil\u00b7jen", "/", "die", "bey", "Fil\u00b7lis", "stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "APPR", "NE", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind f\u00fcr allen Blumen sch\u00f6n.", "tokens": ["Sind", "f\u00fcr", "al\u00b7len", "Blu\u00b7men", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zephyr mit verliebten K\u00fcssen", "tokens": ["Ze\u00b7phyr", "mit", "ver\u00b7lieb\u00b7ten", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielt um ihren zarten Mund/", "tokens": ["Spielt", "um", "ih\u00b7ren", "zar\u00b7ten", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4sst die stoltze Flora wissen/", "tokens": ["L\u00e4sst", "die", "stolt\u00b7ze", "Flo\u00b7ra", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht mit lindem Rauschen kund/", "tokens": ["Macht", "mit", "lin\u00b7dem", "Rau\u00b7schen", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liljen/ die bey Fillis stehn/", "tokens": ["Lil\u00b7jen", "/", "die", "bey", "Fil\u00b7lis", "stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "APPR", "NE", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind f\u00fcr allen Blumen sch\u00f6n.", "tokens": ["Sind", "f\u00fcr", "al\u00b7len", "Blu\u00b7men", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Milch und Schnee kan nicht erreichen", "tokens": ["Milch", "und", "Schnee", "kan", "nicht", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VMFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer reinen Weisse Pracht/", "tokens": ["Ih\u00b7rer", "rei\u00b7nen", "Weis\u00b7se", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Narcissen sind ingleichen", "tokens": ["Die", "Nar\u00b7cis\u00b7sen", "sind", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen ihrem Tage Nacht;", "tokens": ["Ge\u00b7gen", "ih\u00b7rem", "Ta\u00b7ge", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liljen/ die bey Fillis stehn/", "tokens": ["Lil\u00b7jen", "/", "die", "bey", "Fil\u00b7lis", "stehn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "APPR", "NE", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind f\u00fcr allen Blumen sch\u00f6n.", "tokens": ["Sind", "f\u00fcr", "al\u00b7len", "Blu\u00b7men", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Amor selbst hat/ sie zu pflegen/", "tokens": ["A\u00b7mor", "selbst", "hat", "/", "sie", "zu", "pfle\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "$(", "PPER", "PTKZU", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mich zum G\u00e4rtner eingesezt.", "tokens": ["Mich", "zum", "G\u00e4rt\u00b7ner", "ein\u00b7ge\u00b7sezt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Thr\u00e4nen sind der Regen", "tokens": ["Mei\u00b7ne", "Thr\u00e4\u00b7nen", "sind", "der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der sie nach und nach benezt/", "tokens": ["Der", "sie", "nach", "und", "nach", "be\u00b7nezt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "KON", "APPR", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bi\u00df mir Fillis mit der Zeit", "tokens": ["Bi\u00df", "mir", "Fil\u00b7lis", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie zu brechen Gunst verleiht.", "tokens": ["Sie", "zu", "bre\u00b7chen", "Gunst", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}