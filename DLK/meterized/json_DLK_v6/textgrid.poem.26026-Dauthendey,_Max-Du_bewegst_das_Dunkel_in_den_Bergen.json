{"textgrid.poem.26026": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du bewegst das Dunkel in den Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist wie die metallnen Glocken, du bewegst das Dunkel in den Bergen,", "tokens": ["Du", "bist", "wie", "die", "me\u00b7tall\u00b7nen", "Glo\u00b7cken", ",", "du", "be\u00b7wegst", "das", "Dun\u00b7kel", "in", "den", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Du ersch\u00fctterst mein Herz, da\u00df es zwischen Lachen und Weinen schwankt.", "tokens": ["Du", "er\u00b7sch\u00fct\u00b7terst", "mein", "Herz", ",", "da\u00df", "es", "zwi\u00b7schen", "La\u00b7chen", "und", "Wei\u00b7nen", "schwankt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Einst ging ich durch Traurigkeit wie durch Regen, der endlos niederh\u00e4ngt,", "tokens": ["Einst", "ging", "ich", "durch", "Trau\u00b7rig\u00b7keit", "wie", "durch", "Re\u00b7gen", ",", "der", "end\u00b7los", "nie\u00b7der\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KOKOM", "APPR", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Weit und breit fiel mir der Himmel kalt vor die F\u00fc\u00dfe;", "tokens": ["Weit", "und", "breit", "fiel", "mir", "der", "Him\u00b7mel", "kalt", "vor", "die", "F\u00fc\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Ich wei\u00df es schwer, sehr bitter sind die Tr\u00e4nen der Trennung,", "tokens": ["Ich", "wei\u00df", "es", "schwer", ",", "sehr", "bit\u00b7ter", "sind", "die", "Tr\u00e4\u00b7nen", "der", "Tren\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie k\u00f6nnen mit ihrer Sch\u00e4rfe mein Haus zusammenziehen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "mit", "ih\u00b7rer", "Sch\u00e4r\u00b7fe", "mein", "Haus", "zu\u00b7sam\u00b7men\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sie g\u00f6nnen keinen Schutz, und ich kann ihnen nicht entfliehen.", "tokens": ["Sie", "g\u00f6n\u00b7nen", "kei\u00b7nen", "Schutz", ",", "und", "ich", "kann", "ih\u00b7nen", "nicht", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}