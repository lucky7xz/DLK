{"textgrid.poem.32392": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "70. Auf ebendieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So lieb euch, Kinder, Ruh und Gl\u00fcck:", "tokens": ["So", "lieb", "euch", ",", "Kin\u00b7der", ",", "Ruh", "und", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcck von ihm, dem Schalke! weit zur\u00fcck! \u2013", "tokens": ["Zu\u00b7r\u00fcck", "von", "ihm", ",", "dem", "Schal\u00b7ke", "!", "weit", "zu\u00b7r\u00fcck", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "ART", "NN", "$.", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(ich h\u00e4tte viel f\u00fcr diesen Rat gegeben!)", "tokens": ["(", "ich", "h\u00e4t\u00b7te", "viel", "f\u00fcr", "die\u00b7sen", "Rat", "ge\u00b7ge\u00b7ben", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "APPR", "PDAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er stellt sich so nur ohne Leben.", "tokens": ["Er", "stellt", "sich", "so", "nur", "oh\u00b7ne", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}