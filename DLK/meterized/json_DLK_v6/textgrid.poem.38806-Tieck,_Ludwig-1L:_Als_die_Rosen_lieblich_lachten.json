{"textgrid.poem.38806": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als die Rosen lieblich lachten", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die Rosen lieblich lachten", "tokens": ["Als", "die", "Ro\u00b7sen", "lieb\u00b7lich", "lach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Sommerlauben bl\u00fchten,", "tokens": ["Und", "die", "Som\u00b7mer\u00b7lau\u00b7ben", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Purpurn die Granaten gl\u00fchten,", "tokens": ["Pur\u00b7purn", "die", "Gra\u00b7na\u00b7ten", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Deine Augen hold erwachten;", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "hold", "er\u00b7wach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles sprach nur Liebesschmachten,", "tokens": ["Al\u00b7les", "sprach", "nur", "Lie\u00b7bes\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Es verk\u00fcndete die s\u00fcssen", "tokens": ["Es", "ver\u00b7k\u00fcn\u00b7de\u00b7te", "die", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tr\u00e4ume Nachtigallengr\u00fc\u00dfen,", "tokens": ["Tr\u00e4u\u00b7me", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dein Blick, dein Ton ward Wonne,", "tokens": ["Und", "dein", "Blick", ",", "dein", "Ton", "ward", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lebe dann der Liebessonne,", "tokens": ["Le\u00b7be", "dann", "der", "Lie\u00b7bes\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Weihe diesen Tag mit K\u00fcssen.", "tokens": ["Wei\u00b7he", "die\u00b7sen", "Tag", "mit", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}