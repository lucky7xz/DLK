{"dta.poem.20660": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "1765.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch deines Lagers Ueberhang                   ", "tokens": ["Durch", "dei\u00b7nes", "La\u00b7gers", "Ue\u00b7ber\u00b7hang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nicht des Tages Blick gedrungen,", "tokens": ["Ist", "nicht", "des", "Ta\u00b7ges", "Blick", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Phillis! als mich zum Gesang", "tokens": ["O", "Phil\u00b7lis", "!", "als", "mich", "zum", "Ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mein klopfend Herz schon aufgezwungen.", "tokens": ["Mein", "klop\u00b7fend", "Herz", "schon", "auf\u00b7ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich nahm die Leyer, dachte dich,", "tokens": ["Ich", "nahm", "die", "Le\u00b7yer", ",", "dach\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und frug, ob meine Muse w\u00fc\u00dfte,", "tokens": ["Und", "frug", ",", "ob", "mei\u00b7ne", "Mu\u00b7se", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Phillis lieblich tr\u00e4umend sich", "tokens": ["Da\u00df", "Phil\u00b7lis", "lieb\u00b7lich", "tr\u00e4u\u00b7mend", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJD", "VVPP", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir und ihrem Sch\u00e4fer k\u00fc\u00dfte?", "tokens": ["Mit", "mir", "und", "ih\u00b7rem", "Sch\u00e4\u00b7fer", "k\u00fc\u00df\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, meine Freundin! k\u00fcsse mich,", "tokens": ["Komm", ",", "mei\u00b7ne", "Freun\u00b7din", "!", "k\u00fcs\u00b7se", "mich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und la\u00df mich deine Rechte f\u00fchren", "tokens": ["Und", "la\u00df", "mich", "dei\u00b7ne", "Rech\u00b7te", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum H\u00fcgel, den so schauerlich", "tokens": ["Zum", "H\u00fc\u00b7gel", ",", "den", "so", "schau\u00b7er\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeichen der Vernichtung zieren.", "tokens": ["Die", "Zei\u00b7chen", "der", "Ver\u00b7nich\u00b7tung", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein halb zerst\u00f6hrter Tempel ragt", "tokens": ["Ein", "halb", "zer\u00b7st\u00f6hr\u00b7ter", "Tem\u00b7pel", "ragt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hervor, als h\u00e4tte man vor Zeiten", "tokens": ["Her\u00b7vor", ",", "als", "h\u00e4t\u00b7te", "man", "vor", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "KOKOM", "VAFIN", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier den Apoll um Rath gefragt", "tokens": ["Hier", "den", "A\u00b7poll", "um", "Rath", "ge\u00b7fragt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Beim Waffengriff, zu Krieg und Streiten.", "tokens": ["Beim", "Waf\u00b7fen\u00b7griff", ",", "zu", "Krieg", "und", "Strei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Uns d\u00fcnkt, als ob die Pythia", "tokens": ["Uns", "d\u00fcnkt", ",", "als", "ob", "die", "Py\u00b7thia"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Noch in den dunklen Th\u00e4lern s\u00e4\u00dfe,", "tokens": ["Noch", "in", "den", "dunk\u00b7len", "Th\u00e4\u00b7lern", "s\u00e4\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und murmelte was jezt geschah,", "tokens": ["Und", "mur\u00b7mel\u00b7te", "was", "jezt", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das Zuk\u00fcnftige verg\u00e4\u00dfe.", "tokens": ["Und", "das", "Zu\u00b7k\u00fcnf\u00b7ti\u00b7ge", "ver\u00b7g\u00e4\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Komm, meine Phillis! wenn der Tag", "tokens": ["Komm", ",", "mei\u00b7ne", "Phil\u00b7lis", "!", "wenn", "der", "Tag"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NE", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So heiter bleibt, und f\u00fchle Trauer,", "tokens": ["So", "hei\u00b7ter", "bleibt", ",", "und", "f\u00fch\u00b7le", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KON", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie damals, da Dein Daphnis lag", "tokens": ["Wie", "da\u00b7mals", ",", "da", "Dein", "Daph\u00b7nis", "lag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Dir im lezten Todesschauer.", "tokens": ["Vor", "Dir", "im", "lez\u00b7ten", "To\u00b7des\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Nein, nicht so grausam f\u00fchle sie,", "tokens": ["Nein", ",", "nicht", "so", "grau\u00b7sam", "f\u00fch\u00b7le", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKNEG", "ADV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich m\u00fcsse z\u00e4rtlich nur durchdringen", "tokens": ["Dich", "m\u00fcs\u00b7se", "z\u00e4rt\u00b7lich", "nur", "durch\u00b7drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bei Birken die Melancholie,", "tokens": ["Bei", "Bir\u00b7ken", "die", "Me\u00b7lan\u00b7cho\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dich zu sanften Thr\u00e4nen zwingen.", "tokens": ["Und", "dich", "zu", "sanf\u00b7ten", "Thr\u00e4\u00b7nen", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}