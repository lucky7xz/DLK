{"dta.poem.11813": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was wirklich satt dich macht, das wirst du niemals satt,", "tokens": ["Was", "wirk\u00b7lich", "satt", "dich", "macht", ",", "das", "wirst", "du", "nie\u00b7mals", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADJD", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Brot, das immer Reiz f\u00fcr neuen Hunger hat.", "tokens": ["Wie", "Brot", ",", "das", "im\u00b7mer", "Reiz", "f\u00fcr", "neu\u00b7en", "Hun\u00b7ger", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ADV", "NN", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dagegen die Gew\u00fcrz' und alle leckern Sachen,", "tokens": ["Da\u00b7ge\u00b7gen", "die", "Ge\u00b7w\u00fcrz'", "und", "al\u00b7le", "le\u00b7ckern", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wirst du satt so bald, weil sie nie satt dich machen.", "tokens": ["Die", "wirst", "du", "satt", "so", "bald", ",", "weil", "sie", "nie", "satt", "dich", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER