{"textgrid.poem.38762": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Augen! wohin f\u00fchren mich die s\u00fc\u00dfen Scheine?", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Augen! wohin f\u00fchren mich die s\u00fc\u00dfen Scheine?", "tokens": ["O", "Au\u00b7gen", "!", "wo\u00b7hin", "f\u00fch\u00b7ren", "mich", "die", "s\u00fc\u00b7\u00dfen", "Schei\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich meine, da\u00df ich nur zu b\u00fc\u00dfen ein mu\u00df saugen", "tokens": ["Ich", "mei\u00b7ne", ",", "da\u00df", "ich", "nur", "zu", "b\u00fc\u00b7\u00dfen", "ein", "mu\u00df", "sau\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VVINF", "ART", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Augen lieblich Gr\u00fc\u00dfen; wie ich freudig weine", "tokens": ["Der", "Au\u00b7gen", "lieb\u00b7lich", "Gr\u00fc\u00b7\u00dfen", ";", "wie", "ich", "freu\u00b7dig", "wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$.", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mich der Deine f\u00fchl' im K\u00fcssen, fragen mich die Augen", "tokens": ["Und", "mich", "der", "Dei\u00b7ne", "f\u00fchl'", "im", "K\u00fcs\u00b7sen", ",", "fra\u00b7gen", "mich", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "PPOSAT", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Mit sanftem Schimmer: wird auch immer dieses Gl\u00fcck mir lachen?", "tokens": ["Mit", "sanf\u00b7tem", "Schim\u00b7mer", ":", "wird", "auch", "im\u00b7mer", "die\u00b7ses", "Gl\u00fcck", "mir", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "VAFIN", "ADV", "ADV", "PDAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Sie machen,", "tokens": ["Sie", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Da\u00df die Freuden Leiden gleich mir sind: \u2013", "tokens": ["Da\u00df", "die", "Freu\u00b7den", "Lei\u00b7den", "gleich", "mir", "sind", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "PPER", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "O liebstes Kind,", "tokens": ["O", "liebs\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "La\u00df dieses Fragen, sagen kann ich's nie und weint' ich mich auch blind.", "tokens": ["La\u00df", "die\u00b7ses", "Fra\u00b7gen", ",", "sa\u00b7gen", "kann", "ich's", "nie", "und", "weint'", "ich", "mich", "auch", "blind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "$,", "VVINF", "VMFIN", "PIS", "ADV", "KON", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}