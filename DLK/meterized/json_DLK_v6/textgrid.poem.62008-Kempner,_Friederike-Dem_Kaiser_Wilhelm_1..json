{"textgrid.poem.62008": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Dem Kaiser Wilhelm 1.", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Staunest ob der Alpenh\u00f6he,", "tokens": ["Stau\u00b7nest", "ob", "der", "Al\u00b7pen\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sinkest nieder vor den Sternen,", "tokens": ["Sin\u00b7kest", "nie\u00b7der", "vor", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem Glanz des Meteores", "tokens": ["Vor", "dem", "Glanz", "des", "Me\u00b7te\u00b7o\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den unbegriffnen Fernen;", "tokens": ["Aus", "den", "un\u00b7be\u00b7griff\u00b7nen", "Fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Staun' nicht ob der Alpenh\u00f6he,", "tokens": ["Staun'", "nicht", "ob", "der", "Al\u00b7pen\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sink' nicht nieder vor den Sternen,", "tokens": ["Sink'", "nicht", "nie\u00b7der", "vor", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem Glanz des Meteores", "tokens": ["Vor", "dem", "Glanz", "des", "Me\u00b7te\u00b7o\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den unbekannten Fernen:", "tokens": ["Aus", "den", "un\u00b7be\u00b7kann\u00b7ten", "Fer\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An und f\u00fcr sich sind sie wenig", "tokens": ["An", "und", "f\u00fcr", "sich", "sind", "sie", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "APPR", "PRF", "VAFIN", "PPER", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u2013 Wahre Gr\u00f6\u00dfe wohnt im Geist \u2013", "tokens": ["\u2013", "Wah\u00b7re", "Gr\u00f6\u00b7\u00dfe", "wohnt", "im", "Geist", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Staune an den gro\u00dfen K\u00f6nig,", "tokens": ["Stau\u00b7ne", "an", "den", "gro\u00b7\u00dfen", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den mit Recht man \u00bbErsten\u00ab hei\u00dft \u2013", "tokens": ["Den", "mit", "Recht", "man", "\u00bb", "Ers\u00b7ten", "\u00ab", "hei\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "NN", "PIS", "$(", "NN", "$(", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Zoll ein Kaiser-K\u00f6nig,", "tokens": ["Je\u00b7der", "Zoll", "ein", "Kai\u00b7ser\u00b7K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der die V\u00f6lker mit sich rei\u00dft!", "tokens": ["Der", "die", "V\u00f6l\u00b7ker", "mit", "sich", "rei\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}