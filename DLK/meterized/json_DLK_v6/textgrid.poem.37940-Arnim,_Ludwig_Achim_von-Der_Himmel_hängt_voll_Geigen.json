{"textgrid.poem.37940": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Himmel h\u00e4ngt voll Geigen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir genie\u00dfen die himmlischen Freuden,", "tokens": ["Wir", "ge\u00b7nie\u00b7\u00dfen", "die", "himm\u00b7li\u00b7schen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Drum thun wir das Irdische meiden,", "tokens": ["Drum", "thun", "wir", "das", "Ir\u00b7di\u00b7sche", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Kein weltlich Get\u00fcmmel", "tokens": ["Kein", "welt\u00b7lich", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "H\u00f6rt man nicht im Himmel,", "tokens": ["H\u00f6rt", "man", "nicht", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lebt alles in sanftester Ruh;", "tokens": ["Lebt", "al\u00b7les", "in", "sanf\u00b7tes\u00b7ter", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Wir f\u00fchren ein englisches Leben,", "tokens": ["Wir", "f\u00fch\u00b7ren", "ein", "eng\u00b7li\u00b7sches", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Sind dennoch ganz lustig daneben,", "tokens": ["Sind", "den\u00b7noch", "ganz", "lus\u00b7tig", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "PAV", "$,"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wir tanzen und springen,", "tokens": ["Wir", "tan\u00b7zen", "und", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Wir h\u00fcpfen und singen,", "tokens": ["Wir", "h\u00fcp\u00b7fen", "und", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Sanct Peter im Himmel sieht zu.", "tokens": ["Sanct", "Pe\u00b7ter", "im", "Him\u00b7mel", "sieht", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Johannes das L\u00e4mmlein auslasset,", "tokens": ["Jo\u00b7han\u00b7nes", "das", "L\u00e4mm\u00b7lein", "aus\u00b7las\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Metzger Herodes drauf passet,", "tokens": ["Der", "Metz\u00b7ger", "He\u00b7ro\u00b7des", "drauf", "pas\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Wir f\u00fchren ein gedultigs,", "tokens": ["Wir", "f\u00fch\u00b7ren", "ein", "ge\u00b7dul\u00b7tigs", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unschuldigs, gedultigs,", "tokens": ["Un\u00b7schul\u00b7digs", ",", "ge\u00b7dul\u00b7tigs", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ein liebliches L\u00e4mmlein zum Tod.", "tokens": ["Ein", "lieb\u00b7li\u00b7ches", "L\u00e4mm\u00b7lein", "zum", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Sanct Lucas den Ochsen thut schlachten,", "tokens": ["Sanct", "Lu\u00b7cas", "den", "Och\u00b7sen", "thut", "schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Ohn einigs Bedenken und Achten,", "tokens": ["Ohn", "ei\u00b7nigs", "Be\u00b7den\u00b7ken", "und", "Ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Der Wein kost't kein Heller", "tokens": ["Der", "Wein", "kost't", "kein", "Hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Im himmlischen Keller,", "tokens": ["Im", "himm\u00b7li\u00b7schen", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die Engel, die backen das Brod.", "tokens": ["Die", "En\u00b7gel", ",", "die", "ba\u00b7cken", "das", "Brod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Gut Kr\u00e4uter von allerhand Arten,", "tokens": ["Gut", "Kr\u00e4u\u00b7ter", "von", "al\u00b7ler\u00b7hand", "Ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die wachsen im himmlischen Garten,", "tokens": ["Die", "wach\u00b7sen", "im", "himm\u00b7li\u00b7schen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Gut Spargel, Fisolen,", "tokens": ["Gut", "Spar\u00b7gel", ",", "Fi\u00b7so\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und was wir nur wollen,", "tokens": ["Und", "was", "wir", "nur", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ganze Sch\u00fcssel voll sind uns bereit", "tokens": ["Gan\u00b7ze", "Sch\u00fcs\u00b7sel", "voll", "sind", "uns", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Gut Aepfel, gut Birn und gut Trauben,", "tokens": ["Gut", "A\u00b7e\u00b7pfel", ",", "gut", "Birn", "und", "gut", "Trau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Die G\u00e4rtner, die alles erlauben.", "tokens": ["Die", "G\u00e4rt\u00b7ner", ",", "die", "al\u00b7les", "er\u00b7lau\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Willst Rehbock, willst Hasen?", "tokens": ["Willst", "Reh\u00b7bock", ",", "willst", "Ha\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VMFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Auf offner Stra\u00dfen,", "tokens": ["Auf", "off\u00b7ner", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Zur K\u00fcche sie laufen herbei.", "tokens": ["Zur", "K\u00fc\u00b7che", "sie", "lau\u00b7fen", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sollt' etwa ein Fasttag ankommen,", "tokens": ["Sollt'", "et\u00b7wa", "ein", "Fast\u00b7tag", "an\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Fische mit Freuden anstr\u00f6men,", "tokens": ["Die", "Fi\u00b7sche", "mit", "Freu\u00b7den", "an\u00b7str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da laufet Sanct Peter", "tokens": ["Da", "lau\u00b7fet", "Sanct", "Pe\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit Netz und mit K\u00f6der", "tokens": ["Mit", "Netz", "und", "mit", "K\u00f6\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zum himmlischen Weiher hinein;", "tokens": ["Zum", "himm\u00b7li\u00b7schen", "Wei\u00b7her", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Willst Karpfen, willst Hecht, willst Forellen,", "tokens": ["Willst", "Karp\u00b7fen", ",", "willst", "Hecht", ",", "willst", "Fo\u00b7rel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "VMFIN", "NN", "$,", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gut Stockfisch und frische Sardellen?", "tokens": ["Gut", "Stock\u00b7fisch", "und", "fri\u00b7sche", "Sar\u00b7del\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "--+-+-+--", "measure": "anapaest.init"}, "line.8": {"text": "Sanct Lorenz hat m\u00fcssen", "tokens": ["Sanct", "Lo\u00b7renz", "hat", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NE", "VAFIN", "VMFIN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Sein Leben einb\u00fc\u00dfen,", "tokens": ["Sein", "Le\u00b7ben", "ein\u00b7b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Sanct Marta die K\u00f6chin mu\u00df seyn.", "tokens": ["Sanct", "Mar\u00b7ta", "die", "K\u00f6\u00b7chin", "mu\u00df", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Kein Musik ist ja nicht auf Erden,", "tokens": ["Kein", "Mu\u00b7sik", "ist", "ja", "nicht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unsrer verglichen kann werden,", "tokens": ["Die", "uns\u00b7rer", "ver\u00b7gli\u00b7chen", "kann", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "VAINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Eilftausend Jungfrauen", "tokens": ["Eilf\u00b7tau\u00b7send", "Jung\u00b7frau\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu tanzen sich trauen,", "tokens": ["Zu", "tan\u00b7zen", "sich", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sanct Ursula selbst dazu lacht,", "tokens": ["Sanct", "Ur\u00b7su\u00b7la", "selbst", "da\u00b7zu", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Cecilia mit ihren Verwandten,", "tokens": ["Ce\u00b7ci\u00b7lia", "mit", "ih\u00b7ren", "Ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Sind treffliche Hofmusikanten,", "tokens": ["Sind", "treff\u00b7li\u00b7che", "Hof\u00b7mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die englische Stimmen", "tokens": ["Die", "eng\u00b7li\u00b7sche", "Stim\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Ermuntern die Sinnen,", "tokens": ["Er\u00b7mun\u00b7tern", "die", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Da\u00df Alles f\u00fcr Freuden erwacht!", "tokens": ["Da\u00df", "Al\u00b7les", "f\u00fcr", "Freu\u00b7den", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}