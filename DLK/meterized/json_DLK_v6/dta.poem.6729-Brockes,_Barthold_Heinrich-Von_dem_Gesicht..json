{"dta.poem.6729": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dis herrliche Gem\u00e4hld' kan wohl verglichen seyn", "tokens": ["Dis", "herr\u00b7li\u00b7che", "Ge\u00b7m\u00e4hld'", "kan", "wohl", "ver\u00b7gli\u00b7chen", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "VMFIN", "ADV", "VVPP", "VAINF"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Mit Epicurens Bilderlein.", "tokens": ["Mit", "E\u00b7pi\u00b7cu\u00b7rens", "Bil\u00b7der\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er will, da\u00df von den C\u00f6rpern, die uns r\u00fchren,", "tokens": ["Er", "will", ",", "da\u00df", "von", "den", "C\u00f6r\u00b7pern", ",", "die", "uns", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich d\u00fcnne C\u00f6rper abwerts f\u00fchren,", "tokens": ["Sich", "d\u00fcn\u00b7ne", "C\u00f6r\u00b7per", "ab\u00b7werts", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die von den C\u00f6rpern, die Gestalten,", "tokens": ["Die", "von", "den", "C\u00f6r\u00b7pern", ",", "die", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus welchen sie entstehn, behalten.", "tokens": ["Aus", "wel\u00b7chen", "sie", "ent\u00b7stehn", ",", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}