{"textgrid.poem.43569": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Leb wohl, du sch\u00f6ner Wald!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So scheiden wir mit Sang und Klang:", "tokens": ["So", "schei\u00b7den", "wir", "mit", "Sang", "und", "Klang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Leb wohl, du sch\u00f6ner Wald!", "tokens": ["Leb", "wohl", ",", "du", "sch\u00f6\u00b7ner", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit deinem k\u00fchlen Schatten,", "tokens": ["Mit", "dei\u00b7nem", "k\u00fch\u00b7len", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit deinen gr\u00fcnen Matten,", "tokens": ["Mit", "dei\u00b7nen", "gr\u00fc\u00b7nen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du s\u00fc\u00dfer Aufenthalt!", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Auf\u00b7ent\u00b7halt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir singen auf dem Heimweg noch", "tokens": ["Wir", "sin\u00b7gen", "auf", "dem", "Heim\u00b7weg", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied der Dankbarkeit:", "tokens": ["Ein", "Lied", "der", "Dank\u00b7bar\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lad' ein wie heut' uns wieder", "tokens": ["Lad'", "ein", "wie", "heut'", "uns", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "KOKOM", "ADV", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Laubesduft und Lieder", "tokens": ["Auf", "Lau\u00b7bes\u00b7duft", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zur sch\u00f6nen Maienzeit!", "tokens": ["Zur", "sch\u00f6\u00b7nen", "Mai\u00b7en\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schaut hin! von fern noch h\u00f6rt's der Wald", "tokens": ["Schaut", "hin", "!", "von", "fern", "noch", "h\u00f6rt's", "der", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "APPR", "ADJD", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner Abendruh;", "tokens": ["In", "sei\u00b7ner", "A\u00b7ben\u00b7druh", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wipfel m\u00f6cht' er neigen,", "tokens": ["Die", "Wip\u00b7fel", "m\u00f6cht'", "er", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er rauschet mit den Zweigen,", "tokens": ["Er", "rau\u00b7schet", "mit", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lebt wohl! ruft er uns zu.", "tokens": ["Lebt", "wohl", "!", "ruft", "er", "uns", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}