{"dta.poem.15049": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "ze halten in ir pflege", "tokens": ["ze", "hal\u00b7ten", "in", "ir", "pfle\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["XY", "VVINF", "APPR", "PPER", "VVFIN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "... uf des gruenen planes wege", "tokens": ["...", "uf", "des", "gru\u00b7e\u00b7nen", "pla\u00b7nes", "we\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "... ren si das beste", "tokens": ["...", "ren", "si", "das", "bes\u00b7te"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "NE", "ART", "ADJA"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "... zwene v&#250;r die veste", "tokens": ["...", "zwe\u00b7ne", "v", "&#250;", "r", "die", "ves\u00b7te"], "token_info": ["punct", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["$(", "CARD", "XY", "$(", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "... vvurden uzerhalb geleit", "tokens": ["...", "vvur\u00b7den", "u\u00b7zer\u00b7halb", "ge\u00b7leit"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "NE", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "... uo dem mere vvit unde breit", "tokens": ["...", "u\u00b7o", "dem", "me\u00b7re", "vvit", "un\u00b7de", "breit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "vnder boume uf gruenez gras", "tokens": ["vn\u00b7der", "bou\u00b7me", "uf", "gru\u00b7e\u00b7nez", "gras"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "der k&#250;nec von Kerlingen vvas", "tokens": ["der", "k", "&#250;", "nec", "von", "Ker\u00b7lin\u00b7gen", "vvas"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "NN", "NE"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "der Keiserinne vvunnesam", "tokens": ["der", "Kei\u00b7se\u00b7rin\u00b7ne", "vvun\u00b7ne\u00b7sam"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "von herzen vigent unde gram", "tokens": ["von", "her\u00b7zen", "vi\u00b7gent", "un\u00b7de", "gram"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "NE", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "durh das er vvande han verlorn", "tokens": ["durh", "das", "er", "vvan\u00b7de", "han", "ver\u00b7lorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "NE", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "von der vrouvven hochgeborn", "tokens": ["von", "der", "vrouv\u00b7ven", "hoch\u00b7ge\u00b7born"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Partinopieren sinen mac", "tokens": ["Par\u00b7ti\u00b7no\u00b7pie\u00b7ren", "si\u00b7nen", "mac"], "token_info": ["word", "word", "word"], "pos": ["NN", "