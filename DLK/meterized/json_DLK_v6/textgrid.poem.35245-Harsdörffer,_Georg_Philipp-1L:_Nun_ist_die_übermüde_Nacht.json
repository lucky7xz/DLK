{"textgrid.poem.35245": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun ist die \u00fcberm\u00fcde Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist die \u00fcberm\u00fcde Nacht", "tokens": ["Nun", "ist", "die", "\u00fc\u00b7ber\u00b7m\u00fc\u00b7de", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sichrer Ruhe hingebracht,", "tokens": ["In", "sich\u00b7rer", "Ru\u00b7he", "hin\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Morgenr\u00f6the blicket;", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Sonnen Purpurangesicht,", "tokens": ["Der", "Son\u00b7nen", "Pur\u00b7pu\u00b7ran\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Aug' der Welt, das Flammenlicht,", "tokens": ["Das", "Aug'", "der", "Welt", ",", "das", "Flam\u00b7men\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Menschen Sinn erquicket.", "tokens": ["Der", "Men\u00b7schen", "Sinn", "er\u00b7quic\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schauet, s'thauet", "tokens": ["Schau\u00b7et", ",", "s't\u00b7hau\u00b7et"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Perlenthr\u00e4nen,", "tokens": ["Per\u00b7lent\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Zu besch\u00f6nen", "tokens": ["Zu", "be\u00b7sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Unsre Heiden,", "tokens": ["Uns\u00b7re", "Hei\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Die mit fettem Klee sich kleiden.", "tokens": ["Die", "mit", "fet\u00b7tem", "Klee", "sich", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es singt der Vogel in der Luft,", "tokens": ["Es", "singt", "der", "Vo\u00b7gel", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df widerschallt der Th\u00e4ler Gruft,", "tokens": ["Da\u00df", "wi\u00b7der\u00b7schallt", "der", "Th\u00e4\u00b7ler", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem h\u00f6chsten Gott zu Ehren,", "tokens": ["Dem", "h\u00f6chs\u00b7ten", "Gott", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der allem Fleisch zu rechter Zeit", "tokens": ["Der", "al\u00b7lem", "Fleisch", "zu", "rech\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat sein begn\u00fcgtes Mahl bereit',", "tokens": ["Hat", "sein", "be\u00b7gn\u00fcg\u00b7tes", "Mahl", "be\u00b7reit'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Pflegt alles Heer zu n\u00e4hren.", "tokens": ["Pflegt", "al\u00b7les", "Heer", "zu", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Felder, W\u00e4lder,", "tokens": ["Fel\u00b7der", ",", "W\u00e4l\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Was ihr heget,", "tokens": ["Was", "ihr", "he\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Was sich reget", "tokens": ["Was", "sich", "re\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PRF", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Hier und oben,", "tokens": ["Hier", "und", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Soll den Sch\u00f6pfer stetig loben.", "tokens": ["Soll", "den", "Sch\u00f6p\u00b7fer", "ste\u00b7tig", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleichwie der Blumenbl\u00e4ttlein Schrein", "tokens": ["Gleich\u00b7wie", "der", "Blu\u00b7men\u00b7bl\u00e4tt\u00b7lein", "Schrein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zertheilt der warme Sonnenschein,", "tokens": ["Zer\u00b7theilt", "der", "war\u00b7me", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie g\u00e4nzlich zu erquicken,", "tokens": ["Sie", "g\u00e4nz\u00b7lich", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So soll auch mein verd\u00fcstert Herz", "tokens": ["So", "soll", "auch", "mein", "ver\u00b7d\u00fcs\u00b7tert", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich \u00f6ffnen, da\u00df des Geistes Kerz'", "tokens": ["Sich", "\u00f6ff\u00b7nen", ",", "da\u00df", "des", "Geis\u00b7tes", "Ker\u00b7z'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kann seinen Schrein durchblicken.", "tokens": ["Kann", "sei\u00b7nen", "Schrein", "durch\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "R\u00fchrend, zierend,", "tokens": ["R\u00fch\u00b7rend", ",", "zie\u00b7rend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Da\u00df es Gaben", "tokens": ["Da\u00df", "es", "Ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "M\u00f6ge haben,", "tokens": ["M\u00f6\u00b7ge", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die vor allen", "tokens": ["Die", "vor", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "PIAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Gott und Menschen wohlgefallen.", "tokens": ["Gott", "und", "Men\u00b7schen", "wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herr, hilf, da\u00df ich auch diesen Tag,", "tokens": ["Herr", ",", "hilf", ",", "da\u00df", "ich", "auch", "die\u00b7sen", "Tag", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "$,", "KOUS", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und so lang' ich noch leben mag,", "tokens": ["Und", "so", "lang'", "ich", "noch", "le\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mein Amt getreu verrichte,", "tokens": ["Mein", "Amt", "ge\u00b7treu", "ver\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich auf deinen Wegen geh',", "tokens": ["Da\u00df", "ich", "auf", "dei\u00b7nen", "We\u00b7gen", "geh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aller S\u00fcnde m\u00fc\u00dfig steh',", "tokens": ["Und", "al\u00b7ler", "S\u00fcn\u00b7de", "m\u00fc\u00b7\u00dfig", "steh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "All' Eitelkeit vernichte,", "tokens": ["All'", "Ei\u00b7tel\u00b7keit", "ver\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wann kommt dann", "tokens": ["Und", "wann", "kommt", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Tod und Sterben,", "tokens": ["Tod", "und", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "La\u00df mich erben", "tokens": ["La\u00df", "mich", "er\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Und empfangen,", "tokens": ["Und", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Was die Frommen all' erlangen.", "tokens": ["Was", "die", "From\u00b7men", "all'", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}