{"textgrid.poem.51139": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Jesuitenzug", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hussa! hussa! die Hatz geht los!", "tokens": ["Hus\u00b7sa", "!", "hus\u00b7sa", "!", "die", "Hatz", "geht", "los", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Es kommt geritten klein und gro\u00df,", "tokens": ["Es", "kommt", "ge\u00b7rit\u00b7ten", "klein", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das springt und purzelt gar behend,", "tokens": ["Das", "springt", "und", "pur\u00b7zelt", "gar", "be\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Das kreischt und zetert ohne End:", "tokens": ["Das", "kreischt", "und", "ze\u00b7tert", "oh\u00b7ne", "End", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da reiten sie auf Schl\u00e4ngelein", "tokens": ["Da", "rei\u00b7ten", "sie", "auf", "Schl\u00e4n\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hintendrein auf Drach und Schwein;", "tokens": ["Und", "hin\u00b7ten\u00b7drein", "auf", "Drach", "und", "Schwein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was das f\u00fcr muntre Bursche sind!", "tokens": ["Was", "das", "f\u00fcr", "mun\u00b7tre", "Bur\u00b7sche", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl graut im Mutterleib dem Kind:", "tokens": ["Wohl", "graut", "im", "Mut\u00b7ter\u00b7leib", "dem", "Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Hu, wie das krabbelt, kneipt und kriecht,", "tokens": ["Hu", ",", "wie", "das", "krab\u00b7belt", ",", "kneipt", "und", "kriecht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Pfui, wie's so infernalisch riecht!", "tokens": ["Pfui", ",", "wie's", "so", "in\u00b7fer\u00b7na\u00b7lisch", "riecht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt fahre hin, du gute Ruh!", "tokens": ["Jetzt", "fah\u00b7re", "hin", ",", "du", "gu\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geh, Grete, mach das Fenster zu:", "tokens": ["Geh", ",", "Gre\u00b7te", ",", "mach", "das", "Fens\u00b7ter", "zu", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbgewissen, Ehr und Treue nehmt", "tokens": ["\u00bb", "ge\u00b7wis\u00b7sen", ",", "Ehr", "und", "Treu\u00b7e", "nehmt"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Mann und macht ihn ausversch\u00e4mt,", "tokens": ["Dem", "Mann", "und", "macht", "ihn", "aus\u00b7ver\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seines Weibes Unterrock", "tokens": ["Und", "sei\u00b7nes", "Wei\u00b7bes", "Un\u00b7ter\u00b7rock"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4ngt ihm als Fahne an den Stock:", "tokens": ["H\u00e4ngt", "ihm", "als", "Fah\u00b7ne", "an", "den", "Stock", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir kommen, die Jesuiten!\u00ab", "tokens": ["Wir", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Von Kreuz und Fahne angef\u00fchrt,", "tokens": ["Von", "Kreuz", "und", "Fah\u00b7ne", "an\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Giftsack hinten aufgeschn\u00fcrt,", "tokens": ["Den", "Gift\u00b7sack", "hin\u00b7ten", "auf\u00b7ge\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Fanatismus ist Profo\u00df,", "tokens": ["Der", "Fa\u00b7na\u00b7tis\u00b7mus", "ist", "Pro\u00b7fo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Dummheit folgt als Betteltro\u00df:", "tokens": ["Die", "Dumm\u00b7heit", "folgt", "als", "Bet\u00b7tel\u00b7tro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbwir nisten uns im Niederleib", "tokens": ["\u00bb", "wir", "nis\u00b7ten", "uns", "im", "Nie\u00b7der\u00b7leib"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Maden ein bei Mann und Weib,", "tokens": ["Wie", "Ma\u00b7den", "ein", "bei", "Mann", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ein Schwein erfinden kann,", "tokens": ["Und", "was", "ein", "Schwein", "er\u00b7fin\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das bringen wir an Weib und Mann:", "tokens": ["Das", "brin\u00b7gen", "wir", "an", "Weib", "und", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir kommen, die Jesuiten!\u00ab", "tokens": ["Wir", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "O gutes Land, du sch\u00f6ne Braut,", "tokens": ["O", "gu\u00b7tes", "Land", ",", "du", "sch\u00f6\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst dem Teufel angetraut!", "tokens": ["Du", "wirst", "dem", "Teu\u00b7fel", "an\u00b7ge\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, weine nur, du armes Kind!", "tokens": ["Ja", ",", "wei\u00b7ne", "nur", ",", "du", "ar\u00b7mes", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Gotthard weht ein schlimmer Wind:", "tokens": ["Vom", "Got\u00b7thard", "weht", "ein", "schlim\u00b7mer", "Wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kommen, die Jesuiten!", "tokens": ["Sie", "kom\u00b7men", ",", "die", "Je\u00b7su\u00b7i\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}