{"dta.poem.8420": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Weiberfeind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.71", "af:0.28"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Augenpaar,", "tokens": ["Kein", "Au\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Aus welchem tausend Engel blicken,", "tokens": ["Aus", "wel\u00b7chem", "tau\u00b7send", "En\u00b7gel", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Kein blondes Haar!", "tokens": ["Kein", "blon\u00b7des", "Haar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Kein Mund, um den das L\u00e4cheln schwebet,", "tokens": ["Kein", "Mund", ",", "um", "den", "das", "L\u00e4\u00b7cheln", "schwe\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUI", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und keine Brust,", "tokens": ["Und", "kei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Von d\u00fcnnem Silberflor umwebet,", "tokens": ["Von", "d\u00fcn\u00b7nem", "Sil\u00b7ber\u00b7flor", "um\u00b7we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcllt mich mit Lust!", "tokens": ["F\u00fcllt", "mich", "mit", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ein Wuchs, den Venus selber neidet,", "tokens": ["Ein", "Wuchs", ",", "den", "Ve\u00b7nus", "sel\u00b7ber", "nei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eine Hand,", "tokens": ["Und", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Persien in Perlen kleidet,", "tokens": ["Die", "Per\u00b7si\u00b7en", "in", "Per\u00b7len", "klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Kindertand!", "tokens": ["Ist", "Kin\u00b7der\u00b7tand", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich sollte mich darein vergaffen?", "tokens": ["Ich", "soll\u00b7te", "mich", "da\u00b7rein", "ver\u00b7gaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ei grossen Dank!", "tokens": ["Ei", "gros\u00b7sen", "Dank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ich werde nicht, wie junge Laffen,", "tokens": ["Ich", "wer\u00b7de", "nicht", ",", "wie", "jun\u00b7ge", "Laf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor Liebe krank!", "tokens": ["Vor", "Lie\u00b7be", "krank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mir ward ein Herz von Eis beschieden,", "tokens": ["Mir", "ward", "ein", "Herz", "von", "Eis", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Felsensinn!", "tokens": ["Ein", "Fel\u00b7sen\u00b7sinn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Drum wandl' ich auch in s\u00fcssem Frieden", "tokens": ["Drum", "wandl'", "ich", "auch", "in", "s\u00fcs\u00b7sem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Leben hin;", "tokens": ["Durchs", "Le\u00b7ben", "hin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Geh immer, in der Brust den Himmel,", "tokens": ["Geh", "im\u00b7mer", ",", "in", "der", "Brust", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geraden Pfad;", "tokens": ["Ge\u00b7ra\u00b7den", "Pfad", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Durchtaumle niemals das Gewimmel", "tokens": ["Durch\u00b7taum\u00b7le", "nie\u00b7mals", "das", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der goldnen Stadt!", "tokens": ["Der", "gold\u00b7nen", "Stadt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und trink' in meiner Weinblattlaube", "tokens": ["Und", "trink'", "in", "mei\u00b7ner", "Wein\u00b7blatt\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den G\u00f6ttersaft", "tokens": ["Den", "G\u00f6t\u00b7ter\u00b7saft"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der r\u00f6thelnden Burgundertraube,", "tokens": ["Der", "r\u00f6\u00b7theln\u00b7den", "Bur\u00b7gun\u00b7der\u00b7trau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Voll Geist und Kraft!", "tokens": ["Voll", "Geist", "und", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Sollt' ich daf\u00fcr in Gallar\u00f6cken,", "tokens": ["Sollt'", "ich", "da\u00b7f\u00fcr", "in", "Gal\u00b7la\u00b7r\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Liebe krank,", "tokens": ["Vor", "Lie\u00b7be", "krank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Fr\u00e4ulein gn\u00e4dge H\u00e4nde lecken?", "tokens": ["Der", "Fr\u00e4u\u00b7lein", "gn\u00e4d\u00b7ge", "H\u00e4n\u00b7de", "le\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ei grossen Dank!", "tokens": ["Ei", "gros\u00b7sen", "Dank", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sollt' ich den Rosenkelch verlassen?", "tokens": ["Sollt'", "ich", "den", "Ro\u00b7sen\u00b7kelch", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nachtigall?", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf eines M\u00e4dchens Winke passen,", "tokens": ["Auf", "ei\u00b7nes", "M\u00e4d\u00b7chens", "Win\u00b7ke", "pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Spiel und Ball?", "tokens": ["Bei", "Spiel", "und", "Ball", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich w\u00fcrde, k\u00e4men ganze Gruppen", "tokens": ["Ich", "w\u00fcr\u00b7de", ",", "k\u00e4\u00b7men", "gan\u00b7ze", "Grup\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von M\u00e4dchen, traun!", "tokens": ["Von", "M\u00e4d\u00b7chen", ",", "traun", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nicht aus der Laube gehn, die Puppen", "tokens": ["Nicht", "aus", "der", "Lau\u00b7be", "gehn", ",", "die", "Pup\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur anzuschaun!", "tokens": ["Nur", "an\u00b7zu\u00b7schaun", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVIZU", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}