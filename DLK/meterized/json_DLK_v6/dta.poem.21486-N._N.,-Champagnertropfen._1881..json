{"dta.poem.21486": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Champagnertropfen.  \n 1881.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchlingsn\u00e4chtige Stunden \u2026", "tokens": ["Fr\u00fch\u00b7lings\u00b7n\u00e4ch\u00b7ti\u00b7ge", "Stun\u00b7den", "\u2026"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "M\u00e4chtig schwillt die Luft,", "tokens": ["M\u00e4ch\u00b7tig", "schwillt", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rings quillt aus k\u00fchlem Garten", "tokens": ["Rings", "quillt", "aus", "k\u00fch\u00b7lem", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Erde s\u00fc\u00dfer Duft.", "tokens": ["Der", "Er\u00b7de", "s\u00fc\u00b7\u00dfer", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In aufgebrochenen Schollen", "tokens": ["In", "auf\u00b7ge\u00b7bro\u00b7che\u00b7nen", "Schol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gestaltet sich\u2019s bunt und reich,", "tokens": ["Ge\u00b7stal\u00b7tet", "sich's", "bunt", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch\u2019s offene Fenster rankt sich", "tokens": ["Durch's", "of\u00b7fe\u00b7ne", "Fens\u00b7ter", "rankt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Keimendes Rebengezweig.", "tokens": ["Kei\u00b7men\u00b7des", "Re\u00b7ben\u00b7ge\u00b7zweig", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Ueber die Borde dr\u00e4ngt sich", "tokens": ["Ue\u00b7ber", "die", "Bor\u00b7de", "dr\u00e4ngt", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Das Wasser jach enteist,", "tokens": ["Das", "Was\u00b7ser", "jach", "ent\u00b7eist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aus dem Walde quillt es", "tokens": ["Und", "aus", "dem", "Wal\u00b7de", "quillt", "es"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Maienglockengeist.", "tokens": ["Wie", "Mai\u00b7en\u00b7glo\u00b7cken\u00b7geist", "."], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schwarz \u00fcber uns flattern die Wolken", "tokens": ["Schwarz", "\u00fc\u00b7ber", "uns", "flat\u00b7tern", "die", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie Banner in hei\u00dfer Schlacht,", "tokens": ["Wie", "Ban\u00b7ner", "in", "hei\u00b7\u00dfer", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als jagten fl\u00fcchtige Reiter", "tokens": ["Als", "jag\u00b7ten", "fl\u00fcch\u00b7ti\u00b7ge", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wund durch die dunkle Nacht.", "tokens": ["Wund", "durch", "die", "dunk\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die L\u00fcfte brausen und m\u00e4chtig", "tokens": ["Die", "L\u00fcf\u00b7te", "brau\u00b7sen", "und", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sausen sie hinterdrein,", "tokens": ["Sau\u00b7sen", "sie", "hin\u00b7ter\u00b7drein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So st\u00fcrmen siegjubelnde Reiter", "tokens": ["So", "st\u00fcr\u00b7men", "sieg\u00b7ju\u00b7beln\u00b7de", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In fluchtzerrissene Reih\u2019n.", "tokens": ["In", "flucht\u00b7zer\u00b7ris\u00b7se\u00b7ne", "Reih'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Fr\u00fchlingsn\u00e4chtiges Dr\u00e4ngen!", "tokens": ["Fr\u00fch\u00b7lings\u00b7n\u00e4ch\u00b7ti\u00b7ges", "Dr\u00e4n\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "K\u00fcsse mich, Sturmesmund \u2026", "tokens": ["K\u00fcs\u00b7se", "mich", ",", "Stur\u00b7mes\u00b7mund", "\u2026"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "K\u00fcsse die lodernde Stirne", "tokens": ["K\u00fcs\u00b7se", "die", "lo\u00b7dern\u00b7de", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und k\u00fcsse mich gesund!", "tokens": ["Und", "k\u00fcs\u00b7se", "mich", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sieh\u2019, zischend st\u00fcrzt der Champagner", "tokens": ["Sieh'", ",", "zi\u00b7schend", "st\u00fcrzt", "der", "Cham\u00b7pag\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVPP", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mir in das blanke Glas \u2026", "tokens": ["Mir", "in", "das", "blan\u00b7ke", "Glas", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir bring\u2019 ich mit jubelndem Munde", "tokens": ["Dir", "bring'", "ich", "mit", "ju\u00b7beln\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das spr\u00fchende blitzende Na\u00df.", "tokens": ["Das", "spr\u00fc\u00b7hen\u00b7de", "blit\u00b7zen\u00b7de", "Na\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Nicht in der staubigen Flasche", "tokens": ["Nicht", "in", "der", "stau\u00b7bi\u00b7gen", "Fla\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Vermodern mag solch ein Wein, \u2026", "tokens": ["Ver\u00b7mo\u00b7dern", "mag", "solch", "ein", "Wein", ",", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PIAT", "ART", "NN", "$,", "$("], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "In die Adern des Fr\u00fchlings verlodern,", "tokens": ["In", "die", "A\u00b7dern", "des", "Fr\u00fch\u00b7lings", "ver\u00b7lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "In die St\u00fcrme will er hinein.", "tokens": ["In", "die", "St\u00fcr\u00b7me", "will", "er", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Leuchtend in den L\u00fcften", "tokens": ["Leuch\u00b7tend", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zerspr\u00fcht die gold\u2019ne Fluth \u2026", "tokens": ["Zer\u00b7spr\u00fcht", "die", "gold'\u00b7ne", "Fluth", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun mische dich, Sonnenfeuer,", "tokens": ["Nun", "mi\u00b7sche", "dich", ",", "Son\u00b7nen\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit des Fr\u00fchlings Rosenblut.", "tokens": ["Mit", "des", "Fr\u00fch\u00b7lings", "Ro\u00b7sen\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sei k\u00f6stlicher Samen dem Boden,", "tokens": ["Sei", "k\u00f6st\u00b7li\u00b7cher", "Sa\u00b7men", "dem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df, wo ein Tropfen flie\u00dft,", "tokens": ["Da\u00df", ",", "wo", "ein", "Trop\u00b7fen", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald duftend und flammenlockig", "tokens": ["Bald", "duf\u00b7tend", "und", "flam\u00b7men\u00b7lo\u00b7ckig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "KON", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Eine Rose leuchtend entsprie\u00dft \u2026", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", "leuch\u00b7tend", "ent\u00b7sprie\u00dft", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Ein \u00fcppiger Bl\u00fcthenschleier", "tokens": ["Ein", "\u00fcp\u00b7pi\u00b7ger", "Bl\u00fc\u00b7then\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinflute \u00fcber das Land,", "tokens": ["Hin\u00b7flu\u00b7te", "\u00fc\u00b7ber", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie ein von Gott gewobnes", "tokens": ["Wie", "ein", "von", "Gott", "ge\u00b7wob\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "APPR", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Strahlendes Gewand.", "tokens": ["Strah\u00b7len\u00b7des", "Ge\u00b7wand", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Und wenn sich zwei begegnen", "tokens": ["Und", "wenn", "sich", "zwei", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "CARD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In solchem Blumenhain,", "tokens": ["In", "sol\u00b7chem", "Blu\u00b7men\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann ziehe klingend die Liebe", "tokens": ["Dann", "zie\u00b7he", "klin\u00b7gend", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In ihre Herzen hinein.", "tokens": ["In", "ih\u00b7re", "Her\u00b7zen", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}