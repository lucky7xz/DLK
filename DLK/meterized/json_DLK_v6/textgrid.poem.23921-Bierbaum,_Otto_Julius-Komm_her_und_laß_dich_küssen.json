{"textgrid.poem.23921": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Komm her und la\u00df dich k\u00fcssen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Luft ist wie voll Geigen,", "tokens": ["Die", "Luft", "ist", "wie", "voll", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allen Bl\u00fctenzweigen", "tokens": ["Von", "al\u00b7len", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das wei\u00dfe Wunder schneit;", "tokens": ["Das", "wei\u00b7\u00dfe", "Wun\u00b7der", "schneit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Fr\u00fchling tobt im Blute,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "tobt", "im", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu allem Uebermute", "tokens": ["Zu", "al\u00b7lem", "Ue\u00b7ber\u00b7mu\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist jetzt die allerbeste Zeit.", "tokens": ["Ist", "jetzt", "die", "al\u00b7ler\u00b7bes\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Komm her und la\u00df dich k\u00fcssen!", "tokens": ["Komm", "her", "und", "la\u00df", "dich", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wirst es dulden m\u00fcssen,", "tokens": ["Du", "wirst", "es", "dul\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dich mein Arm umschlingt.", "tokens": ["Da\u00df", "dich", "mein", "Arm", "um\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es geht durch alles Leben", "tokens": ["Es", "geht", "durch", "al\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Pochen und ein Beben:", "tokens": ["Ein", "Po\u00b7chen", "und", "ein", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das rote Blut, es singt, es singt.", "tokens": ["Das", "ro\u00b7te", "Blut", ",", "es", "singt", ",", "es", "singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}