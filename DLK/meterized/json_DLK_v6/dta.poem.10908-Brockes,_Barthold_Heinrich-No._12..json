{"dta.poem.10908": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sieht dem ", "tokens": ["Es", "sieht", "dem"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die ernste Stellung zeigt des ", "tokens": ["Die", "erns\u00b7te", "Stel\u00b7lung", "zeigt", "des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Da\u00df sie des Hirsches Brunst ein Vorwurf sey gewesen.", "tokens": ["Da\u00df", "sie", "des", "Hir\u00b7sches", "Brunst", "ein", "Vor\u00b7wurf", "sey", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das schlanke ", "tokens": ["Das", "schlan\u00b7ke"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Jedoch die\u00df Kupfer-St\u00fcck zeigt uns das Wild nicht nur,", "tokens": ["Je\u00b7doch", "die\u00df", "Kup\u00b7fer\u00b7St\u00fcck", "zeigt", "uns", "das", "Wild", "nicht", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es zeigt die\u00df Blatt ein Blatt des Buchs der Creatur.", "tokens": ["Es", "zeigt", "die\u00df", "Blatt", "ein", "Blatt", "des", "Buchs", "der", "Crea\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Des K\u00fcnstlers Lettern sind Figuren, die er f\u00fcget,", "tokens": ["Des", "K\u00fcnst\u00b7lers", "Let\u00b7tern", "sind", "Fi\u00b7gu\u00b7ren", ",", "die", "er", "f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df eine solche Schrift daraus entsteht,", "tokens": ["Da\u00df", "ei\u00b7ne", "sol\u00b7che", "Schrift", "da\u00b7raus", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Worinn so Zug als Text des Sch\u00f6pfers Ruhm erh\u00f6ht,", "tokens": ["Wo\u00b7rinn", "so", "Zug", "als", "Text", "des", "Sch\u00f6p\u00b7fers", "Ruhm", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KOUS", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn jemand, der im Sehn sie liest, sich dran vergn\u00fcget,", "tokens": ["Wenn", "je\u00b7mand", ",", "der", "im", "Sehn", "sie", "liest", ",", "sich", "dran", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "APPRART", "VVFIN", "PPER", "VVFIN", "$,", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durchs Bild zum Urbild erst, und dann zur Urquell geht.", "tokens": ["Durchs", "Bild", "zum", "Ur\u00b7bild", "erst", ",", "und", "dann", "zur", "Ur\u00b7quell", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ADV", "$,", "KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wen nun der sch\u00f6ne Stich und Kupferdruck behaget,", "tokens": ["Wen", "nun", "der", "sch\u00f6\u00b7ne", "Stich", "und", "Kup\u00b7fer\u00b7druck", "be\u00b7ha\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der mu\u00df von der Copie die Zeichnung selbst erst sehn;", "tokens": ["Der", "mu\u00df", "von", "der", "Co\u00b7pie", "die", "Zeich\u00b7nung", "selbst", "erst", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dann wird er, nicht allein, da\u00df ich nicht gnug gesaget,", "tokens": ["Dann", "wird", "er", ",", "nicht", "al\u00b7lein", ",", "da\u00df", "ich", "nicht", "gnug", "ge\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Auch da\u00df man nicht davon gnug sagen kann, gestehn.", "tokens": ["Auch", "da\u00df", "man", "nicht", "da\u00b7von", "gnug", "sa\u00b7gen", "kann", ",", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PTKNEG", "PAV", "ADV", "VVINF", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}