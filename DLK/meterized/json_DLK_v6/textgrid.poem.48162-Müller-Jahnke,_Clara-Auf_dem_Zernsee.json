{"textgrid.poem.48162": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Zernsee", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n bist du auch im fahlen Glanze,", "tokens": ["Sch\u00f6n", "bist", "du", "auch", "im", "fah\u00b7len", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "im Nebelschleier licht wie Schnee,", "tokens": ["im", "Ne\u00b7bel\u00b7schlei\u00b7er", "licht", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in hoher Pappeln Silberkranze,", "tokens": ["in", "ho\u00b7her", "Pap\u00b7peln", "Sil\u00b7ber\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du Traum der Mark, mein Havelsee!", "tokens": ["du", "Traum", "der", "Mark", ",", "mein", "Ha\u00b7vel\u00b7see", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie schmiegt ein Mantel \u2013 weich und l\u00fcstern \u2013", "tokens": ["Wie", "schmiegt", "ein", "Man\u00b7tel", "\u2013", "weich", "und", "l\u00fcs\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$(", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "rotgoldnes Rohr der Flut sich an!", "tokens": ["rot\u00b7gold\u00b7nes", "Rohr", "der", "Flut", "sich", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Binse braune Rispen fl\u00fcstern", "tokens": ["Der", "Bin\u00b7se", "brau\u00b7ne", "Ris\u00b7pen", "fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und schmeicheln sanft um meinen Kahn.", "tokens": ["und", "schmei\u00b7cheln", "sanft", "um", "mei\u00b7nen", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich schrillt wie lautes Weinen", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "schrillt", "wie", "lau\u00b7tes", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein Schwanenschrei durch all die Pracht;", "tokens": ["ein", "Schwa\u00b7nen\u00b7schrei", "durch", "all", "die", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein blau geheimnisvolles Scheinen", "tokens": ["ein", "blau", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7les", "Schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "erleuchtet deiner Tiefe Nacht.", "tokens": ["er\u00b7leuch\u00b7tet", "dei\u00b7ner", "Tie\u00b7fe", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und schwirrend hebt der Ost die Fl\u00fcgel,", "tokens": ["Und", "schwir\u00b7rend", "hebt", "der", "Ost", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die letzten Schleier l\u00f6st er bald \u2013", "tokens": ["die", "letz\u00b7ten", "Schlei\u00b7er", "l\u00f6st", "er", "bald", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "aufflammend gr\u00fc\u00dft vom Uferh\u00fcgel", "tokens": ["auf\u00b7flam\u00b7mend", "gr\u00fc\u00dft", "vom", "U\u00b7fer\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "sein buntes Bild der Apfelwald.", "tokens": ["sein", "bun\u00b7tes", "Bild", "der", "Ap\u00b7fel\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So hat ein Himmelshauch entsiegelt", "tokens": ["So", "hat", "ein", "Him\u00b7mels\u00b7hauch", "ent\u00b7sie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "des Herbstes F\u00fclle auch f\u00fcr mich,", "tokens": ["des", "Herbs\u00b7tes", "F\u00fcl\u00b7le", "auch", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und meiner Tage Reife spiegelt", "tokens": ["und", "mei\u00b7ner", "Ta\u00b7ge", "Rei\u00b7fe", "spie\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "in deinem klaren Auge sich.", "tokens": ["in", "dei\u00b7nem", "kla\u00b7ren", "Au\u00b7ge", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}