{"textgrid.poem.48242": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Ja, das m\u00f6cht' ich noch erleben", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eigentlich ist mir alles gleich,", "tokens": ["Ei\u00b7gent\u00b7lich", "ist", "mir", "al\u00b7les", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine wird arm, der andre wird reich,", "tokens": ["Der", "ei\u00b7ne", "wird", "arm", ",", "der", "and\u00b7re", "wird", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADJD", "$,", "PRELS", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aber mit Bismarck \u2013 was wird das noch geben?", "tokens": ["A\u00b7ber", "mit", "Bis\u00b7marck", "\u2013", "was", "wird", "das", "noch", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$(", "PWS", "VAFIN", "PDS", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Das mit Bismarck, das m\u00f6cht' ich noch erleben.", "tokens": ["Das", "mit", "Bis\u00b7marck", ",", "das", "m\u00f6cht'", "ich", "noch", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Eigentlich ist alles soso,", "tokens": ["Ei\u00b7gent\u00b7lich", "ist", "al\u00b7les", "so\u00b7so", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+--+-++", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heute traurig, morgen froh,", "tokens": ["Heu\u00b7te", "trau\u00b7rig", ",", "mor\u00b7gen", "froh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fchling, Sommer, Herbst und Winter,", "tokens": ["Fr\u00fch\u00b7ling", ",", "Som\u00b7mer", ",", "Herbst", "und", "Win\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, es ist nicht viel dahinter.", "tokens": ["Ach", ",", "es", "ist", "nicht", "viel", "da\u00b7hin\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber mein Enkel, so viel ist richtig,", "tokens": ["A\u00b7ber", "mein", "En\u00b7kel", ",", "so", "viel", "ist", "rich\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wird mit n\u00e4chstem vorschulpflichtig,", "tokens": ["Wird", "mit", "n\u00e4chs\u00b7tem", "vor\u00b7schul\u00b7pflich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in etwa vierzehn Tagen", "tokens": ["Und", "in", "et\u00b7wa", "vier\u00b7zehn", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wird er eine Mappe tragen,", "tokens": ["Wird", "er", "ei\u00b7ne", "Map\u00b7pe", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00f6schbl\u00e4tter will ich ins Heft ihm kleben \u2013", "tokens": ["L\u00f6schbl\u00e4t\u00b7ter", "will", "ich", "ins", "Heft", "ihm", "kle\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Ja, das m\u00f6cht' ich noch erleben.", "tokens": ["Ja", ",", "das", "m\u00f6cht'", "ich", "noch", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eigentlich ist alles nichts,", "tokens": ["Ei\u00b7gent\u00b7lich", "ist", "al\u00b7les", "nichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute h\u00e4lt's, und morgen bricht's,", "tokens": ["Heu\u00b7te", "h\u00e4lt's", ",", "und", "mor\u00b7gen", "bricht's", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hin stirbt alles, ganz geringe", "tokens": ["Hin", "stirbt", "al\u00b7les", ",", "ganz", "ge\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "$,", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Wert der ird'schen Dinge;", "tokens": ["Wird", "der", "Wert", "der", "ird'\u00b7schen", "Din\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch wie tief herabgestimmt", "tokens": ["Doch", "wie", "tief", "her\u00b7ab\u00b7ge\u00b7stimmt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch das W\u00fcnschen Abschied nimmt,", "tokens": ["Auch", "das", "W\u00fcn\u00b7schen", "Ab\u00b7schied", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer klingt es noch daneben:", "tokens": ["Im\u00b7mer", "klingt", "es", "noch", "da\u00b7ne\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja, das m\u00f6cht' ich noch erleben.", "tokens": ["Ja", ",", "das", "m\u00f6cht'", "ich", "noch", "er\u00b7le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}