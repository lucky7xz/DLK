{"dta.poem.13283": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo f\u00fchrst du mich nun hin? Thalia! sagt' ich ihr;", "tokens": ["Wo", "f\u00fchrst", "du", "mich", "nun", "hin", "?", "Tha\u00b7lia", "!", "sagt'", "ich", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$.", "NE", "$.", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir seynd am Ende. \u201eNein\u201e, sprach sie, wir", "tokens": ["Wir", "seynd", "am", "En\u00b7de", ".", "\u201e", "Nein", "\u201e", ",", "sprach", "sie", ",", "wir"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "$(", "PTKANT", "$(", "$,", "VVFIN", "PPER", "$,", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "bleiben hier,", "tokens": ["blei\u00b7ben", "hier", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "\u201edes Saals Vortrefflichkeit und Bau-Kunst zu", "tokens": ["\u201e", "des", "Saals", "Vor\u00b7treff\u00b7lich\u00b7keit", "und", "Bau\u00b7Kunst", "zu"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "betrachten.", "tokens": ["be\u00b7trach\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Es waren Farb und Gold die uns erstaunen machten.", "tokens": ["Es", "wa\u00b7ren", "Farb", "und", "Gold", "die", "uns", "er\u00b7stau\u00b7nen", "mach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "ART", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Fast jede Stelle wies, da\u00df da die Meister-Hand", "tokens": ["Fast", "je\u00b7de", "Stel\u00b7le", "wies", ",", "da\u00df", "da", "die", "Meis\u00b7ter\u00b7Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Kostbarkeit des Wercks durch Arbeit \u00fcberwand.", "tokens": ["Die", "Kost\u00b7bar\u00b7keit", "des", "Wercks", "durch", "Ar\u00b7beit", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Pensel aber wars, der uns am meisten r\u00fchrte,", "tokens": ["Der", "Pen\u00b7sel", "a\u00b7ber", "wars", ",", "der", "uns", "am", "meis\u00b7ten", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "PRELS", "PPER", "PTKA", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indem er \u00fcberall die ersten Pl\u00e4ze zierte.", "tokens": ["In\u00b7dem", "er", "\u00fc\u00b7be\u00b7rall", "die", "ers\u00b7ten", "Pl\u00e4\u00b7ze", "zier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er zeigte seine Macht und Kunst an jeder Wand,", "tokens": ["Er", "zeig\u00b7te", "sei\u00b7ne", "Macht", "und", "Kunst", "an", "je\u00b7der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "10Wo sich nicht ein von Gold geschniztes Werck befand.", "tokens": ["sich", "nicht", "ein", "von", "Gold", "ge\u00b7schniz\u00b7tes", "Werck", "be\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ART", "APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Wir schauten manches Bild, dem es nur an der Sprache,", "tokens": ["Wir", "schau\u00b7ten", "man\u00b7ches", "Bild", ",", "dem", "es", "nur", "an", "der", "Spra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann sonst die Leibs-Gestallt beweglich schien, gebrache.", "tokens": ["Dann", "sonst", "die", "Leibs\u00b7Ge\u00b7stallt", "be\u00b7weg\u00b7lich", "schien", ",", "ge\u00b7bra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Allein Thalia gab mehr auf was anders acht,", "tokens": ["Al\u00b7lein", "Tha\u00b7lia", "gab", "mehr", "auf", "was", "an\u00b7ders", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADV", "APPR", "PRELS", "ADV", "CARD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "So da\u00df sie endlich mich aus meiner Freude bracht:", "tokens": ["So", "da\u00df", "sie", "end\u00b7lich", "mich", "aus", "mei\u00b7ner", "Freu\u00b7de", "bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}