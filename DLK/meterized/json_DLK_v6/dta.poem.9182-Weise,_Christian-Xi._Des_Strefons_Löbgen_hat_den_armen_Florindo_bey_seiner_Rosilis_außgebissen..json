{"dta.poem.9182": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n Des Strefons L\u00f6bgen hat den armen Florindo  \n bey seiner Rosilis au\u00dfgebissen.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich armer schwei\u00df! nun mu\u00df ich wandern/", "tokens": ["Ich", "ar\u00b7mer", "schwei\u00df", "!", "nun", "mu\u00df", "ich", "wan\u00b7dern", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das gl\u00fccke sieht mich sauer an/", "tokens": ["Das", "gl\u00fc\u00b7cke", "sieht", "mich", "sau\u00b7er", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und k\u00f6mmt nunmehr zu einem andern/", "tokens": ["Und", "k\u00f6mmt", "nun\u00b7mehr", "zu", "ei\u00b7nem", "an\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der besser courtisiren kan/", "tokens": ["Der", "bes\u00b7ser", "cour\u00b7ti\u00b7si\u00b7ren", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein freuden-kleid ist gar zerrissen/", "tokens": ["Mein", "freu\u00b7den\u00b7kleid", "ist", "gar", "zer\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das L\u00f6bgen hat mich au\u00dfgebissen.", "tokens": ["Das", "L\u00f6b\u00b7gen", "hat", "mich", "au\u00df\u00b7ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Was hilffts! sol ich mich lange kr\u00e4ncken?", "tokens": ["Was", "hilffts", "!", "sol", "ich", "mich", "lan\u00b7ge", "kr\u00e4n\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich darff an die vergangne zeit", "tokens": ["Ich", "darff", "an", "die", "ver\u00b7gang\u00b7ne", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nicht so k\u00fchnlich wieder dencken/", "tokens": ["Doch", "nicht", "so", "k\u00fchn\u00b7lich", "wie\u00b7der", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil ich doch in ewigkeit", "tokens": ["Die\u00b7weil", "ich", "doch", "in", "e\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den possen werd erdulden m\u00fcssen/", "tokens": ["Den", "pos\u00b7sen", "werd", "er\u00b7dul\u00b7den", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mich ein hund hat au\u00dfgebissen.", "tokens": ["Da\u00df", "mich", "ein", "hund", "hat", "au\u00df\u00b7ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ja bei\u00df/ da\u00df dir die z\u00e4hne krachen/", "tokens": ["Ja", "bei\u00df", "/", "da\u00df", "dir", "die", "z\u00e4h\u00b7ne", "kra\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du elementsches raben-aas/", "tokens": ["Du", "e\u00b7le\u00b7ment\u00b7sches", "ra\u00b7ben\u00b7aas", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wilst du dich an die herren machen/", "tokens": ["Wilst", "du", "dich", "an", "die", "her\u00b7ren", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So w\u00fcnsch ich dir/ ich wei\u00df nicht was:", "tokens": ["So", "w\u00fcnsch", "ich", "dir", "/", "ich", "wei\u00df", "nicht", "was", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich m\u00f6cht es hertzlich gerne wissen/", "tokens": ["Ich", "m\u00f6cht", "es", "hertz\u00b7lich", "ger\u00b7ne", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Warum du mich hast ausgebissen.", "tokens": ["Wa\u00b7rum", "du", "mich", "hast", "aus\u00b7ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ach bei\u00df die kerlen vor zu todte/", "tokens": ["Ach", "bei\u00df", "die", "ker\u00b7len", "vor", "zu", "tod\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "APPR", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir nach meiner liebste stehn/", "tokens": ["Die", "mir", "nach", "mei\u00b7ner", "liebs\u00b7te", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die wir auff die neue mode", "tokens": ["Und", "die", "wir", "auff", "die", "neu\u00b7e", "mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6n in meinem haber gehn:", "tokens": ["So", "sch\u00f6n", "in", "mei\u00b7nem", "ha\u00b7ber", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So hast du dich darauff beflissen/", "tokens": ["So", "hast", "du", "dich", "dar\u00b7auff", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich am ersten au\u00dfgebissen.", "tokens": ["Und", "mich", "am", "ers\u00b7ten", "au\u00df\u00b7ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Die hunde fahren sonst die diebe/", "tokens": ["Die", "hun\u00b7de", "fah\u00b7ren", "sonst", "die", "die\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht aber meines gleichen an/", "tokens": ["Nicht", "a\u00b7ber", "mei\u00b7nes", "glei\u00b7chen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Di\u00df hast du deinem Herrn zu liebe", "tokens": ["Di\u00df", "hast", "du", "dei\u00b7nem", "Herrn", "zu", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir zum schabernack gethan/", "tokens": ["Und", "mir", "zum", "scha\u00b7ber\u00b7nack", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum werd ichs auch zu r\u00fchmen wissen/", "tokens": ["Drum", "werd", "ichs", "auch", "zu", "r\u00fch\u00b7men", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "ADV", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du hast mich unrecht ausgebissen.", "tokens": ["Du", "hast", "mich", "un\u00b7recht", "aus\u00b7ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Nun mu\u00df ich manchen praler h\u00f6ren", "tokens": ["Nun", "mu\u00df", "ich", "man\u00b7chen", "pra\u00b7ler", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich hingegen eingeliebt/", "tokens": ["Der", "sich", "hin\u00b7ge\u00b7gen", "ein\u00b7ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie er spricht/ in allen ehren", "tokens": ["Und", "wie", "er", "spricht", "/", "in", "al\u00b7len", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$(", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein m\u00e4ulgen nach dem andern giebt/", "tokens": ["Ein", "m\u00e4ul\u00b7gen", "nach", "dem", "an\u00b7dern", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine frundschafft ist zerrissen/", "tokens": ["Und", "mei\u00b7ne", "frund\u00b7schafft", "ist", "zer\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weil mich der hund hat au\u00dfgebissen.", "tokens": ["Weil", "mich", "der", "hund", "hat", "au\u00df\u00b7ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Wolan ich gebe mich gefangen/", "tokens": ["Wo\u00b7lan", "ich", "ge\u00b7be", "mich", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin ein armer untermann.", "tokens": ["Ich", "bin", "ein", "ar\u00b7mer", "un\u00b7ter\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil ich nicht so trefflich prangen", "tokens": ["Und", "weil", "ich", "nicht", "so", "treff\u00b7lich", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch mein gel\u00fccke r\u00fchmen kan/", "tokens": ["Noch", "mein", "ge\u00b7l\u00fc\u00b7cke", "r\u00fch\u00b7men", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df ich auch den spa\u00df vermissen.", "tokens": ["So", "mu\u00df", "ich", "auch", "den", "spa\u00df", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df l\u00f6bgen hat mich au\u00dfgebissen.", "tokens": ["Da\u00df", "l\u00f6b\u00b7gen", "hat", "mich", "au\u00df\u00b7ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}