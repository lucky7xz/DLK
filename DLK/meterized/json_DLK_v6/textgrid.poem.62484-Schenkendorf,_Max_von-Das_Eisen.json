{"textgrid.poem.62484": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Das Eisen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Eisen, nur Eisen!", "tokens": ["Nur", "Ei\u00b7sen", ",", "nur", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gebt mir ein Schwert!", "tokens": ["Gebt", "mir", "ein", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich will mich beweisen", "tokens": ["Ich", "will", "mich", "be\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vaterlands werth.", "tokens": ["Va\u00b7ter\u00b7lands", "werth", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Mich zieht nach Gefechten", "tokens": ["Mich", "zieht", "nach", "Ge\u00b7fech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Freudiger Muth,", "tokens": ["Freu\u00b7di\u00b7ger", "Muth", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dem Wahren, dem Rechten", "tokens": ["Dem", "Wah\u00b7ren", ",", "dem", "Rech\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Geh\u00f6rt mein Blut.", "tokens": ["Ge\u00b7h\u00f6rt", "mein", "Blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hinein in Gefahren!", "tokens": ["Hin\u00b7ein", "in", "Ge\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Hinan zu den H\u00f6h'n,", "tokens": ["Hi\u00b7nan", "zu", "den", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo fr\u00f6hlich die Schaaren", "tokens": ["Wo", "fr\u00f6h\u00b7lich", "die", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Br\u00fcder stehn!", "tokens": ["Der", "Br\u00fc\u00b7der", "stehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Vergebens mein Streben,", "tokens": ["Ver\u00b7ge\u00b7bens", "mein", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Kampfbegier!", "tokens": ["Die", "Kampf\u00b7be\u00b7gier", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es zehret am Leben", "tokens": ["Es", "zeh\u00b7ret", "am", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Krankheit mir.", "tokens": ["Die", "Krank\u00b7heit", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Doch kann ich nicht lassen", "tokens": ["Doch", "kann", "ich", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vom Waffenhall,", "tokens": ["Vom", "Waf\u00b7fen\u00b7hall", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mu\u00df immer dich fassen,", "tokens": ["Mu\u00df", "im\u00b7mer", "dich", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Heilig Metall.", "tokens": ["Hei\u00b7lig", "Me\u00b7tall", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Macht nimmer dein Blinken", "tokens": ["Macht", "nim\u00b7mer", "dein", "Blin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Seele mir hell,", "tokens": ["Die", "See\u00b7le", "mir", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So will ich dich trinken", "tokens": ["So", "will", "ich", "dich", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im Sprudelquell.", "tokens": ["Im", "Spru\u00b7del\u00b7quell", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "O seht, wie es rauschet,", "tokens": ["O", "seht", ",", "wie", "es", "rau\u00b7schet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Perlet und g\u00e4hrt!", "tokens": ["Per\u00b7let", "und", "g\u00e4hrt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich habe gelauschet", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7lau\u00b7schet"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Am tiefsten Herd.", "tokens": ["Am", "tiefs\u00b7ten", "Herd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "So hell in der Sonne", "tokens": ["So", "hell", "in", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "W\u00e4chset der Wein;", "tokens": ["W\u00e4ch\u00b7set", "der", "Wein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auch unten, o Wonne!", "tokens": ["Auch", "un\u00b7ten", ",", "o", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "FM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Gibt's ein Gedeihn.", "tokens": ["Gibt's", "ein", "Ge\u00b7deihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Die Wasser, sie ringen", "tokens": ["Die", "Was\u00b7ser", ",", "sie", "rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sich freudig los,", "tokens": ["Sich", "freu\u00b7dig", "los", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Erze durchdringen", "tokens": ["Die", "Er\u00b7ze", "durch\u00b7drin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Erde Schoo\u00df.", "tokens": ["Der", "Er\u00b7de", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "So wirke von innen,", "tokens": ["So", "wir\u00b7ke", "von", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Du Eisenflut,", "tokens": ["Du", "Ei\u00b7sen\u00b7flut", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und st\u00e4hle mir Sinnen", "tokens": ["Und", "st\u00e4h\u00b7le", "mir", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Leib und Muth!", "tokens": ["Und", "Leib", "und", "Muth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Wie will ich dann stehen", "tokens": ["Wie", "will", "ich", "dann", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ein Eisenmann,", "tokens": ["Ein", "Ei\u00b7sen\u00b7mann", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Will eilen und gehen", "tokens": ["Will", "ei\u00b7len", "und", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zum K\u00e4mpferplan.", "tokens": ["Zum", "K\u00e4mp\u00b7fer\u00b7plan", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Die Unbilde r\u00e4chen", "tokens": ["Die", "Un\u00b7bil\u00b7de", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Am Schandgeschlecht,", "tokens": ["Am", "Schand\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und streiten und sprechen", "tokens": ["Und", "strei\u00b7ten", "und", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fcr Gott und Recht.", "tokens": ["F\u00fcr", "Gott", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "O heilige Wasser,", "tokens": ["O", "hei\u00b7li\u00b7ge", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Willkommen mir!", "tokens": ["Will\u00b7kom\u00b7men", "mir", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein liebender Hasser", "tokens": ["Ein", "lie\u00b7ben\u00b7der", "Has\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Trink' ich euch hier.", "tokens": ["Trink'", "ich", "euch", "hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}