{"textgrid.poem.46231": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An Herren Johan Abraham P\u00f6hmern", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das wol beschribne lob mit dem mich deine gunst,", "tokens": ["Das", "wol", "be\u00b7schrib\u00b7ne", "lob", "mit", "dem", "mich", "dei\u00b7ne", "gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "APPR", "PRELS", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mein P\u00f6hmer, meisterlich f\u00fcr andern herausstreichet,", "tokens": ["mein", "P\u00f6h\u00b7mer", ",", "meis\u00b7ter\u00b7lich", "f\u00fcr", "an\u00b7dern", "her\u00b7aus\u00b7strei\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "entdecket einen schatz der wissenschaft und kunst,", "tokens": ["ent\u00b7de\u00b7cket", "ei\u00b7nen", "schatz", "der", "wis\u00b7sen\u00b7schaft", "und", "kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "damit dein herz und geist gesegnet und bereichet.", "tokens": ["da\u00b7mit", "dein", "herz", "und", "geist", "ge\u00b7seg\u00b7net", "und", "be\u00b7rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "KON", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gleichwie nun solcher ruhm, mir zwar lieb, doch umsunst,", "tokens": ["Gleich\u00b7wie", "nun", "sol\u00b7cher", "ruhm", ",", "mir", "zwar", "lieb", ",", "doch", "um\u00b7sunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,", "PPER", "ADV", "ADJD", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dieweil sich mein verdienst dem deinen nicht vergleichet:", "tokens": ["die\u00b7weil", "sich", "mein", "ver\u00b7dienst", "dem", "dei\u00b7nen", "nicht", "ver\u00b7glei\u00b7chet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "VVFIN", "ART", "PPOSAT", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "also er\u00f6fnet sich ein so liebreiche brunst", "tokens": ["al\u00b7so", "er\u00b7\u00f6f\u00b7net", "sich", "ein", "so", "lieb\u00b7rei\u00b7che", "brunst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADV", "ADJA", "NN"], "meter": "+--+--+-++-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "in deiner edlen brust, die keinem reichtum weichet.", "tokens": ["in", "dei\u00b7ner", "ed\u00b7len", "brust", ",", "die", "kei\u00b7nem", "reich\u00b7tum", "wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Daher mit h\u00f6chstem dank mu\u00df ich dein haupt numehr", "tokens": ["Da\u00b7her", "mit", "h\u00f6chs\u00b7tem", "dank", "mu\u00df", "ich", "dein", "haupt", "nu\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ADJA", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "mit deinem eignen kranz und perlein wider zieren", "tokens": ["mit", "dei\u00b7nem", "eig\u00b7nen", "kranz", "und", "per\u00b7lein", "wi\u00b7der", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "ADJD", "PTKVZ", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "zu zeugnus meiner lieb und zeichen deiner lehr;", "tokens": ["zu", "zeug\u00b7nus", "mei\u00b7ner", "lieb", "und", "zei\u00b7chen", "dei\u00b7ner", "lehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Lehr, damit reichlich got gesegnet dein studieren,", "tokens": ["Lehr", ",", "da\u00b7mit", "reich\u00b7lich", "got", "ge\u00b7seg\u00b7net", "dein", "stu\u00b7die\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADJD", "ADJD", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "studieren, welches dir mittheilet lob und ehr,", "tokens": ["stu\u00b7die\u00b7ren", ",", "wel\u00b7ches", "dir", "mit\u00b7thei\u00b7let", "lob", "und", "ehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ehr, dadurch, P\u00f6hmer, du allzeit solt triumfieren.", "tokens": ["ehr", ",", "da\u00b7durch", ",", "P\u00f6h\u00b7mer", ",", "du", "all\u00b7zeit", "solt", "tri\u00b7um\u00b7fie\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PAV", "$,", "NN", "$,", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}