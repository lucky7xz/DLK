{"textgrid.poem.37314": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie ist ein reizendes Gesch\u00f6pfchen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist ein reizendes Gesch\u00f6pfchen,", "tokens": ["Sie", "ist", "ein", "rei\u00b7zen\u00b7des", "Ge\u00b7sch\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit allen Wassern wohl gewaschen;", "tokens": ["Mit", "al\u00b7len", "Was\u00b7sern", "wohl", "ge\u00b7wa\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kennt die s\u00fc\u00dfen S\u00fcndent\u00f6pfchen", "tokens": ["Sie", "kennt", "die", "s\u00fc\u00b7\u00dfen", "S\u00fcn\u00b7den\u00b7t\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und liebt es, h\u00e4ufig draus zu naschen.", "tokens": ["Und", "liebt", "es", ",", "h\u00e4u\u00b7fig", "draus", "zu", "na\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da bleibt den sittlich Hochgestellten", "tokens": ["Da", "bleibt", "den", "sitt\u00b7lich", "Hoch\u00b7ge\u00b7stell\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts weiter \u00fcbrig, als mit Freuden", "tokens": ["Nichts", "wei\u00b7ter", "\u00fcb\u00b7rig", ",", "als", "mit", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADV", "PTKVZ", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf diese Schandperson zu schelten", "tokens": ["Auf", "die\u00b7se", "Schand\u00b7per\u00b7son", "zu", "schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sie mit Schmerzen zu beneiden.", "tokens": ["Und", "sie", "mit", "Schmer\u00b7zen", "zu", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}