{"textgrid.poem.56519": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein L\u00e4cheln schiefen Grames, das verschwindet", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein L\u00e4cheln schiefen Grames, das verschwindet", "tokens": ["Ein", "L\u00e4\u00b7cheln", "schie\u00b7fen", "Gra\u00b7mes", ",", "das", "ver\u00b7schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NE", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinein in seiner Stirne wei\u00dfes Tor.", "tokens": ["Hin\u00b7ein", "in", "sei\u00b7ner", "Stir\u00b7ne", "wei\u00b7\u00dfes", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er sitzt auf seinem Stuhl. Seine H\u00e4nde erhoben", "tokens": ["Er", "sitzt", "auf", "sei\u00b7nem", "Stuhl", ".", "Sei\u00b7ne", "H\u00e4n\u00b7de", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Brechen den Stab und fallen von oben.", "tokens": ["Bre\u00b7chen", "den", "Stab", "und", "fal\u00b7len", "von", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "APPR", "ADV", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Aber wie eine Blume voll gr\u00fcner Helle", "tokens": ["A\u00b7ber", "wie", "ei\u00b7ne", "Blu\u00b7me", "voll", "gr\u00fc\u00b7ner", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "ADJA", "NN"], "meter": "+-++-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Leuchtet im Dunkel der H\u00f6fe der K\u00f6nig der Juden.", "tokens": ["Leuch\u00b7tet", "im", "Dun\u00b7kel", "der", "H\u00f6\u00b7fe", "der", "K\u00f6\u00b7nig", "der", "Ju\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.3": {"text": "Und die Stirn, die sie schattig mit Dornen beluden,", "tokens": ["Und", "die", "Stirn", ",", "die", "sie", "schat\u00b7tig", "mit", "Dor\u00b7nen", "be\u00b7lu\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Brennt wie ein Stein in fahler Grelle.", "tokens": ["Brennt", "wie", "ein", "Stein", "in", "fah\u00b7ler", "Grel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der Gott steigt hinauf, von den Schultern gehoben", "tokens": ["Und", "der", "Gott", "steigt", "hin\u00b7auf", ",", "von", "den", "Schul\u00b7tern", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "ART", "NN", "VVPP"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Riesiger Engel, er singet, ein Schwan,", "tokens": ["Rie\u00b7si\u00b7ger", "En\u00b7gel", ",", "er", "sin\u00b7get", ",", "ein", "Schwan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Leicht und klein f\u00e4hrt er auf in der strahlenden Bahn", "tokens": ["Leicht", "und", "klein", "f\u00e4hrt", "er", "auf", "in", "der", "strah\u00b7len\u00b7den", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "APPR", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und der Vater, im Glanze, wartet sein droben.", "tokens": ["Und", "der", "Va\u00b7ter", ",", "im", "Glan\u00b7ze", ",", "war\u00b7tet", "sein", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPRART", "NN", "$,", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Aber der Richter am blauen Gebirge", "tokens": ["A\u00b7ber", "der", "Rich\u00b7ter", "am", "blau\u00b7en", "Ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "H\u00e4nget im riesigen Mantel wie faltige Frucht.", "tokens": ["H\u00e4n\u00b7get", "im", "rie\u00b7si\u00b7gen", "Man\u00b7tel", "wie", "fal\u00b7ti\u00b7ge", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Wild kommt der Abend \u00fcber die hallenden \u00d6den.", "tokens": ["Wild", "kommt", "der", "A\u00b7bend", "\u00fc\u00b7ber", "die", "hal\u00b7len\u00b7den", "\u00d6\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schweigsame Wasser fallen in gr\u00fcner Schlucht.", "tokens": ["Schweig\u00b7sa\u00b7me", "Was\u00b7ser", "fal\u00b7len", "in", "gr\u00fc\u00b7ner", "Schlucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}}}}