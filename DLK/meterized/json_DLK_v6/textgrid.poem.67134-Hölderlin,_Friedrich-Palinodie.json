{"textgrid.poem.67134": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Palinodie", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was d\u00e4mmert um mich, Erde! dein freundlich Gr\u00fcn?", "tokens": ["Was", "d\u00e4m\u00b7mert", "um", "mich", ",", "Er\u00b7de", "!", "dein", "freund\u00b7lich", "Gr\u00fcn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "$,", "NN", "$.", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was wehst du wieder, L\u00fcftchen, wie einst, mich an?", "tokens": ["Was", "wehst", "du", "wie\u00b7der", ",", "L\u00fcft\u00b7chen", ",", "wie", "einst", ",", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "NN", "$,", "PWAV", "ADV", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In allen Wipfeln rauschts, ...", "tokens": ["In", "al\u00b7len", "Wip\u00b7feln", "rauschts", ",", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": ". . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.2": {"line.1": {"text": "Was weckt ihr mir die Seele? was regt ihr mir", "tokens": ["Was", "weckt", "ihr", "mir", "die", "See\u00b7le", "?", "was", "regt", "ihr", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vergangnes auf, ihr Guten! o schonet mein", "tokens": ["Ver\u00b7gang\u00b7nes", "auf", ",", "ihr", "Gu\u00b7ten", "!", "o", "scho\u00b7net", "mein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "FM", "VVFIN", "PPOSAT"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und la\u00dft sie ruhn, die Asche meiner", "tokens": ["Und", "la\u00dft", "sie", "ruhn", ",", "die", "A\u00b7sche", "mei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$,", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freuden, ihr spottetet nur! o wandelt,", "tokens": ["Freu\u00b7den", ",", "ihr", "spot\u00b7te\u00b7tet", "nur", "!", "o", "wan\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "$.", "FM", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Ihr schicksallosen G\u00f6tter, vorbei und bl\u00fcht", "tokens": ["Ihr", "schick\u00b7sal\u00b7lo\u00b7sen", "G\u00f6t\u00b7ter", ",", "vor\u00b7bei", "und", "bl\u00fcht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In eurer Jugend \u00fcber den Alternden", "tokens": ["In", "eu\u00b7rer", "Ju\u00b7gend", "\u00fc\u00b7ber", "den", "Al\u00b7tern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wollt ihr zu den Sterblichen euch", "tokens": ["Und", "wollt", "ihr", "zu", "den", "Sterb\u00b7li\u00b7chen", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gerne gesellen, so bl\u00fchn der Jungfraun", "tokens": ["Ger\u00b7ne", "ge\u00b7sel\u00b7len", ",", "so", "bl\u00fchn", "der", "Jung\u00b7fraun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Euch viel, der jungen Helden, und sch\u00f6ner spielt", "tokens": ["Euch", "viel", ",", "der", "jun\u00b7gen", "Hel\u00b7den", ",", "und", "sch\u00f6\u00b7ner", "spielt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Morgen um die Wange der Gl\u00fccklichen", "tokens": ["Der", "Mor\u00b7gen", "um", "die", "Wan\u00b7ge", "der", "Gl\u00fcck\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn um ein tr\u00fcbes Aug und lieblich", "tokens": ["Denn", "um", "ein", "tr\u00f