 "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.6": {"line.1": {"text": "Vers\u00f6hnung, o Vers\u00f6hnung, ihr g\u00fctigen,", "tokens": ["Ver\u00b7s\u00f6h\u00b7nung", ",", "o", "Ver\u00b7s\u00f6h\u00b7nung", ",", "ihr", "g\u00fc\u00b7ti\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr immergleichen G\u00f6tter, und haltet ein,", "tokens": ["Ihr", "im\u00b7mer\u00b7glei\u00b7chen", "G\u00f6t\u00b7ter", ",", "und", "hal\u00b7tet", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weil ihr die reinen Quellen liebt ", "tokens": ["Weil", "ihr", "die", "rei\u00b7nen", "Quel\u00b7len", "liebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}