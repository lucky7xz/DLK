de", "sinckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Die Zweige fallen ab von des Piastus Stamme/", "tokens": ["Die", "Zwei\u00b7ge", "fal\u00b7len", "ab", "von", "des", "Pi\u00b7as\u00b7tus", "Stam\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Wor\u00fcber Schlesien unendlich Thr\u00e4nen trinckt.", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "Schle\u00b7si\u00b7en", "un\u00b7end\u00b7lich", "Thr\u00e4\u00b7nen", "trinckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wer w\u00fcnscht nicht aufgel\u00f6st zu seyn von diesen Banden/", "tokens": ["Wer", "w\u00fcnscht", "nicht", "auf\u00b7ge\u00b7l\u00f6st", "zu", "seyn", "von", "die\u00b7sen", "Ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "VVPP", "PTKZU", "VAINF", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Die unser G\u00f6ttlich Theil der Seelen fesseln an?", "tokens": ["Die", "un\u00b7ser", "G\u00f6tt\u00b7lich", "Theil", "der", "See\u00b7len", "fes\u00b7seln", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Die Liebst ist nun im Port/ ih