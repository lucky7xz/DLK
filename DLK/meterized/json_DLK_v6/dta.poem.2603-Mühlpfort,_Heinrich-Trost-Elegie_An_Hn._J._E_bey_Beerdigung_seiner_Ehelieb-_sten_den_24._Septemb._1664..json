{"dta.poem.2603": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Elegie  \n  An Hn. J. E bey Beerdigung seiner Ehelieb-  \n sten/ den 24. Septemb. 1664.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wil mich der Himmel denn in nichts als Flor verh\u00fcllen?", "tokens": ["Wil", "mich", "der", "Him\u00b7mel", "denn", "in", "nichts", "als", "Flor", "ver\u00b7h\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "KON", "APPR", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mu\u00df Myrrh und Aloe mein Rosen-Zucker seyn?", "tokens": ["Mu\u00df", "Myrrh", "und", "A\u00b7loe", "mein", "Ro\u00b7sen\u00b7Zu\u00b7cker", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Mutter Asche brennt/ und ist noch nicht zu stillen/", "tokens": ["Der", "Mut\u00b7ter", "A\u00b7sche", "brennt", "/", "und", "ist", "noch", "nicht", "zu", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "KON", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als sich geh\u00e4ufftes Leid nun wieder findet ein.", "tokens": ["Als", "sich", "ge\u00b7h\u00e4uff\u00b7tes", "Leid", "nun", "wie\u00b7der", "fin\u00b7det", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die tieffe Traurigkeit so meinen Geist umfangen/", "tokens": ["Die", "tief\u00b7fe", "Trau\u00b7rig\u00b7keit", "so", "mei\u00b7nen", "Geist", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat mich ein Grabelied zu singen stets gehemmt.", "tokens": ["Hat", "mich", "ein", "Gra\u00b7be\u00b7lied", "zu", "sin\u00b7gen", "stets", "ge\u00b7hemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKZU", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun itzt die Seelige/ mein Vetter/ nachgegangen/", "tokens": ["Nun", "itzt", "die", "See\u00b7li\u00b7ge", "/", "mein", "Vet\u00b7ter", "/", "nach\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "PPOSAT", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird mein Aug\u2019 aufs neu mit Thr\u00e4nen \u00fcberschwemmt.", "tokens": ["So", "wird", "mein", "Aug'", "aufs", "neu", "mit", "Thr\u00e4\u00b7nen", "\u00fc\u00b7bersc\u00b7hwemmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Thr\u00e4nen die sein Leid bezeigen/ doch nicht brechen/", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "die", "sein", "Leid", "be\u00b7zei\u00b7gen", "/", "doch", "nicht", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PPOSAT", "NN", "VVINF", "$(", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil das zerschlagne Hertz in heissen Aengsten pocht/", "tokens": ["Weil", "das", "zer\u00b7schlag\u00b7ne", "Hertz", "in", "heis\u00b7sen", "A\u00b7engs\u00b7ten", "pocht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Ob seiner Liebsten Tod; wer wil da Trost zusprechen/", "tokens": ["Ob", "sei\u00b7ner", "Liebs\u00b7ten", "Tod", ";", "wer", "wil", "da", "Trost", "zu\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$.", "PWS", "VMFIN", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn beyder Augen-Quell die Z\u00e4hren-Lauge kocht.", "tokens": ["Wenn", "bey\u00b7der", "Au\u00b7gen\u00b7Quell", "die", "Z\u00e4h\u00b7ren\u00b7L\u00b7au\u00b7ge", "kocht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jen\u2019 hie\u00df des Lotos Frucht des Vater-Lands vergessen/", "tokens": ["Jen'", "hie\u00df", "des", "Lo\u00b7tos", "Frucht", "des", "Va\u00b7ter\u00b7Lands", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der wahren Lieb und Treu vergist man ewig nicht/", "tokens": ["Der", "wah\u00b7ren", "Lieb", "und", "Treu", "ver\u00b7gist", "man", "e\u00b7wig", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "PIS", "ADJD", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn Hertzen in der Eh\u2019 in solchem Fried gesessen/", "tokens": ["Wenn", "Hert\u00b7zen", "in", "der", "Eh'", "in", "sol\u00b7chem", "Fried", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Welch Jammer ist es doch wenn eins der Tod zerbricht.", "tokens": ["Welch", "Jam\u00b7mer", "ist", "es", "doch", "wenn", "eins", "der", "Tod", "zer\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Liebe die ergetzt/ die macht auch Seelen-Wunden/", "tokens": ["Die", "Lie\u00b7be", "die", "er\u00b7getzt", "/", "die", "macht", "auch", "See\u00b7len\u00b7Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVPP", "$(", "ART", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jhr Zunder giebt zwar Licht/ beynebenst Glut und Brand/", "tokens": ["Ihr", "Zun\u00b7der", "giebt", "zwar", "Licht", "/", "bey\u00b7ne\u00b7benst", "Glut", "und", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "$(", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Tod/ der Ehen bricht/ wird allzuhoch empfunden/", "tokens": ["Der", "Tod", "/", "der", "E\u00b7hen", "bricht", "/", "wird", "all\u00b7zu\u00b7hoch", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "$(", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df dem betr\u00fcbten Sinn kein Trost-Wort ist bekant.", "tokens": ["Da\u00df", "dem", "be\u00b7tr\u00fcb\u00b7ten", "Sinn", "kein", "Trost\u00b7Wort", "ist", "be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hier kan nicht die Vernunfft die Sieges-Fahnen tragen/", "tokens": ["Hier", "kan", "nicht", "die", "Ver\u00b7nunfft", "die", "Sie\u00b7ges\u00b7Fah\u00b7nen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "In ihre Palmen flicht sich blasser Wermuth ein/", "tokens": ["In", "ih\u00b7re", "Pal\u00b7men", "flicht", "sich", "blas\u00b7ser", "Wer\u00b7muth", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der unverhoffte Sto\u00df wird allen Trost verjagen/", "tokens": ["Der", "un\u00b7ver\u00b7hoff\u00b7te", "Sto\u00df", "wird", "al\u00b7len", "Trost", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Trauer-Nacht bedeckt der Wei\u00dfheit Sonnenschein.", "tokens": ["Die", "Trau\u00b7e\u00b7rNacht", "be\u00b7deckt", "der", "Wei\u00df\u00b7heit", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich suche wo ich wil ein Labsal zu erfinden/", "tokens": ["Ich", "su\u00b7che", "wo", "ich", "wil", "ein", "Lab\u00b7sal", "zu", "er\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "PPER", "VMFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der G\u00f6tter Panace ist hier nur Gauckeley/", "tokens": ["Der", "G\u00f6t\u00b7ter", "Pa\u00b7na\u00b7ce", "ist", "hier", "nur", "Gau\u00b7cke\u00b7ley", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Des Chirons kluger Arm kan Wunden nicht verbinden/", "tokens": ["Des", "Chi\u00b7rons", "klu\u00b7ger", "Arm", "kan", "Wun\u00b7den", "nicht", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die in die Seele gehn und brechen sie entzwey.", "tokens": ["Die", "in", "die", "See\u00b7le", "gehn", "und", "bre\u00b7chen", "sie", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Nicht starcker Bezoar/ nicht fette Mastix-Kl\u00f6sser/", "tokens": ["Nicht", "star\u00b7cker", "Be\u00b7zoar", "/", "nicht", "fet\u00b7te", "Mas\u00b7tix\u00b7K\u00b7l\u00f6s\u00b7ser", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$(", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Und was vor kostbar Oel Aegyptens B\u00fcrger schickt/", "tokens": ["Und", "was", "vor", "kost\u00b7bar", "O\u00b7el", "A\u00b7e\u00b7gyp\u00b7tens", "B\u00fcr\u00b7ger", "schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJD", "NN", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.31": {"text": "Benehmen hier den Schmertz/ das Leiden wird nur gr\u00f6sser/", "tokens": ["Be\u00b7neh\u00b7men", "hier", "den", "Schmertz", "/", "das", "Lei\u00b7den", "wird", "nur", "gr\u00f6s\u00b7ser", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Jemehr mans euserlich mit Nard und Salben schm\u00fcckt.", "tokens": ["Je\u00b7mehr", "mans", "eu\u00b7ser\u00b7lich", "mit", "Nard", "und", "Sal\u00b7ben", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So l\u00e4st es sich auch nicht durch s\u00fcsse Lieder zwingen/", "tokens": ["So", "l\u00e4st", "es", "sich", "auch", "nicht", "durch", "s\u00fcs\u00b7se", "Lie\u00b7der", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und die Tarantula ert\u00f6dtet nicht den Schmertz.", "tokens": ["Und", "die", "Ta\u00b7ran\u00b7tu\u00b7la", "er\u00b7t\u00f6d\u00b7tet", "nicht", "den", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er wird noch hefftiger durch Marck und Adern dringen/", "tokens": ["Er", "wird", "noch", "heff\u00b7ti\u00b7ger", "durch", "Marck", "und", "A\u00b7dern", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Jemehr man Perlen-Tr\u00e4nck ertheilt dem matten Hertz.", "tokens": ["Je\u00b7mehr", "man", "Per\u00b7len\u00b7Tr\u00e4nck", "er\u00b7theilt", "dem", "mat\u00b7ten", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es mu\u00df was h\u00f6hers seyn/ das Seelen ein kan wiegen/", "tokens": ["Es", "mu\u00df", "was", "h\u00f6\u00b7hers", "seyn", "/", "das", "See\u00b7len", "ein", "kan", "wie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PWS", "ADJA", "VAINF", "$(", "ART", "NN", "PTKVZ", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wenn so ein schmertzlich Fall sie fast zu Boden reist.", "tokens": ["Wenn", "so", "ein", "schmertz\u00b7lich", "Fall", "sie", "fast", "zu", "Bo\u00b7den", "reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJD", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Gottseelige Gedult kan \u00fcber alles siegen/", "tokens": ["Gott\u00b7see\u00b7li\u00b7ge", "Ge\u00b7dult", "kan", "\u00fc\u00b7ber", "al\u00b7les", "sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und Hoffnung die uns recht im Glauben unterweist.", "tokens": ["Und", "Hoff\u00b7nung", "die", "uns", "recht", "im", "Glau\u00b7ben", "un\u00b7ter\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er gebe diesen Platz bey solchem Sturm der Zeiten/", "tokens": ["Er", "ge\u00b7be", "die\u00b7sen", "Platz", "bey", "sol\u00b7chem", "Sturm", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "wor