{"dta.poem.19070": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Li.   Gerechte Thr\u00e4nen \u00fcber Julianen, Gr\u00e4-  \n fin von Zinzendorf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Schw\u00e4gerin! wie beugt mich dein Erblassen!", "tokens": ["Ach", "Schw\u00e4\u00b7ge\u00b7rin", "!", "wie", "beugt", "mich", "dein", "Er\u00b7blas\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was ritzet mir dein Tod vor Wunden auf!", "tokens": ["Was", "rit\u00b7zet", "mir", "dein", "Tod", "vor", "Wun\u00b7den", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O tiefer Ri\u00df, ach unterbrochner Lauf!", "tokens": ["O", "tie\u00b7fer", "Ri\u00df", ",", "ach", "un\u00b7ter\u00b7broch\u00b7ner", "Lauf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "XY", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es lehre mich der HErr sein Hertze fassen,", "tokens": ["Es", "leh\u00b7re", "mich", "der", "Herr", "sein", "Hert\u00b7ze", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "VAINF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er binde mir die Augen feste zu,", "tokens": ["Er", "bin\u00b7de", "mir", "die", "Au\u00b7gen", "fes\u00b7te", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und f\u00fchre mich ", "tokens": ["Und", "f\u00fch\u00b7re", "mich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da Salomo die Tugend mit verehrt,", "tokens": ["Da", "Sa\u00b7lo\u00b7mo", "die", "Tu\u00b7gend", "mit", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach Billigkeit vor andern zugeh\u00f6rt,", "tokens": ["Nach", "Bil\u00b7lig\u00b7keit", "vor", "an\u00b7dern", "zu\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und du! liegst schon in einem Sterbe-Kittel!", "tokens": ["Und", "du", "!", "liegst", "schon", "in", "ei\u00b7nem", "Ster\u00b7be\u00b7Kit\u00b7tel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich eilete, da mir der Bruder rief,", "tokens": ["Ich", "ei\u00b7le\u00b7te", ",", "da", "mir", "der", "Bru\u00b7der", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wuste nicht, da\u00df Juliana schlief.", "tokens": ["Und", "wus\u00b7te", "nicht", ",", "da\u00df", "Ju\u00b7li\u00b7a\u00b7na", "schlief", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich trete nur, nach abgelegtem Reisen,", "tokens": ["Ich", "tre\u00b7te", "nur", ",", "nach", "ab\u00b7ge\u00b7leg\u00b7tem", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins Hau\u00df hinein, so wird mir angesagt,", "tokens": ["Ins", "Hau\u00df", "hin\u00b7ein", ",", "so", "wird", "mir", "an\u00b7ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wird mir gleich von jederman geklagt:", "tokens": ["So", "wird", "mir", "gleich", "von", "je\u00b7der\u00b7man", "ge\u00b7klagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Bruder sey best\u00fcrtzt mit seinen W\u00e4ysen.", "tokens": ["Der", "Bru\u00b7der", "sey", "be\u00b7st\u00fcrtzt", "mit", "sei\u00b7nen", "W\u00e4y\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es h\u00e4tte kaum ", "tokens": ["Es", "h\u00e4t\u00b7te", "kaum"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "So sey ", "tokens": ["So", "sey"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Ach harter Fall! davon die Pfosten beben!", "tokens": ["Ach", "har\u00b7ter", "Fall", "!", "da\u00b7von", "die", "Pfos\u00b7ten", "be\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was m\u00e4nniglich an unserm Hause preist,", "tokens": ["Was", "m\u00e4n\u00b7nig\u00b7lich", "an", "un\u00b7serm", "Hau\u00b7se", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das zeugete dein hocherhabner Geist,", "tokens": ["Das", "zeu\u00b7ge\u00b7te", "dein", "ho\u00b7cher\u00b7hab\u00b7ner", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das zeigete dein angenehmes Leben.", "tokens": ["Das", "zei\u00b7ge\u00b7te", "dein", "an\u00b7ge\u00b7neh\u00b7mes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und h\u00e4tte dich der Schnitter nicht gemeynt;", "tokens": ["Und", "h\u00e4t\u00b7te", "dich", "der", "Schnit\u00b7ter", "nicht", "ge\u00b7meynt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie h\u00e4tte sich dein Ruhm noch ausgebreit?", "tokens": ["Wie", "h\u00e4t\u00b7te", "sich", "dein", "Ruhm", "noch", "aus\u00b7ge\u00b7breit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Allein! wer will den Rath der W\u00e4chter meistern?", "tokens": ["Al\u00b7lein", "!", "wer", "will", "den", "Rath", "der", "W\u00e4ch\u00b7ter", "meis\u00b7tern", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bebe zwar, doch mit Ergebenheit,", "tokens": ["Ich", "be\u00b7be", "zwar", ",", "doch", "mit", "Er\u00b7ge\u00b7ben\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Und tr\u00f6ste mich mit der Barmhertzigkeit,", "tokens": ["Und", "tr\u00f6s\u00b7te", "mich", "mit", "der", "Barm\u00b7hert\u00b7zig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die dich so bald versetzt zu denen Geistern.", "tokens": ["Die", "dich", "so", "bald", "ver\u00b7setzt", "zu", "de\u00b7nen", "Geis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "APPR", "PRELS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn, r\u00fcckt der Freund den Lebens-Zeiger fort;", "tokens": ["Denn", ",", "r\u00fcckt", "der", "Freund", "den", "Le\u00b7bens\u00b7Zei\u00b7ger", "fort", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So weiset er an einen guten Ort.", "tokens": ["So", "wei\u00b7set", "er", "an", "ei\u00b7nen", "gu\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wer wei\u00df, als du, ", "tokens": ["Wer", "wei\u00df", ",", "als", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wie theuer mir dein Wohl gewesen ist;", "tokens": ["Wie", "theu\u00b7er", "mir", "dein", "Wohl", "ge\u00b7we\u00b7sen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer wei\u00df das recht, nun du verschieden bist?", "tokens": ["Wer", "wei\u00df", "das", "recht", ",", "nun", "du", "ver\u00b7schie\u00b7den", "bist", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJD", "$,", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein Hoffen liegt begraben in der H\u00f6le,", "tokens": ["Mein", "Hof\u00b7fen", "liegt", "be\u00b7gra\u00b7ben", "in", "der", "H\u00f6\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die deinen Leib bi\u00df auf ", "tokens": ["Die", "dei\u00b7nen", "Leib", "bi\u00df", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "APPR", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da dich der HErr von neuem kommen heist.", "tokens": ["Da", "dich", "der", "Herr", "von", "neu\u00b7em", "kom\u00b7men", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Was deiner seits in aller Augen f\u00e4llt,", "tokens": ["Was", "dei\u00b7ner", "seits", "in", "al\u00b7ler", "Au\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das bleibt bey mir zwar etwas mehr verstellt;", "tokens": ["Das", "bleibt", "bey", "mir", "zwar", "et\u00b7was", "mehr", "ver\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch hat mein Hertz nicht weniger gejohren.", "tokens": ["Doch", "hat", "mein", "Hertz", "nicht", "we\u00b7ni\u00b7ger", "ge\u00b7joh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf, ", "tokens": ["Auf", ","], "token_info": ["word", "punct"], "pos": ["APPR", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und dringe dich (mit mir) zur Ewigkeit!", "tokens": ["Und", "drin\u00b7ge", "dich", "(", "mit", "mir", ")", "zur", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "APPR", "PPER", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}