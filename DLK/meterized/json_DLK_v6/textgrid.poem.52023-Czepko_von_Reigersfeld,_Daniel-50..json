{"textgrid.poem.52023": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Griffel ziehet hier die stachlicht' Arbeit ein:", "tokens": ["Mein", "Grif\u00b7fel", "zie\u00b7het", "hier", "die", "stach\u00b7licht'", "Ar\u00b7beit", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn das lateinsche L wird iedes Buch beschlie\u00dfen.", "tokens": ["Denn", "das", "la\u00b7tein\u00b7sche", "L", "wird", "ie\u00b7des", "Buch", "be\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Momus dr\u00fcber wird die schwartze Galle gie\u00dfen:", "tokens": ["Wo", "Mo\u00b7mus", "dr\u00fc\u00b7ber", "wird", "die", "schwart\u00b7ze", "Gal\u00b7le", "gie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PAV", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sol es ihm umgekehrt ein Griechsches \u0393 seyn:", "tokens": ["Sol", "es", "ihm", "um\u00b7ge\u00b7kehrt", "ein", "Griech\u00b7sches", "\u0393", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du aber solst dadurch, wie durch ein Winckel Scheid,", "tokens": ["Du", "a\u00b7ber", "solst", "da\u00b7durch", ",", "wie", "durch", "ein", "Win\u00b7ckel", "Scheid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PAV", "$,", "PWAV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Schrancken, lieber Freund, der starcken Tugend stehen:", "tokens": ["Im", "Schran\u00b7cken", ",", "lie\u00b7ber", "Freund", ",", "der", "star\u00b7cken", "Tu\u00b7gend", "ste\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff Laster la\u00df ich blo\u00df des Grieffels Sch\u00e4rffe gehen,", "tokens": ["Auff", "Las\u00b7ter", "la\u00df", "ich", "blo\u00df", "des", "Grief\u00b7fels", "Sch\u00e4rf\u00b7fe", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht ich, es hat hiervon dein Leben dich befreit.", "tokens": ["Nicht", "ich", ",", "es", "hat", "hier\u00b7von", "dein", "Le\u00b7ben", "dich", "be\u00b7freit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "PPER", "VAFIN", "PAV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}