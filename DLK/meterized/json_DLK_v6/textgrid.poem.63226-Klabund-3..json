{"textgrid.poem.63226": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so will ich, was ich werde;", "tokens": ["Und", "so", "will", "ich", ",", "was", "ich", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer gr\u00f6sser gr\u00fcsst der Mond.", "tokens": ["Im\u00b7mer", "gr\u00f6s\u00b7ser", "gr\u00fcs\u00b7st", "der", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Palmenbaum und dunkle Erde", "tokens": ["Pal\u00b7men\u00b7baum", "und", "dunk\u00b7le", "Er\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden zarter sich gewohnt.", "tokens": ["Wer\u00b7den", "zar\u00b7ter", "sich", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Silbersee zieht ohne Barke", "tokens": ["Sil\u00b7ber\u00b7see", "zieht", "oh\u00b7ne", "Bar\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stromgleich durch verlassnes Laub.", "tokens": ["Strom\u00b7gleich", "durch", "ver\u00b7lass\u00b7nes", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Winzers goldne Harke", "tokens": ["Und", "des", "Win\u00b7zers", "gold\u00b7ne", "Har\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sank beseligt in den Staub.", "tokens": ["Sank", "be\u00b7se\u00b7ligt", "in", "den", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dass sich Brust an Br\u00fcsten dehne!", "tokens": ["Dass", "sich", "Brust", "an", "Br\u00fcs\u00b7ten", "deh\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gib den Winden ihren Lauf!", "tokens": ["Gib", "den", "Win\u00b7den", "ih\u00b7ren", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer Fl\u00f6te Kantilene", "tokens": ["Ei\u00b7ner", "Fl\u00f6\u00b7te", "Kan\u00b7ti\u00b7le\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielt zum Tanz der Motten auf.", "tokens": ["Spielt", "zum", "Tanz", "der", "Mot\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rote Rose, Winter witternd,", "tokens": ["Ro\u00b7te", "Ro\u00b7se", ",", "Win\u00b7ter", "wit\u00b7ternd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kranke Frau im weissen Thron \u2013", "tokens": ["Kran\u00b7ke", "Frau", "im", "weis\u00b7sen", "Thron", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heute starb, ich ahn es zitternd,", "tokens": ["Heu\u00b7te", "starb", ",", "ich", "ahn", "es", "zit\u00b7ternd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner K\u00fcsse sch\u00f6nster Sohn.", "tokens": ["Mei\u00b7ner", "K\u00fcs\u00b7se", "sch\u00f6ns\u00b7ter", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}