{"dta.poem.19457": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Jungfrau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wo sah ich, M\u00e4dchen, deine Z\u00fcge,", "tokens": ["Wo", "sah", "ich", ",", "M\u00e4d\u00b7chen", ",", "dei\u00b7ne", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die droh'nden Augen, lieblich, wild,", "tokens": ["Die", "droh'n\u00b7den", "Au\u00b7gen", ",", "lieb\u00b7lich", ",", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch rein von Eitelkeit und L\u00fcge?", "tokens": ["Noch", "rein", "von", "Ei\u00b7tel\u00b7keit", "und", "L\u00fc\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Buonarotti's gro\u00dfem Bild:", "tokens": ["Auf", "Buo\u00b7na\u00b7rot\u00b7ti's", "gro\u00b7\u00dfem", "Bild", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00f6pfer senkt sich sachten Fluges", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "senkt", "sich", "sach\u00b7ten", "Flu\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Menschen, welcher schlummernd liegt,", "tokens": ["Zum", "Men\u00b7schen", ",", "wel\u00b7cher", "schlum\u00b7mernd", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Scho\u00dfe seines Mantelbuges", "tokens": ["Im", "Scho\u00b7\u00dfe", "sei\u00b7nes", "Man\u00b7tel\u00b7bu\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruht himmlisches Gesind geschmiegt:", "tokens": ["Ruht", "himm\u00b7li\u00b7sches", "Ge\u00b7sind", "ge\u00b7schmiegt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Voran ein Wesen nicht zu nennen,", "tokens": ["Vo\u00b7ran", "ein", "We\u00b7sen", "nicht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Gottes Mantel keusch umwallt,", "tokens": ["Von", "Got\u00b7tes", "Man\u00b7tel", "keusch", "um\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Weibes Z\u00fcge, zu erkennen", "tokens": ["Des", "Wei\u00b7bes", "Z\u00fc\u00b7ge", ",", "zu", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einer schlanken Traumgestalt.", "tokens": ["In", "ei\u00b7ner", "schlan\u00b7ken", "Traum\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie lauscht, das Haupt hervorgewendet,", "tokens": ["Sie", "lauscht", ",", "das", "Haupt", "her\u00b7vor\u00b7ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Augen schaut sie tief erschreckt,", "tokens": ["Mit", "Au\u00b7gen", "schaut", "sie", "tief", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Adam Er den Funken spendet", "tokens": ["Wie", "A\u00b7dam", "Er", "den", "Fun\u00b7ken", "spen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Rechte mahnend reckt.", "tokens": ["Und", "sei\u00b7ne", "Rech\u00b7te", "mah\u00b7nend", "reckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sieht den Schlumm'rer sich erheben,", "tokens": ["Sie", "sieht", "den", "Schlum\u00b7m'\u00b7rer", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der das bewu\u00dfte Sein empf\u00e4ngt,", "tokens": ["Der", "das", "be\u00b7wu\u00df\u00b7te", "Sein", "emp\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch sie sehnt dunkel sich zu leben,", "tokens": ["Auch", "sie", "sehnt", "dun\u00b7kel", "sich", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Gottes Schulter still gedr\u00e4ngt \u2014", "tokens": ["An", "Got\u00b7tes", "Schul\u00b7ter", "still", "ge\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So harrst du vor des Lebens Schranke,", "tokens": ["So", "harrst", "du", "vor", "des", "Le\u00b7bens", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ungefesselt vom Geschick,", "tokens": ["Noch", "un\u00b7ge\u00b7fes\u00b7selt", "vom", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein unentweihter Gottgedanke,", "tokens": ["Ein", "un\u00b7ent\u00b7weih\u00b7ter", "Gott\u00b7ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00f6ffnest staunend deinen Blick.", "tokens": ["Und", "\u00f6ff\u00b7nest", "stau\u00b7nend", "dei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}