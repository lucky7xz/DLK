{"textgrid.poem.59948": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die reine Frau ist wie ein frischer Quell,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die reine Frau ist wie ein frischer Quell,", "tokens": ["Die", "rei\u00b7ne", "Frau", "ist", "wie", "ein", "fri\u00b7scher", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der uns entgegensprudelt klar und hell,", "tokens": ["Der", "uns", "ent\u00b7ge\u00b7gen\u00b7spru\u00b7delt", "klar", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie eine lautre Gottesoffenbarung;", "tokens": ["Wie", "ei\u00b7ne", "laut\u00b7re", "Got\u00b7te\u00b7sof\u00b7fen\u00b7ba\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er labt und freut uns nur, tr\u00e4gt keine Lasten,", "tokens": ["Er", "labt", "und", "freut", "uns", "nur", ",", "tr\u00e4gt", "kei\u00b7ne", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch die sich beugen unter stolzen Masten,", "tokens": ["Doch", "die", "sich", "beu\u00b7gen", "un\u00b7ter", "stol\u00b7zen", "Mas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PRF", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Str\u00f6m' und Meere, sch\u00f6pfen aus ihm Nahrung.", "tokens": ["Die", "Str\u00f6m'", "und", "Mee\u00b7re", ",", "sch\u00f6p\u00b7fen", "aus", "ihm", "Nah\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}