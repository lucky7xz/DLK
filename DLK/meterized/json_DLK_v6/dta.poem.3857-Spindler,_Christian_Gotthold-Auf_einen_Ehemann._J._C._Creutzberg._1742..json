{"dta.poem.3857": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Ehemann.  \n  J. C. Creutzberg.  1742.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt, Freunde, setzet euch mit mir", "tokens": ["Kommt", ",", "Freun\u00b7de", ",", "set\u00b7zet", "euch", "mit", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu diesem bangen Leichen-Steine,", "tokens": ["Zu", "die\u00b7sem", "ban\u00b7gen", "Lei\u00b7chen\u00b7Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da lieget meine Lust und Zier,", "tokens": ["Da", "lie\u00b7get", "mei\u00b7ne", "Lust", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ich mit Ach und Schmertz beweine.", "tokens": ["Den", "ich", "mit", "Ach", "und", "Schmertz", "be\u00b7wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gott hat den bangen Ri\u00df gethan,", "tokens": ["Gott", "hat", "den", "ban\u00b7gen", "Ri\u00df", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da liegt mein frommer Ehemann.", "tokens": ["Da", "liegt", "mein", "from\u00b7mer", "E\u00b7he\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jhn sollen meine Wittwen-Z\u00e4hren", "tokens": ["Jhn", "sol\u00b7len", "mei\u00b7ne", "Witt\u00b7wen\u00b7Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis in die bange Grufft verehren,", "tokens": ["Bis", "in", "die", "ban\u00b7ge", "Grufft", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bis bald der h\u00f6chst bedr\u00e4ngte Geist", "tokens": ["Bis", "bald", "der", "h\u00f6chst", "be\u00b7dr\u00e4ng\u00b7te", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sich Bochims Last und Schmertz entrei\u00dft.", "tokens": ["Sich", "Bo\u00b7chims", "Last", "und", "Schmertz", "ent\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}