{"textgrid.poem.54102": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Frau spricht", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab meinen ersten Mann gesehn \u2013", "tokens": ["Ich", "hab", "mei\u00b7nen", "ers\u00b7ten", "Mann", "ge\u00b7sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "der ging mit einer!", "tokens": ["der", "ging", "mit", "ei\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "ART", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "H\u00fctchen, Rock und Bluse (Indanthren)", "tokens": ["H\u00fct\u00b7chen", ",", "Rock", "und", "Blu\u00b7se", "(", "In\u00b7dan\u00b7th\u00b7ren", ")"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$(", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und zwei Kopf kleiner!", "tokens": ["und", "zwei", "Kopf", "klei\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Sie mu\u00df ihn wohl ins B\u00fcro begleiten . . .", "tokens": ["Sie", "mu\u00df", "ihn", "wohl", "ins", "B\u00fc\u00b7ro", "be\u00b7glei\u00b7ten", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u00dcber den Geschmack ist nicht zu streiten.", "tokens": ["\u00dc\u00b7ber", "den", "Ge\u00b7schmack", "ist", "nicht", "zu", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Na, herzlichen Gl\u00fcckwunsch!", "tokens": ["Na", ",", "herz\u00b7li\u00b7chen", "Gl\u00fcck\u00b7wunsch", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sein Gehirn ist bei der Liebeswahl", "tokens": ["Sein", "Ge\u00b7hirn", "ist", "bei", "der", "Lie\u00b7bes\u00b7wahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "ganz verkleistert;", "tokens": ["ganz", "ver\u00b7kleis\u00b7tert", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "wenn er siegt, dann ist er allemal", "tokens": ["wenn", "er", "siegt", ",", "dann", "ist", "er", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "schwer begeistert.", "tokens": ["schwer", "be\u00b7geis\u00b7tert", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ob Languettenhemd, ob teure Seiden \u2013", "tokens": ["Ob", "Lan\u00b7gu\u00b7et\u00b7ten\u00b7hemd", ",", "ob", "teu\u00b7re", "Sei\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "seinetwegen kann man sich in S\u00e4cke kleiden . . .", "tokens": ["sei\u00b7net\u00b7we\u00b7gen", "kann", "man", "sich", "in", "S\u00e4\u00b7cke", "klei\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVINF", "VMFIN", "PIS", "PRF", "APPR", "NN", "VVINF", "$.", "$.", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Na, herzlichen Gl\u00fcckwunsch!", "tokens": ["Na", ",", "herz\u00b7li\u00b7chen", "Gl\u00fcck\u00b7wunsch", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Frau ist Frau. Wie gl\u00fccklich ist der Mann,", "tokens": ["Frau", "ist", "Frau", ".", "Wie", "gl\u00fcck\u00b7lich", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$.", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "dem das gleich ist!", "tokens": ["dem", "das", "gleich", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und f\u00fcr sowas zieht man sich nun an!", "tokens": ["Und", "f\u00fcr", "so\u00b7was", "zieht", "man", "sich", "nun", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "VVFIN", "PIS", "PRF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als ob man reich ist!", "tokens": ["Als", "ob", "man", "reich", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Das hei\u00dft: f\u00fcr ihn . . . ?", "tokens": ["Das", "hei\u00dft", ":", "f\u00fcr", "ihn", ".", ".", ".", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PDS", "VVFIN", "$.", "APPR", "PPER", "$.", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wir ziehen unsre Augenbrauen", "tokens": ["Wir", "zie\u00b7hen", "uns\u00b7re", "Au\u00b7gen\u00b7brau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "f\u00fcr und gegen alle andern Frauen.", "tokens": ["f\u00fcr", "und", "ge\u00b7gen", "al\u00b7le", "an\u00b7dern", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Immerhin erwart ich, da\u00df ers merken kann;", "tokens": ["Im\u00b7mer\u00b7hin", "er\u00b7wart", "ich", ",", "da\u00df", "ers", "mer\u00b7ken", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ich will f\u00fchlen, da\u00df ich reizvoll bin.", "tokens": ["ich", "will", "f\u00fch\u00b7len", ",", "da\u00df", "ich", "reiz\u00b7voll", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dreifach spiegeln will ich mich: im Glas, im Neid, im Mann.", "tokens": ["Drei\u00b7fach", "spie\u00b7geln", "will", "ich", "mich", ":", "im", "Glas", ",", "im", "Neid", ",", "im", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "PPER", "PRF", "$.", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Und der guckt gar nicht hin.", "tokens": ["Und", "der", "guckt", "gar", "nicht", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Liebe kostet manche \u00dcberwindung . . .", "tokens": ["Lie\u00b7be", "kos\u00b7tet", "man\u00b7che", "\u00dc\u00b7berw\u00b7in\u00b7dung", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4nner sind eine komische Erfindung.", "tokens": ["M\u00e4n\u00b7ner", "sind", "ei\u00b7ne", "ko\u00b7mi\u00b7sche", "Er\u00b7fin\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}