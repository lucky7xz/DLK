{"textgrid.poem.43200": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbder Tag ist hei\u00df! \u2013 die Piazetta leer! \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Tag ist hei\u00df! \u2013 die Piazetta leer! \u2013", "tokens": ["\u00bb", "der", "Tag", "ist", "hei\u00df", "!", "\u2013", "die", "Pia\u00b7zet\u00b7ta", "leer", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$.", "$(", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich kann vergeblich heut' der Fremden harren,", "tokens": ["Ich", "kann", "ver\u00b7geb\u00b7lich", "heut'", "der", "Frem\u00b7den", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auch kein Liebespaar will einsam fahren.", "tokens": ["Und", "auch", "kein", "Lie\u00b7be\u00b7spaar", "will", "ein\u00b7sam", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tag ist hei\u00df! \u2013 Eh! \u2013 ist das Leben schwer!\u00ab", "tokens": ["Der", "Tag", "ist", "hei\u00df", "!", "\u2013", "Eh", "!", "\u2013", "ist", "das", "Le\u00b7ben", "schwer", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "$(", "NN", "$.", "$(", "VAFIN", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So st\u00f6hnte Beppo, als ich fl\u00fcchtig frug,", "tokens": ["So", "st\u00f6hn\u00b7te", "Bep\u00b7po", ",", "als", "ich", "fl\u00fcch\u00b7tig", "frug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie sein entr\u00fcstet Angesicht zu deuten.", "tokens": ["Wie", "sein", "ent\u00b7r\u00fcs\u00b7tet", "An\u00b7ge\u00b7sicht", "zu", "deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAINF", "VVFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbmadonna glaubt nur nicht den Schifferleuten,", "tokens": ["\u00bb", "ma\u00b7don\u00b7na", "glaubt", "nur", "nicht", "den", "Schif\u00b7fer\u00b7leu\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die haben Geld! \u2013 die haben Brod genug,\u00ab", "tokens": ["Die", "ha\u00b7ben", "Geld", "!", "\u2013", "die", "ha\u00b7ben", "Brod", "ge\u00b7nug", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "NN", "$.", "$(", "PDS", "VAFIN", "NN", "ADV", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So zischte mir ein Judenbube zu", "tokens": ["So", "zischte", "mir", "ein", "Ju\u00b7den\u00b7bu\u00b7be", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKZU"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und w\u00fchlte grimmig in den krausen Locken,", "tokens": ["Und", "w\u00fchl\u00b7te", "grim\u00b7mig", "in", "den", "krau\u00b7sen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch schwieg er pl\u00f6tzlich und entsprang erschrocken,", "tokens": ["Doch", "schwieg", "er", "pl\u00f6tz\u00b7lich", "und", "ent\u00b7sprang", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Beppo h\u00f6rt' ihn und er knirschte: ", "tokens": ["Denn", "Bep\u00b7po", "h\u00f6rt'", "ihn", "und", "er", "knirschte", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und rasch heraus er beide Taschen kehrt,", "tokens": ["Und", "rasch", "he\u00b7raus", "er", "bei\u00b7de", "Ta\u00b7schen", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schnippt mit den Fingern und beginnt zu fluchen:", "tokens": ["Schnippt", "mit", "den", "Fin\u00b7gern", "und", "be\u00b7ginnt", "zu", "flu\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "\u00bbder Teufel selber kann heut' bei mir suchen,", "tokens": ["\u00bb", "der", "Teu\u00b7fel", "sel\u00b7ber", "kann", "heut'", "bei", "mir", "su\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht einen Cent ist dieser Tag mir werth!\u00ab ...", "tokens": ["Nicht", "ei\u00b7nen", "Cent", "ist", "die\u00b7ser", "Tag", "mir", "werth", "!", "\u00ab", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "PDAT", "NN", "PPER", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}