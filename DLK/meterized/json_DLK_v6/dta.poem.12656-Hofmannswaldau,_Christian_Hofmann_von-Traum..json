{"dta.poem.12656": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mjr kam im schlafe vor Dorindens angeficht,", "tokens": ["Mjr", "kam", "im", "schla\u00b7fe", "vor", "Do\u00b7rin\u00b7dens", "an\u00b7ge\u00b7ficht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das zeigt in minen sich gantz gegen mich verpflicht;", "tokens": ["Das", "zeigt", "in", "mi\u00b7nen", "sich", "gantz", "ge\u00b7gen", "mich", "ver\u00b7pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie dr\u00fcckte mir die hand, und das gew\u00fcnschte k\u00fcssen,", "tokens": ["Sie", "dr\u00fcck\u00b7te", "mir", "die", "hand", ",", "und", "das", "ge\u00b7w\u00fcnschte", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Lie\u00df sie sich di\u00dfmuhl nicht, wie andermal, verdr\u00fcssen.", "tokens": ["Lie\u00df", "sie", "sich", "di\u00df\u00b7muhl", "nicht", ",", "wie", "an\u00b7der\u00b7mal", ",", "ver\u00b7dr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$,", "PWAV", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich spielte gantz vergn\u00fcgt, mit dem was die natur", "tokens": ["Ich", "spiel\u00b7te", "gantz", "ver\u00b7gn\u00fcgt", ",", "mit", "dem", "was", "die", "na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,", "APPR", "ART", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr sch\u00f6nes eingelegt, ich sah der liebe spur.", "tokens": ["Ihr", "sch\u00f6\u00b7nes", "ein\u00b7ge\u00b7legt", ",", "ich", "sah", "der", "lie\u00b7be", "spur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVPP", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch ich besann mich bald, da\u00df di\u00df ein traum nur sey;", "tokens": ["Doch", "ich", "be\u00b7sann", "mich", "bald", ",", "da\u00df", "di\u00df", "ein", "traum", "nur", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PDS", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hierauf so liessen mich des schlafes fessel frey.", "tokens": ["Hier\u00b7auf", "so", "lies\u00b7sen", "mich", "des", "schla\u00b7fes", "fes\u00b7sel", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! w\u00fcnscht\u2019 ich, da ich noch besah die leeren h\u00e4nde:", "tokens": ["Ach", "!", "w\u00fcnscht'", "ich", ",", "da", "ich", "noch", "be\u00b7sah", "die", "lee\u00b7ren", "h\u00e4n\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach warum hatte denn der traum so bald ein ende?", "tokens": ["Ach", "wa\u00b7rum", "hat\u00b7te", "denn", "der", "traum", "so", "bald", "ein", "en\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VAFIN", "ADV", "ART", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}