{"textgrid.poem.63954": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Ode an die Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, o Zeit,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "o", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gegr\u00fc\u00dft du flie\u00dfendes Meer,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "du", "flie\u00b7\u00dfen\u00b7des", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sei gegr\u00fc\u00dft,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Du Meer der Zeit!", "tokens": ["Du", "Meer", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ungestadet", "tokens": ["Un\u00b7ge\u00b7sta\u00b7det"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Rollst du dahin,", "tokens": ["Rollst", "du", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "F\u00e4llst erzener Woge,", "tokens": ["F\u00e4llst", "er\u00b7ze\u00b7ner", "Wo\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schnellst wie ein Pfeil", "tokens": ["Schnellst", "wie", "ein", "Pfeil"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Hinein in der Zukunft", "tokens": ["Hin\u00b7ein", "in", "der", "Zu\u00b7kunft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nichtigen Raum.", "tokens": ["Nich\u00b7ti\u00b7gen", "Raum", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es rollten die Welten", "tokens": ["Es", "roll\u00b7ten", "die", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Aus Sch\u00f6pferhand", "tokens": ["Aus", "Sch\u00f6p\u00b7fer\u00b7hand"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ins endlose Blau,", "tokens": ["Ins", "end\u00b7lo\u00b7se", "Blau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Da kommst du geglitten", "tokens": ["Da", "kommst", "du", "ge\u00b7glit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Von Fingern der Allmacht", "tokens": ["Von", "Fin\u00b7gern", "der", "All\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Im Riesensturz,", "tokens": ["Im", "Rie\u00b7sen\u00b7sturz", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Wogtest unter das Sonnen-", "tokens": ["Wog\u00b7test", "un\u00b7ter", "das", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "TRUNC"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Unter das Erdenheer.", "tokens": ["Un\u00b7ter", "das", "Er\u00b7den\u00b7heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.15": {"text": "Sausend vom Gottespuls,", "tokens": ["Sau\u00b7send", "vom", "Got\u00b7tes\u00b7puls", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Hobest dich stolz", "tokens": ["Ho\u00b7best", "dich", "stolz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Unter gl\u00e4nzenden Sph\u00e4ren", "tokens": ["Un\u00b7ter", "gl\u00e4n\u00b7zen\u00b7den", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "Und rolltest sie weiter,", "tokens": ["Und", "roll\u00b7test", "sie", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Schwimmende Inseln,", "tokens": ["Schwim\u00b7men\u00b7de", "In\u00b7seln", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Rollst sie noch jetzt.", "tokens": ["Rollst", "sie", "noch", "jetzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Auf erster erobernder Welle", "tokens": ["Auf", "ers\u00b7ter", "er\u00b7o\u00b7bern\u00b7der", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Tr\u00e4gst du die Welten,", "tokens": ["Tr\u00e4gst", "du", "die", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.23": {"text": "Tr\u00e4gst du mich,", "tokens": ["Tr\u00e4gst", "du", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.24": {"text": "Tr\u00e4gst du mein Lied.", "tokens": ["Tr\u00e4gst", "du", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Auf deiner Wogen Erster", "tokens": ["Auf", "dei\u00b7ner", "Wo\u00b7gen", "Ers\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Schwebt es dahin.", "tokens": ["Schwebt", "es", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.27": {"text": "H\u00f6re das Lied", "tokens": ["H\u00f6\u00b7re", "das", "Lied"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.28": {"text": "Und hebe mich,", "tokens": ["Und", "he\u00b7be", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Wenn sinken ich sollte", "tokens": ["Wenn", "sin\u00b7ken", "ich", "soll\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VMFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Auf Ruhmeswarte", "tokens": ["Auf", "Ruh\u00b7mes\u00b7war\u00b7te"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Flut\u00fcberragendem Fels,", "tokens": ["Flut\u00b7\u00fc\u00b7berr\u00b7a\u00b7gen\u00b7dem", "Fels", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.32": {"text": "Hoch und fest", "tokens": ["Hoch", "und", "fest"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Ob Wogen und Schwinden.", "tokens": ["Ob", "Wo\u00b7gen", "und", "Schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Es kommt und schwindet", "tokens": ["Es", "kommt", "und", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Steten Wechsels,", "tokens": ["Ste\u00b7ten", "Wech\u00b7sels", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Jede Sekunde", "tokens": ["Je\u00b7de", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein anderes Leben.", "tokens": ["Ein", "an\u00b7de\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Sonnenkreise", "tokens": ["Son\u00b7nen\u00b7krei\u00b7se"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wandelt die Erde,", "tokens": ["Wan\u00b7delt", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mondumwandelt.", "tokens": ["Mond\u00b7um\u00b7wan\u00b7delt", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Verschlungen geregelte Bahnen", "tokens": ["Ver\u00b7schlun\u00b7gen", "ge\u00b7re\u00b7gel\u00b7te", "Bah\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Rollt mit Kreisen um Kreise", "tokens": ["Rollt", "mit", "Krei\u00b7sen", "um", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Das funkelnde All,", "tokens": ["Das", "fun\u00b7keln\u00b7de", "All", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Bis es vergeht,", "tokens": ["Bis", "es", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Mit dem letzten St\u00e4ubchen verweht.", "tokens": ["Mit", "dem", "letz\u00b7ten", "St\u00e4ub\u00b7chen", "ver\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Und neue Welten,", "tokens": ["Und", "neu\u00b7e", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Meer der Zeit,", "tokens": ["Meer", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Schaukelt die Woge:", "tokens": ["Schau\u00b7kelt", "die", "Wo\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Staub umw\u00f6lkt die Marke,", "tokens": ["Staub", "um\u00b7w\u00f6lkt", "die", "Mar\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Die eherne Marke,", "tokens": ["Die", "e\u00b7her\u00b7ne", "Mar\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Die Gott gesetzt.", "tokens": ["Die", "Gott", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und wieder bildet", "tokens": ["Und", "wie\u00b7der", "bil\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Und immer wieder", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Die Gotteshand", "tokens": ["Die", "Got\u00b7tes\u00b7hand"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Dem ewigen Auge", "tokens": ["Dem", "e\u00b7wi\u00b7gen", "Au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Vors unendliche Nichts", "tokens": ["Vors", "un\u00b7end\u00b7li\u00b7che", "Nichts"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Das Spielzeug der Welt,", "tokens": ["Das", "Spiel\u00b7zeug", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Kaum da\u00df in kurzer L\u00fccke", "tokens": ["Kaum", "da\u00df", "in", "kur\u00b7zer", "L\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Erhabener \u00d6de", "tokens": ["Er\u00b7ha\u00b7be\u00b7ner", "\u00d6\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Auf den Riesenspiegeln", "tokens": ["Auf", "den", "Rie\u00b7sen\u00b7spie\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Du dich weiterergossen.", "tokens": ["Du", "dich", "wei\u00b7te\u00b7rer\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Und alle die Welten", "tokens": ["Und", "al\u00b7le", "die", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Tr\u00e4gst du", "tokens": ["Tr\u00e4gst", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "--", "measure": "unknown.measure.zero"}, "line.27": {"text": "Auf atlantischem R\u00fccken \u2013", "tokens": ["Auf", "at\u00b7lan\u00b7ti\u00b7schem", "R\u00fc\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.28": {"text": "Und wirst nicht m\u00fcd?", "tokens": ["Und", "wirst", "nicht", "m\u00fcd", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wann o wann", "tokens": ["Wann", "o", "wann"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "FM", "PWAV"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Sch\u00e4umst du hinauf,", "tokens": ["Sch\u00e4umst", "du", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Versch\u00e4umest am Strande der Ruh?", "tokens": ["Ver\u00b7sch\u00e4u\u00b7mest", "am", "Stran\u00b7de", "der", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}