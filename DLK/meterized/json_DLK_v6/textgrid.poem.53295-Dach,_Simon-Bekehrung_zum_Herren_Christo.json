{"textgrid.poem.53295": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Bekehrung zum Herren Christo", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesu, Quell gew\u00fcnschter Frewden,", "tokens": ["Je\u00b7su", ",", "Quell", "ge\u00b7w\u00fcnschter", "Frew\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "O mein Trost, mein bestes Theil,", "tokens": ["O", "mein", "Trost", ",", "mein", "bes\u00b7tes", "Theil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fcsser Hort, gewisses Heyl", "tokens": ["S\u00fcs\u00b7ser", "Hort", ",", "ge\u00b7wis\u00b7ses", "Heyl"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller, die in grossem Leiden", "tokens": ["Al\u00b7ler", ",", "die", "in", "gros\u00b7sem", "Lei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sehr ge\u00e4ngstet sich befinden", "tokens": ["Sehr", "ge\u00b7\u00e4ngs\u00b7tet", "sich", "be\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wegen Drangsals ihrer S\u00fcnden,", "tokens": ["We\u00b7gen", "Drangs\u00b7als", "ih\u00b7rer", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bist Du nicht mit deinen Gaben", "tokens": ["Bist", "Du", "nicht", "mit", "dei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzund schon vor meiner Th\u00fcr?", "tokens": ["Je\u00b7tzund", "schon", "vor", "mei\u00b7ner", "Th\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, du klopffest an bey mir,", "tokens": ["Ja", ",", "du", "klopf\u00b7fest", "an", "bey", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wilst mein Hertz zur Wohnung haben,", "tokens": ["Wilst", "mein", "Hertz", "zur", "Woh\u00b7nung", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber, ach! ich mu\u00df mich sch\u00e4hmen", "tokens": ["A\u00b7ber", ",", "ach", "!", "ich", "mu\u00df", "mich", "sch\u00e4h\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "PPER", "VMFIN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So, dich Christe, auff-zu-nehmen.", "tokens": ["So", ",", "dich", "Chris\u00b7te", ",", "auf\u00b7fzu\u00b7neh\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PRF", "NE", "$,", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Packt euch erst, ihr Laster-seuchen", "tokens": ["Packt", "euch", "erst", ",", "ihr", "Las\u00b7ter\u00b7seu\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der M\u00f6rderischen Schaar!", "tokens": ["Zu", "der", "M\u00f6r\u00b7de\u00b7ri\u00b7schen", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht, Ihr grawsame Gefahr!", "tokens": ["Geht", ",", "Ihr", "graw\u00b7sa\u00b7me", "Ge\u00b7fahr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolt ihr nicht? Ihr m\u00fcsset weichen!", "tokens": ["Wolt", "ihr", "nicht", "?", "Ihr", "m\u00fcs\u00b7set", "wei\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Haus sol meinem Leben,", "tokens": ["Die\u00b7ses", "Haus", "sol", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Christo, einig seyn ergeben.", "tokens": ["Chris\u00b7to", ",", "ei\u00b7nig", "seyn", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "VAINF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun, Herr Christe, steht dir offen,", "tokens": ["Nun", ",", "Herr", "Chris\u00b7te", ",", "steht", "dir", "of\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Was Du dir erwehlet hast,", "tokens": ["Was", "Du", "dir", "er\u00b7weh\u00b7let", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm, du grosser Seelen-Gast!", "tokens": ["Komm", ",", "du", "gros\u00b7ser", "See\u00b7len\u00b7Gast", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm! mein Wunsch, mein gantzes Hoffen!", "tokens": ["Komm", "!", "mein", "Wunsch", ",", "mein", "gant\u00b7zes", "Hof\u00b7fen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6mbstu? Ja, Du bist zugegen,", "tokens": ["K\u00f6mbs\u00b7tu", "?", "Ja", ",", "Du", "bist", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Merck' ich doch schon deinen Seegen.", "tokens": ["Merck", "ich", "doch", "schon", "dei\u00b7nen", "See\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lasst das vnterst' oben stehen!", "tokens": ["Lasst", "das", "vn\u00b7ter\u00b7st'", "o\u00b7ben", "ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+----+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Lasst ihr Felsen ewren Grund,", "tokens": ["Lasst", "ihr", "Fel\u00b7sen", "ew\u00b7ren", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrtzt euch in des Meeres Schlund!", "tokens": ["St\u00fcrtzt", "euch", "in", "des", "Mee\u00b7res", "Schlund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lasst die Welt zu dr\u00fcmmern gehen!", "tokens": ["Lasst", "die", "Welt", "zu", "dr\u00fcm\u00b7mern", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles das, wo Christus wohnet,", "tokens": ["Al\u00b7les", "das", ",", "wo", "Chris\u00b7tus", "woh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bleibt f\u00fcr Vngl\u00fcck wol verschonet.", "tokens": ["Bleibt", "f\u00fcr", "Vn\u00b7gl\u00fcck", "wol", "ver\u00b7scho\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}