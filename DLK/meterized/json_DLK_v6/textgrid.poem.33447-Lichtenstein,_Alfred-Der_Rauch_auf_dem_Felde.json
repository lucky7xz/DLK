{"textgrid.poem.33447": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Der Rauch auf dem Felde", "genre": "verse", "period": "N.A.", "pub_year": 1911, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lene Levi lief am Abend", "tokens": ["Le\u00b7ne", "Le\u00b7vi", "lief", "am", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trippelnd, mit gerafften R\u00f6cken,", "tokens": ["Trip\u00b7pelnd", ",", "mit", "ge\u00b7raff\u00b7ten", "R\u00f6\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die langen, leeren Stra\u00dfen", "tokens": ["Durch", "die", "lan\u00b7gen", ",", "lee\u00b7ren", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer Vorstadt.", "tokens": ["Ei\u00b7ner", "Vor\u00b7stadt", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und sie sprach verweinte, wehe,", "tokens": ["Und", "sie", "sprach", "ver\u00b7wein\u00b7te", ",", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVFIN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirre, wunderliche Worte,", "tokens": ["Wir\u00b7re", ",", "wun\u00b7der\u00b7li\u00b7che", "Wor\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Wind warf, da\u00df sie knallten", "tokens": ["Die", "der", "Wind", "warf", ",", "da\u00df", "sie", "knall\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Schoten,", "tokens": ["Wie", "die", "Scho\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Sich an B\u00e4umen blutig ritzten", "tokens": ["Sich", "an", "B\u00e4u\u00b7men", "blu\u00b7tig", "ritz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verfetzt an H\u00e4usern hingen", "tokens": ["Und", "ver\u00b7fetzt", "an", "H\u00e4u\u00b7sern", "hin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in diesen tauben Stra\u00dfen", "tokens": ["Und", "in", "die\u00b7sen", "tau\u00b7ben", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam starben.", "tokens": ["Ein\u00b7sam", "star\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Lene Levi lief, bis alle", "tokens": ["Le\u00b7ne", "Le\u00b7vi", "lief", ",", "bis", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "VVFIN", "$,", "KOUS", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4cher schiefe M\u00e4uler zogen,", "tokens": ["D\u00e4\u00b7cher", "schie\u00b7fe", "M\u00e4u\u00b7ler", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Fenster Fratzen schnitten", "tokens": ["Und", "die", "Fens\u00b7ter", "Frat\u00b7zen", "schnit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Schatten", "tokens": ["Und", "die", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Ganz betrunkne Sp\u00e4\u00dfe machten \u2013", "tokens": ["Ganz", "be\u00b7trunk\u00b7ne", "Sp\u00e4\u00b7\u00dfe", "mach\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die H\u00e4user hilflos wurden", "tokens": ["Bis", "die", "H\u00e4u\u00b7ser", "hil\u00b7flos", "wur\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die stumme Stadt vergangen", "tokens": ["Und", "die", "stum\u00b7me", "Stadt", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War in weiten", "tokens": ["War", "in", "wei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Feldern, die der Mond beschmierte ...", "tokens": ["Fel\u00b7dern", ",", "die", "der", "Mond", "be\u00b7schmier\u00b7te", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenchen nahm aus ihrer Tasche", "tokens": ["Len\u00b7chen", "nahm", "aus", "ih\u00b7rer", "Ta\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Kiste mit Zigarren,", "tokens": ["Ei\u00b7ne", "Kis\u00b7te", "mit", "Zi\u00b7gar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog sich weinend", "tokens": ["Zog", "sich", "wei\u00b7nend"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Aus und rauchte ...", "tokens": ["Aus", "und", "rauch\u00b7te", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "KON", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}}}}}