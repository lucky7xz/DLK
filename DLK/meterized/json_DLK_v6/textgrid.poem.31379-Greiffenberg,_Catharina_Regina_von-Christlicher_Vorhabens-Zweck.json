{"textgrid.poem.31379": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Christlicher Vorhabens-Zweck", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Allheit / der ich mich in allem hab ergeben /", "tokens": ["Ach", "All\u00b7heit", "/", "der", "ich", "mich", "in", "al\u00b7lem", "hab", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "PRELS", "PPER", "PRF", "APPR", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit allem was ich bin / beginne / denk und dicht!", "tokens": ["mit", "al\u00b7lem", "was", "ich", "bin", "/", "be\u00b7gin\u00b7ne", "/", "denk", "und", "dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "VAFIN", "$(", "VVFIN", "$(", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "zu deiner hohen Ehr mein Spiel und Ziel ich richt.", "tokens": ["zu", "dei\u00b7ner", "ho\u00b7hen", "Ehr", "mein", "Spiel", "und", "Ziel", "ich", "richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ach la\u00df den Engel-Zweck / dein Lob la\u00df mich erstreben.", "tokens": ["ach", "la\u00df", "den", "En\u00b7gel\u00b7Zweck", "/", "dein", "Lob", "la\u00df", "mich", "er\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "VVIMP", "ART", "NN", "$(", "PPOSAT", "NN", "VVIMP", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "la\u00df nichts / als was dich liebt und lobet / an mir leben.", "tokens": ["la\u00df", "nichts", "/", "als", "was", "dich", "liebt", "und", "lo\u00b7bet", "/", "an", "mir", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "$(", "KOUS", "PRELS", "PPER", "VVFIN", "KON", "VVFIN", "$(", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ach gib mir Hitz' und Witz / zu richten meine Pflicht.", "tokens": ["Ach", "gib", "mir", "Hitz'", "und", "Witz", "/", "zu", "rich\u00b7ten", "mei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "NN", "KON", "NN", "$(", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "versag / den Geistes- Strom / die Fl\u00fcgelflamm / mir nicht.", "tokens": ["ver\u00b7sag", "/", "den", "Geis\u00b7tes", "Strom", "/", "die", "Fl\u00fc\u00b7gel\u00b7flamm", "/", "mir", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ART", "TRUNC", "NN", "$(", "ART", "NN", "$(", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ja mach den Muht zu Glut: dich br\u00fcnstig zuerheben!", "tokens": ["ja", "mach", "den", "Muht", "zu", "Glut", ":", "dich", "br\u00fcns\u00b7tig", "zu\u00b7er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich such kein eigne Ehr / verdiene sie auch nie.", "tokens": ["Ich", "such", "kein", "eig\u00b7ne", "Ehr", "/", "ver\u00b7die\u00b7ne", "sie", "auch", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$(", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Siht aber jemand was Geist-nutzliches allhie:", "tokens": ["Siht", "a\u00b7ber", "je\u00b7mand", "was", "Geist\u00b7nutz\u00b7li\u00b7ches", "all\u00b7hie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PWS", "NN", "ADV", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "so bitt' ich ihn durch Gott / er woll mir nicht zuschreiben", "tokens": ["so", "bitt'", "ich", "ihn", "durch", "Gott", "/", "er", "woll", "mir", "nicht", "zu\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das Gut' in meiner Schrifft. der Ewig' ists allein /", "tokens": ["das", "Gut'", "in", "mei\u00b7ner", "Schrifft", ".", "der", "E\u00b7wig'", "ists", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der mir das Gute fl\u00f6st in Geist und Feder ein.", "tokens": ["der", "mir", "das", "Gu\u00b7te", "fl\u00f6st", "in", "Geist", "und", "Fe\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nur sein soll alles Lob / von mir und allen / bleiben.", "tokens": ["Nur", "sein", "soll", "al\u00b7les", "Lob", "/", "von", "mir", "und", "al\u00b7len", "/", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAINF", "VMFIN", "PIAT", "NN", "$(", "APPR", "PPER", "KON", "PIAT", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}