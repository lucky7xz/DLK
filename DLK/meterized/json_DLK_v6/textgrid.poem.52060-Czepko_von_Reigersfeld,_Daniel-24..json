{"textgrid.poem.52060": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sag es, Deutschland, dir, Sol Oesterreich ja fallen,", "tokens": ["Ich", "sag", "es", ",", "Deutschland", ",", "dir", ",", "Sol", "O\u00b7es\u00b7ter\u00b7reich", "ja", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$,", "PPER", "$,", "VMFIN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie iedem seinen Fall der Himmel auserkiest:", "tokens": ["Wie", "ie\u00b7dem", "sei\u00b7nen", "Fall", "der", "Him\u00b7mel", "aus\u00b7er\u00b7kiest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du f\u00e4llst mit ihm: Ich h\u00f6r, ich h\u00f6re Ha\u00fcser schallen,", "tokens": ["Du", "f\u00e4llst", "mit", "ihm", ":", "Ich", "h\u00f6r", ",", "ich", "h\u00f6\u00b7re", "Ha\u00fc\u00b7ser", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dann mit der Saulen f\u00e4llt, was drauff gebauet ist.", "tokens": ["Dann", "mit", "der", "Sau\u00b7len", "f\u00e4llt", ",", "was", "drauff", "ge\u00b7bau\u00b7et", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word",