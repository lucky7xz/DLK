{"textgrid.poem.58035": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "73. Ein stilles Wesen hat Gott erlesen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesus ist ein Friedensk\u00f6nig,", "tokens": ["Je\u00b7sus", "ist", "ein", "Frie\u00b7dens\u00b7k\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehe, h\u00f6r und rede wenig,", "tokens": ["Sie\u00b7he", ",", "h\u00f6r", "und", "re\u00b7de", "we\u00b7nig", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "KON", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was du tust, tu mit Bedacht,", "tokens": ["Was", "du", "tust", ",", "tu", "mit", "Be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "NE", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne St\u00f6ren, still und sacht;", "tokens": ["Oh\u00b7ne", "St\u00f6\u00b7ren", ",", "still", "und", "sacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bei allem deinem Werke", "tokens": ["Und", "bei", "al\u00b7lem", "dei\u00b7nem", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stets auf ihn im Grunde merke,", "tokens": ["Stets", "auf", "ihn", "im", "Grun\u00b7de", "mer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Da\u00df sein Friedensthron und -reich", "tokens": ["Da\u00df", "sein", "Frie\u00b7denst\u00b7hron", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nie aus deinem Herzen weich'!", "tokens": ["Nie", "aus", "dei\u00b7nem", "Her\u00b7zen", "weich'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}