{"textgrid.poem.67095": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mein Herz! noch alterst du nicht; wie Luna den Liebling", "tokens": ["Du", "mein", "Herz", "!", "noch", "al\u00b7terst", "du", "nicht", ";", "wie", "Lu\u00b7na", "den", "Lieb\u00b7ling"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$.", "ADV", "ADV", "PPER", "PTKNEG", "$.", "PWAV", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Weckte des Himmels Kind, die Freude, vom Schlafe dich wieder;", "tokens": ["Weck\u00b7te", "des", "Him\u00b7mels", "Kind", ",", "die", "Freu\u00b7de", ",", "vom", "Schla\u00b7fe", "dich", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN", "PPER", "ADV", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Denn Sie erwacht mit mir zu neuer, gl\u00fchender Jugend,", "tokens": ["Denn", "Sie", "er\u00b7wacht", "mit", "mir", "zu", "neu\u00b7er", ",", "gl\u00fc\u00b7hen\u00b7der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Meine Schwester, die s\u00fc\u00dfe Natur, und meine geliebten", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", ",", "die", "s\u00fc\u00b7\u00dfe", "Na\u00b7tur", ",", "und", "mei\u00b7ne", "ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Tale l\u00e4cheln mich an, und meine geliebteren Haine,", "tokens": ["Ta\u00b7le", "l\u00e4\u00b7cheln", "mich", "an", ",", "und", "mei\u00b7ne", "ge\u00b7lieb\u00b7te\u00b7ren", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Voll erfreulichen Vogelgesangs, und scherzender L\u00fcfte,", "tokens": ["Voll", "er\u00b7freu\u00b7li\u00b7chen", "Vo\u00b7gel\u00b7ge\u00b7sangs", ",", "und", "scher\u00b7zen\u00b7der", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Jauchzen in wilder Lust der freundlichen Gru\u00df mir entgegen.", "tokens": ["Jauch\u00b7zen", "in", "wil\u00b7der", "Lust", "der", "freund\u00b7li\u00b7chen", "Gru\u00df", "mir", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Der du Herzen verj\u00fcngst, und Fluren, heiliger Fr\u00fchling,", "tokens": ["Der", "du", "Her\u00b7zen", "ver\u00b7j\u00fcngst", ",", "und", "Flu\u00b7ren", ",", "hei\u00b7li\u00b7ger", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$,", "KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Heil dir! Erstgeborner der Zeit! erquickender Fr\u00fchling,", "tokens": ["Heil", "dir", "!", "Erst\u00b7ge\u00b7bor\u00b7ner", "der", "Zeit", "!", "er\u00b7qui\u00b7cken\u00b7der", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Erstgeborner im Scho\u00dfe der Zeit! Gewaltiger! Heil dir,", "tokens": ["Erst\u00b7ge\u00b7bor\u00b7ner", "im", "Scho\u00b7\u00dfe", "der", "Zeit", "!", "Ge\u00b7wal\u00b7ti\u00b7ger", "!", "Heil", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$.", "NN", "$.", "NN", "PPER", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Heil! die Fessel zerri\u00df; und t\u00f6nt dir Feierges\u00e4nge,", "tokens": ["Heil", "!", "die", "Fes\u00b7sel", "zer\u00b7ri\u00df", ";", "und", "t\u00f6nt", "dir", "Fei\u00b7er\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$.", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Da\u00df die Gestad erbeben, der Strom, wir J\u00fcnglinge taumeln,", "tokens": ["Da\u00df", "die", "Ge\u00b7stad", "er\u00b7be\u00b7ben", ",", "der", "Strom", ",", "wir", "J\u00fcng\u00b7lin\u00b7ge", "tau\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,", "ART", "NN", "$,", "PPER", "NN", "VVINF", "$,"], "meter": "---+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Jauchzen hinaus, wo der Strom dich preist, wir enth\u00fcllen, du Holder,", "tokens": ["Jauch\u00b7zen", "hin\u00b7aus", ",", "wo", "der", "Strom", "dich", "preist", ",", "wir", "ent\u00b7h\u00fcl\u00b7len", ",", "du", "Hol\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APZR", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "PPER", "VVINF", "$,", "PPER", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Deinem Liebeshauche die gl\u00fchende Brust, und st\u00fcrzen hinunter", "tokens": ["Dei\u00b7nem", "Lie\u00b7bes\u00b7hau\u00b7che", "die", "gl\u00fc\u00b7hen\u00b7de", "Brust", ",", "und", "st\u00fcr\u00b7zen", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "PTKVZ"], "meter": "+-+-+--+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "In den Strom, und jauchzen mit ihm, und nennen dich Bruder.", "tokens": ["In", "den", "Strom", ",", "und", "jauch\u00b7zen", "mit", "ihm", ",", "und", "nen\u00b7nen", "dich", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Bruder! wie tanzt so sch\u00f6n, mit tausendf\u00e4ltiger Freude,", "tokens": ["Bru\u00b7der", "!", "wie", "tanzt", "so", "sch\u00f6n", ",", "mit", "tau\u00b7send\u00b7f\u00e4l\u00b7ti\u00b7ger", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "VVFIN", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ach! und tausendf\u00e4ltiger Lieb im l\u00e4chelnden Aether", "tokens": ["Ach", "!", "und", "tau\u00b7send\u00b7f\u00e4l\u00b7ti\u00b7ger", "Lieb", "im", "l\u00e4\u00b7cheln\u00b7den", "A\u00b7e\u00b7ther"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Deine Erde dahin, seit aus Elysiums Talen", "tokens": ["Dei\u00b7ne", "Er\u00b7de", "da\u00b7hin", ",", "seit", "aus", "E\u00b7ly\u00b7si\u00b7ums", "Ta\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Du mit dem Zauberstab ihr nahtest, himmlischer J\u00fcngling!", "tokens": ["Du", "mit", "dem", "Zau\u00b7ber\u00b7stab", "ihr", "nah\u00b7test", ",", "himm\u00b7li\u00b7scher", "J\u00fcng\u00b7ling", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sahn wir nicht, wie sie freundlicher nun den stolzen Geliebten", "tokens": ["Sahn", "wir", "nicht", ",", "wie", "sie", "freund\u00b7li\u00b7cher", "nun", "den", "stol\u00b7zen", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "PPER", "ADJD", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Gr\u00fc\u00dft', den heiligen Tag, wenn er k\u00fchn vom Siege der Schatten", "tokens": ["Gr\u00fc\u00dft'", ",", "den", "hei\u00b7li\u00b7gen", "Tag", ",", "wenn", "er", "k\u00fchn", "vom", "Sie\u00b7ge", "der", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "\u00dcber die Berge flammt! wie sie sanfterr\u00f6tend im Schleier", "tokens": ["\u00dc\u00b7ber", "die", "Ber\u00b7ge", "flammt", "!", "wie", "sie", "sanf\u00b7ter\u00b7r\u00f6\u00b7tend", "im", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "PWAV", "PPER", "ADJD", "APPRART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Silberner D\u00fcfte verh\u00fcllt, in s\u00fc\u00dfen Erwartungen aufblickt,", "tokens": ["Sil\u00b7ber\u00b7ner", "D\u00fcf\u00b7te", "ver\u00b7h\u00fcllt", ",", "in", "s\u00fc\u00b7\u00dfen", "Er\u00b7war\u00b7tun\u00b7gen", "auf\u00b7blickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+-+--+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Bis sie gl\u00fchet von ihm, und ihre friedlichen Kinder", "tokens": ["Bis", "sie", "gl\u00fc\u00b7het", "von", "ihm", ",", "und", "ih\u00b7re", "fried\u00b7li\u00b7chen", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Alle, Blumen und Hain', und Saaten und sprossende Reben, ...", "tokens": ["Al\u00b7le", ",", "Blu\u00b7men", "und", "Hain'", ",", "und", "Saa\u00b7ten", "und", "spros\u00b7sen\u00b7de", "Re\u00b7ben", ",", "..."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "$,", "NN", "KON", "NE", "$,", "KON", "NN", "KON", "ADJA", "NN", "$,", "$("], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Schlummre, schlummre nun, mit deinen friedlichen Kindern,", "tokens": ["Schlumm\u00b7re", ",", "schlumm\u00b7re", "nun", ",", "mit", "dei\u00b7nen", "fried\u00b7li\u00b7chen", "Kin\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Mutter Erde! denn Helios hat die gl\u00fchenden Rosse", "tokens": ["Mut\u00b7ter", "Er\u00b7de", "!", "denn", "He\u00b7li\u00b7os", "hat", "die", "gl\u00fc\u00b7hen\u00b7den", "Ros\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "KON", "NE", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "L\u00e4ngst zur Ruhe gelenkt, und die freundlichen Helden des Himmels,", "tokens": ["L\u00e4ngst", "zur", "Ru\u00b7he", "ge\u00b7lenkt", ",", "und", "die", "freund\u00b7li\u00b7chen", "Hel\u00b7den", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,", "KON", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Perseus dort, und Herkules dort, sie wallen in stiller", "tokens": ["Per\u00b7seus", "dort", ",", "und", "Her\u00b7ku\u00b7les", "dort", ",", "sie", "wal\u00b7len", "in", "stil\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "KON", "NN", "ADV", "$,", "PPER", "VVINF", "APPR", "ADJA"], "meter": "--+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Liebe vorbei, und leise durchstreift der fl\u00fcsternde Nachthauch", "tokens": ["Lie\u00b7be", "vor\u00b7bei", ",", "und", "lei\u00b7se", "durch\u00b7streift", "der", "fl\u00fcs\u00b7tern\u00b7de", "Nacht\u00b7hauch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Deine fr\u00f6hliche Saat, und die fernher t\u00f6nenden B\u00e4che", "tokens": ["Dei\u00b7ne", "fr\u00f6h\u00b7li\u00b7che", "Saat", ",", "und", "die", "fern\u00b7her", "t\u00f6\u00b7nen\u00b7den", "B\u00e4\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Lispeln Schlummerges\u00e4nge darein, ...", "tokens": ["Lis\u00b7peln", "Schlum\u00b7mer\u00b7ge\u00b7s\u00e4n\u00b7ge", "da\u00b7rein", ",", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "PAV", "$,", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}