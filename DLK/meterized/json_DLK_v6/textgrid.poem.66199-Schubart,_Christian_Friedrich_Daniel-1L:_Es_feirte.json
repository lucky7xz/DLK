{"textgrid.poem.66199": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es feirte ", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es feirte ", "tokens": ["Es", "feir\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sein Lebensfest zum erstenmal,", "tokens": ["Sein", "Le\u00b7bens\u00b7fest", "zum", "ers\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Begr\u00fc\u00dften ihn im neuen Sonnenstrahl.", "tokens": ["Be\u00b7gr\u00fc\u00df\u00b7ten", "ihn", "im", "neu\u00b7en", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da sah in der Verkl\u00e4rung Lichte", "tokens": ["Da", "sah", "in", "der", "Ver\u00b7kl\u00e4\u00b7rung", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die gro\u00dfe Seele sie und sprach:", "tokens": ["Die", "gro\u00b7\u00dfe", "See\u00b7le", "sie", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbwie wohl ist mir! O Vater, Mutter, Nichte,", "tokens": ["\u00bb", "wie", "wohl", "ist", "mir", "!", "O", "Va\u00b7ter", ",", "Mut\u00b7ter", ",", "Nich\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "KOKOM", "ADV", "VAFIN", "PPER", "$.", "NE", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie wohl ist mir! ach, f\u00fchlt mir's nach!", "tokens": ["Wie", "wohl", "ist", "mir", "!", "ach", ",", "f\u00fchlt", "mir's", "nach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "$.", "XY", "$,", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mich wirft nicht mehr mit wildem Tosen", "tokens": ["Mich", "wirft", "nicht", "mehr", "mit", "wil\u00b7dem", "To\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des ersten Lebens Woge hin und her.", "tokens": ["Des", "ers\u00b7ten", "Le\u00b7bens", "Wo\u00b7ge", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die H\u00fcgellasten armer Gro\u00dfen", "tokens": ["Die", "H\u00fc\u00b7gel\u00b7las\u00b7ten", "ar\u00b7mer", "Gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Erm\u00fcden meinen Geist nicht mehr.", "tokens": ["Er\u00b7m\u00fc\u00b7den", "mei\u00b7nen", "Geist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mein abgequ\u00e4lter Leib ruht deinem an der Seite,", "tokens": ["Mein", "ab\u00b7ge\u00b7qu\u00e4l\u00b7ter", "Leib", "ruht", "dei\u00b7nem", "an", "der", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "O, mich durchschauert schon die Ahnung jener Freude,", "tokens": ["O", ",", "mich", "durch\u00b7schau\u00b7ert", "schon", "die", "Ah\u00b7nung", "je\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn diese Leiber Gott herauf ins Leben ruft.", "tokens": ["Wenn", "die\u00b7se", "Lei\u00b7ber", "Gott", "her\u00b7auf", "ins", "Le\u00b7ben", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie lieblich l\u00e4chelt mir der Paradiesesmorgen!", "tokens": ["Wie", "lieb\u00b7lich", "l\u00e4\u00b7chelt", "mir", "der", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So fei'rt' ich auf der Welt nie meinen Werdetag,", "tokens": ["So", "fei'rt'", "ich", "auf", "der", "Welt", "nie", "mei\u00b7nen", "Wer\u00b7de\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ach, dort wo unter tausend Herrschersorgen", "tokens": ["Ach", ",", "dort", "wo", "un\u00b7ter", "tau\u00b7send", "Herr\u00b7scher\u00b7sor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "PWAV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Mein Leib so fr\u00fch \u2013 doch nicht mein Geist erlag.", "tokens": ["Mein", "Leib", "so", "fr\u00fch", "\u2013", "doch", "nicht", "mein", "Geist", "er\u00b7lag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$(", "ADV", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "La\u00dft uns, umstr\u00f6mt von diesen Morgenr\u00f6then,", "tokens": ["La\u00dft", "uns", ",", "um\u00b7str\u00f6mt", "von", "die\u00b7sen", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Bestrahlt von diesem Sonnengold,", "tokens": ["Be\u00b7strahlt", "von", "die\u00b7sem", "Son\u00b7nen\u00b7gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Auf jenem Cedernh\u00fcgel beten", "tokens": ["Auf", "je\u00b7nem", "Ce\u00b7dern\u00b7h\u00fc\u00b7gel", "be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Sie knieten, beteten; als sie gebetet hatten,", "tokens": ["Sie", "knie\u00b7ten", ",", "be\u00b7te\u00b7ten", ";", "als", "sie", "ge\u00b7be\u00b7tet", "hat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da s\u00e4uselt' es im Cedernschatten:", "tokens": ["Da", "s\u00e4u\u00b7selt'", "es", "im", "Ce\u00b7dern\u00b7schat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "\u00bbich bin der Herr! Bin Euch, bin ", "tokens": ["\u00bb", "ich", "bin", "der", "Herr", "!", "Bin", "Euch", ",", "bin"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$.", "VAFIN", "PPER", "$,", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Bin ", "tokens": ["Bin"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}}}}}