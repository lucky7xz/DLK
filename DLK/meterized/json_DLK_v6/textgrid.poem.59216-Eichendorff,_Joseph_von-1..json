{"textgrid.poem.59216": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich klimm zum Berg und schau zur niedern Erde,", "tokens": ["Ich", "klimm", "zum", "Berg", "und", "schau", "zur", "nie\u00b7dern", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich klimm hinab und schau die Berge an,", "tokens": ["Ich", "klimm", "hin\u00b7ab", "und", "schau", "die", "Ber\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "S\u00fc\u00df-melancholisch spitzt sich die Geb\u00e4rde", "tokens": ["S\u00fc\u00df\u00b7me\u00b7lan\u00b7cho\u00b7lisch", "spitzt", "sich", "die", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und gift'ge Weltverachtung ficht mich an;", "tokens": ["Und", "gift'\u00b7ge", "Welt\u00b7ver\u00b7ach\u00b7tung", "ficht", "mich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch will aus Schmerz und Ha\u00df nichts Rechtes werden.", "tokens": ["Doch", "will", "aus", "Schmerz", "und", "Ha\u00df", "nichts", "Rech\u00b7tes", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ermanne dich! \u2013 Ich bin doch wohl ein Mann? \u2013", "tokens": ["Er\u00b7man\u00b7ne", "dich", "!", "\u2013", "Ich", "bin", "doch", "wohl", "ein", "Mann", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ach! wie tr\u00e4ge Silb aus Silbe schleichet,", "tokens": ["Und", "ach", "!", "wie", "tr\u00e4\u00b7ge", "Silb", "aus", "Sil\u00b7be", "schlei\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit Not hab ich den letzten Reim erreichet.", "tokens": ["Mit", "Not", "hab", "ich", "den", "letz\u00b7ten", "Reim", "er\u00b7rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O weg mit Reim und Leierklang und Singen!", "tokens": ["O", "weg", "mit", "Reim", "und", "Lei\u00b7er\u00b7klang", "und", "Sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fa\u00df, Leben, wieder mich lebendig an!", "tokens": ["Fa\u00df", ",", "Le\u00b7ben", ",", "wie\u00b7der", "mich", "le\u00b7ben\u00b7dig", "an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit deiner Woge will ich freudig ringen,", "tokens": ["Mit", "dei\u00b7ner", "Wo\u00b7ge", "will", "ich", "freu\u00b7dig", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die tief mich st\u00fcrzt, hebt mich auch himmelan.", "tokens": ["Die", "tief", "mich", "st\u00fcrzt", ",", "hebt", "mich", "auch", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Sturme spannt der Adler seine Schwingen \u2013", "tokens": ["Im", "Stur\u00b7me", "spannt", "der", "Ad\u00b7ler", "sei\u00b7ne", "Schwin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Blas zu! da sp\u00fcr ich wieder, da\u00df ich Mann!", "tokens": ["Blas", "zu", "!", "da", "sp\u00fcr", "ich", "wie\u00b7der", ",", "da\u00df", "ich", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Viel lieber will ich raschen Tod erwerben,", "tokens": ["Viel", "lie\u00b7ber", "will", "ich", "ra\u00b7schen", "Tod", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als, so verschmachtend, lebenslang zu sterben.", "tokens": ["Als", ",", "so", "ver\u00b7schmach\u00b7tend", ",", "le\u00b7bens\u00b7lang", "zu", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "VVPP", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}