{"textgrid.poem.65269": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Leiermann", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dr\u00fcben hinter'm Dorfe", "tokens": ["Dr\u00fc\u00b7ben", "hin\u00b7ter'm", "Dor\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Steht ein Leiermann,", "tokens": ["Steht", "ein", "Lei\u00b7er\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und mit starren Fingern", "tokens": ["Und", "mit", "star\u00b7ren", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dreht er was er kann.", "tokens": ["Dreht", "er", "was", "er", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Barfu\u00df auf dem Eise", "tokens": ["Bar\u00b7fu\u00df", "auf", "dem", "Ei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schwankt er hin und her;", "tokens": ["Schwankt", "er", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und sein kleiner Teller", "tokens": ["Und", "sein", "klei\u00b7ner", "Tel\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bleibt ihm immer leer.", "tokens": ["Bleibt", "ihm", "im\u00b7mer", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Keiner mag ihn h\u00f6ren,", "tokens": ["Kei\u00b7ner", "mag", "ihn", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Keiner sieht ihn an;", "tokens": ["Kei\u00b7ner", "sieht", "ihn", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Hunde brummen", "tokens": ["Und", "die", "Hun\u00b7de", "brum\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Um den alten Mann.", "tokens": ["Um", "den", "al\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Und er l\u00e4\u00dft es gehen", "tokens": ["Und", "er", "l\u00e4\u00dft", "es", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Alles, wie es will,", "tokens": ["Al\u00b7les", ",", "wie", "es", "will", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dreht, und seine Leier", "tokens": ["Dreht", ",", "und", "sei\u00b7ne", "Lei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Steht ihm nimmer still.", "tokens": ["Steht", "ihm", "nim\u00b7mer", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Wunderlicher Alter,", "tokens": ["Wun\u00b7der\u00b7li\u00b7cher", "Al\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Soll ich mit dir gehn?", "tokens": ["Soll", "ich", "mit", "dir", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Willst zu meinen Liedern", "tokens": ["Willst", "zu", "mei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Deine Leier drehn?", "tokens": ["Dei\u00b7ne", "Lei\u00b7er", "drehn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}