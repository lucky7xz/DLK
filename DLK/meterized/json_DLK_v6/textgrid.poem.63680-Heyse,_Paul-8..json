{"textgrid.poem.63680": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast, mein Herz, Zeit meines Lebens mir", "tokens": ["Du", "hast", ",", "mein", "Herz", ",", "Zeit", "mei\u00b7nes", "Le\u00b7bens", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "NN", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Herausgeholfen aus so manchen F\u00e4hrden;", "tokens": ["Her\u00b7aus\u00b7ge\u00b7hol\u00b7fen", "aus", "so", "man\u00b7chen", "F\u00e4hr\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In geist'gen N\u00f6ten, leiblichen Beschwerden,", "tokens": ["In", "geist'\u00b7gen", "N\u00f6\u00b7ten", ",", "leib\u00b7li\u00b7chen", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein tapfrer Busenfreund, vertraut' ich dir.", "tokens": ["Mein", "tapf\u00b7rer", "Bu\u00b7sen\u00b7freund", ",", "ver\u00b7traut'", "ich", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun aber scheint's f\u00fcrwahr, als sollten wir", "tokens": ["Nun", "a\u00b7ber", "scheint's", "f\u00fcr\u00b7wahr", ",", "als", "soll\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "$,", "KOUS", "VMFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entzweit auf unsre alten Tage werden,", "tokens": ["Ent\u00b7zweit", "auf", "uns\u00b7re", "al\u00b7ten", "Ta\u00b7ge", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn du beginnst, dich unhold zu geb\u00e4rden,", "tokens": ["Denn", "du", "be\u00b7ginnst", ",", "dich", "un\u00b7hold", "zu", "ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und machst mir Not gleich einem Feinde schier.", "tokens": ["Und", "machst", "mir", "Not", "gleich", "ei\u00b7nem", "Fein\u00b7de", "schier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So raffe dich denn auf, statt schwach und tr\u00e4ge", "tokens": ["So", "raf\u00b7fe", "dich", "denn", "auf", ",", "statt", "schwach", "und", "tr\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "KOUI", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein altes Blutumlaufsgesch\u00e4ft zu treiben,", "tokens": ["Dein", "al\u00b7tes", "Blu\u00b7tum\u00b7laufs\u00b7ge\u00b7sch\u00e4ft", "zu", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und regle wieder deine muntren Schl\u00e4ge.", "tokens": ["Und", "re\u00b7gle", "wie\u00b7der", "dei\u00b7ne", "mun\u00b7tren", "Schl\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sonst, tu mir eins noch: pl\u00f6tzlich stehn zu bleiben,", "tokens": ["Sonst", ",", "tu", "mir", "eins", "noch", ":", "pl\u00f6tz\u00b7lich", "stehn", "zu", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PIS", "ADV", "$.", "ADJD", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich im Nu mein Leben enden m\u00f6ge,", "tokens": ["Da\u00df", "sich", "im", "Nu", "mein", "Le\u00b7ben", "en\u00b7den", "m\u00f6\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Statt langsam nach und nach mich aufzureiben!", "tokens": ["Statt", "lang\u00b7sam", "nach", "und", "nach", "mich", "auf\u00b7zu\u00b7rei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "KON", "APPR", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}