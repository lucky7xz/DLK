{"dta.poem.23787": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Das Neue Jahr.  \n Sonnet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "So bleibt auf ewig nun das alte Jahr zur\u00fccke;", "tokens": ["So", "bleibt", "auf", "e\u00b7wig", "nun", "das", "al\u00b7te", "Jahr", "zu\u00b7r\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie theilt der Son\u0303en-Lauf so schnell die Zeiten ab!", "tokens": ["Wie", "theilt", "der", "So\u00f1en\u00b7Lauf", "so", "schnell", "die", "Zei\u00b7ten", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie schleppet uns so bald das Alter in das Grab!", "tokens": ["Wie", "schlep\u00b7pet", "uns", "so", "bald", "das", "Al\u00b7ter", "in", "das", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das hei\u00dft wol schlecht gelebt; Die wenig Augenblicke/", "tokens": ["Das", "hei\u00dft", "wol", "schlecht", "ge\u00b7lebt", ";", "Die", "we\u00b7nig", "Au\u00b7gen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVPP", "$.", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In welche\u0303 viel Verdru\u00df vermischt mit schlechtem Gl\u00fccke/", "tokens": ["In", "welch\u1ebd", "viel", "Ver\u00b7dru\u00df", "ver\u00b7mischt", "mit", "schlech\u00b7tem", "Gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und lauter Unbestand sich zu erkennen gab;", "tokens": ["Und", "lau\u00b7ter", "Un\u00b7be\u00b7stand", "sich", "zu", "er\u00b7ken\u00b7nen", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das hei\u00dft wol schlecht gewohnt/ wenn uns der Wan-", "tokens": ["Das", "hei\u00dft", "wol", "schlecht", "ge\u00b7wohnt", "/", "wenn", "uns", "der", "Wan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVPP", "$(", "KOUS", "PPER", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nie aus den H\u00e4nden k\u00f6mmt; Wenn wir durch List und", "tokens": ["Nie", "aus", "den", "H\u00e4n\u00b7den", "k\u00f6mmt", ";", "Wenn", "wir", "durch", "List", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$.", "KOUS", "PPER", "APPR", "NN", "KON"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Hinstraucheln in der Nacht/ da wenig Licht zu sehn/", "tokens": ["Hins\u00b7trau\u00b7cheln", "in", "der", "Nacht", "/", "da", "we\u00b7nig", "Licht", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$(", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Licht dem allemahl nicht sicher nachzugehn.", "tokens": ["Und", "Licht", "dem", "al\u00b7le\u00b7mahl", "nicht", "si\u00b7cher", "nach\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn so der H\u00f6chste nicht ein eignes Licht wil weisen/", "tokens": ["Denn", "so", "der", "H\u00f6chs\u00b7te", "nicht", "ein", "eig\u00b7nes", "Licht", "wil", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das/ wenn wir uns verwirrt/ uns Sinn und Auge", "tokens": ["Das", "/", "wenn", "wir", "uns", "ver\u00b7wirrt", "/", "uns", "Sinn", "und", "Au\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$(", "KOUS", "PPER", "PRF", "ADJD", "$(", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist alles Licht ein Licht das zur Verdamni\u00df f\u00fchrt:", "tokens": ["Ist", "al\u00b7les", "Licht", "ein", "Licht", "das", "zur", "Ver\u00b7dam\u00b7ni\u00df", "f\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ART", "NN", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O gar zu kurtze Zeit! O gar zu schweres Reisen!", "tokens": ["O", "gar", "zu", "kurt\u00b7ze", "Zeit", "!", "O", "gar", "zu", "schwe\u00b7res", "Rei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "$.", "NE", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}