{"textgrid.poem.41153": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft sieht ", "tokens": ["Oft", "sieht"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Um Mitternacht an dem Waldbrunnen stehn,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "an", "dem", "Wald\u00b7brun\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dort lehnt sie sich ans moos'ge Kreuz von Stein,", "tokens": ["Dort", "lehnt", "sie", "sich", "ans", "moos'\u00b7ge", "Kreuz", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als f\u00fchlt' sie unterm Herzen tiefe Wehn.", "tokens": ["Als", "f\u00fchlt'", "sie", "un\u00b7term", "Her\u00b7zen", "tie\u00b7fe", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bleich, bla\u00df und stumm, wie nur der Mond kann sein,", "tokens": ["Bleich", ",", "bla\u00df", "und", "stumm", ",", "wie", "nur", "der", "Mond", "kann", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV", "ADV", "ART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Blickt erst sie in den Brunnen still hinein,", "tokens": ["Blickt", "erst", "sie", "in", "den", "Brun\u00b7nen", "still", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann wirft sie zitternd was in seinen Schacht", "tokens": ["Dann", "wirft", "sie", "zit\u00b7ternd", "was", "in", "sei\u00b7nen", "Schacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und st\u00fcrzt sich j\u00e4hlings nach in seine Nacht.", "tokens": ["Und", "st\u00fcrzt", "sich", "j\u00e4h\u00b7lings", "nach", "in", "sei\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dumpf aus der Tiefe dr\u00f6hnt der schwere Fall,", "tokens": ["Dumpf", "aus", "der", "Tie\u00b7fe", "dr\u00f6hnt", "der", "schwe\u00b7re", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Die Wasser rauschen auf am Brunnenstein,", "tokens": ["Die", "Was\u00b7ser", "rau\u00b7schen", "auf", "am", "Brun\u00b7nen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch Todesstille wird es bald darauf,", "tokens": ["Doch", "To\u00b7des\u00b7stil\u00b7le", "wird", "es", "bald", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In schwarze Wolken h\u00fcllt das Kreuz sich ein,", "tokens": ["In", "schwar\u00b7ze", "Wol\u00b7ken", "h\u00fcllt", "das", "Kreuz", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und die Waldblume h\u00f6rt zu duften auf.", "tokens": ["Und", "die", "Wald\u00b7blu\u00b7me", "h\u00f6rt", "zu", "duf\u00b7ten", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKZU", "VVFIN", "PTKVZ", "$."], "meter": "--++-+-+-+", "measure": "anapaest.init"}}}}}