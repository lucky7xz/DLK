{"dta.poem.6921": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kan man auch mit Recht begehren,", "tokens": ["Wie", "kan", "man", "auch", "mit", "Recht", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df alle Bilder, welche sich", "tokens": ["Da\u00df", "al\u00b7le", "Bil\u00b7der", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jm Augenblick formiren und verst\u00f6hren,", "tokens": ["Jm", "Au\u00b7gen\u00b7blick", "for\u00b7mi\u00b7ren", "und", "ver\u00b7st\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In welchen eigentlich", "tokens": ["In", "wel\u00b7chen", "ei\u00b7gent\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PWAT", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch Raum noch Zeit enthalten,", "tokens": ["Noch", "Raum", "noch", "Zeit", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}