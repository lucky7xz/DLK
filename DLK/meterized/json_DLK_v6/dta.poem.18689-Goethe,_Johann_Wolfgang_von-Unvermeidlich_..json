{"dta.poem.18689": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Unvermeidlich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wer kann gebieten den V\u00f6geln", "tokens": ["Wer", "kann", "ge\u00b7bie\u00b7ten", "den", "V\u00f6\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Still zu seyn auf der Flur?", "tokens": ["Still", "zu", "seyn", "auf", "der", "Flur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und wer verbieten zu zappeln", "tokens": ["Und", "wer", "ver\u00b7bie\u00b7ten", "zu", "zap\u00b7peln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Schafen unter der Schur?", "tokens": ["Den", "Scha\u00b7fen", "un\u00b7ter", "der", "Schur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Stell\u2019 ich mich wohl ungeb\u00e4rdig", "tokens": ["Stell'", "ich", "mich", "wohl", "un\u00b7ge\u00b7b\u00e4r\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn mir die Wolle kraust?", "tokens": ["Wenn", "mir", "die", "Wol\u00b7le", "kraust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nein! Die Ungeb\u00e4rden entzwingt mir", "tokens": ["Nein", "!", "Die", "Un\u00b7ge\u00b7b\u00e4r\u00b7den", "ent\u00b7zwingt", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der Scheerer der mich zerzaust.", "tokens": ["Der", "Schee\u00b7rer", "der", "mich", "zer\u00b7zaust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer will mir wehren zu singen", "tokens": ["Wer", "will", "mir", "weh\u00b7ren", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach Lust zum Himmel hinan?", "tokens": ["Nach", "Lust", "zum", "Him\u00b7mel", "hi\u00b7nan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Wolken zu vertrauen", "tokens": ["Den", "Wol\u00b7ken", "zu", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie lieb sie mir\u2019s angethan.", "tokens": ["Wie", "lieb", "sie", "mir's", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "NE", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}