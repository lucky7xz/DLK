{"textgrid.poem.28079": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spielen soll Ergetzung seyn;", "tokens": ["Spie\u00b7len", "soll", "Er\u00b7get\u00b7zung", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses wil mir doch nicht ein;", "tokens": ["Die\u00b7ses", "wil", "mir", "doch", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie da\u00df der, der einb\u00fcst viel,", "tokens": ["Wie", "da\u00df", "der", ",", "der", "ein\u00b7b\u00fcst", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "$,", "PRELS", "ADV", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Glauben kan, es sey ein Spiel.", "tokens": ["Glau\u00b7ben", "kan", ",", "es", "sey", "ein", "Spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}