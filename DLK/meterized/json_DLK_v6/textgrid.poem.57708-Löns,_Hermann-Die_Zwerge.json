{"textgrid.poem.57708": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die Zwerge", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Riese lud die Zwerge ein", "tokens": ["Der", "Rie\u00b7se", "lud", "die", "Zwer\u00b7ge", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Fisch und Fleisch und Bier und Wein.", "tokens": ["Zu", "Fisch", "und", "Fleisch", "und", "Bier", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Zwerge sagten: Gro\u00dfe Ehr'!", "tokens": ["Die", "Zwer\u00b7ge", "sag\u00b7ten", ":", "Gro\u00b7\u00dfe", "Ehr'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir kommen gern und danken sehr.", "tokens": ["Wir", "kom\u00b7men", "gern", "und", "dan\u00b7ken", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie machten sich gef\u00e4hrlich breit", "tokens": ["Sie", "mach\u00b7ten", "sich", "ge\u00b7f\u00e4hr\u00b7lich", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und a\u00dfen nach der Schwierigkeit.", "tokens": ["Und", "a\u00b7\u00dfen", "nach", "der", "Schwie\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie tranken mehr, als ihnen gut,", "tokens": ["Sie", "tran\u00b7ken", "mehr", ",", "als", "ih\u00b7nen", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlorn beim Heimgang Stock und Hut.", "tokens": ["Ver\u00b7lorn", "beim", "Heim\u00b7gang", "Stock", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Befanden sich drei Tage schlimm,", "tokens": ["Be\u00b7fan\u00b7den", "sich", "drei", "Ta\u00b7ge", "schlimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "CARD", "NN", "ADJD", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und waren voller Gift und Grimm.", "tokens": ["Und", "wa\u00b7ren", "vol\u00b7ler", "Gift", "und", "Grimm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das Fleisch, das war ja mehr als z\u00e4h',", "tokens": ["Das", "Fleisch", ",", "das", "war", "ja", "mehr", "als", "z\u00e4h'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ADV", "PIAT", "KOKOM", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Weine kriegt man Sch\u00e4delweh.", "tokens": ["Vom", "Wei\u00b7ne", "kriegt", "man", "Sch\u00e4\u00b7del\u00b7weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "War viel zu jung, und dann der Fisch,", "tokens": ["War", "viel", "zu", "jung", ",", "und", "dann", "der", "Fisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der war ganz sicher nicht mehr frisch.", "tokens": ["Der", "war", "ganz", "si\u00b7cher", "nicht", "mehr", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und \u00fcberhaupt: so gro\u00df zu sein,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7haupt", ":", "so", "gro\u00df", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist unmanierlich und nicht fein!", "tokens": ["Ist", "un\u00b7ma\u00b7nier\u00b7lich", "und", "nicht", "fein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}