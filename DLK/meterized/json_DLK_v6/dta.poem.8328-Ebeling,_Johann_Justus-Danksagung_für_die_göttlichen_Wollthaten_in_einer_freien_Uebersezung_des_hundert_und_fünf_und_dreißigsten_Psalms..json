{"dta.poem.8328": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Danksagung f\u00fcr die g\u00f6ttlichen  \n Wollthaten in einer freien Uebersezung  \n des hundert und f\u00fcnf und  \n drei\u00dfigsten Psalms.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lobt des H\u00f6chsten Herrligkeit, seiner Gott-\nheit ewge Macht,", "tokens": ["Lobt", "des", "H\u00f6chs\u00b7ten", "Herr\u00b7lig\u00b7keit", ",", "sei\u00b7ner", "Got\u00b7t", "heit", "ew\u00b7ge", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PPOSAT", "TRUNC", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Seine wunderbahre G\u00fct, seiner Weis-", "tokens": ["Sei\u00b7ne", "wun\u00b7der\u00b7bah\u00b7re", "G\u00fct", ",", "sei\u00b7ner", "Weis"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lobt ihr Knechte euren ", "tokens": ["Lobt", "ihr", "Knech\u00b7te", "eu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seinen hocherhabnen Ruhm, seinen majest\u00e4tschen", "tokens": ["Sei\u00b7nen", "ho\u00b7cher\u00b7hab\u00b7nen", "Ruhm", ",", "sei\u00b7nen", "ma\u00b7jes\u00b7t\u00e4t\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Lobt den HErrn alle Zeit, die ihr steht im Hei-\nligthum,", "tokens": ["Lobt", "den", "Herrn", "al\u00b7le", "Zeit", ",", "die", "ihr", "steht", "im", "Hei", "lig\u00b7thum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "TRUNC", "NN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und verk\u00fcndigt ihm der Welt, die ihr seid sein Ei-", "tokens": ["Und", "ver\u00b7k\u00fcn\u00b7digt", "ihm", "der", "Welt", ",", "die", "ihr", "seid", "sein", "Ei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "PPOSAT", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "La\u00dft durch eurer Lieder-Thon, Tempel und des Vor-", "tokens": ["La\u00dft", "durch", "eu\u00b7rer", "Lie\u00b7der\u00b7Thon", ",", "Tem\u00b7pel", "und", "des", "Vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "$,", "NN", "KON", "ART", "NN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Von des H\u00f6chsten Herrligkeit, immer freudig wie-", "tokens": ["Von", "des", "H\u00f6chs\u00b7ten", "Herr\u00b7lig\u00b7keit", ",", "im\u00b7mer", "freu\u00b7dig", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "TRUNC"], "meter": "--+-+-++-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Lobt ihn weil er freundlich ist, und uns seine\nG\u00fctigkeit,", "tokens": ["Lobt", "ihn", "weil", "er", "freund\u00b7lich", "ist", ",", "und", "uns", "sei\u00b7ne", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "KON", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "In dem Reiche der Natur, in dem Gnadenreich an-", "tokens": ["In", "dem", "Rei\u00b7che", "der", "Na\u00b7tur", ",", "in", "dem", "Gna\u00b7den\u00b7reich", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "ART", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Singet ihm zu seiner Ehr, immer s\u00fcsse Andachts-", "tokens": ["Sin\u00b7get", "ihm", "zu", "sei\u00b7ner", "Ehr", ",", "im\u00b7mer", "s\u00fcs\u00b7se", "An\u00b7dachts"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADJA", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Denn er l\u00e4st sich ja mit Lust wiederum zu uns her-", "tokens": ["Denn", "er", "l\u00e4st", "sich", "ja", "mit", "Lust", "wie\u00b7de\u00b7rum", "zu", "uns", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "APPR", "NN", "ADV", "APPR", "PPER", "TRUNC"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Er hat Jacob auserw\u00e4hlt, der da war sein treu-\ner Knecht,", "tokens": ["Er", "hat", "Ja\u00b7cob", "au\u00b7ser\u00b7w\u00e4hlt", ",", "der", "da", "war", "sein", "treu", "er", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVPP", "$,", "PRELS", "ADV", "VAFIN", "PPOSAT", "TRUNC", "PPER", "NN", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er erkie\u00dft zum Eigenthum Jsrael und sein Ge-", "tokens": ["Er", "er\u00b7kie\u00dft", "zum", "Ei\u00b7gen\u00b7thum", "Js\u00b7rael", "und", "sein", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE", "KON", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Alle die den Jacob gleich, ob sie schon nicht sind", "tokens": ["Al\u00b7le", "die", "den", "Ja\u00b7cob", "gleich", ",", "ob", "sie", "schon", "nicht", "sind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ART", "NE", "ADV", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VAFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Aus dem Stamm des frommen Knechts, hat er", "tokens": ["Aus", "dem", "Stamm", "des", "from\u00b7men", "Knechts", ",", "hat", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "VAFIN", "PPER"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df der H\u00f6chste warlich gros, weis man aus\nNatur und Schrift,", "tokens": ["Da\u00df", "der", "H\u00f6chs\u00b7te", "war\u00b7lich", "gros", ",", "weis", "man", "aus", "Na\u00b7tur", "und", "Schrift", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo man Zeugen seiner Macht allenthalben gnug", "tokens": ["Wo", "man", "Zeu\u00b7gen", "sei\u00b7ner", "Macht", "al\u00b7len\u00b7thal\u00b7ben", "gnug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "NN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Er ist einzig unser HErr, und der Heiden blinde", "tokens": ["Er", "ist", "ein\u00b7zig", "un\u00b7ser", "Herr", ",", "und", "der", "Hei\u00b7den", "blin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "NN", "$,", "KON", "ART", "NN", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Die ein eingebildtes Nichts, sind daher wie Nichts", "tokens": ["Die", "ein", "ein\u00b7ge\u00b7bild\u00b7tes", "Nichts", ",", "sind", "da\u00b7her", "wie", "Nichts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "VAFIN", "PAV", "PWAV", "PIS"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Was sein Wille nur gebeut, was und wie er\netwas spricht,", "tokens": ["Was", "sein", "Wil\u00b7le", "nur", "ge\u00b7beut", ",", "was", "und", "wie", "er", "et\u00b7was", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$,", "PWS", "KON", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Steht im Augenblikke da, alles was er will ge-", "tokens": ["Steht", "im", "Au\u00b7gen\u00b7blik\u00b7ke", "da", ",", "al\u00b7les", "was", "er", "will", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$,", "PIS", "PWS", "PPER", "VMFIN", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "In dem Himmel auf der Erd, in den regen Meeres-", "tokens": ["In", "dem", "Him\u00b7mel", "auf", "der", "Erd", ",", "in", "den", "re\u00b7gen", "Mee\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "In der Tieffen d\u00fcstren Schlund, mu\u00df sich alles gleich", "tokens": ["In", "der", "Tief\u00b7fen", "d\u00fcst\u00b7ren", "Schlund", ",", "mu\u00df", "sich", "al\u00b7les", "gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,", "VMFIN", "PRF", "PIS", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Er erregt der Wolken Dunst, der aus jeder\nGegend zieht,", "tokens": ["Er", "er\u00b7regt", "der", "Wol\u00b7ken", "Dunst", ",", "der", "aus", "je\u00b7der", "Ge\u00b7gend", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und durch seinen Wink bewegt, wo er will, so gleich", "tokens": ["Und", "durch", "sei\u00b7nen", "Wink", "be\u00b7wegt", ",", "wo", "er", "will", ",", "so", "gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er schaft den entglomnen Bliz, und den Tropfen-", "tokens": ["Er", "schaft", "den", "ent\u00b7glom\u00b7nen", "Bliz", ",", "und", "den", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "TRUNC"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aus der tief verborgnen Klufft, kan sein Wink den", "tokens": ["Aus", "der", "tief", "ver\u00b7borg\u00b7nen", "Klufft", ",", "kan", "sein", "Wink", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,", "VMFIN", "PPOSAT", "NN", "ART"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Als ein starker Zebaoth macht er sich Egypten-\nland,", "tokens": ["Als", "ein", "star\u00b7ker", "Ze\u00b7bao\u00b7th", "macht", "er", "sich", "E\u00b7gyp\u00b7ten", "land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "TRUNC", "NN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch die Wundervolle That in dem Untergang be-", "tokens": ["Durch", "die", "Wun\u00b7der\u00b7vol\u00b7le", "That", "in", "dem", "Un\u00b7ter\u00b7gang", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da sich alle Erstgebuhrt von den Menschen, von den", "tokens": ["Da", "sich", "al\u00b7le", "Erst\u00b7ge\u00b7buhrt", "von", "den", "Men\u00b7schen", ",", "von", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PRF", "PIAT", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Muste gleich in einer Nacht, auf der Allmacht", "tokens": ["Mus\u00b7te", "gleich", "in", "ei\u00b7ner", "Nacht", ",", "auf", "der", "All\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.9": {"line.1": {"text": "Seine Macht Egyptenland! d\u00e4mpfte deine Ra-\nserei,", "tokens": ["Sei\u00b7ne", "Macht", "E\u00b7gyp\u00b7ten\u00b7land", "!", "d\u00e4mpf\u00b7te", "dei\u00b7ne", "Ra", "se\u00b7rei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$.", "VVFIN", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und brach Pharao Genik und der Knechte Hals", "tokens": ["Und", "brach", "Pha\u00b7rao", "Ge\u00b7nik", "und", "der", "Knech\u00b7te", "Hals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Woltest du in deinem Stolz, nicht vor ihm gehorsamst", "tokens": ["Wol\u00b7test", "du", "in", "dei\u00b7nem", "Stolz", ",", "nicht", "vor", "ihm", "ge\u00b7hor\u00b7samst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PTKNEG", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "So erschrakst du vor die Krafft seiner grossen Wun-", "tokens": ["So", "er\u00b7schrakst", "du", "vor", "die", "Krafft", "sei\u00b7ner", "gros\u00b7sen", "Wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "ADJA", "TRUNC"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Gros ist seine Majest\u00e4t, die die m\u00e4chtgen Hel-\nden schl\u00e4gt,\nDritter Theil. JUnd\n\nDanksagung f\u00fcr die g\u00f6ttlichen Wolthaten.", "tokens": ["Gros", "ist", "sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", ",", "die", "die", "m\u00e4cht\u00b7gen", "Hel", "den", "schl\u00e4gt", ",", "Drit\u00b7ter", "Theil", ".", "Jund", "Dank\u00b7sa\u00b7gung", "f\u00fcr", "die", "g\u00f6tt\u00b7li\u00b7chen", "Wolt\u00b7ha\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "ART", "VVFIN", "$,", "ADJA", "NN", "$.", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+--+-+-+--+-+--++-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Und der V\u00f6lker grosse Macht, in dem Staub zu", "tokens": ["Und", "der", "V\u00f6l\u00b7ker", "gros\u00b7se", "Macht", ",", "in", "dem", "Staub", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,", "APPR", "ART", "NN", "PTKZU"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die der K\u00f6nige zertritt, da\u00df sie gleich zu seinen F\u00fcs-", "tokens": ["Die", "der", "K\u00f6\u00b7ni\u00b7ge", "zer\u00b7tritt", ",", "da\u00df", "sie", "gleich", "zu", "sei\u00b7nen", "F\u00fcs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In der blassen Ohnmacht todt, und entseelet liegen", "tokens": ["In", "der", "blas\u00b7sen", "Ohn\u00b7macht", "todt", ",", "und", "ent\u00b7see\u00b7let", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word",