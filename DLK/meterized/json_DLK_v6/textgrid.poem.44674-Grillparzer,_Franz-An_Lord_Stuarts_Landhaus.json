{"textgrid.poem.44674": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An Lord Stuarts Landhaus", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wozu die Warnungen auf allen Ecken,", "tokens": ["Wo\u00b7zu", "die", "War\u00b7nun\u00b7gen", "auf", "al\u00b7len", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit denen du verwahrst dein Lustrevier?", "tokens": ["Mit", "de\u00b7nen", "du", "ver\u00b7wahrst", "dein", "Lus\u00b7tre\u00b7vier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um jeden Frohen abzuschrecken", "tokens": ["Um", "je\u00b7den", "Fro\u00b7hen", "ab\u00b7zu\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Brauchts nichts als: \u00bbStuart wohnet hier.\u00ab", "tokens": ["Brauchts", "nichts", "als", ":", "\u00bb", "Stu\u00b7art", "woh\u00b7net", "hier", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "$.", "$(", "NE", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}