{"textgrid.poem.25330": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus heiligen Grotten, wo sie sich barg", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus heiligen Grotten, wo sie sich barg", "tokens": ["Aus", "hei\u00b7li\u00b7gen", "Grot\u00b7ten", ",", "wo", "sie", "sich", "barg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "vor dem grellen Rauschen des Tages,", "tokens": ["vor", "dem", "grel\u00b7len", "Rau\u00b7schen", "des", "Ta\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "kam leise die Nacht.", "tokens": ["kam", "lei\u00b7se", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schweigend blickt sie um sich mit den", "tokens": ["Schwei\u00b7gend", "blickt", "sie", "um", "sich", "mit", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PRF", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "sehnsuchtsstillen Augen, in deren d\u00e4mmernde Tiefen", "tokens": ["sehn\u00b7suchts\u00b7stil\u00b7len", "Au\u00b7gen", ",", "in", "de\u00b7ren", "d\u00e4m\u00b7mern\u00b7de", "Tie\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "du tauchst wie in des ewigen Meeres", "tokens": ["du", "tauchst", "wie", "in", "des", "e\u00b7wi\u00b7gen", "Mee\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "verschwimmende, dunkle Fernen.", "tokens": ["ver\u00b7schwim\u00b7men\u00b7de", ",", "dunk\u00b7le", "Fer\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Scheu flieht vor ihrem milden Glanze", "tokens": ["Scheu", "flieht", "vor", "ih\u00b7rem", "mil\u00b7den", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "der harte Tag.", "tokens": ["der", "har\u00b7te", "Tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Langsam gleitet sie und schweigend heran,", "tokens": ["Lang\u00b7sam", "glei\u00b7tet", "sie", "und", "schwei\u00b7gend", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "und kaum zittern die weiten Falten des", "tokens": ["und", "kaum", "zit\u00b7tern", "die", "wei\u00b7ten", "Fal\u00b7ten", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "dunklen Gewandes, das lang wallend sie umschmiegt.", "tokens": ["dunk\u00b7len", "Ge\u00b7wan\u00b7des", ",", "das", "lang", "wal\u00b7lend", "sie", "um\u00b7schmiegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "VVPP", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "N\u00e4her schwebt sie,", "tokens": ["N\u00e4\u00b7her", "schwebt", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "und atemlos harrend lauschen W\u00e4lder und Wiesen.", "tokens": ["und", "a\u00b7tem\u00b7los", "har\u00b7rend", "lau\u00b7schen", "W\u00e4l\u00b7der", "und", "Wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Denn ein Gottesdienst ist ihr Kommen,", "tokens": ["Denn", "ein", "Got\u00b7tes\u00b7dienst", "ist", "ihr", "Kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "ein Gebet, keusch und mild und voll Inbrunst.", "tokens": ["ein", "Ge\u00b7bet", ",", "keusch", "und", "mild", "und", "voll", "In\u00b7brunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Wehe \u00fcber euch, Menschen,", "tokens": ["We\u00b7he", "\u00fc\u00b7ber", "euch", ",", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.18": {"text": "die ihr nimmer sehet, was Hohes sich \u00fcber euch neigt!", "tokens": ["die", "ihr", "nim\u00b7mer", "se\u00b7het", ",", "was", "Ho\u00b7hes", "sich", "\u00fc\u00b7ber", "euch", "neigt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,", "PWS", "ADJA", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Wehe \u00fcber euch,", "tokens": ["We\u00b7he", "\u00fc\u00b7ber", "euch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "die ihr hinwegstampft \u00fcber den Einzug der Nacht", "tokens": ["die", "ihr", "hin\u00b7weg\u00b7stampft", "\u00fc\u00b7ber", "den", "Ein\u00b7zug", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "und hohnl\u00e4chelt \u00fcber ihr Evangelium", "tokens": ["und", "hohn\u00b7l\u00e4\u00b7chelt", "\u00fc\u00b7ber", "ihr", "E\u00b7van\u00b7ge\u00b7li\u00b7um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-++-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "wie \u00fcber den Gott von Nazareth!", "tokens": ["wie", "\u00fc\u00b7ber", "den", "Gott", "von", "Na\u00b7za\u00b7reth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Wehe \u00fcber euch, die ihr Schutz sucht", "tokens": ["We\u00b7he", "\u00fc\u00b7ber", "euch", ",", "die", "ihr", "Schutz", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.24": {"text": "vor dem gellenden Schrei des Gewissens", "tokens": ["vor", "dem", "gel\u00b7len\u00b7den", "Schrei", "des", "Ge\u00b7wis\u00b7sens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "hinter verschlossenen Fenstern!", "tokens": ["hin\u00b7ter", "ver\u00b7schlos\u00b7se\u00b7nen", "Fens\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Aber euch gr\u00fc\u00dfe ich, andachtsvolle Schw\u00e4rmer,", "tokens": ["A\u00b7ber", "euch", "gr\u00fc\u00b7\u00dfe", "ich", ",", "an\u00b7dachts\u00b7vol\u00b7le", "Schw\u00e4r\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "die ihr nicht wachet, noch schlafet, sondern hintr\u00e4umet", "tokens": ["die", "ihr", "nicht", "wa\u00b7chet", ",", "noch", "schla\u00b7fet", ",", "son\u00b7dern", "hin\u00b7tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.28": {"text": "und lauschet den leisen Stimmen der Nacht ...", "tokens": ["und", "lau\u00b7schet", "den", "lei\u00b7sen", "Stim\u00b7men", "der", "Nacht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Die ihr lauschet gleich der zitternden Natur,", "tokens": ["Die", "ihr", "lau\u00b7schet", "gleich", "der", "zit\u00b7tern\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "die gl\u00fchend den Atem verh\u00e4lt, da\u00df sie h\u00f6re,", "tokens": ["die", "gl\u00fc\u00b7hend", "den", "A\u00b7tem", "ver\u00b7h\u00e4lt", ",", "da\u00df", "sie", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.31": {"text": "was tr\u00e4umenden Mundes die Herrscherin singt:", "tokens": ["was", "tr\u00e4u\u00b7men\u00b7den", "Mun\u00b7des", "die", "Herr\u00b7sche\u00b7rin", "singt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.32": {"text": "\u00bbsiehe, ich kam, gezogen von fernen Inseln,", "tokens": ["\u00bb", "sie\u00b7he", ",", "ich", "kam", ",", "ge\u00b7zo\u00b7gen", "von", "fer\u00b7nen", "In\u00b7seln", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "$,", "PPER", "VVFIN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "wo ich ruhte in tiefen, dunklen Grotten.", "tokens": ["wo", "ich", "ruh\u00b7te", "in", "tie\u00b7fen", ",", "dunk\u00b7len", "Grot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Siehe, ich kam, und wie ich auswarf die seidenen,", "tokens": ["Sie\u00b7he", ",", "ich", "kam", ",", "und", "wie", "ich", "aus\u00b7warf", "die", "sei\u00b7de\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "$,", "KON", "PWAV", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "weichen Seile \u00fcber die Bl\u00fctenb\u00fcsche und hinschmiegte", "tokens": ["wei\u00b7chen", "Sei\u00b7le", "\u00fc\u00b7ber", "die", "Bl\u00fc\u00b7ten\u00b7b\u00fc\u00b7sche", "und", "hin\u00b7schmieg\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "durchsichtige Schleier auf Fels und Gras,", "tokens": ["durch\u00b7sich\u00b7ti\u00b7ge", "Schlei\u00b7er", "auf", "Fels", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.37": {"text": "da floh vor mir der Tag.", "tokens": ["da", "floh", "vor", "mir", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Weiche Harfenlieder entwuchsen meinem stillen Schreiten", "tokens": ["Wei\u00b7che", "Har\u00b7fen\u00b7lie\u00b7der", "ent\u00b7wuch\u00b7sen", "mei\u00b7nem", "stil\u00b7len", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.39": {"text": "und \u00fcberschwollen sanft sein rauhes Br\u00fcllen.", "tokens": ["und", "\u00fc\u00b7bersc\u00b7hwol\u00b7len", "sanft", "sein", "rau\u00b7hes", "Br\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Siehe, ich kam, und Blumen streutet ihr mir auf den Pfad,", "tokens": ["Sie\u00b7he", ",", "ich", "kam", ",", "und", "Blu\u00b7men", "streu\u00b7tet", "ihr", "mir", "auf", "den", "Pfad", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "$,", "KON", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.41": {"text": "duftende Blumen gl\u00fchender Erwartung.", "tokens": ["duf\u00b7ten\u00b7de", "Blu\u00b7men", "gl\u00fc\u00b7hen\u00b7der", "Er\u00b7war\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.42": {"text": "Aber die, denen ich ganz mich schenken wollte,", "tokens": ["A\u00b7ber", "die", ",", "de\u00b7nen", "ich", "ganz", "mich", "schen\u00b7ken", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.43": {"text": "denen ich k\u00fcnden wollte ewige Milde und Sch\u00f6ne,", "tokens": ["de\u00b7nen", "ich", "k\u00fcn\u00b7den", "woll\u00b7te", "e\u00b7wi\u00b7ge", "Mil\u00b7de", "und", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVINF", "VMFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.44": {"text": "sie verkrochen sich vor mir.", "tokens": ["sie", "ver\u00b7kro\u00b7chen", "sich", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Ich sah sie an:", "tokens": ["Ich", "sah", "sie", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "Sie achteten nicht der kl\u00e4renden Sehnsucht,", "tokens": ["Sie", "ach\u00b7te\u00b7ten", "nicht", "der", "kl\u00e4\u00b7ren\u00b7den", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.47": {"text": "die wie ferner Glockenklang mir aus dem Auge brach.", "tokens": ["die", "wie", "fer\u00b7ner", "Glo\u00b7cken\u00b7klang", "mir", "aus", "dem", "Au\u00b7ge", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ADV", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.48": {"text": "Ich sang zu ihnen:", "tokens": ["Ich", "sang", "zu", "ih\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.49": {"text": "Sie h\u00f6rten nicht meine Stimme, die l\u00f6sende ...", "tokens": ["Sie", "h\u00f6r\u00b7ten", "nicht", "mei\u00b7ne", "Stim\u00b7me", ",", "die", "l\u00f6\u00b7sen\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PP