{"dta.poem.23771": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Das gr\u00f6\u00dfte Gl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Von dem Gl\u00fccke alle Gaben,", "tokens": ["Von", "dem", "Gl\u00fc\u00b7cke", "al\u00b7le", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reichthum, Ehr und Sch\u00e4tze haben,", "tokens": ["Reicht\u00b7hum", ",", "Ehr", "und", "Sch\u00e4t\u00b7ze", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die\u00df ist zwar, ich mu\u00df gestehn,", "tokens": ["Die\u00df", "ist", "zwar", ",", "ich", "mu\u00df", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcnschenswerth und wundersch\u00f6n.", "tokens": ["W\u00fcn\u00b7schens\u00b7werth", "und", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch das gr\u00f6\u00dfte Gl\u00fcck auf Erden,", "tokens": ["Doch", "das", "gr\u00f6\u00df\u00b7te", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das uns kann zu Theile werden,", "tokens": ["Das", "uns", "kann", "zu", "Thei\u00b7le", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VMFIN", "APPR", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist des Gl\u00fccks, de\u00df wir uns freun,", "tokens": ["Ist", "des", "Gl\u00fccks", ",", "de\u00df", "wir", "uns", "freun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, des gr\u00f6\u00dften w\u00fcrdig seyn.", "tokens": ["Ja", ",", "des", "gr\u00f6\u00df\u00b7ten", "w\u00fcr\u00b7dig", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}