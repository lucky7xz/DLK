{"dta.poem.12018": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sowahr in dir er ist, der diese Welt erh\u00e4lt,", "tokens": ["So\u00b7wahr", "in", "dir", "er", "ist", ",", "der", "die\u00b7se", "Welt", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPER", "VAFIN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sowahr auch ist er in, nicht au\u00dferhalb der Welt.", "tokens": ["So\u00b7wahr", "auch", "ist", "er", "in", ",", "nicht", "au\u00b7\u00dfer\u00b7halb", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "$,", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch in ihm ist die Welt, sowahr in ihm du bist,", "tokens": ["Doch", "in", "ihm", "ist", "die", "Welt", ",", "so\u00b7wahr", "in", "ihm", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ART", "NN", "$,", "ADV", "APPR", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nicht in dir noch Welt, nur in sich selber ist.", "tokens": ["Der", "nicht", "in", "dir", "noch", "Welt", ",", "nur", "in", "sich", "sel\u00b7ber", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPER", "ADV", "NN", "$,", "ADV", "APPR", "PRF", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Solang du denken nicht die Widerspr\u00fcche kannst,", "tokens": ["So\u00b7lang", "du", "den\u00b7ken", "nicht", "die", "Wi\u00b7der\u00b7spr\u00fc\u00b7che", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O denke nicht, da\u00df du durch Denken Gott gewannst.", "tokens": ["O", "den\u00b7ke", "nicht", ",", "da\u00df", "du", "durch", "Den\u00b7ken", "Gott", "ge\u00b7wannst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}