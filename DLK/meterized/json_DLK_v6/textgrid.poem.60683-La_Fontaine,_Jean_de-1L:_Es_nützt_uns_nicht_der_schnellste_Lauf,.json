{"textgrid.poem.60683": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es n\u00fctzt uns nicht der schnellste Lauf,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es n\u00fctzt uns nicht der schnellste Lauf,", "tokens": ["Es", "n\u00fctzt", "uns", "nicht", "der", "schnells\u00b7te", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bricht man zur rechten Zeit nicht auf.", "tokens": ["Bricht", "man", "zur", "rech\u00b7ten", "Zeit", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bezeugt wird dies von Schildkr\u00f6te und Hase.", "tokens": ["Be\u00b7zeugt", "wird", "dies", "von", "Schild\u00b7kr\u00f6\u00b7te", "und", "Ha\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PDS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbich wette,\u00ab sagte jene einst im Grase", "tokens": ["\u00bb", "ich", "wet\u00b7te", ",", "\u00ab", "sag\u00b7te", "je\u00b7ne", "einst", "im", "Gra\u00b7se"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "$,", "$(", "VVFIN", "PDS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Uferwiesen, \u00bbda\u00df du nicht so bald wie ich", "tokens": ["Der", "U\u00b7fer\u00b7wie\u00b7sen", ",", "\u00bb", "da\u00df", "du", "nicht", "so", "bald", "wie", "ich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "KOKOM", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Ziel erreichst.\u00ab Vor Lachen w\u00e4lzte sich", "tokens": ["Das", "Ziel", "er\u00b7reichst", ".", "\u00ab", "Vor", "La\u00b7chen", "w\u00e4lz\u00b7te", "sich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPR", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der leichtf\u00fc\u00dfige Hase:", "tokens": ["Der", "leicht\u00b7f\u00fc\u00b7\u00dfi\u00b7ge", "Ha\u00b7se", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bbdu bist wohl nicht gescheit, geschw\u00e4tzige Base!", "tokens": ["\u00bb", "du", "bist", "wohl", "nicht", "ge\u00b7scheit", ",", "ge\u00b7schw\u00e4t\u00b7zi\u00b7ge", "Ba\u00b7se", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nimm Nieswurzk\u00f6rner ein, sie heilen dich", "tokens": ["Nimm", "Nies\u00b7wurz\u00b7k\u00f6r\u00b7ner", "ein", ",", "sie", "hei\u00b7len", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "PTKVZ", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vielleicht von deinem Irrwahn noch.\u00ab", "tokens": ["Viel\u00b7leicht", "von", "dei\u00b7nem", "Irr\u00b7wahn", "noch", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbsag, was du willst, ich wette doch!\u00ab", "tokens": ["\u00bb", "sag", ",", "was", "du", "willst", ",", "ich", "wet\u00b7te", "doch", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Man schlo\u00df den Pakt und legte gleich beim Ziel", "tokens": ["Man", "schlo\u00df", "den", "Pakt", "und", "leg\u00b7te", "gleich", "beim", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Den Einsatz nieder. Was \u2013 wieviel \u2013", "tokens": ["Den", "Ein\u00b7satz", "nie\u00b7der", ".", "Was", "\u2013", "wie\u00b7viel", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PWS", "$(", "PWAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ist einerlei, auch wer der Richter war im Spiel.", "tokens": ["Ist", "ei\u00b7ner\u00b7lei", ",", "auch", "wer", "der", "Rich\u00b7ter", "war", "im", "Spiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "ADV", "PWS", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Vier Spr\u00fcnge brauchte Meister Lampe nur zu machen.", "tokens": ["Vier", "Spr\u00fcn\u00b7ge", "brauch\u00b7te", "Meis\u00b7ter", "Lam\u00b7pe", "nur", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie oft schon hatte er, umringt von Hunden,", "tokens": ["Wie", "oft", "schon", "hat\u00b7te", "er", ",", "um\u00b7ringt", "von", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "PPER", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Im letzten Augenblick mit Spott und Lachen", "tokens": ["Im", "letz\u00b7ten", "Au\u00b7gen\u00b7blick", "mit", "Spott", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Aus aller Not sich blitzgeschwind herausgewunden.", "tokens": ["Aus", "al\u00b7ler", "Not", "sich", "blitz\u00b7ge\u00b7schwind", "her\u00b7aus\u00b7ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da er nun diesmal glaubte Zeit genug zu haben,", "tokens": ["Da", "er", "nun", "dies\u00b7mal", "glaub\u00b7te", "Zeit", "ge\u00b7nug", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zu warten und zu ruhn und sich am Kraut zu laben,", "tokens": ["Zu", "war\u00b7ten", "und", "zu", "ruhn", "und", "sich", "am", "Kraut", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "KON", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So l\u00e4\u00dft er unbesorgt die Andere traben.", "tokens": ["So", "l\u00e4\u00dft", "er", "un\u00b7be\u00b7sorgt", "die", "An\u00b7de\u00b7re", "tra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "So langsam die auch ist, sie strengt sich kr\u00e4ftig an,", "tokens": ["So", "lang\u00b7sam", "die", "auch", "ist", ",", "sie", "strengt", "sich", "kr\u00e4f\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADV", "VAFIN", "$,", "PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und kommt, trotz ihres Senatorengangs, voran.", "tokens": ["Und", "kommt", ",", "trotz", "ih\u00b7res", "Se\u00b7na\u00b7to\u00b7ren\u00b7gangs", ",", "vo\u00b7ran", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ihn k\u00fcmmert's nicht, mu\u00df ihm ja doch bei seinem Springen", "tokens": ["Ihn", "k\u00fcm\u00b7mert's", "nicht", ",", "mu\u00df", "ihm", "ja", "doch", "bei", "sei\u00b7nem", "Sprin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Im Augenblick der Sieg gelingen.", "tokens": ["Im", "Au\u00b7gen\u00b7blick", "der", "Sieg", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der Ruhm des Sieges ist f\u00fcr ihn nur klein,", "tokens": ["Der", "Ruhm", "des", "Sie\u00b7ges", "ist", "f\u00fcr", "ihn", "nur", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Unw\u00fcrdig scheint er ihm zu sein.", "tokens": ["Un\u00b7w\u00fcr\u00b7dig", "scheint", "er", "ihm", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Er fri\u00dft, er ruht, belustigt sich mit hundert Dingen,", "tokens": ["Er", "fri\u00dft", ",", "er", "ruht", ",", "be\u00b7lus\u00b7tigt", "sich", "mit", "hun\u00b7dert", "Din\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es dr\u00e4ngt ihn nichts. Erst als die Gegnerin", "tokens": ["Es", "dr\u00e4ngt", "ihn", "nichts", ".", "Erst", "als", "die", "Geg\u00b7ne\u00b7rin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$.", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Schon nah beim Ziele ist und beim Gewinn,", "tokens": ["Schon", "nah", "beim", "Zie\u00b7le", "ist", "und", "beim", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VAFIN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Umsonst! Der Sieg war nicht mehr zu erringen.", "tokens": ["Um\u00b7sonst", "!", "Der", "Sieg", "war", "nicht", "mehr", "zu", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Schildkr\u00f6te kam vor ihm als erste an.", "tokens": ["Schild\u00b7kr\u00f6\u00b7te", "kam", "vor", "ihm", "als", "ers\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "KOUS", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Sie rief: \u00bbNun, bin ich wirklich nicht gescheit?", "tokens": ["Sie", "rief", ":", "\u00bb", "Nun", ",", "bin", "ich", "wirk\u00b7lich", "nicht", "ge\u00b7scheit", "?"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "$,", "VAFIN", "PPER", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Was n\u00fctzte dir denn deine Schnelligkeit?", "tokens": ["Was", "n\u00fctz\u00b7te", "dir", "denn", "dei\u00b7ne", "Schnel\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Ich bin es, die den Preis gewann!", "tokens": ["Ich", "bin", "es", ",", "die", "den", "Preis", "ge\u00b7wann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wie aber s\u00e4h's mit dir, du Leichtfu\u00df, dann erst aus,", "tokens": ["Wie", "a\u00b7ber", "s\u00e4h's", "mit", "dir", ",", "du", "Leicht\u00b7fu\u00df", ",", "dann", "erst", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "APPR", "PPER", "$,", "PPER", "NN", "$,", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Tr\u00fcgst du auch noch, gleich mir, ein schweres Panzerhaus!\u00ab", "tokens": ["Tr\u00fcgst", "du", "auch", "noch", ",", "gleich", "mir", ",", "ein", "schwe\u00b7res", "Pan\u00b7zer\u00b7haus", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "PPER", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}