{"dta.poem.3222": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl mag es dir Verdru\u00df erwecken oder Bangen,", "tokens": ["Wohl", "mag", "es", "dir", "Ver\u00b7dru\u00df", "er\u00b7we\u00b7cken", "o\u00b7der", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "NN", "VVINF", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Irrthum so sich gibt f\u00fcr Wahrheit unbefangen.", "tokens": ["Wenn", "Irr\u00b7thum", "so", "sich", "gibt", "f\u00fcr", "Wahr\u00b7heit", "un\u00b7be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PRF", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn wie erkennst du, da\u00df dich lauter Wahrheit s\u00e4ugt,", "tokens": ["Denn", "wie", "er\u00b7kennst", "du", ",", "da\u00df", "dich", "lau\u00b7ter", "Wahr\u00b7heit", "s\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn auch der Irrthum von sich selbst ist \u00fcberzeugt?", "tokens": ["Wenn", "auch", "der", "Irr\u00b7thum", "von", "sich", "selbst", "ist", "\u00fc\u00b7berz\u00b7eugt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PRF", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gewi\u00df wird euern Streit einmal die Zeit entscheiden;", "tokens": ["Ge\u00b7wi\u00df", "wird", "eu\u00b7ern", "Streit", "ein\u00b7mal", "die", "Zeit", "ent\u00b7schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein zu jener Zeit, wo seid ihr dann, ihr Beiden?", "tokens": ["Al\u00b7lein", "zu", "je\u00b7ner", "Zeit", ",", "wo", "seid", "ihr", "dann", ",", "ihr", "Bei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wenn die Wahrheit dir mehr gilt, als Recht zu haben,", "tokens": ["Doch", "wenn", "die", "Wahr\u00b7heit", "dir", "mehr", "gilt", ",", "als", "Recht", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,", "KOUS", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tr\u00f6ste dich und stirb! denn sie wird nicht begraben.", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", "und", "stirb", "!", "denn", "sie", "wird", "nicht", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVIMP", "$.", "KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}