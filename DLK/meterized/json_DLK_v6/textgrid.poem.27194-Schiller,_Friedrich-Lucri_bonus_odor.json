{"textgrid.poem.27194": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lucri bonus odor", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00f6blich haben wir dich behandelt; das brauche zum Vorteil,", "tokens": ["Gr\u00f6b\u00b7lich", "ha\u00b7ben", "wir", "dich", "be\u00b7han\u00b7delt", ";", "das", "brau\u00b7che", "zum", "Vor\u00b7teil", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$.", "PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und im zw\u00f6lften Band schilt uns, da gibt es ein Blatt.", "tokens": ["Und", "im", "zw\u00f6lf\u00b7ten", "Band", "schilt", "uns", ",", "da", "gibt", "es", "ein", "Blatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}