{"textgrid.poem.29391": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fleisch, das in dem Leime wohnet, lebt in M\u00fch bey schlechter Kost;", "tokens": ["Fleisch", ",", "das", "in", "dem", "Lei\u00b7me", "woh\u00b7net", ",", "lebt", "in", "M\u00fch", "bey", "schlech\u00b7ter", "Kost", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Fleisch, das in den Steinen wohnet, lebt in Pracht und eitler Lust;", "tokens": ["Fleisch", ",", "das", "in", "den", "Stei\u00b7nen", "woh\u00b7net", ",", "lebt", "in", "Pracht", "und", "eit\u00b7ler", "Lust", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Fleisch im Leime, Fleisch in Steinen macht de\u00df Todes freyer Raub,", "tokens": ["Fleisch", "im", "Lei\u00b7me", ",", "Fleisch", "in", "Stei\u00b7nen", "macht", "de\u00df", "To\u00b7des", "frey\u00b7er", "Raub", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Das, wie jenes also dieses, iedes wird ein leichter Staub.", "tokens": ["Das", ",", "wie", "je\u00b7nes", "al\u00b7so", "die\u00b7ses", ",", "ie\u00b7des", "wird", "ein", "leich\u00b7ter", "Staub", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PDS", "ADV", "PDAT", "$,", "PIS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}