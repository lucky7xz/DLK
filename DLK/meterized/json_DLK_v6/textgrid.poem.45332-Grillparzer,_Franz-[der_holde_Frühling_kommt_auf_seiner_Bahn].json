{"textgrid.poem.45332": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der holde Fr\u00fchling kommt auf seiner Bahn]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der holde Fr\u00fchling kommt auf seiner Bahn,", "tokens": ["Der", "hol\u00b7de", "Fr\u00fch\u00b7ling", "kommt", "auf", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sch\u00f6pfung scheint willkommen ihn zu hei\u00dfen.", "tokens": ["Die", "Sch\u00f6p\u00b7fung", "scheint", "will\u00b7kom\u00b7men", "ihn", "zu", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch bei den Menschen k\u00fcndigt er sich an,", "tokens": ["Auch", "bei", "den", "Men\u00b7schen", "k\u00fcn\u00b7digt", "er", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch Zahnschmerz und durch Ohrenrei\u00dfen.", "tokens": ["Durch", "Zahn\u00b7schmerz", "und", "durch", "Oh\u00b7ren\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}}}}}