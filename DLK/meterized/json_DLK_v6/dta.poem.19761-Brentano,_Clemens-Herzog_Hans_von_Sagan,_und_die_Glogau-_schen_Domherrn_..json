{"dta.poem.19761": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Herzog Hans von Sagan, und die Glogau-  \n schen Domherrn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hannes der Herzog zu Sagan               ", "tokens": ["Han\u00b7nes", "der", "Her\u00b7zog", "zu", "Sa\u00b7gan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Grimme lag in schwerem Bann,", "tokens": ["Der", "Grim\u00b7me", "lag", "in", "schwe\u00b7rem", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Bischof ", "tokens": ["Der", "Bi\u00b7schof"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Den Bann lie\u00df \u00fcber ihn sprechen.", "tokens": ["Den", "Bann", "lie\u00df", "\u00fc\u00b7ber", "ihn", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201eund lieg ich auch in tiefem Bann,", "tokens": ["\u201e", "und", "lieg", "ich", "auch", "in", "tie\u00b7fem", "Bann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So kehr ich mich kein Daumen dran:", "tokens": ["So", "kehr", "ich", "mich", "kein", "Dau\u00b7men", "dran", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4t Herzog Hannes sagen,", "tokens": ["Th\u00e4t", "Her\u00b7zog", "Han\u00b7nes", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Domherrn will ich fragen.", "tokens": ["Die", "Dom\u00b7herrn", "will", "ich", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eihr Glogschen Domherrn kommt herbey,", "tokens": ["\u201e", "ihr", "Glog\u00b7schen", "Dom\u00b7herrn", "kommt", "her\u00b7bey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ela\u00dft mit euch reden frank und frey,", "tokens": ["\u201e", "la\u00dft", "mit", "euch", "re\u00b7den", "frank", "und", "frey", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "APPR", "PPER", "VVINF", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "\u201ekommt ihr zu meinen vier Pf\u00e4hlen,", "tokens": ["\u201e", "kommt", "ihr", "zu", "mei\u00b7nen", "vier", "Pf\u00e4h\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "\u201eihr k\u00f6nnts euch selber w\u00e4hlen.\u201c", "tokens": ["\u201e", "ihr", "k\u00f6nnts", "euch", "sel\u00b7ber", "w\u00e4h\u00b7len", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ein euern vier Pf\u00e4hlen gehts nicht an,", "tokens": ["\u201e", "in", "eu\u00b7ern", "vier", "Pf\u00e4h\u00b7len", "gehts", "nicht", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "CARD", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201edieweil ihr seyd in schwerem Bann,", "tokens": ["\u201e", "die\u00b7weil", "ihr", "seyd", "in", "schwe\u00b7rem", "Bann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eruft uns zu andern Orten,", "tokens": ["\u201e", "ruft", "uns", "zu", "an\u00b7dern", "Or\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eda wollen wir eurer warten.", "tokens": ["\u201e", "da", "wol\u00b7len", "wir", "eu\u00b7rer", "war\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PPOSAT", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Er b'stellt sie auf die Br\u00fccke schlau,", "tokens": ["Er", "stell", "sie", "auf", "die", "Br\u00fc\u00b7cke", "schlau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die werthen Domherrn von Glogau,", "tokens": ["Die", "wert\u00b7hen", "Dom\u00b7herrn", "von", "Glo\u00b7gau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Herzog kam gegangen,", "tokens": ["Der", "Her\u00b7zog", "kam", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Rede th\u00e4t er anfangen.", "tokens": ["Die", "Re\u00b7de", "th\u00e4t", "er", "an\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie sprachen viel und mancherley", "tokens": ["Sie", "spra\u00b7chen", "viel", "und", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Riz, Raz, da ging der Boden entzwey,", "tokens": ["Riz", ",", "Raz", ",", "da", "ging", "der", "Bo\u00b7den", "ent\u00b7zwey", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wohl hinter ihrem R\u00fccken", "tokens": ["Wohl", "hin\u00b7ter", "ih\u00b7rem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zers\u00e4gte man die Br\u00fccken.", "tokens": ["Zer\u00b7s\u00e4g\u00b7te", "man", "die", "Br\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u201enun seht euch um, ihr Herrn gemach,", "tokens": ["\u201e", "nun", "seht", "euch", "um", ",", "ihr", "Herrn", "ge\u00b7mach", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herzog grimmen Tones sprach,", "tokens": ["Der", "Her\u00b7zog", "grim\u00b7men", "To\u00b7nes", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eihr Herren wollt ihr singen,", "tokens": ["\u201e", "ihr", "Her\u00b7ren", "wollt", "ihr", "sin\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eihr Herren wollt ihr springen?", "tokens": ["\u201e", "ihr", "Her\u00b7ren", "wollt", "ihr", "sprin\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Herren sahn die Wassersnoth,", "tokens": ["Die", "Her\u00b7ren", "sahn", "die", "Was\u00b7sers\u00b7noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sahen vorn und hinten Tod:", "tokens": ["Sie", "sa\u00b7hen", "vorn", "und", "hin\u00b7ten", "Tod", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ees mu\u00df euch wohl gelingen", "tokens": ["\u201e", "es", "mu\u00df", "euch", "wohl", "ge\u00b7lin\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eherr Hans, wir wollen singen.", "tokens": ["\u201e", "herr", "Hans", ",", "wir", "wol\u00b7len", "sin\u00b7gen", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und darauf gingen all nach Haus,", "tokens": ["Und", "da\u00b7rauf", "gin\u00b7gen", "all", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PIAT", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herzog lacht sie lustig aus:", "tokens": ["Der", "Her\u00b7zog", "lacht", "sie", "lus\u00b7tig", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Spas, der war gelungen,", "tokens": ["Sein", "Spas", ",", "der", "war", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Lied, das ist gesungen.", "tokens": ["Mein", "Lied", ",", "das", "ist", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}