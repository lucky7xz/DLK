{"textgrid.poem.24899": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne sinkt; mit dunkelroten Wogen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sinkt; mit dunkelroten Wogen", "tokens": ["Die", "Son\u00b7ne", "sinkt", ";", "mit", "dun\u00b7kel\u00b7ro\u00b7ten", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wallt noch das Sp\u00e4trot durch die Fensterbogen,", "tokens": ["Wallt", "noch", "das", "Sp\u00e4t\u00b7rot", "durch", "die", "Fens\u00b7ter\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Indes schon D\u00e4mmrung auf der Vega ruht;", "tokens": ["In\u00b7des", "schon", "D\u00e4mm\u00b7rung", "auf", "der", "Ve\u00b7ga", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vergoldet gl\u00fchn Granadas Tempelspitzen,", "tokens": ["Ver\u00b7gol\u00b7det", "gl\u00fchn", "Gra\u00b7na\u00b7das", "Tem\u00b7pel\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und die Nevada wirft in Purpurblitzen", "tokens": ["Und", "die", "Ne\u00b7va\u00b7da", "wirft", "in", "Pur\u00b7pur\u00b7blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ins Thal zur\u00fcck die Abendglut.", "tokens": ["Ins", "Thal", "zu\u00b7r\u00fcck", "die", "A\u00b7bend\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann bleicht der Glanz, so wie auf Wangen Bl\u00e4sse", "tokens": ["Dann", "bleicht", "der", "Glanz", ",", "so", "wie", "auf", "Wan\u00b7gen", "Bl\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "KOKOM", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der R\u00f6te folgt; der Schatten der Cypresse", "tokens": ["Der", "R\u00f6\u00b7te", "folgt", ";", "der", "Schat\u00b7ten", "der", "Cyp\u00b7res\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dehnt l\u00e4nger sich, bis er in Nacht zerbricht;", "tokens": ["Dehnt", "l\u00e4n\u00b7ger", "sich", ",", "bis", "er", "in", "Nacht", "zer\u00b7bricht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PRF", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Durch duft'ge W\u00f6lkchen, die am Himmel schwimmen,", "tokens": ["Durch", "duft'\u00b7ge", "W\u00f6lk\u00b7chen", ",", "die", "am", "Him\u00b7mel", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dringt, wie ein Liebesblick, mit sanftem Glimmen", "tokens": ["Dringt", ",", "wie", "ein", "Lie\u00b7bes\u00b7blick", ",", "mit", "sanf\u00b7tem", "Glim\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Abendsternes Silberlicht.", "tokens": ["Des", "A\u00b7bends\u00b7ter\u00b7nes", "Sil\u00b7ber\u00b7licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Schon seh' ich, wie die Fluren m\u00e4hlich dunkeln,", "tokens": ["Schon", "seh'", "ich", ",", "wie", "die", "Flu\u00b7ren", "m\u00e4h\u00b7lich", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von unten hier und da ein L\u00e4mpchen funkeln,", "tokens": ["Von", "un\u00b7ten", "hier", "und", "da", "ein", "L\u00e4mp\u00b7chen", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das vor dem Bild der Mutter Gottes brennt,", "tokens": ["Das", "vor", "dem", "Bild", "der", "Mut\u00b7ter", "Got\u00b7tes", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und weiter, in den H\u00e4usern und Kapellen", "tokens": ["Und", "wei\u00b7ter", ",", "in", "den", "H\u00e4u\u00b7sern", "und", "Ka\u00b7pel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Lichter z\u00fcndend, sich die Stadt erhellen,", "tokens": ["Die", "Lich\u00b7ter", "z\u00fcn\u00b7dend", ",", "sich", "die", "Stadt", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie \u00fcber ihr das Firmament.", "tokens": ["Wie", "\u00fc\u00b7ber", "ihr", "das", "Fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zur Ruhe unter ihren Bl\u00fcten\u00e4sten", "tokens": ["Zur", "Ru\u00b7he", "un\u00b7ter", "ih\u00b7ren", "Bl\u00fc\u00b7ten\u00b7\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Streckt sich Granada hin, indes aus Westen", "tokens": ["Streckt", "sich", "Gra\u00b7na\u00b7da", "hin", ",", "in\u00b7des", "aus", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "NE", "PTKVZ", "$,", "ADV", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sich tiefrer Schatten um die Erde schlingt;", "tokens": ["Sich", "tief\u00b7rer", "Schat\u00b7ten", "um", "die", "Er\u00b7de", "schlingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sanft, wie sie entschl\u00e4ft beim Sternenglanze,", "tokens": ["Und", "sanft", ",", "wie", "sie", "ent\u00b7schl\u00e4ft", "beim", "Ster\u00b7nen\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verklingt in ihre Tr\u00e4ume die Romanze,", "tokens": ["Ver\u00b7klingt", "in", "ih\u00b7re", "Tr\u00e4u\u00b7me", "die", "Ro\u00b7man\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die am Balkon der Ritter singt.", "tokens": ["Die", "am", "Bal\u00b7kon", "der", "Rit\u00b7ter", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}