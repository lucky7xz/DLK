{"textgrid.poem.54695": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "9. An L\u00e6lien", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr preiset mir umbsonst des Vatern hohen Stand/", "tokens": ["Ihr", "prei\u00b7set", "mir", "um\u00b7bsonst", "des", "Va\u00b7tern", "ho\u00b7hen", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mutter grosses Gutt/ der Br\u00fcder sch\u00f6nes Land/", "tokens": ["Der", "Mut\u00b7ter", "gros\u00b7ses", "Gutt", "/", "der", "Br\u00fc\u00b7der", "sch\u00f6\u00b7nes", "Land", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr la\u00dft mir ", "tokens": ["Ihr", "la\u00dft", "mir"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "++-", "measure": "unknown.measure.di"}, "line.4": {"text": "Wann an euch selbst nichts ist/ das w\u00fcrdig sey zu preisen.", "tokens": ["Wann", "an", "euch", "selbst", "nichts", "ist", "/", "das", "w\u00fcr\u00b7dig", "sey", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ADV", "PIS", "VAFIN", "$(", "ART", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}