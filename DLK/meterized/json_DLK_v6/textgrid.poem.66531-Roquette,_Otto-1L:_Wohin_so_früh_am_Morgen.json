{"textgrid.poem.66531": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin so fr\u00fch am Morgen", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin so fr\u00fch am Morgen", "tokens": ["Wo\u00b7hin", "so", "fr\u00fch", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit R\u00e4nzel und mit Stab?", "tokens": ["Mit", "R\u00e4n\u00b7zel", "und", "mit", "Stab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was treibt mit neuen Sorgen", "tokens": ["Was", "treibt", "mit", "neu\u00b7en", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich fort, beth\u00f6rter Knab?", "tokens": ["Dich", "fort", ",", "be\u00b7th\u00f6r\u00b7ter", "Knab", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es steht in deinen Z\u00fcgen", "tokens": ["Es", "steht", "in", "dei\u00b7nen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein lang' verhehlter Gram.", "tokens": ["Ein", "lang'", "ver\u00b7hehl\u00b7ter", "Gram", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sag, welch ein Ungen\u00fcgen", "tokens": ["Sag", ",", "welch", "ein", "Un\u00b7ge\u00b7n\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Daheim dich \u00fcberkam?", "tokens": ["Da\u00b7heim", "dich", "\u00fc\u00b7ber\u00b7kam", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbo la\u00dft getrost mich scheiden!", "tokens": ["\u00bb", "o", "la\u00dft", "ge\u00b7trost", "mich", "schei\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es will mein eigner Sinn", "tokens": ["Es", "will", "mein", "eig\u00b7ner", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Heimath mir verleiden,", "tokens": ["Die", "Hei\u00b7math", "mir", "ver\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ich entfremdet bin!", "tokens": ["Der", "ich", "ent\u00b7frem\u00b7det", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn noch einmal die Ferne", "tokens": ["Wenn", "noch", "ein\u00b7mal", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir Herz und Muth bewehrt,", "tokens": ["Mir", "Herz", "und", "Muth", "be\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann kehr' ich doppelt gerne", "tokens": ["Dann", "kehr'", "ich", "dop\u00b7pelt", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur\u00fcck zum alten Herd.\u00ab", "tokens": ["Zu\u00b7r\u00fcck", "zum", "al\u00b7ten", "Herd", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit Fliehen und mit Meiden", "tokens": ["Mit", "Flie\u00b7hen", "und", "mit", "Mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erhoffe nicht Gewinn!", "tokens": ["Er\u00b7hof\u00b7fe", "nicht", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Welt wird dich nicht scheiden", "tokens": ["Die", "Welt", "wird", "dich", "nicht", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von deinem eignen Sinn.", "tokens": ["Von", "dei\u00b7nem", "eig\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und traust du deinem Sterne,", "tokens": ["Und", "traust", "du", "dei\u00b7nem", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wirf ab, was dich beschwert!", "tokens": ["Wirf", "ab", ",", "was", "dich", "be\u00b7schwert", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es liegt in kleinster Ferne,", "tokens": ["Es", "liegt", "in", "kleins\u00b7ter", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was all dein Herz begehrt!", "tokens": ["Was", "all", "dein", "Herz", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbes ist kein weiches Bangen,", "tokens": ["\u00bb", "es", "ist", "kein", "wei\u00b7ches", "Ban\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist ein bittrer Groll,", "tokens": ["Es", "ist", "ein", "bit\u00b7trer", "Groll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der l\u00e4nger nicht befangen", "tokens": ["Der", "l\u00e4n\u00b7ger", "nicht", "be\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Herz mir halten soll.", "tokens": ["Das", "Herz", "mir", "hal\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft ab, la\u00dft ab, zu fragen!", "tokens": ["La\u00dft", "ab", ",", "la\u00dft", "ab", ",", "zu", "fra\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir selber bin ich feind,", "tokens": ["Mir", "sel\u00b7ber", "bin", "ich", "feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich so lang ertragen,", "tokens": ["Da\u00df", "ich", "so", "lang", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was dauernd ich vermeint!\u00ab", "tokens": ["Was", "dau\u00b7ernd", "ich", "ver\u00b7meint", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu schnell, zu hastig wandte", "tokens": ["Zu", "schnell", ",", "zu", "has\u00b7tig", "wand\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKA", "ADJD", "$,", "PTKA", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Groll sich zum Verzicht!", "tokens": ["Der", "Groll", "sich", "zum", "Ver\u00b7zicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was nur der Stolz verkannte,", "tokens": ["Was", "nur", "der", "Stolz", "ver\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Herz verkennt es nicht.", "tokens": ["Das", "Herz", "ver\u00b7kennt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Willst l\u00e4nger du beth\u00f6ren", "tokens": ["Willst", "l\u00e4n\u00b7ger", "du", "be\u00b7th\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Seele freien Blick,", "tokens": ["Der", "See\u00b7le", "frei\u00b7en", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du k\u00f6nntest mehr zerst\u00f6ren", "tokens": ["Du", "k\u00f6nn\u00b7test", "mehr", "zer\u00b7st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als nur dein eignes Gl\u00fcck!", "tokens": ["Als", "nur", "dein", "eig\u00b7nes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbzum alten Zauberkreise,", "tokens": ["\u00bb", "zum", "al\u00b7ten", "Zau\u00b7ber\u00b7krei\u00b7se", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den schon das Herz verschwor,", "tokens": ["Den", "schon", "das", "Herz", "ver\u00b7schwor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verlockt ihr holder Weise", "tokens": ["Ver\u00b7lockt", "ihr", "hol\u00b7der", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein allzuwillig Ohr!", "tokens": ["Mein", "all\u00b7zu\u00b7wil\u00b7lig", "Ohr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "W\u00e4r's Freude, w\u00e4r' es Bu\u00dfe,", "tokens": ["W\u00e4r's", "Freu\u00b7de", ",", "w\u00e4r'", "es", "Bu\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gewagt sei Gl\u00fcck und Noth!", "tokens": ["Ge\u00b7wagt", "sei", "Gl\u00fcck", "und", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und w\u00e4r's zum Abschiedsgru\u00dfe", "tokens": ["Und", "w\u00e4r's", "zum", "Ab\u00b7schieds\u00b7gru\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das schmerzlichste Verbot!\u00ab", "tokens": ["Das", "schmerz\u00b7lichs\u00b7te", "Ver\u00b7bot", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Ziel der langen Reise", "tokens": ["Das", "Ziel", "der", "lan\u00b7gen", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lugt an der Stra\u00dfe vor.", "tokens": ["Lugt", "an", "der", "Stra\u00b7\u00dfe", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kehr' um zum alten Kreise,", "tokens": ["Kehr'", "um", "zum", "al\u00b7ten", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und poch' an's rechte Thor!", "tokens": ["Und", "poch'", "an's", "rech\u00b7te", "Thor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimmst du mit raschem Fu\u00dfe,", "tokens": ["Nimmst", "du", "mit", "ra\u00b7schem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was dir der Groll verbot,", "tokens": ["Was", "dir", "der", "Groll", "ver\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So lachst du einst mit Mu\u00dfe", "tokens": ["So", "lachst", "du", "einst", "mit", "Mu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der \u00fcberstandnen Noth!", "tokens": ["Der", "\u00fc\u00b7bers\u00b7tand\u00b7nen", "Noth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}