{"textgrid.poem.49948": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Eduard Weigelin bisher Professor am Katharinenstift", "genre": "verse", "period": "N.A.", "pub_year": 1865, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund! dein heiterer Blick und deine gelassene Miene", "tokens": ["Freund", "!", "dein", "hei\u00b7te\u00b7rer", "Blick", "und", "dei\u00b7ne", "ge\u00b7las\u00b7se\u00b7ne", "Mie\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Hei\u00dft uns die Klage des Abschieds sparen; doch tief in der Brust dir", "tokens": ["Hei\u00dft", "uns", "die", "Kla\u00b7ge", "des", "Ab\u00b7schieds", "spa\u00b7ren", ";", "doch", "tief", "in", "der", "Brust", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$.", "ADV", "ADJD", "APPR", "ART", "NN", "PPER"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Selber bewegt sich das m\u00e4nnliche Herz. Wer m\u00f6cht es ihm wehren?", "tokens": ["Sel\u00b7ber", "be\u00b7wegt", "sich", "das", "m\u00e4nn\u00b7li\u00b7che", "Herz", ".", "Wer", "m\u00f6cht", "es", "ihm", "weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$.", "PWS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Denn du verl\u00e4ssest das Haus, das dir wie dein eigenes lieb war,", "tokens": ["Denn", "du", "ver\u00b7l\u00e4s\u00b7sest", "das", "Haus", ",", "das", "dir", "wie", "dein", "ei\u00b7ge\u00b7nes", "lieb", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "KOKOM", "PPOSAT", "ADJA", "ADJD", "VAFIN", "$,"], "meter": "-+-+--+-+-+---+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Dem du die Bl\u00fcte der Jahre geweiht im redlichen Tagwerk.", "tokens": ["Dem", "du", "die", "Bl\u00fc\u00b7te", "der", "Jah\u00b7re", "ge\u00b7weiht", "im", "red\u00b7li\u00b7chen", "Tag\u00b7werk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Aber gl\u00fccklich genug, der still sich dessen bewu\u00dft ist!", "tokens": ["A\u00b7ber", "gl\u00fcck\u00b7lich", "ge\u00b7nug", ",", "der", "still", "sich", "des\u00b7sen", "be\u00b7wu\u00dft", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "PRELS", "ADJD", "PRF", "PDS", "ADJD", "VAFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Siehe, die Zeit kommt auch, da ", "tokens": ["Sie\u00b7he", ",", "die", "Zeit", "kommt", "auch", ",", "da"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$,", "ART", "NN", "VVFIN", "ADV", "$,", "KOUS"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Ungern jeder f\u00fcrwahr, doch keiner mit besserem Ruhme,", "tokens": ["Un\u00b7gern", "je\u00b7der", "f\u00fcr\u00b7wahr", ",", "doch", "kei\u00b7ner", "mit", "bes\u00b7se\u00b7rem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$,", "ADV", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Noch von treueren W\u00fcnschen der dankbaren Liebe begleitet.", "tokens": ["Noch", "von", "treu\u00b7e\u00b7ren", "W\u00fcn\u00b7schen", "der", "dank\u00b7ba\u00b7ren", "Lie\u00b7be", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+--+-", "measure": "anapaest.di.plus"}}}}}