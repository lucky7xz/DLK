{"textgrid.poem.45980": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "An G\u00f6rres", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kn\u00fcpf' leichtes Lied zwei Freunde mir zusammen,", "tokens": ["Kn\u00fcpf'", "leich\u00b7tes", "Lied", "zwei", "Freun\u00b7de", "mir", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "CARD", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Deutsch, fromm, berauscht aus freier Kunst Pokalen.", "tokens": ["Deutsch", ",", "fromm", ",", "be\u00b7rauscht", "aus", "frei\u00b7er", "Kunst", "Po\u00b7ka\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Prophet'scher Warnung von den Lippen flammen,", "tokens": ["Pro\u00b7phet'\u00b7scher", "War\u00b7nung", "von", "den", "Lip\u00b7pen", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Frei Herz, das Pharis\u00e4er nur verdammen,", "tokens": ["Frei", "Herz", ",", "das", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "nur", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Weil Zukunft ihm vertraute ihre Qualen,", "tokens": ["Weil", "Zu\u00b7kunft", "ihm", "ver\u00b7trau\u00b7te", "ih\u00b7re", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mordrosse wollt' den offnen Weg verrammen,", "tokens": ["Mor\u00b7dros\u00b7se", "wollt'", "den", "off\u00b7nen", "Weg", "ver\u00b7ram\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Nimm hin dies Spiel gefesselt von der Zeit!", "tokens": ["Nimm", "hin", "dies", "Spiel", "ge\u00b7fes\u00b7selt", "von", "der", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PDS", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht quirlt ", "tokens": ["Nicht", "quirlt"], "token_info": ["word", "word"], "pos": ["PTKNEG", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Den Berg ", "tokens": ["Den", "Berg"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Im Milchmeer, brauend die Unsterblichkeit.", "tokens": ["Im", "Milch\u00b7meer", ",", "brau\u00b7end", "die", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nicht mir, Dir nur ist sie zu Dienst bereit,", "tokens": ["Nicht", "mir", ",", "Dir", "nur", "ist", "sie", "zu", "Dienst", "be\u00b7reit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "PPER", "ADV", "VAFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Da\u00df Deine Weltgesichte sie umfange", "tokens": ["Da\u00df", "Dei\u00b7ne", "Welt\u00b7ge\u00b7sich\u00b7te", "sie", "um\u00b7fan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und durch Natur und Zeit im Wirbeldrange", "tokens": ["Und", "durch", "Na\u00b7tur", "und", "Zeit", "im", "Wir\u00b7bel\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Umw\u00e4lze, spiegelnd eine Ewigkeit.", "tokens": ["Um\u00b7w\u00e4l\u00b7ze", ",", "spie\u00b7gelnd", "ei\u00b7ne", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gleich ", "tokens": ["Gleich"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Die bauernstolz am Schlangenschwanz nicht zogen", "tokens": ["Die", "bau\u00b7ern\u00b7stolz", "am", "Schlan\u00b7gen\u00b7schwanz", "nicht", "zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und von ", "tokens": ["Und", "von"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Den Trank der Ewigkeit den Weisen lie\u00dfen.", "tokens": ["Den", "Trank", "der", "E\u00b7wig\u00b7keit", "den", "Wei\u00b7sen", "lie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als himmelst\u00fcrmend sie gen diese stie\u00dfen,", "tokens": ["Als", "him\u00b7mel\u00b7st\u00fcr\u00b7mend", "sie", "gen", "die\u00b7se", "stie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da fl\u00fcchteten in H\u00f6hlen und in Wogen,", "tokens": ["Da", "fl\u00fcch\u00b7te\u00b7ten", "in", "H\u00f6h\u00b7len", "und", "in", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die nicht zum Tode ", "tokens": ["Die", "nicht", "zum", "To\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Ein leichtres Kriegsspiel habe ich zu geben;", "tokens": ["Ein", "leicht\u00b7res", "Kriegs\u00b7spiel", "ha\u00b7be", "ich", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch Liebe wiegt ja mit bei den Geschenken,", "tokens": ["Doch", "Lie\u00b7be", "wiegt", "ja", "mit", "bei", "den", "Ge\u00b7schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Gabe will des Trostes nur gedenken,", "tokens": ["Die", "Ga\u00b7be", "will", "des", "Tros\u00b7tes", "nur", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den du mir \u00fcberschwenglich gabst im Leben,", "tokens": ["Den", "du", "mir", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "gabst", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als unter mir die Erde schien zu beben,", "tokens": ["Als", "un\u00b7ter", "mir", "die", "Er\u00b7de", "schien", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Half mir Dein Arm, was st\u00fcrzte, leis zu senken,", "tokens": ["Half", "mir", "Dein", "Arm", ",", "was", "st\u00fcrz\u00b7te", ",", "leis", "zu", "sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "PWS", "VVFIN", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Lernt' ich an Deiner Brust die Schmerzen lenken", "tokens": ["Lernt'", "ich", "an", "Dei\u00b7ner", "Brust", "die", "Schmer\u00b7zen", "len\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und auf den finstern Wolken lichtw\u00e4rts schweben.", "tokens": ["Und", "auf", "den", "fins\u00b7tern", "Wol\u00b7ken", "licht\u00b7w\u00e4rts", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich leg' dies Liederband in Deine H\u00e4nde", "tokens": ["Ich", "leg'", "dies", "Lie\u00b7der\u00b7band", "in", "Dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schwing' hin\u00fcber es in leichten Wellen,", "tokens": ["Und", "schwing'", "hin\u00b7\u00fc\u00b7ber", "es", "in", "leich\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu einem andern teuern Kunstgesellen;", "tokens": ["Zu", "ei\u00b7nem", "an\u00b7dern", "teu\u00b7ern", "Kunst\u00b7ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn schm\u00fcckend es ein Freundesband umw\u00e4nde,", "tokens": ["Wenn", "schm\u00fc\u00b7ckend", "es", "ein", "Freun\u00b7des\u00b7band", "um\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Rhein gewebt von Euch geliebten Beiden,", "tokens": ["Am", "Rhein", "ge\u00b7webt", "von", "Euch", "ge\u00b7lieb\u00b7ten", "Bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVPP", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "M\u00fc\u00dft' ich mein Lied um solches Gl\u00fcck beneiden.", "tokens": ["M\u00fc\u00dft'", "ich", "mein", "Lied", "um", "sol\u00b7ches", "Gl\u00fcck", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}