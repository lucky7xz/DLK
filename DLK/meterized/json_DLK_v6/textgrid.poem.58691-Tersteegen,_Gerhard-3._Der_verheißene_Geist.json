{"textgrid.poem.58691": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "3. Der verhei\u00dfene Geist", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hebet eine Wolk' den Lebensbrunnen auf;", "tokens": ["Es", "he\u00b7bet", "ei\u00b7ne", "Wol\u00b7k'", "den", "Le\u00b7bens\u00b7brun\u00b7nen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie reichlich werden nun die Himmelswolken regnen,", "tokens": ["Wie", "reich\u00b7lich", "wer\u00b7den", "nun", "die", "Him\u00b7mels\u00b7wol\u00b7ken", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Gnade, Geist und Trost verschmacht'te Herzen segnen \u2013", "tokens": ["Mit", "Gna\u00b7de", ",", "Geist", "und", "Trost", "ver\u00b7schmacht'\u00b7te", "Her\u00b7zen", "seg\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Herr, \u00fcberstr\u00f6me mich mit deines Geistes Tauf'!", "tokens": ["Herr", ",", "\u00fc\u00b7ber\u00b7str\u00f6\u00b7me", "mich", "mit", "dei\u00b7nes", "Geis\u00b7tes", "Tauf'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}