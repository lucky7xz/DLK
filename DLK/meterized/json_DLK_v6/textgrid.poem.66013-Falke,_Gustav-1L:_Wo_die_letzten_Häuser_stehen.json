{"textgrid.poem.66013": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo die letzten H\u00e4user stehen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die letzten H\u00e4user stehen", "tokens": ["Wo", "die", "letz\u00b7ten", "H\u00e4u\u00b7ser", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter zart begr\u00fcnten Hecken,", "tokens": ["Hin\u00b7ter", "zart", "be\u00b7gr\u00fcn\u00b7ten", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchrt der Weg zum Wald hinein.", "tokens": ["F\u00fchrt", "der", "Weg", "zum", "Wald", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst doch gilt's zwei Dirnlein necken,", "tokens": ["Erst", "doch", "gilt's", "zwei", "Dirn\u00b7lein", "ne\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mit hellen Augen sehen", "tokens": ["Die", "mit", "hel\u00b7len", "Au\u00b7gen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcbern Zaun her. Spa\u00df muss sein.", "tokens": ["\u00dc\u00b7bern", "Zaun", "her", ".", "Spa\u00df", "muss", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PTKVZ", "$.", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hinter mir ihr helles Lachen,", "tokens": ["Hin\u00b7ter", "mir", "ihr", "hel\u00b7les", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O du s\u00fc\u00dfes M\u00e4dchenlachen,", "tokens": ["O", "du", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlendre ich auf weichen Wegen", "tokens": ["Schlend\u00b7re", "ich", "auf", "wei\u00b7chen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchlingsfroh dem Wald entgegen.", "tokens": ["Fr\u00fch\u00b7lings\u00b7froh", "dem", "Wald", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Feuchter Schimmer, gr\u00fcner Hauch,", "tokens": ["Feuch\u00b7ter", "Schim\u00b7mer", ",", "gr\u00fc\u00b7ner", "Hauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Voll in S\u00e4ften Baum und Strauch.", "tokens": ["Voll", "in", "S\u00e4f\u00b7ten", "Baum", "und", "Strauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rings das Spiel des jungen Lichtes.", "tokens": ["Rings", "das", "Spiel", "des", "jun\u00b7gen", "Lich\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch das offene Wipfeldach", "tokens": ["Durch", "das", "of\u00b7fe\u00b7ne", "Wip\u00b7fel\u00b7dach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Wie ein goldner Regen bricht es,", "tokens": ["Wie", "ein", "gold\u00b7ner", "Re\u00b7gen", "bricht", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Tropft durch leis bewegte Zweige,", "tokens": ["Tropft", "durch", "leis", "be\u00b7weg\u00b7te", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "\u00dcberrieselt Moos und Steige,", "tokens": ["\u00dc\u00b7berr\u00b7ie\u00b7selt", "Moos", "und", "Stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "K\u00fcsst im Gras die Primeln wach.", "tokens": ["K\u00fcsst", "im", "Gras", "die", "Pri\u00b7meln", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo die kleinen Veilchen stehen,", "tokens": ["Wo", "die", "klei\u00b7nen", "Veil\u00b7chen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh ich helle Kleider wehen;", "tokens": ["Seh", "ich", "hel\u00b7le", "Klei\u00b7der", "we\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fchlingsh\u00fcte, Kinderk\u00f6pfchen,", "tokens": ["Fr\u00fch\u00b7lings\u00b7h\u00fc\u00b7te", ",", "Kin\u00b7der\u00b7k\u00f6pf\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Buntes Band in blonden Z\u00f6pfchen,", "tokens": ["Bun\u00b7tes", "Band", "in", "blon\u00b7den", "Z\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fchlingsstimmen, helles Lachen.", "tokens": ["Fr\u00fch\u00b7lings\u00b7stim\u00b7men", ",", "hel\u00b7les", "La\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O du s\u00fc\u00dfes Kinderlachen!", "tokens": ["O", "du", "s\u00fc\u00b7\u00dfes", "Kin\u00b7der\u00b7la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Keine Nachtigallenlieder", "tokens": ["Kei\u00b7ne", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7lie\u00b7der"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geben deinen Zauber wieder.", "tokens": ["Ge\u00b7ben", "dei\u00b7nen", "Zau\u00b7ber", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Komm ich an die kleine Schar,", "tokens": ["Komm", "ich", "an", "die", "klei\u00b7ne", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie die H\u00e4schen, naht Gefahr,", "tokens": ["Wie", "die", "H\u00e4\u00b7schen", ",", "naht", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sitzen sie auf einmal stumm", "tokens": ["Sit\u00b7zen", "sie", "auf", "ein\u00b7mal", "stumm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADV", "ADJD"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.12": {"text": "All im gr\u00fcnen Gras herum.", "tokens": ["All", "im", "gr\u00fc\u00b7nen", "Gras", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Dann ein Kichern, Zischen, Lachen:", "tokens": ["Dann", "ein", "Ki\u00b7chern", ",", "Zi\u00b7schen", ",", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Lassen uns nicht bange machen.", "tokens": ["Las\u00b7sen", "uns", "nicht", "ban\u00b7ge", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur das gro\u00dfe, schlanke M\u00e4dchen,", "tokens": ["Nur", "das", "gro\u00b7\u00dfe", ",", "schlan\u00b7ke", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zierlich h\u00e4lt sich's wie am Dr\u00e4htchen,", "tokens": ["Zier\u00b7lich", "h\u00e4lt", "sich's", "wie", "am", "Dr\u00e4ht\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "KOKOM", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df auf einmal sehr verlegen,", "tokens": ["Wei\u00df", "auf", "ein\u00b7mal", "sehr", "ver\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht, wie soll ich mich bewegen.", "tokens": ["Nicht", ",", "wie", "soll", "ich", "mich", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Herr, was sehn Sie so hierher?", "tokens": ["Herr", ",", "was", "sehn", "Sie", "so", "hier\u00b7her", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie bel\u00e4stigen uns sehr.", "tokens": ["Sie", "be\u00b7l\u00e4s\u00b7ti\u00b7gen", "uns", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freilich kann ich es nicht wehren,", "tokens": ["Frei\u00b7lich", "kann", "ich", "es", "nicht", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollen Sie uns doch beehren.", "tokens": ["Wol\u00b7len", "Sie", "uns", "doch", "be\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwischen Birken, zwischen Buchen", "tokens": ["Zwi\u00b7schen", "Bir\u00b7ken", ",", "zwi\u00b7schen", "Bu\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geh nun ich auch Veilchen suchen.", "tokens": ["Geh", "nun", "ich", "auch", "Veil\u00b7chen", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pfl\u00fccke sittsam erst allein,", "tokens": ["Pfl\u00fc\u00b7cke", "sitt\u00b7sam", "erst", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Besser geht's nachher zu zwein.", "tokens": ["Bes\u00b7ser", "geht's", "nach\u00b7her", "zu", "zwein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Hier ein Bl\u00fcmchen, da, und da!", "tokens": ["Hier", "ein", "Bl\u00fcm\u00b7chen", ",", "da", ",", "und", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "$,", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis wir abseits uns verirrt.", "tokens": ["Bis", "wir", "ab\u00b7seits", "uns", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Keines wei\u00df recht, wie's geschah.", "tokens": ["Kei\u00b7nes", "wei\u00df", "recht", ",", "wie's", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$,", "PWAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Leis nur aus der Ferne schwirrt", "tokens": ["Leis", "nur", "aus", "der", "Fer\u00b7ne", "schwirrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lachen, Rufen uns ans Ohr.", "tokens": ["La\u00b7chen", ",", "Ru\u00b7fen", "uns", "ans", "Ohr", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch das kommt beim Veilchensuchen", "tokens": ["Doch", "das", "kommt", "beim", "Veil\u00b7chen\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Birken, zwischen Buchen,", "tokens": ["Zwi\u00b7schen", "Bir\u00b7ken", ",", "zwi\u00b7schen", "Bu\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei den besten Leuten vor.", "tokens": ["Bei", "den", "bes\u00b7ten", "Leu\u00b7ten", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4sst's die Mutter auch nicht gelten,", "tokens": ["L\u00e4sst's", "die", "Mut\u00b7ter", "auch", "nicht", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Andere werden uns nicht schelten.", "tokens": ["An\u00b7de\u00b7re", "wer\u00b7den", "uns", "nicht", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}