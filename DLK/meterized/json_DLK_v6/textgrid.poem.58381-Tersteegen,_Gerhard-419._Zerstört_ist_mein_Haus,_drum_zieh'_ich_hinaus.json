{"textgrid.poem.58381": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "419. Zerst\u00f6rt ist mein Haus, drum zieh' ich hinaus", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchlst du dein Elend, Seel', so geh aus dir hinaus,", "tokens": ["F\u00fchlst", "du", "dein", "E\u00b7lend", ",", "Seel'", ",", "so", "geh", "aus", "dir", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "NN", "$,", "ADV", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlasse ganz und gar dein alt', zerst\u00f6rtes Haus!", "tokens": ["Ver\u00b7las\u00b7se", "ganz", "und", "gar", "dein", "alt'", ",", "zer\u00b7st\u00f6r\u00b7tes", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kannst du Gott selber dir im Geist zur Wohnung w\u00e4hlen,", "tokens": ["Kannst", "du", "Gott", "sel\u00b7ber", "dir", "im", "Geist", "zur", "Woh\u00b7nung", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "ADV", "PPER", "APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wirst du ewig frei von aller Last und Qu\u00e4len.", "tokens": ["So", "wirst", "du", "e\u00b7wig", "frei", "von", "al\u00b7ler", "Last", "und", "Qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}