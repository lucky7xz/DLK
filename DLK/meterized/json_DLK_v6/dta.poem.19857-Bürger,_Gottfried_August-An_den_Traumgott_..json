{"dta.poem.19857": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "An den Traumgott .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Du Schw\u00e4rmer um die Ruhebetten               ", "tokens": ["Du", "Schw\u00e4r\u00b7mer", "um", "die", "Ru\u00b7he\u00b7bet\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Moos und Flaum,", "tokens": ["Von", "Moos", "und", "Flaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O Br\u00fcderchen der Amoretten,", "tokens": ["O", "Br\u00fc\u00b7der\u00b7chen", "der", "A\u00b7mo\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebter Traum!", "tokens": ["Ge\u00b7lieb\u00b7ter", "Traum", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wo fandest du, sie nachzubilden,", "tokens": ["Wo", "fan\u00b7dest", "du", ",", "sie", "nach\u00b7zu\u00b7bil\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Stof so fein? \u2014", "tokens": ["Den", "Stof", "so", "fein", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In \u00fcberirdischen Gefilden", "tokens": ["In", "\u00fc\u00b7be\u00b7rir\u00b7di\u00b7schen", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gewis allein!", "tokens": ["Ge\u00b7wis", "al\u00b7lein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Zu freundlich nur f\u00fcr Adelinen", "tokens": ["Zu", "freund\u00b7lich", "nur", "f\u00fcr", "A\u00b7de\u00b7li\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War dies ihr Bild.", "tokens": ["War", "dies", "ihr", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wann w\u00e4re sie mir selbst erschienen", "tokens": ["Wann", "w\u00e4\u00b7re", "sie", "mir", "selbst", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sanft, so mild? \u2014", "tokens": ["So", "sanft", ",", "so", "mild", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Verk\u00fcndigst du wol noch mir Armen", "tokens": ["Ver\u00b7k\u00fcn\u00b7digst", "du", "wol", "noch", "mir", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPER", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Barmherzigkeit? \u2014", "tokens": ["Barm\u00b7her\u00b7zig\u00b7keit", "?"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nein! Nein! sie f\u00fchlet kein Erbarmen", "tokens": ["Nein", "!", "Nein", "!", "sie", "f\u00fch\u00b7let", "kein", "Er\u00b7bar\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PTKANT", "$.", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Ewigkeit!", "tokens": ["In", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O Traumgott ist es ja dein Wille", "tokens": ["O", "Traum\u00b7gott", "ist", "es", "ja", "dein", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir wolzuthun,", "tokens": ["Mir", "wol\u00b7zu\u00b7thun", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So wandle deine sch\u00f6ne H\u00fclle,", "tokens": ["So", "wand\u00b7le", "dei\u00b7ne", "sch\u00f6\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kleide nun", "tokens": ["Und", "klei\u00b7de", "nun"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dich in ein Wesen, wie das Meine.", "tokens": ["Dich", "in", "ein", "We\u00b7sen", ",", "wie", "das", "Mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PWAV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Gram verzehrt,", "tokens": ["Von", "Gram", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und wie ein Leidender erscheine,", "tokens": ["Und", "wie", "ein", "Lei\u00b7den\u00b7der", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Trost begehrt.", "tokens": ["Der", "Trost", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Den Schatten las mein Bildnis gleichen,", "tokens": ["Den", "Schat\u00b7ten", "las", "mein", "Bild\u00b7nis", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die bei der Nacht", "tokens": ["Die", "bei", "der", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Durch Hallen und um Gr\u00e4ber schleichen,", "tokens": ["Durch", "Hal\u00b7len", "und", "um", "Gr\u00e4\u00b7ber", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Trauertracht;", "tokens": ["In", "Trau\u00b7er\u00b7tracht", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit hagrer Wang\u2019 und einer Miene,", "tokens": ["Mit", "hag\u00b7rer", "Wang'", "und", "ei\u00b7ner", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gnade fleht,", "tokens": ["Die", "Gna\u00b7de", "fleht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Trit hin zu dieser Adeline,", "tokens": ["Trit", "hin", "zu", "die\u00b7ser", "A\u00b7de\u00b7li\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die mich verschm\u00e4ht;", "tokens": ["Die", "mich", "ver\u00b7schm\u00e4ht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und neige dich mit leisen T\u00f6nen", "tokens": ["Und", "nei\u00b7ge", "dich", "mit", "lei\u00b7sen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ihrem Ohr;", "tokens": ["Zu", "ih\u00b7rem", "Ohr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Z\u00e4l ihr die Seufzer und die Thr\u00e4nen", "tokens": ["Z\u00e4l", "ihr", "die", "Seuf\u00b7zer", "und", "die", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Liebe vor;", "tokens": ["Der", "Lie\u00b7be", "vor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und bring in Aufruhr ihr Gewissen!", "tokens": ["Und", "bring", "in", "Auf\u00b7ruhr", "ihr", "Ge\u00b7wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Schlaf entflieh\u2019!", "tokens": ["Ihr", "Schlaf", "ent\u00b7flieh'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und schluchzend unter Z\u00e4reng\u00fcssen", "tokens": ["Und", "schluch\u00b7zend", "un\u00b7ter", "Z\u00e4\u00b7ren\u00b7g\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erwache sie!", "tokens": ["Er\u00b7wa\u00b7che", "sie", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}