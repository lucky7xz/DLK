{"dta.poem.971": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Weisser gesang", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Dass ich f&#252;r sie den weissen traum ers&#228;nne &#183; &#183;                 ", "tokens": ["Dass", "ich", "f", "&#252;", "r", "sie", "den", "weis\u00b7sen", "traum", "ers", "&#228;", "nne", "&#183;", "&#183;"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "XML_entity", "XML_entity"], "pos": ["KOUS", "PPER", "NE", "$(", "VVIMP", "PPER", "ART", "ADJA", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Mir schien im schloss das herbe strahlen tr&#228;nken                 ", "tokens": ["Mir", "schien", "im", "schloss", "das", "her\u00b7be", "strah\u00b7len", "tr", "&#228;", "nken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "APPRART", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und blasse bl&#252;ten-b&#228;ume nur umschr&#228;nken                 ", "tokens": ["Und", "blas\u00b7se", "bl", "&#252;", "ten\u00b7\u00b7b", "&#228;", "u\u00b7me", "nur", "um\u00b7schr", "&#228;", "nken"], "token_info": ["word", "word", "word", "XML_entity", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "FM", "FM", "FM", "FM", "$(", "VVFIN", "ADV", "ADV", "$(", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dass er mit zweier kinder fr&#252;htag r&#228;nne               ", "tokens": ["Dass", "er", "mit", "zwei\u00b7er", "kin\u00b7der", "fr", "&#252;", "htag", "r", "&#228;", "nne"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "NE", "$(", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Ein jedes einen schlanken strauss umschl&#228;nge                 ", "tokens": ["Ein", "je\u00b7des", "ei\u00b7nen", "schlan\u00b7ken", "strauss", "um\u00b7schl", "&#228;", "nge"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "PIAT", "PIS", "VVINF", "VVFIN", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hell-flitternd wie von leichtgeregter espe                 ", "tokens": ["Hell\u00b7flit\u00b7ternd", "wie", "von", "leicht\u00b7ge\u00b7reg\u00b7ter", "es\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Daraus als wimpel eine silber-trespe                 ", "tokens": ["Da\u00b7raus", "als", "wim\u00b7pel", "ei\u00b7ne", "sil\u00b7ber\u00b7tre\u00b7spe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "KOUS", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Hoch &#252;ber ihre schwachen stirnen schw&#228;nge               ", "tokens": ["Hoch", "&#252;", "ber", "ih\u00b7re", "schwa\u00b7chen", "stir\u00b7nen", "schw", "&#228;", "nge"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADJD", "$(", "APPR", "PPOSAT", "ADJA", "ADJA", "NE", "$(", "VVFIN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Und beide langsam k&#228;men nach dem weiher                 ", "tokens": ["Und", "bei\u00b7de", "lang\u00b7sam", "k", "&#228;", "men", "nach", "dem", "wei\u00b7her"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "$(", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf breitem marmelstiege manchmal wankend                 ", "tokens": ["Auf", "brei\u00b7tem", "mar\u00b7mel\u00b7stie\u00b7ge", "manch\u00b7mal", "wan\u00b7kend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis bei dem fl&#252;gelschlag der nahen reiher                 ", "tokens": ["Bis", "bei", "dem", "fl", "&#252;", "gel\u00b7schlag", "der", "na\u00b7hen", "rei\u00b7her"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "$(", "NN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Der arme sanfte b&#252;rde heftig schwankend               ", "tokens": ["Der", "ar\u00b7me", "sanf\u00b7te", "b", "&#252;", "rde", "hef\u00b7tig", "schwan\u00b7kend"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "ADJA", "FM", "FM", "FM", "FM", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Duft-nebel wirbelte von k&#252;hlen narden                 ", "tokens": ["Duft\u00b7ne\u00b7bel", "wir\u00b7bel\u00b7te", "von", "k", "&#252;", "hlen", "nar\u00b7den"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NE", "$(", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Mit denen die Vereinten h\u00f6herem raume                 ", "tokens": ["Mit", "de\u00b7nen", "die", "Ver\u00b7ein\u00b7ten", "h\u00f6\u00b7he\u00b7rem", "rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entgegen schwebend immer lichter warden                 ", "tokens": ["Ent\u00b7ge\u00b7gen", "schwe\u00b7bend", "im\u00b7mer", "lich\u00b7ter", "war\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald eines mit dem reinen &#228;ther-flaume.               ", "tokens": ["Bald", "ei\u00b7nes", "mit", "dem", "rei\u00b7nen", "&#228;", "ther\u00b7flau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ADV", "ART", "APPR", "ART", "ADJA", "$(", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}