{"dta.poem.95": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er bey ihr schlieffe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ein angenehmes Kind schleust mich in seine Arme/", "tokens": ["Ein", "an\u00b7ge\u00b7neh\u00b7mes", "Kind", "schleust", "mich", "in", "sei\u00b7ne", "Ar\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sich mein matter Geist auf ihrer Brust erwarme/", "tokens": ["Da\u00df", "sich", "mein", "mat\u00b7ter", "Geist", "auf", "ih\u00b7rer", "Brust", "er\u00b7war\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf ihrer Schwaanen-Brust der angenehmen H\u00f6h\u2019", "tokens": ["Auf", "ih\u00b7rer", "Schwaa\u00b7nen\u00b7Brust", "der", "an\u00b7ge\u00b7neh\u00b7men", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allwo ein weicher Sammt beleget ist mit Schnee/", "tokens": ["All\u00b7wo", "ein", "wei\u00b7cher", "Sammt", "be\u00b7le\u00b7get", "ist", "mit", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Daraus ein Liebes-Feur als einem ", "tokens": ["Da\u00b7raus", "ein", "Lie\u00b7bes\u00b7Feur", "als", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "KOKOM", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das durch geheimen Zug die Hertzen zu sich neiget/", "tokens": ["Das", "durch", "ge\u00b7hei\u00b7men", "Zug", "die", "Hert\u00b7zen", "zu", "sich", "nei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ihnen solche Lust in ihre Sinnen fl\u00f6\u00dft/", "tokens": ["Und", "ih\u00b7nen", "sol\u00b7che", "Lust", "in", "ih\u00b7re", "Sin\u00b7nen", "fl\u00f6\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df man ein ", "tokens": ["Da\u00df", "man", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Ja was ein ", "tokens": ["Ja", "was", "ein"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "PRELS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Verdienet sie doch nicht/ da\u00df sie verglichen werde/", "tokens": ["Ver\u00b7die\u00b7net", "sie", "doch", "nicht", "/", "da\u00df", "sie", "ver\u00b7gli\u00b7chen", "wer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der stoltzen Lieblichkeit/ und angenehmen Lust/", "tokens": ["Der", "stolt\u00b7zen", "Lieb\u00b7lich\u00b7keit", "/", "und", "an\u00b7ge\u00b7neh\u00b7men", "Lust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die mir/ mein Engel/ macht jetzt deine Schwaanen-Brust.", "tokens": ["Die", "mir", "/", "mein", "En\u00b7gel", "/", "macht", "jetzt", "dei\u00b7ne", "Schwaa\u00b7nen\u00b7Brust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$(", "PPOSAT", "NN", "$(", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}