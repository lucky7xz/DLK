{"textgrid.poem.55554": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Reliquie", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn, o J\u00fcngling, deine Freude,", "tokens": ["Ich", "kenn", ",", "o", "J\u00fcng\u00b7ling", ",", "dei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erwischest du einmal zur Beute", "tokens": ["Er\u00b7wi\u00b7schest", "du", "ein\u00b7mal", "zur", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Band, ein St\u00fcckchen von dem Kleide,", "tokens": ["Ein", "Band", ",", "ein", "St\u00fcck\u00b7chen", "von", "dem", "Klei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das dein geliebtes M\u00e4dchen trug.", "tokens": ["Das", "dein", "ge\u00b7lieb\u00b7tes", "M\u00e4d\u00b7chen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Schleier, Halstuch, Strumpfband, Ringe", "tokens": ["Ein", "Schlei\u00b7er", ",", "Hal\u00b7stuch", ",", "Strumpf\u00b7band", ",", "Rin\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind wirklich keine kleinen Dinge,", "tokens": ["Sind", "wirk\u00b7lich", "kei\u00b7ne", "klei\u00b7nen", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Allein mir sind sie nicht genug.", "tokens": ["Al\u00b7lein", "mir", "sind", "sie", "nicht", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein zweites Gl\u00fccke nach dem Leben,", "tokens": ["Mein", "zwei\u00b7tes", "Gl\u00fc\u00b7cke", "nach", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein M\u00e4dchen hat mir was gegeben;", "tokens": ["Mein", "M\u00e4d\u00b7chen", "hat", "mir", "was", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Setzt eure Sch\u00e4tze mir darneben,", "tokens": ["Setzt", "eu\u00b7re", "Sch\u00e4t\u00b7ze", "mir", "dar\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "PAV", "$,"], "meter": "++-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und ihre Herrlichkeit wird nichts.", "tokens": ["Und", "ih\u00b7re", "Herr\u00b7lich\u00b7keit", "wird", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lach ich all der Tr\u00f6delware!", "tokens": ["Wie", "lach", "ich", "all", "der", "Tr\u00f6\u00b7del\u00b7wa\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schenkte mir die sch\u00f6nsten Haare,", "tokens": ["Sie", "schenk\u00b7te", "mir", "die", "sch\u00f6ns\u00b7ten", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Schmuck des sch\u00f6nen Angesichts.", "tokens": ["Den", "Schmuck", "des", "sch\u00f6\u00b7nen", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll ich dich gleich, Geliebte, missen,", "tokens": ["Soll", "ich", "dich", "gleich", ",", "Ge\u00b7lieb\u00b7te", ",", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "$,", "NN", "$,", "VVPP", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wirst du mir doch nicht ganz entrissen:", "tokens": ["Wirst", "du", "mir", "doch", "nicht", "ganz", "ent\u00b7ris\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sehn, zu t\u00e4ndeln und zu k\u00fcssen", "tokens": ["Zu", "sehn", ",", "zu", "t\u00e4n\u00b7deln", "und", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt mir der sch\u00f6nste Teil von dir.", "tokens": ["Bleibt", "mir", "der", "sch\u00f6ns\u00b7te", "Teil", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gleich ist des Haars und mein Geschicke:", "tokens": ["Gleich", "ist", "des", "Haars", "und", "mein", "Ge\u00b7schi\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst buhlten wir mit einem Gl\u00fccke", "tokens": ["Sonst", "buhl\u00b7ten", "wir", "mit", "ei\u00b7nem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um sie, jetzt sind wir fern von ihr.", "tokens": ["Um", "sie", ",", "jetzt", "sind", "wir", "fern", "von", "ihr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fest waren wir an sie gehangen;", "tokens": ["Fest", "wa\u00b7ren", "wir", "an", "sie", "ge\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wir streichelten die runden Wangen", "tokens": ["Wir", "strei\u00b7chel\u00b7ten", "die", "run\u00b7den", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Und gleiteten oft mit Verlangen", "tokens": ["Und", "glei\u00b7te\u00b7ten", "oft", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von da herab zur rundern Brust.", "tokens": ["Von", "da", "her\u00b7ab", "zur", "run\u00b7dern", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Nebenbuhler, frei vom Neide,", "tokens": ["O", "Ne\u00b7ben\u00b7buh\u00b7ler", ",", "frei", "vom", "Nei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Reliquie, du sch\u00f6ne Beute,", "tokens": ["Re\u00b7li\u00b7qui\u00b7e", ",", "du", "sch\u00f6\u00b7ne", "Beu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Erinnre mich der alten Lust.", "tokens": ["E\u00b7rinn\u00b7re", "mich", "der", "al\u00b7ten", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}