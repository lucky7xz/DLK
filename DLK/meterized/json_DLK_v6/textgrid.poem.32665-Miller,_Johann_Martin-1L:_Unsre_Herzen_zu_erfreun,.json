{"textgrid.poem.32665": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unsre Herzen zu erfreun,", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsre Herzen zu erfreun,", "tokens": ["Uns\u00b7re", "Her\u00b7zen", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab uns Gott den edeln Wein;", "tokens": ["Gab", "uns", "Gott", "den", "e\u00b7deln", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum dankt ihm, Br\u00fcder!", "tokens": ["Da\u00b7rum", "dankt", "ihm", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Harm und gr\u00e4mliches Gesicht", "tokens": ["Harm", "und", "gr\u00e4m\u00b7li\u00b7ches", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Danken unserm Geber nicht,", "tokens": ["Dan\u00b7ken", "un\u00b7serm", "Ge\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber Freudenlieder.", "tokens": ["A\u00b7ber", "Freu\u00b7den\u00b7lie\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wer als braver Biedermann", "tokens": ["Wer", "als", "bra\u00b7ver", "Bie\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "KOUS", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Seines Tages Pflicht gethan,", "tokens": ["Sei\u00b7nes", "Ta\u00b7ges", "Pflicht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag im Frieden trinken!", "tokens": ["Mag", "im", "Frie\u00b7den", "trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Freuden, wie die Tugend rein,", "tokens": ["Freu\u00b7den", ",", "wie", "die", "Tu\u00b7gend", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieht er aus dem edeln Wein", "tokens": ["Sieht", "er", "aus", "dem", "e\u00b7deln", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm entgegen blinken.", "tokens": ["Ihm", "ent\u00b7ge\u00b7gen", "blin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber den, der Arme ha\u00dft,", "tokens": ["A\u00b7ber", "den", ",", "der", "Ar\u00b7me", "ha\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von ihrem Schwei\u00dfe pra\u00dft,", "tokens": ["Und", "von", "ih\u00b7rem", "Schwei\u00b7\u00dfe", "pra\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcsse Schande decken!", "tokens": ["M\u00fcs\u00b7se", "Schan\u00b7de", "de\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Taumel m\u00fcss' ihm jeder Wein,", "tokens": ["Tau\u00b7mel", "m\u00fcss'", "ihm", "je\u00b7der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jeder Tropfen Gift ihm sein,", "tokens": ["Je\u00b7der", "Trop\u00b7fen", "Gift", "ihm", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wie Wermut schmecken!", "tokens": ["Und", "wie", "Wer\u00b7mut", "schme\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}