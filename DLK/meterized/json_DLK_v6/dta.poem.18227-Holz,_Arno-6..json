{"dta.poem.18227": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchl ich, wie tief ich dich geliebt,", "tokens": ["F\u00fchl", "ich", ",", "wie", "tief", "ich", "dich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df, wenn du sie mir nicht spendest,", "tokens": ["Und", "da\u00df", ",", "wenn", "du", "sie", "mir", "nicht", "spen\u00b7dest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "PPER", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es keine Lust mehr f\u00fcr mich giebt.", "tokens": ["Es", "kei\u00b7ne", "Lust", "mehr", "f\u00fcr", "mich", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was soll mir noch des Maien Bl\u00fcthe,", "tokens": ["Was", "soll", "mir", "noch", "des", "Mai\u00b7en", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich so krank bin im Gem\u00fcthe,", "tokens": ["Da", "ich", "so", "krank", "bin", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und was des Sommers Duft und Pracht?", "tokens": ["Und", "was", "des", "Som\u00b7mers", "Duft", "und", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich mag nicht mehr den Schmelz der Auen,", "tokens": ["Ich", "mag", "nicht", "mehr", "den", "Schmelz", "der", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich will hinfort nur Eins noch schauen:", "tokens": ["Ich", "will", "hin\u00b7fort", "nur", "Eins", "noch", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das w\u00fcste Nebelgrau der Nacht!", "tokens": ["Das", "w\u00fcs\u00b7te", "Ne\u00b7bel\u00b7grau", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich lockte auf dem hohen Firne", "tokens": ["Mich", "lock\u00b7te", "auf", "dem", "ho\u00b7hen", "Fir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lebenskrone goldner Glanz,", "tokens": ["Der", "Le\u00b7bens\u00b7kro\u00b7ne", "gold\u00b7ner", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber pre\u00dftest in die Stirne", "tokens": ["Du", "a\u00b7ber", "pre\u00df\u00b7test", "in", "die", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ach, nur einen Dornenkranz!", "tokens": ["Mir", "ach", ",", "nur", "ei\u00b7nen", "Dor\u00b7nen\u00b7kranz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "XY", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verflucht durch dieses Kainszeichen,", "tokens": ["Ver\u00b7flucht", "durch", "die\u00b7ses", "Ka\u00b7ins\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Werd ich nun durch das Leben schleichen,", "tokens": ["Werd", "ich", "nun", "durch", "das", "Le\u00b7ben", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das keine Freuden f\u00fcr mich hat;", "tokens": ["Das", "kei\u00b7ne", "Freu\u00b7den", "f\u00fcr", "mich", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Denn immer mu\u00df ich dein gedenken,", "tokens": ["Denn", "im\u00b7mer", "mu\u00df", "ich", "dein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und nimmer will sich auf mich senken", "tokens": ["Und", "nim\u00b7mer", "will", "sich", "auf", "mich", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PRF", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Taube mit dem Friedensblatt!", "tokens": ["Die", "Tau\u00b7be", "mit", "dem", "Frie\u00b7dens\u00b7blatt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und kommt denn keiner, mir zu sagen,", "tokens": ["Und", "kommt", "denn", "kei\u00b7ner", ",", "mir", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PIS", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ich vergessen mag die Zeit,", "tokens": ["Wie", "ich", "ver\u00b7ges\u00b7sen", "mag", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da dir mein armes Herz geschlagen", "tokens": ["Da", "dir", "mein", "ar\u00b7mes", "Herz", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So himmelhoch, so weltenweit?", "tokens": ["So", "him\u00b7mel\u00b7hoch", ",", "so", "wel\u00b7ten\u00b7weit", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und soll sie denn auf ewig rinnen,", "tokens": ["Und", "soll", "sie", "denn", "auf", "e\u00b7wig", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wunde tief im Busen drinnen,", "tokens": ["Die", "Wun\u00b7de", "tief", "im", "Bu\u00b7sen", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die einst dein stolzer Sinn mir schlug?", "tokens": ["Die", "einst", "dein", "stol\u00b7zer", "Sinn", "mir", "schlug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O Herz, mein Herz, h\u00f6r auf zu gluthen!", "tokens": ["O", "Herz", ",", "mein", "Herz", ",", "h\u00f6r", "auf", "zu", "glu\u00b7then", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "H\u00f6r auf zu zucken, auf zu bluten!", "tokens": ["H\u00f6r", "auf", "zu", "zu\u00b7cken", ",", "auf", "zu", "blu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PTKZU", "VVINF", "$,", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es ist genug! Es ist genug!", "tokens": ["Es", "ist", "ge\u00b7nug", "!", "Es", "ist", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}