{"textgrid.poem.62395": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: V\u00f6glein, einsam in dem Bauer,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein, einsam in dem Bauer,", "tokens": ["V\u00f6\u00b7glein", ",", "ein\u00b7sam", "in", "dem", "Bau\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herzchen, einsam in der Brust,", "tokens": ["Herz\u00b7chen", ",", "ein\u00b7sam", "in", "der", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Beide haben gro\u00dfe Trauer", "tokens": ["Bei\u00b7de", "ha\u00b7ben", "gro\u00b7\u00dfe", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die s\u00fc\u00dfe Fr\u00fchlingslust.", "tokens": ["Um", "die", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Um das Wandern, um das Fliegen", "tokens": ["Um", "das", "Wan\u00b7dern", ",", "um", "das", "Flie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "KOUI", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Thal von Zweig zu Zweig,", "tokens": ["In", "dem", "Thal", "von", "Zweig", "zu", "Zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um das Wiegen, um das Schmiegen", "tokens": ["Um", "das", "Wie\u00b7gen", ",", "um", "das", "Schmie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "KOUI", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An die Liebste warm und weich.", "tokens": ["An", "die", "Liebs\u00b7te", "warm", "und", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "V\u00f6glein, singe deine Klagen,", "tokens": ["V\u00f6\u00b7glein", ",", "sin\u00b7ge", "dei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis die kleine Brust zerspringt;", "tokens": ["Bis", "die", "klei\u00b7ne", "Brust", "zer\u00b7springt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herz, mein Herz, auch du wirst schlagen", "tokens": ["Herz", ",", "mein", "Herz", ",", "auch", "du", "wirst", "schla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis dein letzter Ton verklingt.", "tokens": ["Bis", "dein", "letz\u00b7ter", "Ton", "ver\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}