{"textgrid.poem.58616": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Vernunft und Fleischessinn, glaube, Christus und die Seinen", "tokens": ["Ja", ",", "Ver\u00b7nunft", "und", "Flei\u00b7sches\u00b7sinn", ",", "glau\u00b7be", ",", "Chris\u00b7tus", "und", "die", "Sei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "KON", "NN", "$,", "VVFIN", "$,", "NE", "KON", "ART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Sind so schlecht und h\u00e4\u00dflich nicht, als sie deinen Augen scheinen!", "tokens": ["Sind", "so", "schlecht", "und", "h\u00e4\u00df\u00b7lich", "nicht", ",", "als", "sie", "dei\u00b7nen", "Au\u00b7gen", "schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "\u00c4u\u00dferlich sind sie veracht't, arm und auch in Leiden meist,", "tokens": ["\u00c4u\u00b7\u00dfer\u00b7lich", "sind", "sie", "ver\u00b7acht't", ",", "arm", "und", "auch", "in", "Lei\u00b7den", "meist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,", "ADJD", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--++-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Aber ach, wie sch\u00f6ne prangt ihr in Gott verkl\u00e4rter Geist!", "tokens": ["A\u00b7ber", "ach", ",", "wie", "sch\u00f6\u00b7ne", "prangt", "ihr", "in", "Gott", "ver\u00b7kl\u00e4r\u00b7ter", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PWAV", "ADJA", "VVFIN", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Schlecht und recht und ohne Schein so verborgen hier zu leben.", "tokens": ["Schlecht", "und", "recht", "und", "oh\u00b7ne", "Schein", "so", "ver\u00b7bor\u00b7gen", "hier", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "APPR", "NN", "ADV", "VVPP", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "La\u00df sich \u00e4rgern, wer da will, wenn ich dir gefallen kann;", "tokens": ["La\u00df", "sich", "\u00e4r\u00b7gern", ",", "wer", "da", "will", ",", "wenn", "ich", "dir", "ge\u00b7fal\u00b7len", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "VVINF", "$,", "PWS", "ADV", "VMFIN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Wenn mich jedermann verl\u00e4\u00dft, so nimm du dich meiner an!", "tokens": ["Wenn", "mich", "je\u00b7der\u00b7mann", "ver\u00b7l\u00e4\u00dft", ",", "so", "nimm", "du", "dich", "mei\u00b7ner", "an", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,", "ADV", "VVIMP", "PPER", "PRF", "PPOSAT", "PTKVZ", "$."], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}}}}}