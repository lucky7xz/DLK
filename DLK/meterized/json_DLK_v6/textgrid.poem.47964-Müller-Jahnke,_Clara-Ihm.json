{"textgrid.poem.47964": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Ihm", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab mich dir so ganz ergeben", "tokens": ["Ich", "hab", "mich", "dir", "so", "ganz", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und bin mit Leib und Seele dein,", "tokens": ["und", "bin", "mit", "Leib", "und", "See\u00b7le", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "du meines Lebens wahres Leben,", "tokens": ["du", "mei\u00b7nes", "Le\u00b7bens", "wah\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "du meines Daseins tiefstes Sein!", "tokens": ["du", "mei\u00b7nes", "Da\u00b7seins", "tiefs\u00b7tes", "Sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie sich der Mond sein mild Gefunkel", "tokens": ["Wie", "sich", "der", "Mond", "sein", "mild", "Ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "vom goldnen Glanz der Sonne leiht,", "tokens": ["vom", "gold\u00b7nen", "Glanz", "der", "Son\u00b7ne", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so f\u00e4llt in meiner Seele Dunkel", "tokens": ["so", "f\u00e4llt", "in", "mei\u00b7ner", "See\u00b7le", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der Schimmer deiner Herrlichkeit!", "tokens": ["der", "Schim\u00b7mer", "dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn was dereinst mit s\u00fc\u00dfem Beben", "tokens": ["Denn", "was", "de\u00b7reinst", "mit", "s\u00fc\u00b7\u00dfem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "durch meines Busens Tiefen drang,", "tokens": ["durch", "mei\u00b7nes", "Bu\u00b7sens", "Tie\u00b7fen", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "vermocht ich Worte nicht zu geben \u2013", "tokens": ["ver\u00b7mocht", "ich", "Wor\u00b7te", "nicht", "zu", "ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da sah ich dich, und sieh! \u2013 ich sang!", "tokens": ["da", "sah", "ich", "dich", ",", "und", "sieh", "!", "\u2013", "ich", "sang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KON", "VVIMP", "$.", "$(", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was in geheimnisvoller Stille", "tokens": ["Was", "in", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "in meines Herzens Garten spro\u00df,", "tokens": ["in", "mei\u00b7nes", "Her\u00b7zens", "Gar\u00b7ten", "spro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "verborgen lag's in duft'ger H\u00fclle,", "tokens": ["ver\u00b7bor\u00b7gen", "lag's", "in", "duft'\u00b7ger", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bis es sich deinem Licht erschlo\u00df!", "tokens": ["bis", "es", "sich", "dei\u00b7nem", "Licht", "er\u00b7schlo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}