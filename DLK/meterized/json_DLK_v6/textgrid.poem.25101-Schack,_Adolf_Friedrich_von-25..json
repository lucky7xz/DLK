{"textgrid.poem.25101": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft in mein Aug', o Kleine,", "tokens": ["So", "oft", "in", "mein", "Aug'", ",", "o", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "FM", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von deinen Blicken ein Lichtstrahl f\u00e4llt,", "tokens": ["Von", "dei\u00b7nen", "Bli\u00b7cken", "ein", "Licht\u00b7strahl", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wird wieder von Fr\u00fchlingsscheine", "tokens": ["Wird", "wie\u00b7der", "von", "Fr\u00fch\u00b7lings\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die erstorbene Seele mir sanft erhellt;", "tokens": ["Die", "ers\u00b7tor\u00b7be\u00b7ne", "See\u00b7le", "mir", "sanft", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-++--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Beben und Sprossen und Keimen,", "tokens": ["Ein", "Be\u00b7ben", "und", "Spros\u00b7sen", "und", "Kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie auf der Flur bei des Ostwinds Wehn,", "tokens": ["Wie", "auf", "der", "Flur", "bei", "des", "Ost\u00b7winds", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Beginnt in ihren geheimen", "tokens": ["Be\u00b7ginnt", "in", "ih\u00b7ren", "ge\u00b7hei\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Grabkammern, ein Werden und Auferstehn.", "tokens": ["Grab\u00b7kam\u00b7mern", ",", "ein", "Wer\u00b7den", "und", "Auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Bei Nachtigallengeschmetter", "tokens": ["Bei", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Regt Knosp' an Knospe, die aufbl\u00fchn will,", "tokens": ["Regt", "Knosp'", "an", "Knos\u00b7pe", ",", "die", "auf\u00b7bl\u00fchn", "will", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Kelche die zarten Bl\u00e4tter;", "tokens": ["Im", "Kel\u00b7che", "die", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann wieder alles \u00f6de und still.", "tokens": ["Dann", "wie\u00b7der", "al\u00b7les", "\u00f6\u00b7de", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Und ach! wenn der wonnige Schauer", "tokens": ["Und", "ach", "!", "wenn", "der", "won\u00b7ni\u00b7ge", "Schau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Verflogen, der mich fl\u00fcchtig durchrann,", "tokens": ["Ver\u00b7flo\u00b7gen", ",", "der", "mich", "fl\u00fcch\u00b7tig", "durch\u00b7rann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Bleibt mir im Herzen nur Trauer,", "tokens": ["Bleibt", "mir", "im", "Her\u00b7zen", "nur", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich wie sonst nicht mehr lieben kann.", "tokens": ["Da\u00df", "ich", "wie", "sonst", "nicht", "mehr", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}