{"dta.poem.15765": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo soviel Blumen bl\u00fchn, wie jetzt auf unsrer Flur,", "tokens": ["Wo", "so\u00b7viel", "Blu\u00b7men", "bl\u00fchn", ",", "wie", "jetzt", "auf", "uns\u00b7rer", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt endlich der Gesammteindruck von allen nur.", "tokens": ["Bleibt", "end\u00b7lich", "der", "Ge\u00b7samm\u00b7tein\u00b7druck", "von", "al\u00b7len", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zu schm\u00fccken ihren Platz mag jede sich bem\u00fchn,", "tokens": ["Zu", "schm\u00fc\u00b7cken", "ih\u00b7ren", "Platz", "mag", "je\u00b7de", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "VMFIN", "PIAT", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch keine wird so leicht die andern niederbl\u00fchn.", "tokens": ["Doch", "kei\u00b7ne", "wird", "so", "leicht", "die", "an\u00b7dern", "nie\u00b7der\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die sich besonders m\u00fcht, da\u00df sie Besondres bringe,", "tokens": ["Die", "sich", "be\u00b7son\u00b7ders", "m\u00fcht", ",", "da\u00df", "sie", "Be\u00b7sond\u00b7res", "brin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird leicht zur Misgeburt anstatt zum Wunderdinge.", "tokens": ["Wird", "leicht", "zur", "Mis\u00b7ge\u00b7burt", "an\u00b7statt", "zum", "Wun\u00b7der\u00b7din\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bla\u00df werde nicht vor Gram, die \u00fcbersehn sich sieht,", "tokens": ["Bla\u00df", "wer\u00b7de", "nicht", "vor", "Gram", ",", "die", "\u00fc\u00b7ber\u00b7sehn", "sich", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "APPR", "NN", "$,", "PRELS", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch jene roth vor Scham, die alle Welt vorzieht;", "tokens": ["Noch", "je\u00b7ne", "roth", "vor", "Scham", ",", "die", "al\u00b7le", "Welt", "vor\u00b7zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADJD", "APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Noch gelb vor Neide, die sieht alle vorgezogen;", "tokens": ["Noch", "gelb", "vor", "Nei\u00b7de", ",", "die", "sieht", "al\u00b7le", "vor\u00b7ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "PRELS", "VVFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschiednen Blumen ist verschiedner Sinn gewogen.", "tokens": ["Ver\u00b7schied\u00b7nen", "Blu\u00b7men", "ist", "ver\u00b7schied\u00b7ner", "Sinn", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die thun alsob Gem\u00fcth sie nur bei jenem f\u00e4nden,", "tokens": ["Die", "thun", "al\u00b7sob", "Ge\u00b7m\u00fcth", "sie", "nur", "bei", "je\u00b7nem", "f\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "NN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie liebten diesen auch, wenn sie den Geist verst\u00e4nden.", "tokens": ["Sie", "lieb\u00b7ten", "die\u00b7sen", "auch", ",", "wenn", "sie", "den", "Geist", "ver\u00b7st\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}