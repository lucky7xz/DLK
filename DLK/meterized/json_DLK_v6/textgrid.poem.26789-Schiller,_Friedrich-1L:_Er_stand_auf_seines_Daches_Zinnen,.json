{"textgrid.poem.26789": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Er stand auf seines Daches Zinnen,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er stand auf seines Daches Zinnen,", "tokens": ["Er", "stand", "auf", "sei\u00b7nes", "Da\u00b7ches", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er schaute mit vergn\u00fcgten Sinnen", "tokens": ["Er", "schau\u00b7te", "mit", "ver\u00b7gn\u00fcg\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf das beherrschte Samos hin.", "tokens": ["Auf", "das", "be\u00b7herrschte", "Sa\u00b7mos", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbdies alles ist mir untert\u00e4nig\u00ab,", "tokens": ["\u00bb", "dies", "al\u00b7les", "ist", "mir", "un\u00b7ter\u00b7t\u00e4\u00b7nig", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "PIS", "VAFIN", "PPER", "ADJD", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Begann er zu \u00c4gyptens K\u00f6nig,", "tokens": ["Be\u00b7gann", "er", "zu", "\u00c4\u00b7gyp\u00b7tens", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "\u00bbgestehe, da\u00df ich gl\u00fccklich bin.\u00ab", "tokens": ["\u00bb", "ge\u00b7ste\u00b7he", ",", "da\u00df", "ich", "gl\u00fcck\u00b7lich", "bin", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdu hast der G\u00f6tter Gunst erfahren!", "tokens": ["\u00bb", "du", "hast", "der", "G\u00f6t\u00b7ter", "Gunst", "er\u00b7fah\u00b7ren", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die vormals deinesgleichen waren,", "tokens": ["Die", "vor\u00b7mals", "dei\u00b7nes\u00b7glei\u00b7chen", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PDS", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zwingt jetzt deines Szepters Macht.", "tokens": ["Sie", "zwingt", "jetzt", "dei\u00b7nes", "Szep\u00b7ters", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch einer lebt noch, sie zu r\u00e4chen,", "tokens": ["Doch", "ei\u00b7ner", "lebt", "noch", ",", "sie", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dich kann mein Mund nicht gl\u00fccklich sprechen,", "tokens": ["Dich", "kann", "mein", "Mund", "nicht", "gl\u00fcck\u00b7lich", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Solang des Feindes Auge wacht.\u00ab", "tokens": ["So\u00b7lang", "des", "Fein\u00b7des", "Au\u00b7ge", "wacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und eh der K\u00f6nig noch geendet,", "tokens": ["Und", "eh", "der", "K\u00f6\u00b7nig", "noch", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da stellt sich, von Milet gesendet,", "tokens": ["Da", "stellt", "sich", ",", "von", "Mi\u00b7let", "ge\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Bote dem Tyrannen dar:", "tokens": ["Ein", "Bo\u00b7te", "dem", "Ty\u00b7ran\u00b7nen", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbla\u00df, Herr! des Opfers D\u00fcfte steigen", "tokens": ["\u00bb", "la\u00df", ",", "Herr", "!", "des", "Op\u00b7fers", "D\u00fcf\u00b7te", "stei\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "$,", "NN", "$.", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit des Lorbeers muntern Zweigen", "tokens": ["Und", "mit", "des", "Lor\u00b7beers", "mun\u00b7tern", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bekr\u00e4nze dir dein festlich Haar.", "tokens": ["Be\u00b7kr\u00e4n\u00b7ze", "dir", "dein", "fest\u00b7lich", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Getroffen sank dein Feind vom Speere,", "tokens": ["Ge\u00b7trof\u00b7fen", "sank", "dein", "Feind", "vom", "Spee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich sendet mit der frohen M\u00e4re", "tokens": ["Mich", "sen\u00b7det", "mit", "der", "fro\u00b7hen", "M\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein treuer Feldherr Polydor \u2013\u00ab", "tokens": ["Dein", "treu\u00b7er", "Feld\u00b7herr", "Po\u00b7ly\u00b7dor", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimmt aus einem schwarzen Becken,", "tokens": ["Und", "nimmt", "aus", "ei\u00b7nem", "schwar\u00b7zen", "Be\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noch blutig, zu der beiden Schrecken,", "tokens": ["Noch", "blu\u00b7tig", ",", "zu", "der", "bei\u00b7den", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein wohlbekanntes Haupt hervor.", "tokens": ["Ein", "wohl\u00b7be\u00b7kann\u00b7tes", "Haupt", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig tritt zur\u00fcck mit Grauen:", "tokens": ["Der", "K\u00f6\u00b7nig", "tritt", "zu\u00b7r\u00fcck", "mit", "Grau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdoch warn ich dich, dem Gl\u00fcck zu trauen\u00ab,", "tokens": ["\u00bb", "doch", "warn", "ich", "dich", ",", "dem", "Gl\u00fcck", "zu", "trau\u00b7en", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PRF", "$,", "ART", "NN", "PTKZU", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Versetzt er mit besorgtem Blick.", "tokens": ["Ver\u00b7setzt", "er", "mit", "be\u00b7sorg\u00b7tem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbbedenk, auf ungetreuen Wellen,", "tokens": ["\u00bb", "be\u00b7denk", ",", "auf", "un\u00b7ge\u00b7treu\u00b7en", "Wel\u00b7len", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie leicht kann sie der Sturm zerschellen,", "tokens": ["Wie", "leicht", "kann", "sie", "der", "Sturm", "zer\u00b7schel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schwimmt deiner Flotte zweifelnd Gl\u00fcck.\u00ab", "tokens": ["Schwimmt", "dei\u00b7ner", "Flot\u00b7te", "zwei\u00b7felnd", "Gl\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und eh er noch das Wort gesprochen,", "tokens": ["Und", "eh", "er", "noch", "das", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat ihn der Jubel unterbrochen,", "tokens": ["Hat", "ihn", "der", "Ju\u00b7bel", "un\u00b7ter\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der von der Reede jauchzend schallt.", "tokens": ["Der", "von", "der", "Ree\u00b7de", "jauch\u00b7zend", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit fremden Sch\u00e4tzen reich beladen,", "tokens": ["Mit", "frem\u00b7den", "Sch\u00e4t\u00b7zen", "reich", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kehrt zu den heimischen Gestaden", "tokens": ["Kehrt", "zu", "den", "hei\u00b7mi\u00b7schen", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schiffe mastenreicher Wald.", "tokens": ["Der", "Schif\u00b7fe", "mas\u00b7ten\u00b7rei\u00b7cher", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der k\u00f6nigliche Gast erstaunet:", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Gast", "er\u00b7stau\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbdein Gl\u00fcck ist heute gut gelaunet,", "tokens": ["\u00bb", "dein", "Gl\u00fcck", "ist", "heu\u00b7te", "gut", "ge\u00b7lau\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch f\u00fcrchte seinen Unbestand.", "tokens": ["Doch", "f\u00fcrch\u00b7te", "sei\u00b7nen", "Un\u00b7be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kreter waffenkundge Scharen", "tokens": ["Der", "Kre\u00b7ter", "waf\u00b7fen\u00b7kund\u00b7ge", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bedr\u00e4uen dich mit Kriegsgefahren,", "tokens": ["Be\u00b7dr\u00e4u\u00b7en", "dich", "mit", "Kriegs\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schon nahe sind sie diesem Strand.\u00ab", "tokens": ["Schon", "na\u00b7he", "sind", "sie", "die\u00b7sem", "Strand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und eh ihm noch das Wort entfallen,", "tokens": ["Und", "eh", "ihm", "noch", "das", "Wort", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da sieht mans von den Schiffen wallen,", "tokens": ["Da", "sieht", "mans", "von", "den", "Schif\u00b7fen", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und tausend Stimmen rufen: \u00bbSieg!", "tokens": ["Und", "tau\u00b7send", "Stim\u00b7men", "ru\u00b7fen", ":", "\u00bb", "Sieg", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Feindesnot sind wir befreiet,", "tokens": ["Von", "Fein\u00b7des\u00b7not", "sind", "wir", "be\u00b7frei\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kreter hat der Sturm zerstreuet,", "tokens": ["Die", "Kre\u00b7ter", "hat", "der", "Sturm", "zer\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vorbei, geendet ist der Krieg.\u00ab", "tokens": ["Vor\u00b7bei", ",", "ge\u00b7en\u00b7det", "ist", "der", "Krieg", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "VVPP", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das h\u00f6rt der Gastfreund mit Entsetzen:", "tokens": ["Das", "h\u00f6rt", "der", "Gast\u00b7freund", "mit", "Ent\u00b7set\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "\u00bbf\u00fcrwahr, ich mu\u00df dich gl\u00fccklich sch\u00e4tzen,", "tokens": ["\u00bb", "f\u00fcr\u00b7wahr", ",", "ich", "mu\u00df", "dich", "gl\u00fcck\u00b7lich", "sch\u00e4t\u00b7zen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch\u00ab, spricht er, \u00bbzittr ich f\u00fcr dein Heil.", "tokens": ["Doch", "\u00ab", ",", "spricht", "er", ",", "\u00bb", "zittr", "ich", "f\u00fcr", "dein", "Heil", "."], "token_info": ["word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir grauet vor der G\u00f6tter Neide,", "tokens": ["Mir", "grau\u00b7et", "vor", "der", "G\u00f6t\u00b7ter", "Nei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "N