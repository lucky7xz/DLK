{"textgrid.poem.52433": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "14. Elegie an die Ruhe", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie nach dem r\u00f6tenden Abend die Schnitterm\u00e4dchen sich sehnen,", "tokens": ["Wie", "nach", "dem", "r\u00f6\u00b7ten\u00b7den", "A\u00b7bend", "die", "Schnit\u00b7ter\u00b7m\u00e4d\u00b7chen", "sich", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Also sehnt sich mein Herz, l\u00e4ndliche Ruhe, nach dir!", "tokens": ["Al\u00b7so", "sehnt", "sich", "mein", "Herz", ",", "l\u00e4nd\u00b7li\u00b7che", "Ru\u00b7he", ",", "nach", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dich zu finden, verberg' ich mich gern in entlegener Wildnis,", "tokens": ["Dich", "zu", "fin\u00b7den", ",", "ver\u00b7ber\u00b7g'", "ich", "mich", "gern", "in", "ent\u00b7le\u00b7ge\u00b7ner", "Wild\u00b7nis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wie der Vogel des Forsts unter den Bl\u00e4ttern sich birgt.", "tokens": ["Wie", "der", "Vo\u00b7gel", "des", "Forsts", "un\u00b7ter", "den", "Bl\u00e4t\u00b7tern", "sich", "birgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "H\u00e4tt' ich ein l\u00e4ndliches Haus, in waldiger Windung des Seethals,", "tokens": ["H\u00e4tt'", "ich", "ein", "l\u00e4nd\u00b7li\u00b7ches", "Haus", ",", "in", "wal\u00b7di\u00b7ger", "Win\u00b7dung", "des", "See\u00b7thals", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Halb vom gl\u00e4nzenden Gr\u00fcn k\u00fchlender Linden verh\u00fcllt,", "tokens": ["Halb", "vom", "gl\u00e4n\u00b7zen\u00b7den", "Gr\u00fcn", "k\u00fch\u00b7len\u00b7der", "Lin\u00b7den", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "ADJA", "NE", "VVPP", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Wo auf schwankendem Spro\u00df sich wiegte der lockende Buchfink,", "tokens": ["Wo", "auf", "schwan\u00b7ken\u00b7dem", "Spro\u00df", "sich", "wieg\u00b7te", "der", "lo\u00b7cken\u00b7de", "Buch\u00b7fink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Oder ein H\u00e4nflingspaar baute sein schwebendes Nest:", "tokens": ["O\u00b7der", "ein", "H\u00e4nf\u00b7lings\u00b7paar", "bau\u00b7te", "sein", "schwe\u00b7ben\u00b7des", "Nest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Dann umfl\u00f6cht' ich mit hochrot bl\u00fchenden Bohnen die Gitter", "tokens": ["Dann", "um\u00b7fl\u00f6cht'", "ich", "mit", "hoch\u00b7rot", "bl\u00fc\u00b7hen\u00b7den", "Boh\u00b7nen", "die", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Meines Sommergemachs, da\u00df durch des s\u00e4uselnden Laubs", "tokens": ["Mei\u00b7nes", "Som\u00b7mer\u00b7ge\u00b7machs", ",", "da\u00df", "durch", "des", "s\u00e4u\u00b7seln\u00b7den", "Laubs"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "\u00d6ffnungen blinkte der Mond und der Purpurschimmer der Fr\u00fche,", "tokens": ["\u00d6ff\u00b7nun\u00b7gen", "blink\u00b7te", "der", "Mond", "und", "der", "Pur\u00b7pur\u00b7schim\u00b7mer", "der", "Fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Oder des Sonnenscheins gr\u00fcnlich durchwobenes Gold.", "tokens": ["O\u00b7der", "des", "Son\u00b7nen\u00b7scheins", "gr\u00fcn\u00b7lich", "durch\u00b7wo\u00b7be\u00b7nes", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.13": {"text": "Bl\u00fchendes Gei\u00dfblatt verb\u00e4nde des Gartens Lilageb\u00fcsche,", "tokens": ["Bl\u00fc\u00b7hen\u00b7des", "Gei\u00df\u00b7blatt", "ver\u00b7b\u00e4n\u00b7de", "des", "Gar\u00b7tens", "Li\u00b7la\u00b7ge\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.14": {"text": "Und umatmete s\u00fc\u00df meine verborgene Bank.", "tokens": ["Und", "u\u00b7mat\u00b7me\u00b7te", "s\u00fc\u00df", "mei\u00b7ne", "ver\u00b7bor\u00b7ge\u00b7ne", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Emsig beg\u00f6ss' ich am Morgen und Abend die d\u00fcrstenden Nelken,", "tokens": ["Em\u00b7sig", "be\u00b7g\u00f6ss'", "ich", "am", "Mor\u00b7gen", "und", "A\u00b7bend", "die", "d\u00fcrs\u00b7ten\u00b7den", "Nel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Tr\u00e4ufelt' erquickendes Na\u00df auf das verwelkende Kraut.", "tokens": ["Tr\u00e4u\u00b7felt'", "er\u00b7qui\u00b7cken\u00b7des", "Na\u00df", "auf", "das", "ver\u00b7wel\u00b7ken\u00b7de", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "Bald bestieg ich selbst die Leiter am r\u00f6telnden Kirschbaum,", "tokens": ["Bald", "be\u00b7stieg", "ich", "selbst", "die", "Lei\u00b7ter", "am", "r\u00f6\u00b7teln\u00b7den", "Kirschbaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Bald entri\u00df ich die Nu\u00df ihrem versagenden Stiel.", "tokens": ["Bald", "ent\u00b7ri\u00df", "ich", "die", "Nu\u00df", "ih\u00b7rem", "ver\u00b7sa\u00b7gen\u00b7den", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Rauschend entst\u00fcrzten dem Wipfel die purpurwangigen \u00c4pfel,", "tokens": ["Rau\u00b7schend", "ent\u00b7st\u00fcrz\u00b7ten", "dem", "Wip\u00b7fel", "die", "pur\u00b7pur\u00b7wan\u00b7gi\u00b7gen", "\u00c4p\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Oder es tropften ins Gras bl\u00e4uliche Pflaumen herab.", "tokens": ["O\u00b7der", "es", "tropf\u00b7ten", "ins", "Gras", "bl\u00e4u\u00b7li\u00b7che", "Pflau\u00b7men", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Ich begleitete gern die Schwade der M\u00e4her im Heumond,", "tokens": ["Ich", "be\u00b7glei\u00b7te\u00b7te", "gern", "die", "Schwa\u00b7de", "der", "M\u00e4\u00b7her", "im", "Heu\u00b7mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+--++-++", "measure": "anapaest.di.plus"}, "line.22": {"text": "N\u00e4hme selber sogar \u00f6fters die Sense zur Hand.", "tokens": ["N\u00e4h\u00b7me", "sel\u00b7ber", "so\u00b7gar", "\u00f6f\u00b7ters", "die", "Sen\u00b7se", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "K\u00fchlte mit Milch den Durst in des Mittags sengender Schw\u00fcle,", "tokens": ["K\u00fchl\u00b7te", "mit", "Milch", "den", "Durst", "in", "des", "Mit\u00b7tags", "sen\u00b7gen\u00b7der", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--++-+-+-", "measure": "iambic.septa.invert"}, "line.24": {"text": "Wenn sich des Landvolks Kreis lagert im Schatten des Zauns.", "tokens": ["Wenn", "sich", "des", "Land\u00b7volks", "Kreis", "la\u00b7gert", "im", "Schat\u00b7ten", "des", "Zauns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.25": {"text": "Aus den T\u00f6chtern des Landes erw\u00e4hlt' ich eine zum Weibe,", "tokens": ["Aus", "den", "T\u00f6ch\u00b7tern", "des", "Lan\u00b7des", "er\u00b7w\u00e4hlt'", "ich", "ei\u00b7ne", "zum", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ART", "APPRART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.26": {"text": "Sittsam wie Veilchen und keusch wie die Viole der Nacht.", "tokens": ["Sitt\u00b7sam", "wie", "Veil\u00b7chen", "und", "keusch", "wie", "die", "Vi\u00b7o\u00b7le", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "KON", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.27": {"text": "O dann l\u00e4chelte mir ihr Blick in die h\u00e4uslichen Schatten,", "tokens": ["O", "dann", "l\u00e4\u00b7chel\u00b7te", "mir", "ihr", "Blick", "in", "die", "h\u00e4us\u00b7li\u00b7chen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Wie der D\u00e4mmerung Stern, Wehmut und liebliche Ruh'! \u2013", "tokens": ["Wie", "der", "D\u00e4m\u00b7me\u00b7rung", "Stern", ",", "Weh\u00b7mut", "und", "lieb\u00b7li\u00b7che", "Ruh'", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Aber was lullst du mich ein in Zauberschlummer der T\u00e4uschung,", "tokens": ["A\u00b7ber", "was", "lullst", "du", "mich", "ein", "in", "Zau\u00b7ber\u00b7schlum\u00b7mer", "der", "T\u00e4u\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PRF", "ART", "APPR", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Nichtige Phantasie? Selten, ach! selten gedeiht", "tokens": ["Nich\u00b7ti\u00b7ge", "Phan\u00b7ta\u00b7sie", "?", "Sel\u00b7ten", ",", "ach", "!", "sel\u00b7ten", "ge\u00b7deiht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "$,", "ITJ", "$.", "ADJD", "VVFIN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.31": {"text": "Deine Bl\u00fcte zur Frucht! Mir ruft die wirbelnde Trommel,", "tokens": ["Dei\u00b7ne", "Bl\u00fc\u00b7te", "zur", "Frucht", "!", "Mir", "ruft", "die", "wir\u00b7beln\u00b7de", "Trom\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "Und der Kanonen Zug klirrt durch die W\u00f6lbung des Thors;", "tokens": ["Und", "der", "Ka\u00b7no\u00b7nen", "Zug", "klirrt", "durch", "die", "W\u00f6l\u00b7bung", "des", "Thors", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.33": {"text": "Bajonette blitzen in langen, starrenden Reihen,", "tokens": ["Ba\u00b7jo\u00b7net\u00b7te", "blit\u00b7zen", "in", "lan\u00b7gen", ",", "star\u00b7ren\u00b7den", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Hoch vom Flatterger\u00e4usch farbiger Fahnen umweht.", "tokens": ["Hoch", "vom", "Flat\u00b7ter\u00b7ge\u00b7r\u00e4usch", "far\u00b7bi\u00b7ger", "Fah\u00b7nen", "um\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Gebt mir die Lanz' und das Schwert, da\u00df ich mich g\u0