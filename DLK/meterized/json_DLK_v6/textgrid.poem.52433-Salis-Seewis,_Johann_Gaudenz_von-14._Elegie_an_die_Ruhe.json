die Blicke;", "tokens": ["Ru\u00b7he", ",", "dich", "lieb'", "ich", "um\u00b7sonst", "!", "Ich", "flieh'", "und", "wen\u00b7de", "die", "Bli\u00b7cke", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.40": {"text": "Nur noch ein Seufzer entschl\u00fcpft mir in bet\u00e4ubendem L\u00e4rm,", "tokens": ["Nur", "noch", "ein", "Seuf\u00b7zer", "ent\u00b7schl\u00fcpft", "mir", "in", "be\u00b7t\u00e4u\u00b7ben\u00b7dem", "L\u00e4rm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Wie der entf\u00fchrten Braut im Arme des siegenden J\u00fcnglings,", "tokens": ["Wie", "der", "ent\u00b7f\u00fchr\u00b7ten", "Braut", "im", "Ar\u00b7me", "des", "sie\u00b7gen\u00b7den", "J\u00fcng\u00b7lings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--++-+-", "measure": "iambic.septa.relaxed"}, "line.42": {"text": "Wenn sie ans heimische Haus z\u00e4rtlicher Eltern gedenkt.", "tokens": ["Wenn", "sie", "ans", "hei\u00b7mi\u00b7sche", "Haus", "z\u00e4rt\u00b7li\u00b7cher", "El\u00b7tern", "ge\u00b7denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}}}}