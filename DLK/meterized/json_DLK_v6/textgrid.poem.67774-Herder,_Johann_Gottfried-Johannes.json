{"textgrid.poem.67774": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Johannes", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Engel des Herrn!", "tokens": ["Der", "En\u00b7gel", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Geist und Kraft!", "tokens": ["Geist", "und", "Kraft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ein Sturmwind, der durch W\u00fcsten rafft", "tokens": ["Ein", "Sturm\u00b7wind", ",", "der", "durch", "W\u00fcs\u00b7ten", "rafft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Thal' hinauf, die H\u00f6hn zu Boden!", "tokens": ["Die", "Thal'", "hin\u00b7auf", ",", "die", "H\u00f6hn", "zu", "Bo\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jehovah's Stimme! Sein Odem!", "tokens": ["Je\u00b7ho\u00b7vah's", "Stim\u00b7me", "!", "Sein", "O\u00b7dem", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Elias! Wen ein Weib gebar,", "tokens": ["E\u00b7lias", "!", "Wen", "ein", "Weib", "ge\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "War Keiner gr\u00f6\u00dfer, als er war!", "tokens": ["War", "Kei\u00b7ner", "gr\u00f6\u00b7\u00dfer", ",", "als", "er", "war", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Johannes! Sollte Viel' bekehren,", "tokens": ["Jo\u00b7han\u00b7nes", "!", "Soll\u00b7te", "Viel'", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Erfreun und strafen, f\u00e4llen, lehren,", "tokens": ["Er\u00b7freun", "und", "stra\u00b7fen", ",", "f\u00e4l\u00b7len", ",", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vorchristus sein!", "tokens": ["Vor\u00b7chris\u00b7tus", "sein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Am Opferaltar", "tokens": ["Am", "Op\u00b7fer\u00b7al\u00b7tar"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gabriel", "tokens": ["Ga\u00b7bri\u00b7el"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Verk\u00fcndet' ihn: \u00bbDer windesschnell", "tokens": ["Ver\u00b7k\u00fcn\u00b7det'", "ihn", ":", "\u00bb", "Der", "win\u00b7des\u00b7schnell"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vorm Herrn daherging! ungeboren", "tokens": ["Vorm", "Herrn", "da\u00b7her\u00b7ging", "!", "un\u00b7ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "$.", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Voll Geistes! aber Volkes Ohren", "tokens": ["Voll", "Geis\u00b7tes", "!", "a\u00b7ber", "Vol\u00b7kes", "Oh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "ADV", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Sausen! Wein und Ueppigkeit", "tokens": ["Ein", "Sau\u00b7sen", "!", "Wein", "und", "Uep\u00b7pig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat er verschm\u00e4ht! In Freudigkeit", "tokens": ["Hat", "er", "ver\u00b7schm\u00e4ht", "!", "In", "Freu\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Elias', Greise zu verj\u00fcngen", "tokens": ["E\u00b7li\u00b7as'", ",", "Grei\u00b7se", "zu", "ver\u00b7j\u00fcn\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "PTKZU", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Zu Kindesherzen! Volk zu bringen", "tokens": ["Zu", "Kin\u00b7des\u00b7her\u00b7zen", "!", "Volk", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem Kommenden!\u00ab", "tokens": ["Dem", "Kom\u00b7men\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Sprach Flammen und Schwert:", "tokens": ["Sprach", "Flam\u00b7men", "und", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "\u00bbotternbrut!", "tokens": ["\u00bb", "ot\u00b7tern\u00b7brut", "!"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "O schauet's, was Ihr Fr\u00fcchte thut!", "tokens": ["O", "schau\u00b7et's", ",", "was", "Ihr", "Fr\u00fcch\u00b7te", "thut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon f\u00e4llt die Axt und schonet Keinen;", "tokens": ["Schon", "f\u00e4llt", "die", "Axt", "und", "scho\u00b7net", "Kei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Volk Gottes kann er aus Steinen", "tokens": ["Volk", "Got\u00b7tes", "kann", "er", "aus", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sich selbst erwecken. Gr\u00fcnest kaum", "tokens": ["Sich", "selbst", "er\u00b7we\u00b7cken", ".", "Gr\u00fc\u00b7nest", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ADV", "VVINF", "$.", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ohn' Art und Gutes, \u00f6der Baum!", "tokens": ["Ohn'", "Art", "und", "Gu\u00b7tes", ",", "\u00f6\u00b7der", "Baum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schon f\u00e4llt die Axt! Dein warten Flammen,", "tokens": ["Schon", "f\u00e4llt", "die", "Axt", "!", "Dein", "war\u00b7ten", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die weht Jehovah's Zorn zusammen,", "tokens": ["Die", "weht", "Je\u00b7ho\u00b7vah's", "Zorn", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Lasterbrut!", "tokens": ["Der", "Las\u00b7ter\u00b7brut", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Schon tritt er daher,", "tokens": ["Schon", "tritt", "er", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Worfelt schon!", "tokens": ["Wor\u00b7felt", "schon", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und Gluth ist Spreu und Unkraut Lohn.", "tokens": ["Und", "Gluth", "ist", "Spreu", "und", "Un\u00b7kraut", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich taufe Wasser! er von oben!", "tokens": ["Ich", "tau\u00b7fe", "Was\u00b7ser", "!", "er", "von", "o\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "PPER", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Seht, wie die G\u00f6tter ihn loben!", "tokens": ["Seht", ",", "wie", "die", "G\u00f6t\u00b7ter", "ihn", "lo\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Der Himmel rei\u00dft! der Himmel bricht!", "tokens": ["Der", "Him\u00b7mel", "rei\u00dft", "!", "der", "Him\u00b7mel", "bricht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jehovah kommt! Jehovah spricht:", "tokens": ["Je\u00b7ho\u00b7vah", "kommt", "!", "Je\u00b7ho\u00b7vah", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Auserw\u00e4hlter! Du f\u00fcr Allen,", "tokens": ["Mein", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ter", "!", "Du", "f\u00fcr", "Al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Durch den mir Alles soll gefallen!\u00ab", "tokens": ["Durch", "den", "mir", "Al\u00b7les", "soll", "ge\u00b7fal\u00b7len", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "PPER", "PIS", "VMFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und ruht auf ihm.", "tokens": ["Und", "ruht", "auf", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "\u00bbder ist es, der kommt,", "tokens": ["\u00bb", "der", "ist", "es", ",", "der", "kommt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "VAFIN", "PPER", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Gottes Lamm,", "tokens": ["Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Das Weltens\u00fcnden auf sich nahm!", "tokens": ["Das", "Wel\u00b7ten\u00b7s\u00fcn\u00b7den", "auf", "sich", "nahm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Feuer tauft die Welt der B\u00f6sen.", "tokens": ["Mit", "Feu\u00b7er", "tauft", "die", "Welt", "der", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bin unwerth, Kleid ihm zu l\u00f6sen!", "tokens": ["Bin", "un\u00b7werth", ",", "Kleid", "ihm", "zu", "l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Was hat ein Mensch, es sei ihm dann", "tokens": ["Was", "hat", "ein", "Mensch", ",", "es", "sei", "ihm", "dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gegeben? Er ist Br\u00e4utigam,", "tokens": ["Ge\u00b7ge\u00b7ben", "?", "Er", "ist", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich F\u00fchrer nur der Braut! von Erde,", "tokens": ["Ich", "F\u00fch\u00b7rer", "nur", "der", "Braut", "!", "von", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "ART", "NN", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ein Irdischer. Er ist und werde", "tokens": ["Ein", "Ir\u00b7di\u00b7scher", ".", "Er", "ist", "und", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "KON", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Himmlische.\u00ab", "tokens": ["Der", "Himm\u00b7li\u00b7sche", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Im Feuer empor", "tokens": ["Im", "Feu\u00b7er", "em\u00b7por"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Fuhr er auf!", "tokens": ["Fuhr", "er", "auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vollbrachte seinen Heldenlauf,", "tokens": ["Voll\u00b7brach\u00b7te", "sei\u00b7nen", "Hel\u00b7den\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Elias! Deiner Lebenstage", "tokens": ["E\u00b7lias", "!", "Dei\u00b7ner", "Le\u00b7bens\u00b7ta\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "War wenig, W\u00fcst' und voll Plage,", "tokens": ["War", "we\u00b7nig", ",", "W\u00fcst'", "und", "voll", "Pla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "VVFIN", "KON", "ADJD", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Doch gro\u00df! \u00bbBist Du's, der kommen soll?\u00ab", "tokens": ["Doch", "gro\u00df", "!", "\u00bb", "Bist", "Du's", ",", "der", "kom\u00b7men", "soll", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$.", "$(", "VAFIN", "NE", "$,", "PRELS", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Tod und Banden sind ihm wohl.", "tokens": ["Und", "Tod", "und", "Ban\u00b7den", "sind", "ihm", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sonn' ist da! die Morgenr\u00f6the", "tokens": ["Die", "Sonn'", "ist", "da", "!", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verr\u00f6thet! Schwert im Tanz, o t\u00f6dte", "tokens": ["Ver\u00b7r\u00f6t\u00b7het", "!", "Schwert", "im", "Tanz", ",", "o", "t\u00f6d\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "NN", "APPRART