{"dta.poem.19520": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Zwingburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Gebrochen ist der alte Twing,", "tokens": ["Ge\u00b7bro\u00b7chen", "ist", "der", "al\u00b7te", "Twing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ringsum ergr\u00fcnt sein Mauerring,", "tokens": ["Ring\u00b7sum", "er\u00b7gr\u00fcnt", "sein", "Mau\u00b7er\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der Eppich schwankt im Fenster,", "tokens": ["Der", "Ep\u00b7pich", "schwankt", "im", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versunken in der Erde Schoo\u00df", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "der", "Er\u00b7de", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tief unter das besonnte Moos", "tokens": ["Tief", "un\u00b7ter", "das", "be\u00b7sonn\u00b7te", "Moos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind Ritter und Gespenster.", "tokens": ["Sind", "Rit\u00b7ter", "und", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo durch das tiefgew\u00f6lbte Thor", "tokens": ["Wo", "durch", "das", "tief\u00b7ge\u00b7w\u00f6lb\u00b7te", "Thor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die zorn'ge Fehde schritt hervor", "tokens": ["Die", "zorn'\u00b7ge", "Feh\u00b7de", "schritt", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lie\u00df die H\u00f6rner schmettern,", "tokens": ["Und", "lie\u00df", "die", "H\u00f6r\u00b7ner", "schmet\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da hat sich, duftig eingeengt,", "tokens": ["Da", "hat", "sich", ",", "duf\u00b7tig", "ein\u00b7ge\u00b7engt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Zicklein ans Gestr\u00e4uch geh\u00e4ngt", "tokens": ["Ein", "Zic\u00b7klein", "ans", "Ge\u00b7str\u00e4uch", "ge\u00b7h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und nascht von jungen Bl\u00e4ttern.", "tokens": ["Und", "nascht", "von", "jun\u00b7gen", "Bl\u00e4t\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo wild vertr\u00e4umt Frau Minne stund,", "tokens": ["Wo", "wild", "ver\u00b7tr\u00e4umt", "Frau", "Min\u00b7ne", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVPP", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zerrann auf blauem Himmelsgrund", "tokens": ["Zer\u00b7rann", "auf", "blau\u00b7em", "Him\u00b7mels\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der kecke Bau des Erkers.", "tokens": ["Der", "ke\u00b7cke", "Bau", "des", "Er\u00b7kers", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo tief der stumme Ha\u00df gegrollt,", "tokens": ["Wo", "tief", "der", "stum\u00b7me", "Ha\u00df", "ge\u00b7grollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Liegt weich, ins hohe Gras gerollt,", "tokens": ["Liegt", "weich", ",", "ins", "ho\u00b7he", "Gras", "ge\u00b7rollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein feuchter Stein des Kerkers.", "tokens": ["Ein", "feuch\u00b7ter", "Stein", "des", "Ker\u00b7kers", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wo den Teich vom H\u00fcgelhang", "tokens": ["Und", "wo", "den", "Teich", "vom", "H\u00fc\u00b7gel\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab die trotz'ge Feste zwang", "tokens": ["Her\u00b7ab", "die", "trotz'\u00b7ge", "Fes\u00b7te", "zwang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein finster Bild zu spiegeln,", "tokens": ["Ein", "fins\u00b7ter", "Bild", "zu", "spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da rudert, von der Flut benetzt,", "tokens": ["Da", "ru\u00b7dert", ",", "von", "der", "Flut", "be\u00b7netzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Burg zerst\u00f6rtes Wappen jetzt:", "tokens": ["Der", "Burg", "zer\u00b7st\u00f6r\u00b7tes", "Wap\u00b7pen", "jetzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schwan mit Silberfl\u00fcgeln.", "tokens": ["Ein", "Schwan", "mit", "Sil\u00b7ber\u00b7fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}