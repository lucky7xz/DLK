{"textgrid.poem.40658": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du aber siehst es nicht, was wir beklagen:", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du aber siehst es nicht, was wir beklagen:", "tokens": ["Du", "a\u00b7ber", "siehst", "es", "nicht", ",", "was", "wir", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt Nebel schleichend, wo's so sch\u00f6n gewittert,", "tokens": ["Jetzt", "Ne\u00b7bel", "schlei\u00b7chend", ",", "wo's", "so", "sch\u00f6n", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "$,", "PWAV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Zeit Panier in Koth geschleift, zerknittert", "tokens": ["Der", "Zeit", "Pa\u00b7nier", "in", "Koth", "ge\u00b7schleift", ",", "zer\u00b7knit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von H\u00e4nden, die's zu Sternen sollten tragen;", "tokens": ["Von", "H\u00e4n\u00b7den", ",", "die's", "zu", "Ster\u00b7nen", "soll\u00b7ten", "tra\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Einheit Ring am M\u00e4kelsinn zersplittert,", "tokens": ["Der", "Ein\u00b7heit", "Ring", "am", "M\u00e4\u00b7kel\u00b7sinn", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Liebesgluth am Ehepakt zerschlagen;", "tokens": ["Wie", "Lie\u00b7bes\u00b7gluth", "am", "E\u00b7he\u00b7pakt", "zer\u00b7schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch Leichen, die schon auf der Bahre lagen,", "tokens": ["Doch", "Lei\u00b7chen", ",", "die", "schon", "auf", "der", "Bah\u00b7re", "la\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Lebensl\u00fcge neugeschminkt, beflittert;", "tokens": ["Zur", "Le\u00b7bens\u00b7l\u00fc\u00b7ge", "neu\u00b7ge\u00b7schminkt", ",", "be\u00b7flit\u00b7tert", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die wilde Freiheit nur der Leidenschaften,", "tokens": ["Die", "wil\u00b7de", "Frei\u00b7heit", "nur", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blutwunden, die durch Bruderliebe klafften,", "tokens": ["Blut\u00b7wun\u00b7den", ",", "die", "durch", "Bru\u00b7der\u00b7lie\u00b7be", "klaff\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Despoten, die das Purpurkleid nur meiden,", "tokens": ["Des\u00b7po\u00b7ten", ",", "die", "das", "Pur\u00b7pur\u00b7kleid", "nur", "mei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verrath und Schmach mit unsrer Flagge fahren,", "tokens": ["Ver\u00b7rath", "und", "Schmach", "mit", "uns\u00b7rer", "Flag\u00b7ge", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Sternenbanner, deckend den Korsaren! \u2013", "tokens": ["Das", "Ster\u00b7nen\u00b7ban\u00b7ner", ",", "de\u00b7ckend", "den", "Kor\u00b7sa\u00b7ren", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch mu\u00df den Kranken der Gesunde neiden.", "tokens": ["Noch", "mu\u00df", "den", "Kran\u00b7ken", "der", "Ge\u00b7sun\u00b7de", "nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}