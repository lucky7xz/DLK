{"textgrid.poem.55722": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: O da\u00df ich doch von dir geschieden w\u00e4re,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich doch von dir geschieden w\u00e4re,", "tokens": ["O", "da\u00df", "ich", "doch", "von", "dir", "ge\u00b7schie\u00b7den", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich am Gipfel meines Gl\u00fcckes stand;", "tokens": ["Als", "ich", "am", "Gip\u00b7fel", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur s\u00fc\u00dfe Schmerzen h\u00e4tt' ich dann gekannt,", "tokens": ["Nur", "s\u00fc\u00b7\u00dfe", "Schmer\u00b7zen", "h\u00e4tt'", "ich", "dann", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht diese grenzenlose, w\u00fcste Leere.", "tokens": ["Nicht", "die\u00b7se", "gren\u00b7zen\u00b7lo\u00b7se", ",", "w\u00fcs\u00b7te", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn all mein Lieben ist zu nichts zerronnen,", "tokens": ["Denn", "all", "mein", "Lie\u00b7ben", "ist", "zu", "nichts", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "VAFIN", "PTKA", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und all mein Gl\u00fcck zerfiel in eklen Staub;", "tokens": ["Und", "all", "mein", "Gl\u00fcck", "zer\u00b7fiel", "in", "ek\u00b7len", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Du, auch Du, mein Lieb, der S\u00fcnde Raub, \u2013", "tokens": ["Da\u00df", "Du", ",", "auch", "Du", ",", "mein", "Lieb", ",", "der", "S\u00fcn\u00b7de", "Raub", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O da\u00df ich ", "tokens": ["O", "da\u00df", "ich"], "token_info": ["word", "word", "word"], "pos": ["NE", "KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Der Dich so s\u00fc\u00df verkl\u00e4rt, der Heil'genschimmer,", "tokens": ["Der", "Dich", "so", "s\u00fc\u00df", "ver\u00b7kl\u00e4rt", ",", "der", "Heil'\u00b7gen\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verblichen ist er, und ein Phosphorlicht", "tokens": ["Ver\u00b7bli\u00b7chen", "ist", "er", ",", "und", "ein", "Phos\u00b7phor\u00b7licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit bl\u00e4ulichgr\u00fcnen Strahlen Dich umflicht, \u2013", "tokens": ["Mit", "bl\u00e4u\u00b7lich\u00b7gr\u00fc\u00b7nen", "Strah\u00b7len", "Dich", "um\u00b7flicht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich aber glaub' an reine Liebe nimmer.", "tokens": ["Ich", "a\u00b7ber", "glaub'", "an", "rei\u00b7ne", "Lie\u00b7be", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}