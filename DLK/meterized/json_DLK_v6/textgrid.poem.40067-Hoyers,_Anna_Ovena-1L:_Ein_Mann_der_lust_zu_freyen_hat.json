{"textgrid.poem.40067": {"metadata": {"author": {"name": "Hoyers, Anna Ovena", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Mann der lust zu freyen hat/", "genre": "verse", "period": "N.A.", "pub_year": 1643, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann der lust zu freyen hat/", "tokens": ["Ein", "Mann", "der", "lust", "zu", "frey\u00b7en", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll erstlich bitten Gott umb gnad:", "tokens": ["Soll", "erst\u00b7lich", "bit\u00b7ten", "Gott", "umb", "gnad", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd darnach folgen mit der that", "tokens": ["Vnd", "dar\u00b7nach", "fol\u00b7gen", "mit", "der", "that"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "APPR", "ART", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ders nicht will thun, wird haben schad.", "tokens": ["Ders", "nicht", "will", "thun", ",", "wird", "ha\u00b7ben", "schad", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VMFIN", "VVINF", "$,", "VAFIN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Abschewlich ist mir anzusehn", "tokens": ["Ab\u00b7schew\u00b7lich", "ist", "mir", "an\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "VVIZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein altes Weib f\u00fcrm Pfaffen stehn/", "tokens": ["Ein", "al\u00b7tes", "Weib", "f\u00fcrm", "Pfaf\u00b7fen", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beym Mann sich lassen trawen.", "tokens": ["Beym", "Mann", "sich", "las\u00b7sen", "tra\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Kein lust/ frewd oder zeitvertreib", "tokens": ["Kein", "lust", "/", "frewd", "o\u00b7der", "zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kan seyn bey einem alten Weib/", "tokens": ["Kan", "seyn", "bey", "ei\u00b7nem", "al\u00b7ten", "Weib", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Ansehn bringet grawen.", "tokens": ["Das", "An\u00b7sehn", "brin\u00b7get", "gra\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Was solt denn das anr\u00fchren nicht?", "tokens": ["Was", "solt", "denn", "das", "an\u00b7r\u00fch\u00b7ren", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "KON", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O armer Mann der sich verpflicht", "tokens": ["O", "ar\u00b7mer", "Mann", "der", "sich", "ver\u00b7pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ART", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sein zeit so hin zu bringen", "tokens": ["Sein", "zeit", "so", "hin", "zu", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ohn frucht/ frewd und ergetzligkeit/", "tokens": ["Ohn", "frucht", "/", "frewd", "und", "er\u00b7getz\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Verlohren ist all sein arbeit/", "tokens": ["Ver\u00b7loh\u00b7ren", "ist", "all", "sein", "ar\u00b7beit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Das heist nach ungl\u00fcck ringen.", "tokens": ["Das", "heist", "nach", "un\u00b7gl\u00fcck", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ist doch kein lieblichkeit daran!", "tokens": ["Ist", "doch", "kein", "lieb\u00b7lich\u00b7keit", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Man schm\u00fcck es auch so sch\u00f6n man kan/", "tokens": ["Man", "schm\u00fcck", "es", "auch", "so", "sch\u00f6n", "man", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "PIS", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "La\u00df koltzen/ kosen/ kallen", "tokens": ["La\u00df", "kolt\u00b7zen", "/", "ko\u00b7sen", "/", "kal\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VVIMP", "VVINF", "$(", "VVINF", "$(", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Babst/ Bischoff/ Bader/ Herr und Knecht/", "tokens": ["Babst", "/", "Bi\u00b7schoff", "/", "Ba\u00b7der", "/", "Herr", "und", "Knecht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "NE", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wenn sie es hielten all f\u00fcr recht/", "tokens": ["Wenn", "sie", "es", "hiel\u00b7ten", "all", "f\u00fcr", "recht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "PIAT", "APPR", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wills mir doch nicht gefallen.", "tokens": ["Wills", "mir", "doch", "nicht", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ich halt der Alten Weiber Frey", "tokens": ["Ich", "halt", "der", "Al\u00b7ten", "Wei\u00b7ber", "Frey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "F\u00fcr ein Ehrbare Hurerey", "tokens": ["F\u00fcr", "ein", "Ehr\u00b7ba\u00b7re", "Hu\u00b7re\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Warheit h\u00f6fflich zu sagen:", "tokens": ["War\u00b7heit", "h\u00f6ff\u00b7lich", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Aber man wil sie h\u00f6ren nicht/", "tokens": ["A\u00b7ber", "man", "wil", "sie", "h\u00f6\u00b7ren", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Sie findt kein Herberg/ wie man spricht/", "tokens": ["Sie", "findt", "kein", "Her\u00b7berg", "/", "wie", "man", "spricht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PWAV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Dennoch mu\u00df ich es wagen/", "tokens": ["Den\u00b7noch", "mu\u00df", "ich", "es", "wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Vnd sagen: Es ist schand im Land", "tokens": ["Vnd", "sa\u00b7gen", ":", "Es", "ist", "schand", "im", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "PPER", "VAFIN", "VVFIN", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.26": {"text": "Da\u00df man setzt solche im Ehestand", "tokens": ["Da\u00df", "man", "setzt", "sol\u00b7che", "im", "E\u00b7hes\u00b7tand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "PIAT", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Die nicht mehr k\u00f6nnen taugen.", "tokens": ["Die", "nicht", "mehr", "k\u00f6n\u00b7nen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Betrachtet des Ehestands ", "tokens": ["Be\u00b7trach\u00b7tet", "des", "E\u00b7hes\u00b7tands"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Ich bitt euch alle noch einmal/", "tokens": ["Ich", "bitt", "euch", "al\u00b7le", "noch", "ein\u00b7mal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Seht doch mit eignen augen/", "tokens": ["Seht", "doch", "mit", "eig\u00b7nen", "au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Wachset und mehrt euch/ spricht der Herr/", "tokens": ["Wach\u00b7set", "und", "mehrt", "euch", "/", "spricht", "der", "Herr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$(", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Kein altes Weib dient darzu mehr/", "tokens": ["Kein", "al\u00b7tes", "Weib", "dient", "dar\u00b7zu", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PAV", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Ist dann solch Frey nicht grewlich?", "tokens": ["Ist", "dann", "solch", "Frey", "nicht", "grew\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "O lieben M\u00e4nner jung und alt/", "tokens": ["O", "lie\u00b7ben", "M\u00e4n\u00b7ner", "jung", "und", "alt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Der Alten Weiber euch enthalt", "tokens": ["Der", "Al\u00b7ten", "Wei\u00b7ber", "euch", "ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Ich warn euch f\u00fcr sie trewlich:", "tokens": ["Ich", "warn", "euch", "f\u00fcr", "sie", "trew\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Denn es ist doch bey ihrer frey/", "tokens": ["Denn", "es", "ist", "doch", "bey", "ih\u00b7rer", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.38": {"text": "Kein Gottes segen noch gedey.", "tokens": ["Kein", "Got\u00b7tes", "se\u00b7gen", "noch", "ge\u00b7dey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Ein solch Heyrath geh\u00f6ret", "tokens": ["Ein", "solch", "Hey\u00b7rath", "ge\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.40": {"text": "Zum grewel der Verw\u00fcstung mit/", "tokens": ["Zum", "gre\u00b7wel", "der", "Ver\u00b7w\u00fcs\u00b7tung", "mit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART