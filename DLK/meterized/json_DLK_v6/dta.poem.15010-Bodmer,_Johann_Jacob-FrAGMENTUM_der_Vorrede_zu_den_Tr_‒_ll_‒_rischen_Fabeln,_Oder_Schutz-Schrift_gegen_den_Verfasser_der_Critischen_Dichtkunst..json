{"dta.poem.15010": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "FrAGMENTUM  \n der Vorrede zu den  \n  Tr \u2012 ll \u2012 rischen Fabeln,  \n Oder  \n  Schutz-Schrift  \n gegen den Verfasser der  \n  Critischen Dichtkunst.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind Wort und Sylben denn von solcher Wichtigkeit,", "tokens": ["Sind", "Wort", "und", "Syl\u00b7ben", "denn", "von", "sol\u00b7cher", "Wich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "KON", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man so ungest\u00fcm, wie H\u00e4rings-Weiber, schreyt?", "tokens": ["Da\u00df", "man", "so", "un\u00b7ge\u00b7st\u00fcm", ",", "wie", "H\u00e4\u00b7rings\u00b7Wei\u00b7ber", ",", "schreyt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df bringt der Wissenschaft gewi\u00df den gr\u00f6sten Schaden,", "tokens": ["Die\u00df", "bringt", "der", "Wis\u00b7sen\u00b7schaft", "ge\u00b7wi\u00df", "den", "gr\u00f6s\u00b7ten", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ihre Meister selbst durch Grobheit auf sich laden,", "tokens": ["Den", "ih\u00b7re", "Meis\u00b7ter", "selbst", "durch", "Grob\u00b7heit", "auf", "sich", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dadurch wird sie hernach vern\u00fcnftigen verha\u00dft,", "tokens": ["Da\u00b7durch", "wird", "sie", "her\u00b7nach", "ver\u00b7n\u00fcnf\u00b7ti\u00b7gen", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ein Gelehrter ist so viel als ein Fantast.", "tokens": ["Und", "ein", "Ge\u00b7lehr\u00b7ter", "ist", "so", "viel", "als", "ein", "Fan\u00b7tast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}