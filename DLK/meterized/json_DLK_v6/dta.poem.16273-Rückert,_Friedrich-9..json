{"dta.poem.16273": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich denk' an euch, die ihr vom Schoo\u00df mir aufgeflogen,", "tokens": ["Ich", "denk'", "an", "euch", ",", "die", "ihr", "vom", "Schoo\u00df", "mir", "auf\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nun herab auf mich l\u00e4chelt vom Himmelsbogen.", "tokens": ["Und", "nun", "her\u00b7ab", "auf", "mich", "l\u00e4\u00b7chelt", "vom", "Him\u00b7mels\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der holde Fr\u00fchling kommt, wo alles Sch\u00f6ne nieder", "tokens": ["Der", "hol\u00b7de", "Fr\u00fch\u00b7ling", "kommt", ",", "wo", "al\u00b7les", "Sch\u00f6\u00b7ne", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PIS", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Himmel steigt, da kommt auch euer Bild mir wieder.", "tokens": ["Vom", "Him\u00b7mel", "steigt", ",", "da", "kommt", "auch", "eu\u00b7er", "Bild", "mir", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun fliegt der Schmetterling, nach welchem sonst ihr lieft;", "tokens": ["Nun", "fliegt", "der", "Schmet\u00b7ter\u00b7ling", ",", "nach", "wel\u00b7chem", "sonst", "ihr", "lieft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Vogel singt, von dem ihr eingesungen schlieft.", "tokens": ["Der", "Vo\u00b7gel", "singt", ",", "von", "dem", "ihr", "ein\u00b7ge\u00b7sun\u00b7gen", "schlieft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun bl\u00fchn die Blumen, die an eur Verbl\u00fchn mich mahnen,", "tokens": ["Nun", "bl\u00fchn", "die", "Blu\u00b7men", ",", "die", "an", "eur", "Ver\u00b7bl\u00fchn", "mich", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und L\u00fcfte wehn, die eure N\u00e4h mich lassen ahnen.", "tokens": ["Und", "L\u00fcf\u00b7te", "wehn", ",", "die", "eu\u00b7re", "N\u00e4h", "mich", "las\u00b7sen", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was ihr mir waret, was ich euch gewesen bin,", "tokens": ["Was", "ihr", "mir", "wa\u00b7ret", ",", "was", "ich", "euch", "ge\u00b7we\u00b7sen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "PWS", "PPER", "PPER", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was ihr jetzt mir seid, besch\u00e4ftigt meinen Sinn.", "tokens": ["Und", "was", "ihr", "jetzt", "mir", "seid", ",", "be\u00b7sch\u00e4f\u00b7tigt", "mei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "PPER", "VAFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ihr wart an mich gekn\u00fcpft durch ein nat\u00fcrlich Band,", "tokens": ["Ihr", "wart", "an", "mich", "ge\u00b7kn\u00fcpft", "durch", "ein", "na\u00b7t\u00fcr\u00b7lich", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das aber hat gel\u00f6st des Todes kalte Hand.", "tokens": ["Das", "a\u00b7ber", "hat", "ge\u00b7l\u00f6st", "des", "To\u00b7des", "kal\u00b7te", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "VVPP", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nur da\u00df ihr im Gef\u00fchl der Liebe waret mein,", "tokens": ["Nur", "da\u00df", "ihr", "im", "Ge\u00b7f\u00fchl", "der", "Lie\u00b7be", "wa\u00b7ret", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verhei\u00dft mir, da\u00df ihr auch mein werdet ewig seyn.", "tokens": ["Ver\u00b7hei\u00dft", "mir", ",", "da\u00df", "ihr", "auch", "mein", "wer\u00b7det", "e\u00b7wig", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "VAFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Um dis Gef\u00fchl und euch in ihm nie zu verlieren,", "tokens": ["Um", "dis", "Ge\u00b7f\u00fchl", "und", "euch", "in", "ihm", "nie", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "NN", "KON", "PPER", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Will ich noch oft mein Lied mit euern Namen zieren.", "tokens": ["Will", "ich", "noch", "oft", "mein", "Lied", "mit", "eu\u00b7ern", "Na\u00b7men", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}