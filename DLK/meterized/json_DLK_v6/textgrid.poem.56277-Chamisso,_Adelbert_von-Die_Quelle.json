{"textgrid.poem.56277": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Quelle", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsre Quelle kommt im Schatten", "tokens": ["Uns\u00b7re", "Quel\u00b7le", "kommt", "im", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Duft'ger Linden an das Licht,", "tokens": ["Duft'\u00b7ger", "Lin\u00b7den", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie dort die V\u00f6gel singen,", "tokens": ["Und", "wie", "dort", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nein, das wei\u00df doch jeder nicht!", "tokens": ["Nein", ",", "das", "wei\u00df", "doch", "je\u00b7der", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "ADV", "PIS", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und das M\u00e4dchen kam zur Quelle,", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "kam", "zur", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Krug in jeder Hand,", "tokens": ["Ei\u00b7nen", "Krug", "in", "je\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollte schnell die Kr\u00fcge f\u00fcllen,", "tokens": ["Woll\u00b7te", "schnell", "die", "Kr\u00fc\u00b7ge", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein J\u00fcngling vor ihr stand.", "tokens": ["Als", "ein", "J\u00fcng\u00b7ling", "vor", "ihr", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00f6gen wohl geplaudert haben,", "tokens": ["M\u00f6\u00b7gen", "wohl", "ge\u00b7plau\u00b7dert", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam das M\u00e4dchen sp\u00e4t nach Haus:", "tokens": ["Kam", "das", "M\u00e4d\u00b7chen", "sp\u00e4t", "nach", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gute Mutter, sollst nicht schelten,", "tokens": ["Gu\u00b7te", "Mut\u00b7ter", ",", "sollst", "nicht", "schel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sandtest selbst ja mich hinaus.", "tokens": ["Sand\u00b7test", "selbst", "ja", "mich", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Geht man leicht zur Quelle, tr\u00e4gt man", "tokens": ["Geht", "man", "leicht", "zur", "Quel\u00b7le", ",", "tr\u00e4gt", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "APPRART", "NN", "$,", "VVFIN", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch zu Haus ein schwer Gewicht,", "tokens": ["Doch", "zu", "Haus", "ein", "schwer", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wie dort die V\u00f6gel singen \u2013", "tokens": ["Und", "wie", "dort", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Mutter, nein, das wei\u00dft du nicht!", "tokens": ["Mut\u00b7ter", ",", "nein", ",", "das", "wei\u00dft", "du", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}