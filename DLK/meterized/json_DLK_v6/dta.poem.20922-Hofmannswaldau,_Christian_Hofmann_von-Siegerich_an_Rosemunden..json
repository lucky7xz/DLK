-+-+-+-", "measure": "iambic.penta"}, "line.78": {"text": "schauen/", "tokens": ["schau\u00b7en", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.79": {"text": "Ob deine Feder auch den Lippen \u00e4hnlich sey/", "tokens": ["Ob", "dei\u00b7ne", "Fe\u00b7der", "auch", "den", "Lip\u00b7pen", "\u00e4hn\u00b7lich", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Schreib itzt ohn alle Scheu/ du kanst mir sicher trauen/", "tokens": ["Schreib", "itzt", "ohn", "al\u00b7le", "Scheu", "/", "du", "kanst", "mir", "si\u00b7cher", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Mein Nahmen machet dich von allen Sorgen frey.", "tokens": ["Mein", "Nah\u00b7men", "ma\u00b7chet", "dich", "von", "al\u00b7len", "Sor\u00b7gen", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Der Kummer mu\u00df itzund aus deinem Hertzen weichen/", "tokens": ["Der", "Kum\u00b7mer", "mu\u00df", "it\u00b7zund", "aus", "dei\u00b7nem", "Hert\u00b7zen", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Du hast ein sch\u00f6nes Pfand mein hohes Wort von mir/", "tokens": ["Du", "hast", "ein", "sch\u00f6\u00b7nes", "Pfand", "mein", "ho\u00b7hes", "Wort", "von", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Das Gl\u00fccke mu\u00df nunmehr vor dir sein Seegel strei-", "tokens": ["Das", "Gl\u00fc\u00b7cke", "mu\u00df", "nun\u00b7mehr", "vor", "dir", "sein", "See\u00b7gel", "strei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.85": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.86": {"text": "Und was ein gr\u00f6ssers ist/ auch anckern neben dir.", "tokens": ["Und", "was", "ein", "gr\u00f6s\u00b7sers", "ist", "/", "auch", "an\u00b7ckern", "ne\u00b7ben", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "VAFIN", "$(", "ADV", "VVINF", "APPR", "PPER", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.87": {"text": "Ich fasse was du schreibst mit hochgeneigten H\u00e4nden/", "tokens": ["Ich", "fas\u00b7se", "was", "du", "schreibst", "mit", "hoch\u00b7ge\u00b7neig\u00b7ten", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.88": {"text": "Und deinem Briefe will ich selbst entgegen gehn/", "tokens": ["Und", "dei\u00b7nem", "Brie\u00b7fe", "will", "ich", "selbst", "ent\u00b7ge\u00b7gen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "ADV", "PTKVZ", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.89": {"text": "Ich wei\u00df du wirst mir nichts als Zucker \u00fcbersenden/", "tokens": ["Ich", "wei\u00df", "du", "wirst", "mir", "nichts", "als", "Zu\u00b7cker", "\u00fc\u00b7ber\u00b7sen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "PIS", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.90": {"text": "Der wol mit W\u00fcrden kan auf meiner Tafel stehn.", "tokens": ["Der", "wol", "mit", "W\u00fcr\u00b7den", "kan", "auf", "mei\u00b7ner", "Ta\u00b7fel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.91": {"text": "Mich deucht ich schmecke schon die s\u00fcsse G\u00f6tter Speise/", "tokens": ["Mich", "deucht", "ich", "schme\u00b7cke", "schon", "die", "s\u00fcs\u00b7se", "G\u00f6t\u00b7ter", "Spei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.92": {"text": "Die Gott den Menschen auch vor Menschen machen", "tokens": ["Die", "Gott", "den", "Men\u00b7schen", "auch", "vor", "Men\u00b7schen", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.93": {"text": "heist/", "tokens": ["heist", "/"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$("], "meter": "-", "measure": "single.down"}, "line.94": {"text": "Und sp\u00fchre durch den Brief der Lieder s\u00fcsse Weise/", "tokens": ["Und", "sp\u00fch\u00b7re", "durch", "den", "Brief", "der", "Lie\u00b7der", "s\u00fcs\u00b7se", "Wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.95": {"text": "Die dein ber\u00fchmeer Mund zusingen sich beflei\u00dft.", "tokens": ["Die", "dein", "be\u00b7r\u00fch\u00b7meer", "Mund", "zu\u00b7sin\u00b7gen", "sich", "be\u00b7flei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.96": {"text": "Doch schreib mir nicht allein/ denn Schreiben seyn nur", "tokens": ["Doch", "schreib", "mir", "nicht", "al\u00b7lein", "/", "denn", "Schrei\u00b7ben", "seyn", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$(", "KON", "NN", "PPOSAT", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.97": {"text": "Schreiben/", "tokens": ["Schrei\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.98": {"text": "Und wer alleine schreibt/ der thut nicht allzuviel/", "tokens": ["Und", "wer", "al\u00b7lei\u00b7ne", "schreibt", "/", "der", "thut", "nicht", "all\u00b7zu\u00b7viel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$(", "ART", "VVFIN", "PTKNEG", "PIAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.99": {"text": "So du bey mir begehrst in guter Gunst zubleiben/", "tokens": ["So", "du", "bey", "mir", "be\u00b7gehrst", "in", "gu\u00b7ter", "Gunst", "zu\u00b7blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.100": {"text": "So kom wie ich gesagt/ und singe wie ich will.", "tokens": ["So", "kom", "wie", "ich", "ge\u00b7sagt", "/", "und", "sin\u00b7ge", "wie", "ich", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "PPER", "VVPP", "$(", "KON", "VVFIN", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.101": {"text": "Ich wei\u00df die H\u00f6fligkeit/ so mit dir ist gebohren/", "tokens": ["Ich", "wei\u00df", "die", "H\u00f6f\u00b7lig\u00b7keit", "/", "so", "mit", "dir", "ist", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ADV", "APPR", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.102": {"text": "Verbietet dir itzund zubrauchen Nicht nnd Nein/", "tokens": ["Ver\u00b7bie\u00b7tet", "dir", "it\u00b7zund", "zu\u00b7brau\u00b7chen", "Nicht", "nnd", "Nein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "PTKNEG", "KON", "PTKANT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.103": {"text": "Denn was mein Sinn ihm hat zu seiner Gunst erkoh-", "tokens": ["Denn", "was", "mein", "Sinn", "ihm", "hat", "zu", "sei\u00b7ner", "Gunst", "er\u00b7koh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "NN", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.104": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.105": {"text": "Das mu\u00df bey stetem Ja ihm auch gehorsam seyn.", "tokens": ["Das", "mu\u00df", "bey", "ste\u00b7tem", "Ja", "ihm", "auch", "ge\u00b7hor\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "NN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.106": {"text": "Kom Sch\u00f6nste/ glaube mir/ mein Thor das steht dir", "tokens": ["Kom", "Sch\u00f6ns\u00b7te", "/", "glau\u00b7be", "mir", "/", "mein", "Thor", "das", "steht", "dir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.107": {"text": "offen/", "tokens": ["of\u00b7fen", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.108": {"text": "Und wilst du mehr als di\u00df/ mein Hertze selbst dazu/", "tokens": ["Und", "wilst", "du", "mehr", "als", "di\u00df", "/", "mein", "Hert\u00b7ze", "selbst", "da\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "KOKOM", "PDS", "$(", "PPOSAT", "VVFIN", "ADV", "PAV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.109": {"text": "Di\u00df was du nur ersinnst/ das hastu auch zuhoffen/", "tokens": ["Di\u00df", "was", "du", "nur", "er\u00b7sinnst", "/", "das", "has\u00b7tu", "auch", "zu\u00b7hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ADV", "ADV", "$(", "PDS", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.110": {"text": "(schreib\u2019/ eil\u2019 und singe mir/ Ach was verweilest du?", "tokens": ["(", "schreib'", "/", "eil'", "und", "sin\u00b7ge", "mir", "/", "Ach", "was", "ver\u00b7wei\u00b7lest", "du", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "VVFIN", "KON", "VVFIN", "PPER", "$(", "ITJ", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.111": {"text": "Die Fl\u00fcgel meiner Gunst die sollen dich bedecken/", "tokens": ["Die", "Fl\u00fc\u00b7gel", "mei\u00b7ner", "Gunst", "die", "sol\u00b7len", "dich", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "VMFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.112": {"text": "Was hier nicht sicher ist kan nirgend sicher stehn;", "tokens": ["Was", "hier", "nicht", "si\u00b7cher", "ist", "kan", "nir\u00b7gend", "si\u00b7cher", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "ADJD", "VAFIN", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.113": {"text": "Wo eine Crone liebt/ da werden keine Flecken/", "tokens": ["Wo", "ei\u00b7ne", "Cro\u00b7ne", "liebt", "/", "da", "wer\u00b7den", "kei\u00b7ne", "Fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.114": {"text": "Und weren Flecken da/ so m\u00fcsten sie vergehn.", "tokens": ["Und", "we\u00b7ren", "Fle\u00b7cken", "da", "/", "so", "m\u00fcs\u00b7ten", "sie", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}