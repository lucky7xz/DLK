{"dta.poem.20922": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Siegerich an Rosemunden.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Djr w\u00fcnschet Siegerich mehr freudenreiche", "tokens": ["Djr", "w\u00fcn\u00b7schet", "Sie\u00b7ge\u00b7rich", "mehr", "freu\u00b7den\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stunden/", "tokens": ["Stun\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Als Rosen/ Jungfrau/ dir auf deinen Wan-", "tokens": ["Als", "Ro\u00b7sen", "/", "Jung\u00b7frau", "/", "dir", "auf", "dei\u00b7nen", "Wan"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$(", "NN", "$(", "PPER", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gen stehn/", "tokens": ["gen", "stehn", "/"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "VVINF", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Als Lilgen die Natur um deinen Hals gewunden/", "tokens": ["Als", "Lil\u00b7gen", "die", "Na\u00b7tur", "um", "dei\u00b7nen", "Hals", "ge\u00b7wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Zucker Silben stets au\u00df deinem Munde gehn.", "tokens": ["Und", "Zu\u00b7cker", "Sil\u00b7ben", "stets", "au\u00df", "dei\u00b7nem", "Mun\u00b7de", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "K\u00f6nt ich was ich gew\u00fcnscht/ dir auch zugleiche geben/", "tokens": ["K\u00f6nt", "ich", "was", "ich", "ge\u00b7w\u00fcnscht", "/", "dir", "auch", "zu\u00b7glei\u00b7che", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWS", "PPER", "VVPP", "$(", "PPER", "ADV", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So \u00f6ffnet ich itzund dir v\u00f6llig meine Handt/", "tokens": ["So", "\u00f6ff\u00b7net", "ich", "it\u00b7zund", "dir", "v\u00f6l\u00b7lig", "mei\u00b7ne", "Handt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Himmel lasse doch umb deine Scheitel schweben/", "tokens": ["Der", "Him\u00b7mel", "las\u00b7se", "doch", "umb", "dei\u00b7ne", "Schei\u00b7tel", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was keine Sch\u00f6nheit nicht bey einem Helden fandt.", "tokens": ["Was", "kei\u00b7ne", "Sch\u00f6n\u00b7heit", "nicht", "bey", "ei\u00b7nem", "Hel\u00b7den", "fandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nicht wunder dich darob/ was ich itzund geschrieben/", "tokens": ["Nicht", "wun\u00b7der", "dich", "da\u00b7rob", "/", "was", "ich", "it\u00b7zund", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PRF", "PAV", "$(", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Betrachte dich nur recht/ kenst du dich selber nicht?", "tokens": ["Be\u00b7trach\u00b7te", "dich", "nur", "recht", "/", "kenst", "du", "dich", "sel\u00b7ber", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$(", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Spiegel will/ du solst dich in dich selbst verlieben/", "tokens": ["Der", "Spie\u00b7gel", "will", "/", "du", "solst", "dich", "in", "dich", "selbst", "ver\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$(", "PPER", "VMFIN", "PRF", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und dein Gesichte lehnt den Sternen Krafft und Licht;", "tokens": ["Und", "dein", "Ge\u00b7sich\u00b7te", "lehnt", "den", "Ster\u00b7nen", "Krafft", "und", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es hat das lange Jahr vier Zeiten/ du nur eine;", "tokens": ["Es", "hat", "das", "lan\u00b7ge", "Jahr", "vier", "Zei\u00b7ten", "/", "du", "nur", "ei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "CARD", "NN", "$(", "PPER", "ADV", "ART", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es bl\u00fcht der Fr\u00fchling stets um deinen frischen Mund/", "tokens": ["Es", "bl\u00fcht", "der", "Fr\u00fch\u00b7ling", "stets", "um", "dei\u00b7nen", "fri\u00b7schen", "Mund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein Winter ist bey dir/ f\u00fcr deiner Augen Scheine", "tokens": ["Kein", "Win\u00b7ter", "ist", "bey", "dir", "/", "f\u00fcr", "dei\u00b7ner", "Au\u00b7gen", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPER", "$(", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ist fast der Sonne selbst zuscheinen nicht vergunt.", "tokens": ["Ist", "fast", "der", "Son\u00b7ne", "selbst", "zu\u00b7schei\u00b7nen", "nicht", "ver\u00b7gunt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die Tugend tr\u00e4gest du in purpurreichen Schalen/", "tokens": ["Die", "Tu\u00b7gend", "tr\u00e4\u00b7gest", "du", "in", "pur\u00b7pur\u00b7rei\u00b7chen", "Scha\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Geziehret wie es scheint/ durch weisses Helffenbein/", "tokens": ["Ge\u00b7zieh\u00b7ret", "wie", "es", "scheint", "/", "durch", "weis\u00b7ses", "Helf\u00b7fen\u00b7bein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PPER", "VVFIN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dein M\u00fcndlein ist ein Orth von tausend Nachtigalen/", "tokens": ["Dein", "M\u00fcnd\u00b7lein", "ist", "ein", "Orth", "von", "tau\u00b7send", "Nach\u00b7ti\u00b7ga\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wo Engels Zungen selbst Geh\u00fclffen wollen seyn.", "tokens": ["Wo", "En\u00b7gels", "Zun\u00b7gen", "selbst", "Ge\u00b7h\u00fclf\u00b7fen", "wol\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "ADV", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Di\u00df/ was der kleine Brief itzund an dir gepriesen/", "tokens": ["Di\u00df", "/", "was", "der", "klei\u00b7ne", "Brief", "it\u00b7zund", "an", "dir", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "ART", "ADJA", "NN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Di\u00df hat dein Siegerich von weitem nur erblickt/", "tokens": ["Di\u00df", "hat", "dein", "Sie\u00b7ge\u00b7rich", "von", "wei\u00b7tem", "nur", "er\u00b7blickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPR", "PIS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Durch Wolcken hat sich itzt die Sonne mir gewiesen/", "tokens": ["Durch", "Wol\u00b7cken", "hat", "sich", "itzt", "die", "Son\u00b7ne", "mir", "ge\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PRF", "ADV", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie da\u00df mir nicht ihr Glantz frey in die Augen r\u00fcckt?", "tokens": ["Wie", "da\u00df", "mir", "nicht", "ihr", "Glantz", "frey", "in", "die", "Au\u00b7gen", "r\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich habe mehr von dir/ als du vermeinst/ geh\u00f6ret/", "tokens": ["Ich", "ha\u00b7be", "mehr", "von", "dir", "/", "als", "du", "ver\u00b7meinst", "/", "ge\u00b7h\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "$(", "KOUS", "PPER", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du kennest nicht den Ruhm/ den dir die Warheit gibt/", "tokens": ["Du", "ken\u00b7nest", "nicht", "den", "Ruhm", "/", "den", "dir", "die", "War\u00b7heit", "gibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$(", "ART", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und meine hohe Gunst wird gegen dich vermehret/", "tokens": ["Und", "mei\u00b7ne", "ho\u00b7he", "Gunst", "wird", "ge\u00b7gen", "dich", "ver\u00b7meh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weil deiner Jugend nicht der Jugend Lust beliebt.", "tokens": ["Weil", "dei\u00b7ner", "Ju\u00b7gend", "nicht", "der", "Ju\u00b7gend", "Lust", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df von guter Handt wie du dich hast bem\u00fchet/", "tokens": ["Ich", "wei\u00df", "von", "gu\u00b7ter", "Handt", "wie", "du", "dich", "hast", "be\u00b7m\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KOKOM", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Auf einen reinen Grund zubauen deinen Ruhm/", "tokens": ["Auf", "ei\u00b7nen", "rei\u00b7nen", "Grund", "zu\u00b7bau\u00b7en", "dei\u00b7nen", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Auf derer keuschen Brust die Tugend Rose bl\u00fchet/", "tokens": ["Auf", "de\u00b7rer", "keu\u00b7schen", "Brust", "die", "Tu\u00b7gend", "Ro\u00b7se", "bl\u00fc\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die hat bey D\u00fcrfftigkeit ein reiches Eigenthum.", "tokens": ["Die", "hat", "bey", "D\u00fcr\u00b7ff\u00b7tig\u00b7keit", "ein", "rei\u00b7ches", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Mein Aug\u2019 und Ohre w\u00fcnscht/ O z\u00fcchtige Sirene/", "tokens": ["Mein", "Aug'", "und", "Oh\u00b7re", "w\u00fcnscht", "/", "O", "z\u00fcch\u00b7ti\u00b7ge", "Si\u00b7re\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$(", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Zuh\u00f6ren und zusehn/ was deine Jugend ziehrt/", "tokens": ["Zu\u00b7h\u00f6\u00b7ren", "und", "zu\u00b7sehn", "/", "was", "dei\u00b7ne", "Ju\u00b7gend", "ziehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mich deucht/ wie albereit dein liebliches Geth\u00f6ne/", "tokens": ["Mich", "deucht", "/", "wie", "al\u00b7be\u00b7reit", "dein", "lieb\u00b7li\u00b7ches", "Ge\u00b7th\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der Ohren Wachs zerschmeltzt/ und nur uns selbst ent-", "tokens": ["Der", "Oh\u00b7ren", "Wachs", "zer\u00b7schmeltzt", "/", "und", "nur", "uns", "selbst", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "KON", "ADV", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "f\u00fchrt.", "tokens": ["f\u00fchrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.40": {"text": "Mich deucht/ ich schaue schon wie deiner Augen Blicke", "tokens": ["Mich", "deucht", "/", "ich", "schau\u00b7e", "schon", "wie", "dei\u00b7ner", "Au\u00b7gen", "Bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "KOKOM", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Bald freudig/ bald best\u00fcrtzt/ bald lieblich/ bald betr\u00fcbt", "tokens": ["Bald", "freu\u00b7dig", "/", "bald", "be\u00b7st\u00fcrtzt", "/", "bald", "lieb\u00b7lich", "/", "bald", "be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$(", "ADV", "VVPP", "$(", "ADV", "ADJD", "$(", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Begleiten deinen Thon/ und deine Wunderst\u00fccke/", "tokens": ["Be\u00b7glei\u00b7ten", "dei\u00b7nen", "Thon", "/", "und", "dei\u00b7ne", "Wun\u00b7der\u00b7st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$(", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Da\u00df sich der Himmel selbst in deine Ku