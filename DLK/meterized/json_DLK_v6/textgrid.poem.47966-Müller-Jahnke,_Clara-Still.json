{"textgrid.poem.47966": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Still", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Waldes Dunkel, an Baches Borden,", "tokens": ["In", "Wal\u00b7des", "Dun\u00b7kel", ",", "an", "Ba\u00b7ches", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die jubelnden S\u00e4nger sind still geworden.", "tokens": ["die", "ju\u00b7beln\u00b7den", "S\u00e4n\u00b7ger", "sind", "still", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VAPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und mir auch erging es wundersam: \u2013", "tokens": ["Und", "mir", "auch", "er\u00b7ging", "es", "wun\u00b7der\u00b7sam", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Als meinem Leben der Sommer kam", "tokens": ["Als", "mei\u00b7nem", "Le\u00b7ben", "der", "Som\u00b7mer", "kam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "und die Rosend\u00fcfte mein Haupt umfingen,", "tokens": ["und", "die", "Ro\u00b7sen\u00b7d\u00fcf\u00b7te", "mein", "Haupt", "um\u00b7fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "da wollt ich singen und konnt nicht singen.", "tokens": ["da", "wollt", "ich", "sin\u00b7gen", "und", "konnt", "nicht", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "KON", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von der Lippe flutet das Lied zur\u00fcck \u2013", "tokens": ["Von", "der", "Lip\u00b7pe", "flu\u00b7tet", "das", "Lied", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "im namenlosen, im stummen Gl\u00fcck", "tokens": ["im", "na\u00b7men\u00b7lo\u00b7sen", ",", "im", "stum\u00b7men", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "nur kann ich vor Gott die Seele neigen,", "tokens": ["nur", "kann", "ich", "vor", "Gott", "die", "See\u00b7le", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "nur lieben und schweigen.", "tokens": ["nur", "lie\u00b7ben", "und", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}