{"textgrid.poem.34695": {"metadata": {"author": {"name": "Weisser, Friedrich Christoph", "birth": "N.A.", "death": "N.A."}, "title": "10. Der heilige Prediger", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Heiliger d\u00fcnkt Stentor sich zu sein? \u2013", "tokens": ["Ein", "Hei\u00b7li\u00b7ger", "d\u00fcnkt", "Sten\u00b7tor", "sich", "zu", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PRF", "PTKZU", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So macht's wohl gar der helle Schein,", "tokens": ["So", "macht's", "wohl", "gar", "der", "hel\u00b7le", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sein hochheilig Haupt umflie\u00dft,", "tokens": ["Der", "sein", "hoch\u00b7hei\u00b7lig", "Haupt", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df, wenn er predigt, jedes Aug' sich schlie\u00dft.", "tokens": ["Da\u00df", ",", "wenn", "er", "pre\u00b7digt", ",", "je\u00b7des", "Aug'", "sich", "schlie\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVFIN", "$,", "PIAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}