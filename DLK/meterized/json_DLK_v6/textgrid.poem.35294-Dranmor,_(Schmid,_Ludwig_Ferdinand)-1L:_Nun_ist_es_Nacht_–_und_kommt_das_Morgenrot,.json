{"textgrid.poem.35294": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun ist es Nacht \u2013 und kommt das Morgenrot,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es Nacht \u2013 und kommt das Morgenrot,", "tokens": ["Nun", "ist", "es", "Nacht", "\u2013", "und", "kommt", "das", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann wird Bedr\u00e4ngnis an mein Fenster pochen,", "tokens": ["Dann", "wird", "Be\u00b7dr\u00e4ng\u00b7nis", "an", "mein", "Fens\u00b7ter", "po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sie noch nie so grausam mich bedroht.", "tokens": ["Wie", "sie", "noch", "nie", "so", "grau\u00b7sam", "mich", "be\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allm\u00e4cht'ger, der du meinen Stolz gebrochen,", "tokens": ["All\u00b7m\u00e4cht'\u00b7ger", ",", "der", "du", "mei\u00b7nen", "Stolz", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Errette mich aus dieser Todesnot;", "tokens": ["Er\u00b7ret\u00b7te", "mich", "aus", "die\u00b7ser", "To\u00b7des\u00b7not", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "La\u00df der Verleumdung Gift mich nicht erreichen!", "tokens": ["La\u00df", "der", "Ver\u00b7leum\u00b7dung", "Gift", "mich", "nicht", "er\u00b7rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich wei\u00df nicht mehr, wo aus noch ein \u2013", "tokens": ["Ich", "wei\u00df", "nicht", "mehr", ",", "wo", "aus", "noch", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "APPR", "ADV", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df Sorgen meine Haare bleichen,", "tokens": ["La\u00df", "Sor\u00b7gen", "mei\u00b7ne", "Haa\u00b7re", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch la\u00df mein Herz nicht trostlos sein!", "tokens": ["Doch", "la\u00df", "mein", "Herz", "nicht", "trost\u00b7los", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn du zu neuen Schmerzen mich erkoren,", "tokens": ["Wenn", "du", "zu", "neu\u00b7en", "Schmer\u00b7zen", "mich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu meinem Heile mich erniedrigt hast,", "tokens": ["Zu", "mei\u00b7nem", "Hei\u00b7le", "mich", "er\u00b7nied\u00b7rigt", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur jetzt sei gn\u00e4dig, mehre nicht die Last \u2013", "tokens": ["Nur", "jetzt", "sei", "gn\u00e4\u00b7dig", ",", "meh\u00b7re", "nicht", "die", "Last", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJD", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch ", "tokens": ["Noch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich kann, wenn tausend Pfeile mich durchbohren,", "tokens": ["Ich", "kann", ",", "wenn", "tau\u00b7send", "Pfei\u00b7le", "mich", "durch\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "CARD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Genesen; doch ich brauche kurze Rast.", "tokens": ["Ge\u00b7ne\u00b7sen", ";", "doch", "ich", "brau\u00b7che", "kur\u00b7ze", "Rast", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O s\u00fc\u00dfe Ruhe, wie verlang' ich dein!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Ru\u00b7he", ",", "wie", "ver\u00b7lang'", "ich", "dein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Was du gef\u00fcgt, Allweiser, das geschehe;", "tokens": ["Was", "du", "ge\u00b7f\u00fcgt", ",", "All\u00b7wei\u00b7ser", ",", "das", "ge\u00b7sche\u00b7he", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nur g\u00f6nne mir die Frist, um die ich flehe,", "tokens": ["Nur", "g\u00f6n\u00b7ne", "mir", "die", "Frist", ",", "um", "die", "ich", "fle\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "La\u00df mich noch einmal gl\u00fccklich sein!", "tokens": ["La\u00df", "mich", "noch", "ein\u00b7mal", "gl\u00fcck\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Kreuz des S\u00fcdens leuchtet \u00fcberm Meer;", "tokens": ["Das", "Kreuz", "des", "S\u00fc\u00b7dens", "leuch\u00b7tet", "\u00fc\u00b7berm", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fern in der Heimat schlummern, die mich lieben;", "tokens": ["Fern", "in", "der", "Hei\u00b7mat", "schlum\u00b7mern", ",", "die", "mich", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Herz ist mir geblieben,", "tokens": ["Ihr", "Herz", "ist", "mir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihr Gebet ist meine beste Wehr.", "tokens": ["Und", "ihr", "Ge\u00b7bet", "ist", "mei\u00b7ne", "bes\u00b7te", "Wehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Willst du es nicht erh\u00f6ren?", "tokens": ["Willst", "du", "es", "nicht", "er\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Soll mein Verderben auch das ihre sein?", "tokens": ["Soll", "mein", "Ver\u00b7der\u00b7ben", "auch", "das", "ih\u00b7re", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "ART", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O la\u00df die Unschuld den Orkan beschw\u00f6ren,", "tokens": ["O", "la\u00df", "die", "Un\u00b7schuld", "den", "Or\u00b7kan", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Allg\u00fct'ger, \u2013 ich vermag es nicht allein.", "tokens": ["All\u00b7g\u00fct'\u00b7ger", ",", "\u2013", "ich", "ver\u00b7mag", "es", "nicht", "al\u00b7lein", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}