{"textgrid.poem.43051": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es fragen mich die Menschen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fragen mich die Menschen,", "tokens": ["Es", "fra\u00b7gen", "mich", "die", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was mich so elend gemacht;", "tokens": ["Was", "mich", "so", "e\u00b7lend", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich sag' euch, ich habe mein Elend", "tokens": ["Ich", "sag'", "euch", ",", "ich", "ha\u00b7be", "mein", "E\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit auf die Welt gebracht.", "tokens": ["Mit", "auf", "die", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es liegt in meinem F\u00fchlen", "tokens": ["Es", "liegt", "in", "mei\u00b7nem", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dem halbentfesselten Geist,", "tokens": ["In", "dem", "hal\u00b7bent\u00b7fes\u00b7sel\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der aufw\u00e4rts will und der Alles", "tokens": ["Der", "auf\u00b7w\u00e4rts", "will", "und", "der", "Al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VMFIN", "KON", "ART", "PIS"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zur Erde doch wieder rei\u00dft.", "tokens": ["Zur", "Er\u00b7de", "doch", "wie\u00b7der", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}