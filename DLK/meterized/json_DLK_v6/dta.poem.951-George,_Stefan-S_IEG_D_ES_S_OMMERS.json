{"dta.poem.951": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Den blauen raden und dem blutigen mohne               ", "tokens": ["Den", "blau\u00b7en", "ra\u00b7den", "und", "dem", "blu\u00b7ti\u00b7gen", "moh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entgeht dem lispelnden und lichten korn               ", "tokens": ["Ent\u00b7geht", "dem", "lis\u00b7peln\u00b7den", "und", "lich\u00b7ten", "korn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Durchwandert diese waldung sinnens ohne               ", "tokens": ["Durch\u00b7wan\u00b7dert", "die\u00b7se", "wal\u00b7dung", "sin\u00b7nens", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeden vielverschlungnen pfad von vorn             ", "tokens": ["Und", "je\u00b7den", "viel\u00b7ver\u00b7schlung\u00b7nen", "pfad", "von", "vorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verharrt nicht vor den zeichen in den birken               ", "tokens": ["Ver\u00b7harrt", "nicht", "vor", "den", "zei\u00b7chen", "in", "den", "bir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschwunden sei die hand die einst sie schnitt               ", "tokens": ["Ge\u00b7schwun\u00b7den", "sei", "die", "hand", "die", "einst", "sie", "schnitt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun f&#252;hlt wie andre namen wunder wirken               ", "tokens": ["Nun", "f", "&#252;", "hlt", "wie", "and\u00b7re", "na\u00b7men", "wun\u00b7der", "wir\u00b7ken"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$(", "VVFIN", "KOKOM", "ADJA", "NN", "NN", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Zu jungen frischen st&#228;mmen lenkt den schritt             ", "tokens": ["Zu", "jun\u00b7gen", "fri\u00b7schen", "st", "&#228;", "mmen", "lenkt", "den", "schritt"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$(", "VVINF", "VVFIN", "ART", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vergesst der schmerzen und des alten blutes               ", "tokens": ["Ver\u00b7gesst", "der", "schmer\u00b7zen", "und", "des", "al\u00b7ten", "blu\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "KON", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gerissen am verfallnen dorngestr&#228;uch               ", "tokens": ["Ge\u00b7ris\u00b7sen", "am", "ver\u00b7fall\u00b7nen", "dorn\u00b7gestr", "&#228;", "uch"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bl&#228;tter d&#252;rrer zeiten leichten mutes               ", "tokens": ["Und", "bl", "&#228;", "tter", "d", "&#252;", "rrer", "zei\u00b7ten", "leich\u00b7ten", "mu\u00b7tes"], "token_info": ["word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["KON", "NE", "$(", "XY", "XY", "$(", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Betretet sie und lasst sie hinter euch!             ", "tokens": ["Be\u00b7tre\u00b7tet", "sie", "und", "lasst", "sie", "hin\u00b7ter", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}