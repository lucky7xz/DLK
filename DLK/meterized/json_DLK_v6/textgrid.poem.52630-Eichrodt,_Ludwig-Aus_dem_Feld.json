{"textgrid.poem.52630": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Feld", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ab die Helme! weg die Schwerter!", "tokens": ["Ab", "die", "Hel\u00b7me", "!", "weg", "die", "Schwer\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werfet euch ins Gras!", "tokens": ["Wer\u00b7fet", "euch", "ins", "Gras", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Weicher ruht kein Eheg\u00e4rter \u2013", "tokens": ["Wei\u00b7cher", "ruht", "kein", "E\u00b7he\u00b7g\u00e4r\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt nur Wein ins Glas!", "tokens": ["Jetzt", "nur", "Wein", "ins", "Glas", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Marketendrin, schenke ein!", "tokens": ["Mar\u00b7ke\u00b7ten\u00b7drin", ",", "schen\u00b7ke", "ein", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Marketendrin, hast du Wein?", "tokens": ["Mar\u00b7ke\u00b7ten\u00b7drin", ",", "hast", "du", "Wein", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Lustig wollen wir sein.", "tokens": ["Lus\u00b7tig", "wol\u00b7len", "wir", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Freunde trinkt, der letzte Batzen", "tokens": ["Freun\u00b7de", "trinkt", ",", "der", "letz\u00b7te", "Bat\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll hinunter heut!", "tokens": ["Soll", "hin\u00b7un\u00b7ter", "heut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aus, vorbei sind die Strapatzen,", "tokens": ["Aus", ",", "vor\u00b7bei", "sind", "die", "Stra\u00b7pat\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Feind zerstreut.", "tokens": ["Und", "der", "Feind", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Spielleut, stellt euch in die Reihn,", "tokens": ["Spiel\u00b7leut", ",", "stellt", "euch", "in", "die", "Reihn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht Musik und schenkt euch ein,", "tokens": ["Macht", "Mu\u00b7sik", "und", "schenkt", "euch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trommler wirble drein!", "tokens": ["Tromm\u00b7ler", "wirb\u00b7le", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Heisa lustig Lagerleben", "tokens": ["Hei\u00b7sa", "lus\u00b7tig", "La\u00b7ger\u00b7le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der bittern Schlacht!", "tokens": ["Nach", "der", "bit\u00b7tern", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Gott hat uns den Sieg gegeben,", "tokens": ["Gott", "hat", "uns", "den", "Sieg", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er hats brav gemacht.", "tokens": ["Er", "hats", "brav", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Herrlich ist, nach Noth und Pein", "tokens": ["Herr\u00b7lich", "ist", ",", "nach", "Noth", "und", "Pein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auszuruhn im Sonnenschein,", "tokens": ["Aus\u00b7zu\u00b7ruhn", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wieder lustig sein.", "tokens": ["Wie\u00b7der", "lus\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hoch soll unser Feldherr leben,", "tokens": ["Hoch", "soll", "un\u00b7ser", "Feld\u00b7herr", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er ist unser Mann!", "tokens": ["Er", "ist", "un\u00b7ser", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Liebt die Schlacht, das Weib, die Reben,", "tokens": ["Liebt", "die", "Schlacht", ",", "das", "Weib", ",", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er Alles kann.", "tokens": ["Weil", "er", "Al\u00b7les", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Marketendrin, schenk doch ein!", "tokens": ["Mar\u00b7ke\u00b7ten\u00b7drin", ",", "schenk", "doch", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Marketendrin, bessern Wein,", "tokens": ["Mar\u00b7ke\u00b7ten\u00b7drin", ",", "bes\u00b7sern", "Wein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Wenn wir Vivat schrein!", "tokens": ["Wenn", "wir", "Vi\u00b7vat", "schrein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Unsre Br\u00fcder sollen leben,", "tokens": ["Uns\u00b7re", "Br\u00fc\u00b7der", "sol\u00b7len", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verscharrt im Sand,", "tokens": ["Die", "ver\u00b7scharrt", "im", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wir, und selbst der Feind daneben,", "tokens": ["Wir", ",", "und", "selbst", "der", "Feind", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KON", "ADV", "ART", "NN", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der uns wacker stand!", "tokens": ["Der", "uns", "wa\u00b7cker", "stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Hurra heisa schenket ein!", "tokens": ["Hur\u00b7ra", "hei\u00b7sa", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vaterland, auf dein Gedeihn!", "tokens": ["Va\u00b7ter\u00b7land", ",", "auf", "dein", "Ge\u00b7deihn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Marketendrin, Wein!", "tokens": ["Mar\u00b7ke\u00b7ten\u00b7drin", ",", "Wein", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+---+", "measure": "dactylic.init"}}}}}