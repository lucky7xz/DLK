{"textgrid.poem.41327": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An Euphem", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich schilt ein Staar, ein Papagei:", "tokens": ["Dich", "schilt", "ein", "Staar", ",", "ein", "Pa\u00b7pa\u00b7gei", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das h\u00f6rst du mit gerechtem Lachen,", "tokens": ["Das", "h\u00f6rst", "du", "mit", "ge\u00b7rech\u00b7tem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dich wird auch ihr Lobgeschrei", "tokens": ["Denn", "dich", "wird", "auch", "ihr", "Lob\u00b7ge\u00b7schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht eitel, noch ber\u00fchmter machen.", "tokens": ["Nicht", "ei\u00b7tel", ",", "noch", "be\u00b7r\u00fchm\u00b7ter", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Sbrullus sprach j\u00fcngst wider dich,", "tokens": ["Nur", "Sbrul\u00b7lus", "sprach", "j\u00fcngst", "wi\u00b7der", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als er auch wider Gr\u00f6\u00dfre tobte.", "tokens": ["Als", "er", "auch", "wi\u00b7der", "Gr\u00f6\u00df\u00b7re", "tob\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist dieses dir so \u00e4rgerlich?", "tokens": ["Ist", "die\u00b7ses", "dir", "so", "\u00e4r\u00b7ger\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie? W\u00e4rst du stolz, wenn er dich lobte?", "tokens": ["Wie", "?", "W\u00e4rst", "du", "stolz", ",", "wenn", "er", "dich", "lob\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}