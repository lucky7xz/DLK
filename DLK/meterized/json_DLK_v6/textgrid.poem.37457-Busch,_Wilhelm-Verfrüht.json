{"textgrid.poem.37457": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Verfr\u00fcht", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Papa, nicht wahr,", "tokens": ["Pa\u00b7pa", ",", "nicht", "wahr", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im n\u00e4chsten Jahr,", "tokens": ["Im", "n\u00e4chs\u00b7ten", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wenn ich erst gro\u00df", "tokens": ["Wenn", "ich", "erst", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und lesen kann und schreiben kann,", "tokens": ["Und", "le\u00b7sen", "kann", "und", "schrei\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann krieg ich einen h\u00fcbschen Mann", "tokens": ["Dann", "krieg", "ich", "ei\u00b7nen", "h\u00fcb\u00b7schen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einer Ticktackuhr", "tokens": ["Mit", "ei\u00b7ner", "Tick\u00b7ta\u00b7ckuhr"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An einer goldnen Schnur.", "tokens": ["An", "ei\u00b7ner", "gold\u00b7nen", "Schnur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der nimmt mich auf den Scho\u00df", "tokens": ["Der", "nimmt", "mich", "auf", "den", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und sagt zu mir: Mein Engel,", "tokens": ["Und", "sagt", "zu", "mir", ":", "Mein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und gibt mir Zuckerkrengel", "tokens": ["Und", "gibt", "mir", "Zu\u00b7cker\u00b7kren\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und Kuchen und Pasteten.", "tokens": ["Und", "Ku\u00b7chen", "und", "Pas\u00b7te\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.12": {"text": "Nicht wahr, Papa?", "tokens": ["Nicht", "wahr", ",", "Pa\u00b7pa", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Der Vater brummt: Na, na,", "tokens": ["Der", "Va\u00b7ter", "brummt", ":", "Na", ",", "na", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ITJ", "$,", "ITJ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Was ist das f\u00fcr Gefabel.", "tokens": ["Was", "ist", "das", "f\u00fcr", "Ge\u00b7fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die V\u00f6gel, die dann fl\u00f6ten,", "tokens": ["Die", "V\u00f6\u00b7gel", ",", "die", "dann", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die haben noch keinen Schnabel.", "tokens": ["Die", "ha\u00b7ben", "noch", "kei\u00b7nen", "Schna\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}