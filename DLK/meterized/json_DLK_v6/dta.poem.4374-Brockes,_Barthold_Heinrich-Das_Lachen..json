{"dta.poem.4374": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Lachen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df GOtt, vor allen andern Thieren, auf Erden uns", "tokens": ["Da\u00df", "Gott", ",", "vor", "al\u00b7len", "an\u00b7dern", "Thie\u00b7ren", ",", "auf", "Er\u00b7den", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,", "APPR", "NN", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "vergn\u00fcgen wollen,", "tokens": ["ver\u00b7gn\u00fc\u00b7gen", "wol\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und da\u00df es eigentlich Sein Wille, da\u00df wir uns hier ver-", "tokens": ["Und", "da\u00df", "es", "ei\u00b7gent\u00b7lich", "Sein", "Wil\u00b7le", ",", "da\u00df", "wir", "uns", "hier", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "gn\u00fcgen sollen,", "tokens": ["gn\u00fc\u00b7gen", "sol\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Davon scheint in der That das Lachen ein deutlicher Be-", "tokens": ["Da\u00b7von", "scheint", "in", "der", "That", "das", "La\u00b7chen", "ein", "deut\u00b7li\u00b7cher", "Be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "weis zu seyn,", "tokens": ["weis", "zu", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKVZ", "PTKZU", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Als welches allen Thieren fehlt, es hat es blo\u00df der Mensch", "tokens": ["Als", "wel\u00b7ches", "al\u00b7len", "Thie\u00b7ren", "fehlt", ",", "es", "hat", "es", "blo\u00df", "der", "Mensch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELS", "PIAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "allein.", "tokens": ["al\u00b7lein", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Sprich nicht: Ja, ja, wir k\u00f6nnen lachen; wie aber wirds", "tokens": ["Sprich", "nicht", ":", "Ja", ",", "ja", ",", "wir", "k\u00f6n\u00b7nen", "la\u00b7chen", ";", "wie", "a\u00b7ber", "wirds"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "$.", "PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "VVINF", "$.", "PWAV", "ADV", "VAFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "ums Weinen stehen?", "tokens": ["ums", "Wei\u00b7nen", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Jm selben k\u00f6nnen wir nicht minder ein Merkmahl der", "tokens": ["Jm", "sel\u00b7ben", "k\u00f6n\u00b7nen", "wir", "nicht", "min\u00b7der", "ein", "Merk\u00b7mahl", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "VVINF", "VMFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-+-+++-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Betr\u00fcbni\u00df sehen.", "tokens": ["Be\u00b7tr\u00fcb\u00b7ni\u00df", "se\u00b7hen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Nein, wenn du es recht \u00fcberlegst; so kann das Weinen", "tokens": ["Nein", ",", "wenn", "du", "es", "recht", "\u00fc\u00b7ber\u00b7legst", ";", "so", "kann", "das", "Wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$.", "ADV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "die\u00df nicht hindern;", "tokens": ["die\u00df", "nicht", "hin\u00b7dern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Es dient nicht, unser Leid zu mehren; vielmehr dasselbige", "tokens": ["Es", "dient", "nicht", ",", "un\u00b7ser", "Leid", "zu", "meh\u00b7ren", ";", "viel\u00b7mehr", "das\u00b7sel\u00b7bi\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "ADV", "PDAT"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "zu lindern,", "tokens": ["zu", "lin\u00b7dern", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Wie uns ja die Erfahrung lehrt. Wenn ein Betr\u00fcbter", "tokens": ["Wie", "uns", "ja", "die", "Er\u00b7fah\u00b7rung", "lehrt", ".", "Wenn", "ein", "Be\u00b7tr\u00fcb\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "weinen kann;", "tokens": ["wei\u00b7nen", "kann", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "So greift der schwarze Gram sein Herz so heftig, wie vor-", "tokens": ["So", "greift", "der", "schwar\u00b7ze", "Gram", "sein", "Herz", "so", "hef\u00b7tig", ",", "wie", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "ADJD", "$,", "PWAV", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.20": {"text": "her, nicht an;", "tokens": ["her", ",", "nicht", "an", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Ja, Thr\u00e4nen k\u00f6nnen einen Trost in andern uns sogar er-", "tokens": ["Ja", ",", "Thr\u00e4\u00b7nen", "k\u00f6n\u00b7nen", "ei\u00b7nen", "Trost", "in", "an\u00b7dern", "uns", "so\u00b7gar", "er"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "VMFIN", "ART", "NN", "APPR", "ADJA", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "regen,", "tokens": ["re\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Und \u00f6fters andere zu Thr\u00e4nen, zum Mitleid, ja zur H\u00fclf\u2019,", "tokens": ["Und", "\u00f6f\u00b7ters", "an\u00b7de\u00b7re", "zu", "Thr\u00e4\u00b7nen", ",", "zum", "Mit\u00b7leid", ",", "ja", "zur", "H\u00fcl\u00b7f'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "APPR", "NN", "$,", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "bewegen;", "tokens": ["be\u00b7we\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Drum bleibt mein erster Lehr-Satz fest, zu welchem ich mich", "tokens": ["Drum", "bleibt", "mein", "ers\u00b7ter", "Lehr\u00b7Satz", "fest", ",", "zu", "wel\u00b7chem", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "PRF"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.26": {"text": "wieder lenke:", "tokens": ["wie\u00b7der", "len\u00b7ke", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.27": {"text": "Das Lachen selbst zeigt GOttes G\u00fcte, und ist ein g\u00f6ttli-", "tokens": ["Das", "La\u00b7chen", "selbst", "zeigt", "Got\u00b7tes", "G\u00fc\u00b7te", ",", "und", "ist", "ein", "g\u00f6tt\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "NN", "NN", "$,", "KON", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "ches Geschenke.", "tokens": ["ches", "Ge\u00b7schen\u00b7ke", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}}}}}