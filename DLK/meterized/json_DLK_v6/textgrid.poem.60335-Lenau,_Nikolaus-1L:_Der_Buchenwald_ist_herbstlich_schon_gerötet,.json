{"textgrid.poem.60335": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Buchenwald ist herbstlich schon ger\u00f6tet,", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Buchenwald ist herbstlich schon ger\u00f6tet,", "tokens": ["Der", "Bu\u00b7chen\u00b7wald", "ist", "herbst\u00b7lich", "schon", "ge\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wie ein Kranker, der sich neigt zum Sterben,", "tokens": ["So", "wie", "ein", "Kran\u00b7ker", ",", "der", "sich", "neigt", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn fl\u00fcchtig noch sich seine Wangen f\u00e4rben,", "tokens": ["Wenn", "fl\u00fcch\u00b7tig", "noch", "sich", "sei\u00b7ne", "Wan\u00b7gen", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch Rosen sinds, wobei kein Lied mehr fl\u00f6tet.", "tokens": ["Doch", "Ro\u00b7sen", "sinds", ",", "wo\u00b7bei", "kein", "Lied", "mehr", "fl\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "PWAV", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das B\u00e4chlein zieht und rieselt, kaum zu h\u00f6ren,", "tokens": ["Das", "B\u00e4ch\u00b7lein", "zieht", "und", "rie\u00b7selt", ",", "kaum", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Tal hinab, und seine Wellen gleiten,", "tokens": ["Das", "Tal", "hin\u00b7ab", ",", "und", "sei\u00b7ne", "Wel\u00b7len", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie durch das Sterbgemach die Freunde schreiten,", "tokens": ["Wie", "durch", "das", "Sterb\u00b7ge\u00b7mach", "die", "Freun\u00b7de", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den letzten Traum des Lebens nicht zu st\u00f6ren.", "tokens": ["Den", "letz\u00b7ten", "Traum", "des", "Le\u00b7bens", "nicht", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein tr\u00fcber Wandrer findet hier Genossen,", "tokens": ["Ein", "tr\u00fc\u00b7ber", "Wand\u00b7rer", "fin\u00b7det", "hier", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es ist Natur, der auch die Freuden schwanden,", "tokens": ["Es", "ist", "Na\u00b7tur", ",", "der", "auch", "die", "Freu\u00b7den", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit seiner ganzen Schwermut einverstanden,", "tokens": ["Mit", "sei\u00b7ner", "gan\u00b7zen", "Schwer\u00b7mut", "ein\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ist in ihre Klagen eingeschlossen.", "tokens": ["Es", "ist", "in", "ih\u00b7re", "Kla\u00b7gen", "ein\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}