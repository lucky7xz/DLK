{"textgrid.poem.42847": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Die Lupe bietet sich an", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will euch dienen,", "tokens": ["Ich", "will", "euch", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Will euer Auge sein,", "tokens": ["Will", "eu\u00b7er", "Au\u00b7ge", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ihr im Allzuklein", "tokens": ["Wenn", "ihr", "im", "A\u00b7llzu\u00b7klein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Suchet wie Bienen.", "tokens": ["Su\u00b7chet", "wie", "Bie\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich deute euch jederzeit", "tokens": ["Ich", "deu\u00b7te", "euch", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Falsches und Wahres,", "tokens": ["Fal\u00b7sches", "und", "Wah\u00b7res", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und Wunderbares", "tokens": ["Und", "Wun\u00b7der\u00b7ba\u00b7res"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der bunten Winzigkeit.", "tokens": ["Der", "bun\u00b7ten", "Win\u00b7zig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die spiegelt geheimnisvoll", "tokens": ["Die", "spie\u00b7gelt", "ge\u00b7heim\u00b7nis\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das gro\u00dfe Treiben. \u2013", "tokens": ["Das", "gro\u00b7\u00dfe", "Trei\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und im kleinsten Winkel soll", "tokens": ["Und", "im", "kleins\u00b7ten", "Win\u00b7kel", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kein Schmutz bei euch bleiben.", "tokens": ["Kein", "Schmutz", "bei", "euch", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich kann, aber will nicht gern", "tokens": ["Ich", "kann", ",", "a\u00b7ber", "will", "nicht", "gern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "ADV", "VMFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch L\u00f6cher brennen.", "tokens": ["Euch", "L\u00f6\u00b7cher", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Haltet mir Blendlicht fern!", "tokens": ["Hal\u00b7tet", "mir", "Blend\u00b7licht", "fern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ihr sollt mich kennen.", "tokens": ["Ihr", "sollt", "mich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ihr sollt mich durchschaun,", "tokens": ["Ihr", "sollt", "mich", "durch\u00b7schaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie ich die Spitzb\u00fcbchen,", "tokens": ["Wie", "ich", "die", "Spitz\u00b7b\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,"], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sollt ganz mir vertraun,", "tokens": ["Sollt", "ganz", "mir", "ver\u00b7traun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Eurem konvexen Lins'chen L\u00fcpchen.", "tokens": ["Eu\u00b7rem", "kon\u00b7ve\u00b7xen", "Lins'\u00b7chen", "L\u00fcpc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}}}}}