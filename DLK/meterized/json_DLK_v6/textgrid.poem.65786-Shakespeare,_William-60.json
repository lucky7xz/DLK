{"textgrid.poem.65786": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "60", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Wellen an des Ufers Kieseln bersten,", "tokens": ["Wie", "Wel\u00b7len", "an", "des", "U\u00b7fers", "Kie\u00b7seln", "bers\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So eilen unsre Stunden an ihr Ziel:", "tokens": ["So", "ei\u00b7len", "uns\u00b7re", "Stun\u00b7den", "an", "ihr", "Ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die folgende tritt an den Platz der ersten;", "tokens": ["Die", "fol\u00b7gen\u00b7de", "tritt", "an", "den", "Platz", "der", "ers\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vor, immer vorw\u00e4rts dr\u00e4ngt sie das Gew\u00fchl.", "tokens": ["Vor", ",", "im\u00b7mer", "vor\u00b7w\u00e4rts", "dr\u00e4ngt", "sie", "das", "Ge\u00b7w\u00fchl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geburt, einmal zu Tag entlassen, kriecht", "tokens": ["Ge\u00b7burt", ",", "ein\u00b7mal", "zu", "Tag", "ent\u00b7las\u00b7sen", ",", "kriecht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Reife zu; ", "tokens": ["Der", "Rei\u00b7fe", "zu", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Gekr\u00fcmmte Finsternisse schon ihr Licht,", "tokens": ["Ge\u00b7kr\u00fcmm\u00b7te", "Fins\u00b7ter\u00b7nis\u00b7se", "schon", "ihr", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Zeit verw\u00fcstet selbst, was sie gegeben.", "tokens": ["Und", "Zeit", "ver\u00b7w\u00fcs\u00b7tet", "selbst", ",", "was", "sie", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zeit unterw\u00fchlet Jugendflores Spur,", "tokens": ["Zeit", "un\u00b7ter\u00b7w\u00fch\u00b7let", "Ju\u00b7gend\u00b7flo\u00b7res", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Gr\u00e4bt in der Sch\u00f6nheit Stirnen tiefe Zeilen,", "tokens": ["Gr\u00e4bt", "in", "der", "Sch\u00f6n\u00b7heit", "Stir\u00b7nen", "tie\u00b7fe", "Zei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "N\u00e4hrt sich von allem Seltnen der Natur;", "tokens": ["N\u00e4hrt", "sich", "von", "al\u00b7lem", "Selt\u00b7nen", "der", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nichts stehet, das nicht fiel vor ihren Pfeilen.", "tokens": ["Nichts", "ste\u00b7het", ",", "das", "nicht", "fiel", "vor", "ih\u00b7ren", "Pfei\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und dennoch dauert, deinem Ruhm geweiht,", "tokens": ["Und", "den\u00b7noch", "dau\u00b7ert", ",", "dei\u00b7nem", "Ruhm", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mein Lied trotz ihrer Wut, durch alle Zeit.", "tokens": ["Mein", "Lied", "trotz", "ih\u00b7rer", "Wut", ",", "durch", "al\u00b7le", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}