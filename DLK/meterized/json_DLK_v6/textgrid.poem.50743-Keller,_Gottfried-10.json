{"textgrid.poem.50743": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "10", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist denn wohl der Diamant entstanden", "tokens": ["Wie", "ist", "denn", "wohl", "der", "Di\u00b7a\u00b7mant", "ent\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu seiner unverg\u00e4nglich festgeschlo\u00dfnen Einheit,", "tokens": ["Zu", "sei\u00b7ner", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "fest\u00b7ge\u00b7schlo\u00df\u00b7nen", "Ein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu seiner ungetr\u00fcbten, strahlenhellen Reinheit,", "tokens": ["Zu", "sei\u00b7ner", "un\u00b7ge\u00b7tr\u00fcb\u00b7ten", ",", "strah\u00b7len\u00b7hel\u00b7len", "Rein\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verkn\u00fcpft von so viel unsichtbaren Banden?", "tokens": ["Ver\u00b7kn\u00fcpft", "von", "so", "viel", "un\u00b7sicht\u00b7ba\u00b7ren", "Ban\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn aus der V\u00f6lker Schwellen und Versanden", "tokens": ["Wenn", "aus", "der", "V\u00f6l\u00b7ker", "Schwel\u00b7len", "und", "Ver\u00b7san\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Neues sich zu einem Ganzen einreiht,", "tokens": ["Ein", "Neu\u00b7es", "sich", "zu", "ei\u00b7nem", "Gan\u00b7zen", "ein\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Lieb und Bed\u00fcrfnis es zum Volke einweiht,", "tokens": ["Lieb", "und", "Be\u00b7d\u00fcrf\u00b7nis", "es", "zum", "Vol\u00b7ke", "ein\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wo Gleichgesinnte eine Heimat fanden:", "tokens": ["Wo", "Gleich\u00b7ge\u00b7sinn\u00b7te", "ei\u00b7ne", "Hei\u00b7mat", "fan\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer will denn da noch r\u00fctteln gar und feilen?", "tokens": ["Wer", "will", "denn", "da", "noch", "r\u00fct\u00b7teln", "gar", "und", "fei\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ADV", "VVFIN", "ADV", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu sp\u00e4t! zu sp\u00e4t! schon ist's ein Diamant,", "tokens": ["Zu", "sp\u00e4t", "!", "zu", "sp\u00e4t", "!", "schon", "ist's", "ein", "Di\u00b7a\u00b7mant", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$.", "PTKA", "ADJD", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der nicht mehr ist zu tr\u00fcben und zu teilen!", "tokens": ["Der", "nicht", "mehr", "ist", "zu", "tr\u00fc\u00b7ben", "und", "zu", "tei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VAFIN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn, wie man im Edelstein erkannt,", "tokens": ["Und", "wenn", ",", "wie", "man", "im", "E\u00b7del\u00b7stein", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWAV", "PIS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Darin noch kleine, fremde K\u00f6rper weilen,", "tokens": ["Da\u00b7rin", "noch", "klei\u00b7ne", ",", "frem\u00b7de", "K\u00f6r\u00b7per", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So sind sie fest umgossen und gebannt.", "tokens": ["So", "sind", "sie", "fest", "um\u00b7gos\u00b7sen", "und", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}