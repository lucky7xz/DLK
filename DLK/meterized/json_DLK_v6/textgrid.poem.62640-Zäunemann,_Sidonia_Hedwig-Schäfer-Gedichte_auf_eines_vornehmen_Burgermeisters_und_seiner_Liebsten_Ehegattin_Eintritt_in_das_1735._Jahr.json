{"textgrid.poem.62640": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4fer-Gedichte auf eines vornehmen Burgermeisters und seiner Liebsten Ehegattin Eintritt in das 1735. Jahr", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menalcas, den das Gl\u00fcck aus seiner Vaters-Stadt", "tokens": ["Me\u00b7nal\u00b7cas", ",", "den", "das", "Gl\u00fcck", "aus", "sei\u00b7ner", "Va\u00b7ter\u00b7sStadt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "In eine andre Flur und Feld gerufen hat,", "tokens": ["In", "ei\u00b7ne", "and\u00b7re", "Flur", "und", "Feld", "ge\u00b7ru\u00b7fen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Woselbst er jetzt sein Brod in sichrer Ruh verzehret,", "tokens": ["Wo\u00b7selbst", "er", "jetzt", "sein", "Brod", "in", "sich\u00b7rer", "Ruh", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sich durch Flei\u00df und Kunst in seiner H\u00fctte nehret.", "tokens": ["Und", "sich", "durch", "Flei\u00df", "und", "Kunst", "in", "sei\u00b7ner", "H\u00fct\u00b7te", "neh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sa\u00df einst auf einem Tag darinnen sehr erfreut,", "tokens": ["Sa\u00df", "einst", "auf", "ei\u00b7nem", "Tag", "da\u00b7rin\u00b7nen", "sehr", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und schm\u00fcckte sich dabey nach ietzger Jahres-Zeit", "tokens": ["Und", "schm\u00fcck\u00b7te", "sich", "da\u00b7bey", "nach", "ietz\u00b7ger", "Jah\u00b7res\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit Bl\u00fcthen, Blumen, Laub und buntgef\u00e4rbten B\u00e4ndern.", "tokens": ["Mit", "Bl\u00fc\u00b7then", ",", "Blu\u00b7men", ",", "Laub", "und", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7ten", "B\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er suchte auch die Lust in etwas zu ver\u00e4ndern:", "tokens": ["Er", "such\u00b7te", "auch", "die", "Lust", "in", "et\u00b7was", "zu", "ver\u00b7\u00e4n\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er nahm sein Haber-Rohr und sang ein Hirten-Lied,", "tokens": ["Er", "nahm", "sein", "Ha\u00b7ber\u00b7Rohr", "und", "sang", "ein", "Hir\u00b7ten\u00b7Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Worunter dieses war: Seht! mein Vergn\u00fcgen bl\u00fcht!", "tokens": ["Wo\u00b7run\u00b7ter", "die\u00b7ses", "war", ":", "Seht", "!", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "bl\u00fcht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VAFIN", "$.", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Philis trat zu ihm und unterbrach sein Singen:", "tokens": ["Die", "Phi\u00b7lis", "trat", "zu", "ihm", "und", "un\u00b7ter\u00b7brach", "sein", "Sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sie sprach: was treibet dich zu diesen Wunder-Dingen?", "tokens": ["Sie", "sprach", ":", "was", "trei\u00b7bet", "dich", "zu", "die\u00b7sen", "Wun\u00b7der\u00b7Din\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du hast dich ausgeschm\u00fcckt, du hebst die Hand empor,", "tokens": ["Du", "hast", "dich", "aus\u00b7ge\u00b7schm\u00fcckt", ",", "du", "hebst", "die", "Hand", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und singst in Einsamkeit vergn\u00fcgt durch Mund und Rohr?", "tokens": ["Und", "singst", "in", "Ein\u00b7sam\u00b7keit", "ver\u00b7gn\u00fcgt", "durch", "Mund", "und", "Rohr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wer putzt sich, wenn es schneyt? Wer freut sich, wenn es frieret,", "tokens": ["Wer", "putzt", "sich", ",", "wenn", "es", "schneyt", "?", "Wer", "freut", "sich", ",", "wenn", "es", "frie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$.", "PWS", "VVFIN", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn Wiesen Feld und Wald ein weises Kleid bezieret?", "tokens": ["Wenn", "Wie\u00b7sen", "Feld", "und", "Wald", "ein", "wei\u00b7ses", "Kleid", "be\u00b7zie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Kein Sch\u00e4fer nimmt den Stab und gehet auf das Feld,", "tokens": ["Kein", "Sch\u00e4\u00b7fer", "nimmt", "den", "Stab", "und", "ge\u00b7het", "auf", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und weydet da sein Vieh. Die H\u00fctte ihm gef\u00e4llt;", "tokens": ["Und", "wey\u00b7det", "da", "sein", "Vieh", ".", "Die", "H\u00fct\u00b7te", "ihm", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da pfleget er sein Lamm, und w\u00e4rmt sich bey dem Lichte,", "tokens": ["Da", "pfle\u00b7get", "er", "sein", "Lamm", ",", "und", "w\u00e4rmt", "sich", "bey", "dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und zeigt nicht, wie im May ein aufgekl\u00e4rt Gesichte.", "tokens": ["Und", "zeigt", "nicht", ",", "wie", "im", "May", "ein", "auf\u00b7ge\u00b7kl\u00e4rt", "Ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "APPRART", "NN", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich aber sehe dich besonders wohl vergn\u00fcgt:", "tokens": ["Ich", "a\u00b7ber", "se\u00b7he", "dich", "be\u00b7son\u00b7ders", "wohl", "ver\u00b7gn\u00fcgt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Drum bitt ich, sage mir, was dir im Herzen liegt;", "tokens": ["Drum", "bitt", "ich", ",", "sa\u00b7ge", "mir", ",", "was", "dir", "im", "Her\u00b7zen", "liegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Damit ich ferner nicht dein Thun, wie jetzt besch\u00e4me,", "tokens": ["Da\u00b7mit", "ich", "fer\u00b7ner", "nicht", "dein", "Thun", ",", "wie", "jetzt", "be\u00b7sch\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "$,", "PWAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und einen gleichen Theil an deiner Freude nehme.", "tokens": ["Und", "ei\u00b7nen", "glei\u00b7chen", "Theil", "an", "dei\u00b7ner", "Freu\u00b7de", "neh\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Menalcas l\u00e4chelte, und sah die Philis an,", "tokens": ["Me\u00b7nal\u00b7cas", "l\u00e4\u00b7chel\u00b7te", ",", "und", "sah", "die", "Phi\u00b7lis", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KON", "VVFIN", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und sprach: O Sch\u00e4ferin! gedenkest du nicht dran,", "tokens": ["Und", "sprach", ":", "O", "Sch\u00e4\u00b7fe\u00b7rin", "!", "ge\u00b7den\u00b7kest", "du", "nicht", "dran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df heut ein Oberhirt in unserm Feld und Fluren,", "tokens": ["Da\u00df", "heut", "ein", "O\u00b7berh\u00b7irt", "in", "un\u00b7serm", "Feld", "und", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dem wir Gehorsam, Pflicht und treue Dienste schwuren,", "tokens": ["Dem", "wir", "Ge\u00b7hor\u00b7sam", ",", "Pflicht", "und", "treu\u00b7e", "Diens\u00b7te", "schwu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "$,", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das Neue Jahr betrit. Sie feyren diese Stunden", "tokens": ["Das", "Neu\u00b7e", "Jahr", "be\u00b7trit", ".", "Sie", "fey\u00b7ren", "die\u00b7se", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mit ungewohnter Lust: Drum halt ich mich verbunden", "tokens": ["Mit", "un\u00b7ge\u00b7wohn\u00b7ter", "Lust", ":", "Drum", "halt", "ich", "mich", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PAV", "VVFIN", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Heut aufgeweckt zu seyn. Menalcas fuhre fort,", "tokens": ["Heut", "auf\u00b7ge\u00b7weckt", "zu", "seyn", ".", "Me\u00b7nal\u00b7cas", "fuh\u00b7re", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "VAINF", "$.", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und sprach: O Philis! Ach! mir fehlet Geist und Wort", "tokens": ["Und", "sprach", ":", "O", "Phi\u00b7lis", "!", "Ach", "!", "mir", "feh\u00b7let", "Geist", "und", "Wort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "NE", "NE", "$.", "ITJ", "$.", "PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die grosse Freudigkeit der Seelen auszudr\u00fccken.", "tokens": ["Die", "gros\u00b7se", "Freu\u00b7dig\u00b7keit", "der", "See\u00b7len", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Du weist so wohl als ich, mit was vor holden Blicken", "tokens": ["Du", "weist", "so", "wohl", "als", "ich", ",", "mit", "was", "vor", "hol\u00b7den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KOUS", "PPER", "$,", "APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und dieses ", "tokens": ["Und", "die\u00b7ses"], "token_info": ["word", "word"], "pos": ["KON", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Und der es auch bisher so wohl in acht genommen;", "tokens": ["Und", "der", "es", "auch", "bis\u00b7her", "so", "wohl", "in", "acht", "ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ADV", "ADV", "ADV", "APPR", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Da\u00df auch kein Sch\u00e4fer nicht, nur um ein Lamm gekommen.", "tokens": ["Da\u00df", "auch", "kein", "Sch\u00e4\u00b7fer", "nicht", ",", "nur", "um", "ein", "Lamm", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "PTKNEG", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Sie treiben unter ihm vergn\u00fcgt das Vieh ins Feld,", "tokens": ["Sie", "trei\u00b7ben", "un\u00b7ter", "ihm", "ver\u00b7gn\u00fcgt", "das", "Vieh", "ins", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und ihre H\u00fctten sind besch\u00fctzt und wohl bestellt.", "tokens": ["Und", "ih\u00b7re", "H\u00fct\u00b7ten", "sind", "be\u00b7sch\u00fctzt", "und", "wohl", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn andres Sch\u00e4fer-Volk von harten Diensten saget;", "tokens": ["Wenn", "and\u00b7res", "Sch\u00e4\u00b7fer\u00b7Volk", "von", "har\u00b7ten", "Diens\u00b7ten", "sa\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So wissen wir von nichts; wir leben unverzaget.", "tokens": ["So", "wis\u00b7sen", "wir", "von", "nichts", ";", "wir", "le\u00b7ben", "un\u00b7ver\u00b7za\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIS", "$.", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Drum hat ihm auch der Herr der Sterne hochbegl\u00fcckt,", "tokens": ["Drum", "hat", "ihm", "auch", "der", "Herr", "der", "Ster\u00b7ne", "hoch\u00b7be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und ihm ein Silber-Haar mit Ehren zugeschickt,", "tokens": ["Und", "ihm", "ein", "Sil\u00b7ber\u00b7Haar", "mit", "Eh\u00b7ren", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Er giebt ihm St\u00e4rk und Kraft. Gott hat ihm auch darneben", "tokens": ["Er", "giebt", "ihm", "St\u00e4rk", "und", "Kraft", ".", "Gott", "hat", "ihm", "auch", "dar\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "NN", "VAFIN", "PPER", "ADV", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "In dreyen Ehen Fried und Einigkeit gegeben.", "tokens": ["In", "drey\u00b7en", "E\u00b7hen", "Fried", "und", "Ei\u00b7nig\u00b7keit", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Nichts minder zeitlich Gut. Und dieses wendet er", "tokens": ["Nichts", "min\u00b7der", "zeit\u00b7lich", "Gut", ".", "Und", "die\u00b7ses", "wen\u00b7det", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADJD", "ADJD", "$.", "KON", "PDS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Zum Dienst des N\u00e4chsten an. Nein! nicht von ohngefehr", "tokens": ["Zum", "Dienst", "des", "N\u00e4chs\u00b7ten", "an", ".", "Nein", "!", "nicht", "von", "ohn\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$.", "PTKANT", "$.", "PTKNEG", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Hat mich des Schicksals Macht an diesen Ort gesetzet,", "tokens": ["Hat", "mich", "des", "Schick\u00b7sals", "Macht", "an", "die\u00b7sen", "Ort", "ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Allwo mich ", "tokens": ["All\u00b7wo", "mich"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.50": {"text": "O Philis! schau mit mir die Tag und Stunden an,", "tokens": ["O", "Phi\u00b7lis", "!", "schau", "mit", "mir", "die", "Tag", "und", "Stun\u00b7den", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ADJD", "APPR", "PPER", "ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Da uns ", "tokens": ["Da", "uns"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.52": {"text": "Die wir doch warlich nicht verschuldet und verdienet.", "tokens": ["Die", "wir", "doch", "war\u00b7lich", "nicht", "ver\u00b7schul\u00b7det", "und", "ver\u00b7die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "PTKNEG", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ja, durch ", "tokens": ["Ja", ",", "durch"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$,", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.54": {"text": "Wenn nun ", "tokens": ["Wenn", "nun"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.55": {"text": "So kans nicht anders seyn, ich sing ein Freuden-Lied.", "tokens": ["So", "kans", "nicht", "an\u00b7ders", "seyn", ",", "ich", "sing", "ein", "Freu\u00b7den\u00b7Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PTKNEG", "ADV", "VAINF", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun hab ich dir gesagt, was mich zur Lust beweget/", "tokens": ["Nun", "hab", "ich", "dir", "ge\u00b7sagt", ",", "was", "mich", "zur", "Lust", "be\u00b7we\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und 