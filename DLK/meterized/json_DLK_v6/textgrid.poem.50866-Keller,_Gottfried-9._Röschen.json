{"textgrid.poem.50866": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9. R\u00f6schen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6schen bi\u00df den Apfel an,", "tokens": ["R\u00f6\u00b7schen", "bi\u00df", "den", "Ap\u00b7fel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, zu ihrem Schrecken,", "tokens": ["Und", ",", "zu", "ih\u00b7rem", "Schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blieb ein perlengleicher Zahn", "tokens": ["Blieb", "ein", "per\u00b7len\u00b7glei\u00b7cher", "Zahn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In demselben stecken.", "tokens": ["In", "dem\u00b7sel\u00b7ben", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und das gute Kind verga\u00df", "tokens": ["Und", "das", "gu\u00b7te", "Kind", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Morgenlieder!", "tokens": ["Ih\u00b7re", "Mor\u00b7gen\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4nen ohne Unterla\u00df", "tokens": ["Tr\u00e4\u00b7nen", "oh\u00b7ne", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4ufelten ihr nieder!", "tokens": ["Tr\u00e4u\u00b7fel\u00b7ten", "ihr", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}