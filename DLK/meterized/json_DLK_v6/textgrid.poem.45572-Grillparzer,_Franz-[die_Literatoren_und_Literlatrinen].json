{"textgrid.poem.45572": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Literatoren und Literlatrinen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Literatoren und Literlatrinen", "tokens": ["Die", "Li\u00b7te\u00b7ra\u00b7to\u00b7ren", "und", "Li\u00b7ter\u00b7lat\u00b7ri\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sind nicht \u00fcbel zu plaudern mit ihnen;", "tokens": ["Sind", "nicht", "\u00fc\u00b7bel", "zu", "plau\u00b7dern", "mit", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "APPR", "PPER", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Doch sei nicht zu offen, ihr Maul ist nicht sicher,", "tokens": ["Doch", "sei", "nicht", "zu", "of\u00b7fen", ",", "ihr", "Maul", "ist", "nicht", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "PTKA", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Auch leih ihnen niemals Geld oder B\u00fccher.", "tokens": ["Auch", "leih", "ih\u00b7nen", "nie\u00b7mals", "Geld", "o\u00b7der", "B\u00fc\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}