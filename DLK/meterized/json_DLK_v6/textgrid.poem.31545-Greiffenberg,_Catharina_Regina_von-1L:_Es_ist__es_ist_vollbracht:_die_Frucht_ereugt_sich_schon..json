{"textgrid.poem.31545": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist / es ist vollbracht: die Frucht ereugt sich schon.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist / es ist vollbracht: die Frucht ereugt sich schon.", "tokens": ["Es", "ist", "/", "es", "ist", "voll\u00b7bracht", ":", "die", "Frucht", "e\u00b7reugt", "sich", "schon", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VAFIN", "VVPP", "$.", "ART", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Herzen-Diamant ist durch sein Blut erweichet.", "tokens": ["Der", "Her\u00b7zen\u00b7Di\u00b7a\u00b7mant", "ist", "durch", "sein", "Blut", "er\u00b7wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Heilig Geist-Magnet die eisnen Sinnen streichet.", "tokens": ["Der", "Hei\u00b7lig", "Geist\u00b7Ma\u00b7gnet", "die", "eis\u00b7nen", "Sin\u00b7nen", "strei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es ritzet neue Reu die Stachel-Dornen-Kron.", "tokens": ["Es", "rit\u00b7zet", "neu\u00b7e", "Reu", "die", "Sta\u00b7chel\u00b7Dor\u00b7nen\u00b7Kron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sicht ihn hier am Creutz / und glaubt Ihm dort im Thron", "tokens": ["Man", "sicht", "ihn", "hier", "am", "Creutz", "/", "und", "glaubt", "Ihm", "dort", "im", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$(", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der hohen G\u00f6ttlichkeit / da\u00df er dem Vatter gleichet /", "tokens": ["der", "ho\u00b7hen", "G\u00f6tt\u00b7lich\u00b7keit", "/", "da\u00df", "er", "dem", "Vat\u00b7ter", "glei\u00b7chet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er sey der Ewig Gott / da Er doch todts verbleichet /", "tokens": ["Er", "sey", "der", "E\u00b7wig", "Gott", "/", "da", "Er", "doch", "todts", "ver\u00b7blei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "siht einem W\u00fcrmlein gleich; man nennt ihn Gottes Sohn.", "tokens": ["siht", "ei\u00b7nem", "W\u00fcrm\u00b7lein", "gleich", ";", "man", "nennt", "ihn", "Got\u00b7tes", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$.", "PIS", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O seltner Wunder-Glaub / den Fluch vor Segen sch\u00e4tzen /", "tokens": ["O", "selt\u00b7ner", "Wun\u00b7der\u00b7Glaub", "/", "den", "Fluch", "vor", "Se\u00b7gen", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "zum selbst-verlassenen die Zuflucht nehmen noch /", "tokens": ["zum", "selbst\u00b7ver\u00b7las\u00b7se\u00b7nen", "die", "Zu\u00b7flucht", "neh\u00b7men", "noch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "sein Leben auf den Tod / Trost auf Trostlose / setzen /", "tokens": ["sein", "Le\u00b7ben", "auf", "den", "Tod", "/", "Trost", "auf", "Trost\u00b7lo\u00b7se", "/", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$(", "NN", "APPR", "NN", "$(", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "und hoffen H\u00fclff' von dem / der unter seinem Joch", "tokens": ["und", "hof\u00b7fen", "H\u00fclff'", "von", "dem", "/", "der", "un\u00b7ter", "sei\u00b7nem", "Joch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "$(", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "schier selbst verschmachtet ist. So hat es Gott gefallen:", "tokens": ["schier", "selbst", "ver\u00b7schmach\u00b7tet", "ist", ".", "So", "hat", "es", "Gott", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$.", "ADV", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "das alles Er erlitt' / und uns erl\u00f6st von allen.", "tokens": ["das", "al\u00b7les", "Er", "er\u00b7litt'", "/", "und", "uns", "er\u00b7l\u00f6st", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "VVFIN", "$(", "KON", "PPER", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}