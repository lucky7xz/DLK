{"textgrid.poem.56149": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenige ihr, der einstigen Kindheit Gespielen", "tokens": ["We\u00b7ni\u00b7ge", "ihr", ",", "der", "eins\u00b7ti\u00b7gen", "Kind\u00b7heit", "Ge\u00b7spie\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "$,", "ART", "ADJA", "NN", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "in den zerstreuten G\u00e4rten der Stadt:", "tokens": ["in", "den", "zer\u00b7streu\u00b7ten", "G\u00e4r\u00b7ten", "der", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "wie wir uns fanden und uns z\u00f6gernd gefielen", "tokens": ["wie", "wir", "uns", "fan\u00b7den", "und", "uns", "z\u00f6\u00b7gernd", "ge\u00b7fie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "KON", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und, wie das Lamm mit dem redenden Blatt,", "tokens": ["und", ",", "wie", "das", "Lamm", "mit", "dem", "re\u00b7den\u00b7den", "Blatt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "sprachen als Schweigende. Wenn wir uns einmal freuten,", "tokens": ["spra\u00b7chen", "als", "Schwei\u00b7gen\u00b7de", ".", "Wenn", "wir", "uns", "ein\u00b7mal", "freu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "NN", "$.", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+--+---+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "keinem geh\u00f6rte es. Wessen wars?", "tokens": ["kei\u00b7nem", "ge\u00b7h\u00f6r\u00b7te", "es", ".", "Wes\u00b7sen", "wars", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$.", "NN", "VAFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und wie zergings unter allen den gehenden Leuten", "tokens": ["Und", "wie", "zer\u00b7gings", "un\u00b7ter", "al\u00b7len", "den", "ge\u00b7hen\u00b7den", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und im Bangen des langen Jahrs.", "tokens": ["und", "im", "Ban\u00b7gen", "des", "lan\u00b7gen", "Jahrs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wagen umrollten uns fremd, vor\u00fcbergezogen,", "tokens": ["Wa\u00b7gen", "um\u00b7roll\u00b7ten", "uns", "fremd", ",", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,", "VVPP", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "H\u00e4user umstanden uns stark, aber unwahr, \u2013 und keines", "tokens": ["H\u00e4u\u00b7ser", "um\u00b7stan\u00b7den", "uns", "stark", ",", "a\u00b7ber", "un\u00b7wahr", ",", "\u2013", "und", "kei\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,", "ADV", "ADJD", "$,", "$(", "KON", "PIS"], "meter": "+--+--++--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "kannte uns je. ", "tokens": ["kann\u00b7te", "uns", "je", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Nichts. Nur die B\u00e4lle. Ihre herrlichen Bogen.", "tokens": ["Nichts", ".", "Nur", "die", "B\u00e4l\u00b7le", ".", "Ih\u00b7re", "herr\u00b7li\u00b7chen", "Bo\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "ADV", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auch nicht die Kinder... Aber manchmal trat eines,", "tokens": ["Auch", "nicht", "die", "Kin\u00b7der", "...", "A\u00b7ber", "manch\u00b7mal", "trat", "ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$(", "KON", "ADV", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ach ein vergehendes, unter den fallenden Ball.", "tokens": ["ach", "ein", "ver\u00b7ge\u00b7hen\u00b7des", ",", "un\u00b7ter", "den", "fal\u00b7len\u00b7den", "Ball", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}