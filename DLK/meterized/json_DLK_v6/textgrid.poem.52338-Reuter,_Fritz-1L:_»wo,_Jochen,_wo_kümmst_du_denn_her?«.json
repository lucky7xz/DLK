{"textgrid.poem.52338": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwo, Jochen, wo k\u00fcmmst du denn her?\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "nl:0.14", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwo, Jochen, wo k\u00fcmmst du denn her?\u00ab", "tokens": ["\u00bb", "wo", ",", "Jo\u00b7chen", ",", "wo", "k\u00fcmmst", "du", "denn", "her", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "$,", "NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbas ick? Ick was tau Stadt herin.\u00ab", "tokens": ["\u00bb", "as", "ick", "?", "Ick", "was", "tau", "Stadt", "he\u00b7rin", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "$.", "NN", "PWS", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00bbwat? Haddst du wat Besonners v\u00f6r?\u00ab", "tokens": ["\u00bb", "wat", "?", "Haddst", "du", "wat", "Be\u00b7son\u00b7ners", "v\u00f6r", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "$.", "VAFIN", "PPER", "VVFIN", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbdat eben nich. Denn s\u00fch, ick b\u00fcn", "tokens": ["\u00bb", "dat", "e\u00b7ben", "nich", ".", "Denn", "s\u00fch", ",", "ick", "b\u00fcn"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "ADV", "PTKNEG", "$.", "KON", "ADJD", "$,", "XY", "XY"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man blot einmal herinnerreden,", "tokens": ["Man", "blot", "ein\u00b7mal", "he\u00b7rin\u00b7ner\u00b7re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00dcm mi mal ens tau verlustieren,", "tokens": ["\u00dcm", "mi", "mal", "ens", "tau", "ver\u00b7lus\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Denn mine Nahwersl\u00fcd, de s\u00e4den,", "tokens": ["Denn", "mi\u00b7ne", "Nah\u00b7wer\u00b7sl\u00fcd", ",", "de", "s\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dat dor jetzt Kunstenmakers wiren.\u00ab", "tokens": ["Dat", "dor", "jetzt", "Kuns\u00b7ten\u00b7ma\u00b7kers", "wi\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbwo so? Wat maken de v\u00f6r Kunst?\u00ab", "tokens": ["\u00bb", "wo", "so", "?", "Wat", "ma\u00b7ken", "de", "v\u00f6r", "Kunst", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADV", "$.", "NN", "NE", "NE", "NE", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "\u00bbje, wat weit ick? Is't blagen Dunst,", "tokens": ["\u00bb", "je", ",", "wat", "weit", "ick", "?", "Is't", "bla\u00b7gen", "Dunst", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PRELS", "ADJD", "PPER", "$.", "NE", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Is't Fixigkeit in ehre H\u00e4n'n?\u00ab", "tokens": ["Is't", "Fi\u00b7xig\u00b7keit", "in", "eh\u00b7re", "H\u00e4n'n", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "\u00bbna, segg doch mal, wat makten s' denn?", "tokens": ["\u00bb", "na", ",", "segg", "doch", "mal", ",", "wat", "mak\u00b7ten", "s'", "denn", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "ADV", "ADV", "$,", "PWS", "VVFIN", "NE", "KON", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Dat was woll blot man Ogen tau verblen'n?\u00ab", "tokens": ["Dat", "was", "woll", "blot", "man", "O\u00b7gen", "tau", "ver\u00b7blen'n", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u00bbje h\u00fcr mal, Brauder Krischan, wenn", "tokens": ["\u00bb", "je", "h\u00fcr", "mal", ",", "Brau\u00b7der", "Krisc\u00b7han", ",", "wenn"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "ADV", "ADJD", "ADV", "$,", "ADJA", "NN", "$,", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ick di dat all so s\u00fcll vertellen,", "tokens": ["Ick", "di", "dat", "all", "so", "s\u00fcll", "ver\u00b7tel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "PIAT", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Du w\u00fcrd'st f\u00f6r'n Dummerjan mi schellen.", "tokens": ["Du", "w\u00fcrd'st", "f\u00f6r'n", "Dum\u00b7mer\u00b7jan", "mi", "schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Gef\u00e4hrlich was't, dat's wohr! Gef\u00e4hrlich was't,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "was't", ",", "dat's", "wohr", "!", "Ge\u00b7f\u00e4hr\u00b7lich", "was't", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "PRELS", "ADJD", "$.", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wat sei dor allens maken deden.", "tokens": ["Wat", "sei", "dor", "al\u00b7lens", "ma\u00b7ken", "de\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ick heww ehr eklich up de Fingern pa\u00dft,", "tokens": ["Ick", "heww", "ehr", "ek\u00b7lich", "up", "de", "Fin\u00b7gern", "pa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Doch kunn ick nicks nich an ehr sp\u00f6ren.", "tokens": ["Doch", "kunn", "ick", "nicks", "nich", "an", "ehr", "sp\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Des Middags reden s' d\u00f6rch de Strat", "tokens": ["Des", "Mid\u00b7dags", "re\u00b7den", "s'", "d\u00f6rch", "de", "Strat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.22": {"text": "Un makten einen groten Prat,", "tokens": ["Un", "mak\u00b7ten", "ei\u00b7nen", "gro\u00b7ten", "Prat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wat sei des Abends wullen maken.", "tokens": ["Wat", "sei", "des", "A\u00b7bends", "wul\u00b7len", "ma\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "In'n Haut, dor wull'n sei Eierkauken backen,", "tokens": ["In'n", "Haut", ",", "dor", "wull'n", "sei", "Ei\u00b7er\u00b7kau\u00b7ken", "ba\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVINF", "VAFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Band spucken wull'n sei, F\u00fcer freten,", "tokens": ["Band", "spu\u00b7cken", "wull'n", "sei", ",", "F\u00fcer", "fre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "VAFIN", "$,", "NN", "VVFIN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Sei wull'n de K\u00f6pp sick runner sniden laten", "tokens": ["Sei", "wull'n", "de", "K\u00f6pp", "sick", "run\u00b7ner", "sni\u00b7den", "la\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Un sei sick wedder ankurieren", "tokens": ["Un", "sei", "sick", "wed\u00b7der", "an\u00b7ku\u00b7rie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Un denn dormit her\u00fcmspazieren.", "tokens": ["Un", "denn", "dor\u00b7mit", "he\u00b7r\u00fcm\u00b7spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Sei wullen, ahn em antaufaten,", "tokens": ["Sei", "wul\u00b7len", ",", "ahn", "em", "an\u00b7tau\u00b7fa\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Den Aben pitschen ut de D\u00f6r", "tokens": ["Den", "A\u00b7ben", "pit\u00b7schen", "ut", "de", "D\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Un wull'n den D\u00fcwel danzen laten.", "tokens": ["Un", "wull'n", "den", "D\u00fc\u00b7wel", "dan\u00b7zen", "la\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Un wenn dit all gescheihn, nahher,", "tokens": ["Un", "wenn", "dit", "all", "ge\u00b7scheihn", ",", "nah\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KOUS", "NE", "PIAT", "VVPP", "$,", "ADJA", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Denn wull'n sei wisen uns en Jung'n", "tokens": ["Denn", "wull'n", "sei", "wi\u00b7sen", "uns", "en", "Jung'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Von einen Hahn un ein Karninken. \u2013", "tokens": ["Von", "ei\u00b7nen", "Hahn", "un", "ein", "Kar\u00b7nin\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "FM", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Na, as dat St\u00fcck denn nu anfung'n,", "tokens": ["Na", ",", "as", "dat", "St\u00fcck", "denn", "nu", "an\u00b7fung'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "ART", "NN", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Dunn ward ick denn mit Hanne Wienken", "tokens": ["Dunn", "ward", "ick", "denn", "mit", "Han\u00b7ne", "Wi\u00b7en\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Ok richtig nah den Kram herinner d\u00e4sen.", "tokens": ["Ok", "rich\u00b7tig", "nah", "den", "Kram", "he\u00b7rin\u00b7ner", "d\u00e4\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJD", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Gotts Dunner! Ne! Wat was dat dor f\u00f6r Wesen!", "tokens": ["Gotts", "Dun\u00b7ner", "!", "Ne", "!", "Wat", "was", "dat", "dor", "f\u00f6r", "We\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ART", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "De eine Kirl, de makt denn so'ne St\u00fccker!", "tokens": ["De", "ei\u00b7ne", "Kirl", ",", "de", "makt", "denn", "so'\u00b7ne", "St\u00fc\u00b7cker", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "FM", "FM", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "En Husd\u00f6rnsl\u00e4tel un en Stuwendr\u00fccker,", "tokens": ["En", "Hus\u00b7d\u00f6rns\u00b7l\u00e4\u00b7tel", "un", "en", "Stu\u00b7wen\u00b7dr\u00fc\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "De putzt hei weg, as wir't ne gele W\u00f6rtel.\u00ab", "tokens": ["De", "putzt", "hei", "weg", ",", "as", "wir't", "ne", "ge\u00b7le", "W\u00f6r\u00b7tel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "PTKVZ", "$,", "FM", "FM", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "\u00bbih, Jochen, 't is jo doch am En'n", "tokens": ["\u00bb", "ih", ",", "Jo\u00b7chen", ",", "'t", "is", "jo", "doch", "am", "En'n"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "$,", "NE", "$,", "FM", "FM", "NE", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.43": {"text": "Nicks wider as blot Ogenblen'n.\u00ab", "tokens": ["Nicks", "wi\u00b7der", "as", "blot", "O\u00b7gen\u00b7blen'", "n.", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "\u00bbdu wardst dat h\u00fcren. Lat man sin!", "tokens": ["\u00bb", "du", "wardst", "dat", "h\u00fc\u00b7ren", ".", "Lat", "man", "sin", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$.", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "In sinen Haut, as in 'ne Sch\u00f6rtel,", "tokens": ["In", "si\u00b7nen", "Haut", ",", "as", "in", "'ne", "Sch\u00f6r\u00b7tel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.46": {"text": "Slog hei 'ne Mandel Eier rin.", "tokens": ["Slog", "hei", "'ne", "Man\u00b7del", "Ei\u00b7er", "rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Makt F\u00fcer unnen an geswinn", "tokens": ["Makt", "F\u00fcer", "un\u00b7nen", "an", "ges\u00b7winn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJA", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Un backt dorin, as ick all seggt,", "tokens": ["Un", "backt", "do\u00b7rin", ",", "as", "ick", "all", "seggt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PRELS", "PPER", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "En sch\u00f6nen Eierkauken t'recht.", "tokens": ["En", "sch\u00f6\u00b7nen", "Ei\u00b7er\u00b7kau\u00b7k