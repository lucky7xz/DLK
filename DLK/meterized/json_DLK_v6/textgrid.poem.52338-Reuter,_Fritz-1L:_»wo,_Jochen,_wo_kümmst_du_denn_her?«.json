ra.chol"}, "line.116": {"text": "\u203aich gab heut middag mir die Ehre,", "tokens": ["\u203a", "ich", "gab", "heut", "mid\u00b7dag", "mir", "die", "Eh\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.117": {"text": "Ein sch\u00f6nes St\u00fcck Sie zu versprechen,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "St\u00fcck", "Sie", "zu", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.118": {"text": "Jetzt mu\u00df mein Wort ich leider brechen \u2013", "tokens": ["Jetzt", "mu\u00df", "mein", "Wort", "ich", "lei\u00b7der", "bre\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.119": {"text": "Das Junge von Karninken un von Hahn", "tokens": ["Das", "Jun\u00b7ge", "von", "Kar\u00b7nin\u00b7ken", "un", "von", "Hahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "FM", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.120": {"text": "Is leider mich mit Dod afgahn.", "tokens": ["Is", "lei\u00b7der", "mich", "mit", "Dod", "af\u00b7gahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.121": {"text": "Doch sollen Sie zu kort nich schie\u00dfen,", "tokens": ["Doch", "sol\u00b7len", "Sie", "zu", "kort", "nich", "schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.122": {"text": "Ich will Sie gleich was anners wisen.", "tokens": ["Ich", "will", "Sie", "gleich", "was", "an\u00b7ners", "wi\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PWS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.123": {"text": "Ich will daf\u00fcr die beiden \u00d6llern zeigen,", "tokens": ["Ich", "will", "da\u00b7f\u00fcr", "die", "bei\u00b7den", "\u00d6l\u00b7lern", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.124": {"text": "Die soll'n Sie gleich zu sehen kreigen.\u2039", "tokens": ["Die", "soll'n", "Sie", "gleich", "zu", "se\u00b7hen", "krei\u00b7gen", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.125": {"text": "Un dormit wis't hei mi un Hanne Wienken", "tokens": ["Un", "dor\u00b7mit", "wis't", "hei", "mi", "un", "Han\u00b7ne", "Wi\u00b7en\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NE", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.126": {"text": "En sch\u00f6nen Hahn un en Karninken!", "tokens": ["En", "sch\u00f6\u00b7nen", "Hahn", "un", "en", "Kar\u00b7nin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "FM", "FM", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.127": {"text": "En wittes, lewiges Karninken!", "tokens": ["En", "wit\u00b7tes", ",", "le\u00b7wi\u00b7ges", "Kar\u00b7nin\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.128": {"text": "Un einen bunten, sch\u00f6nen Hahn!", "tokens": ["Un", "ei\u00b7nen", "bun\u00b7ten", ",", "sch\u00f6\u00b7nen", "Hahn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.129": {"text": "Kann dat woll tau mit rechten Dingen gahn?\u00ab", "tokens": ["Kann", "dat", "woll", "tau", "mit", "rech\u00b7ten", "Din\u00b7gen", "gahn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "ADV", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.130": {"text": "\u00bbblot Ogenblen'n, Brauder Jochen!\u00ab", "tokens": ["\u00bb", "blot", "O\u00b7gen\u00b7blen'n", ",", "Brau\u00b7der", "Jo\u00b7chen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "NE", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.131": {"text": "\u00bbwo is dat m\u00e4glich! Heww ick denn kein Ogen?", "tokens": ["\u00bb", "wo", "is", "dat", "m\u00e4g\u00b7lich", "!", "Heww", "ick", "denn", "kein", "O\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "FM", "ART", "ADJD", "$.", "NE", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.132": {"text": "Ick ward doch en Karninken kenn'n!\u00ab", "tokens": ["Ick", "ward", "doch", "en", "Kar\u00b7nin\u00b7ken", "kenn'n", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.133": {"text": "\u00bbdat schad't nich! Is doch Ogen tau verblen'n!\u00ab", "tokens": ["\u00bb", "dat", "schad't", "nich", "!", "Is", "doch", "O\u00b7gen", "tau", "ver\u00b7blen'n", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VVFIN", "PTKNEG", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}