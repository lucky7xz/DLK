{"textgrid.poem.24932": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Strohhalm", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwo blieb er, da\u00df er so pl\u00f6tzlich verschwand?", "tokens": ["\u00bb", "wo", "blieb", "er", ",", "da\u00df", "er", "so", "pl\u00f6tz\u00b7lich", "ver\u00b7schwand", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wir m\u00fcssen ihn suchen, den H\u00f6llenbrand;", "tokens": ["Wir", "m\u00fcs\u00b7sen", "ihn", "su\u00b7chen", ",", "den", "H\u00f6l\u00b7len\u00b7brand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Denn solch ein Hauptmann, beim Teufel, ist rar!\u00ab", "tokens": ["Denn", "solch", "ein", "Haupt\u00b7mann", ",", "beim", "Teu\u00b7fel", ",", "ist", "rar", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "ART", "NN", "$,", "APPRART", "NN", "$,", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So schallt's durch den Haufen von Mansfelds Schar", "tokens": ["So", "schallt's", "durch", "den", "Hau\u00b7fen", "von", "Mans\u00b7felds", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Im Dickicht der d\u00fcsteren Tannen.", "tokens": ["Im", "Di\u00b7ckicht", "der", "d\u00fcs\u00b7te\u00b7ren", "Tan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der Hauptmann indes, von den Seinen verirrt,", "tokens": ["Der", "Haupt\u00b7mann", "in\u00b7des", ",", "von", "den", "Sei\u00b7nen", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "St\u00fcrmt, wo das Gebirg sich am wildesten wirrt,", "tokens": ["St\u00fcrmt", ",", "wo", "das", "Ge\u00b7birg", "sich", "am", "wil\u00b7des\u00b7ten", "wirrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Nachdem er die Kl\u00f6ster in Brand gesteckt", "tokens": ["Nach\u00b7dem", "er", "die", "Kl\u00f6s\u00b7ter", "in", "Brand", "ge\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und den Boden mit Tr\u00fcmmern und Leichen bedeckt,", "tokens": ["Und", "den", "Bo\u00b7den", "mit", "Tr\u00fcm\u00b7mern", "und", "Lei\u00b7chen", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "Auf brausendem Rosse von dannen.", "tokens": ["Auf", "brau\u00b7sen\u00b7dem", "Ros\u00b7se", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Es dunkelt; da unter dem Felsen sieht", "tokens": ["Es", "dun\u00b7kelt", ";", "da", "un\u00b7ter", "dem", "Fel\u00b7sen", "sieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er einen Siedler, der betend kniet.", "tokens": ["Er", "ei\u00b7nen", "Sied\u00b7ler", ",", "der", "be\u00b7tend", "kniet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbei, Bruder, gr\u00fc\u00dfe dich Lucifer,", "tokens": ["\u00bb", "ei", ",", "Bru\u00b7der", ",", "gr\u00fc\u00b7\u00dfe", "dich", "Lu\u00b7ci\u00b7fer", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VVFIN", "PRF", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und liebst du dein Leben, so la\u00df das Gepl\u00e4rr!", "tokens": ["Und", "liebst", "du", "dein", "Le\u00b7ben", ",", "so", "la\u00df", "das", "Ge\u00b7pl\u00e4rr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVIMP", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Hervor mit den Truhen und Kisten!\u00ab", "tokens": ["Her\u00b7vor", "mit", "den", "Tru\u00b7hen", "und", "Kis\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Doch der Siedler giebt Antwort: \u00bbVon Stroh ist mein Pf\u00fchl;", "tokens": ["Doch", "der", "Sied\u00b7ler", "giebt", "Ant\u00b7wort", ":", "\u00bb", "Von", "Stroh", "ist", "mein", "Pf\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$.", "$(", "APPR", "NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Trank bietet der Bach mir frisch und k\u00fchl;", "tokens": ["Trank", "bie\u00b7tet", "der", "Bach", "mir", "frisch", "und", "k\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als die bitteren Beeren vom Heidegestr\u00e4uch,", "tokens": ["Als", "die", "bit\u00b7te\u00b7ren", "Bee\u00b7ren", "vom", "Hei\u00b7de\u00b7ge\u00b7str\u00e4uch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Die sp\u00e4rlich das Leben mir fristen.\u00ab", "tokens": ["Die", "sp\u00e4r\u00b7lich", "das", "Le\u00b7ben", "mir", "fris\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Drauf jener: \u00bbSchicke, du Kuttenmann \u2013", "tokens": ["Drauf", "je\u00b7ner", ":", "\u00bb", "Schi\u00b7cke", ",", "du", "Kut\u00b7ten\u00b7mann", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PDAT", "$.", "$(", "NN", "$,", "PPER", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verderben euch allen \u2013 zum Sterben dich an!\u00ab", "tokens": ["Ver\u00b7der\u00b7ben", "euch", "al\u00b7len", "\u2013", "zum", "Ster\u00b7ben", "dich", "an", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PIAT", "$(", "APPRART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das Schwert erhebt er; doch, wie er auch droht,", "tokens": ["Das", "Schwert", "er\u00b7hebt", "er", ";", "doch", ",", "wie", "er", "auch", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADV", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht zittert der Siedler, da ihn der Tod", "tokens": ["Nicht", "zit\u00b7tert", "der", "Sied\u00b7ler", ",", "da", "ihn", "der", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Anblitzt von der funkelnden Schneide;", "tokens": ["An\u00b7blitzt", "von", "der", "fun\u00b7keln\u00b7den", "Schnei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zu singen beginnt er: \u00bbO Herr, geh' nicht,", "tokens": ["Zu", "sin\u00b7gen", "be\u00b7ginnt", "er", ":", "\u00bb", "O", "Herr", ",", "geh'", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "$.", "$(", "NE", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Mit deinem Knechte geh nicht ins Gericht!\u00ab", "tokens": ["Mit", "dei\u00b7nem", "Knech\u00b7te", "geh", "nicht", "ins", "Ge\u00b7richt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dann beut er dem t\u00f6dlichen Streich das Genick;", "tokens": ["Dann", "beut", "er", "dem", "t\u00f6d\u00b7li\u00b7chen", "Streich", "das", "Ge\u00b7nick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Doch der Hauptmann schl\u00e4gt zu Boden den Blick,", "tokens": ["Doch", "der", "Haupt\u00b7mann", "schl\u00e4gt", "zu", "Bo\u00b7den", "den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Und es sinkt ihm das Schwert in die Scheide.", "tokens": ["Und", "es", "sinkt", "ihm", "das", "Schwert", "in", "die", "Schei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbdas ist das Lied \u2013 wohl kenn' ich den Klang \u2013,", "tokens": ["\u00bb", "das", "ist", "das", "Lied", "\u2013", "wohl", "kenn'", "ich", "den", "Klang", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "ART", "NN", "$(", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das vor dem Sterben mein Vater sang,\u00ab", "tokens": ["Das", "vor", "dem", "Ster\u00b7ben", "mein", "Va\u00b7ter", "sang", ",", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "So murmelt er leise und starrt und sinnt,", "tokens": ["So", "mur\u00b7melt", "er", "lei\u00b7se", "und", "starrt", "und", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und der Siedler, der es gewahrt, beginnt", "tokens": ["Und", "der", "Sied\u00b7ler", ",", "der", "es", "ge\u00b7wahrt", ",", "be\u00b7ginnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den Frevler zur Bu\u00dfe zu mahnen;", "tokens": ["Den", "Frev\u00b7ler", "zur", "Bu\u00b7\u00dfe", "zu", "mah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Er redet von Gott, der dem S\u00fcnder vergiebt", "tokens": ["Er", "re\u00b7det", "von", "Gott", ",", "der", "dem", "S\u00fcn\u00b7der", "ver\u00b7giebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und den Sohn, den verlorenen, doppelt liebt;", "tokens": ["Und", "den", "Sohn", ",", "den", "ver\u00b7lo\u00b7re\u00b7nen", ",", "dop\u00b7pelt", "liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "ADJD", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Von dem Himmel der Gnade, der \u00fcber uns blaut", "tokens": ["Von", "dem", "Him\u00b7mel", "der", "Gna\u00b7de", ",", "der", "\u00fc\u00b7ber", "uns", "blaut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Und den Frost in dem eisigen Herzen taut,", "tokens": ["Und", "den", "Frost", "in", "dem", "ei\u00b7si\u00b7gen", "Her\u00b7zen", "taut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Noch eh' wir es denken und ahnen.", "tokens": ["Noch", "eh'", "wir", "es", "den\u00b7ken", "und", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbvergeben?\u00ab spricht jener \u2013 \u00bbo eitler Wahn!", "tokens": ["\u00bb", "ver\u00b7ge\u00b7ben", "?", "\u00ab", "spricht", "je\u00b7ner", "\u2013", "\u00bb", "o", "eit\u00b7ler", "Wahn", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "$(", "VVFIN", "PDAT", "$(", "$(", "FM", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schon seh' ich den Anklagengel nahn", "tokens": ["Schon", "seh'", "ich", "den", "An\u00b7kla\u00b7gen\u00b7gel", "nahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA"], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und zwischen mich und des Himmels Huld", "tokens": ["Und", "zwi\u00b7schen", "mich", "und", "des", "Him\u00b7mels", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "KON", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Berge, die ich geh\u00e4uft von Schuld,", "tokens": ["Die", "Ber\u00b7ge", ",", "die", "ich", "ge\u00b7h\u00e4uft", "von", "Schuld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als ewiges Bollwerk w\u00e4lzen.\u00ab", "tokens": ["Als", "e\u00b7wi\u00b7ges", "Boll\u00b7werk", "w\u00e4l\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Er spricht es und seufzt, doch der Siedler ruft:", "tokens": ["Er", "spricht", "es", "und", "seufzt", ",", "doch", "der", "Sied\u00b7ler", "ruft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$,", "ADV", "