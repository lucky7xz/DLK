 "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "In jede Seele", "tokens": ["In", "je\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit Blutschrift gebrannt:", "tokens": ["Mit", "Blut\u00b7schrift", "ge\u00b7brannt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.17": {"line.1": {"text": "Ueber die weiten M\u00e4rkte des Lebens", "tokens": ["Ue\u00b7ber", "die", "wei\u00b7ten", "M\u00e4rk\u00b7te", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Rollt unaufhaltsam,", "tokens": ["Rollt", "un\u00b7auf\u00b7halt\u00b7sam", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "N\u00e4chte und Tage,", "tokens": ["N\u00e4ch\u00b7te", "und", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ohne Labung und tr\u00f6stende Sonne", "tokens": ["Oh\u00b7ne", "La\u00b7bung", "und", "tr\u00f6s\u00b7ten\u00b7de", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Die Sklavenkolonne", "tokens": ["Die", "Skla\u00b7ven\u00b7ko\u00b7lon\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der verkauften Kreaturen, \u2013", "tokens": ["Der", "ver\u00b7kauf\u00b7ten", "Kre\u00b7a\u00b7tu\u00