b7tauft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Leider! \u2013 oh leider", "tokens": ["Lei\u00b7der", "!", "\u2013", "oh", "lei\u00b7der"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["ADV", "$.", "$(", "FM", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ist sie zu drei Viertel", "tokens": ["Ist", "sie", "zu", "drei", "Vier\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "CARD", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Auch schon gl\u00fccklich \u2013 ", "tokens": ["Auch", "schon", "gl\u00fcck\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$("], "meter": "+-+-", "measure": "trochaic.di"}}}}}