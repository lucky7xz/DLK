{"textgrid.poem.35465": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Verkauft", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht war mir zu Willen", "tokens": ["Nicht", "war", "mir", "zu", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Deine lebendige Seele!", "tokens": ["Dei\u00b7ne", "le\u00b7ben\u00b7di\u00b7ge", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und nicht umt\u00f6nte mich", "tokens": ["Und", "nicht", "um\u00b7t\u00f6n\u00b7te", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihrer tiefsinnigen Sprache", "tokens": ["Ih\u00b7rer", "tief\u00b7sin\u00b7ni\u00b7gen", "Spra\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ergreifender Urlaut ...", "tokens": ["Er\u00b7grei\u00b7fen\u00b7der", "Ur\u00b7laut", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch deinen Leib \u2013 doch deinen Leib", "tokens": ["Doch", "dei\u00b7nen", "Leib", "\u2013", "doch", "dei\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$(", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich besessen", "tokens": ["Hab'", "ich", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und deine Glieder", "tokens": ["Und", "dei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "K\u00fchnlich betastet \u2013", "tokens": ["K\u00fchn\u00b7lich", "be\u00b7tas\u00b7tet", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und meine Hand \u2013", "tokens": ["Und", "mei\u00b7ne", "Hand", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Meine hei\u00dfe irrende Hand \u2013", "tokens": ["Mei\u00b7ne", "hei\u00b7\u00dfe", "ir\u00b7ren\u00b7de", "Hand", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Fand Huld und Heimat", "tokens": ["Fand", "Huld", "und", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Im Tal deiner Br\u00fcste ...", "tokens": ["Im", "Tal", "dei\u00b7ner", "Br\u00fcs\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Und dein Leben sp\u00fcrt' ich \u2013", "tokens": ["Und", "dein", "Le\u00b7ben", "sp\u00fcrt'", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dein lebendiges Leben! ...", "tokens": ["Dein", "le\u00b7ben\u00b7di\u00b7ges", "Le\u00b7ben", "!", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Rhythmus des Blutes \u2013", "tokens": ["Den", "Rhyth\u00b7mus", "des", "Blu\u00b7tes", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von den Lippen dir sog ich", "tokens": ["Von", "den", "Lip\u00b7pen", "dir", "sog", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "PPER"], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.5": {"text": "Die Frucht seines Kreisens ...", "tokens": ["Die", "Frucht", "sei\u00b7nes", "Krei\u00b7sens", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und das Leben umfing ich \u2013", "tokens": ["Und", "das", "Le\u00b7ben", "um\u00b7fing", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Das lebendige Leben ...", "tokens": ["Das", "le\u00b7ben\u00b7di\u00b7ge", "Le\u00b7ben", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aber deine Seele war stumm,", "tokens": ["A\u00b7ber", "dei\u00b7ne", "See\u00b7le", "war", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wortlos dein Auge,", "tokens": ["Und", "wort\u00b7los", "dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Als ahnten sie kaum", "tokens": ["Als", "ahn\u00b7ten", "sie", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der Wonneschmerzen", "tokens": ["Der", "Won\u00b7ne\u00b7schmer\u00b7zen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Verschleierten Tiefgang \u2013", "tokens": ["Ver\u00b7schlei\u00b7er\u00b7ten", "Tief\u00b7gang", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Schmerzenswonnen,", "tokens": ["Die", "Schmer\u00b7zens\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die sich geb\u00e4ren,", "tokens": ["Die", "sich", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Flackernde Flammen,", "tokens": ["Fla\u00b7ckern\u00b7de", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Gibt sich dem Menschen", "tokens": ["Gibt", "sich", "dem", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Der g\u00f6ttliche Mensch", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7che", "Mensch"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Im Namen des Geistes,", "tokens": ["Im", "Na\u00b7men", "des", "Geis\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Der das Ewige f\u00fcgt", "tokens": ["Der", "das", "E\u00b7wi\u00b7ge", "f\u00fcgt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "VVFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Zum Gebilde der Stunde \u2013", "tokens": ["Zum", "Ge\u00b7bil\u00b7de", "der", "Stun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "All-einig Bewu\u00dftsein", "tokens": ["All\u00b7ei\u00b7nig", "Be\u00b7wu\u00df\u00b7tsein"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Zeugt und entfaltet,", "tokens": ["Zeugt", "und", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Ein pfadkundiger Tr\u00f6ster! ...", "tokens": ["Ein", "pfad\u00b7kun\u00b7di\u00b7ger", "Tr\u00f6s\u00b7ter", "!", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Aber deine Seele war stumm,", "tokens": ["A\u00b7ber", "dei\u00b7ne", "See\u00b7le", "war", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als deckte sie Schlummer \u2013", "tokens": ["Als", "deck\u00b7te", "sie", "Schlum\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Als tr\u00e4umte entr\u00fcckt sie", "tokens": ["Als", "tr\u00e4um\u00b7te", "ent\u00b7r\u00fcckt", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu anderen Sph\u00e4ren,", "tokens": ["Zu", "an\u00b7de\u00b7ren", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Nahsein den G\u00f6ttern", "tokens": ["Die", "Na\u00b7hsein", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Heiter gew\u00e4hren ...", "tokens": ["Hei\u00b7ter", "ge\u00b7w\u00e4h\u00b7ren", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Mich aber verwarf sie", "tokens": ["Mich", "a\u00b7ber", "ver\u00b7warf", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und meiner Seele", "tokens": ["Und", "mei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Br\u00fcnstiges Rufen ...", "tokens": ["Br\u00fcns\u00b7ti\u00b7ges", "Ru\u00b7fen", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Da quoll es empor \u2013", "tokens": ["Da", "quoll", "es", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und meine Sehnsucht,", "tokens": ["Und", "mei\u00b7ne", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die dich nicht beseelt,", "tokens": ["Die", "dich", "nicht", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wandelte trotzig", "tokens": ["Wan\u00b7del\u00b7te", "trot\u00b7zig"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Zu irdischer Lust sich", "tokens": ["Zu", "ir\u00b7di\u00b7scher", "Lust", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nach jener S\u00fcnde, \u2013", "tokens": ["Nach", "je\u00b7ner", "S\u00fcn\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die wurzelnd im Staube", "tokens": ["Die", "wur\u00b7zelnd", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Vom Staube sich s\u00e4ttigt ...", "tokens": ["Vom", "Stau\u00b7be", "sich", "s\u00e4t\u00b7tigt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Und mich zerfra\u00df", "tokens": ["Und", "mich", "zer\u00b7fra\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Flamme der Wollust \u2013", "tokens": ["Die", "Flam\u00b7me", "der", "Wol\u00b7lust", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und w\u00fchlte sich ein", "tokens": ["Und", "w\u00fchl\u00b7te", "sich", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Und f\u00fcllte mich ganz", "tokens": ["Und", "f\u00fcll\u00b7te", "mich", "ganz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und mordete meuchlings", "tokens": ["Und", "mor\u00b7de\u00b7te", "meuch\u00b7lings"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "All meine Gottheit! ...", "tokens": ["All", "mei\u00b7ne", "Got\u00b7theit", "!", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Und ich betastete dich \u2013", "tokens": ["Und", "ich", "be\u00b7tas\u00b7te\u00b7te", "dich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "In deine Glieder verkrampft \u2013", "tokens": ["In", "dei\u00b7ne", "Glie\u00b7der", "ver\u00b7krampft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Als sei ich von Sinnen \u2013", "tokens": ["Als", "sei", "ich", "von", "Sin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Als h\u00e4tte ich niemals", "tokens": ["Als", "h\u00e4t\u00b7te", "ich", "nie\u00b7mals"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Meiner Seele Freiheit", "tokens": ["Mei\u00b7ner", "See\u00b7le", "Frei\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Auch nur geahnt \u2013", "tokens": ["Auch", "nur", "ge\u00b7ahnt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Als h\u00e4tt' ich mich niemals", "tokens": ["Als", "h\u00e4tt'", "ich", "mich", "nie\u00b7mals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Voll feuriger Kraft", "tokens": ["Voll", "feu\u00b7ri\u00b7ger", "Kraft"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Zu den G\u00f6ttern entrafft!", "tokens": ["Zu", "den", "G\u00f6t\u00b7tern", "en\u00b7trafft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Durch mein Hirn", "tokens": ["Durch", "mein", "Hirn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Schossen die Str\u00f6me", "tokens": ["Schos\u00b7sen", "die", "Str\u00f6\u00b7me"], "token_info": ["word", "word", "