{"textgrid.poem.55836": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Schutzengel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist der Vogel, dessen Fl\u00fcgel kamen,", "tokens": ["Du", "bist", "der", "Vo\u00b7gel", ",", "des\u00b7sen", "Fl\u00fc\u00b7gel", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NE", "$,", "PRELAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wenn ich erwachte in der Nacht und rief.", "tokens": ["wenn", "ich", "er\u00b7wach\u00b7te", "in", "der", "Nacht", "und", "rief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur mit den Armen rief ich, denn dein Namen", "tokens": ["Nur", "mit", "den", "Ar\u00b7men", "rief", "ich", ",", "denn", "dein", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "ist wie ein Abgrund, tausend N\u00e4chte tief.", "tokens": ["ist", "wie", "ein", "Ab\u00b7grund", ",", "tau\u00b7send", "N\u00e4ch\u00b7te", "tief", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "CARD", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du bist der Schatten, drin ich still entschlief,", "tokens": ["Du", "bist", "der", "Schat\u00b7ten", ",", "drin", "ich", "still", "ent\u00b7schlief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und jeden Traum ersinnt in mir dein Samen, \u2013", "tokens": ["und", "je\u00b7den", "Traum", "er\u00b7sinnt", "in", "mir", "dein", "Sa\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "du bist das Bild, ich aber bin der Rahmen,", "tokens": ["du", "bist", "das", "Bild", ",", "ich", "a\u00b7ber", "bin", "der", "Rah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "der dich erg\u00e4nzt in gl\u00e4nzendem Relief.", "tokens": ["der", "dich", "er\u00b7g\u00e4nzt", "in", "gl\u00e4n\u00b7zen\u00b7dem", "Re\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie nenn ich dich? Sieh, meine Lippen lahmen.", "tokens": ["Wie", "nenn", "ich", "dich", "?", "Sieh", ",", "mei\u00b7ne", "Lip\u00b7pen", "lah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$.", "NE", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist der Anfang, der sich gro\u00df ergie\u00dft,", "tokens": ["Du", "bist", "der", "An\u00b7fang", ",", "der", "sich", "gro\u00df", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ich bin das langsame und bange Amen,", "tokens": ["ich", "bin", "das", "lang\u00b7sa\u00b7me", "und", "ban\u00b7ge", "A\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das deine Sch\u00f6nheit scheu beschlie\u00dft.", "tokens": ["das", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "scheu", "be\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast mich oft aus dunklem Ruhn gerissen,", "tokens": ["Du", "hast", "mich", "oft", "aus", "dunk\u00b7lem", "Ruhn", "ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wenn mir das Schlafen wie ein Grab erschien", "tokens": ["wenn", "mir", "das", "Schla\u00b7fen", "wie", "ein", "Grab", "er\u00b7schien"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und wie Verlorengehen und Entfliehn, \u2013", "tokens": ["und", "wie", "Ver\u00b7lo\u00b7ren\u00b7ge\u00b7hen", "und", "Ent\u00b7fliehn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "da hobst du mich aus Herzensfinsternissen", "tokens": ["da", "hobst", "du", "mich", "aus", "Her\u00b7zens\u00b7fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und wolltest mich auf allen T\u00fcrmen hissen", "tokens": ["und", "woll\u00b7test", "mich", "auf", "al\u00b7len", "T\u00fcr\u00b7men", "his\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PRF", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wie Scharlachfahnen und wie Draperien.", "tokens": ["wie", "Schar\u00b7lach\u00b7fah\u00b7nen", "und", "wie", "Dra\u00b7pe\u00b7ri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "PWAV", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Du: der von Wundern redet wie vom Wissen", "tokens": ["Du", ":", "der", "von", "Wun\u00b7dern", "re\u00b7det", "wie", "vom", "Wis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$.", "ART", "APPR", "NN", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und von den Menschen wie von Melodien", "tokens": ["und", "von", "den", "Men\u00b7schen", "wie", "von", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KOKOM", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und von den Rosen: von Ereignissen,", "tokens": ["und", "von", "den", "Ro\u00b7sen", ":", "von", "Er\u00b7eig\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$.", "APPR", "NN", "$,"], "meter": "-+-+--++--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die flammend sich in deinem Blick vollziehn, \u2013", "tokens": ["die", "flam\u00b7mend", "sich", "in", "dei\u00b7nem", "Blick", "voll\u00b7ziehn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "du Seliger, wann nennst du einmal Ihn,", "tokens": ["du", "Se\u00b7li\u00b7ger", ",", "wann", "nennst", "du", "ein\u00b7mal", "Ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "aus dessen siebentem und letztem Tage", "tokens": ["aus", "des\u00b7sen", "sie\u00b7ben\u00b7tem", "und", "letz\u00b7tem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "noch immer Glanz auf deinem Fl\u00fcgelschlage", "tokens": ["noch", "im\u00b7mer", "Glanz", "auf", "dei\u00b7nem", "Fl\u00fc\u00b7gel\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "verloren liegt...", "tokens": ["ver\u00b7lo\u00b7ren", "liegt", "..."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Befiehlst du, da\u00df ich frage?", "tokens": ["Be\u00b7fiehlst", "du", ",", "da\u00df", "ich", "fra\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}