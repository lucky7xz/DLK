{"textgrid.poem.31399": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es scheint des H\u00f6chsten Gnad / aus seinem Wunderschicken.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es scheint des H\u00f6chsten Gnad / aus seinem Wunderschicken.", "tokens": ["Es", "scheint", "des", "H\u00f6chs\u00b7ten", "Gnad", "/", "aus", "sei\u00b7nem", "Wun\u00b7der\u00b7schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wer solches recht betracht / wird hoch dadurch erfreut.", "tokens": ["wer", "sol\u00b7ches", "recht", "be\u00b7tracht", "/", "wird", "hoch", "da\u00b7durch", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVPP", "$(", "VAFIN", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Anla\u00df dieser Kunst / ist Creutz und Tr\u00fcbsal-Zeit.", "tokens": ["Der", "An\u00b7la\u00df", "die\u00b7ser", "Kunst", "/", "ist", "Creutz", "und", "Tr\u00fcb\u00b7sa\u00b7lZeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$(", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein unverd\u00f6rrtes Feld kein Regen darff erquicken.", "tokens": ["Ein", "un\u00b7ver\u00b7d\u00f6rr\u00b7tes", "Feld", "kein", "Re\u00b7gen", "darff", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Saul veranlast ward durch Esel Flucht entr\u00fccken /", "tokens": ["Der", "Saul", "ver\u00b7an\u00b7last", "ward", "durch", "E\u00b7sel", "Flucht", "ent\u00b7r\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "APPR", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df er zum Seher gieng' und kriegt die Herrlichkeit.", "tokens": ["da\u00df", "er", "zum", "Se\u00b7her", "gieng'", "und", "kriegt", "die", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "de\u00df Schenk- und Becken Traum hat Joseph auch bereit", "tokens": ["de\u00df", "Schenk", "und", "Be\u00b7cken", "Traum", "hat", "Jo\u00b7se\u00b7ph", "auch", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "NN", "VAFIN", "NE", "ADV", "ADJD"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "den Weg zur Herrschers-Macht / und Seine zu begl\u00fccken.", "tokens": ["den", "Weg", "zur", "Herr\u00b7scher\u00b7sMacht", "/", "und", "Sei\u00b7ne", "zu", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$(", "KON", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der jenig / so Gott f\u00fcrcht kan in der gr\u00f6sten Noht", "tokens": ["Der", "je\u00b7nig", "/", "so", "Gott", "f\u00fcrcht", "kan", "in", "der", "gr\u00f6s\u00b7ten", "Noht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "$(", "ADV", "NN", "VVFIN", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "durch unverhoffte wei\u00df verhofft errettet werden.", "tokens": ["durch", "un\u00b7ver\u00b7hoff\u00b7te", "wei\u00df", "ver\u00b7hofft", "er\u00b7ret\u00b7tet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es wird de\u00df Himmels thau / zu Frucht / Getraid und Wein.", "tokens": ["Es", "wird", "de\u00df", "Him\u00b7mels", "thau", "/", "zu", "Frucht", "/", "Ge\u00b7traid", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$(", "APPR", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Vngl\u00fcck wird zu Gl\u00fcck / zu Leben gar der Tod!", "tokens": ["Das", "Vn\u00b7gl\u00fcck", "wird", "zu", "Gl\u00fcck", "/", "zu", "Le\u00b7ben", "gar", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$(", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der Himmlisch K\u00fcnstler macht zu Freuden die beschwerden.", "tokens": ["der", "Himm\u00b7lisch", "K\u00fcnst\u00b7ler", "macht", "zu", "Freu\u00b7den", "die", "be\u00b7schwer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der Mensch ist Gottes Ziel / Gott alles guten Schrein.", "tokens": ["der", "Mensch", "ist", "Got\u00b7tes", "Ziel", "/", "Gott", "al\u00b7les", "gu\u00b7ten", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "NN", "$(", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}