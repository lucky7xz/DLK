{"textgrid.poem.30009": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nana zwar ist nicht gesehn, wer sie vornen sihet an;", "tokens": ["Na\u00b7na", "zwar", "ist", "nicht", "ge\u00b7sehn", ",", "wer", "sie", "vor\u00b7nen", "si\u00b7het", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PTKNEG", "VVPP", "$,", "PWS", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "R\u00fcckwerts ist sie hoch gesehn; dennoch kriegt sie keinen Mann.", "tokens": ["R\u00fcck\u00b7werts", "ist", "sie", "hoch", "ge\u00b7sehn", ";", "den\u00b7noch", "kriegt", "sie", "kei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}