{"textgrid.poem.48444": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15. An Herrn Johan Klipstein", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Orpheus jener Zeit auf Venus Klippen sunge", "tokens": ["Was", "Or\u00b7pheus", "je\u00b7ner", "Zeit", "auf", "Ve\u00b7nus", "Klip\u00b7pen", "sun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "PDAT", "NN", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und wie er durch den Witz die starken Saiten zwunge", "tokens": ["und", "wie", "er", "durch", "den", "Witz", "die", "star\u00b7ken", "Sai\u00b7ten", "zwun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in dem beseelten Ton, da\u00df auch der Thracen Hain'", "tokens": ["in", "dem", "be\u00b7seel\u00b7ten", "Ton", ",", "da\u00df", "auch", "der", "Thra\u00b7cen", "Hain'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und ungef\u00fc\u00dfte Kl\u00fcft' ihm nachgegangen sein;", "tokens": ["und", "un\u00b7ge\u00b7f\u00fc\u00df\u00b7te", "Kl\u00fcft'", "ihm", "nach\u00b7ge\u00b7gan\u00b7gen", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und da\u00df der Linus auch die Thebischen Gefilder,", "tokens": ["und", "da\u00df", "der", "Li\u00b7nus", "auch", "die", "The\u00b7bi\u00b7schen", "Ge\u00b7fil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das ungezahmte Land, gemachet hat viel milder", "tokens": ["das", "un\u00b7ge\u00b7zahm\u00b7te", "Land", ",", "ge\u00b7ma\u00b7chet", "hat", "viel", "mil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "VAFIN", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "durch seiner Harfen Kraft; wie auch Arions Kunst", "tokens": ["durch", "sei\u00b7ner", "Har\u00b7fen", "Kraft", ";", "wie", "auch", "A\u00b7rions", "Kunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "PWAV", "ADV", "NE", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "den liebenden Delfin zu einer solchen Gunst,", "tokens": ["den", "lie\u00b7ben\u00b7den", "Del\u00b7fin", "zu", "ei\u00b7ner", "sol\u00b7chen", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die \u00fcber Wundern ist, bei Lesbus hat bewogen;", "tokens": ["die", "\u00fc\u00b7ber", "Wun\u00b7dern", "ist", ",", "bei", "Les\u00b7bus", "hat", "be\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VAFIN", "$,", "APPR", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und da\u00df das wilde Wild Amphion nachgezogen,", "tokens": ["und", "da\u00df", "das", "wil\u00b7de", "Wild", "Am\u00b7phi\u00b7on", "nach\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "im Fall' er Stimm' und Spiel zugleiche t\u00f6nen lie\u00df, -", "tokens": ["im", "Fall'", "er", "Stimm'", "und", "Spiel", "zu\u00b7glei\u00b7che", "t\u00f6\u00b7nen", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "PPER", "NN", "KON", "NN", "ADJA", "VVINF", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ist mancher Kl\u00fcgling noch zu gl\u00e4uben ungewi\u00df.", "tokens": ["ist", "man\u00b7cher", "Kl\u00fcg\u00b7ling", "noch", "zu", "gl\u00e4u\u00b7ben", "un\u00b7ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie, spricht er, kan es sein, da\u00df Felsen h\u00f6ren k\u00f6nnen?", "tokens": ["Wie", ",", "spricht", "er", ",", "kan", "es", "sein", ",", "da\u00df", "Fel\u00b7sen", "h\u00f6\u00b7ren", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VAINF", "$,", "KOUS", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und hat der dumme Forst auch die Vernunft der Sinnen,", "tokens": ["Und", "hat", "der", "dum\u00b7me", "Forst", "auch", "die", "Ver\u00b7nunft", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "da\u00df er den Ton vernimmt? Drum wei\u00df er nicht, wohin", "tokens": ["da\u00df", "er", "den", "Ton", "ver\u00b7nimmt", "?", "Drum", "wei\u00df", "er", "nicht", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "PAV", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "die hohen Schriften sich in ihrer Deutung ziehn.", "tokens": ["die", "ho\u00b7hen", "Schrif\u00b7ten", "sich", "in", "ih\u00b7rer", "Deu\u00b7tung", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das strenge Heidenvolk sind die bewegten Klippen,", "tokens": ["Das", "stren\u00b7ge", "Hei\u00b7den\u00b7volk", "sind", "die", "be\u00b7weg\u00b7ten", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "der Wald das wilde Tun der ungeschlachten Lippen", "tokens": ["der", "Wald", "das", "wil\u00b7de", "Tun", "der", "un\u00b7ge\u00b7schlach\u00b7ten", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und grober Sitten Wust, die durch die kluge Hand", "tokens": ["und", "gro\u00b7ber", "Sit\u00b7ten", "Wust", ",", "die", "durch", "die", "klu\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "und feurige Vernunft der Weisen sich erkannt", "tokens": ["und", "feu\u00b7ri\u00b7ge", "Ver\u00b7nunft", "der", "Wei\u00b7sen", "sich", "er\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "und sich aus sich verjagt. Di\u00df hat der Kastalinnen", "tokens": ["und", "sich", "aus", "sich", "ver\u00b7jagt", ".", "Di\u00df", "hat", "der", "Kas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PRF", "VVPP", "$.", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "geneunte Schwesterzunft so gl\u00fccklich enden k\u00f6nnen", "tokens": ["ge\u00b7neun\u00b7te", "Schwes\u00b7ter\u00b7zunft", "so", "gl\u00fcck\u00b7lich", "en\u00b7den", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "ADJD", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "durch ihrer Musik Macht. Wer ist so taub und blind,", "tokens": ["durch", "ih\u00b7rer", "Mu\u00b7sik", "Macht", ".", "Wer", "ist", "so", "taub", "und", "blind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "PWS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "da\u00df er der Stimmen nicht ein Ohr und Auge g\u00f6nnt?", "tokens": ["da\u00df", "er", "der", "Stim\u00b7men", "nicht", "ein", "Ohr", "und", "Au\u00b7ge", "g\u00f6nnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie hat uns Menschen erst zu Menschen recht gemachet", "tokens": ["Sie", "hat", "uns", "Men\u00b7schen", "erst", "zu", "Men\u00b7schen", "recht", "ge\u00b7ma\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ADV", "APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "und durch ihr Lieblichsein uns freundlich zugelachet", "tokens": ["und", "durch", "ihr", "Lieb\u00b7lich\u00b7sein", "uns", "freund\u00b7lich", "zu\u00b7ge\u00b7la\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "als wir noch waren grob. Was nichts nicht zwingen kan,", "tokens": ["als", "wir", "noch", "wa\u00b7ren", "grob", ".", "Was", "nichts", "nicht", "zwin\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ADJD", "$.", "PWS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "das b\u00e4ndigt ein Gesang. Wenn Hermes stimmet an,", "tokens": ["das", "b\u00e4n\u00b7digt", "ein", "Ge\u00b7sang", ".", "Wenn", "Her\u00b7mes", "stim\u00b7met", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "KOUS", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "so schl\u00e4ft auch Argus ein. Was kan man bessers finden,", "tokens": ["so", "schl\u00e4ft", "auch", "Ar\u00b7gus", "ein", ".", "Was", "kan", "man", "bes\u00b7sers", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "PTKVZ", "$.", "PWS", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "wenn uns der Trauermut die lassen Geister binden", "tokens": ["wenn", "uns", "der", "Trau\u00b7er\u00b7mut", "die", "las\u00b7sen", "Geis\u00b7ter", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und ganz umnebeln will, als wenn bei guter Kost", "tokens": ["und", "ganz", "um\u00b7ne\u00b7beln", "will", ",", "als", "wenn", "bei", "gu\u00b7ter", "Kost"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "man um sich haben kan der Musik s\u00fc\u00dfe Lust,", "tokens": ["man", "um", "sich", "ha\u00b7ben", "kan", "der", "Mu\u00b7sik", "s\u00fc\u00b7\u00dfe", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PRF", "VAINF", "VMFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "der Kummert\u00f6terin? Da k\u00f6nnen deine Gaben,", "tokens": ["der", "Kum\u00b7mer\u00b7t\u00f6\u00b7te\u00b7rin", "?", "Da", "k\u00f6n\u00b7nen", "dei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Licinta, dich und uns in voller Wollust laben", "tokens": ["Li\u00b7cin\u00b7ta", ",", "dich", "und", "uns", "in", "vol\u00b7ler", "Wol\u00b7lust", "la\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "KON", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "und uns dir machen gleich, wenn dein bejahrter Wein", "tokens": ["und", "uns", "dir", "ma\u00b7chen", "gleich", ",", "wenn", "dein", "be\u00b7jahr\u00b7ter", "Wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "springt in der Schalen auf, und Einer spielet drein", "tokens": ["springt", "in", "der", "Scha\u00b7len", "auf", ",", "und", "Ei\u00b7ner", "spie\u00b7let", "drein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "es sei auch was es sei. Die Sing- und Saiten-Schulen,", "tokens": ["es", "sei", "auch", "was", "es", "sei", ".", "Die", "Sing", "und", "Sai\u00b7ten\u00b7Schu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWS", "PPER", "VAFIN", "$.", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "die lernen uns bevor das wolvergunte Buhlen,", "tokens": ["die", "ler\u00b7nen", "uns", "be\u00b7vor", "das", "wol\u00b7ver\u00b7gun\u00b7te", "Buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "und wie man sittsam wird. Cytheris und ihr Sohn", "tokens": ["und", "wie", "man", "sitt\u00b7sam", "wird", ".", "Cy\u00b7the\u00b7ris", "und", "ihr", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "ADJD", "VAFIN", "$.", "NE", "KON", "PPOSAT", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "sind, wo man singt und spielt, nicht gerne weit darvon.", "tokens": ["sind", ",", "wo", "man", "singt", "und", "spielt", ",", "nicht", "ger\u00b7ne", "weit", "dar\u00b7von", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$,", "PTKNEG", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Saul raset ohne sie. Misenus muste singen,", "tokens": ["Saul", "ra\u00b7set", "oh\u00b7ne", "sie", ".", "Mi\u00b7se\u00b7nus", "mus\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$.", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "solt' Hector lustig sein. Wenn ", "tokens": ["solt'", "Hec\u00b7tor", "lus\u00b7tig", "sein", ".", "Wenn"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VMFIN", "NE", "ADJD", "VAINF", "$.", "KOUS"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.43": {"text": "so w\u00e4chst des Sachsens Lust. Wenn ", "tokens": ["so", "w\u00e4chst", "des", "Sach\u00b7sens", "Lust", ".", "Wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$.", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "l\u00e4\u00dft h\u00f6ren und mit ihm den k\u00fcnstlichen Tenor,", "tokens": ["l\u00e4\u00dft", "h\u00f6\u00b7ren", "und", "mit", "ihm", "den", "k\u00fcnst\u00b7li\u00b7chen", "Te\u00b7nor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "KON", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "da wacht mein ", "tokens": ["da", "wacht", "mein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.46": {"text": "so hoch, wo \u00fcber uns der Leier Sternen klimmen,", "tokens": ["so", "hoch", ",", "wo", "\u00fc\u00b7ber", "uns", "der", "Lei\u00b7er", "Ster\u00b7nen", "klim\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "durch seinen ersten Preis, die deutschen Vers' emp\u00f6rt,", "tokens": ["durch", "sei\u00b7nen", "ers\u00b7ten", "Preis", ",", "die", "deut\u00b7schen", "Ver\u00b7s'", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "wor