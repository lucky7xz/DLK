{"dta.poem.5950": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Krafft und Schnelligkeit verursacht nun ein Drehen,", "tokens": ["Die", "Krafft", "und", "Schnel\u00b7lig\u00b7keit", "ver\u00b7ur\u00b7sacht", "nun", "ein", "Dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dar aus die Wirbel dann entstehen.", "tokens": ["Dar", "aus", "die", "Wir\u00b7bel", "dann", "ent\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die C\u00f6rper, so die Hinderni\u00df noch mehr", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "so", "die", "Hin\u00b7der\u00b7ni\u00df", "noch", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Versammlet und zusammen dringt,", "tokens": ["Ver\u00b7samm\u00b7let", "und", "zu\u00b7sam\u00b7men", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch strengen Druck gepresst, vereinen sich gar sehr;", "tokens": ["Durch", "stren\u00b7gen", "Druck", "ge\u00b7presst", ",", "ver\u00b7ei\u00b7nen", "sich", "gar", "sehr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wodurch er pl\u00f6tzlich sie, sich zu bewegen, zwingt:", "tokens": ["Wo\u00b7durch", "er", "pl\u00f6tz\u00b7lich", "sie", ",", "sich", "zu", "be\u00b7we\u00b7gen", ",", "zwingt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "$,", "PRF", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So, da\u00df sie alle sich in einer R\u00fcnde schwingen,", "tokens": ["So", ",", "da\u00df", "sie", "al\u00b7le", "sich", "in", "ei\u00b7ner", "R\u00fcn\u00b7de", "schwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PIS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wodurch sich alle stets im Creyse wiederbringen.", "tokens": ["Wo\u00b7durch", "sich", "al\u00b7le", "stets", "im", "Crey\u00b7se", "wie\u00b7der\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIS", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}