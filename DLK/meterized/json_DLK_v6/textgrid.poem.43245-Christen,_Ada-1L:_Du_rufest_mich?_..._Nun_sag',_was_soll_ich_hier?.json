{"textgrid.poem.43245": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du rufest mich? ... Nun sag', was soll ich hier?", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du rufest mich? ... Nun sag', was soll ich hier?", "tokens": ["Du", "ru\u00b7fest", "mich", "?", "...", "Nun", "sag'", ",", "was", "soll", "ich", "hier", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "$(", "ADV", "VVFIN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Affen sehen und die Papageien", "tokens": ["Die", "Af\u00b7fen", "se\u00b7hen", "und", "die", "Pa\u00b7pa\u00b7gei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die Gem\u00e4cher voll von Spielereien?", "tokens": ["Und", "die", "Ge\u00b7m\u00e4\u00b7cher", "voll", "von", "Spie\u00b7le\u00b7rei\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich singen h\u00f6ren? Sag', was bin ich Dir? ...", "tokens": ["Dich", "sin\u00b7gen", "h\u00f6\u00b7ren", "?", "Sag'", ",", "was", "bin", "ich", "Dir", "?", "..."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVINF", "VVINF", "$.", "NN", "$,", "PWS", "VAFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und selber Du? ... Was k\u00f6nntest Du mir sein?", "tokens": ["Und", "sel\u00b7ber", "Du", "?", "...", "Was", "k\u00f6nn\u00b7test", "Du", "mir", "sein", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$.", "$(", "PWS", "VMFIN", "PPER", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich glaube nur an Deine sch\u00f6ne H\u00fclle,", "tokens": ["Ich", "glau\u00b7be", "nur", "an", "Dei\u00b7ne", "sch\u00f6\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Deiner Locken goldigrothe F\u00fclle,", "tokens": ["An", "Dei\u00b7ner", "Lo\u00b7cken", "gol\u00b7di\u00b7gro\u00b7the", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Dir vereint w\u00e4r' dennoch ich allein.", "tokens": ["Mit", "Dir", "ver\u00b7eint", "w\u00e4r'", "den\u00b7noch", "ich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht sch\u00fcttle stumm die Rosen aus dem Haar,", "tokens": ["Nicht", "sch\u00fctt\u00b7le", "stumm", "die", "Ro\u00b7sen", "aus", "dem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und horche auf: Es taugen wohl zusammen", "tokens": ["Und", "hor\u00b7che", "auf", ":", "Es", "tau\u00b7gen", "wohl", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verrauschte Fluthen und verwehte Flammen,", "tokens": ["Ver\u00b7rauschte", "Flut\u00b7hen", "und", "ver\u00b7weh\u00b7te", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch niemals Herzen, alles Glaubens bar.", "tokens": ["Doch", "nie\u00b7mals", "Her\u00b7zen", ",", "al\u00b7les", "Glau\u00b7bens", "bar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}