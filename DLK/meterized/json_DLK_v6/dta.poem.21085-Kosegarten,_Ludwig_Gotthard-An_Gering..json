{"dta.poem.21085": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Gering.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den Tr&#252;mmern der Vergangenheiten               ", "tokens": ["Aus", "den", "Tr", "&#252;", "mmern", "der", "Ver\u00b7gan\u00b7gen\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aus den Aschen unsrer Jugendzeiten", "tokens": ["Aus", "den", "A\u00b7schen", "uns\u00b7rer", "Ju\u00b7gend\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Windet sich ein selger Schatten los.", "tokens": ["Win\u00b7det", "sich", "ein", "sel\u00b7ger", "Schat\u00b7ten", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Freund, es ist der Schatten jener Stunde,", "tokens": ["Freund", ",", "es", "ist", "der", "Schat\u00b7ten", "je\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wo mein Geist zum hohen Freundschaftsbunde", "tokens": ["Wo", "mein", "Geist", "zum", "ho\u00b7hen", "Freund\u00b7schafts\u00b7bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mit dem deinigen zusammenfloss;", "tokens": ["Mit", "dem", "dei\u00b7ni\u00b7gen", "zu\u00b7sam\u00b7men\u00b7floss", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wo ich einen Blutsfreund in dir sp\u00fcrte,", "tokens": ["Wo", "ich", "ei\u00b7nen", "Bluts\u00b7freund", "in", "dir", "sp\u00fcr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit der Wahrheit Pr\u00fcfstein dich ber\u00fchrte,", "tokens": ["Mit", "der", "Wahr\u00b7heit", "Pr\u00fcf\u00b7stein", "dich", "be\u00b7r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Und bew\u00e4hrt, wie \u00e4chtes Gold, dich fand;", "tokens": ["Und", "be\u00b7w\u00e4hrt", ",", "wie", "\u00e4ch\u00b7tes", "Gold", ",", "dich", "fand", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo ich dann mit dir und unserm Dritten", "tokens": ["Wo", "ich", "dann", "mit", "dir", "und", "un\u00b7serm", "Drit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "F\u00fcr der Trennung und des Bleibens H\u00fctten", "tokens": ["F\u00fcr", "der", "Tren\u00b7nung", "und", "des", "Blei\u00b7bens", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Unsrer Freundschaft k\u00fchnes Ideal erfand;", "tokens": ["Uns\u00b7rer", "Freund\u00b7schaft", "k\u00fch\u00b7nes", "I\u00b7deal", "er\u00b7fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}