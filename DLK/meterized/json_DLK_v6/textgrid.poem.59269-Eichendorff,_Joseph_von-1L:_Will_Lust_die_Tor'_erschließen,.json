{"textgrid.poem.59269": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Will Lust die Tor' erschlie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will Lust die Tor' erschlie\u00dfen,", "tokens": ["Will", "Lust", "die", "Tor'", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da bleib ich drau\u00dfen nicht,", "tokens": ["Da", "bleib", "ich", "drau\u00b7\u00dfen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Hohe zu begr\u00fc\u00dfen,", "tokens": ["Das", "Ho\u00b7he", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist des S\u00e4ngers Pflicht.", "tokens": ["Das", "ist", "des", "S\u00e4n\u00b7gers", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist die alte Halle,", "tokens": ["Das", "ist", "die", "al\u00b7te", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hier sang ich manches Mal,", "tokens": ["Hier", "sang", "ich", "man\u00b7ches", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hohen Ritter alle", "tokens": ["Die", "ho\u00b7hen", "Rit\u00b7ter", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rings um mich her im Saal.", "tokens": ["Rings", "um", "mich", "her", "im", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APZR", "APPRART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und von dem Heldenstreiten", "tokens": ["Und", "von", "dem", "Hel\u00b7den\u00b7strei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erklang manch k\u00fchnes Lied,", "tokens": ["Er\u00b7klang", "manch", "k\u00fch\u00b7nes", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das noch in n\u00e4cht'gen Zeiten", "tokens": ["Das", "noch", "in", "n\u00e4cht'\u00b7gen", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den stillen Bau durchzieht.", "tokens": ["Den", "stil\u00b7len", "Bau", "durch\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch farbenlos vergrauen", "tokens": ["Doch", "far\u00b7ben\u00b7los", "ver\u00b7grau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Bl\u00fcte Fels und Au \u2013", "tokens": ["Ohn", "Bl\u00fc\u00b7te", "Fels", "und", "Au", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fehlt' der Schmuck der Frauen", "tokens": ["Es", "fehlt'", "der", "Schmuck", "der", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem hochgewalt'gen Bau.", "tokens": ["Dem", "hoch\u00b7ge\u00b7walt'\u00b7gen", "Bau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die St\u00e4rke regt das Wilde,", "tokens": ["Die", "St\u00e4r\u00b7ke", "regt", "das", "Wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nur, der Kraft gesellt,", "tokens": ["Und", "nur", ",", "der", "Kraft", "ge\u00b7sellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die k\u00f6nigliche Milde", "tokens": ["Die", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Mil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bezwingt die starre Welt. \u2013", "tokens": ["Be\u00b7zwingt", "die", "star\u00b7re", "Welt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Welch Glanz hat mich umflogen", "tokens": ["Welch", "Glanz", "hat", "mich", "um\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00fcllt das ganze Haus,", "tokens": ["Und", "f\u00fcllt", "das", "gan\u00b7ze", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als pfeilerten die Bogen", "tokens": ["Als", "pfei\u00b7ler\u00b7ten", "die", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Himmelreich hinaus!", "tokens": ["Ins", "Him\u00b7mel\u00b7reich", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und was der Stein will sagen,", "tokens": ["Und", "was", "der", "Stein", "will", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mensch in tiefster Brust,", "tokens": ["Der", "Mensch", "in", "tiefs\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Kl\u00e4ngen anzuschlagen,", "tokens": ["In", "Kl\u00e4n\u00b7gen", "an\u00b7zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist des S\u00e4ngers Lust:", "tokens": ["Das", "ist", "des", "S\u00e4n\u00b7gers", "Lust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O du \u2013 gleichbar der Hohen,", "tokens": ["O", "du", "\u2013", "gleich\u00b7bar", "der", "Ho\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dieses Haus bewacht", "tokens": ["Die", "die\u00b7ses", "Haus", "be\u00b7wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Morgenrotes Lohen", "tokens": ["Und", "Mor\u00b7gen\u00b7ro\u00b7tes", "Lo\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Norden angefacht \u2013", "tokens": ["Im", "Nor\u00b7den", "an\u00b7ge\u00b7facht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Was Gro\u00dfes hier ersonnen,", "tokens": ["Was", "Gro\u00b7\u00dfes", "hier", "er\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "All Segen, der hier weilt,", "tokens": ["All", "Se\u00b7gen", ",", "der", "hier", "weilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "All Wohl, das hier begonnen,", "tokens": ["All", "Wohl", ",", "das", "hier", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir, hohe Frau, zum Heil!", "tokens": ["Dir", ",", "ho\u00b7he", "Frau", ",", "zum", "Heil", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und so nun will ich neigen", "tokens": ["Und", "so", "nun", "will", "ich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich vor der Majest\u00e4t \u2013", "tokens": ["Mich", "vor", "der", "Ma\u00b7jes\u00b7t\u00e4t", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann la\u00dft mich gehn und schweigen,", "tokens": ["Dann", "la\u00dft", "mich", "gehn", "und", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis ihr Sie wiederseht.", "tokens": ["Bis", "ihr", "Sie", "wie\u00b7der\u00b7seht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}