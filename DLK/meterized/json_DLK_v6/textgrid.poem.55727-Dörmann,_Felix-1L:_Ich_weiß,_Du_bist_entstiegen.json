{"textgrid.poem.55727": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wei\u00df, Du bist entstiegen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df, Du bist entstiegen", "tokens": ["Ich", "wei\u00df", ",", "Du", "bist", "ent\u00b7stie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Mondes eisigem Pf\u00fchl,", "tokens": ["Des", "Mon\u00b7des", "ei\u00b7si\u00b7gem", "Pf\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Durch Deine Adern fliegen", "tokens": ["Durch", "Dei\u00b7ne", "A\u00b7dern", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wiegen", "tokens": ["Und", "wie\u00b7gen"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Lichtwellen bleich und k\u00fchl.", "tokens": ["Licht\u00b7wel\u00b7len", "bleich", "und", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ich hab' mit Dir Erbarmen,", "tokens": ["Ich", "hab'", "mit", "Dir", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erbarmen auch mit mir.", "tokens": ["Er\u00b7bar\u00b7men", "auch", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst in meinen Armen", "tokens": ["Du", "wirst", "in", "mei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwarmen, \u2013", "tokens": ["Er\u00b7war\u00b7men", ",", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$,", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ich werde kalt bei Dir.", "tokens": ["Ich", "wer\u00b7de", "kalt", "bei", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}