{"textgrid.poem.55193": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An Tischbein", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst ein Deutscher, dann ein Schweizer,", "tokens": ["Erst", "ein", "Deut\u00b7scher", ",", "dann", "ein", "Schwei\u00b7zer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann ein Berg- und Taldurchkreuzer,", "tokens": ["Dann", "ein", "Ber\u00b7g", "und", "Tal\u00b7durch\u00b7kreu\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "R\u00f6mer, dann Napolitaner,", "tokens": ["R\u00f6\u00b7mer", ",", "dann", "Na\u00b7po\u00b7li\u00b7ta\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Philosoph und doch kein Aner,", "tokens": ["Phi\u00b7lo\u00b7soph", "und", "doch", "kein", "A\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dichter, fruchtbar aller Orten", "tokens": ["Dich\u00b7ter", ",", "frucht\u00b7bar", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bald mit Zeichen, bald mit Worten,", "tokens": ["Bald", "mit", "Zei\u00b7chen", ",", "bald", "mit", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer bleibest du derselbe", "tokens": ["Im\u00b7mer", "blei\u00b7best", "du", "der\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von der Tiber bis zur Elbe!", "tokens": ["Von", "der", "Ti\u00b7ber", "bis", "zur", "El\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gl\u00fcck und Heil! so wie du strebest,", "tokens": ["Gl\u00fcck", "und", "Heil", "!", "so", "wie", "du", "stre\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Leben! so wie du belebest,", "tokens": ["Le\u00b7ben", "!", "so", "wie", "du", "be\u00b7le\u00b7best", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "So genie\u00dfe! la\u00df genie\u00dfen!", "tokens": ["So", "ge\u00b7nie\u00b7\u00dfe", "!", "la\u00df", "ge\u00b7nie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VVIMP", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bis die Nymphen dich begr\u00fc\u00dfen,", "tokens": ["Bis", "die", "Nym\u00b7phen", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die sich in der Ilme baden", "tokens": ["Die", "sich", "in", "der", "Il\u00b7me", "ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und aufs freundlichste dich laden.", "tokens": ["Und", "aufs", "freund\u00b7lichs\u00b7te", "dich", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}