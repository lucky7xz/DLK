{"textgrid.poem.26308": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Nacht um Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Nacht um Nacht der Wind hinrauscht,", "tokens": ["Und", "Nacht", "um", "Nacht", "der", "Wind", "hin\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Nacht um Nacht mein Ohr hinlauscht.", "tokens": ["Und", "Nacht", "um", "Nacht", "mein", "Ohr", "hin\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer die gleichen Sterne ziehn,", "tokens": ["Und", "im\u00b7mer", "die", "glei\u00b7chen", "Ster\u00b7ne", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und immer dieselben Stunden fliehn.", "tokens": ["Und", "im\u00b7mer", "die\u00b7sel\u00b7ben", "Stun\u00b7den", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und immer nagt in mir derselbe Gram,", "tokens": ["Und", "im\u00b7mer", "nagt", "in", "mir", "der\u00b7sel\u00b7be", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und keine Nacht ich weiterkam.", "tokens": ["Und", "kei\u00b7ne", "Nacht", "ich", "wei\u00b7ter\u00b7kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}