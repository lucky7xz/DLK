{"textgrid.poem.52381": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "31. 'ne sch\u00f6ne Gesellschaft", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "En por von de Burmeisters wiren", "tokens": ["En", "por", "von", "de", "Burm\u00b7eis\u00b7ters", "wi\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "NE", "NE", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Verleden Landdag tau Malchin", "tokens": ["Ver\u00b7le\u00b7den", "Land\u00b7dag", "tau", "Mal\u00b7chin"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "CARD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Un w\u00fcrden wedder dor logieren,", "tokens": ["Un", "w\u00fcr\u00b7den", "wed\u00b7der", "dor", "lo\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KON", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sei all fr\u00fcher deden sin.", "tokens": ["Wo", "sei", "all", "fr\u00fc\u00b7her", "de\u00b7den", "sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ehr oll Quartier gef\u00f6ll ok sihr de beiden,", "tokens": ["Ehr", "oll", "Quar\u00b7tier", "ge\u00b7f\u00f6ll", "ok", "sihr", "de", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Un allens was ok, as sick't h\u00fcrt,", "tokens": ["Un", "al\u00b7lens", "was", "ok", ",", "as", "sick't", "h\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PWS", "NE", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Blot de Upwohrung was 'ne anner word'n:", "tokens": ["Blot", "de", "Up\u00b7woh\u00b7rung", "was", "'ne", "an\u00b7ner", "word'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PWS", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Ehr Wirt m\u00fc\u00dft sick en niges M\u00e4ten meiden,", "tokens": ["Ehr", "Wirt", "m\u00fc\u00dft", "sick", "en", "ni\u00b7ges", "M\u00e4\u00b7ten", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "FM", "FM", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So'n recht l\u00fctt dralles Fiken wir't.", "tokens": ["So'n", "recht", "l\u00fctt", "dral\u00b7les", "Fi\u00b7ken", "wir'", "t."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADJD", "ADJD", "ADJA", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "As de nu kamm den irsten Morgen", "tokens": ["As", "de", "nu", "kamm", "den", "irs\u00b7ten", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Un ehre Herrn den Koffe br\u00f6cht,", "tokens": ["Un", "eh\u00b7re", "Herrn", "den", "Kof\u00b7fe", "br\u00f6cht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Dunn lacht de ein oll Herr ehr an un fr\u00f6ggt:", "tokens": ["Dunn", "lacht", "de", "ein", "oll", "Herr", "ehr", "an", "un", "fr\u00f6ggt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.13": {"text": "\u00bbna, segg, min D\u00f6chting, segg mal, Fiken,", "tokens": ["\u00bb", "na", ",", "segg", ",", "min", "D\u00f6ch\u00b7ting", ",", "segg", "mal", ",", "Fi\u00b7ken", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ITJ", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.14": {"text": "K\u00fcmmst du mit twei Burmeisters woll taurecht?\u00ab", "tokens": ["K\u00fcmmst", "du", "mit", "twei", "Burm\u00b7eis\u00b7ters", "woll", "tau\u00b7recht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NE", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Un will ehr dor de Backen striken.", "tokens": ["Un", "will", "ehr", "dor", "de", "Ba\u00b7cken", "stri\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NN", "NE", "NE", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Denn so'n oll Herrn, de s\u00fcnd vull Schelmenst\u00fccken.", "tokens": ["Denn", "so'n", "oll", "Herrn", ",", "de", "s\u00fcnd", "vull", "Schel\u00b7men\u00b7st\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "$,", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Doch s\u00fcll dat Straken em nich gl\u00fccken,", "tokens": ["Doch", "s\u00fcll", "dat", "Stra\u00b7ken", "em", "nich", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Denn Fiken flitscht em weg un seggt:", "tokens": ["Denn", "Fi\u00b7ken", "flitscht", "em", "weg", "un", "seggt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbmit twei Burmeisters kem ick woll taurecht,", "tokens": ["\u00bb", "mit", "twei", "Burm\u00b7eis\u00b7ters", "kem", "ick", "woll", "tau\u00b7recht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "NE", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.20": {"text": "Mit de kem ick woll in de Reih,", "tokens": ["Mit", "de", "kem", "ick", "woll", "in", "de", "Reih", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ADV", "APPR", "NE", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.21": {"text": "Ick heww man uter dem noch all dat anner Veih!\u00ab", "tokens": ["Ick", "heww", "man", "u\u00b7ter", "dem", "noch", "all", "dat", "an\u00b7ner", "Veih", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "ADV", "PIAT", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}