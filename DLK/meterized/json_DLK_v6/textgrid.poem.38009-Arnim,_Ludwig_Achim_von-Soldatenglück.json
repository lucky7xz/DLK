{"textgrid.poem.38009": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Soldatengl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf ins weite Feld!", "tokens": ["Frisch", "auf", "ins", "wei\u00b7te", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu Wasser und zu Lande", "tokens": ["Zu", "Was\u00b7ser", "und", "zu", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bin ich Soldat f\u00fcr's Geld.", "tokens": ["Bin", "ich", "Sol\u00b7dat", "f\u00fcr's", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn alle Menschen schlafen,", "tokens": ["Wenn", "al\u00b7le", "Men\u00b7schen", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soldaten m\u00fcssen wachen,", "tokens": ["Sol\u00b7da\u00b7ten", "m\u00fcs\u00b7sen", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dazu sind sie bestellt.", "tokens": ["Da\u00b7zu", "sind", "sie", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nig tr\u00e4gt die Kron,", "tokens": ["Der", "K\u00f6\u00b7nig", "tr\u00e4gt", "die", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "In seiner Hand den Scepter,", "tokens": ["In", "sei\u00b7ner", "Hand", "den", "Scep\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er sitzt auf dem Thron,", "tokens": ["Wenn", "er", "sitzt", "auf", "dem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ein langes Schwerdt zur Seite,", "tokens": ["Ein", "lan\u00b7ges", "Schwerdt", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu gehen mit zum Streite,", "tokens": ["Zu", "ge\u00b7hen", "mit", "zum", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf Frieden und Pardon.", "tokens": ["Auf", "Frie\u00b7den", "und", "Par\u00b7don", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Adeliche Dam,", "tokens": ["Ein", "A\u00b7de\u00b7li\u00b7che", "Dam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die schl\u00e4ft bei ein'm Soldaten,", "tokens": ["Die", "schl\u00e4ft", "bei", "ein'm", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus lauter Liebes-Flamm.", "tokens": ["Aus", "lau\u00b7ter", "Lie\u00b7bes\u00b7Flamm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es klingt ihr in den Ohren", "tokens": ["Es", "klingt", "ihr", "in", "den", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soldaten sind gebohren", "tokens": ["Sol\u00b7da\u00b7ten", "sind", "ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus ritterlichem Stamm.", "tokens": ["Aus", "rit\u00b7ter\u00b7li\u00b7chem", "Stamm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Soldat du edles Blut,", "tokens": ["Sol\u00b7dat", "du", "ed\u00b7les", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weil du bist hochgebohren", "tokens": ["Weil", "du", "bist", "hoch\u00b7ge\u00b7boh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus lebensfrischem Muth,", "tokens": ["Aus", "le\u00b7bens\u00b7fri\u00b7schem", "Muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn schon die Kugeln sausen", "tokens": ["Wenn", "schon", "die", "Ku\u00b7geln", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df dir davor nicht grausen,", "tokens": ["La\u00df", "dir", "da\u00b7vor", "nicht", "grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wems gl\u00fcckt, der kommt davon.", "tokens": ["Wems", "gl\u00fcckt", ",", "der", "kommt", "da\u00b7von", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}