{"textgrid.poem.41633": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den b\u00f6sen Engeln zu vergleichen", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den b\u00f6sen Engeln zu vergleichen", "tokens": ["Den", "b\u00f6\u00b7sen", "En\u00b7geln", "zu", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich zu deinem Lager schleichen,", "tokens": ["Will", "ich", "zu", "dei\u00b7nem", "La\u00b7ger", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zur\u00fcck dir kehrend, heimlich-sacht,", "tokens": ["Zu\u00b7r\u00fcck", "dir", "keh\u00b7rend", ",", "heim\u00b7lich\u00b7sacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Schattenspuk der grauen Nacht.", "tokens": ["Im", "Schat\u00b7tens\u00b7puk", "der", "grau\u00b7en", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und K\u00fcsse geb ich dir, du S\u00fc\u00dfe,", "tokens": ["Und", "K\u00fcs\u00b7se", "geb", "ich", "dir", ",", "du", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kalt wie des Mondes Strahlengr\u00fc\u00dfe,", "tokens": ["Kalt", "wie", "des", "Mon\u00b7des", "Strah\u00b7len\u00b7gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie einer Schlange Schmeichelein,", "tokens": ["Wie", "ei\u00b7ner", "Schlan\u00b7ge", "Schmei\u00b7che\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich ringelnd um der Gr\u00fcfte Stein.", "tokens": ["Sich", "rin\u00b7gelnd", "um", "der", "Gr\u00fcf\u00b7te", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Morgenlicht, im d\u00e4mmerblassen,", "tokens": ["Im", "Mor\u00b7gen\u00b7licht", ",", "im", "d\u00e4m\u00b7mer\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Siehst meine St\u00e4tte du verlassen,", "tokens": ["Siehst", "mei\u00b7ne", "St\u00e4t\u00b7te", "du", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die kalt bleibt bis zum Abendgraun.", "tokens": ["Die", "kalt", "bleibt", "bis", "zum", "A\u00b7bend\u00b7graun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie andre Jugend dir und Leben", "tokens": ["Wie", "and\u00b7re", "Ju\u00b7gend", "dir", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beherrschen, die dir Liebe geben,", "tokens": ["Be\u00b7herr\u00b7schen", ",", "die", "dir", "Lie\u00b7be", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich dein Herr sein durch das Graun!", "tokens": ["Will", "ich", "dein", "Herr", "sein", "durch", "das", "Graun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}