{"textgrid.poem.44427": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der Halbmond gl\u00e4nzet am Himmel]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Halbmond gl\u00e4nzet am Himmel,", "tokens": ["Der", "Halb\u00b7mond", "gl\u00e4n\u00b7zet", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und es ist neblicht und kalt.", "tokens": ["Und", "es", "ist", "neb\u00b7licht", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegr\u00fc\u00dft sei du Halber dort oben,", "tokens": ["Ge\u00b7gr\u00fc\u00dft", "sei", "du", "Hal\u00b7ber", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "ADV", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie du, bin ich einer, der halb.", "tokens": ["Wie", "du", ",", "bin", "ich", "ei\u00b7ner", ",", "der", "halb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VAFIN", "PPER", "PIS", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Halb gut, halb \u00fcbel geboren,", "tokens": ["Halb", "gut", ",", "halb", "\u00fc\u00b7bel", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Und d\u00fcrftig in beider Gestalt,", "tokens": ["Und", "d\u00fcrf\u00b7tig", "in", "bei\u00b7der", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Gutes ohne W\u00fcrde,", "tokens": ["Mein", "Gu\u00b7tes", "oh\u00b7ne", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das B\u00f6se ohne Gewalt.", "tokens": ["Das", "B\u00f6\u00b7se", "oh\u00b7ne", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Halb schmeckt ich die Freuden des Lebens,", "tokens": ["Halb", "schmeckt", "ich", "die", "Freu\u00b7den", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nichts ganz als meine Reu;", "tokens": ["Nichts", "ganz", "als", "mei\u00b7ne", "Reu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOUS", "PPOSAT", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ersten Bissen genossen,", "tokens": ["Die", "ers\u00b7ten", "Bis\u00b7sen", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schien alles mir einerlei.", "tokens": ["Schien", "al\u00b7les", "mir", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Halb gab ich mich hin den Musen,", "tokens": ["Halb", "gab", "ich", "mich", "hin", "den", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ADV", "ART", "NN", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und sie erh\u00f6rten mich halb;", "tokens": ["Und", "sie", "er\u00b7h\u00f6r\u00b7ten", "mich", "halb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hart auf der H\u00e4lfte des Lebens", "tokens": ["Hart", "auf", "der", "H\u00e4lf\u00b7te", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Entflohn sie und lie\u00dfen mich alt.", "tokens": ["Ent\u00b7flohn", "sie", "und", "lie\u00b7\u00dfen", "mich", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und also sitz ich verdrossen,", "tokens": ["Und", "al\u00b7so", "sitz", "ich", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch l\u00e4\u00dft die Zersplitterung nach;", "tokens": ["Doch", "l\u00e4\u00dft", "die", "Zer\u00b7split\u00b7te\u00b7rung", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die leere H\u00e4lfte der Seele", "tokens": ["Die", "lee\u00b7re", "H\u00e4lf\u00b7te", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verdr\u00e4ngt die noch volle gemach.", "tokens": ["Ver\u00b7dr\u00e4ngt", "die", "noch", "vol\u00b7le", "ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADV", "ADJA", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}