{"dta.poem.10274": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Wogen taucht ein blasser Strand,", "tokens": ["Aus", "Wo\u00b7gen", "taucht", "ein", "blas\u00b7ser", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es schimmert fern durch meine Thr\u00e4nen", "tokens": ["Es", "schim\u00b7mert", "fern", "durch", "mei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Vaterlandes K\u00fcstenrand,", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "K\u00fcs\u00b7ten\u00b7rand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ersch\u00f6pft mu\u00df ich am Maste lehnen.", "tokens": ["Er\u00b7sch\u00f6pft", "mu\u00df", "ich", "am", "Mas\u00b7te", "leh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Flieder bl\u00fcht, die Schwalbe zieht,", "tokens": ["Der", "Flie\u00b7der", "bl\u00fcht", ",", "die", "Schwal\u00b7be", "zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf den D\u00e4chern schwatzen Staare,", "tokens": ["Und", "auf", "den", "D\u00e4\u00b7chern", "schwat\u00b7zen", "Staa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Orgeldreher dreht sein Lied,", "tokens": ["Der", "Or\u00b7gel\u00b7dre\u00b7her", "dreht", "sein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein linder Wind k\u00fc\u00dft mir die Haare.", "tokens": ["Ein", "lin\u00b7der", "Wind", "k\u00fc\u00dft", "mir", "die", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die M\u00e4dchen lachen Arm in Arm,", "tokens": ["Die", "M\u00e4d\u00b7chen", "la\u00b7chen", "Arm", "in", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soldaten stehen vor der Wache,", "tokens": ["Sol\u00b7da\u00b7ten", "ste\u00b7hen", "vor", "der", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und aus der Schule bricht ein Schwarm,", "tokens": ["Und", "aus", "der", "Schu\u00b7le", "bricht", "ein", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der lustig l\u00e4rmt in ", "tokens": ["Der", "lus\u00b7tig", "l\u00e4rmt", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Es schreit mein Herz, es jauchzt und bebt", "tokens": ["Es", "schreit", "mein", "Herz", ",", "es", "jauchzt", "und", "bebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der alten Heimat hei\u00df entgegen.", "tokens": ["Der", "al\u00b7ten", "Hei\u00b7mat", "hei\u00df", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was als Kind ich je durchlebt,", "tokens": ["Und", "was", "als", "Kind", "ich", "je", "durch\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "KOUS", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Klingt wieder mir auf allen Wegen.", "tokens": ["Klingt", "wie\u00b7der", "mir", "auf", "al\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}