{"textgrid.poem.62166": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[goldne Tr\u00e4ume ging't verloren]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Goldne Tr\u00e4ume ging't verloren,", "tokens": ["Gold\u00b7ne", "Tr\u00e4u\u00b7me", "ging't", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Lebens Dunkelheit,", "tokens": ["In", "des", "Le\u00b7bens", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blieb't zum Traume auserkoren!", "tokens": ["Blie\u00b7b't", "zum", "Trau\u00b7me", "au\u00b7ser\u00b7ko\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Traum ist keine Wirklichkeit. \u2013", "tokens": ["Traum", "ist", "kei\u00b7ne", "Wirk\u00b7lich\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}