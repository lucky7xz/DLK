{"textgrid.poem.45783": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die f\u00fcnfundzwanzig, aus \u00d6streich verbannt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die f\u00fcnfundzwanzig, aus \u00d6streich verbannt,", "tokens": ["Die", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig", ",", "aus", "\u00d6s\u00b7treich", "ver\u00b7bannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind noch im Reich der Liebe bekannt,", "tokens": ["Sind", "noch", "im", "Reich", "der", "Lie\u00b7be", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und wo eine Ehe schon muffig und ranzig,", "tokens": ["Und", "wo", "ei\u00b7ne", "E\u00b7he", "schon", "muf\u00b7fig", "und", "ran\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Erfrischt sie mit eins das Wort f\u00fcnfundzwanzig.", "tokens": ["Er\u00b7frischt", "sie", "mit", "eins", "das", "Wort", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "ART", "NN", "CARD", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}