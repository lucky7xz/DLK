{"textgrid.poem.34172": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Nur weiter", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vormund setzt ihr nach Vormund, wer sollt' es nicht loben und preisen?", "tokens": ["Vor\u00b7mund", "setzt", "ihr", "nach", "Vor\u00b7mund", ",", "wer", "sollt'", "es", "nicht", "lo\u00b7ben", "und", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,", "PWS", "VMFIN", "PPER", "PTKNEG", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+--++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Geht nur weiter, ihr seid noch nicht am Ziele der Bahn.", "tokens": ["Geht", "nur", "wei\u00b7ter", ",", "ihr", "seid", "noch", "nicht", "am", "Zie\u00b7le", "der", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Setzt, wie jeglichem Dorf, so jeglichem Menschen den seinen,", "tokens": ["Setzt", ",", "wie", "jeg\u00b7li\u00b7chem", "Dorf", ",", "so", "jeg\u00b7li\u00b7chem", "Men\u00b7schen", "den", "sei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "ART", "PPOSAT", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Dann wird wieder, wie einst, Jeder sein eigener sein.", "tokens": ["Dann", "wird", "wie\u00b7der", ",", "wie", "einst", ",", "Je\u00b7der", "sein", "ei\u00b7ge\u00b7ner", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "PWAV", "ADV", "$,", "PIS", "PPOSAT", "ADJA", "VAINF", "$."], "meter": "-+----+--+--+", "measure": "iambic.tetra.relaxed"}}}}}