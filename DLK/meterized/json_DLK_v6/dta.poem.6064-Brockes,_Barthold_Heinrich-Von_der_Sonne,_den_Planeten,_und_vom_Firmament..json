{"dta.poem.6064": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stellt er in seinem Lauf", "tokens": ["Stellt", "er", "in", "sei\u00b7nem", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich zwischen uns und auch dem Sonnen-Lichte,", "tokens": ["Sich", "zwi\u00b7schen", "uns", "und", "auch", "dem", "Son\u00b7nen\u00b7Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So f\u00e4ngt er, da sein dunckler Klump so dichte;", "tokens": ["So", "f\u00e4ngt", "er", ",", "da", "sein", "dunck\u00b7ler", "Klump", "so", "dich\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der hellen Sonnen-Strahlen auf.", "tokens": ["Der", "hel\u00b7len", "Son\u00b7nen\u00b7Strah\u00b7len", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hiedurch nun schwindet offtermal", "tokens": ["Hie\u00b7durch", "nun", "schwin\u00b7det", "off\u00b7ter\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der aufgefangne Sonnen-Strahl.", "tokens": ["Der", "auf\u00b7ge\u00b7fang\u00b7ne", "Son\u00b7nen\u00b7Strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}