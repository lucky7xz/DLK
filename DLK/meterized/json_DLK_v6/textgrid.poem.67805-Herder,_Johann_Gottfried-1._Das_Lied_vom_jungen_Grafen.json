{"textgrid.poem.67805": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1. Das Lied vom jungen Grafen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steh auf einem hohen Berg,", "tokens": ["Ich", "steh", "auf", "ei\u00b7nem", "ho\u00b7hen", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh 'nunter ins tiefe Thal,", "tokens": ["Seh", "'n\u00b7un\u00b7ter", "ins", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Da sah ich ein Schifflein schweben,", "tokens": ["Da", "sah", "ich", "ein", "Schif\u00b7flein", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Darinn drey Grafen sass'n.", "tokens": ["Da\u00b7rinn", "drey", "Gra\u00b7fen", "sass'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PAV", "CARD", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der allerj\u00fcngst, der drunter war,", "tokens": ["Der", "al\u00b7ler\u00b7j\u00fcngst", ",", "der", "drun\u00b7ter", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PAV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die in dem Schifflein sass'n,", "tokens": ["Die", "in", "dem", "Schif\u00b7flein", "sass'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der gebot seiner Lieben zu trinken", "tokens": ["Der", "ge\u00b7bot", "sei\u00b7ner", "Lie\u00b7ben", "zu", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "PTKZU", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Aus einem venedischen Glas.", "tokens": ["Aus", "ei\u00b7nem", "ve\u00b7ne\u00b7di\u00b7schen", "Glas", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbwas giebst mir lang zu trinken,", "tokens": ["\u00bb", "was", "giebst", "mir", "lang", "zu", "trin\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was schenkst du mir lang ein?", "tokens": ["Was", "schenkst", "du", "mir", "lang", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will jezt in ein Kloster gehn,", "tokens": ["Ich", "will", "jezt", "in", "ein", "Klos\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will Gottes Dienerin seyn.\u00ab", "tokens": ["Will", "Got\u00b7tes", "Die\u00b7ne\u00b7rin", "seyn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "NN", "NN", "VAINF", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbwillst du jetzt in ein Kloster gehn,", "tokens": ["\u00bb", "willst", "du", "jetzt", "in", "ein", "Klos\u00b7ter", "gehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Willst Gottes Dienerin seyn,", "tokens": ["Willst", "Got\u00b7tes", "Die\u00b7ne\u00b7rin", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "NN", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So geh in Gottes Namen;", "tokens": ["So", "geh", "in", "Got\u00b7tes", "Na\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Deins gleichen giebts noch mehr!\u00ab", "tokens": ["Deins", "glei\u00b7chen", "giebts", "noch", "mehr", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und als es war um Mitternacht,", "tokens": ["Und", "als", "es", "war", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem jung'n Graf tr\u00e4umts so schwer,", "tokens": ["Dem", "jung'n", "Graf", "tr\u00e4umts", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ob sein allerliebster Schaz", "tokens": ["Als", "ob", "sein", "al\u00b7ler\u00b7liebs\u00b7ter", "Schaz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Kloster gezogen w\u00e4r.", "tokens": ["Ins", "Klos\u00b7ter", "ge\u00b7zo\u00b7gen", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbauf Knecht, steh auf und tummle dich;", "tokens": ["\u00bb", "auf", "Knecht", ",", "steh", "auf", "und", "tumm\u00b7le", "dich", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "VVFIN", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sattl' unser beide Pferd!", "tokens": ["Sattl'", "un\u00b7ser", "bei\u00b7de", "Pferd", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen reiten, sey Tag oder Nacht;", "tokens": ["Wir", "wol\u00b7len", "rei\u00b7ten", ",", "sey", "Tag", "o\u00b7der", "Nacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "$,", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Lieb ist reitens werth!\u00ab", "tokens": ["Die", "Lieb", "ist", "rei\u00b7tens", "werth", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und da sie vor jen's Kloster kamen,", "tokens": ["Und", "da", "sie", "vor", "jen's", "Klos\u00b7ter", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl vor das hohe Thor,", "tokens": ["Wohl", "vor", "das", "ho\u00b7he", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fragt er nach j\u00fcngst der Nonnen,", "tokens": ["Fragt", "er", "nach", "j\u00fcngst", "der", "Non\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die in dem Kloster war.", "tokens": ["Die", "in", "dem", "Klos\u00b7ter", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das N\u00f6nnlein kam gegangen", "tokens": ["Das", "N\u00f6nn\u00b7lein", "kam", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In einem schneeweissen Kleid;", "tokens": ["In", "ei\u00b7nem", "schnee\u00b7weis\u00b7sen", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ihr H\u00e4\u00e4rl war abgeschnitten,", "tokens": ["Ihr", "H\u00e4\u00b7\u00e4rl", "war", "ab\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr rother Mund war bleich.", "tokens": ["Ihr", "ro\u00b7ther", "Mund", "war", "bleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Knab er sezt sich nieder,", "tokens": ["Der", "Knab", "er", "sezt", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sa\u00df auf einem Stein;", "tokens": ["Er", "sa\u00df", "auf", "ei\u00b7nem", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er weint die hellen Thr\u00e4nen,", "tokens": ["Er", "weint", "die", "hel\u00b7len", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Brach ihm sein Herz entzwey.", "tokens": ["Brach", "ihm", "sein", "Herz", "ent\u00b7zwey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}}}}