{"textgrid.poem.39167": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[wenn ich mir so das Tun der Welt betrachte]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich mir so das Tun der Welt betrachte,", "tokens": ["Wenn", "ich", "mir", "so", "das", "Tun", "der", "Welt", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das fad und geistlos ist und kalt und trocken,", "tokens": ["Das", "fad", "und", "geist\u00b7los", "ist", "und", "kalt", "und", "tro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "VAFIN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ich so ganz aus tiefster Brust verachte,", "tokens": ["Das", "ich", "so", "ganz", "aus", "tiefs\u00b7ter", "Brust", "ver\u00b7ach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und schaue dann auf Deine Feenlocken,", "tokens": ["Und", "schau\u00b7e", "dann", "auf", "Dei\u00b7ne", "Feen\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Auf Deiner Wangen, Deiner Augen Gluten,", "tokens": ["Auf", "Dei\u00b7ner", "Wan\u00b7gen", ",", "Dei\u00b7ner", "Au\u00b7gen", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6re hallen Deines Mundes Glocken:", "tokens": ["Und", "h\u00f6\u00b7re", "hal\u00b7len", "Dei\u00b7nes", "Mun\u00b7des", "Glo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dann ist die Welt ein Ozean voll Fluten,", "tokens": ["Dann", "ist", "die", "Welt", "ein", "O\u00b7ze\u00b7an", "voll", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll St\u00fcrmen mir und bodenlosen Gr\u00fcften", "tokens": ["Voll", "St\u00fcr\u00b7men", "mir", "und", "bo\u00b7den\u00b7lo\u00b7sen", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "PPER", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Klippen, dran mein Herz sich will verbluten;", "tokens": ["Und", "Klip\u00b7pen", ",", "dran", "mein", "Herz", "sich", "will", "ver\u00b7blu\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PAV", "PPOSAT", "NN", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du aber scheinst ein reines \u00c4therd\u00fcften,", "tokens": ["Du", "a\u00b7ber", "scheinst", "ein", "rei\u00b7nes", "\u00c4t\u00b7her\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das s\u00e4uselnd hinschwebt durch des Meeres Br\u00fcllen,", "tokens": ["Das", "s\u00e4u\u00b7selnd", "hin\u00b7schwebt", "durch", "des", "Mee\u00b7res", "Br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Das rosenatmend rollt auf Morgenl\u00fcften,", "tokens": ["Das", "ro\u00b7se\u00b7nat\u00b7mend", "rollt", "auf", "Mor\u00b7gen\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Des kranken Dichters sch\u00e4umend Blut zu stillen.", "tokens": ["Des", "kran\u00b7ken", "Dich\u00b7ters", "sch\u00e4u\u00b7mend", "Blut", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}