{"dta.poem.12139": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stets unterhaltend ist die Reise f\u00fcr den Mann;", "tokens": ["Stets", "un\u00b7ter\u00b7hal\u00b7tend", "ist", "die", "Rei\u00b7se", "f\u00fcr", "den", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald ziehn die Gegenden, bald dich die Menschen an.", "tokens": ["Bald", "ziehn", "die", "Ge\u00b7gen\u00b7den", ",", "bald", "dich", "die", "Men\u00b7schen", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wo anziehend nicht der Mensch ist noch die Gegend,", "tokens": ["Und", "wo", "an\u00b7zie\u00b7hend", "nicht", "der", "Mensch", "ist", "noch", "die", "Ge\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PTKNEG", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehst du Gespr\u00e4ch mit dir und fernen Lieben pflegend.", "tokens": ["Gehst", "du", "Ge\u00b7spr\u00e4ch", "mit", "dir", "und", "fer\u00b7nen", "Lie\u00b7ben", "pfle\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPER", "KON", "ADJA", "NN