{"dta.poem.9312": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vierdte Handlung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.57", "en:0.42"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich thun/ da hab ich nun", "tokens": ["Was", "soll", "ich", "thun", "/", "da", "hab", "ich", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$(", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gantze nacht umsonst gewacht/", "tokens": ["Die", "gant\u00b7ze", "nacht", "um\u00b7sonst", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wei\u00df noch nicht wie mir geschicht/", "tokens": ["Und", "wei\u00df", "noch", "nicht", "wie", "mir", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob mir der tod in meiner noth", "tokens": ["Ob", "mir", "der", "tod", "in", "mei\u00b7ner", "noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fein sanfft und still", "tokens": ["Fein", "sanfft", "und", "still"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJD"], "meter": "---+", "measure": "unknown.measure.single"}, "line.6": {"text": "Ein gn\u00e4digs ende machen will.", "tokens": ["Ein", "gn\u00e4\u00b7digs", "en\u00b7de", "ma\u00b7chen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bald k\u00f6mmt ein b\u00e4r und schleicht sich her/", "tokens": ["Bald", "k\u00f6mmt", "ein", "b\u00e4r", "und", "schleicht", "sich", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "KON", "VVFIN", "PRF", "APZR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald k\u00f6mmt ein luchs und bald ein fuchs/", "tokens": ["Bald", "k\u00f6mmt", "ein", "luchs", "und", "bald", "ein", "fuchs", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "KON", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bald k\u00f6mmt ein has\u2019 und sucht sein gra\u00df", "tokens": ["Bald", "k\u00f6mmt", "ein", "has'", "und", "sucht", "sein", "gra\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Um meine schuh/ bald k\u00f6mmt darzu", "tokens": ["Um", "mei\u00b7ne", "schuh", "/", "bald", "k\u00f6mmt", "dar\u00b7zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein wildes schwein", "tokens": ["Ein", "wil\u00b7des", "schwein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Und zwickt mich an das lincke bein.", "tokens": ["Und", "zwickt", "mich", "an", "das", "lin\u00b7cke", "bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ach weh/ ach weh/ ich steh und steh/", "tokens": ["Ach", "weh", "/", "ach", "weh", "/", "ich", "steh", "und", "steh", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "XY", "PTKVZ", "$(", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und kan nicht fort/ ob mir der ort", "tokens": ["Und", "kan", "nicht", "fort", "/", "ob", "mir", "der", "ort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "PTKVZ", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In diesem zelt gleich nicht gef\u00e4llt:", "tokens": ["In", "die\u00b7sem", "zelt", "gleich", "nicht", "ge\u00b7f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ach tod komm bald und thu gewalt/", "tokens": ["Ach", "tod", "komm", "bald", "und", "thu", "ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "ADV", "KON", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Sonst kriech ich noch", "tokens": ["Sonst", "kriech", "ich", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Vor \u00e4ngsten in ein m\u00e4useloch.", "tokens": ["Vor", "\u00e4ngs\u00b7ten", "in", "ein", "m\u00e4u\