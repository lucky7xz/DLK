{"textgrid.poem.60280": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Abmahnung", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft ab, la\u00dft ab, bauw\u00fctig rauhe Leute,", "tokens": ["La\u00dft", "ab", ",", "la\u00dft", "ab", ",", "bau\u00b7w\u00fc\u00b7tig", "rau\u00b7he", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00f6ret mir die liebe Stelle nimmer,", "tokens": ["Und", "st\u00f6\u00b7ret", "mir", "die", "lie\u00b7be", "Stel\u00b7le", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo spielend sich des St\u00e4dtchens Jugend freute", "tokens": ["Wo", "spie\u00b7lend", "sich", "des", "St\u00e4dt\u00b7chens", "Ju\u00b7gend", "freu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seines Gl\u00fcckes fl\u00fcchtgem Morgenschimmer.", "tokens": ["In", "sei\u00b7nes", "Gl\u00fc\u00b7ckes", "fl\u00fccht\u00b7gem", "Mor\u00b7gen\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier spielten eure V\u00e4ter, eure Ahnen;", "tokens": ["Hier", "spiel\u00b7ten", "eu\u00b7re", "V\u00e4\u00b7ter", ",", "eu\u00b7re", "Ah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier hat sie abgerufen einst das Leben", "tokens": ["Hier", "hat", "sie", "ab\u00b7ge\u00b7ru\u00b7fen", "einst", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf seines Ernstes dornenvolle Bahnen;", "tokens": ["Auf", "sei\u00b7nes", "Erns\u00b7tes", "dor\u00b7nen\u00b7vol\u00b7le", "Bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O wollet euch der Stelle fromm begeben!", "tokens": ["O", "wol\u00b7let", "euch", "der", "Stel\u00b7le", "fromm", "be\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl heilig ist zu achten solche St\u00e4tte,", "tokens": ["Wohl", "hei\u00b7lig", "ist", "zu", "ach\u00b7ten", "sol\u00b7che", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PTKZU", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich vom Ahn zum fernen Kind gewunden", "tokens": ["Wo", "sich", "vom", "Ahn", "zum", "fer\u00b7nen", "Kind", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPRART", "NN", "APPRART", "ADJA", "NN", "VAPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Jugendspiele goldne Freudenkette,", "tokens": ["Der", "Ju\u00b7gend\u00b7spie\u00b7le", "gold\u00b7ne", "Freu\u00b7den\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo viele lebten ihre liebsten Stunden.", "tokens": ["Wo", "vie\u00b7le", "leb\u00b7ten", "ih\u00b7re", "liebs\u00b7ten", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wollt ihr bauen, bauet Kirchhofw\u00e4nde,", "tokens": ["Doch", "wollt", "ihr", "bau\u00b7en", ",", "bau\u00b7et", "Kirch\u00b7hof\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df man den Toten hier zu seinem Grabe,", "tokens": ["Da\u00df", "man", "den", "To\u00b7ten", "hier", "zu", "sei\u00b7nem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zugleich zur St\u00e4tte seiner Jugend sende,", "tokens": ["Zu\u00b7gleich", "zur", "St\u00e4t\u00b7te", "sei\u00b7ner", "Ju\u00b7gend", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er sein Bestes hier beisammen habe!", "tokens": ["Da\u00df", "er", "sein", "Bes\u00b7tes", "hier", "bei\u00b7sam\u00b7men", "ha\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}