{"textgrid.poem.47996": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Heilige Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eh der Stern von Bethlehem", "tokens": ["Eh", "der", "Stern", "von", "Beth\u00b7le\u00b7hem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "noch im dunklen Tal erschienen,", "tokens": ["noch", "im", "dunk\u00b7len", "Tal", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "l\u00f6sten, Sklaven zu bedienen,", "tokens": ["l\u00f6s\u00b7ten", ",", "Skla\u00b7ven", "zu", "be\u00b7die\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrsten schon ihr Diadem;", "tokens": ["F\u00fcrs\u00b7ten", "schon", "ihr", "Di\u00b7a\u00b7dem", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "ahnend eine h\u00f6hre Macht,", "tokens": ["ah\u00b7nend", "ei\u00b7ne", "h\u00f6h\u00b7re", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "gr\u00fc\u00dften sie die heil'ge Nacht.", "tokens": ["gr\u00fc\u00df\u00b7ten", "sie", "die", "heil'\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eh das Licht der Welt genaht,", "tokens": ["Eh", "das", "Licht", "der", "Welt", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "flammten schon in tiefer, scheuer", "tokens": ["flamm\u00b7ten", "schon", "in", "tie\u00b7fer", ",", "scheu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJD", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waldesnacht die Sonnwendfeuer", "tokens": ["Wal\u00b7des\u00b7nacht", "die", "Sonn\u00b7wend\u00b7feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "himmelw\u00e4rts; vom Bergesgrat", "tokens": ["him\u00b7mel\u00b7w\u00e4rts", ";", "vom", "Ber\u00b7ges\u00b7grat"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$.", "APPRART", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "lohte talw\u00e4rts ihre Pracht,", "tokens": ["loh\u00b7te", "tal\u00b7w\u00e4rts", "ih\u00b7re", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "gr\u00fc\u00dfend die geweihte Nacht.", "tokens": ["gr\u00fc\u00b7\u00dfend", "die", "ge\u00b7weih\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hoben Geisterh\u00e4nde nicht", "tokens": ["Ho\u00b7ben", "Geis\u00b7ter\u00b7h\u00e4n\u00b7de", "nicht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in der Vorzeit heil'ger Feier", "tokens": ["in", "der", "Vor\u00b7zeit", "heil'\u00b7ger", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "den geheimnisvollen Schleier", "tokens": ["den", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "Schlei\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "von der Zukunft Angesicht?", "tokens": ["von", "der", "Zu\u00b7kunft", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ahnte deiner Wunder Macht", "tokens": ["Ahn\u00b7te", "dei\u00b7ner", "Wun\u00b7der", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "schon die Welt, geweihte Nacht? \u2013", "tokens": ["schon", "die", "Welt", ",", "ge\u00b7weih\u00b7te", "Nacht", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht auf einen kurzen Tag", "tokens": ["Nicht", "auf", "ei\u00b7nen", "kur\u00b7zen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ward die Freiheit dir erschlossen \u2013", "tokens": ["ward", "die", "Frei\u00b7heit", "dir", "er\u00b7schlos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "jauchze mit den Festgenossen,", "tokens": ["jauch\u00b7ze", "mit", "den", "Fest\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sklave, deine Kette brach!", "tokens": ["Skla\u00b7ve", ",", "dei\u00b7ne", "Ket\u00b7te", "brach", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe hat dich frei gemacht \u2013", "tokens": ["Lie\u00b7be", "hat", "dich", "frei", "ge\u00b7macht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "beug dein Knie in heil'ger Nacht!", "tokens": ["beug", "dein", "Knie", "in", "heil'\u00b7ger", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht im unwirtbaren Raum", "tokens": ["Nicht", "im", "un\u00b7wirt\u00b7ba\u00b7ren", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "flammt die Glut der Sonnenwende,", "tokens": ["flammt", "die", "Glut", "der", "Son\u00b7nen\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "unsrer Kinder zarte H\u00e4nde", "tokens": ["uns\u00b7rer", "Kin\u00b7der", "zar\u00b7te", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schm\u00fccken heut den Tannenbaum.", "tokens": ["schm\u00fc\u00b7cken", "heut", "den", "Tan\u00b7nen\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schimmernd strahlt der Kerzen Pracht", "tokens": ["Schim\u00b7mernd", "strahlt", "der", "Ker\u00b7zen", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u2013 sei gegr\u00fc\u00dft, geweihte Nacht!", "tokens": ["\u2013", "sei", "ge\u00b7gr\u00fc\u00dft", ",", "ge\u00b7weih\u00b7te", "Nacht", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und durch klares Schneegefild,", "tokens": ["Und", "durch", "kla\u00b7res", "Schnee\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schwebend auf des Mondlichts Wogen,", "tokens": ["schwe\u00b7bend", "auf", "des", "Mond\u00b7lichts", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "kommt ein Glockenton gezogen,", "tokens": ["kommt", "ein", "Glo\u00b7cken\u00b7ton", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der die tiefste Sehnsucht stillt \u2013", "tokens": ["der", "die", "tiefs\u00b7te", "Sehn\u00b7sucht", "stillt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "lenzhauchmild durch Winterpracht", "tokens": ["lenz\u00b7hauch\u00b7mild", "durch", "Win\u00b7ter\u00b7pracht"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "klingt der Gru\u00df der Weihenacht:", "tokens": ["klingt", "der", "Gru\u00df", "der", "Wei\u00b7he\u00b7nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bballer Menschheit, ruhelos,", "tokens": ["\u00bb", "al\u00b7ler", "Menschheit", ",", "ru\u00b7he\u00b7los", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PIAT", "NN", "$,", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "schmerzbefangen, wahnverloren,", "tokens": ["schmerz\u00b7be\u00b7fan\u00b7gen", ",", "wahn\u00b7ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "ward der Friede heut geboren", "tokens": ["ward", "der", "Frie\u00b7de", "heut", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aus der ew'gen Liebe Scho\u00df! \u2013", "tokens": ["aus", "der", "ew'\u00b7gen", "Lie\u00b7be", "Scho\u00df", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Welt das Heil gebracht,", "tokens": ["Die", "der", "Welt", "das", "Heil", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sei gegr\u00fc\u00dft, geweihte Nacht!", "tokens": ["sei", "ge\u00b7gr\u00fc\u00dft", ",", "ge\u00b7weih\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}