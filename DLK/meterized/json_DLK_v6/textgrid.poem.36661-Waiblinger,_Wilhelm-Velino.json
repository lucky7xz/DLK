{"textgrid.poem.36661": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Velino", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unersch\u00fctterlich gl\u00e4nzt die Schneepyramide zum Himmel,", "tokens": ["Un\u00b7er\u00b7sch\u00fct\u00b7ter\u00b7lich", "gl\u00e4nzt", "die", "Schnee\u00b7py\u00b7ra\u00b7mi\u00b7de", "zum", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "R\u00f6mer kamen, es kam selber der Deutsche, sie blieb!", "tokens": ["R\u00f6\u00b7mer", "ka\u00b7men", ",", "es", "kam", "sel\u00b7ber", "der", "Deut\u00b7sche", ",", "sie", "blieb", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ja, sie ist ewig, und w\u00e4re sie's nicht, sie st\u00fcrzte zusammen,", "tokens": ["Ja", ",", "sie", "ist", "e\u00b7wig", ",", "und", "w\u00e4\u00b7re", "sie's", "nicht", ",", "sie", "st\u00fcrz\u00b7te", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,", "KON", "VAFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als du die Feldschlacht hier, letzter der Staufen, verlorst!", "tokens": ["Als", "du", "die", "Feld\u00b7schlacht", "hier", ",", "letz\u00b7ter", "der", "Stau\u00b7fen", ",", "ver\u00b7lorst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "$,", "ADJA", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}