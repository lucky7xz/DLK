{"textgrid.poem.53192": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Cvpido wollt' erlernen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Cvpido wollt' erlernen", "tokens": ["Cv\u00b7pi\u00b7do", "wollt'", "er\u00b7ler\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "VVINF"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den schnellen Himmels-Lauff,", "tokens": ["Den", "schnel\u00b7len", "Him\u00b7mels\u00b7Lauff", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd sahe nach den Sternen", "tokens": ["Vnd", "sa\u00b7he", "nach", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vmb unsre B\u00f6r\u00df hinauff,", "tokens": ["Vmb", "uns\u00b7re", "B\u00f6r\u00df", "hin\u00b7auff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Indessen wird er innen,", "tokens": ["In\u00b7des\u00b7sen", "wird", "er", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df eine Laute klingt,", "tokens": ["Da\u00df", "ei\u00b7ne", "Lau\u00b7te", "klingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die von der Mauer Zinnen", "tokens": ["Die", "von", "der", "Mau\u00b7er", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihm wo zu Ohren dringt.", "tokens": ["Ihm", "wo", "zu", "Oh\u00b7ren", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PWAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihn trog der Schall von weiten,", "tokens": ["Ihn", "trog", "der", "Schall", "von", "wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Daher er eilends sprach:", "tokens": ["Da\u00b7her", "er", "ei\u00b7lends", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sind Apollos Seiten,", "tokens": ["Das", "sind", "A\u00b7pol\u00b7los", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd flog dem Klange nach", "tokens": ["Vnd", "flog", "dem", "Klan\u00b7ge", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd fand ", "tokens": ["Vnd", "fand"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Vnd mit gelehrter Hand", "tokens": ["Vnd", "mit", "ge\u00b7lehr\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So auff den Seiten blitzen,", "tokens": ["So", "auff", "den", "Sei\u00b7ten", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als er kaum je erkant.", "tokens": ["Als", "er", "kaum", "je", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sang, der Liebe Wesen", "tokens": ["Sie", "sang", ",", "der", "Lie\u00b7be", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4r' ihr durchaus nicht lieb,", "tokens": ["W\u00e4r'", "ihr", "durc\u00b7haus", "nicht", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie h\u00e4tt' ihr Zucht erlesen,", "tokens": ["Sie", "h\u00e4tt'", "ihr", "Zucht", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn nennt sie einen Dieb", "tokens": ["Ihn", "nennt", "sie", "ei\u00b7nen", "Dieb"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd aller Laster Futter,", "tokens": ["Vnd", "al\u00b7ler", "Las\u00b7ter", "Fut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er blend' uns unsern Sinn,", "tokens": ["Er", "blend'", "uns", "un\u00b7sern", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd seine stoltze Mutter", "tokens": ["Vnd", "sei\u00b7ne", "stolt\u00b7ze", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "W\u00e4r eine Z\u00e4uberinn.", "tokens": ["W\u00e4r", "ei\u00b7ne", "Z\u00e4u\u00b7be\u00b7rinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das drang ihm tieff zu Hertzen,", "tokens": ["Das", "drang", "ihm", "tieff", "zu", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kunst beliebt' er sehr,", "tokens": ["Die", "Kunst", "be\u00b7liebt'", "er", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Wort empfand er Schmertzen,", "tokens": ["Der", "Wort", "emp\u00b7fand", "er", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er k\u00e4m' umb Reich und Ehr',", "tokens": ["Er", "k\u00e4m'", "umb", "Reich", "und", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hielt' er es ungerochen,", "tokens": ["Hielt'", "er", "es", "un\u00b7ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr Vnmuth h\u00e4tt' er schier", "tokens": ["F\u00fcr", "Vn\u00b7muth", "h\u00e4tt'", "er", "schier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Liebes-Zeug zerbrochen,", "tokens": ["Den", "Lie\u00b7bes\u00b7Zeug", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weh, zarte Jungfraw, dir!", "tokens": ["Weh", ",", "zar\u00b7te", "Jung\u00b7fraw", ",", "dir", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er spannet seinen Bogen", "tokens": ["Er", "span\u00b7net", "sei\u00b7nen", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd greifft nach einem Pfeil,", "tokens": ["Vnd", "greifft", "nach", "ei\u00b7nem", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den gantz das Gifft bezogen,", "tokens": ["Den", "gantz", "das", "Gifft", "be\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Irrt aber in der Eil,", "tokens": ["Irrt", "a\u00b7ber", "in", "der", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Zucht hatt' ihn geblendet,", "tokens": ["Die", "Zucht", "hatt'", "ihn", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihm den Pfeil beygebracht,", "tokens": ["Ihm", "den", "Pfeil", "bey\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der gantz und gar nicht sch\u00e4ndet", "tokens": ["Der", "gantz", "und", "gar", "nicht", "sch\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "KON", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd s\u00fcsse Liebe macht.", "tokens": ["Vnd", "s\u00fcs\u00b7se", "Lie\u00b7be", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "(seht was die Tugent thut!)", "tokens": ["(", "seht", "was", "die", "Tu\u00b7gent", "thut", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PWS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch \u00fcber alles Hoffen", "tokens": ["Doch", "\u00fc\u00b7ber", "al\u00b7les", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch tieffe Gegen-Glut.", "tokens": ["Durch", "tief\u00b7fe", "Ge\u00b7gen\u00b7Glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Zucht in ihrem Hertzen,", "tokens": ["Die", "Zucht", "in", "ih\u00b7rem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der g\u00fcldnen Demut Schein", "tokens": ["Der", "g\u00fcld\u00b7nen", "De\u00b7mut", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nahm durch weit andre Kertzen", "tokens": ["Nahm", "durch", "weit", "and\u00b7re", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Dich, werther ", "tokens": ["Dich", ",", "wert\u00b7her"], "token_info": ["word", "punct", "word"], "pos": ["PPER", "$,", "PWS"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Sie zwar w\u00e4r' einsam blieben,", "tokens": ["Sie", "zwar", "w\u00e4r'", "ein\u00b7sam", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Tugend wollt' es nicht,", "tokens": ["Die", "Tu\u00b7gend", "wollt'", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat sie so lang getrieben,", "tokens": ["Hat", "sie", "so", "lang", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis sie das JaWort spricht.", "tokens": ["Bis", "sie", "das", "Ja\u00b7Wort", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Itzt kommen sie zusammen,", "tokens": ["Itzt", "kom\u00b7men", "sie", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Nacht ist hoch erfreut,", "tokens": ["Die", "Nacht", "ist", "hoch", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die ihren keuschen Flammen", "tokens": ["Die", "ih\u00b7ren", "keu\u00b7schen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht schlechten Fortgang drewt.", "tokens": ["Nicht", "schlech\u00b7ten", "Fort\u00b7gang", "drewt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Lebt, edles Par, ohn Leiden,", "tokens": ["Lebt", ",", "ed\u00b7les", "Par", ",", "ohn", "Lei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stimmt fort der Lauten Schall,", "tokens": ["Stimmt", "fort", "der", "Lau\u00b7ten", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd lasst in s\u00fcssen Frewden", "tokens": ["Vnd", "lasst", "in", "s\u00fcs\u00b7sen", "Frew\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie klingen \u00fcberall,", "tokens": ["Sie", "klin\u00b7gen", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Besetzt die ", "tokens": ["Be\u00b7setzt", "die"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der sich darinnen kaum", "tokens": ["Der", "sich", "da\u00b7rin\u00b7nen", "kaum"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr Menge k\u00f6nne regen", "tokens": ["F\u00fcr", "Men\u00b7ge", "k\u00f6n\u00b7ne", "re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd such' im Lande Raum.", "tokens": ["Vnd", "such'", "im", "Lan\u00b7de", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du aber, Jugend, \u00fcbe", "tokens": ["Du", "a\u00b7ber", ",", "Ju\u00b7gend", ",", "\u00fc\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stets Zucht und Erbarkeit,", "tokens": ["Stets", "Zucht", "und", "Er\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So hat es mit der Liebe", "tokens": ["So", "hat", "es", "mit", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word