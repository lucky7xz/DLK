{"textgrid.poem.65201": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gezittert hab' ich um dein teures Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gezittert hab' ich um dein teures Leben,", "tokens": ["Ge\u00b7zit\u00b7tert", "hab'", "ich", "um", "dein", "teu\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit der Verzweiflung Aug' in Aug' gerungen,", "tokens": ["Mit", "der", "Ver\u00b7zwei\u00b7flung", "Aug'", "in", "Aug'", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ich, von namenloser Angst durchdrungen,", "tokens": ["Als", "ich", ",", "von", "na\u00b7men\u00b7lo\u00b7ser", "Angst", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich schon im Scheiden w\u00e4hnte und Entschweben.", "tokens": ["Dich", "schon", "im", "Schei\u00b7den", "w\u00e4hn\u00b7te", "und", "Ent\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jetzt, da du wieder mir zur\u00fcckgegeben,", "tokens": ["Jetzt", ",", "da", "du", "wie\u00b7der", "mir", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufjubeln sollte ich mit Flammenzungen!", "tokens": ["Auf\u00b7ju\u00b7beln", "soll\u00b7te", "ich", "mit", "Flam\u00b7men\u00b7zun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann es nicht. Von deinem Arm umschlungen,", "tokens": ["Ich", "kann", "es", "nicht", ".", "Von", "dei\u00b7nem", "Arm", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$.", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fchl' ich die finstre Macht uns doch umweben.", "tokens": ["F\u00fchl'", "ich", "die", "finst\u00b7re", "Macht", "uns", "doch", "um\u00b7we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Fr\u00fch oder sp\u00e4t, die Stunde ", "tokens": ["Fr\u00fch", "o\u00b7der", "sp\u00e4t", ",", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$,", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die, trennend uns mit grimmem Todeshiebe,", "tokens": ["Die", ",", "tren\u00b7nend", "uns", "mit", "grim\u00b7mem", "To\u00b7des\u00b7hie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Seelen Glut dem Staube wird vereinen.", "tokens": ["Der", "See\u00b7len", "Glut", "dem", "Stau\u00b7be", "wird", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und Schauder fa\u00dft mich an vor einer Welt,", "tokens": ["Und", "Schau\u00b7der", "fa\u00dft", "mich", "an", "vor", "ei\u00b7ner", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In der das H\u00f6chste, Heiligste, die Liebe,", "tokens": ["In", "der", "das", "H\u00f6chs\u00b7te", ",", "Hei\u00b7ligs\u00b7te", ",", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "$,", "ADJA", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sowie das Niedrigste dem Nichts verf\u00e4llt.", "tokens": ["So\u00b7wie", "das", "Nied\u00b7rigs\u00b7te", "dem", "Nichts", "ver\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}}}}