{"textgrid.poem.53906": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Horoskop 1928", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich werde, meine Herren Mitchinesen,", "tokens": ["Ich", "wer\u00b7de", ",", "mei\u00b7ne", "Her\u00b7ren", "Mit\u00b7chi\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "euch nunmehr etwas aus eurer Zukunft lesen.", "tokens": ["euch", "nun\u00b7mehr", "et\u00b7was", "aus", "eu\u00b7rer", "Zu\u00b7kunft", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Diesen Goldlack lasse ich von dem Pl\u00e4ttchen aufsaugen \u2013 \u2013 \u2013", "tokens": ["Die\u00b7sen", "Gold\u00b7lack", "las\u00b7se", "ich", "von", "dem", "Pl\u00e4tt\u00b7chen", "auf\u00b7sau\u00b7gen", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$(", "$(", "$("], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und was sehen meine entz\u00fcndeten Augen?", "tokens": ["Und", "was", "se\u00b7hen", "mei\u00b7ne", "ent\u00b7z\u00fcn\u00b7de\u00b7ten", "Au\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Im Jahre 1928 werdet ihr oft zu wiederholten Malen", "tokens": ["Im", "Jah\u00b7re", "19\u00b728", "wer\u00b7det", "ihr", "oft", "zu", "wie\u00b7der\u00b7hol\u00b7ten", "Ma\u00b7len"], "token_info": ["word", "word", "number", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "CARD", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "zehn Prozent Einkommensteuer bezahlen \u2013", "tokens": ["zehn", "Pro\u00b7zent", "Ein\u00b7kom\u00b7men\u00b7steu\u00b7er", "be\u00b7zah\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "aber zum Gl\u00fcck nicht aus eurer Verm\u00f6gensvermehrung,", "tokens": ["a\u00b7ber", "zum", "Gl\u00fcck", "nicht", "aus", "eu\u00b7rer", "Ver\u00b7m\u00f6\u00b7gens\u00b7ver\u00b7meh\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.8": {"text": "sondern nur nach eurer Steuererkl\u00e4rung,", "tokens": ["son\u00b7dern", "nur", "nach", "eu\u00b7rer", "Steu\u00b7er\u00b7er\u00b7kl\u00e4\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Auch werdet ihr durch einen rosa Brief in euern \u00dcberziehertaschen", "tokens": ["Auch", "wer\u00b7det", "ihr", "durch", "ei\u00b7nen", "ro\u00b7sa", "Brief", "in", "eu\u00b7ern", "\u00dc\u00b7ber\u00b7zie\u00b7her\u00b7ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.10": {"text": "eure verehrte Frau Gemahlin auf das erfreulichste \u00fcberraschen,", "tokens": ["eu\u00b7re", "ver\u00b7ehr\u00b7te", "Frau", "Ge\u00b7mah\u00b7lin", "auf", "das", "er\u00b7freu\u00b7lichs\u00b7te", "\u00fc\u00b7berr\u00b7a\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "+--+-+-+-+--+--+-+-", "measure": "iambic.octa.plus.invert"}, "line.11": {"text": "als welche euch, weil sie heftig erregt,", "tokens": ["als", "wel\u00b7che", "euch", ",", "weil", "sie", "hef\u00b7tig", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELS", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "ihren Pantoffel um die Ohren schl\u00e4gt.", "tokens": ["ih\u00b7ren", "Pan\u00b7tof\u00b7fel", "um", "die", "Oh\u00b7ren", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": " sa \u2013 sa \u2013!", "tokens": ["sa", "\u2013", "sa", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["FM.la", "$(", "FM", "$(", "$."], "meter": "OO", "measure": "unknown.measure.zero"}}, "stanza.2": {"line.1": {"text": "Ihr werdet in euern Zeitungen lesen,", "tokens": ["Ihr", "wer\u00b7det", "in", "eu\u00b7ern", "Zei\u00b7tun\u00b7gen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "da\u00df die S\u00fcdarmee wieder siegreich gewesen;", "tokens": ["da\u00df", "die", "S\u00fcd\u00b7ar\u00b7mee", "wie\u00b7der", "sieg\u00b7reich", "ge\u00b7we\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VAPP", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und da\u00df der General Tuang-fu-tscho", "tokens": ["und", "da\u00df", "der", "Ge\u00b7ne\u00b7ral", "Tuang\u00b7fu\u00b7\u00b7ts\u00b7cho"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "besiegte den Kontregeneral Tschung-po-po.", "tokens": ["be\u00b7sieg\u00b7te", "den", "Kont\u00b7re\u00b7ge\u00b7ne\u00b7ral", "Tschung\u00b7po\u00b7po", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Dieses braucht euch aber nicht die Ruhe zu rauben \u2013", "tokens": ["Die\u00b7ses", "braucht", "euch", "a\u00b7ber", "nicht", "die", "Ru\u00b7he", "zu", "rau\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "ihr m\u00fc\u00dft mitnichten alles, was in der Zeitung steht, glauben.", "tokens": ["ihr", "m\u00fc\u00dft", "mit\u00b7nich\u00b7ten", "al\u00b7les", ",", "was", "in", "der", "Zei\u00b7tung", "steht", ",", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Denn dies ist der Zeitung tiefer Sinn:", "tokens": ["Denn", "dies", "ist", "der", "Zei\u00b7tung", "tie\u00b7fer", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "die bessern Sachen stehen nicht drin.", "tokens": ["die", "bes\u00b7sern", "Sa\u00b7chen", "ste\u00b7hen", "nicht", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Sa \u2013 sa \u2013!", "tokens": ["Sa", "\u2013", "sa", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NE", "$(", "FM", "$(", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Ihr werdet ferner, hochverehrte Mitchinesen,", "tokens": ["Ihr", "wer\u00b7det", "fer\u00b7ner", ",", "hoch\u00b7ver\u00b7ehr\u00b7te", "Mit\u00b7chi\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "eines neuen Rundfunksenders genesen.", "tokens": ["ei\u00b7nes", "neu\u00b7en", "Rund\u00b7funk\u00b7sen\u00b7ders", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Daran wird der Oberchinese Al-phred-braun", "tokens": ["Da\u00b7ran", "wird", "der", "O\u00b7ber\u00b7chi\u00b7ne\u00b7se", "Al\u00b7ph\u00b7re\u00b7dbraun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "eure Kinder bet\u00f6ren sowie eure werten Ehefraun.", "tokens": ["eu\u00b7re", "Kin\u00b7der", "be\u00b7t\u00f6\u00b7ren", "so\u00b7wie", "eu\u00b7re", "wer\u00b7ten", "E\u00b7he\u00b7fraun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "In den chinesischen Autobussen werdet ihr gepre\u00dft sein,", "tokens": ["In", "den", "chi\u00b7ne\u00b7si\u00b7schen", "Au\u00b7to\u00b7bus\u00b7sen", "wer\u00b7det", "ihr", "ge\u00b7pre\u00dft", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "und der Reis wird weich, und die B\u00f6rse vorwiegend fest sein.", "tokens": ["und", "der", "Reis", "wird", "weich", ",", "und", "die", "B\u00f6r\u00b7se", "vor\u00b7wie\u00b7gend", "fest", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VVPP", "ADJD", "VAINF", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Toi \u2013 toi \u2013!", "tokens": ["Toi", "\u2013", "toi", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["NE", "$(", "XY", "$(", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Dieses alles ersehe ich aus meinen Zauberger\u00e4ten.", "tokens": ["Die\u00b7ses", "al\u00b7les", "er\u00b7se\u00b7he", "ich", "aus", "mei\u00b7nen", "Zau\u00b7ber\u00b7ge\u00b7r\u00e4\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Zeuchet nunmehr hin und entbrennet die Neujahrsraketen!", "tokens": ["Zeuc\u00b7het", "nun\u00b7mehr", "hin", "und", "ent\u00b7bren\u00b7net", "die", "Neu\u00b7jahrs\u00b7ra\u00b7ke\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+---+--+--++-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Was aber den 2. Januar angeht, so prophezeie ich hier:", "tokens": ["Was", "a\u00b7ber", "den", "2.", "Ja\u00b7nu\u00b7ar", "an\u00b7geht", ",", "so", "pro\u00b7phe\u00b7zei\u00b7e", "ich", "hier", ":"], "token_info": ["word", "word", "word", "ordinal", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+---+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "der Kater ist ein durchaus heiliges Tier.", "tokens": ["der", "Ka\u00b7ter", "ist", "ein", "durc\u00b7haus", "hei\u00b7li\u00b7ges", "Tier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Ich habe schon manchen gesehn, der beneujahrt nach Hause rollte,", "tokens": ["Ich", "ha\u00b7be", "schon", "man\u00b7chen", "ge\u00b7sehn", ",", "der", "be\u00b7neu\u00b7jahrt", "nach", "Hau\u00b7se", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "VVPP", "$,", "PRELS", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+---+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "weil er die ganze Nacht beim sa-cha-rin-sekt tollte,", "tokens": ["weil", "er", "die", "gan\u00b7ze", "Nacht", "beim", "sa\u00b7cha\u00b7rin\u00b7sekt", "toll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "weswegen er dem Weinstubenwirt am n\u00e4chsten Morgen heftig grollte,", "tokens": ["wes\u00b7we\u00b7gen", "er", "dem", "Wein\u00b7stu\u00b7ben\u00b7wirt", "am", "n\u00e4chs\u00b7ten", "Mor\u00b7gen", "hef\u00b7tig", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "ADJD", "ADJA", "$,"], "meter": "-+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "und seine liebe Frau ihm gleich zu Beginn des Jahres schmollte,", "tokens": ["und", "sei\u00b7ne", "lie\u00b7be", "Frau", "ihm", "gleich", "zu", "Be\u00b7ginn", "des", "Jah\u00b7res", "schmoll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "indem sie nicht w\u00fcnschte, da\u00df er sich so zurichten sollte.", "tokens": ["in\u00b7dem", "sie", "nicht", "w\u00fcnschte", ",", "da\u00df", "er", "sich", "so", "zu\u00b7rich\u00b7ten", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Womit ich um ein kleines Trinkgeld oder Douceur gebeten haben wollte.", "tokens": ["Wo\u00b7mit", "ich", "um", "ein", "klei\u00b7nes", "Trink\u00b7geld", "o\u00b7der", "Dou\u00b7ce\u00b7ur", "ge\u00b7be\u00b7ten", "ha\u00b7ben", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "KON", "NN", "VVPP", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}}}}