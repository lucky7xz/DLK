{"dta.poem.20450": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verkehrtes sonnet der schreibe-  \n feder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die kunst ist ohne macht/ wo sich nicht Mars gesellet/", "tokens": ["Die", "kunst", "ist", "oh\u00b7ne", "macht", "/", "wo", "sich", "nicht", "Mars", "ge\u00b7sel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "VVFIN", "$(", "PWAV", "PRF", "PTKNEG", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die feder hat den stahl offt blo\u00df aus furcht verstellet/", "tokens": ["Die", "fe\u00b7der", "hat", "den", "stahl", "offt", "blo\u00df", "aus", "furcht", "ver\u00b7stel\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ob sich itzt ein held auff freye k\u00fcnste leget/", "tokens": ["Und", "ob", "sich", "itzt", "ein", "held", "auff", "frey\u00b7e", "k\u00fcns\u00b7te", "le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ADV", "ART", "NN", "APPR", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ward Germanien noch 1000. mahl so gro\u00df/", "tokens": ["Doch", "ward", "Ger\u00b7ma\u00b7ni\u00b7en", "noch", "1000", ".", "mahl", "so", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "number", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "ADV", "CARD", "$.", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als seiner waffen-plitz das stoltze Rom beweget.", "tokens": ["Als", "sei\u00b7ner", "waf\u00b7fen\u00b7plitz", "das", "stolt\u00b7ze", "Rom", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es sey/ da\u00df ein Virgil sa\u00df in Augustus schoo\u00df/", "tokens": ["Es", "sey", "/", "da\u00df", "ein", "Vir\u00b7gil", "sa\u00df", "in", "Au\u00b7gus\u00b7tus", "schoo\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOUS", "ART", "NE", "VVFIN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.7": {"text": "So stand die majest\u00e4t nur auff der schwerdter pracht.", "tokens": ["So", "stand", "die", "ma\u00b7jes\u00b7t\u00e4t", "nur", "auff", "der", "schwerd\u00b7ter", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df k\u00e4ysers purpur stieg durch gl\u00fccke von den kriegen/", "tokens": ["Di\u00df", "k\u00e4y\u00b7sers", "pur\u00b7pur", "stieg", "durch", "gl\u00fc\u00b7cke", "von", "den", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVFIN", "APPR", "VVFIN", "APPR", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Offt mu\u00df beredsamkeit sich vor dem f\u00fcrsten biegen/", "tokens": ["Offt", "mu\u00df", "be\u00b7red\u00b7sam\u00b7keit", "sich", "vor", "dem", "f\u00fcrs\u00b7ten", "bie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "PRF", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Und ein schlecht feder-kiel wird nicht so gro\u00df geacht/", "tokens": ["Und", "ein", "schlecht", "fe\u00b7der\u00b7kiel", "wird", "nicht", "so", "gro\u00df", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als wenn ein tapffrer muth den feind zu nichte macht/", "tokens": ["Als", "wenn", "ein", "tapf\u00b7frer", "muth", "den", "feind", "zu", "nich\u00b7te", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor dem sein unterthan geb\u00fccket muste liegen/", "tokens": ["Vor", "dem", "sein", "un\u00b7ter\u00b7than", "ge\u00b7b\u00fc\u00b7cket", "mus\u00b7te", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJD", "VVFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der harnisch und der stahl kan ja die welt besiegen/", "tokens": ["Der", "har\u00b7nisch", "und", "der", "stahl", "kan", "ja", "die", "welt", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch di\u00df hat Rom die welt zur dienstbarkeit gebracht;", "tokens": ["Durch", "di\u00df", "hat", "Rom", "die", "welt", "zur", "dienst\u00b7bar\u00b7keit", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VAFIN", "NE", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}