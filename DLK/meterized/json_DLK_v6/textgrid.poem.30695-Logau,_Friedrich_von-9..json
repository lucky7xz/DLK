{"textgrid.poem.30695": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man brennt ietzund viel Hexen; der Teuffel ist gesch\u00e4fftig;", "tokens": ["Man", "brennt", "ie\u00b7tzund", "viel", "He\u00b7xen", ";", "der", "Teuf\u00b7fel", "ist", "ge\u00b7sch\u00e4ff\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIAT", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor waren sie in M\u00e4nnern, sind ietzt in Weibern hefftig.", "tokens": ["Vor", "wa\u00b7ren", "sie", "in", "M\u00e4n\u00b7nern", ",", "sind", "ietzt", "in", "Wei\u00b7bern", "heff\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PPER", "APPR", "NN", "$,", "VAFIN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es tobten sich im Kriege die M\u00e4nner vormals m\u00fcde;", "tokens": ["Es", "tob\u00b7ten", "sich", "im", "Krie\u00b7ge", "die", "M\u00e4n\u00b7ner", "vor\u00b7mals", "m\u00fc\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum halten nun die Teuffel der M\u00e4nner Stell im Friede.", "tokens": ["Drum", "hal\u00b7ten", "nun", "die", "Teuf\u00b7fel", "der", "M\u00e4n\u00b7ner", "Stell", "im", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}