{"dta.poem.636": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sisserahs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich suchte Schutz/ und fand daselbsten mein Verderben/", "tokens": ["Ich", "such\u00b7te", "Schutz", "/", "und", "fand", "da\u00b7selbs\u00b7ten", "mein", "Ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "KON", "VVFIN", "PAV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schuld war mein/ warum? ich hing mich an ein Weib/", "tokens": ["Die", "Schuld", "war", "mein", "/", "wa\u00b7rum", "?", "ich", "hing", "mich", "an", "ein", "Weib", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$(", "PWAV", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer ihren Worten traut/ der t\u00f6dtet seinen Leib/", "tokens": ["Wer", "ih\u00b7ren", "Wor\u00b7ten", "traut", "/", "der", "t\u00f6d\u00b7tet", "sei\u00b7nen", "Leib", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "ART", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mu\u00df/ eh\u2019 er es meynt durch einen Nagel sterben.", "tokens": ["Und", "mu\u00df", "/", "eh'", "er", "es", "meynt", "durch", "ei\u00b7nen", "Na\u00b7gel", "ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "APPR", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}