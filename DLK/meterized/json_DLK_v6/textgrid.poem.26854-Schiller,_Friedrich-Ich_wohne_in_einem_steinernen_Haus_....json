{"textgrid.poem.26854": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ich wohne in einem steinernen Haus ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wohne in einem steinernen Haus,", "tokens": ["Ich", "woh\u00b7ne", "in", "ei\u00b7nem", "stei\u00b7ner\u00b7nen", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da liege ich verborgen und schlafe,", "tokens": ["Da", "lie\u00b7ge", "ich", "ver\u00b7bor\u00b7gen", "und", "schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch ich trete hervor, ich eile heraus,", "tokens": ["Doch", "ich", "tre\u00b7te", "her\u00b7vor", ",", "ich", "ei\u00b7le", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Gefodert mit eiserner Waffe.", "tokens": ["Ge\u00b7fo\u00b7dert", "mit", "ei\u00b7ser\u00b7ner", "Waf\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Erst bin ich unscheinbar und schwach und klein,", "tokens": ["Erst", "bin", "ich", "un\u00b7schein\u00b7bar", "und", "schwach", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich kann dein Atem bezwingen,", "tokens": ["Mich", "kann", "dein", "A\u00b7tem", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ein Regentropfen schon saugt mich ein,", "tokens": ["Ein", "Re\u00b7gen\u00b7trop\u00b7fen", "schon", "saugt", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Doch mir wachsen im Siege die Schwingen,", "tokens": ["Doch", "mir", "wach\u00b7sen", "im", "Sie\u00b7ge", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Wenn die m\u00e4chtige Schwester sich zu mir gesellt,", "tokens": ["Wenn", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Schwes\u00b7ter", "sich", "zu", "mir", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Erwachs ich zum furchtbarn Gebieter der Welt.", "tokens": ["Er\u00b7wachs", "ich", "zum", "furcht\u00b7barn", "Ge\u00b7bie\u00b7ter", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}