{"textgrid.poem.41441": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Natter", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst der L\u00f6we Hochzeit machte,", "tokens": ["Als", "einst", "der", "L\u00f6\u00b7we", "Hoch\u00b7zeit", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kroch zu der neuen K\u00f6nigin", "tokens": ["Kroch", "zu", "der", "neu\u00b7en", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch eine kleine Natter hin,", "tokens": ["Auch", "ei\u00b7ne", "klei\u00b7ne", "Nat\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zum Geschenk die sch\u00f6nste Rose brachte.", "tokens": ["Die", "zum", "Ge\u00b7schenk", "die", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch jene weist sie ab, und spricht:", "tokens": ["Doch", "je\u00b7ne", "weist", "sie", "ab", ",", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich nehme Rosen an; allein von Nattern nicht.", "tokens": ["Ich", "neh\u00b7me", "Ro\u00b7sen", "an", ";", "al\u00b7lein", "von", "Nat\u00b7tern", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$.", "ADV", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}