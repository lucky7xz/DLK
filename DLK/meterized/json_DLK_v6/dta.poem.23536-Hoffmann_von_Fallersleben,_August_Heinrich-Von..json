{"dta.poem.23536": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "An meine Heimath dacht' ich eben,", "tokens": ["An", "mei\u00b7ne", "Hei\u00b7math", "dacht'", "ich", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da schrieb ich mich von Fallersleben.", "tokens": ["Da", "schrieb", "ich", "mich", "von", "Fal\u00b7lers\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schrieb's und dachte nie dabei", "tokens": ["Ich", "schrie\u00b7b's", "und", "dach\u00b7te", "nie", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PAV"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "An Staatscensur und Polizei.", "tokens": ["An", "Staat\u00b7scen\u00b7sur", "und", "Po\u00b7li\u00b7zei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So schrieben sich viel Biederleute", "tokens": ["So", "schrie\u00b7ben", "sich", "viel", "Bie\u00b7der\u00b7leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach ihrem Ort und thun's noch heute,", "tokens": ["Nach", "ih\u00b7rem", "Ort", "und", "thun's", "noch", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und keiner dachte je daran,", "tokens": ["Und", "kei\u00b7ner", "dach\u00b7te", "je", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ", "tokens": ["Durch"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}}}}