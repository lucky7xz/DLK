{"textgrid.poem.31596": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann Vngl\u00fcck noch so offt sich an die Tugend machte /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann Vngl\u00fcck noch so offt sich an die Tugend machte /", "tokens": ["Wann", "Vn\u00b7gl\u00fcck", "noch", "so", "offt", "sich", "an", "die", "Tu\u00b7gend", "mach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADV", "ADV", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So richtet es nichts aus / ja wird von ihr besiegt.", "tokens": ["So", "rich\u00b7tet", "es", "nichts", "aus", "/", "ja", "wird", "von", "ihr", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$(", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Ph\u00f6bus Helden-Hand / die Schlange todt da ligt.", "tokens": ["Von", "Ph\u00f6\u00b7bus", "Hel\u00b7den\u00b7Hand", "/", "die", "Schlan\u00b7ge", "todt", "da", "ligt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$(", "ART", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der dapfre Cadmus auch / den Drachen steiff umbrachte.", "tokens": ["Der", "dapf\u00b7re", "Cad\u00b7mus", "auch", "/", "den", "Dra\u00b7chen", "steiff", "um\u00b7brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$(", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch Perseus sich nicht lang auf seinen Sieg bedachte:", "tokens": ["Auch", "Per\u00b7seus", "sich", "nicht", "lang", "auf", "sei\u00b7nen", "Sieg", "be\u00b7dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PRF", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des G\u00f6ldnen Gartens-Wacht auch bald den Garaus kriegt.", "tokens": ["Des", "G\u00f6ld\u00b7nen", "Gar\u00b7ten\u00b7sWacht", "auch", "bald", "den", "Ga\u00b7raus", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald / auf der Riesen Trutz / ein Pfeil und Keil herfliegt.", "tokens": ["Bald", "/", "auf", "der", "Rie\u00b7sen", "Trutz", "/", "ein", "Pfeil", "und", "Keil", "her\u00b7fliegt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPR", "ART", "NN", "NN", "$(", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Alcidens Hydren-Sieg vor alles hier betrachte.", "tokens": ["Al\u00b7ci\u00b7dens", "Hyd\u00b7ren\u00b7Sieg", "vor", "al\u00b7les", "hier", "be\u00b7trach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was G\u00f6ttlich ist / wie sie / leidt kein Vertilglichkeit.", "tokens": ["Was", "G\u00f6tt\u00b7lich", "ist", "/", "wie", "sie", "/", "leidt", "kein", "Ver\u00b7tilg\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$(", "PWAV", "PPER", "$(", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ihr mu\u00df der Widerstand zu letzt gewonnen geben.", "tokens": ["Ihr", "mu\u00df", "der", "Wi\u00b7der\u00b7stand", "zu", "letzt", "ge\u00b7won\u00b7nen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ADV", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Richt sie auf Erden nichts / so kan sie sich erheben", "tokens": ["Richt", "sie", "auf", "Er\u00b7den", "nichts", "/", "so", "kan", "sie", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PIS", "$(", "ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "zu ihrem obern Zweck / an dem sie allzeit Freud", "tokens": ["zu", "ih\u00b7rem", "o\u00b7bern", "Zweck", "/", "an", "dem", "sie", "all\u00b7zeit", "Freud"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "in allen St\u00fcrmen hat. Er gibt ihr solch Vergn\u00fcgen /", "tokens": ["in", "al\u00b7len", "St\u00fcr\u00b7men", "hat", ".", "Er", "gibt", "ihr", "solch", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "von dem das minste nur / das eusserliche Siegen.", "tokens": ["von", "dem", "das", "mins\u00b7te", "nur", "/", "das", "eus\u00b7ser\u00b7li\u00b7che", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "ADV", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}