{"dta.poem.1003": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Dies leid und diese last: zu bannen               ", "tokens": ["Dies", "leid", "und", "die\u00b7se", "last", ":", "zu", "ban\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADJD", "KON", "PDS", "VVFIN", "$.", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was nah erst war und mein               ", "tokens": ["Was", "nah", "erst", "war", "und", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "ADV", "VAFIN", "KON", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergebliches die arme spannen               ", "tokens": ["Ver\u00b7geb\u00b7li\u00b7ches", "die", "ar\u00b7me", "span\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dem was nur mehr schein             ", "tokens": ["Nach", "dem", "was", "nur", "mehr", "schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PRELS", "ADV", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dies heilungslose sich bet&#228;uben               ", "tokens": ["Dies", "hei\u00b7lungs\u00b7lo\u00b7se", "sich", "bet", "&#228;", "u\u00b7ben"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["PDS", "VVFIN", "PRF", "VVFIN", "$(", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit eitlem nein und kein               ", "tokens": ["Mit", "eit\u00b7lem", "nein", "und", "kein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PTKANT", "KON", "PIAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies unbegr&#252;ndete sich str&#228;uben               ", "tokens": ["Dies", "un\u00b7be\u00b7gr", "&#252;", "nde\u00b7te", "sich", "str", "&#228;", "u\u00b7ben"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["PDS", "ADJD", "$(", "VVFIN", "PRF", "ADJD", "$(", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dies unabwendbar-sein             ", "tokens": ["Dies", "un\u00b7ab\u00b7wend\u00b7ba\u00b7rsein"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Beklemmendes gef&#252;hl der schwere               ", "tokens": ["Be\u00b7klem\u00b7men\u00b7des", "gef", "&#252;", "hl", "der", "schwe\u00b7re"], "token_info": ["word", "word", "XML_entity", "word", "word", "word"], "pos": ["NN", "ADJD", "$(", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf m&#252;d gewordner pein               ", "tokens": ["Auf", "m", "&#252;", "d", "ge\u00b7word\u00b7ner", "pein"], "token_info": ["word", "word", "XML_entity", "word", "word", "word"], "pos": ["APPR", "NE", "$(", "XY", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dann dies dumpfe weh der leere &#8212;               ", "tokens": ["Und", "dann", "dies", "dump\u00b7fe", "weh", "der", "lee\u00b7re", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["KON", "ADV", "PDS", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O dies: mit mir allein.             ", "tokens": ["O", "dies", ":", "mit", "mir", "al\u00b7lein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "$.", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}