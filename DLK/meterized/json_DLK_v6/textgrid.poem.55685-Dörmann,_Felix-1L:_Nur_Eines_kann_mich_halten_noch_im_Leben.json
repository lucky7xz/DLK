{"textgrid.poem.55685": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur Eines kann mich halten noch im Leben", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur Eines kann mich halten noch im Leben", "tokens": ["Nur", "Ei\u00b7nes", "kann", "mich", "hal\u00b7ten", "noch", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kann die dunkle Stunde weiter schieben:", "tokens": ["Und", "kann", "die", "dunk\u00b7le", "Stun\u00b7de", "wei\u00b7ter", "schie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein schrankenloses, weltverge\u00dfnes Lieben", "tokens": ["Ein", "schran\u00b7ken\u00b7lo\u00b7ses", ",", "welt\u00b7ver\u00b7ge\u00df\u00b7nes", "Lie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "PWAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Gegenliebe heischendes Bestreben.", "tokens": ["Und", "Ge\u00b7gen\u00b7lie\u00b7be", "hei\u00b7schen\u00b7des", "Be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn Jeder, der das Dasein will ertragen,", "tokens": ["Denn", "Je\u00b7der", ",", "der", "das", "Da\u00b7sein", "will", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df sich auf irgend eine Art berauschen,", "tokens": ["Mu\u00df", "sich", "auf", "ir\u00b7gend", "ei\u00b7ne", "Art", "be\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um nicht verzweifelnd und entsetzt zu lauschen", "tokens": ["Um", "nicht", "ver\u00b7zwei\u00b7felnd", "und", "ent\u00b7setzt", "zu", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PTKNEG", "VVPP", "KON", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der armen Erde wehevollem Klagen.", "tokens": ["Der", "ar\u00b7men", "Er\u00b7de", "we\u00b7he\u00b7vol\u00b7lem", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dem Einen wird im scharfen, reinsten Denken", "tokens": ["Dem", "Ei\u00b7nen", "wird", "im", "schar\u00b7fen", ",", "reins\u00b7ten", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Sonderdaseins seliges Vergessen,", "tokens": ["Des", "Son\u00b7der\u00b7da\u00b7seins", "se\u00b7li\u00b7ges", "Ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Andrer mu\u00df den Schmerz in Wein ertr\u00e4nken,", "tokens": ["Ein", "A\u00b7ndrer", "mu\u00df", "den", "Schmerz", "in", "Wein", "er\u00b7tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich aber mu\u00df ein trautes Weib umpressen,", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "ein", "trau\u00b7tes", "Weib", "um\u00b7pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In hei\u00dfer Liebe ihm die Seele schenken,", "tokens": ["In", "hei\u00b7\u00dfer", "Lie\u00b7be", "ihm", "die", "See\u00b7le", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nicht, \u2013 so eines Abgrunds Tiefe messen.", "tokens": ["Wenn", "nicht", ",", "\u2013", "so", "ei\u00b7nes", "Ab\u00b7grunds", "Tie\u00b7fe", "mes\u00b7sen", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "$(", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}