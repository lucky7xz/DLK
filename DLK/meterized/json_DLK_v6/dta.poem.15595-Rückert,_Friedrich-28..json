{"dta.poem.15595": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein langentfernter Freund, ein weitgetrennter, kam", "tokens": ["Ein", "lan\u00b7gent\u00b7fern\u00b7ter", "Freund", ",", "ein", "weit\u00b7ge\u00b7trenn\u00b7ter", ",", "kam"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lebhaft mir im Traum, als ich ihn nie vernahm.", "tokens": ["So", "leb\u00b7haft", "mir", "im", "Traum", ",", "als", "ich", "ihn", "nie", "ver\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie freute sich mein Herz, da es ihn wieder fand,", "tokens": ["Wie", "freu\u00b7te", "sich", "mein", "Herz", ",", "da", "es", "ihn", "wie\u00b7der", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den es verloren hatt', und ihn so nah empfand.", "tokens": ["Den", "es", "ver\u00b7lo\u00b7ren", "hatt'", ",", "und", "ihn", "so", "nah", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,", "KON", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nach derselben Nacht, da ich den Freund erworben,", "tokens": ["Doch", "nach", "der\u00b7sel\u00b7ben", "Nacht", ",", "da", "ich", "den", "Freund", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In kurzen Tagen kam die Kund', er sei gestorben.", "tokens": ["In", "kur\u00b7zen", "Ta\u00b7gen", "kam", "die", "Kund'", ",", "er", "sei", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und mu\u00dft' er eben da er neu mir lebte sterben,", "tokens": ["Und", "mu\u00dft'", "er", "e\u00b7ben", "da", "er", "neu", "mir", "leb\u00b7te", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "KOUS", "PPER", "ADJD", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mu\u00dft' ich nur um zu verlieren ihn erwerben?", "tokens": ["Und", "mu\u00dft'", "ich", "nur", "um", "zu", "ver\u00b7lie\u00b7ren", "ihn", "er\u00b7wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "KOUI", "PTKZU", "VVINF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ja, sterben, da\u00df sich mir sein Leben neu geb\u00e4re,", "tokens": ["Ja", ",", "ster\u00b7ben", ",", "da\u00df", "sich", "mir", "sein", "Le\u00b7ben", "neu", "ge\u00b7b\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "KOUS", "PRF", "PPER", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Er nicht, von Zeit und Raum geschieden, todt mir w\u00e4re.", "tokens": ["Er", "nicht", ",", "von", "Zeit", "und", "Raum", "ge\u00b7schie\u00b7den", ",", "todt", "mir", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}