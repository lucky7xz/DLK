{"textgrid.poem.60165": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von deinem besten Freund begleitet,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von deinem besten Freund begleitet,", "tokens": ["Von", "dei\u00b7nem", "bes\u00b7ten", "Freund", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchwandelst du das Feld voll Saat,", "tokens": ["Durch\u00b7wan\u00b7delst", "du", "das", "Feld", "voll", "Saat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und findest Seegen da verbreitet,", "tokens": ["Und", "fin\u00b7dest", "See\u00b7gen", "da", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin das Pferd des Kriegers trat!", "tokens": ["Wo\u00b7hin", "das", "Pferd", "des", "Krie\u00b7gers", "trat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dir giebt die Heerde Milch und Wolle;", "tokens": ["Dir", "giebt", "die", "Heer\u00b7de", "Milch", "und", "Wol\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Obstbaum zinset dir genug", "tokens": ["Der", "Obst\u00b7baum", "zin\u00b7set", "dir", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schmackhafte Fr\u00fcchte, die der volle", "tokens": ["Schmack\u00b7haf\u00b7te", "Fr\u00fcch\u00b7te", ",", "die", "der", "vol\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herabgezogne Wipfel trug!", "tokens": ["Her\u00b7ab\u00b7ge\u00b7zog\u00b7ne", "Wip\u00b7fel", "trug", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Um dich versammlen sich die Tauben!", "tokens": ["Um", "dich", "ver\u00b7samm\u00b7len", "sich", "die", "Tau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sperling scheuet deinen Blick,", "tokens": ["Der", "Sper\u00b7ling", "scheu\u00b7et", "dei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergi\u00dft dein Weitzenkorn zu rauben", "tokens": ["Ver\u00b7gi\u00dft", "dein", "Weit\u00b7zen\u00b7korn", "zu", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und flattert wie besch\u00e4mt, zur\u00fcck!", "tokens": ["Und", "flat\u00b7tert", "wie", "be\u00b7sch\u00e4mt", ",", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJD", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein J\u00e4ger bringt viel aufgehangne", "tokens": ["Dein", "J\u00e4\u00b7ger", "bringt", "viel", "auf\u00b7ge\u00b7hang\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu blind gewesne V\u00f6gel dir.", "tokens": ["Zu", "blind", "ge\u00b7wes\u00b7ne", "V\u00f6\u00b7gel", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wurden ihrer Lust Gefangne;", "tokens": ["Sie", "wur\u00b7den", "ih\u00b7rer", "Lust", "Ge\u00b7fang\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie, nur zu oft der S\u00fcnde, wir!", "tokens": ["Wie", ",", "nur", "zu", "oft", "der", "S\u00fcn\u00b7de", ",", "wir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "PTKA", "ADV", "ART", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die V\u00f6gel, in geschlo\u00dfnen Heeren", "tokens": ["Die", "V\u00f6\u00b7gel", ",", "in", "ge\u00b7schlo\u00df\u00b7nen", "Hee\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verlassen ihr bewohntes Land", "tokens": ["Ver\u00b7las\u00b7sen", "ihr", "be\u00b7wohn\u00b7tes", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von der Natur gef\u00fchrt, und kehren", "tokens": ["Von", "der", "Na\u00b7tur", "ge\u00b7f\u00fchrt", ",", "und", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur\u00fcck, an ihrer vollen Hand!", "tokens": ["Zu\u00b7r\u00fcck", ",", "an", "ih\u00b7rer", "vol\u00b7len", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Herbst und die gelinden Weste", "tokens": ["Der", "Herbst", "und", "die", "ge\u00b7lin\u00b7den", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entfliehn von uns, und auf der Flur", "tokens": ["Ent\u00b7fliehn", "von", "uns", ",", "und", "auf", "der", "Flur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stehn hier und dort noch Ueberreste", "tokens": ["Stehn", "hier", "und", "dort", "noch", "Ue\u00b7ber\u00b7res\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom gr\u00fcnen Kleide der Natur!", "tokens": ["Vom", "gr\u00fc\u00b7nen", "Klei\u00b7de", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In Schneegew\u00f6lke tief verh\u00fcllet", "tokens": ["In", "Schnee\u00b7ge\u00b7w\u00f6l\u00b7ke", "tief", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt der betr\u00fcbte Wintertag;", "tokens": ["Kommt", "der", "be\u00b7tr\u00fcb\u00b7te", "Win\u00b7ter\u00b7tag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Nordwind wirbelt sich und br\u00fcllet,", "tokens": ["Der", "Nord\u00b7wind", "wir\u00b7belt", "sich", "und", "br\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Durch Mauren, wie ein Donnerschlag!", "tokens": ["Durch", "Mau\u00b7ren", ",", "wie", "ein", "Don\u00b7ner\u00b7schlag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dich aber w\u00e4rmt die trockne Fichte", "tokens": ["Dich", "a\u00b7ber", "w\u00e4rmt", "die", "trock\u00b7ne", "Fich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Herauf geflammet im Camin;", "tokens": ["Her\u00b7auf", "ge\u00b7flam\u00b7met", "im", "Ca\u00b7min", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du h\u00f6rest h\u00e4usliche Berichte,", "tokens": ["Du", "h\u00f6\u00b7rest", "h\u00e4us\u00b7li\u00b7che", "Be\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und giebst Gesch\u00e4fte zu vollziehn!", "tokens": ["Und", "giebst", "Ge\u00b7sch\u00e4f\u00b7te", "zu", "voll\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Nachbarinnen Wagen rollen!", "tokens": ["Der", "Nach\u00b7ba\u00b7rin\u00b7nen", "Wa\u00b7gen", "rol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ganzer Crey\u00df kommt zum Besuch!", "tokens": ["Ein", "gan\u00b7zer", "Crey\u00df", "kommt", "zum", "Be\u00b7such", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Izt fr\u00e4gst du, ob sie h\u00f6ren wollen", "tokens": ["Izt", "fr\u00e4gst", "du", ",", "ob", "sie", "h\u00f6\u00b7ren", "wol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4hlst aus meinem Liederbuch", "tokens": ["Und", "w\u00e4hlst", "aus", "mei\u00b7nem", "Lie\u00b7der\u00b7buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ges\u00e4nge, dem gedacht zur Ehre,", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", ",", "dem", "ge\u00b7dacht", "zur", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der aus dem Staube mich erhob;", "tokens": ["Der", "aus", "dem", "Stau\u00b7be", "mich", "er\u00b7hob", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alles wird um dich Geh\u00f6re,", "tokens": ["Und", "al\u00b7les", "wird", "um", "dich", "Ge\u00b7h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Thr\u00e4nen reden Gottes Lob!", "tokens": ["Und", "Thr\u00e4\u00b7nen", "re\u00b7den", "Got\u00b7tes", "Lob", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}