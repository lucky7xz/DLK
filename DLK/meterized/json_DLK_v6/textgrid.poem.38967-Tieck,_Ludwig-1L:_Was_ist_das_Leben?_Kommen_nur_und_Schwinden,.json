{"textgrid.poem.38967": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist das Leben? Kommen nur und Schwinden,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist das Leben? Kommen nur und Schwinden,", "tokens": ["Was", "ist", "das", "Le\u00b7ben", "?", "Kom\u00b7men", "nur", "und", "Schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wechsel nur von Nacht und Tageshelle,", "tokens": ["Ein", "Wech\u00b7sel", "nur", "von", "Nacht", "und", "Ta\u00b7ges\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verlust und Schmerz, Sehnsucht und Wiederfinden,", "tokens": ["Ver\u00b7lust", "und", "Schmerz", ",", "Sehn\u00b7sucht", "und", "Wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So schwebt durch Traum und Wachen hin die Welle, \u2013", "tokens": ["So", "schwebt", "durch", "Traum", "und", "Wa\u00b7chen", "hin", "die", "Wel\u00b7le", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum l\u00e4chelt hoffend in der Trennung Wehen,", "tokens": ["Drum", "l\u00e4\u00b7chelt", "hof\u00b7fend", "in", "der", "Tren\u00b7nung", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch Abschiedsthr\u00e4nen schon das Wiedersehen.", "tokens": ["Durch", "Ab\u00b7schiedst\u00b7hr\u00e4\u00b7nen", "schon", "das", "Wie\u00b7der\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}