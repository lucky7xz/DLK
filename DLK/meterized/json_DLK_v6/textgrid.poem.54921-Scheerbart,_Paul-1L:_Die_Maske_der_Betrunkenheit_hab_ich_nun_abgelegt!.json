{"textgrid.poem.54921": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Maske der Betrunkenheit hab ich nun abgelegt!", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Maske der Betrunkenheit hab ich nun abgelegt!", "tokens": ["Die", "Mas\u00b7ke", "der", "Be\u00b7trun\u00b7ken\u00b7heit", "hab", "ich", "nun", "ab\u00b7ge\u00b7legt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ich bin allein \u2013 und tue, was ich wollte.", "tokens": ["Ich", "bin", "al\u00b7lein", "\u2013", "und", "tue", ",", "was", "ich", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "KON", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wer jemals \u00fcber Albernes sich kindlich aufgeregt,", "tokens": ["Wer", "je\u00b7mals", "\u00fc\u00b7ber", "Al\u00b7ber\u00b7nes", "sich", "kind\u00b7lich", "auf\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Der wei\u00df nun endlich, da\u00df ich stets ihm grollte.", "tokens": ["Der", "wei\u00df", "nun", "end\u00b7lich", ",", "da\u00df", "ich", "stets", "ihm", "groll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich l\u00e4chle nur und l\u00e4chle immer wieder \u2013 wieder!", "tokens": ["Ich", "l\u00e4ch\u00b7le", "nur", "und", "l\u00e4ch\u00b7le", "im\u00b7mer", "wie\u00b7der", "\u2013", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "ADV", "$(", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir h\u00e4ngt die Luft voll kreischend-toller Jubellieder!", "tokens": ["Mir", "h\u00e4ngt", "die", "Luft", "voll", "krei\u00b7schen\u00b7dtol\u00b7ler", "Ju\u00b7bel\u00b7lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}