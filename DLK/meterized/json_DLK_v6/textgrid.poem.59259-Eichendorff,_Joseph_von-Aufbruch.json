{"textgrid.poem.59259": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Aufbruch", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Silbern Str\u00f6me ziehn herunter,", "tokens": ["Sil\u00b7bern", "Str\u00f6\u00b7me", "ziehn", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen schwanken fern und nah,", "tokens": ["Blu\u00b7men", "schwan\u00b7ken", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringsum regt sich's bunt und bunter \u2013", "tokens": ["Ring\u00b7sum", "regt", "sich's", "bunt", "und", "bun\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lenz! bist du schon wieder da?", "tokens": ["Lenz", "!", "bist", "du", "schon", "wie\u00b7der", "da", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbreiter sind's, die blitzend ziehen,", "tokens": ["\u00bb", "rei\u00b7ter", "sin\u00b7d's", ",", "die", "blit\u00b7zend", "zie\u00b7hen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wieviel gl\u00e4nz'ger Str\u00f6me Lauf,", "tokens": ["Wie\u00b7viel", "gl\u00e4nz'\u00b7ger", "Str\u00f6\u00b7me", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Fahnen, liliengleich, erbl\u00fchen,", "tokens": ["Fah\u00b7nen", ",", "li\u00b7li\u00b7en\u00b7gleich", ",", "er\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "VVINF", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lerchenwirbel, Trommelwirbel", "tokens": ["Ler\u00b7chen\u00b7wir\u00b7bel", ",", "Trom\u00b7mel\u00b7wir\u00b7bel"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wecken rings den Fr\u00fchling auf.\u00ab", "tokens": ["We\u00b7cken", "rings", "den", "Fr\u00fch\u00b7ling", "auf", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Horch! was h\u00f6r ich drau\u00dfen klingen", "tokens": ["Horch", "!", "was", "h\u00f6r", "ich", "drau\u00b7\u00dfen", "klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild verlockend wie zur Jagd?", "tokens": ["Wild", "ver\u00b7lo\u00b7ckend", "wie", "zur", "Jagd", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, das Herz m\u00f6cht mir zerspringen,", "tokens": ["Ach", ",", "das", "Herz", "m\u00f6cht", "mir", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es jauchzt und weint und klagt.", "tokens": ["Wie", "es", "jauchzt", "und", "weint", "und", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbund in Waldes gr\u00fcnen Hallen,", "tokens": ["\u00bb", "und", "in", "Wal\u00b7des", "gr\u00fc\u00b7nen", "Hal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefe Schauer in der Brust,", "tokens": ["Tie\u00b7fe", "Schau\u00b7er", "in", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lassen wir die H\u00f6rner schallen,", "tokens": ["Las\u00b7sen", "wir", "die", "H\u00f6r\u00b7ner", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Blau die Stimmen hallen,", "tokens": ["In", "das", "Blau", "die", "Stim\u00b7men", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So zum Schrecken wie zur Lust.\u00ab", "tokens": ["So", "zum", "Schre\u00b7cken", "wie", "zur", "Lust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "KOKOM", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wehe! dunkle Wolken decken", "tokens": ["We\u00b7he", "!", "dunk\u00b7le", "Wol\u00b7ken", "de\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh ich all die junge Pracht,", "tokens": ["Seh", "ich", "all", "die", "jun\u00b7ge", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Feur'ge Todeszungen strecken", "tokens": ["Feur'\u00b7ge", "To\u00b7des\u00b7zun\u00b7gen", "stre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die grimme Wetternacht.", "tokens": ["Durch", "die", "grim\u00b7me", "Wet\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbwettern gleich bl\u00fcht Kampfesf\u00fclle,", "tokens": ["\u00bb", "wet\u00b7tern", "gleich", "bl\u00fcht", "Kamp\u00b7fes\u00b7f\u00fcl\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitze zieht das gute Schwert,", "tokens": ["Blit\u00b7ze", "zieht", "das", "gu\u00b7te", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mancher wird auf ewig stille \u2013", "tokens": ["Man\u00b7cher", "wird", "auf", "e\u00b7wig", "stil\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr Gott, es gescheh Dein Wille!", "tokens": ["Herr", "Gott", ",", "es", "ge\u00b7scheh", "Dein", "Wil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Blast Trompeten! Frisch mein Pferd!\u00ab", "tokens": ["Blast", "Trom\u00b7pe\u00b7ten", "!", "Frisch", "mein", "Pferd", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "$.", "NE", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Regenbogen seh ich steigen,", "tokens": ["Re\u00b7gen\u00b7bo\u00b7gen", "seh", "ich", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von Tr\u00e4nen spr\u00fchn die Au,", "tokens": ["Wie", "von", "Tr\u00e4\u00b7nen", "spr\u00fchn", "die", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenen sich erbarmend neigen", "tokens": ["Je\u00b7nen", "sich", "er\u00b7bar\u00b7mend", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PRF", "VVPP", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber den verweinten Gau.", "tokens": ["\u00dc\u00b7ber", "den", "ver\u00b7wein\u00b7ten", "Gau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbalso \u00fcber Graus und Wogen,", "tokens": ["\u00bb", "al\u00b7so", "\u00fc\u00b7ber", "Graus", "und", "Wo\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der Vater gnadenreich", "tokens": ["Hat", "der", "Va\u00b7ter", "gna\u00b7den\u00b7reich"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Triumphtor still gezogen.", "tokens": ["Ein", "Tri\u00b7umph\u00b7tor", "still", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer da f\u00e4llt, zieht durch den Bogen", "tokens": ["Wer", "da", "f\u00e4llt", ",", "zieht", "durch", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heim ins ew'ge Himmelreich.\u00ab", "tokens": ["Heim", "ins", "ew'\u00b7ge", "Him\u00b7mel\u00b7reich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}