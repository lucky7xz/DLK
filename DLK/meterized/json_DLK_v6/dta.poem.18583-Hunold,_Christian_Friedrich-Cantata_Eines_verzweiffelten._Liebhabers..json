{"dta.poem.18583": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Cantata  \n Eines verzweiffelten. Liebhabers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Hertze brich entzwey/", "tokens": ["Mein", "Hert\u00b7ze", "brich", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ende meine Schmertzen.", "tokens": ["Und", "en\u00b7de", "mei\u00b7ne", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach komm gew\u00fcnschter Tod!", "tokens": ["Ach", "komm", "ge\u00b7w\u00fcnschter", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Vers\u00fcsse meine Noth/", "tokens": ["Ver\u00b7s\u00fcs\u00b7se", "mei\u00b7ne", "Noth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und lesche nur die matten Lebens-Kertzen.", "tokens": ["Und", "le\u00b7sche", "nur", "die", "mat\u00b7ten", "Le\u00b7bens\u00b7Kert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Hertze brich entzwey/", "tokens": ["Mein", "Hert\u00b7ze", "brich", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ende meine Schmertzen.", "tokens": ["Und", "en\u00b7de", "mei\u00b7ne", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Komm Tod und strecke mich", "tokens": ["Komm", "Tod", "und", "stre\u00b7cke", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nur auff das Ruhe-Bette/", "tokens": ["Nur", "auff", "das", "Ru\u00b7he\u00b7Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Komm doch was s\u00e4umst du dich/", "tokens": ["Komm", "doch", "was", "s\u00e4umst", "du", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PWS", "VVFIN", "PPER", "PRF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zerbrich die Lebens-Kette!", "tokens": ["Zer\u00b7brich", "die", "Le\u00b7bens\u00b7Ket\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zerreisse meine Brust!", "tokens": ["Zer\u00b7reis\u00b7se", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und auch das Bild/ so mich darinnen qv\u00e4let:", "tokens": ["Und", "auch", "das", "Bild", "/", "so", "mich", "da\u00b7rin\u00b7nen", "qv\u00e4\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$(", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dem Hencker ist die Marter nicht bewust/", "tokens": ["Dem", "Hen\u00b7cker", "ist", "die", "Mar\u00b7ter", "nicht", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Damit sie mich nur halb nicht gantz entseelet.", "tokens": ["Da\u00b7mit", "sie", "mich", "nur", "halb", "nicht", "gantz", "ent\u00b7see\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ach! Himmel ach! erkalte doch mein Blut", "tokens": ["Ach", "!", "Him\u00b7mel", "ach", "!", "er\u00b7kal\u00b7te", "doch", "mein", "Blut"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "NN", "ITJ", "$.", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und auch die heisse Glut/", "tokens": ["Und", "auch", "die", "heis\u00b7se", "Glut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die sich in allen ", "tokens": ["Die", "sich", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PIAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Damit die Qvaal ihr Ende sp\u00fchret", "tokens": ["Da\u00b7mit", "die", "Qvaal", "ihr", "En\u00b7de", "sp\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Beraube mir die Sinnen/", "tokens": ["Be\u00b7rau\u00b7be", "mir", "die", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Da\u00df sie nicht mehr bezaubert sind.", "tokens": ["Da\u00df", "sie", "nicht", "mehr", "be\u00b7zau\u00b7bert", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ach Armer! Ach/ was wilst du noch beginnen/", "tokens": ["Ach", "Ar\u00b7mer", "!", "Ach", "/", "was", "wilst", "du", "noch", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ITJ", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dein W\u00fcnschen geht im Wind.", "tokens": ["Dein", "W\u00fcn\u00b7schen", "geht", "im", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Drum \u00f6ffne dich du H\u00f6llen-Schlund!", "tokens": ["Drum", "\u00f6ff\u00b7ne", "dich", "du", "H\u00f6l\u00b7len\u00b7Sch\u00b7lund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und schlucke meine Pein", "tokens": ["Und", "schlu\u00b7cke", "mei\u00b7ne", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Nur in dich ein!", "tokens": ["Nur", "in", "dich", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Jedoch mein Seufftzen wird den tauben L\u00fcfften kun", "tokens": ["Je\u00b7doch", "mein", "Seufft\u00b7zen", "wird", "den", "tau\u00b7ben", "L\u00fcff\u00b7ten", "kun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Himmel/ Erde/ Tod und H\u00f6lle", "tokens": ["Him\u00b7mel", "/", "Er\u00b7de", "/", "Tod", "und", "H\u00f6l\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt mich ungl\u00fcckselgen nicht.", "tokens": ["H\u00f6rt", "mich", "un\u00b7gl\u00fcck\u00b7sel\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich liebe/ wil mich hassen/", "tokens": ["Was", "ich", "lie\u00b7be", "/", "wil", "mich", "has\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "VMFIN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich suche/ mu\u00df ich lassen/", "tokens": ["Was", "ich", "su\u00b7che", "/", "mu\u00df", "ich", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein hartes Schicksal spricht:", "tokens": ["Und", "mein", "har\u00b7tes", "Schick\u00b7sal", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Andre schertzen/", "tokens": ["And\u00b7re", "schert\u00b7zen", "/"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Nur die Schmertzen", "tokens": ["Nur", "die", "Schmert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sind allein auff dich gericht.", "tokens": ["Sind", "al\u00b7lein", "auff", "dich", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Himmel/ Erde/ Tod und H\u00f6lle/", "tokens": ["Him\u00b7mel", "/", "Er\u00b7de", "/", "Tod", "und", "H\u00f6l\u00b7le", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "H\u00f6rt mich Ungl\u00fcckselgen nicht.", "tokens": ["H\u00f6rt", "mich", "Un\u00b7gl\u00fcck\u00b7sel\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach Grausame! doch auch mein Leben!", "tokens": ["Ach", "Grau\u00b7sa\u00b7me", "!", "doch", "auch", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich darzu versehn?", "tokens": ["Bin", "ich", "dar\u00b7zu", "ver\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir meine Brust zur Sclaverey zu geben", "tokens": ["Dir", "mei\u00b7ne", "Brust", "zur", "Scla\u00b7ve\u00b7rey", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So dencke nur/ ich bin dir noch verbunden:", "tokens": ["So", "den\u00b7cke", "nur", "/", "ich", "bin", "dir", "noch", "ver\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich durch diese Wunden", "tokens": ["Wenn", "ich", "durch", "die\u00b7se", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur darff zum Sterben gehn.", "tokens": ["Nur", "darff", "zum", "Ster\u00b7ben", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Stirbt mein Geist durch dein Verlangen/", "tokens": ["Stirbt", "mein", "Geist", "durch", "dein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach so stirbt er wunder-sch\u00f6n.", "tokens": ["Ach", "so", "stirbt", "er", "wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word",