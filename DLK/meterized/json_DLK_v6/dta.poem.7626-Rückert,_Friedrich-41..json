{"dta.poem.7626": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Unvollkommenheit der Welt hat zu beklagen,", "tokens": ["Die", "Un\u00b7voll\u00b7kom\u00b7men\u00b7heit", "der", "Welt", "hat", "zu", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sie geschaffen glaubt zur Lust und zum Behagen.", "tokens": ["Wer", "sie", "ge\u00b7schaf\u00b7fen", "glaubt", "zur", "Lust", "und", "zum", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VVFIN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geschaffen ist sie wol zu anderem Bedarf,", "tokens": ["Ge\u00b7schaf\u00b7fen", "ist", "sie", "wol", "zu", "an\u00b7de\u00b7rem", "Be\u00b7darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "PTKA", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie der f\u00fcr gut befand, der so den Plan entwarf,", "tokens": ["Wie", "der", "f\u00fcr", "gut", "be\u00b7fand", ",", "der", "so", "den", "Plan", "ent\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "APPR", "ADJD", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu seinem nicht, und nicht zu unserem Vergn\u00fcgen,", "tokens": ["Zu", "sei\u00b7nem", "nicht", ",", "und", "nicht", "zu", "un\u00b7se\u00b7rem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PTKNEG", "$,", "KON", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu unserm Heil gewis; darein mu\u00dft du dich f\u00fcgen.", "tokens": ["Zu", "un\u00b7serm", "Heil", "ge\u00b7wis", ";", "da\u00b7rein", "mu\u00dft", "du", "dich", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$.", "PAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}