{"textgrid.poem.41368": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der kr\u00e4nke Hirsch und die W\u00f6lfe", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hirsch, der sich nicht wohl befand,", "tokens": ["Ein", "Hirsch", ",", "der", "sich", "nicht", "wohl", "be\u00b7fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blieb lange Zeit daheim, die Ballen auszuheilen,", "tokens": ["Blieb", "lan\u00b7ge", "Zeit", "da\u00b7heim", ",", "die", "Bal\u00b7len", "aus\u00b7zu\u00b7hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jeder Freund kam angerannt,", "tokens": ["Und", "je\u00b7der", "Freund", "kam", "an\u00b7ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm Trost und Beirath mitzutheilen.", "tokens": ["Ihm", "Trost", "und", "Bei\u00b7rath", "mit\u00b7zu\u00b7thei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gesellschaft pfleget zu erfreun:", "tokens": ["Ge\u00b7sell\u00b7schaft", "pfle\u00b7get", "zu", "er\u00b7freun", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum stellten sich am zw\u00f6lften Tage", "tokens": ["Drum", "stell\u00b7ten", "sich", "am", "zw\u00f6lf\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zween W\u00f6lfe voller Mitleid ein,", "tokens": ["Zween", "W\u00f6l\u00b7fe", "vol\u00b7ler", "Mit\u00b7leid", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jeder kam mit dieser Frage:", "tokens": ["Und", "je\u00b7der", "kam", "mit", "die\u00b7ser", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie mag es mit dem Kranken sein,", "tokens": ["Wie", "mag", "es", "mit", "dem", "Kran\u00b7ken", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den ich gewi\u00df recht sehr beklage?", "tokens": ["Den", "ich", "ge\u00b7wi\u00df", "recht", "sehr", "be\u00b7kla\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hat man auf ihn geh\u00f6rig Acht?", "tokens": ["Hat", "man", "auf", "ihn", "ge\u00b7h\u00f6\u00b7rig", "Acht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "PPER", "ADJD", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist's gut, so eng' ihn einzusperren?", "tokens": ["Ist's", "gut", ",", "so", "eng'", "ihn", "ein\u00b7zu\u00b7sper\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADV", "VVFIN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wie stund's mit ihm die vor'ge Nacht?", "tokens": ["Wie", "stun\u00b7d's", "mit", "ihm", "die", "vor'\u00b7ge", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Das Hirschkalb sagte mit Bedacht:", "tokens": ["Das", "Hirschkalb", "sag\u00b7te", "mit", "Be\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Viel besser, als ihr's w\u00fcnscht, ihr Herren.", "tokens": ["Viel", "bes\u00b7ser", ",", "als", "ih\u00b7r's", "w\u00fcnscht", ",", "ihr", "Her\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}