{"dta.poem.20854": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Gedenck ich ohngefehr/ da\u00df ich einmal geschifft/", "tokens": ["Ge\u00b7denck", "ich", "ohn\u00b7ge\u00b7fehr", "/", "da\u00df", "ich", "ein\u00b7mal", "ge\u00b7schifft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird der schnelle Geistden Wolcken zugelencket/", "tokens": ["So", "wird", "der", "schnel\u00b7le", "Geist\u00b7den", "Wol\u00b7cken", "zu\u00b7ge\u00b7len\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der geringste Zeug/ so diese Kunst betrifft/", "tokens": ["Und", "der", "ge\u00b7rings\u00b7te", "Zeug", "/", "so", "die\u00b7se", "Kunst", "be\u00b7tr\u00b7ifft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "ADV", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Macht da\u00df man alsobald an Mast und Seegel dencket.", "tokens": ["Macht", "da\u00df", "man", "al\u00b7so\u00b7bald", "an", "Mast", "und", "See\u00b7gel", "den\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}