{"textgrid.poem.25492": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein S\u00e4nger, der dich herzlich liebt,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein S\u00e4nger, der dich herzlich liebt,", "tokens": ["Ein", "S\u00e4n\u00b7ger", ",", "der", "dich", "herz\u00b7lich", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schickt mich, dir etwas vorzusingen;", "tokens": ["Schickt", "mich", ",", "dir", "et\u00b7was", "vor\u00b7zu\u00b7sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was k\u00f6nnt' er dir auch be\u00dfres bringen,", "tokens": ["Was", "k\u00f6nnt'", "er", "dir", "auch", "be\u00df\u00b7res", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als wenn er dir sein Liebstes gibt?", "tokens": ["Als", "wenn", "er", "dir", "sein", "Liebs\u00b7tes", "gibt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er streute mir mit eigner Hand", "tokens": ["Er", "streu\u00b7te", "mir", "mit", "eig\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die K\u00f6rner auf des K\u00e4fichs Boden,", "tokens": ["Die", "K\u00f6r\u00b7ner", "auf", "des", "K\u00e4\u00b7fichs", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fcr die Lieder und die Oden,", "tokens": ["Und", "f\u00fcr", "die", "Lie\u00b7der", "und", "die", "O\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ich in meiner Zell' erfand,", "tokens": ["Die", "ich", "in", "mei\u00b7ner", "Zell'", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Gab er mir Zucker, den das Land", "tokens": ["Gab", "er", "mir", "Zu\u00b7cker", ",", "den", "das", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Erzeugt, aus dem mein Ahnherr stammte,", "tokens": ["Er\u00b7zeugt", ",", "aus", "dem", "mein", "Ahn\u00b7herr", "stamm\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Statt da\u00df des Gebers Vaterland", "tokens": ["Statt", "da\u00df", "des", "Ge\u00b7bers", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Oft S\u00e4nger, die es g\u00f6ttlich fand,", "tokens": ["Oft", "S\u00e4n\u00b7ger", ",", "die", "es", "g\u00f6tt\u00b7lich", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zu Wasser und zu Brod verdammte,", "tokens": ["Zu", "Was\u00b7ser", "und", "zu", "Brod", "ver\u00b7damm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wohl aber mit freigeb'ger Hand", "tokens": ["Wohl", "a\u00b7ber", "mit", "frei\u00b7ge\u00b7b'\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Um ihre Stirne Lorbeern wand.", "tokens": ["Um", "ih\u00b7re", "Stir\u00b7ne", "Lor\u00b7beern", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die S\u00e4nger wohnen gern in Lauben,", "tokens": ["Die", "S\u00e4n\u00b7ger", "woh\u00b7nen", "gern", "in", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Die hat mein Herr auch mir gebaut,", "tokens": ["Die", "hat", "mein", "Herr", "auch", "mir", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Um aus dem frischen Vogelkraut,", "tokens": ["Um", "aus", "dem", "fri\u00b7schen", "Vo\u00b7gel\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wie er die Beeren aus den Trauben,", "tokens": ["Wie", "er", "die", "Bee\u00b7ren", "aus", "den", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die Bl\u00fctenf\u00e4den auszuklauben,", "tokens": ["Die", "Bl\u00fc\u00b7ten\u00b7f\u00e4\u00b7den", "aus\u00b7zu\u00b7klau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wobei er freundlich zugeschaut.", "tokens": ["Wo\u00b7bei", "er", "freund\u00b7lich", "zu\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Er, der die Freiheit selber liebt,", "tokens": ["Er", ",", "der", "die", "Frei\u00b7heit", "sel\u00b7ber", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Von einem Sklaven nichts mag hoffen,", "tokens": ["Von", "ei\u00b7nem", "Skla\u00b7ven", "nichts", "mag", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Lie\u00df Tage lang mein Pf\u00f6rtchen offen.", "tokens": ["Lie\u00df", "Ta\u00b7ge", "lang", "mein", "Pf\u00f6rt\u00b7chen", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wenn mich hinaus die Neugier trieb,", "tokens": ["Wenn", "mich", "hin\u00b7aus", "die", "Neu\u00b7gier", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APZR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Pickt' ich an Gl\u00e4sern und an Tassen,", "tokens": ["Pickt'", "ich", "an", "Gl\u00e4\u00b7sern", "und", "an", "Tas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Allein ich hatt' ihn viel zu lieb,", "tokens": ["Al\u00b7lein", "ich", "hatt'", "ihn", "viel", "zu", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Um undankbar ihn zu verlassen.", "tokens": ["Um", "un\u00b7dank\u00b7bar", "ihn", "zu", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Schwer, liebe Frau, kommt mir es an,", "tokens": ["Schwer", ",", "lie\u00b7be", "Frau", ",", "kommt", "mir", "es", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.30": {"text": "Von meinem guten Herrn zu scheiden,", "tokens": ["Von", "mei\u00b7nem", "gu\u00b7ten", "Herrn", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Doch wenn ich deines Alters Freuden", "tokens": ["Doch", "wenn", "ich", "dei\u00b7nes", "Al\u00b7ters", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Durch meinen Sang vermehren kann:", "tokens": ["Durch", "mei\u00b7nen", "Sang", "ver\u00b7meh\u00b7ren", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wohl! so vermehr' ich sie Euch beiden.", "tokens": ["Wohl", "!", "so", "ver\u00b7mehr'", "ich", "sie", "Euch", "bei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "PPER", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}