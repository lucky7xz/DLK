{"textgrid.poem.24626": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey einem vergn\u00fcgten Hochzeit-Feste", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn unter allen wohl und den Gl\u00fcckseeligkeiten/", "tokens": ["Wenn", "un\u00b7ter", "al\u00b7len", "wohl", "und", "den", "Gl\u00fcck\u00b7see\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "ADV", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So man auf Erden hat/ das beste dieses ist/", "tokens": ["So", "man", "auf", "Er\u00b7den", "hat", "/", "das", "bes\u00b7te", "die\u00b7ses", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "NN", "VAFIN", "$(", "ART", "ADJA", "PDS", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu lieben was uns liebt/ gew\u00fcnscht zur Ehe schreiten/", "tokens": ["Zu", "lie\u00b7ben", "was", "uns", "liebt", "/", "ge\u00b7w\u00fcnscht", "zur", "E\u00b7he", "schrei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PWS", "PPER", "VVFIN", "$(", "VVPP", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo sich die Tugend so/ als die Vergn\u00fcgung k\u00fc\u00dft:", "tokens": ["Wo", "sich", "die", "Tu\u00b7gend", "so", "/", "als", "die", "Ver\u00b7gn\u00fc\u00b7gung", "k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "ADV", "$(", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So wei\u00df ich/ da\u00df kein Schatz vom irdischem Vergn\u00fcgen/", "tokens": ["So", "wei\u00df", "ich", "/", "da\u00df", "kein", "Schatz", "vom", "ir\u00b7di\u00b7schem", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PIAT", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Demselben gleichen kan/ so itzt dem edlen Paar", "tokens": ["Dem\u00b7sel\u00b7ben", "glei\u00b7chen", "kan", "/", "so", "itzt", "dem", "ed\u00b7len", "Paar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "VMFIN", "$(", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In seine Seele fleu\u00dft/ in seinem Arm wird liegen/", "tokens": ["In", "sei\u00b7ne", "See\u00b7le", "fleu\u00dft", "/", "in", "sei\u00b7nem", "Arm", "wird", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und das den Anfang nimt/ so bald es Sternen klar.", "tokens": ["Und", "das", "den", "An\u00b7fang", "nimt", "/", "so", "bald", "es", "Ster\u00b7nen", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "NN", "VVFIN", "$(", "ADV", "ADV", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So wei\u00df ich auch zugleich/ da\u00df/ weil der Eltern Freude/", "tokens": ["So", "wei\u00df", "ich", "auch", "zu\u00b7gleich", "/", "da\u00df", "/", "weil", "der", "El\u00b7tern", "Freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "$(", "KOUS", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch seiner Kinder-Wohl zum h\u00f6chsten Gipffel steigt/", "tokens": ["Durch", "sei\u00b7ner", "Kin\u00b7der\u00b7Wohl", "zum", "h\u00f6chs\u00b7ten", "Gipf\u00b7fel", "steigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Des Edlen Vaters-Hertz anitzo \u00fcber beyde", "tokens": ["Des", "Ed\u00b7len", "Va\u00b7ter\u00b7sHertz", "a\u00b7nit\u00b7zo", "\u00fc\u00b7ber", "bey\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich so gl\u00fcckseelig prei\u00dft/ als wie vergn\u00fcgt bezeugt.", "tokens": ["Sich", "so", "gl\u00fcck\u00b7see\u00b7lig", "prei\u00dft", "/", "als", "wie", "ver\u00b7gn\u00fcgt", "be\u00b7zeugt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVFIN", "$(", "KOUS", "KOKOM", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er kan die Fr\u00fcchte nun von seiner sorgfalt brechen.", "tokens": ["Er", "kan", "die", "Fr\u00fcch\u00b7te", "nun", "von", "sei\u00b7ner", "sorg\u00b7falt", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Jungfer Tochter ziert des klugen Vaters-Zucht/", "tokens": ["Die", "Jung\u00b7fer", "Toch\u00b7ter", "ziert", "des", "klu\u00b7gen", "Va\u00b7ter\u00b7sZucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Tugend soll vor mich das Wort am besten sprechen;", "tokens": ["Die", "Tu\u00b7gend", "soll", "vor", "mich", "das", "Wort", "am", "bes\u00b7ten", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPER", "ART", "NN", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Herr \u2013 \u2013 \u2013 rede selbst/ warum er sie gesucht.", "tokens": ["Herr", "\u2013", "\u2013", "\u2013", "re\u00b7de", "selbst", "/", "wa\u00b7rum", "er", "sie", "ge\u00b7sucht", "."], "token_info": ["word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "$(", "$(", "VVFIN", "ADV", "$(", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "Der Wohlverdiente Mann wird zu sich selber sagen:", "tokens": ["Der", "Wohl\u00b7ver\u00b7dien\u00b7te", "Mann", "wird", "zu", "sich", "sel\u00b7ber", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df Anmuht und Verstand/ ein l\u00f6bliches Gem\u00fcht/", "tokens": ["Da\u00df", "An\u00b7muht", "und", "Ver\u00b7stand", "/", "ein", "l\u00f6b\u00b7li\u00b7ches", "Ge\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als wie ein Schatz bey ihm das Hertz davon getragen/", "tokens": ["Als", "wie", "ein", "Schatz", "bey", "ihm", "das", "Hertz", "da\u00b7von", "ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "PPER", "ART", "NN", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als ein ", "tokens": ["Als", "ein"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Ein solches Kleinod ist bey wenigen finden:", "tokens": ["Ein", "sol\u00b7ches", "Klei\u00b7nod", "ist", "bey", "we\u00b7ni\u00b7gen", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Wo Tugend ausgesteurt/ ist oft die Sch\u00f6nheit nicht;", "tokens": ["Wo", "Tu\u00b7gend", "aus\u00b7ge\u00b7steurt", "/", "ist", "oft", "die", "Sch\u00f6n\u00b7heit", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$(", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wo aber Sch\u00f6nheit reitzt/ oft kein Verstand zu gr\u00fcnden:", "tokens": ["Wo", "a\u00b7ber", "Sch\u00f6n\u00b7heit", "reitzt", "/", "oft", "kein", "Ver\u00b7stand", "zu", "gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "$(", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was aber nutzt ein Haus/ das sch\u00f6n und ohne Licht?", "tokens": ["Was", "a\u00b7ber", "nutzt", "ein", "Haus", "/", "das", "sch\u00f6n", "und", "oh\u00b7ne", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "NN", "$(", "ART", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bey einer dummen der Mann im finstern wohnen.", "tokens": ["Bey", "ei\u00b7ner", "dum\u00b7men", "der", "Mann", "im", "fins\u00b7tern", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "APPRART", "VVINF", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Unedle Sch\u00f6nheit ist ein solches heyraths-Gut/", "tokens": ["U\u00b7ned\u00b7le", "Sch\u00f6n\u00b7heit", "ist", "ein", "sol\u00b7ches", "hey\u00b7raths\u00b7Gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "PIAT", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "Das ohne Ruh und Lust bey mehr als tausend Cronen", "tokens": ["Das", "oh\u00b7ne", "Ruh", "und", "Lust", "bey", "mehr", "als", "tau\u00b7send", "Cro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "APPR", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dem Mann im Ehestand gar schlechten Vortheil thut.", "tokens": ["Dem", "Mann", "im", "E\u00b7hes\u00b7tand", "gar", "schlech\u00b7ten", "Vor\u00b7theil", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wo aber Tugenden in reicher Anmuht lachen/", "tokens": ["Wo", "a\u00b7ber", "Tu\u00b7gen\u00b7den", "in", "rei\u00b7cher", "An\u00b7muht", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wo ein so kluger Geist/ wie bey der Edlen Braut/", "tokens": ["Wo", "ein", "so", "klu\u00b7ger", "Geist", "/", "wie", "bey", "der", "Ed\u00b7len", "Braut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADV", "ADJA", "NN", "$(", "KOKOM", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da kan Herr \u2013 \u2013 \u2013 vergn\u00fcgte Hochzeit machen/", "tokens": ["Da", "kan", "Herr", "\u2013", "\u2013", "\u2013", "ver\u00b7gn\u00fcg\u00b7te", "Hoch\u00b7zeit", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "$(", "$(", "$(", "ADJA", "NN", "VVINF", "$("], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.32": {"text": "Da hat man seine Lust auf sch\u00f6nen Grund gebaut.", "tokens": ["Da", "hat", "man", "sei\u00b7ne", "Lust", "auf", "sch\u00f6\u00b7nen", "Grund", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Bey andern sey der Tag ein Anfang schlimmer Zeiten/", "tokens": ["Bey", "an\u00b7dern", "sey", "der", "Tag", "ein", "An\u00b7fang", "schlim\u00b7mer", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da man an eine Frau sich ewig binden l\u00e4st.", "tokens": ["Da", "man", "an", "ei\u00b7ne", "Frau", "sich", "e\u00b7wig", "bin\u00b7den", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "PRF", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Allhier verk\u00fcndigen der Braut Vortrefflichkeiten/", "tokens": ["All\u00b7hier", "ver\u00b7k\u00fcn\u00b7di\u00b7gen", "der", "Braut", "Vor\u00b7treff\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Den s\u00fc\u00dfen Lebens-Lauf/ auf dieses Hochzeit-Fest.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Le\u00b7bens\u00b7Lauf", "/", "auf", "die\u00b7ses", "Hoch\u00b7zeit\u00b7Fest", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie wird Ihm Ihre Gunst stets abzumahlen wissen/", "tokens": ["Sie", "wird", "Ihm", "Ih\u00b7re", "Gunst", "stets", "ab\u00b7zu\u00b7mah\u00b7len", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "VVIZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Weil die geschickte Hand die Mahlerey versteht;", "tokens": ["Weil", "die", "ge\u00b7schick\u00b7te", "Hand", "die", "Mah\u00b7le\u00b7rey", "ver\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bi\u00df sie durch seine H\u00fclf ein Bildni\u00df abgerissen/", "tokens": ["Bi\u00df", "sie", "durch", "sei\u00b7ne", "H\u00fclf", "ein", "Bild\u00b7ni\u00df", "ab\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das ihnen mit der Zeit gleicht und entgegen geht.", "tokens": ["Das", "ih\u00b7nen", "mit", "der", "Zeit", "gleicht", "und", "ent\u00b7ge\u00b7gen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ART", "NN", "VVFIN", "KON", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.41": {"text": "Den Kummer/ wenn er auch in dem gew\u00fcnschten Stande", "tokens": ["Den", "Kum\u00b7mer", "/", "wenn", "er", "auch", "in", "dem", "ge\u00b7w\u00fcnschten", "Stan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Zu finden 