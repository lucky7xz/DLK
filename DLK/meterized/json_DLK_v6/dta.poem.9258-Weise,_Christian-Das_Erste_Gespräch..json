{"dta.poem.9258": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Erste Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ach du unverhoffte stunde!", "tokens": ["Ach", "du", "un\u00b7ver\u00b7hoff\u00b7te", "stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringstu nun den herben schlu\u00df/", "tokens": ["Brings\u00b7tu", "nun", "den", "her\u00b7ben", "schlu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich aussen mit dem munde", "tokens": ["Da\u00df", "ich", "aus\u00b7sen", "mit", "dem", "mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zu was versprechen mu\u00df/", "tokens": ["Mich", "zu", "was", "ver\u00b7spre\u00b7chen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da mein hertz in furcht und list", "tokens": ["Da", "mein", "hertz", "in", "furcht", "und", "list"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weit davon entfernet ist.", "tokens": ["Weit", "da\u00b7von", "ent\u00b7fer\u00b7net", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Steht es oben angeschrieben?", "tokens": ["Steht", "es", "o\u00b7ben", "an\u00b7ge\u00b7schrie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und befihlt der himmel wol/", "tokens": ["Und", "be\u00b7fihlt", "der", "him\u00b7mel", "wol", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df man ein gem\u00fcthe lieben", "tokens": ["Da\u00df", "man", "ein", "ge\u00b7m\u00fc\u00b7the", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein anders hassen soll?", "tokens": ["Und", "ein", "an\u00b7ders", "has\u00b7sen", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder ist der s\u00fcsse stand", "tokens": ["O\u00b7der", "ist", "der", "s\u00fcs\u00b7se", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur ein blosser menschen-tand?", "tokens": ["Nur", "ein", "blos\u00b7ser", "men\u00b7schen\u00b7tand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ach ich zweiffle da\u00df von oben", "tokens": ["Ach", "ich", "zweiff\u00b7le", "da\u00df", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "KOUS", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solcher zwang entspringen kan.", "tokens": ["Sol\u00b7cher", "zwang", "ent\u00b7sprin\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die sterne was verloben", "tokens": ["Wenn", "die", "ster\u00b7ne", "was", "ver\u00b7lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Locken sie die sehnsucht an/", "tokens": ["Lo\u00b7cken", "sie", "die", "sehn\u00b7sucht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df die lieb in rath und that", "tokens": ["Da\u00df", "die", "lieb", "in", "rath", "und", "that"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "APPR", "NN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen gleichen willen hat.", "tokens": ["Ei\u00b7nen", "glei\u00b7chen", "wil\u00b7len", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Aber hier wil man verbinden", "tokens": ["A\u00b7ber", "hier", "wil", "man", "ver\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Was sich nicht zusammen schickt/", "tokens": ["Was", "sich", "nicht", "zu\u00b7sam\u00b7men", "schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich sol eine lust empfinden/", "tokens": ["Ich", "sol", "ei\u00b7ne", "lust", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche mich im hertzen dr\u00fcckt:", "tokens": ["Wel\u00b7che", "mich", "im", "hert\u00b7zen", "dr\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und das absehn meiner treu", "tokens": ["Und", "das", "ab\u00b7sehn", "mei\u00b7ner", "treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist umsonst und geht vorbey.", "tokens": ["Ist", "um\u00b7sonst", "und", "geht", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Was ich liebe sol ich hassen.", "tokens": ["Was", "ich", "lie\u00b7be", "sol", "ich", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich hasse f\u00e4llt mir zu/", "tokens": ["Was", "ich", "has\u00b7se", "f\u00e4llt", "mir", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "PPER", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich halte sol ich lassen/", "tokens": ["Was", "ich", "hal\u00b7te", "sol", "ich", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein verdru\u00df soll meine ruh", "tokens": ["Mein", "ver\u00b7dru\u00df", "soll", "mei\u00b7ne", "ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine gunst soll meine pein", "tokens": ["Mei\u00b7ne", "gunst", "soll", "mei\u00b7ne", "pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein todt mein leben seyn.", "tokens": ["Und", "mein", "todt", "mein", "le\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "PPOSAT", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Doch was wil ich weiter sagen", "tokens": ["Doch", "was", "wil", "ich", "wei\u00b7ter", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun es nicht zu \u00e4ndern steht/", "tokens": ["Nun", "es", "nicht", "zu", "\u00e4n\u00b7dern", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was sol ich ferner klagen", "tokens": ["Und", "was", "sol", "ich", "fer\u00b7ner", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es geht schon wie es geht:", "tokens": ["Denn", "es", "geht", "schon", "wie", "es", "geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KOKOM", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hab ich selbsten keine schuld", "tokens": ["Hab", "ich", "selbs\u00b7ten", "kei\u00b7ne", "schuld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun so leid ichs mit gedult.", "tokens": ["Nun", "so", "leid", "ichs", "mit", "ge\u00b7dult", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "7. Zwar wenn ich die lust zu b\u00fcssen", "tokens": ["Zwar", "wenn", "ich", "die", "lust", "zu", "b\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werde zu derselben gehn/", "tokens": ["Wer\u00b7de", "zu", "der\u00b7sel\u00b7ben", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich mir die augen schliessen", "tokens": ["Will", "ich", "mir", "die", "au\u00b7gen", "schlies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in den gedancken stehn/", "tokens": ["Und", "in", "den", "ge\u00b7dan\u00b7cken", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df dieselbe/ die mich k\u00fcst/", "tokens": ["Da\u00df", "die\u00b7sel\u00b7be", "/", "die", "mich", "k\u00fcst", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein erw\u00fcnschtes liebgen ist.", "tokens": ["Mein", "er\u00b7w\u00fcnschtes", "lieb\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}