{"textgrid.poem.49202": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "[19.]", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Corydon sprach mit Verlangen", "tokens": ["Co\u00b7ry\u00b7don", "sprach", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der liebsten Feldg\u00f6ttin:", "tokens": ["Zu", "der", "liebs\u00b7ten", "Feld\u00b7g\u00f6t\u00b7tin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--", "measure": "anapaest.init"}, "line.3": {"text": "Wer' es, Lydia, dein Sinn,", "tokens": ["Wer'", "es", ",", "Ly\u00b7dia", ",", "dein", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da\u00df du woltest mich umfangen,", "tokens": ["Da\u00df", "du", "wol\u00b7test", "mich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wir m\u00f6chten noch in Freud'", "tokens": ["Da\u00df", "wir", "m\u00f6ch\u00b7ten", "noch", "in", "Freud'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit?", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles Wildni\u00df in den W\u00e4ldern", "tokens": ["Al\u00b7les", "Wild\u00b7ni\u00df", "in", "den", "W\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmeckt die s\u00fc\u00dfe Liebeskost;", "tokens": ["Schmeckt", "die", "s\u00fc\u00b7\u00dfe", "Lie\u00b7bes\u00b7kost", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es gebrauchen sich der Lust", "tokens": ["Es", "ge\u00b7brau\u00b7chen", "sich", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herd' und Hirten auf den Feldern;", "tokens": ["Herd'", "und", "Hir\u00b7ten", "auf", "den", "Fel\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollen wir dann ohne Freud'", "tokens": ["Wol\u00b7len", "wir", "dann", "oh\u00b7ne", "Freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit?", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alle V\u00f6gel in den L\u00fcften", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt man singen f\u00fcr und f\u00fcr,", "tokens": ["H\u00f6rt", "man", "sin\u00b7gen", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Nymphen da und hier", "tokens": ["Al\u00b7le", "Nym\u00b7phen", "da", "und", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "KON", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sieht man neue Heirath stiften;", "tokens": ["Sieht", "man", "neu\u00b7e", "Hei\u00b7rath", "stif\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, la\u00df uns doch auch in Freud'", "tokens": ["Ei", ",", "la\u00df", "uns", "doch", "auch", "in", "Freud'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit.", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar, der Wahrheit nicht zu schonen,", "tokens": ["Zwar", ",", "der", "Wahr\u00b7heit", "nicht", "zu", "scho\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich bin nur ein Bauerknecht,", "tokens": ["Ich", "bin", "nur", "ein", "Bau\u00b7er\u00b7knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch noch eins so fromm und recht,", "tokens": ["Doch", "noch", "eins", "so", "fromm", "und", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die in den St\u00e4dten wohnen;", "tokens": ["Als", "die", "in", "den", "St\u00e4d\u00b7ten", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum so la\u00df uns doch in Freud'", "tokens": ["Drum", "so", "la\u00df", "uns", "doch", "in", "Freud'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVIMP", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit.", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df gar wol deine Sinnen,", "tokens": ["Ich", "wei\u00df", "gar", "wol", "dei\u00b7ne", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Du vermeinst, es were Kunst,", "tokens": ["Du", "ver\u00b7meinst", ",", "es", "we\u00b7re", "Kunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann du mich durch Liebesbrunst", "tokens": ["Wann", "du", "mich", "durch", "Lie\u00b7bes\u00b7brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrdest ganz verzehren k\u00f6nnen;", "tokens": ["W\u00fcr\u00b7dest", "ganz", "ver\u00b7zeh\u00b7ren", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum solln wir ohne Freud'", "tokens": ["Da\u00b7rum", "solln", "wir", "oh\u00b7ne", "Freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit.", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nymphe, wilst du mir entgehen,", "tokens": ["Nym\u00b7phe", ",", "wilst", "du", "mir", "ent\u00b7ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil ich nur vom Dorfe bin?", "tokens": ["Weil", "ich", "nur", "vom", "Dor\u00b7fe", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau auf alle G\u00f6tter hin,", "tokens": ["Schau", "auf", "al\u00b7le", "G\u00f6t\u00b7ter", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die nach Baurenliebe stehen;", "tokens": ["Die", "nach", "Bau\u00b7ren\u00b7lie\u00b7be", "ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnen wir dann ohne Freud'", "tokens": ["K\u00f6n\u00b7nen", "wir", "dann", "oh\u00b7ne", "Freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit?", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Venus hat vielmal geschlafen", "tokens": ["Ve\u00b7nus", "hat", "viel\u00b7mal", "ge\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Bei Adonis in dem Wald,", "tokens": ["Bei", "A\u00b7do\u00b7nis", "in", "dem", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob gleich schon sein Aufenthalt", "tokens": ["Ob", "gleich", "schon", "sein", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Nirgend war als bei den Schafen:", "tokens": ["Nir\u00b7gend", "war", "als", "bei", "den", "Scha\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir nur wollen ohne Freud'", "tokens": ["Wir", "nur", "wol\u00b7len", "oh\u00b7ne", "Freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit.", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch ich wil mich nicht betr\u00fcben,", "tokens": ["Doch", "ich", "wil", "mich", "nicht", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich begehr' es nicht so sehr;", "tokens": ["Ich", "be\u00b7gehr'", "es", "nicht", "so", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber denke, wer dich mehr", "tokens": ["A\u00b7ber", "den\u00b7ke", ",", "wer", "dich", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird als ich so heftig lieben,", "tokens": ["Wird", "als", "ich", "so", "hef\u00b7tig", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann wir jetzund ohne Freud'", "tokens": ["Wann", "wir", "je\u00b7tzund", "oh\u00b7ne", "Freud'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Enden unsre junge Zeit.", "tokens": ["En\u00b7den", "uns\u00b7re", "jun\u00b7ge", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}