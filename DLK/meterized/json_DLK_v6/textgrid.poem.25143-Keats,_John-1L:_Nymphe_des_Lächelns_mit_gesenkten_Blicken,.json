{"textgrid.poem.25143": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nymphe des L\u00e4chelns mit gesenkten Blicken,", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nymphe des L\u00e4chelns mit gesenkten Blicken,", "tokens": ["Nym\u00b7phe", "des", "L\u00e4\u00b7chelns", "mit", "ge\u00b7senk\u00b7ten", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "In welchen glanzverkl\u00e4rten Tagesstunden", "tokens": ["In", "wel\u00b7chen", "glanz\u00b7ver\u00b7kl\u00e4r\u00b7ten", "Ta\u00b7gess\u00b7tun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sei deiner Lieblichkeit der Kranz gewunden:", "tokens": ["Sei", "dei\u00b7ner", "Lieb\u00b7lich\u00b7keit", "der", "Kranz", "ge\u00b7wun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn s\u00fc\u00dfe wirre Reden dich verstricken \u2013", "tokens": ["Wenn", "s\u00fc\u00b7\u00dfe", "wir\u00b7re", "Re\u00b7den", "dich", "ver\u00b7stri\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn du in himmelheiterem Verz\u00fccken", "tokens": ["Wenn", "du", "in", "him\u00b7mel\u00b7hei\u00b7te\u00b7rem", "Ver\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedanken lebst \u2013 wenn du so ungebunden", "tokens": ["Ge\u00b7dan\u00b7ken", "lebst", "\u2013", "wenn", "du", "so", "un\u00b7ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hintanzest durch des Gartens Sonnenstunden", "tokens": ["Hin\u00b7tan\u00b7zest", "durch", "des", "Gar\u00b7tens", "Son\u00b7nen\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hundert Blumen dir Willkommen nicken?", "tokens": ["Und", "hun\u00b7dert", "Blu\u00b7men", "dir", "Will\u00b7kom\u00b7men", "ni\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oder wenn du gebannt in s\u00fc\u00dfem Lauschen", "tokens": ["O\u00b7der", "wenn", "du", "ge\u00b7bannt", "in", "s\u00fc\u00b7\u00dfem", "Lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Rosenlippen teilst? \u2013 Wie darf ich fragen!", "tokens": ["Die", "Ro\u00b7sen\u00b7lip\u00b7pen", "teilst", "?", "\u2013", "Wie", "darf", "ich", "fra\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Sch\u00f6nstes gegen Sch\u00f6nstes umzutauschen,", "tokens": ["Ein", "Sch\u00f6ns\u00b7tes", "ge\u00b7gen", "Sch\u00f6ns\u00b7tes", "um\u00b7zu\u00b7tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "War Torheit nur. Ich k\u00f6nnte dann auch sagen,", "tokens": ["War", "Tor\u00b7heit", "nur", ".", "Ich", "k\u00f6nn\u00b7te", "dann", "auch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welche der Grazien in Apolls Geleit", "tokens": ["Wel\u00b7che", "der", "Gra\u00b7zi\u00b7en", "in", "A\u00b7polls", "Ge\u00b7leit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "NE", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die erste sei an holder Lieblichkeit.", "tokens": ["Die", "ers\u00b7te", "sei", "an", "hol\u00b7der", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}