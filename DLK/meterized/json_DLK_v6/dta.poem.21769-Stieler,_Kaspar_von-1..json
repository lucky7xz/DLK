{"dta.poem.21769": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df uns/ Kind/ der Jugend brauchen/", "tokens": ["La\u00df", "uns", "/", "Kind", "/", "der", "Ju\u00b7gend", "brau\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "weil uns noch die Sch\u00f6nheit bl\u00fcht:", "tokens": ["weil", "uns", "noch", "die", "Sch\u00f6n\u00b7heit", "bl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Geister einst verrauchen", "tokens": ["Wenn", "die", "Geis\u00b7ter", "einst", "ver\u00b7rau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und die Todten-farb\u2019 umzieht", "tokens": ["und", "die", "Tod\u00b7ten\u00b7fa\u00b7rb'", "um\u00b7zieht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "unser runzlichtes Gesichte:", "tokens": ["un\u00b7ser", "runz\u00b7lich\u00b7tes", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer begehrt denn unsern Ku\u00df?", "tokens": ["Wer", "be\u00b7gehrt", "denn", "un\u00b7sern", "Ku\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimm sie an der Rosen Fr\u00fcchte/", "tokens": ["Nimm", "sie", "an", "der", "Ro\u00b7sen", "Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "eh ihr Blat verwelken mu\u00df.", "tokens": ["eh", "ihr", "Blat", "ver\u00b7wel\u00b7ken", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob die Alten murrisch zanken/", "tokens": ["Ob", "die", "Al\u00b7ten", "mur\u00b7risch", "zan\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nehmen sie der Freude wahr;", "tokens": ["neh\u00b7men", "sie", "der", "Freu\u00b7de", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "mu\u00df man drum mit ihnen krankken?", "tokens": ["mu\u00df", "man", "drum", "mit", "ih\u00b7nen", "krank\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PAV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein/ ich acht\u2019 es nicht ein Haar.", "tokens": ["Nein", "/", "ich", "acht'", "es", "nicht", "ein", "Haar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollte der mich Sitten lehren/", "tokens": ["Soll\u00b7te", "der", "mich", "Sit\u00b7ten", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "PPER", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der bereit hat au\u00dfgelehrt?", "tokens": ["der", "be\u00b7reit", "hat", "au\u00df\u00b7ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn werd\u2019 ich mich auch bekehren/", "tokens": ["Denn", "werd'", "ich", "mich", "auch", "be\u00b7keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$("], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "wenn mein Alter sich verkehrt.", "tokens": ["wenn", "mein", "Al\u00b7ter", "sich", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die bes\u00fc\u00dften Fr\u00fchlings-tage", "tokens": ["Die", "be\u00b7s\u00fc\u00df\u00b7ten", "Fr\u00fch\u00b7lings\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "lauffen fl\u00fcgel-schnelle fort/", "tokens": ["lauf\u00b7fen", "fl\u00fc\u00b7gel\u00b7schnel\u00b7le", "fort", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "denn so hilft uns keine Klage/", "tokens": ["denn", "so", "hilft", "uns", "kei\u00b7ne", "Kla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kein er seufzend Bitte-wort/", "tokens": ["kein", "er", "seuf\u00b7zend", "Bit\u00b7te\u00b7wort", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPER", "ADJD", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was hin ist/ das bleibet hin.", "tokens": ["Was", "hin", "ist", "/", "das", "blei\u00b7bet", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "$(", "PDS", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Di\u00df beruht auff einem Blikke/", "tokens": ["Di\u00df", "be\u00b7ruht", "auff", "ei\u00b7nem", "Blik\u00b7ke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "da\u00df ich froh und traurig bin.", "tokens": ["da\u00df", "ich", "froh", "und", "trau\u00b7rig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum so brauch/ mein Kind/ der Zeiten/", "tokens": ["Drum", "so", "brauch", "/", "mein", "Kind", "/", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "weil die Zeiten gr\u00fcnend sein.", "tokens": ["weil", "die", "Zei\u00b7ten", "gr\u00fc\u00b7nend", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was uns bleibt sind Traurigkeiten/", "tokens": ["Was", "uns", "bleibt", "sind", "Trau\u00b7rig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VAFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "gehn uns diese Zeiten ein.", "tokens": ["gehn", "uns", "die\u00b7se", "Zei\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey wie pl\u00f6tzlich k\u00f6mmt die Stunde/", "tokens": ["Ey", "wie", "pl\u00f6tz\u00b7lich", "k\u00f6mmt", "die", "Stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da\u00df uns Kloto in der Eil", "tokens": ["da\u00df", "uns", "Klo\u00b7to", "in", "der", "Eil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "durch des Todes Frevel-Pfeil.", "tokens": ["durch", "des", "To\u00b7des", "Fre\u00b7vel\u00b7Pfeil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So sey mit den Scharlachs-Wangen/", "tokens": ["So", "sey", "mit", "den", "Scha\u00b7rlachs\u00b7Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sch\u00f6ne/ ferner nicht zu teur/", "tokens": ["Sch\u00f6\u00b7ne", "/", "fer\u00b7ner", "nicht", "zu", "teur", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "PTKNEG", "PTKA", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Linder meiner Qwaal Verlangen/", "tokens": ["Lin\u00b7der", "mei\u00b7ner", "Qwaal", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchl\u2019/ ach", "tokens": ["K\u00fchl'", "/", "ach"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$(", "XY"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wo von den bes\u00fcssten Fluhten/", "tokens": ["Wo", "von", "den", "be\u00b7s\u00fcs\u00b7sten", "Fluh\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "deines Zukker-M\u00fcndgens Na\u00df/", "tokens": ["dei\u00b7nes", "Zuk\u00b7ker\u00b7M\u00fcnd\u00b7gens", "Na\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "mir kein Tau ist zuvermuhten", "tokens": ["mir", "kein", "Tau", "ist", "zu\u00b7ver\u00b7muh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "werd\u2019 ich noch vor Abends bla\u00df.", "tokens": ["werd'", "ich", "noch", "vor", "A\u00b7bends", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Gib zwey K\u00fc\u00dfchen/ gib mir eines", "tokens": ["Gib", "zwey", "K\u00fc\u00df\u00b7chen", "/", "gib", "mir", "ei\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "CARD", "NN", "$(", "VVIMP", "PPER", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gib/ mein Schazz/ mir nur nicht keines/", "tokens": ["gib", "/", "mein", "Schazz", "/", "mir", "nur", "nicht", "kei\u00b7nes", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PPOSAT", "NN", "$(", "PPER", "ADV", "PTKNEG", "PIS", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wiltu mich dem Todten-schrein\u2019", "tokens": ["wil\u00b7tu", "mich", "dem", "Tod\u00b7ten\u00b7schrein'"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "auff ein wenigs noch ersparen.", "tokens": ["auff", "ein", "we\u00b7nigs", "noch", "er\u00b7spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was nuzzt denn ein kalter Ku\u00df", "tokens": ["Was", "nuzzt", "denn", "ein", "kal\u00b7ter", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "wenn ich auff der Leichen-Baaren", "tokens": ["wenn", "ich", "auff", "der", "Lei\u00b7chen\u00b7Baa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "deiner Reu erst warten mu\u00df?", "tokens": ["dei\u00b7ner", "Reu", "erst", "war\u00b7ten", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}