{"textgrid.poem.51363": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann unsre Hertzen ich mir bilde, G\u00f6ttin, ein:", "tokens": ["Wann", "uns\u00b7re", "Hert\u00b7zen", "ich", "mir", "bil\u00b7de", ",", "G\u00f6t\u00b7tin", ",", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "PPER", "VVFIN", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist deines und zugleich auch meines wie ein Stein.", "tokens": ["Ist", "dei\u00b7nes", "und", "zu\u00b7gleich", "auch", "mei\u00b7nes", "wie", "ein", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "KON", "ADV", "ADV", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar deins, weil es die Angst des Meinen nicht erwegt,", "tokens": ["Zwar", "deins", ",", "weil", "es", "die", "Angst", "des", "Mei\u00b7nen", "nicht", "er\u00b7wegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meins, weil es den Grimm des Deinen so ertr\u00e4gt.", "tokens": ["Und", "meins", ",", "weil", "es", "den", "Grimm", "des", "Dei\u00b7nen", "so", "er\u00b7tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "KOUS", "PPER", "ART", "NE", "ART", "PPOSAT", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jedoch, bedenck ich ie mein Leiden umb und an,", "tokens": ["Je\u00b7doch", ",", "be\u00b7denck", "ich", "ie", "mein", "Lei\u00b7den", "umb", "und", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das du mir schickest zu, und ich ertragen kan:", "tokens": ["Das", "du", "mir", "schi\u00b7ckest", "zu", ",", "und", "ich", "er\u00b7tra\u00b7gen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00e4lt grosses ungleich f\u00fcr. Dann sind die Hertzen Stein,", "tokens": ["F\u00e4lt", "gros\u00b7ses", "un\u00b7gleich", "f\u00fcr", ".", "Dann", "sind", "die", "Hert\u00b7zen", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "ADJD", "APPR", "$.", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist es, da\u00df mein an Treu, das Dein an grausam seyn.", "tokens": ["Ist", "es", ",", "da\u00df", "mein", "an", "Treu", ",", "das", "Dein", "an", "grau\u00b7sam", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPOSAT", "APPR", "NN", "$,", "PRELS", "PPOSAT", "APPR", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}