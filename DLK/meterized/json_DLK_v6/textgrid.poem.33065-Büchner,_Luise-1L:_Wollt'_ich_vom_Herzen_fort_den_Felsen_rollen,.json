{"textgrid.poem.33065": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wollt' ich vom Herzen fort den Felsen rollen,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wollt' ich vom Herzen fort den Felsen rollen,", "tokens": ["Wollt'", "ich", "vom", "Her\u00b7zen", "fort", "den", "Fel\u00b7sen", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKVZ", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein tiefgeheimstes Inn're mir erschlie\u00dfen,", "tokens": ["Sein", "tief\u00b7ge\u00b7heims\u00b7tes", "Inn'\u00b7re", "mir", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Stein erstarren m\u00fc\u00dft' ich dann vor diesen", "tokens": ["Zum", "Stein", "er\u00b7star\u00b7ren", "m\u00fc\u00dft'", "ich", "dann", "vor", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "PPER", "ADV", "APPR", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Medusen, die es schmerzlich wild umgrollen.", "tokens": ["Me\u00b7du\u00b7sen", ",", "die", "es", "schmerz\u00b7lich", "wild", "um\u00b7grol\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "H\u00e4tt' ich in Klagen mich erleichtern wollen,", "tokens": ["H\u00e4tt'", "ich", "in", "Kla\u00b7gen", "mich", "er\u00b7leich\u00b7tern", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie m\u00fc\u00dften wie ein Strom sich laut ergie\u00dfen,", "tokens": ["Sie", "m\u00fc\u00df\u00b7ten", "wie", "ein", "Strom", "sich", "laut", "er\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KOKOM", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es m\u00fc\u00dft' in bitt'ren Thr\u00e4nen mir entflie\u00dfen", "tokens": ["Es", "m\u00fc\u00dft'", "in", "bitt'\u00b7ren", "Thr\u00e4\u00b7nen", "mir", "ent\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Lebens beste Kraft und h\u00f6chstes Wollen.", "tokens": ["Des", "Le\u00b7bens", "bes\u00b7te", "Kraft", "und", "h\u00f6chs\u00b7tes", "Wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nein, wie ein R\u00e4thsel fast soll mich umschweben,", "tokens": ["Nein", ",", "wie", "ein", "R\u00e4th\u00b7sel", "fast", "soll", "mich", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Was so mich dr\u00fcckt und peinigt ohne Schuld \u2013", "tokens": ["Was", "so", "mich", "dr\u00fcckt", "und", "pei\u00b7nigt", "oh\u00b7ne", "Schuld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wie k\u00f6nnt' ich sonst mich je zum Gl\u00fcck noch heben?", "tokens": ["Wie", "k\u00f6nnt'", "ich", "sonst", "mich", "je", "zum", "Gl\u00fcck", "noch", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PPER", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Blind, taub und schweigend, will ich weiter schreiten,", "tokens": ["Blind", ",", "taub", "und", "schwei\u00b7gend", ",", "will", "ich", "wei\u00b7ter", "schrei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und \u00fcber Alles kalt und ruhig breiten", "tokens": ["Und", "\u00fc\u00b7ber", "Al\u00b7les", "kalt", "und", "ru\u00b7hig", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "ADJD", "KON", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die starre Leichendecke der Geduld!", "tokens": ["Die", "star\u00b7re", "Lei\u00b7chen\u00b7de\u00b7cke", "der", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}