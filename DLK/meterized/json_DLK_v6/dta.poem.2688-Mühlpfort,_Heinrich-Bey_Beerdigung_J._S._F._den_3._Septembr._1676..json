{"dta.poem.2688": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung J. S. F. den 3.  \n Septembr. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So machst du dich so bald im Morgen erster Jahre/", "tokens": ["So", "machst", "du", "dich", "so", "bald", "im", "Mor\u00b7gen", "ers\u00b7ter", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADV", "APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du zarte ", "tokens": ["Du", "zar\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Gibst den verblasten Leib der schwartz-bedeckten Bahre", "tokens": ["Gibst", "den", "ver\u00b7blas\u00b7ten", "Leib", "der", "schwartz\u00b7be\u00b7deck\u00b7ten", "Bah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deine reine Seel\u2019 in des Erl\u00f6sers Scho\u00df?", "tokens": ["Und", "dei\u00b7ne", "rei\u00b7ne", "Seel'", "in", "des", "Er\u00b7l\u00f6\u00b7sers", "Scho\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sol dein Br\u00e4utgam dich so fr\u00fch in Rosen weiden/", "tokens": ["So", "sol", "dein", "Br\u00e4ut\u00b7gam", "dich", "so", "fr\u00fch", "in", "Ro\u00b7sen", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und magst das Leben nicht mit seinem Schatten sehn?", "tokens": ["Und", "magst", "das", "Le\u00b7ben", "nicht", "mit", "sei\u00b7nem", "Schat\u00b7ten", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So eilst du franck und frey zu den gekr\u00f6nten Freuden/", "tokens": ["So", "eilst", "du", "franck", "und", "frey", "zu", "den", "ge\u00b7kr\u00f6n\u00b7ten", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und l\u00e4st umbsonst nach dir der Seufftzer Winde wehn?", "tokens": ["Und", "l\u00e4st", "um\u00b7bsonst", "nach", "dir", "der", "Seufft\u00b7zer", "Win\u00b7de", "wehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gewi\u00df dein Freyheits-Stand ist wol mit nichts zu gleichen", "tokens": ["Ge\u00b7wi\u00df", "dein", "Frey\u00b7heits\u00b7Stand", "ist", "wol", "mit", "nichts", "zu", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und dein erlauchtes Heil geht allen W\u00fcrden f\u00fcr.", "tokens": ["Und", "dein", "er\u00b7lauch\u00b7tes", "Heil", "geht", "al\u00b7len", "W\u00fcr\u00b7den", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach aber! wer entdeckt bey der entseelten Leichen", "tokens": ["Ach", "a\u00b7ber", "!", "wer", "ent\u00b7deckt", "bey", "der", "ent\u00b7seel\u00b7ten", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$.", "PWS", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was f\u00fcr ein tieffer Schmertz der ", "tokens": ["Was", "f\u00fcr", "ein", "tief\u00b7fer", "Schmertz", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Je hefftiger die Lieb/ je gr\u00f6sser auch die Wunden/", "tokens": ["Je", "heff\u00b7ti\u00b7ger", "die", "Lieb", "/", "je", "gr\u00f6s\u00b7ser", "auch", "die", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$(", "ADV", "ADJD", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn ihrem Ursprung nach entsprost sie aus dem Blut/", "tokens": ["Denn", "ih\u00b7rem", "Ur\u00b7sprung", "nach", "ent\u00b7sprost", "sie", "aus", "dem", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und wird hinwiederumb zum zartesten empfunden", "tokens": ["Und", "wird", "hin\u00b7wie\u00b7de\u00b7rumb", "zum", "zar\u00b7tes\u00b7ten", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn einen solchen Streich des Todes Mord-Beil thut.", "tokens": ["Wenn", "ei\u00b7nen", "sol\u00b7chen", "Streich", "des", "To\u00b7des", "Mord\u00b7Beil", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie sol/ ", "tokens": ["Wie", "sol", "/"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VMFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Das Auge wie ein Brunn voll heisser Thr\u00e4nen stehn?", "tokens": ["Das", "Au\u00b7ge", "wie", "ein", "Brunn", "voll", "heis\u00b7ser", "Thr\u00e4\u00b7nen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nach dem so einen Sturm von rauhen Ungewittern.", "tokens": ["Nach", "dem", "so", "ei\u00b7nen", "Sturm", "von", "rau\u00b7hen", "Un\u00b7ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "L\u00e4st \u00fcber euer Hau\u00df der Schlu\u00df des Himmels gehn.", "tokens": ["L\u00e4st", "\u00fc\u00b7ber", "eu\u00b7er", "Hau\u00df", "der", "Schlu\u00df", "des", "Him\u00b7mels", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Tochter schau\u2019n verblast/ so eure h\u00f6chste Freude/", "tokens": ["Die", "Toch\u00b7ter", "schau'n", "ver\u00b7blast", "/", "so", "eu\u00b7re", "h\u00f6chs\u00b7te", "Freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$(", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ja gar auff dieser Welt das andre Leben hie\u00df/", "tokens": ["Ja", "gar", "auff", "die\u00b7ser", "Welt", "das", "and\u00b7re", "Le\u00b7ben", "hie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Des Vatern Trost und Lust/ der Mutter Augen-Weide.", "tokens": ["Des", "Va\u00b7tern", "Trost", "und", "Lust", "/", "der", "Mut\u00b7ter", "Au\u00b7gen\u00b7Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sehn auff der Baare stehn/ di\u00df ist ein Seelen-Rie\u00df.", "tokens": ["Sehn", "auff", "der", "Baa\u00b7re", "stehn", "/", "di\u00df", "ist", "ein", "See\u00b7len\u00b7Rie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$(", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "H\u00e4lt denn die W\u00fctterey des Todes nichts zur\u00fccke?", "tokens": ["H\u00e4lt", "denn", "die", "W\u00fct\u00b7te\u00b7rey", "des", "To\u00b7des", "nichts", "zu\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Kont ihre Liebligkeit und Anmuth nicht entfliehn?", "tokens": ["Kont", "ih\u00b7re", "Lieb\u00b7lig\u00b7keit", "und", "An\u00b7muth", "nicht", "ent\u00b7fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wo bleibt ihr munter Geist/ die freundlich-holden Blicke?", "tokens": ["Wo", "bleibt", "ihr", "mun\u00b7ter", "Geist", "/", "die", "freund\u00b7lich\u00b7hol\u00b7den", "Bli\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mu\u00df diese Blume denn im ersten Lentz verbl\u00fchn?", "tokens": ["Mu\u00df", "die\u00b7se", "Blu\u00b7me", "denn", "im", "ers\u00b7ten", "Lentz", "ver\u00b7bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach ja der Augen Liecht/ so Sternen gleich gestralet/", "tokens": ["Ach", "ja", "der", "Au\u00b7gen", "Liecht", "/", "so", "Ster\u00b7nen", "gleich", "ge\u00b7stra\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "NN", "NN", "$(", "ADV", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Blickt in der Sterbligkeit nicht mehr die Eltern an:", "tokens": ["Blickt", "in", "der", "Ster\u00b7blig\u00b7keit", "nicht", "mehr", "die", "El\u00b7tern", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Mund/ den Lieb und Huld mit Rosen \u00fcbermahlet/", "tokens": ["Der", "Mund", "/", "den", "Lieb", "und", "Huld", "mit", "Ro\u00b7sen", "\u00fc\u00b7ber\u00b7mah\u00b7let", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Er zehlt nicht wie zuvor/ was sie ergetzen kan:", "tokens": ["Er", "zehlt", "nicht", "wie", "zu\u00b7vor", "/", "was", "sie", "er\u00b7get\u00b7zen", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOKOM", "ADV", "$(", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Glieder Schnee zergeht/ der Jahre Wachs zerrinnet", "tokens": ["Der", "Glie\u00b7der", "Schnee", "zer\u00b7geht", "/", "der", "Jah\u00b7re", "Wachs", "zer\u00b7rin\u00b7net"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die sittsame Gestalt/ so allen wol gefiel/", "tokens": ["Die", "sitt\u00b7sa\u00b7me", "Ge\u00b7stalt", "/", "so", "al\u00b7len", "wol", "ge\u00b7fiel", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PIAT", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Verstellt des Todes Grimm/ die Tugend liegt entsinnet/", "tokens": ["Ver\u00b7stellt", "des", "To\u00b7des", "Grimm", "/", "die", "Tu\u00b7gend", "liegt", "ent\u00b7sin\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$(", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Anmuth abgemeyht/ es schl\u00e4fft nun Schertz und Spiel.", "tokens": ["Die", "An\u00b7muth", "ab\u00b7ge\u00b7meyht", "/", "es", "schl\u00e4fft", "nun", "Schertz", "und", "Spiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hier mu\u00df absonderlich der Mutter Hertze bluten/", "tokens": ["Hier", "mu\u00df", "ab\u00b7son\u00b7der\u00b7lich", "der", "Mut\u00b7ter", "Hert\u00b7ze", "blu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADJD", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nun ihrer M\u00fch und Flei\u00df gerathner Seegen f\u00e4llt.", "tokens": ["Nun", "ih\u00b7rer", "M\u00fch", "und", "Flei\u00df", "ge\u00b7rath\u00b7ner", "See\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Bevor/ wenn sie erwegt/ wie stets zu allem guten", "tokens": ["Be\u00b7vor", "/", "wenn", "sie", "er\u00b7wegt", "/", "wie", "stets", "zu", "al\u00b7lem", "gu\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "PPER", "VVFIN", "$(", "KOKOM", "ADV", "APPR", "PIS", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Von erster Kindheit an die Zucht war angestellt.", "tokens": ["Von", "ers\u00b7ter", "Kind\u00b7heit", "an", "die", "Zucht", "war", "an\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie ihre Gottes furcht dem Sch\u00f6pffer dieser Erden", "tokens": ["Wie", "ih\u00b7re", "Got\u00b7tes", "furcht", "dem", "Sch\u00f6pf\u00b7fer", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ein angenehm Geruch und Opffer m\u00fcssen seyn/", "tokens": ["Ein", "an\u00b7ge\u00b7nehm", "Ge\u00b7ruch", "und", "Opf\u00b7fer", "m\u00fcs\u00b7sen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, 