{"textgrid.poem.60517": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Kompetenz", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4nner, welche eine H\u00f6h erklommen,", "tokens": ["M\u00e4n\u00b7ner", ",", "wel\u00b7che", "ei\u00b7ne", "H\u00f6h", "er\u00b7klom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sind als Richter wert uns und willkommen;", "tokens": ["Sind", "als", "Rich\u00b7ter", "wert", "uns", "und", "will\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "VVFIN", "PPER", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist es nicht die H\u00f6he des Gesanges,", "tokens": ["Ist", "es", "nicht", "die", "H\u00f6\u00b7he", "des", "Ge\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seis die H\u00f6he doch des Forscherganges.", "tokens": ["Seis", "die", "H\u00f6\u00b7he", "doch", "des", "For\u00b7scher\u00b7gan\u00b7ges", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Solchen steht es an, ein Wort zu reden", "tokens": ["Sol\u00b7chen", "steht", "es", "an", ",", "ein", "Wort", "zu", "re\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von des k\u00fchnen Wandrers M\u00fchn und Fehden", "tokens": ["Von", "des", "k\u00fch\u00b7nen", "Wand\u00b7rers", "M\u00fchn", "und", "Feh\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Mit Abgr\u00fcnden, Klippen, Eisesfl\u00e4chen,", "tokens": ["Mit", "Ab\u00b7gr\u00fcn\u00b7den", ",", "Klip\u00b7pen", ",", "Ei\u00b7ses\u00b7fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wo die J\u00e4ger sich die H\u00e4lse brechen.", "tokens": ["Wo", "die", "J\u00e4\u00b7ger", "sich", "die", "H\u00e4l\u00b7se", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Solche m\u00f6gen auch mit Recht verspotten", "tokens": ["Sol\u00b7che", "m\u00f6\u00b7gen", "auch", "mit", "Recht", "ver\u00b7spot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "In der niedern Marsch die P\u00f6belrotten.", "tokens": ["In", "der", "nie\u00b7dern", "Marsch", "die", "P\u00f6\u00b7bel\u00b7rot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Wer mit Gemsen eine Luft getrunken,", "tokens": ["Wer", "mit", "Gem\u00b7sen", "ei\u00b7ne", "Luft", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Atmet nicht behaglich bei den Unken.", "tokens": ["At\u00b7met", "nicht", "be\u00b7hag\u00b7lich", "bei", "den", "Un\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Wer zum Abgrund schwindellos gesehen,", "tokens": ["Wer", "zum", "Ab\u00b7grund", "schwin\u00b7del\u00b7los", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Wird des Bruders k\u00fchnen Tritt verstehen;", "tokens": ["Wird", "des", "Bru\u00b7ders", "k\u00fch\u00b7nen", "Tritt", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Wer den Fels der Meisterschaft erklettert,", "tokens": ["Wer", "den", "Fels", "der", "Meis\u00b7ter\u00b7schaft", "er\u00b7klet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Ehrt den Mann, der hier nicht sank zerschmettert", "tokens": ["Ehrt", "den", "Mann", ",", "der", "hier", "nicht", "sank", "zer\u00b7schmet\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Aber alle andern sollen schweigen,", "tokens": ["A\u00b7ber", "al\u00b7le", "an\u00b7dern", "sol\u00b7len", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Wenn sich M\u00e4nner ihrem Volke zeigen;", "tokens": ["Wenn", "sich", "M\u00e4n\u00b7ner", "ih\u00b7rem", "Vol\u00b7ke", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Schweigen sollen sie und sollen lernen,", "tokens": ["Schwei\u00b7gen", "sol\u00b7len", "sie", "und", "sol\u00b7len", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Wie man n\u00e4her wandeln mag den Sternen.", "tokens": ["Wie", "man", "n\u00e4\u00b7her", "wan\u00b7deln", "mag", "den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Scheu mit seinem Urteil sich verschliefe,", "tokens": ["Scheu", "mit", "sei\u00b7nem", "Ur\u00b7teil", "sich", "ver\u00b7schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Wer herum noch st\u00fcmpert in der Tiefe.", "tokens": ["Wer", "he\u00b7rum", "noch", "st\u00fcm\u00b7pert", "in", "der", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Glaubt ihr denn, ihr lahmen Kr\u00fcppelwichte,", "tokens": ["Glaubt", "ihr", "denn", ",", "ihr", "lah\u00b7men", "Kr\u00fcp\u00b7pel\u00b7wich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Da\u00df die Welt nach eurer Weisheit richte?", "tokens": ["Da\u00df", "die", "Welt", "nach", "eu\u00b7rer", "Weis\u00b7heit", "rich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Ha! ihr wollt als Ellen eure Kr\u00fccken", "tokens": ["Ha", "!", "ihr", "wollt", "als", "El\u00b7len", "eu\u00b7re", "Kr\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "KOUS", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Kindisch messend an die Geister dr\u00fccken!", "tokens": ["Kin\u00b7disch", "mes\u00b7send", "an", "die", "Geis\u00b7ter", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Und indem ihr mit der Kr\u00fccke schaltet", "tokens": ["Und", "in\u00b7dem", "ihr", "mit", "der", "Kr\u00fc\u00b7cke", "schal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Und den Stecken in die L\u00fcfte haltet,", "tokens": ["Und", "den", "Ste\u00b7cken", "in", "die", "L\u00fcf\u00b7te", "hal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Raubt ihr eurer lahmen Wucht die St\u00fctze,", "tokens": ["Raubt", "ihr", "eu\u00b7rer", "lah\u00b7men", "Wucht", "die", "St\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Und ihr st\u00fcrzt erb\u00e4rmlich in die Pf\u00fctze,", "tokens": ["Und", "ihr", "st\u00fcrzt", "er\u00b7b\u00e4rm\u00b7lich", "in", "die", "Pf\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Denn der Windhauch, den ihr wolltet messen,", "tokens": ["Denn", "der", "Wind\u00b7hauch", ",", "den", "ihr", "woll\u00b7tet", "mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "--++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Hat euch umgeblasen unterdessen.", "tokens": ["Hat", "euch", "um\u00b7ge\u00b7bla\u00b7sen", "un\u00b7ter\u00b7des\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.33": {"text": "Und es hinken weiter unsre Richter,", "tokens": ["Und", "es", "hin\u00b7ken", "wei\u00b7ter", "uns\u00b7re", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Vorw\u00e4rts tragend schmutzige Gesichter,", "tokens": ["Vor\u00b7w\u00e4rts", "tra\u00b7gend", "schmut\u00b7zi\u00b7ge", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.35": {"text": "W\u00e4hrend hier und dort aus lyrischen Lacken", "tokens": ["W\u00e4h\u00b7rend", "hier", "und", "dort", "aus", "ly\u00b7ri\u00b7schen", "La\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Ihre Lieder ihnen M\u00e4rsche quaken.", "tokens": ["Ih\u00b7re", "Lie\u00b7der", "ih\u00b7nen", "M\u00e4r\u00b7sche", "qua\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}