{"dta.poem.15672": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schon wieder hast du nicht, was ich gewollt, gethan,", "tokens": ["Schon", "wie\u00b7der", "hast", "du", "nicht", ",", "was", "ich", "ge\u00b7wollt", ",", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VMPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schon wieder hast du, was du nicht gesollt, gethan.", "tokens": ["Schon", "wie\u00b7der", "hast", "du", ",", "was", "du", "nicht", "ge\u00b7sollt", ",", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "PTKNEG", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "\u201eges\u00fcndigt hab' ich wol, allein vernimm die Gr\u00fcnde", "tokens": ["\u201e", "ge\u00b7s\u00fcn\u00b7digt", "hab'", "ich", "wol", ",", "al\u00b7lein", "ver\u00b7nimm", "die", "Gr\u00fcn\u00b7de"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVPP", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unterlassung dort, hier der Begehungs\u00fcnde.\u201c", "tokens": ["Der", "Un\u00b7ter\u00b7las\u00b7sung", "dort", ",", "hier", "der", "Be\u00b7ge\u00b7hun\u00b7gs\u00fcn\u00b7de", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und S\u00fcnden meinest du mit S\u00fcnden abgethan?", "tokens": ["Und", "S\u00fcn\u00b7den", "mei\u00b7nest", "du", "mit", "S\u00fcn\u00b7den", "ab\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gr\u00fcnde gehn mich nichts, mich gehn die S\u00fcnden an.", "tokens": ["Die", "Gr\u00fcn\u00b7de", "gehn", "mich", "nichts", ",", "mich", "gehn", "die", "S\u00fcn\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIS", "$,", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wer sich auf Gr\u00fcnde wollt' einlassen aller S\u00fcnden,", "tokens": ["Wer", "sich", "auf", "Gr\u00fcn\u00b7de", "wollt'", "ein\u00b7las\u00b7sen", "al\u00b7ler", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "VMFIN", "VVINF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf einen sch\u00f6nen Grund w\u00e4r jede wol zu gr\u00fcnden.", "tokens": ["Auf", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Grund", "w\u00e4r", "je\u00b7de", "wol", "zu", "gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "PIAT", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hex