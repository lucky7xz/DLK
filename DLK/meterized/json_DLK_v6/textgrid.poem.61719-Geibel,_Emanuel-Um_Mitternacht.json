{"textgrid.poem.61719": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Um Mitternacht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Saal gedankenvoll", "tokens": ["Im", "Saal", "ge\u00b7dan\u00b7ken\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df ich bei Lampenschein;", "tokens": ["Sa\u00df", "ich", "bei", "Lam\u00b7pen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchs offne Fenster quoll", "tokens": ["Durchs", "off\u00b7ne", "Fens\u00b7ter", "quoll"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Sommernacht herein.", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Bild, von treuer Hand", "tokens": ["Dein", "Bild", ",", "von", "treu\u00b7er", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geschm\u00fcckt mit frischem Kranz,", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "fri\u00b7schem", "Kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah von der dunkeln Wand", "tokens": ["Sah", "von", "der", "dun\u00b7keln", "Wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich an im D\u00e4mmerglanz.", "tokens": ["Mich", "an", "im", "D\u00e4m\u00b7mer\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da, auf der Sehnsucht Pfad", "tokens": ["Da", ",", "auf", "der", "Sehn\u00b7sucht", "Pfad"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vertiefte sich mein Sinn,", "tokens": ["Ver\u00b7tief\u00b7te", "sich", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und himmlisch leuchtend trat", "tokens": ["Und", "himm\u00b7lisch", "leuch\u00b7tend", "trat"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dein Wesen vor mich hin;", "tokens": ["Dein", "We\u00b7sen", "vor", "mich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, wie du lilienrein", "tokens": ["Ach", ",", "wie", "du", "li\u00b7li\u00b7en\u00b7rein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie nach dem deinen frugst", "tokens": ["Nie", "nach", "dem", "dei\u00b7nen", "frugst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4chelnd selbst die Pein", "tokens": ["Und", "l\u00e4\u00b7chelnd", "selbst", "die", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie eine Heil'ge trugst.", "tokens": ["Wie", "ei\u00b7ne", "Heil'\u00b7ge", "trugst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und \u00fcberm Abgrund dann,", "tokens": ["Und", "\u00fc\u00b7berm", "Ab\u00b7grund", "dann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem d\u00fcstern, Tod und Grab,", "tokens": ["Dem", "d\u00fcs\u00b7tern", ",", "Tod", "und", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hing mein Gedank' und sann", "tokens": ["Hing", "mein", "Ge\u00b7dank'", "und", "sann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In seine Tief' hinab.", "tokens": ["In", "sei\u00b7ne", "Tie\u00b7f'", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Werd' ich dich wiedersehn?", "tokens": ["Werd'", "ich", "dich", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kann je, was Liebe hier", "tokens": ["Kann", "je", ",", "was", "Lie\u00b7be", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "$,", "PWS", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erwarb, verlorengehn?", "tokens": ["Er\u00b7warb", ",", "ver\u00b7lo\u00b7ren\u00b7gehn", "?"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wei\u00dft du noch von mir?", "tokens": ["Und", "wei\u00dft", "du", "noch", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O gib mir, hast du Macht,", "tokens": ["O", "gib", "mir", ",", "hast", "du", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Zeichen noch so stumm! \u2013", "tokens": ["Ein", "Zei\u00b7chen", "noch", "so", "stumm", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da schlug es Mitternacht,", "tokens": ["Da", "schlug", "es", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und zaudernd blickt' ich um.", "tokens": ["Und", "zau\u00b7dernd", "blickt'", "ich", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein s\u00fc\u00dfes Duften flog", "tokens": ["Ein", "s\u00fc\u00b7\u00dfes", "Duf\u00b7ten", "flog"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom Kranz, der zitternd hing,", "tokens": ["Vom", "Kranz", ",", "der", "zit\u00b7ternd", "hing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und um die Lampe zog", "tokens": ["Und", "um", "die", "Lam\u00b7pe", "zog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein wei\u00dfer Schmetterling. \u2013", "tokens": ["Ein", "wei\u00b7\u00dfer", "Schmet\u00b7ter\u00b7ling", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}