{"textgrid.poem.52473": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "55. Eignung an meine Gattin", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, von der ich seine Z\u00fcg' entlieh',", "tokens": ["O", "du", ",", "von", "der", "ich", "sei\u00b7ne", "Z\u00fcg'", "ent\u00b7lieh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "PRELS", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verg\u00f6nne, da\u00df ich dieses Bild dir weihe,", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", ",", "da\u00df", "ich", "die\u00b7ses", "Bild", "dir", "wei\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Schattenbild erregter Phantasie;", "tokens": ["Kein", "Schat\u00b7ten\u00b7bild", "er\u00b7reg\u00b7ter", "Phan\u00b7ta\u00b7sie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah und malte dich mit strenger Treue.", "tokens": ["Ich", "sah", "und", "mal\u00b7te", "dich", "mit", "stren\u00b7ger", "Treu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein Sinn, zu edel sch\u00f6n, flieht lauten Ruhm,", "tokens": ["Dein", "Sinn", ",", "zu", "e\u00b7del", "sch\u00f6n", ",", "flieht", "lau\u00b7ten", "Ruhm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKA", "ADJD", "ADJD", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wehrt dem Gatten Dank und Preisges\u00e4nge!", "tokens": ["Und", "wehrt", "dem", "Gat\u00b7ten", "Dank", "und", "Preis\u00b7ge\u00b7s\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo ist der Muttertreue Heiligtum,", "tokens": ["Wo", "ist", "der", "Mut\u00b7ter\u00b7treu\u00b7e", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df ich dies Bild zu seinen Kr\u00e4nzen h\u00e4nge!", "tokens": ["Da\u00df", "ich", "dies", "Bild", "zu", "sei\u00b7nen", "Kr\u00e4n\u00b7zen", "h\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}