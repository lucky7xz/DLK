{"textgrid.poem.33863": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6n Hedwig", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Kreise der Vasallen sitzt", "tokens": ["Im", "Krei\u00b7se", "der", "Va\u00b7sal\u00b7len", "sitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der Ritter, jung und k\u00fchn;", "tokens": ["Der", "Rit\u00b7ter", ",", "jung", "und", "k\u00fchn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein dunkles Feuerauge blitzt,", "tokens": ["Sein", "dunk\u00b7les", "Feu\u00b7er\u00b7au\u00b7ge", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wollt' er zieh'n zum Kampfe,", "tokens": ["Als", "wollt'", "er", "zieh'n", "zum", "Kamp\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und seine Wangen gl\u00fch'n.", "tokens": ["Und", "sei\u00b7ne", "Wan\u00b7gen", "gl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein zartes M\u00e4gdlein tritt heran", "tokens": ["Ein", "zar\u00b7tes", "M\u00e4gd\u00b7lein", "tritt", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und f\u00fcllt ihm den Pocal.", "tokens": ["Und", "f\u00fcllt", "ihm", "den", "Po\u00b7cal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fcck mit L\u00e4cheln tritt sie dann,", "tokens": ["Zu\u00b7r\u00fcck", "mit", "L\u00e4\u00b7cheln", "tritt", "sie", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da f\u00e4llt auf ihre Stirne", "tokens": ["Da", "f\u00e4llt", "auf", "ih\u00b7re", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der klarste Morgenstral.", "tokens": ["Der", "klars\u00b7te", "Mor\u00b7gen\u00b7stral", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Ritter aber fa\u00dft sie schnell", "tokens": ["Der", "Rit\u00b7ter", "a\u00b7ber", "fa\u00dft", "sie", "schnell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei ihrer wei\u00dfen Hand.", "tokens": ["Bei", "ih\u00b7rer", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr blaues Auge, frisch und hell,", "tokens": ["Ihr", "blau\u00b7es", "Au\u00b7ge", ",", "frisch", "und", "hell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schl\u00e4gt es erst zu Boden,", "tokens": ["Sie", "schl\u00e4gt", "es", "erst", "zu", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann hebt sie's unverwandt.", "tokens": ["Dann", "hebt", "sie's", "un\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbsch\u00f6n Hedwig, die du vor mir stehst,", "tokens": ["\u00bb", "sch\u00f6n", "Hed\u00b7wig", ",", "die", "du", "vor", "mir", "stehst", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NE", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drei Dinge sag' mir frei:", "tokens": ["Drei", "Din\u00b7ge", "sag'", "mir", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Woher du kommst, wohin du gehst,", "tokens": ["Wo\u00b7her", "du", "kommst", ",", "wo\u00b7hin", "du", "gehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warum du stets mir folgest;", "tokens": ["Wa\u00b7rum", "du", "stets", "mir", "fol\u00b7gest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das sind der Dinge drei!\u00ab", "tokens": ["Das", "sind", "der", "Din\u00b7ge", "drei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "CARD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Woher ich komm'? Ich komm' von Gott,", "tokens": ["Wo\u00b7her", "ich", "komm'", "?", "Ich", "komm'", "von", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hat man mir gesagt,", "tokens": ["So", "hat", "man", "mir", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich, verfolgt von Hohn und Spott,", "tokens": ["Als", "ich", ",", "ver\u00b7folgt", "von", "Hohn", "und", "Spott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Vater und nach Mutter", "tokens": ["Nach", "Va\u00b7ter", "und", "nach", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Thr\u00e4nen einst gefragt.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "einst", "ge\u00b7fragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohin ich geh'? Nichts treibt mich fort,", "tokens": ["Wo\u00b7hin", "ich", "geh'", "?", "Nichts", "treibt", "mich", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist gar zu weit.", "tokens": ["Die", "Welt", "ist", "gar", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was tauscht' ich eitel Ort um Ort?", "tokens": ["Was", "tauscht'", "ich", "ei\u00b7tel", "Ort", "um", "Ort", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie ist ja allenthalben", "tokens": ["Sie", "ist", "ja", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Voll Lust und Herrlichkeit.", "tokens": ["Voll", "Lust", "und", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Warum ich folg', wohin du winkst?", "tokens": ["Wa\u00b7rum", "ich", "fol\u00b7g'", ",", "wo\u00b7hin", "du", "winkst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ei, sprich, wie k\u00f6nnt' ich ruh'n?", "tokens": ["Ei", ",", "sprich", ",", "wie", "k\u00f6nnt'", "ich", "ruh'n", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schenk' den Wein dir, den du trinkst,", "tokens": ["Ich", "schenk'", "den", "Wein", "dir", ",", "den", "du", "trinkst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bat dich drum auf Knieen", "tokens": ["Ich", "bat", "dich", "drum", "auf", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "APPR", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Und m\u00f6gt' es ewig thun!", "tokens": ["Und", "m\u00f6gt'", "es", "e\u00b7wig", "thun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbso frage ich, du blondes Kind,", "tokens": ["\u00bb", "so", "fra\u00b7ge", "ich", ",", "du", "blon\u00b7des", "Kind", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch um ein Viertes dich;", "tokens": ["Noch", "um", "ein", "Vier\u00b7tes", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies Letzte sag' mir an geschwind,", "tokens": ["Dies", "Letz\u00b7te", "sag'", "mir", "an", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann frag' ich dich Nichts weiter,", "tokens": ["Dann", "frag'", "ich", "dich", "Nichts", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sag', M\u00e4gdlein, liebst du mich?\u00ab", "tokens": ["Sag'", ",", "M\u00e4gd\u00b7lein", ",", "liebst", "du", "mich", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "PRF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Im Anfang steht sie starr und stumm,", "tokens": ["Im", "An\u00b7fang", "steht", "sie", "starr", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann schaut sie langsam sich", "tokens": ["Dann", "schaut", "sie", "lang\u00b7sam", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Kreis der ernsten G\u00e4ste um,", "tokens": ["Im", "Kreis", "der", "erns\u00b7ten", "G\u00e4s\u00b7te", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und faltet ihre H\u00e4nde", "tokens": ["Und", "fal\u00b7tet", "ih\u00b7re", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und spricht: Ich liebe dich!", "tokens": ["Und", "spricht", ":", "Ich", "lie\u00b7be", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nun aber wei\u00df ich auch, wohin", "tokens": ["Nun", "a\u00b7ber", "wei\u00df", "ich", "auch", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich gehen mu\u00df von hier;", "tokens": ["Ich", "ge\u00b7hen", "mu\u00df", "von", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl ist's mir klar in meinem Sinn:", "tokens": ["Wohl", "ist's", "mir", "klar", "in", "mei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem ich die\u00df gestanden,", "tokens": ["Nach\u00b7dem", "ich", "die\u00df", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ziemt nur der Schleier mir?", "tokens": ["Ziemt", "nur", "der", "Schlei\u00b7er", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "\u00bbund wenn du sagst, du kommst von Gott,", "tokens": ["\u00bb", "und", "wenn", "du", "sagst", ",", "du", "kommst", "von", "Gott", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So f\u00fchl' ich, das ist wahr.", "tokens": ["So", "f\u00fchl'", "ich", ",", "das", "ist", "wahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum f\u00fchr' ich auch, trotz Hohn und Spott,", "tokens": ["Drum", "f\u00fchr'", "ich", "auch", ",", "trotz", "Hohn", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als seine liebste Tochter", "tokens": ["Als", "sei\u00b7ne", "liebs\u00b7te", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Noch heut' dich zum Altar.", "tokens": ["Noch", "heut'", "dich", "zum", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ihr edlen Herrn, ich lud verbl\u00fcmt", "tokens": ["Ihr", "ed\u00b7len", "Herrn", ",", "ich", "lud", "ver\u00b7bl\u00fcmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem Fest euch ein;", "tokens": ["Zu", "ei\u00b7nem", "Fest", "euch", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Ritter, stolz und hoch ger\u00fchmt,", "tokens": ["Ihr", "Rit\u00b7ter", ",", "stolz", "und", "hoch", "ge\u00b7r\u00fchmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So folgt mir zur Kapelle,", "tokens": ["So", "folgt", "mir", "zur", "Ka\u00b7pel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es soll mein sch\u00f6nstes sein!\u00ab", "tokens": ["Es", "soll", "mein", "sch\u00f6ns\u00b7tes", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}