{"dta.poem.23319": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wie ist doch die Zeitung interessant!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist doch die Zeitung interessant", "tokens": ["Wie", "ist", "doch", "die", "Zei\u00b7tung", "in\u00b7ter\u00b7es\u00b7sant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr unser liebes Vaterland!", "tokens": ["F\u00fcr", "un\u00b7ser", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was haben wir heute nicht Alles vernommen!", "tokens": ["Was", "ha\u00b7ben", "wir", "heu\u00b7te", "nicht", "Al\u00b7les", "ver\u00b7nom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKNEG", "PIS", "VVPP", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die F\u00fcrstin ist gestern niedergekommen,", "tokens": ["Die", "F\u00fcrs\u00b7tin", "ist", "ge\u00b7stern", "nie\u00b7der\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und morgen wird der Herzog kommen,", "tokens": ["Und", "mor\u00b7gen", "wird", "der", "Her\u00b7zog", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier ist der K\u00f6nig heimgekommen,", "tokens": ["Hier", "ist", "der", "K\u00f6\u00b7nig", "heim\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dort ist der Kaiser durchgekommen,", "tokens": ["Dort", "ist", "der", "Kai\u00b7ser", "durch\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bald werden sie alle zusammenkommen \u2014", "tokens": ["Bald", "wer\u00b7den", "sie", "al\u00b7le", "zu\u00b7sam\u00b7men\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Wie interessant! wie interessant!", "tokens": ["Wie", "in\u00b7ter\u00b7es\u00b7sant", "!", "wie", "in\u00b7ter\u00b7es\u00b7sant", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "PWAV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Gott segne das liebe Vaterland!", "tokens": ["Gott", "seg\u00b7ne", "das", "lie\u00b7be", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie ist doch die Zeitung interessant", "tokens": ["Wie", "ist", "doch", "die", "Zei\u00b7tung", "in\u00b7ter\u00b7es\u00b7sant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00fcr unser liebes Vaterland!", "tokens": ["F\u00fcr", "un\u00b7ser", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist uns nicht Alles berichtet worden!", "tokens": ["Was", "ist", "uns", "nicht", "Al\u00b7les", "be\u00b7rich\u00b7tet", "wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "PIS", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Portep", "tokens": ["Ein", "Por\u00b7tep"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ein Oberhofprediger erhielt einen Orden,", "tokens": ["Ein", "O\u00b7ber\u00b7hof\u00b7pre\u00b7di\u00b7ger", "er\u00b7hielt", "ei\u00b7nen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Lakaien erhielten silberne Borden,", "tokens": ["Die", "La\u00b7kai\u00b7en", "er\u00b7hiel\u00b7ten", "sil\u00b7ber\u00b7ne", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die h\u00f6chsten Herrschaften gehen nach Norden", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Herr\u00b7schaf\u00b7ten", "ge\u00b7hen", "nach", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und zeitig ist es Fr\u00fchling geworden \u2014", "tokens": ["Und", "zei\u00b7tig", "ist", "es", "Fr\u00fch\u00b7ling", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "NN", "VAPP", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wie interessant, wie interessant!", "tokens": ["Wie", "in\u00b7ter\u00b7es\u00b7sant", ",", "wie", "in\u00b7ter\u00b7es\u00b7sant", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Gott segne das liebe Vaterland!", "tokens": ["Gott", "seg\u00b7ne", "das", "lie\u00b7be", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}