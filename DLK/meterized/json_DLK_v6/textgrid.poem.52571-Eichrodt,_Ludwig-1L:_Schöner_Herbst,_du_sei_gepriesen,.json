{"textgrid.poem.52571": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6ner Herbst, du sei gepriesen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner Herbst, du sei gepriesen,", "tokens": ["Sch\u00f6\u00b7ner", "Herbst", ",", "du", "sei", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winzer mir und Winzerin!", "tokens": ["Win\u00b7zer", "mir", "und", "Win\u00b7ze\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Spiel, Gesang und T\u00e4nzen", "tokens": ["Un\u00b7ter", "Spiel", ",", "Ge\u00b7sang", "und", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilte froh der Tag dahin.", "tokens": ["Eil\u00b7te", "froh", "der", "Tag", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Duftig stieg die Nacht hernieder,", "tokens": ["Duf\u00b7tig", "stieg", "die", "Nacht", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nur voller t\u00f6nt der Chor,", "tokens": ["Und", "nur", "vol\u00b7ler", "t\u00f6nt", "der", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Fackel lustge Flamme", "tokens": ["Und", "der", "Fa\u00b7ckel", "lust\u00b7ge", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Brauste mit dem Wind empor \u2013", "tokens": ["Braus\u00b7te", "mit", "dem", "Wind", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf den H\u00fcgeln, schwellenden Rebenh\u00fcgeln.", "tokens": ["Auf", "den", "H\u00fc\u00b7geln", ",", "schwel\u00b7len\u00b7den", "Re\u00b7ben\u00b7h\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber, K\u00f6nigin der Feste,", "tokens": ["A\u00b7ber", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Fes\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner denk ich immerdar \u2013", "tokens": ["Dei\u00b7ner", "denk", "ich", "im\u00b7mer\u00b7dar", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O! ich war so \u00fcbergl\u00fccklich,", "tokens": ["O", "!", "ich", "war", "so", "\u00fc\u00b7berg\u00b7l\u00fcck\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ich um die Holde war!", "tokens": ["Als", "ich", "um", "die", "Hol\u00b7de", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "S\u00fc\u00dfes seliges Begleiten,", "tokens": ["S\u00fc\u00b7\u00dfes", "se\u00b7li\u00b7ges", "Be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trautes Beieinandersein,", "tokens": ["Trau\u00b7tes", "Bei\u00b7ein\u00b7an\u00b7der\u00b7sein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Stumme Sprache mit den Blicken,", "tokens": ["Stum\u00b7me", "Spra\u00b7che", "mit", "den", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter Liederklang und Wein \u2013", "tokens": ["Un\u00b7ter", "Lie\u00b7der\u00b7klang", "und", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Kehret wieder, frohe sch\u00f6ne Tage!", "tokens": ["Keh\u00b7ret", "wie\u00b7der", ",", "fro\u00b7he", "sch\u00f6\u00b7ne", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Jaget nicht so rasch, ihr Wellen,", "tokens": ["Ja\u00b7get", "nicht", "so", "rasch", ",", "ihr", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An Erinnerungen reich!", "tokens": ["An", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "reich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Habt ihr, sprecht, sie nicht gesehen,", "tokens": ["Habt", "ihr", ",", "sprecht", ",", "sie", "nicht", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVFIN", "$,", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schickt sie keinen Gru\u00df mit euch?", "tokens": ["Schickt", "sie", "kei\u00b7nen", "Gru\u00df", "mit", "euch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Droben an den Rebenh\u00fcgeln", "tokens": ["Dro\u00b7ben", "an", "den", "Re\u00b7ben\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flo\u00dft vor\u00fcber ihr einmal,", "tokens": ["Flo\u00dft", "vor\u00b7\u00fc\u00b7ber", "ihr", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schautet ihr herunter gr\u00fc\u00dfen", "tokens": ["Schau\u00b7tet", "ihr", "her\u00b7un\u00b7ter", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht zwei Augen in das Thal,", "tokens": ["Nicht", "zwei", "Au\u00b7gen", "in", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Thr\u00e4nenvolle sch\u00f6ne blaue Augen?", "tokens": ["Thr\u00e4\u00b7nen\u00b7vol\u00b7le", "sch\u00f6\u00b7ne", "blau\u00b7e", "Au\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}