{"dta.poem.9209": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.  Satz", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Die kuh wird auf dem seile tantzen/", "tokens": ["Die", "kuh", "wird", "auf", "dem", "sei\u00b7le", "tant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ochse wird Latein verstehn/", "tokens": ["Der", "oc\u00b7hse", "wird", "La\u00b7tein", "ver\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der bock wird junge b\u00e4umgen pflantzen/", "tokens": ["Der", "bock", "wird", "jun\u00b7ge", "b\u00e4um\u00b7gen", "pflant\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sau zur juden-hochzeit gehn/", "tokens": ["Die", "sau", "zur", "ju\u00b7den\u00b7hoch\u00b7zeit", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der kater wird die messe singen/", "tokens": ["Der", "ka\u00b7ter", "wird", "die", "mes\u00b7se", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die henne wird den fuchs verschlingen.", "tokens": ["Die", "hen\u00b7ne", "wird", "den", "fuchs", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der esel wird die laute schlagen/", "tokens": ["Der", "e\u00b7sel", "wird", "die", "lau\u00b7te", "schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die schafe werden feuer spr\u00fchn/", "tokens": ["Die", "scha\u00b7fe", "wer\u00b7den", "feu\u00b7er", "spr\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der b\u00e4r wird sammt und seide tragen/", "tokens": ["Der", "b\u00e4r", "wird", "sammt", "und", "sei\u00b7de", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wolff wird an dem karne ziehn/", "tokens": ["Der", "wolff", "wird", "an", "dem", "kar\u00b7ne", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das pferd wird mu\u00df und suppe lecken/", "tokens": ["Das", "pferd", "wird", "mu\u00df", "und", "sup\u00b7pe", "le\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VMFIN", "KON", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem hunde wird der haber schmecken.", "tokens": ["Dem", "hun\u00b7de", "wird", "der", "ha\u00b7ber", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der hase wird calender machen/", "tokens": ["Der", "ha\u00b7se", "wird", "ca\u00b7len\u00b7der", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der wiedehopff wird geld verstreun/", "tokens": ["Der", "wie\u00b7de\u00b7hopff", "wird", "geld", "ver\u00b7streun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ratze wird am thore wachen/", "tokens": ["Die", "rat\u00b7ze", "wird", "am", "tho\u00b7re", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der affe wird ein doctor seyn/", "tokens": ["Der", "af\u00b7fe", "wird", "ein", "doc\u00b7tor", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das eichhorn wird die fr\u00f6sche fangen/", "tokens": ["Das", "eich\u00b7horn", "wird", "die", "fr\u00f6\u00b7sche", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der storch wird haseln\u00fcsse langen.", "tokens": ["Der", "storch", "wird", "ha\u00b7sel\u00b7n\u00fcs\u00b7se", "lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der guggug wird ", "tokens": ["Der", "gug\u00b7gug", "wird"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das murmelth\u00fcr wird freundlich thun/", "tokens": ["Das", "mur\u00b7melt\u00b7h\u00fcr", "wird", "freund\u00b7lich", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die taube wird im miste w\u00fchlen/", "tokens": ["Die", "tau\u00b7be", "wird", "im", "mis\u00b7te", "w\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wachtel wird im wasser ruhn/", "tokens": ["Die", "wach\u00b7tel", "wird", "im", "was\u00b7ser", "ruhn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So fern ich mich so hoch vermesse/", "tokens": ["So", "fern", "ich", "mich", "so", "hoch", "ver\u00b7mes\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich der Rosilis vergesse.", "tokens": ["Da\u00df", "ich", "der", "Ro\u00b7si\u00b7lis", "ver\u00b7ges\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}