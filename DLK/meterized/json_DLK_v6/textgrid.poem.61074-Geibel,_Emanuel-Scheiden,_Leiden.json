{"textgrid.poem.61074": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Scheiden, Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und bist du fern, und bist du weit", "tokens": ["Und", "bist", "du", "fern", ",", "und", "bist", "du", "weit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und z\u00fcrnst noch immer mir,", "tokens": ["Und", "z\u00fcrnst", "noch", "im\u00b7mer", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch Tag und Nacht voll Traurigkeit", "tokens": ["Doch", "Tag", "und", "Nacht", "voll", "Trau\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist all mein Sinn bei dir.", "tokens": ["Ist", "all", "mein", "Sinn", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich denk' an deine Augen blau", "tokens": ["Ich", "denk'", "an", "dei\u00b7ne", "Au\u00b7gen", "blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und an dein Herz dazu -", "tokens": ["Und", "an", "dein", "Herz", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach, keine, keine find' ich je,", "tokens": ["Ach", ",", "kei\u00b7ne", ",", "kei\u00b7ne", "find'", "ich", "je", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "$,", "PIAT", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die so mich liebt wie du.", "tokens": ["Die", "so", "mich", "liebt", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie stand die Welt in Rosen sch\u00f6n,", "tokens": ["Wie", "stand", "die", "Welt", "in", "Ro\u00b7sen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich bei dir noch war;", "tokens": ["Da", "ich", "bei", "dir", "noch", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da rauscht' es gr\u00fcn von allen H\u00f6hn,", "tokens": ["Da", "rauscht'", "es", "gr\u00fcn", "von", "al\u00b7len", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da schien der Mond so klar.", "tokens": ["Da", "schien", "der", "Mond", "so", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du brachst die Ros', ich k\u00fc\u00dfte dich,", "tokens": ["Du", "brachst", "die", "Ros'", ",", "ich", "k\u00fc\u00df\u00b7te", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich k\u00fc\u00dft' und sang dazu:", "tokens": ["Ich", "k\u00fc\u00dft'", "und", "sang", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wohl keine, keine find' ich je,", "tokens": ["Wohl", "kei\u00b7ne", ",", "kei\u00b7ne", "find'", "ich", "je", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "$,", "PIAT", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die so mich liebt wie du.", "tokens": ["Die", "so", "mich", "liebt", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wohl bin ich frei nun wie der Falk,", "tokens": ["Wohl", "bin", "ich", "frei", "nun", "wie", "der", "Falk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der \u00fcber die Berge fliegt,", "tokens": ["Der", "\u00fc\u00b7ber", "die", "Ber\u00b7ge", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vor dem die Welt, die sch\u00f6ne Welt", "tokens": ["Vor", "dem", "die", "Welt", ",", "die", "sch\u00f6\u00b7ne", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hellsonnig offen liegt;", "tokens": ["Hell\u00b7son\u00b7nig", "of\u00b7fen", "liegt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Doch hat der Falk sein heimisch Nest,", "tokens": ["Doch", "hat", "der", "Falk", "sein", "hei\u00b7misch", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wo wird mir einst Ruh'?", "tokens": ["Und", "wo", "wird", "mir", "einst", "Ruh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach, keine, keine find' ich je,", "tokens": ["Ach", ",", "kei\u00b7ne", ",", "kei\u00b7ne", "find'", "ich", "je", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "$,", "PIAT", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die so mich liebt wie du.", "tokens": ["Die", "so", "mich", "liebt", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O schlimmer Tag, o schlimme Stund'", "tokens": ["O", "schlim\u00b7mer", "Tag", ",", "o", "schlim\u00b7me", "Stund'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die uns f\u00fcr immer schied!", "tokens": ["Die", "uns", "f\u00fcr", "im\u00b7mer", "schied", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sind aus meines Herzens Grund", "tokens": ["Da", "sind", "aus", "mei\u00b7nes", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschieden Freud' und Fried'.", "tokens": ["Ge\u00b7schie\u00b7den", "Freud'", "und", "Fried'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun such' ich wohl durch Land und See", "tokens": ["Nun", "such'", "ich", "wohl", "durch", "Land", "und", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und habe nicht Rast noch Ruh';", "tokens": ["Und", "ha\u00b7be", "nicht", "Rast", "noch", "Ruh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch keine, keine find' ich je,", "tokens": ["Doch", "kei\u00b7ne", ",", "kei\u00b7ne", "find'", "ich", "je", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "$,", "PIAT", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die so mich liebt wie du.", "tokens": ["Die", "so", "mich", "liebt", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}