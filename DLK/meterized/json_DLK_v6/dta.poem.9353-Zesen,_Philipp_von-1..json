{"dta.poem.9353": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hatt nun mehr das g\u00fcldne Licht", "tokens": ["Es", "hatt", "nun", "mehr", "das", "g\u00fcld\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "des Himmels seinen Lauff verricht/", "tokens": ["des", "Him\u00b7mels", "sei\u00b7nen", "Lauff", "ver\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der tag hatt sich geneiget:", "tokens": ["der", "tag", "hatt", "sich", "ge\u00b7nei\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der blasse Mond steht auf der wacht/", "tokens": ["Der", "blas\u00b7se", "Mond", "steht", "auf", "der", "wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Sternen lenchten durch die nacht/", "tokens": ["die", "Ster\u00b7nen", "lench\u00b7ten", "durch", "die", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "der s\u00fc\u00dfe schlaff sich zeiget.", "tokens": ["der", "s\u00fc\u00b7\u00dfe", "schlaff", "sich", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ey nun will ich in stoltzer ruh/", "tokens": ["Ey", "nun", "will", "ich", "in", "stolt\u00b7zer", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Nacht mit schlaffen bringen zu/", "tokens": ["die", "Nacht", "mit", "schlaf\u00b7fen", "brin\u00b7gen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVINF", "VVINF", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "erm\u00fcdet von studieren/", "tokens": ["er\u00b7m\u00fc\u00b7det", "von", "stu\u00b7die\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "das durch den langen tag ich trieb/", "tokens": ["das", "durch", "den", "lan\u00b7gen", "tag", "ich", "trieb", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "bi\u00df mier die nacht den pa\u00df verhieb", "tokens": ["bi\u00df", "mier", "die", "nacht", "den", "pa\u00df", "ver\u00b7hieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die feder fort zuf\u00fchren.", "tokens": ["die", "fe\u00b7der", "fort", "zu\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Indessen sey mein glantz und Licht", "tokens": ["In\u00b7des\u00b7sen", "sey", "mein", "glantz", "und", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dein freudenreiches Angesicht/", "tokens": ["dein", "freu\u00b7den\u00b7rei\u00b7ches", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "du Sonne meiner Seelen/", "tokens": ["du", "Son\u00b7ne", "mei\u00b7ner", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da\u00df nicht der N\u00e4chte schatten mich", "tokens": ["da\u00df", "nicht", "der", "N\u00e4ch\u00b7te", "schat\u00b7ten", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit furcht und schrecken inniglich", "tokens": ["mit", "furcht", "und", "schre\u00b7cken", "in\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "VVFIN", "ADJD"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "im hertzen m\u00f6ge quelen.", "tokens": ["im", "hert\u00b7zen", "m\u00f6\u00b7ge", "que\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nim weg den schweren s\u00fcnden-schwall/", "tokens": ["Nim", "weg", "den", "schwe\u00b7ren", "s\u00fcn\u00b7den\u00b7schwall", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so sich ereiget \u00fcberall/", "tokens": ["so", "sich", "er\u00b7ei\u00b7get", "\u00fc\u00b7be\u00b7rall", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "aus meines Hertzens-schrancken/", "tokens": ["aus", "mei\u00b7nes", "Hert\u00b7zens\u00b7schran\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "da\u00df ich fein sanfte ruhen mag", "tokens": ["da\u00df", "ich", "fein", "sanf\u00b7te", "ru\u00b7hen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ADJA", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und wann nun k\u00f6mpt der fr\u00fche-tag", "tokens": ["und", "wann", "nun", "k\u00f6mpt", "der", "fr\u00fc\u00b7he\u00b7tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "dier H\u00f6chster freudig dancken.", "tokens": ["dier", "H\u00f6chs\u00b7ter", "freu\u00b7dig", "dan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hiermit will ich nun schlaffen ein", "tokens": ["Hier\u00b7mit", "will", "ich", "nun", "schlaf\u00b7fen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Dier/ ", "tokens": ["und", "Dier", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "du wirst mich wohl erretten;", "tokens": ["du", "wirst", "mich", "wohl", "er\u00b7ret\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beh\u00fcte mich f\u00fcr schnellen todt/", "tokens": ["Be\u00b7h\u00fc\u00b7te", "mich", "f\u00fcr", "schnel\u00b7len", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "f\u00fcr aller angst und krieges-noth", "tokens": ["f\u00fcr", "al\u00b7ler", "angst", "und", "krie\u00b7ge\u00b7snoth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und f\u00fcr des Teufels ketten.", "tokens": ["und", "f\u00fcr", "des", "Teu\u00b7fels", "ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}