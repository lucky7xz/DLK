{"textgrid.poem.56176": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Vi", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem sind wir nah? Dem Tode oder dem,", "tokens": ["Wem", "sind", "wir", "nah", "?", "Dem", "To\u00b7de", "o\u00b7der", "dem", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "$.", "ART", "NN", "KON", "ART", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "was noch nicht ist? Was w\u00e4re Lehm an Lehm,", "tokens": ["was", "noch", "nicht", "ist", "?", "Was", "w\u00e4\u00b7re", "Lehm", "an", "Lehm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VAFIN", "$.", "PWS", "VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "formte der Gott nicht f\u00fchlend die Figur,", "tokens": ["form\u00b7te", "der", "Gott", "nicht", "f\u00fch\u00b7lend", "die", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "die zwischen uns erw\u00e4chst. Begreife nur:", "tokens": ["die", "zwi\u00b7schen", "uns", "er\u00b7w\u00e4chst", ".", "Be\u00b7grei\u00b7fe", "nur", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$.", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "das ist mein K\u00f6rper, welcher aufersteht.", "tokens": ["das", "ist", "mein", "K\u00f6r\u00b7per", ",", "wel\u00b7cher", "auf\u00b7er\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nun hilf ihm leise aus dem hei\u00dfen Grabe", "tokens": ["Nun", "hilf", "ihm", "lei\u00b7se", "aus", "dem", "hei\u00b7\u00dfen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "in jenen Himmel, den ich in dir habe:", "tokens": ["in", "je\u00b7nen", "Him\u00b7mel", ",", "den", "ich", "in", "dir", "ha\u00b7be", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "da\u00df k\u00fchn aus ihm das \u00dcberleben geht.", "tokens": ["da\u00df", "k\u00fchn", "aus", "ihm", "das", "\u00dc\u00b7berl\u00b7e\u00b7ben", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Du junger Ort, der tiefen Himmelfahrt.", "tokens": ["Du", "jun\u00b7ger", "Ort", ",", "der", "tie\u00b7fen", "Him\u00b7mel\u00b7fahrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Du dunkle Luft voll sommerlicher Pollen.", "tokens": ["Du", "dunk\u00b7le", "Luft", "voll", "som\u00b7mer\u00b7li\u00b7cher", "Pol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wenn ihre tausend Geister in dir tollen,", "tokens": ["Wenn", "ih\u00b7re", "tau\u00b7send", "Geis\u00b7ter", "in", "dir", "tol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "CARD", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "wird meine steife Leiche wieder zart.", "tokens": ["wird", "mei\u00b7ne", "stei\u00b7fe", "Lei\u00b7che", "wie\u00b7der", "zart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}