{"textgrid.poem.26511": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fche schon aus leisem Schlummer", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fche schon aus leisem Schlummer", "tokens": ["Fr\u00fc\u00b7he", "schon", "aus", "lei\u00b7sem", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6rt mich auf der wache Kummer,", "tokens": ["St\u00f6rt", "mich", "auf", "der", "wa\u00b7che", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit stumm getrag'ner Pein", "tokens": ["Und", "mit", "stumm", "ge\u00b7trag'\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schreit' ich in den Tag hinein.", "tokens": ["Schreit'", "ich", "in", "den", "Tag", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer schwerer das Vollbringen,", "tokens": ["Im\u00b7mer", "schwe\u00b7rer", "das", "Voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer selt'ner das Gelingen,", "tokens": ["Im\u00b7mer", "selt'\u00b7ner", "das", "Ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es schwindet die Geduld \u2013", "tokens": ["Und", "es", "schwin\u00b7det", "die", "Ge\u00b7duld", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich f\u00fchl' die eig'ne Schuld.", "tokens": ["Und", "ich", "f\u00fchl'", "die", "eig'\u00b7ne", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00fchl' es mit geheimem Beben:", "tokens": ["F\u00fchl'", "es", "mit", "ge\u00b7hei\u00b7mem", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uferlos verrinnt mein Leben", "tokens": ["U\u00b7fer\u00b7los", "ver\u00b7rinnt", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In ein Meer von Qual und Noth \u2013", "tokens": ["In", "ein", "Meer", "von", "Qual", "und", "Noth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm', o komme, Tod!", "tokens": ["Komm'", ",", "o", "kom\u00b7me", ",", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}