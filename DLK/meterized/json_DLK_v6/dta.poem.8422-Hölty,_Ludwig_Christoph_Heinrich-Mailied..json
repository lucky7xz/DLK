{"dta.poem.8422": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mailied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mai beginnt,", "tokens": ["Der", "Mai", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Bl\u00fcten keimen", "tokens": ["Die", "Bl\u00fc\u00b7ten", "kei\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auf Gartenb\u00e4umen,", "tokens": ["Auf", "Gar\u00b7ten\u00b7b\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und Vogelschall", "tokens": ["Und", "Vo\u00b7gel\u00b7schall"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "T\u00f6nt \u00fcberall.", "tokens": ["T\u00f6nt", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Pfl\u00fcckt einen Kranz,", "tokens": ["Pfl\u00fcckt", "ei\u00b7nen", "Kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und haltet Tanz", "tokens": ["Und", "hal\u00b7tet", "Tanz"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auf gr\u00fcnen Auen,", "tokens": ["Auf", "gr\u00fc\u00b7nen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ihr sch\u00f6nen Frauen,", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wo junge Main", "tokens": ["Wo", "jun\u00b7ge", "Main"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Uns K\u00fchlung streun.", "tokens": ["Uns", "K\u00fch\u00b7lung", "streun", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wer weiss, wie bald", "tokens": ["Wer", "weiss", ",", "wie", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Klocke schallt,", "tokens": ["Die", "Klo\u00b7cke", "schallt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da wir des Maien", "tokens": ["Da", "wir", "des", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Uns nicht mehr freuen:", "tokens": ["Uns", "nicht", "mehr", "freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wer weiss, wie bald", "tokens": ["Wer", "weiss", ",", "wie", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Klocke schallt!", "tokens": ["Die", "Klo\u00b7cke", "schallt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Drum werdet froh!", "tokens": ["Drum", "wer\u00b7det", "froh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gott will es so,", "tokens": ["Gott", "will", "es", "so", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der uns dies Leben", "tokens": ["Der", "uns", "dies", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PDS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zur Lust gegeben!", "tokens": ["Zur", "Lust", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Geniesst der Zeit,", "tokens": ["Ge\u00b7niesst", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Gott verleiht!", "tokens": ["Die", "Gott", "ver\u00b7leiht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}