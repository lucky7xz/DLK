{"textgrid.poem.45871": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Lureley", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Bacharach am Rheine,", "tokens": ["Zu", "Bac\u00b7ha\u00b7rach", "am", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohnt eine Zauberin,", "tokens": ["Wohnt", "ei\u00b7ne", "Zau\u00b7be\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die war so sch\u00f6n und feine", "tokens": ["Die", "war", "so", "sch\u00f6n", "und", "fei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ri\u00df viel Herzen hin,", "tokens": ["Und", "ri\u00df", "viel", "Her\u00b7zen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und machte viel zuschanden", "tokens": ["Und", "mach\u00b7te", "viel", "zu\u00b7schan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der M\u00e4nner rings umher,", "tokens": ["Der", "M\u00e4n\u00b7ner", "rings", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus ihren Liebesbanden", "tokens": ["Aus", "ih\u00b7ren", "Lie\u00b7bes\u00b7ban\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War keine Rettung mehr.", "tokens": ["War", "kei\u00b7ne", "Ret\u00b7tung", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Bischof lie\u00df sie laden", "tokens": ["Der", "Bi\u00b7schof", "lie\u00df", "sie", "la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor geistliche Gewalt,", "tokens": ["Vor", "geist\u00b7li\u00b7che", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mu\u00dfte sie begnaden,", "tokens": ["Und", "mu\u00df\u00b7te", "sie", "be\u00b7gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sch\u00f6n war ihr' Gestalt.", "tokens": ["So", "sch\u00f6n", "war", "ih\u00b7r'", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er sprach zu ihr ger\u00fchret,", "tokens": ["Er", "sprach", "zu", "ihr", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdu arme Lore Lay.", "tokens": ["\u00bb", "du", "ar\u00b7me", "Lo\u00b7re", "Lay", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer hat dich dann verf\u00fchret", "tokens": ["Wer", "hat", "dich", "dann", "ver\u00b7f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu b\u00f6ser Zauberei.\u00ab", "tokens": ["Zu", "b\u00f6\u00b7ser", "Zau\u00b7be\u00b7rei", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbherr Bischof la\u00dft mich sterben,", "tokens": ["\u00bb", "herr", "Bi\u00b7schof", "la\u00dft", "mich", "ster\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin des Lebens m\u00fcd,", "tokens": ["Ich", "bin", "des", "Le\u00b7bens", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil jeder mu\u00df verderben", "tokens": ["Weil", "je\u00b7der", "mu\u00df", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VMFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der meine Augen sieht.", "tokens": ["Der", "mei\u00b7ne", "Au\u00b7gen", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Augen sind zwei Flammen,", "tokens": ["Die", "Au\u00b7gen", "sind", "zwei", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Arm ein Zauberstab,", "tokens": ["Mein", "Arm", "ein", "Zau\u00b7ber\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O schickt mich in die Flammen,", "tokens": ["O", "schickt", "mich", "in", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O brechet mir den Stab.\u00ab", "tokens": ["O", "bre\u00b7chet", "mir", "den", "Stab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbden Stab kann ich nicht brechen,", "tokens": ["\u00bb", "den", "Stab", "kann", "ich", "nicht", "bre\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du sch\u00f6ne Lore Lay,", "tokens": ["Du", "sch\u00f6\u00b7ne", "Lo\u00b7re", "Lay", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich m\u00fc\u00dfte dann zerbrechen,", "tokens": ["Ich", "m\u00fc\u00df\u00b7te", "dann", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein eigen Herz entzwei.", "tokens": ["Mein", "ei\u00b7gen", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich kann dich nicht verdammen,", "tokens": ["Ich", "kann", "dich", "nicht", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis du mir erst bekennt", "tokens": ["Bis", "du", "mir", "erst", "be\u00b7kennt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warum in deinen Flammen", "tokens": ["Wa\u00b7rum", "in", "dei\u00b7nen", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein eignes Herz schon brennt.\u00ab", "tokens": ["Mein", "eig\u00b7nes", "Herz", "schon", "brennt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbherr Bischof mit mir Armen", "tokens": ["\u00bb", "herr", "Bi\u00b7schof", "mit", "mir", "Ar\u00b7men"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "NN", "APPR", "PPER", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Treibt nicht so b\u00f6sen Spott,", "tokens": ["Treibt", "nicht", "so", "b\u00f6\u00b7sen", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bittet um Erbarmen", "tokens": ["Und", "bit\u00b7tet", "um", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr mich den lieben Gott,", "tokens": ["F\u00fcr", "mich", "den", "lie\u00b7ben", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich darf nicht l\u00e4nger leben,", "tokens": ["Ich", "darf", "nicht", "l\u00e4n\u00b7ger", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich lieb' kein Leben mehr,", "tokens": ["Ich", "lieb'", "kein", "Le\u00b7ben", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Tod sollt ihr mir geben,", "tokens": ["Den", "Tod", "sollt", "ihr", "mir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum kam ich zu euch her.", "tokens": ["Drum", "kam", "ich", "zu", "euch", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein Mann hat mich betrogen,", "tokens": ["Ein", "Mann", "hat", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat sich von mir gewandt,", "tokens": ["Hat", "sich", "von", "mir", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Ist fort von mir gezogen", "tokens": ["Ist", "fort", "von", "mir", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKVZ", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fort in ein andres Land.", "tokens": ["Fort", "in", "ein", "and\u00b7res", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Blicke sanft und wilde,", "tokens": ["Die", "Bli\u00b7cke", "sanft", "und", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Wangen rot und wei\u00df,", "tokens": ["Die", "Wan\u00b7gen", "rot", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Worte still und milde,", "tokens": ["Die", "Wor\u00b7te", "still", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sind mein Zauberkreis.", "tokens": ["Die", "sind", "mein", "Zau\u00b7ber\u00b7kreis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich selbst mu\u00df drin verderben,", "tokens": ["Ich", "selbst", "mu\u00df", "drin", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Herz tut mir so weh,", "tokens": ["Das", "Herz", "tut", "mir", "so", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor Jammer m\u00f6cht' ich sterben,", "tokens": ["Vor", "Jam\u00b7mer", "m\u00f6cht'", "ich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ich zum Spiegel seh'.", "tokens": ["Wenn", "ich", "zum", "Spie\u00b7gel", "seh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Drum la\u00dft mein Recht mich finden,", "tokens": ["Drum", "la\u00dft", "mein", "Recht", "mich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich sterben, wie ein Christ,", "tokens": ["Mich", "ster\u00b7ben", ",", "wie", "ein", "Christ", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn alles mu\u00df verschwinden", "tokens": ["Denn", "al\u00b7les", "mu\u00df", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil er mir treulos ist.\u00ab", "tokens": ["Weil", "er", "mir", "treu\u00b7los", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Drei Ritter lie\u00df er holen:", "tokens": ["Drei", "Rit\u00b7ter", "lie\u00df", "er", "ho\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbbringt sie ins Kloster hin,", "tokens": ["\u00bb", "bringt", "sie", "ins", "Klos\u00b7ter", "hin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geh Lore! Gott befohlen,", "tokens": ["Geh", "Lo\u00b7re", "!", "Gott", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sei dein ber\u00fcckter Sinn.", "tokens": ["Sei", "dein", "be\u00b7r\u00fcck\u00b7ter", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Du sollst ein N\u00f6nnchen werden,", "tokens": ["Du", "sollst", "ein", "N\u00f6nn\u00b7chen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein N\u00f6nnchen schwarz und wei\u00df.", "tokens": ["Ein", "N\u00f6nn\u00b7chen", "schwarz", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bereite dich auf