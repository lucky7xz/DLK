{"textgrid.poem.31726": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das Nackte", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalt bleibt dein Sinn, kalt bleibt dein bestes Streben,", "tokens": ["Kalt", "bleibt", "dein", "Sinn", ",", "kalt", "bleibt", "dein", "bes\u00b7tes", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Du gleichst dem Wurme, der verlassen w\u00fchlt \u2013", "tokens": ["Du", "gleichst", "dem", "Wur\u00b7me", ",", "der", "ver\u00b7las\u00b7sen", "w\u00fchlt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begl\u00fcckt nur, wer ein warmes Menschenleben", "tokens": ["Be\u00b7gl\u00fcckt", "nur", ",", "wer", "ein", "war\u00b7mes", "Men\u00b7schen\u00b7le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinen beiden Armen einst gef\u00fchlt!", "tokens": ["Mit", "sei\u00b7nen", "bei\u00b7den", "Ar\u00b7men", "einst", "ge\u00b7f\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An dessen Herz ein ander Herz geschlagen,", "tokens": ["An", "des\u00b7sen", "Herz", "ein", "an\u00b7der", "Herz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "An dessen Haupt ein ander Haupt gelehnt,", "tokens": ["An", "des\u00b7sen", "Haupt", "ein", "an\u00b7der", "Haupt", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der von dem Strom der Liebe fortgetragen", "tokens": ["Der", "von", "dem", "Strom", "der", "Lie\u00b7be", "fort\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum Meere der Erf\u00fcllung sich gesehnt.", "tokens": ["Zum", "Mee\u00b7re", "der", "Er\u00b7f\u00fcl\u00b7lung", "sich", "ge\u00b7sehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Strom der Liebe wiegt auf blauen Wellen", "tokens": ["Der", "Strom", "der", "Lie\u00b7be", "wiegt", "auf", "blau\u00b7en", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor\u00fcber dich an blumenreichem Strand;", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "dich", "an", "blu\u00b7men\u00b7rei\u00b7chem", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Rose gr\u00fc\u00dft den st\u00fcrmischen Gesellen,", "tokens": ["Die", "Ro\u00b7se", "gr\u00fc\u00dft", "den", "st\u00fcr\u00b7mi\u00b7schen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihm nickt die Rebe von der Felsenwand.", "tokens": ["Ihm", "nickt", "die", "Re\u00b7be", "von", "der", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch weiter eilst du, bis gewalt'gen Flusses", "tokens": ["Doch", "wei\u00b7ter", "eilst", "du", ",", "bis", "ge\u00b7walt'\u00b7gen", "Flus\u00b7ses"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Ozean vor deinen Augen blinkt,", "tokens": ["Der", "O\u00b7ze\u00b7an", "vor", "dei\u00b7nen", "Au\u00b7gen", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis jauchzend im Orkane des Genusses", "tokens": ["Bis", "jauch\u00b7zend", "im", "Or\u00b7ka\u00b7ne", "des", "Ge\u00b7nus\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Dein Herz vernichtet in die Wogen sinkt.", "tokens": ["Dein", "Herz", "ver\u00b7nich\u00b7tet", "in", "die", "Wo\u00b7gen", "sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist die Taufe, draus ein neues Wesen", "tokens": ["Das", "ist", "die", "Tau\u00b7fe", ",", "draus", "ein", "neu\u00b7es", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Begl\u00fcckt entwandelt zu der Sonne Strahl.", "tokens": ["Be\u00b7gl\u00fcckt", "ent\u00b7wan\u00b7delt", "zu", "der", "Son\u00b7ne", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Liebling hat dich die Natur erlesen,", "tokens": ["Zum", "Lieb\u00b7ling", "hat", "dich", "die", "Na\u00b7tur", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ganzer Mensch warst du zum ersten Mal.", "tokens": ["Ein", "gan\u00b7zer", "Mensch", "warst", "du", "zum", "ers\u00b7ten", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Augenblick, der Alles dir erschlossen,", "tokens": ["Der", "Au\u00b7gen\u00b7blick", ",", "der", "Al\u00b7les", "dir", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er ist's, er stempelt dich sofort zum Mann;", "tokens": ["Er", "ist's", ",", "er", "stem\u00b7pelt", "dich", "so\u00b7fort", "zum", "Mann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Aus der Umarmung ist dir frisch entsprossen,", "tokens": ["Aus", "der", "Um\u00b7ar\u00b7mung", "ist", "dir", "frisch", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Worauf die Keuschheit tausend Jahre sann!", "tokens": ["Wo\u00b7rauf", "die", "Keuschheit", "tau\u00b7send", "Jah\u00b7re", "sann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "CARD", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Der das Erhabenste zu mei\u00dfeln dachte,", "tokens": ["Der", "das", "Er\u00b7ha\u00b7bens\u00b7te", "zu", "mei\u00b7\u00dfeln", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Dem weisen Griechen, ihm gelang es nur:", "tokens": ["Dem", "wei\u00b7sen", "Grie\u00b7chen", ",", "ihm", "ge\u00b7lang", "es", "nur", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ihm der Nacktheit s\u00fc\u00dfer Zauber lachte,", "tokens": ["Als", "ihm", "der", "Nackt\u00b7heit", "s\u00fc\u00b7\u00dfer", "Zau\u00b7ber", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die F\u00fclle der entschleierten Natur.", "tokens": ["Die", "F\u00fcl\u00b7le", "der", "ent\u00b7schlei\u00b7er\u00b7ten", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie das Bild, dem Marmor losgewunden,", "tokens": ["Und", "wie", "das", "Bild", ",", "dem", "Mar\u00b7mor", "los\u00b7ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So strahlt der Meister auch durch alle Zeit,", "tokens": ["So", "strahlt", "der", "Meis\u00b7ter", "auch", "durch", "al\u00b7le", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der G\u00f6ttliches im Menschen nur gefunden", "tokens": ["Der", "G\u00f6tt\u00b7li\u00b7ches", "im", "Men\u00b7schen", "nur", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Sitte nur in reiner Sinnlichkeit.", "tokens": ["Und", "Sit\u00b7te", "nur", "in", "rei\u00b7ner", "Sinn\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das oft geweint mit weinenden Madonnen,", "tokens": ["Das", "oft", "ge\u00b7weint", "mit", "wei\u00b7nen\u00b7den", "Ma\u00b7don\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Auge, das durchflog der Dome Chor:", "tokens": ["Ein", "Au\u00b7ge", ",", "das", "durch\u00b7flog", "der", "Do\u00b7me", "Chor", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es mag sich freudig auch im Glanze sonnen", "tokens": ["Es", "mag", "sich", "freu\u00b7dig", "auch", "im", "Glan\u00b7ze", "son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Heitern und der Reize frischem Flor.", "tokens": ["Des", "Hei\u00b7tern", "und", "der", "Rei\u00b7ze", "fri\u00b7schem", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was bei der Nacht geheimnisvoller Feier", "tokens": ["Was", "bei", "der", "Nacht", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7ler", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein Gott, dein Leben und dein Liebstes war:", "tokens": ["Dein", "Gott", ",", "dein", "Le\u00b7ben", "und", "dein", "Liebs\u00b7tes", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "La\u00df es beseelen Mei\u00dfel auch und Leier", "tokens": ["La\u00df", "es", "be\u00b7see\u00b7len", "Mei\u00b7\u00dfel", "auch", "und", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sich gestalten nackt und frei und klar.", "tokens": ["Und", "sich", "ge\u00b7stal\u00b7ten", "nackt", "und", "frei", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zum Schatten wandelt es das beste Leben,", "tokens": ["Zum", "Schat\u00b7ten", "wan\u00b7delt", "es", "das", "bes\u00b7te", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es hat den k\u00fchnsten Adler schon gel\u00e4hmt,", "tokens": ["Es", "hat", "den", "k\u00fchns\u00b7ten", "Ad\u00b7ler", "schon", "ge\u00b7l\u00e4hmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sich die Kraft in ihrem vollsten Streben", "tokens": ["Wenn", "sich", "die", "Kraft", "in", "ih\u00b7rem", "volls\u00b7ten", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erzitternd der Nat\u00fcrlichkeit gesch\u00e4mt.", "tokens": ["Er\u00b7zit\u00b7ternd", "der", "Na\u00b7t\u00fcr\u00b7lich\u00b7keit", "ge\u00b7sch\u00e4mt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie die Natur in ihrer ew'gen Sch\u00f6ne,", "tokens": ["Wie", "die", "Na\u00b7tur", "in", "ih\u00b7rer", "ew'\u00b7gen", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In edler Nacktheit schimmert nur allein,", "tokens": ["In", "ed\u00b7ler", "Nackt\u00b7heit", "schim\u00b7mert", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So m\u00f6gen ihre T\u00f6chter auch und S\u00f6hne", "tokens": ["So", "m\u00f6\u00b7gen", "ih\u00b7re", "T\u00f6ch\u00b7ter", "auch", "und", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht f\u00fcrchten, sinnlich, wie sie sind, zu sein.", "tokens": ["Nicht", "f\u00fcrch\u00b7ten", ",", "sinn\u00b7lich", ",", "wie", "sie", "sind", ",", "zu", "sein", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}