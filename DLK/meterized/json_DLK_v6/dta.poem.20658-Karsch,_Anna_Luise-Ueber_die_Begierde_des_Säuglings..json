{"dta.poem.20658": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Ueber  \n  die Begierde des S\u00e4uglings.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob Weizen reift zu Semmel oder Kuchen,                 ", "tokens": ["Ob", "Wei\u00b7zen", "reift", "zu", "Sem\u00b7mel", "o\u00b7der", "Ku\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dar\u00fcber sorgt der S\u00e4ugling nicht,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "sorgt", "der", "S\u00e4ug\u00b7ling", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der einen Busen wei\u00df zu suchen,", "tokens": ["Der", "ei\u00b7nen", "Bu\u00b7sen", "wei\u00df", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lallend mit der Amme spricht.", "tokens": ["Und", "lal\u00b7lend", "mit", "der", "Am\u00b7me", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er bittet nicht um Regen oder helle", "tokens": ["Er", "bit\u00b7tet", "nicht", "um", "Re\u00b7gen", "o\u00b7der", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Lerchenchor durchsungne Luft,", "tokens": ["Vom", "Ler\u00b7chen\u00b7chor", "durch\u00b7sung\u00b7ne", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn selbst die halbversiegte Quelle", "tokens": ["Wenn", "selbst", "die", "halb\u00b7ver\u00b7sieg\u00b7te", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Jupiter um N\u00e4sse ruft.", "tokens": ["Zum", "Ju\u00b7pi\u00b7ter", "um", "N\u00e4s\u00b7se", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er kennet keine G\u00fcter, des Bestrebens,", "tokens": ["Er", "ken\u00b7net", "kei\u00b7ne", "G\u00fc\u00b7ter", ",", "des", "Be\u00b7stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Wunsches seiner Seele werth,", "tokens": ["Des", "Wun\u00b7sches", "sei\u00b7ner", "See\u00b7le", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm ist das ganze Gl\u00fcck des Lebens", "tokens": ["Ihm", "ist", "das", "gan\u00b7ze", "Gl\u00fcck", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die volle Brust, die ihn ern\u00e4hrt.", "tokens": ["Die", "vol\u00b7le", "Brust", ",", "die", "ihn", "er\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nach ihr verlangt er hei\u00dfer als die Schaaren", "tokens": ["Nach", "ihr", "ver\u00b7langt", "er", "hei\u00b7\u00dfer", "als", "die", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der R\u00f6mer bey dem Marc Anton", "tokens": ["Der", "R\u00f6\u00b7mer", "bey", "dem", "Marc", "An\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Wasser, als sie schmachtend waren,", "tokens": ["Nach", "Was\u00b7ser", ",", "als", "sie", "schmach\u00b7tend", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00e4mpfend vor den Parther flohn.", "tokens": ["Und", "k\u00e4mp\u00b7fend", "vor", "den", "Par\u00b7ther", "flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "An diese Brust f\u00e4llt er mit gr\u00f6\u00dferm Geize", "tokens": ["An", "die\u00b7se", "Brust", "f\u00e4llt", "er", "mit", "gr\u00f6\u00b7\u00dferm", "Gei\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ein verliebter J\u00fcnglingsmund", "tokens": ["Als", "ein", "ver\u00b7lieb\u00b7ter", "J\u00fcng\u00b7lings\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Lippen, die durch ihre Reize", "tokens": ["An", "Lip\u00b7pen", ",", "die", "durch", "ih\u00b7re", "Rei\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein junges Herze machten wund.", "tokens": ["Sein", "jun\u00b7ges", "Her\u00b7ze", "mach\u00b7ten", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wenn er nun dies erste Gl\u00fcck verlieret", "tokens": ["Und", "wenn", "er", "nun", "dies", "ers\u00b7te", "Gl\u00fcck", "ver\u00b7lie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "PDS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seinen ersten Kummer weint,", "tokens": ["Und", "sei\u00b7nen", "ers\u00b7ten", "Kum\u00b7mer", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird seine Mutter tief ger\u00fchret,", "tokens": ["Wird", "sei\u00b7ne", "Mut\u00b7ter", "tief", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihm zur Traurigkeit vereint.", "tokens": ["Mit", "ihm", "zur", "Trau\u00b7rig\u00b7keit", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es d\u00fcnkt ihr hart, den S\u00e4ugling so zu qu\u00e4len,", "tokens": ["Es", "d\u00fcnkt", "ihr", "hart", ",", "den", "S\u00e4ug\u00b7ling", "so", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und doch ists ein nothwendig Mu\u00df:", "tokens": ["Und", "doch", "ists", "ein", "noth\u00b7wen\u00b7dig", "Mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJD", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So weislich l\u00e4\u00dft der Himmel fehlen", "tokens": ["So", "weis\u00b7lich", "l\u00e4\u00dft", "der", "Him\u00b7mel", "feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns Gr\u00f6\u00dfern oft den Ueberflu\u00df.", "tokens": ["Uns", "Gr\u00f6\u00b7\u00dfern", "oft", "den", "Ue\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er th\u00e4t es nie, wenn nicht Sein Auge w\u00fc\u00dfte,", "tokens": ["Er", "th\u00e4t", "es", "nie", ",", "wenn", "nicht", "Sein", "Au\u00b7ge", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was jedem Menschen n\u00fctzlich sey,", "tokens": ["Was", "je\u00b7dem", "Men\u00b7schen", "n\u00fctz\u00b7lich", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er nimmt die Nahrung unsrer L\u00fcste", "tokens": ["Er", "nimmt", "die", "Nah\u00b7rung", "uns\u00b7rer", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und legt uns etwas Be\u00dfres bey.", "tokens": ["Und", "legt", "uns", "et\u00b7was", "Be\u00df\u00b7res", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}