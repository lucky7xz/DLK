{"dta.poem.19430": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "163.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieblingsaufenthalt, noch einmal recht zum Schlu\u00df", "tokens": ["Mein", "Lieb\u00b7lings\u00b7au\u00b7fent\u00b7halt", ",", "noch", "ein\u00b7mal", "recht", "zum", "Schlu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lachst du mich freundlich an, eh ich dich lassen mu\u00df.", "tokens": ["Lachst", "du", "mich", "freund\u00b7lich", "an", ",", "eh", "ich", "dich", "las\u00b7sen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gern thatest du es ehr, das Wetter litt es nicht,", "tokens": ["Gern", "tha\u00b7test", "du", "es", "ehr", ",", "das", "Wet\u00b7ter", "litt", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch l\u00e4cheln hilft dir nun Herbstabendpurpurlicht.", "tokens": ["Doch", "l\u00e4\u00b7cheln", "hilft", "dir", "nun", "Herbs\u00b7ta\u00b7bend\u00b7pur\u00b7pur\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja l\u00e4cheln helfe dir der Himmel und die Erde!", "tokens": ["Ja", "l\u00e4\u00b7cheln", "hel\u00b7fe", "dir", "der", "Him\u00b7mel", "und", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer wei\u00df, ob ich so sch\u00f6n noch einmal sehn dich werde.", "tokens": ["Wer", "wei\u00df", ",", "ob", "ich", "so", "sch\u00f6n", "noch", "ein\u00b7mal", "sehn", "dich", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADV", "ADV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ists doch als w\u00fc\u00dftest du's, da\u00df nun sich mu\u00df entscheiden,", "tokens": ["Ists", "doch", "als", "w\u00fc\u00df\u00b7test", "du's", ",", "da\u00df", "nun", "sich", "mu\u00df", "ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KOKOM", "VVFIN", "PIS", "$,", "KOUS", "ADV", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob ich dich k\u00fcnftig noch besuchen soll, ob meiden.", "tokens": ["Ob", "ich", "dich", "k\u00fcnf\u00b7tig", "noch", "be\u00b7su\u00b7chen", "soll", ",", "ob", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So schm\u00fcckst du dich gefallbegierig meinen Blicken,", "tokens": ["So", "schm\u00fcckst", "du", "dich", "ge\u00b7fall\u00b7be\u00b7gie\u00b7rig", "mei\u00b7nen", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und von Gewohntem l\u00e4\u00dft sich gern mein Herz bestricken.", "tokens": ["Und", "von", "Ge\u00b7wohn\u00b7tem", "l\u00e4\u00dft", "sich", "gern", "mein", "Herz", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PRF", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wo bl\u00fchte mir ein Gl\u00fcck, wie das dein Schoo\u00df mir trug?", "tokens": ["Wo", "bl\u00fch\u00b7te", "mir", "ein", "Gl\u00fcck", ",", "wie", "das", "dein", "Schoo\u00df", "mir", "trug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschr\u00e4nkt, mir aussichtreich, klein, eng, mir gro\u00df genug;", "tokens": ["Be\u00b7schr\u00e4nkt", ",", "mir", "aus\u00b7sicht\u00b7reich", ",", "klein", ",", "eng", ",", "mir", "gro\u00df", "ge\u00b7nug", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Ansprechend anspruchlos, lieb, weil vorlieb ich nehme,", "tokens": ["An\u00b7spre\u00b7chend", "an\u00b7spruch\u00b7los", ",", "lieb", ",", "weil", "vor\u00b7lieb", "ich", "neh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "$,", "KOUS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Behaglich und bequem, weil ich mich still bequeme:", "tokens": ["Be\u00b7hag\u00b7lich", "und", "be\u00b7quem", ",", "weil", "ich", "mich", "still", "be\u00b7que\u00b7me", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Freu dich! noch manchen Herbst sollst du mich wieder sehn,", "tokens": ["Freu", "dich", "!", "noch", "man\u00b7chen", "Herbst", "sollst", "du", "mich", "wie\u00b7der", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "PIAT", "NN", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Lieder, diesen gleich, auf deiner Flur entstehn.", "tokens": ["Und", "Lie\u00b7der", ",", "die\u00b7sen", "gleich", ",", "auf", "dei\u00b7ner", "Flur", "ent\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PDAT", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}