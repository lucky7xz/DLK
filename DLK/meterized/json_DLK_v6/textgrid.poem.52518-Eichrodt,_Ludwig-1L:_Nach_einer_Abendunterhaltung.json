{"textgrid.poem.52518": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach einer Abendunterhaltung", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach einer Abendunterhaltung", "tokens": ["Nach", "ei\u00b7ner", "A\u00b7bend\u00b7un\u00b7ter\u00b7hal\u00b7tung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sah j\u00fcngst ich einen alten Herrn,", "tokens": ["Sah", "j\u00fcngst", "ich", "ei\u00b7nen", "al\u00b7ten", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von unansehnlichster Gestaltung,", "tokens": ["Von", "un\u00b7an\u00b7sehn\u00b7lichs\u00b7ter", "Ge\u00b7stal\u00b7tung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Der k\u00fc\u00dft ein M\u00e4dchen aus der Fern.", "tokens": ["Der", "k\u00fc\u00dft", "ein", "M\u00e4d\u00b7chen", "aus", "der", "Fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann kam er auf sie zugegangen,", "tokens": ["Dann", "kam", "er", "auf", "sie", "zu\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sitte schien mir \u00e4u\u00dferst bunt,", "tokens": ["Die", "Sit\u00b7te", "schien", "mir", "\u00e4u\u00b7\u00dferst", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und k\u00fc\u00dfte zwischen beide Wangen", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "zwi\u00b7schen", "bei\u00b7de", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie grad auf ihren rothen Mund.", "tokens": ["Sie", "grad", "auf", "ih\u00b7ren", "ro\u00b7then", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Das ist ein gro\u00dfes Gl\u00fcck auf Erden,", "tokens": ["Das", "ist", "ein", "gro\u00b7\u00dfes", "Gl\u00fcck", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Vetter sch\u00f6ner Basen sein!", "tokens": ["Ein", "Vet\u00b7ter", "sch\u00f6\u00b7ner", "Ba\u00b7sen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Vetter m\u00f6cht ich gerne werden", "tokens": ["Ein", "Vet\u00b7ter", "m\u00f6cht", "ich", "ger\u00b7ne", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von also sch\u00f6nen Jungfr\u00e4ulein!", "tokens": ["Von", "al\u00b7so", "sch\u00f6\u00b7nen", "Jung\u00b7fr\u00e4u\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er ist vorhanden, auszu\u00fcben", "tokens": ["Er", "ist", "vor\u00b7han\u00b7den", ",", "aus\u00b7zu\u00b7\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein herrlich Privilegium,", "tokens": ["Ein", "herr\u00b7lich", "Pri\u00b7vi\u00b7le\u00b7gi\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er ist schon \u00fcberm Graben dr\u00fcben", "tokens": ["Er", "ist", "schon", "\u00fc\u00b7berm", "Gra\u00b7ben", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch sein behaglich Vetterthum.", "tokens": ["Durch", "sein", "be\u00b7hag\u00b7lich", "Vet\u00b7ter\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ist beneidenswerth vor Allen,", "tokens": ["Und", "ist", "be\u00b7nei\u00b7dens\u00b7werth", "vor", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn f\u00e4llts ihm eben bei, er k\u00fc\u00dft", "tokens": ["Denn", "f\u00e4llts", "ihm", "e\u00b7ben", "bei", ",", "er", "k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6ne Base nach Gefallen,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Ba\u00b7se", "nach", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gerad weil er der Vetter ist.", "tokens": ["Ge\u00b7rad", "weil", "er", "der", "Vet\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! k\u00f6nnt ich auf ein Augenblickchen,", "tokens": ["Ach", "!", "k\u00f6nnt", "ich", "auf", "ein", "Au\u00b7gen\u00b7bli\u00b7ck\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Nur manchesmal, zu zweitallein,", "tokens": ["Nur", "man\u00b7ches\u00b7mal", ",", "zu", "zwei\u00b7tal\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ein privilegirtes St\u00fcckchen", "tokens": ["So", "ein", "pri\u00b7vi\u00b7le\u00b7gir\u00b7tes", "St\u00fcck\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem alten Vetter sein!", "tokens": ["Von", "ei\u00b7nem", "al\u00b7ten", "Vet\u00b7ter", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}