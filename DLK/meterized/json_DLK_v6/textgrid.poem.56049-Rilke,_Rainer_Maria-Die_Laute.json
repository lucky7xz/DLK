{"textgrid.poem.56049": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Laute", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin die Laute. Willst du meinen Leib", "tokens": ["Ich", "bin", "die", "Lau\u00b7te", ".", "Willst", "du", "mei\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "beschreiben, seine sch\u00f6n gew\u00f6lbten Streifen:", "tokens": ["be\u00b7schrei\u00b7ben", ",", "sei\u00b7ne", "sch\u00f6n", "ge\u00b7w\u00f6lb\u00b7ten", "Strei\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "sprich so, als spr\u00e4chest du von einer reifen", "tokens": ["sprich", "so", ",", "als", "spr\u00e4\u00b7chest", "du", "von", "ei\u00b7ner", "rei\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gew\u00f6lbten Feige. \u00dcbertreib", "tokens": ["ge\u00b7w\u00f6lb\u00b7ten", "Fei\u00b7ge", ".", "\u00dc\u00b7bert\u00b7reib"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "das Dunkel, das du in mir siehst. Es war", "tokens": ["das", "Dun\u00b7kel", ",", "das", "du", "in", "mir", "siehst", ".", "Es", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tullias Dunkelheit. In ihrer Scham", "tokens": ["Tul\u00b7li\u00b7as", "Dun\u00b7kel\u00b7heit", ".", "In", "ih\u00b7rer", "Scham"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "war nicht so viel, und ihr erhelltes Haar", "tokens": ["war", "nicht", "so", "viel", ",", "und", "ihr", "er\u00b7hell\u00b7tes", "Haar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "war wie ein heller Saal. Zuweilen nahm", "tokens": ["war", "wie", "ein", "hel\u00b7ler", "Saal", ".", "Zu\u00b7wei\u00b7len", "nahm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "sie etwas Klang von meiner Oberfl\u00e4che", "tokens": ["sie", "et\u00b7was", "Klang", "von", "mei\u00b7ner", "O\u00b7berf\u00b7l\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in ihr Gesicht und sang zu mir.", "tokens": ["in", "ihr", "Ge\u00b7sicht", "und", "sang", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann spannte ich mich gegen ihre Schw\u00e4che,", "tokens": ["Dann", "spann\u00b7te", "ich", "mich", "ge\u00b7gen", "ih\u00b7re", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und endlich war mein Inneres in ihr.", "tokens": ["und", "end\u00b7lich", "war", "mein", "In\u00b7ne\u00b7res", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}