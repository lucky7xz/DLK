{"dta.poem.20041": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Gesanges Erwachen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich einmal wieder singen,", "tokens": ["K\u00f6nnt'", "ich", "ein\u00b7mal", "wie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4r' ich wiederum gesund,", "tokens": ["W\u00e4r'", "ich", "wie\u00b7de\u00b7rum", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber noch will's Herz zerspringen", "tokens": ["A\u00b7ber", "noch", "will's", "Herz", "zer\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Trauern schweigt der Mund.", "tokens": ["Und", "in", "Trau\u00b7ern", "schweigt", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Kaum, da\u00df diese leise Klage", "tokens": ["Kaum", ",", "da\u00df", "die\u00b7se", "lei\u00b7se", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem vollen Busen drang,", "tokens": ["Aus", "dem", "vol\u00b7len", "Bu\u00b7sen", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie an einem Wintertage", "tokens": ["Wie", "an", "ei\u00b7nem", "Win\u00b7ter\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft schon halb ein Vogel sang.", "tokens": ["Oft", "schon", "halb", "ein", "Vo\u00b7gel", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie aus Wolken eng verschlossen", "tokens": ["Wie", "aus", "Wol\u00b7ken", "eng", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb oft dringt ein Sonnenblick,", "tokens": ["Halb", "oft", "dringt", "ein", "Son\u00b7nen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bald von Regen \u00fcbergossen,", "tokens": ["Bald", "von", "Re\u00b7gen", "\u00fc\u00b7ber\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiederkehrt in sich zur\u00fcck.", "tokens": ["Wie\u00b7der\u00b7kehrt", "in", "sich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Also hellte mein Gem\u00fcthe", "tokens": ["Al\u00b7so", "hell\u00b7te", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach nur kurz ein lichter Traum,", "tokens": ["Ach", "nur", "kurz", "ein", "lich\u00b7ter", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vom aufgeweckten Liede", "tokens": ["Und", "vom", "auf\u00b7ge\u00b7weck\u00b7ten", "Lie\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hallten diese T\u00f6ne kaum.", "tokens": ["Hall\u00b7ten", "die\u00b7se", "T\u00f6\u00b7ne", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}