{"textgrid.poem.58579": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Christi Todesstand mit Christus durchgedrungen,", "tokens": ["Wer", "Chris\u00b7ti", "To\u00b7des\u00b7stand", "mit", "Chris\u00b7tus", "durch\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Auferstehung gar hinangekommen ist,", "tokens": ["Zur", "Auf\u00b7er\u00b7ste\u00b7hung", "gar", "hin\u00b7an\u00b7ge\u00b7kom\u00b7men", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In solchem ist der Tod auch ewiglich verschlungen,", "tokens": ["In", "sol\u00b7chem", "ist", "der", "Tod", "auch", "e\u00b7wig\u00b7lich", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gott wischt die Tr\u00e4nen ab, da\u00df man der Schmach vergi\u00dft.", "tokens": ["Gott", "wischt", "die", "Tr\u00e4\u00b7nen", "ab", ",", "da\u00df", "man", "der", "Schmach", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u00bbschaut, das ist unser Gott!\u00ab spricht dann das neue Leben,", "tokens": ["\u00bb", "schaut", ",", "das", "ist", "un\u00b7ser", "Gott", "!", "\u00ab", "spricht", "dann", "das", "neu\u00b7e", "Le\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$.", "$(", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "\u00bbauf den ich harrete in Kreuz und Todespein;", "tokens": ["\u00bb", "auf", "den", "ich", "har\u00b7re\u00b7te", "in", "Kreuz", "und", "To\u00b7de\u00b7spein", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Er hat geholfen nun, ihm will ich Ehre geben,", "tokens": ["Er", "hat", "ge\u00b7hol\u00b7fen", "nun", ",", "ihm", "will", "ich", "Eh\u00b7re", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "$,", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich freue mich in ihm und seinem Heil allein.\u00ab", "tokens": ["Ich", "freu\u00b7e", "mich", "in", "ihm", "und", "sei\u00b7nem", "Heil", "al\u00b7lein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "KON", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}