{"textgrid.poem.44944": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wach auf, o Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wach auf, o Welt! und zeige, da\u00df du wachst,", "tokens": ["Wach", "auf", ",", "o", "Welt", "!", "und", "zei\u00b7ge", ",", "da\u00df", "du", "wachst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NN", "$.", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Frei wandelnd in den aufgeschlo\u00dfnen R\u00e4umen.", "tokens": ["Frei", "wan\u00b7delnd", "in", "den", "auf\u00b7ge\u00b7schlo\u00df\u00b7nen", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht, da\u00df du wie ein S\u00e4ugling weinst und lachst,", "tokens": ["Nicht", ",", "da\u00df", "du", "wie", "ein", "S\u00e4ug\u00b7ling", "weinst", "und", "lachst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "KOKOM", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach Bildern greifst und l\u00e4ngstentschwundnen Tr\u00e4umen.", "tokens": ["Nach", "Bil\u00b7dern", "greifst", "und", "l\u00e4ngs\u00b7tent\u00b7schwund\u00b7nen", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}