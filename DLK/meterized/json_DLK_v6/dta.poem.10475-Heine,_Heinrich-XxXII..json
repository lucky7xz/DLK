{"dta.poem.10475": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehat sie sich denn nie ge\u00e4u\u00dfert", "tokens": ["\u201e", "hat", "sie", "sich", "denn", "nie", "ge\u00b7\u00e4u\u00b7\u00dfert"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "PRF", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber dein verliebtes Wesen?", "tokens": ["Ue\u00b7ber", "dein", "ver\u00b7lieb\u00b7tes", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Konntest du in ihren Augen", "tokens": ["Konn\u00b7test", "du", "in", "ih\u00b7ren", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemals Gegenliebe lesen?", "tokens": ["Nie\u00b7mals", "Ge\u00b7gen\u00b7lie\u00b7be", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Konntest du in ihren Augen", "tokens": ["Konn\u00b7test", "du", "in", "ih\u00b7ren", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niemals bis zur Seele dringen?", "tokens": ["Nie\u00b7mals", "bis", "zur", "See\u00b7le", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du bist ja sonst kein Esel,", "tokens": ["Und", "du", "bist", "ja", "sonst", "kein", "E\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Theurer Freund, in solchen Dingen.\u201c", "tokens": ["Theu\u00b7rer", "Freund", ",", "in", "sol\u00b7chen", "Din\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "$,", "APPR", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}