{"dta.poem.6655": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sinnen k\u00f6nnen uns durch ein Gef\u00fchl erkl\u00e4ren,", "tokens": ["Die", "Sin\u00b7nen", "k\u00f6n\u00b7nen", "uns", "durch", "ein", "Ge\u00b7f\u00fchl", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was unserm C\u00f6rper n\u00fctzt, und was ihn kan versehren.", "tokens": ["Was", "un\u00b7serm", "C\u00f6r\u00b7per", "n\u00fctzt", ",", "und", "was", "ihn", "kan", "ver\u00b7seh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch t\u00e4uschen sie uns offt, wenn wir sie fragen,", "tokens": ["Doch", "t\u00e4u\u00b7schen", "sie", "uns", "offt", ",", "wenn", "wir", "sie", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um uns die Wahrheit recht zu sagen.", "tokens": ["Um", "uns", "die", "Wahr\u00b7heit", "recht", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn die Vernunfft dieselbigen nicht leitet,", "tokens": ["Wenn", "die", "Ver\u00b7nunfft", "die\u00b7sel\u00b7bi\u00b7gen", "nicht", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Versp\u00fcret man, wie bald der Fu\u00df in Jrrthum gleitet.", "tokens": ["Ver\u00b7sp\u00fc\u00b7ret", "man", ",", "wie", "bald", "der", "Fu\u00df", "in", "Jrr\u00b7thum", "glei\u00b7tet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch starcke Vorurtheil, wohin die Sinnen f\u00fchren,", "tokens": ["Durch", "star\u00b7cke", "Vor\u00b7urt\u00b7heil", ",", "wo\u00b7hin", "die", "Sin\u00b7nen", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Statt ihnen unterthan zu seyn,", "tokens": ["Statt", "ih\u00b7nen", "un\u00b7ter\u00b7than", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Beherrschen und regieren", "tokens": ["Be\u00b7herr\u00b7schen", "und", "re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sie \u00f6ffters unsern Geist. Es scheinet anders nicht,", "tokens": ["Sie", "\u00f6ff\u00b7ters", "un\u00b7sern", "Geist", ".", "Es", "schei\u00b7net", "an\u00b7ders", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als da\u00df, erbarmenswerth geblendet, blo\u00df allein", "tokens": ["Als", "da\u00df", ",", "er\u00b7bar\u00b7mens\u00b7werth", "ge\u00b7blen\u00b7det", ",", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "KOUS", "$,", "ADJD", "VVPP", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man sich nach ihnen richt.", "tokens": ["Man", "sich", "nach", "ih\u00b7nen", "richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die Seele, welche sich", "tokens": ["Die", "See\u00b7le", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Nur gar zu starck gew\u00f6hnt an das, was c\u00f6rperlich,", "tokens": ["Nur", "gar", "zu", "starck", "ge\u00b7w\u00f6hnt", "an", "das", ",", "was", "c\u00f6r\u00b7per\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "VVPP", "APPR", "PDS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Versp\u00fcrt, da\u00df sie durch ihre starcke Z\u00fcge,", "tokens": ["Ver\u00b7sp\u00fcrt", ",", "da\u00df", "sie", "durch", "ih\u00b7re", "star\u00b7cke", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die sie ber\u00fchren, sich vergn\u00fcge.", "tokens": ["Die", "sie", "be\u00b7r\u00fch\u00b7ren", ",", "sich", "ver\u00b7gn\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie l\u00e4sst ohn Unterla\u00df, sich von demselben lencken,", "tokens": ["Sie", "l\u00e4sst", "ohn", "Un\u00b7ter\u00b7la\u00df", ",", "sich", "von", "dem\u00b7sel\u00b7ben", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRF", "APPR", "PDAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und recht, als litte sie in ihrem Dencken;", "tokens": ["Und", "recht", ",", "als", "lit\u00b7te", "sie", "in", "ih\u00b7rem", "Den\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Beraubet sie sich selbst von ihrem eignem Licht.", "tokens": ["Be\u00b7rau\u00b7bet", "sie", "sich", "selbst", "von", "ih\u00b7rem", "eig\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ja sie verwirret sich,", "tokens": ["Ja", "sie", "ver\u00b7wir\u00b7ret", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Und hat ihr Augenmerck auf anders nichts gericht\u2019t", "tokens": ["Und", "hat", "ihr", "Au\u00b7gen\u00b7merck", "auf", "an\u00b7ders", "nichts", "ge\u00b7richt't"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "ADV", "PIS", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als auf den Druck und Trieb, die so unordentlich,", "tokens": ["Als", "auf", "den", "Druck", "und", "Trieb", ",", "die", "so", "un\u00b7or\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "KON", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Und welche ihr recht mit Gewalt verwehren", "tokens": ["Und", "wel\u00b7che", "ihr", "recht", "mit", "Ge\u00b7walt", "ver\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "PPOSAT", "NN", "APPR", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Auf ihre wahre Lust den Blick zu kehren.", "tokens": ["Auf", "ih\u00b7re", "wah\u00b7re", "Lust", "den", "Blick", "zu", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "So wie, bey einer grossen Menge,", "tokens": ["So", "wie", ",", "bey", "ei\u00b7ner", "gros\u00b7sen", "Men\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der Zweck der k\u00fcnstlichsten Beredsamkeit", "tokens": ["Der", "Zweck", "der", "k\u00fcnst\u00b7lichs\u00b7ten", "Be\u00b7red\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Verfehlet wird, durch ein Get\u00f6s\u2019 und Schw\u00e4rmen;", "tokens": ["Ver\u00b7feh\u00b7let", "wird", ",", "durch", "ein", "Ge\u00b7t\u00f6s'", "und", "Schw\u00e4r\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und wie der Reitz der lieblichsten Ges\u00e4nge", "tokens": ["Und", "wie", "der", "Reitz", "der", "lieb\u00b7lichs\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.29": {"text": "Gestohret wird durch ein zu starckes Lermen.", "tokens": ["Ge\u00b7stoh\u00b7ret", "wird", "durch", "ein", "zu", "star\u00b7ckes", "Ler\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "ART", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}