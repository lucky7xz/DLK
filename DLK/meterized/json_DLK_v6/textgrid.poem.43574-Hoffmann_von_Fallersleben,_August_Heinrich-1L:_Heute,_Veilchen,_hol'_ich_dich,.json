{"textgrid.poem.43574": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heute, Veilchen, hol' ich dich,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heute, Veilchen, hol' ich dich,", "tokens": ["Heu\u00b7te", ",", "Veil\u00b7chen", ",", "hol'", "ich", "dich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft du fort mit mir!", "tokens": ["Mu\u00dft", "du", "fort", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und das liebe Veilchen spricht:", "tokens": ["Und", "das", "lie\u00b7be", "Veil\u00b7chen", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbknabe, la\u00df mich hier!", "tokens": ["\u00bb", "kna\u00b7be", ",", "la\u00df", "mich", "hier", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVIMP", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "La\u00df mich hier im Sonnenschein", "tokens": ["La\u00df", "mich", "hier", "im", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der gr\u00fcnen Au!", "tokens": ["Auf", "der", "gr\u00fc\u00b7nen", "Au", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tages labt mich Maienluft,", "tokens": ["Ta\u00b7ges", "labt", "mich", "Mai\u00b7en\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachts des Himmels Thau.\u00ab", "tokens": ["Nachts", "des", "Him\u00b7mels", "Thau", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Alles was du w\u00fcnschen kannst,", "tokens": ["Al\u00b7les", "was", "du", "w\u00fcn\u00b7schen", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du auch bei mir:", "tokens": ["Hast", "du", "auch", "bei", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Maienluft und frischen Trank", "tokens": ["Mai\u00b7en\u00b7luft", "und", "fri\u00b7schen", "Trank"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich geben dir.", "tokens": ["Will", "ich", "ge\u00b7ben", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbwenn du mir auch Alles giebst,", "tokens": ["\u00bb", "wenn", "du", "mir", "auch", "Al\u00b7les", "giebst", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmst mir doch mein Gl\u00fcck:", "tokens": ["Nimmst", "mir", "doch", "mein", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Meine Heimat giebst du mir", "tokens": ["Mei\u00b7ne", "Hei\u00b7mat", "giebst", "du", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmermehr zur\u00fcck.\u00ab", "tokens": ["Nim\u00b7mer\u00b7mehr", "zu\u00b7r\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}