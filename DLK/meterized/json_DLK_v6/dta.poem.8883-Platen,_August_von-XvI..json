{"dta.poem.8883": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und ganz versagt ist mir, zu dir zu kommen,", "tokens": ["Und", "ganz", "ver\u00b7sagt", "ist", "mir", ",", "zu", "dir", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "PPER", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir selten zu begegnen und beklommen", "tokens": ["Dir", "sel\u00b7ten", "zu", "be\u00b7geg\u00b7nen", "und", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich anzuschau'n, das ist mein Loos hienieden.", "tokens": ["Dich", "an\u00b7zu\u00b7schau'n", ",", "das", "ist", "mein", "Loos", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch von dir tr\u00e4umen, dichten, Plane schmieden,", "tokens": ["Doch", "von", "dir", "tr\u00e4u\u00b7men", ",", "dich\u00b7ten", ",", "Pla\u00b7ne", "schmie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "$,", "ADJA", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um dir zu nahn, das ist mir unbenommen,", "tokens": ["Um", "dir", "zu", "nahn", ",", "das", "ist", "mir", "un\u00b7be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "ADJA", "$,", "PDS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das soll, so lang' es frommen will, mir frommen,", "tokens": ["Das", "soll", ",", "so", "lang'", "es", "from\u00b7men", "will", ",", "mir", "from\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mit so Wen'gem stell' ich mich zufrieden.", "tokens": ["Und", "mit", "so", "Wen'\u00b7gem", "stell'", "ich", "mich", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PIS", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn ach! ich habe Schlimmeres ertragen,", "tokens": ["Denn", "ach", "!", "ich", "ha\u00b7be", "Schlim\u00b7me\u00b7res", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als dieses Schlimme jezt, und duld' ergeben,", "tokens": ["Als", "die\u00b7ses", "Schlim\u00b7me", "jezt", ",", "und", "duld'", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "$,", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Statt heft'ger Qual, ein s\u00fc\u00dfes Mi\u00dfbehagen.", "tokens": ["Statt", "heft'\u00b7ger", "Qual", ",", "ein", "s\u00fc\u00b7\u00dfes", "Mi\u00df\u00b7be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Mein Wunsch, bey Andern, zeugte Widerstreben:", "tokens": ["Mein", "Wunsch", ",", "bey", "An\u00b7dern", ",", "zeug\u00b7te", "Wi\u00b7der\u00b7stre\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast ihn nicht erh\u00f6rt, doch abgeschlagen", "tokens": ["Du", "hast", "ihn", "nicht", "er\u00b7h\u00f6rt", ",", "doch", "ab\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hast du ihn auch nicht, o mein s\u00fc\u00dfes Leben!", "tokens": ["Hast", "du", "ihn", "auch", "nicht", ",", "o", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "$,", "FM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}