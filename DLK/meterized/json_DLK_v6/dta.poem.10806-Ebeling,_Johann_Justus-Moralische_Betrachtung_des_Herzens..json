{"dta.poem.10806": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Moralische Betrachtung des  \n Herzens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Herz ist oben breit, und unten zu-\ngespizt,", "tokens": ["Das", "Herz", "ist", "o\u00b7ben", "breit", ",", "und", "un\u00b7ten", "zu", "ge\u00b7spizt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "ADV", "TRUNC", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie es im Leibe h\u00e4ngt, in seiner La-", "tokens": ["Wie", "es", "im", "Lei\u00b7be", "h\u00e4ngt", ",", "in", "sei\u00b7ner", "La"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Lufftr\u00f6hr ist damit aufs festeste vereinet,", "tokens": ["Die", "Luff\u00b7tr\u00f6hr", "ist", "da\u00b7mit", "aufs", "fes\u00b7tes\u00b7te", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die man zur Rede braucht: Und dieses alles schei-", "tokens": ["Die", "man", "zur", "Re\u00b7de", "braucht", ":", "Und", "die\u00b7ses", "al\u00b7les", "schei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$.", "KON", "PDS", "PIS", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Lehrbild uns zu seyn, wodurch der Sch\u00f6pfer", "tokens": ["Ein", "Lehr\u00b7bild", "uns", "zu", "seyn", ",", "wo\u00b7durch", "der", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VAINF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mensch lerne an dem Herz, was hie sey\ndeine Pflicht:\nDu must dein Herz vielmehr nach dem was\nhimmlisch lenken,\nUnd nur das kleinste Theil davon der Erde\nschenken.", "tokens": ["Mensch", "ler\u00b7ne", "an", "dem", "Herz", ",", "was", "hie", "sey", "dei\u00b7ne", "Pflicht", ":", "Du", "must", "dein", "Herz", "viel\u00b7mehr", "nach", "dem", "was", "himm\u00b7lisch", "len\u00b7ken", ",", "Und", "nur", "das", "kleins\u00b7te", "Theil", "da\u00b7von", "der", "Er\u00b7de", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "PRELS", "ADJD", "VVINF", "$,", "KON", "ADV", "ART", "ADJA", "NN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "---+-+-+-+-+-+-+--+--+----+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Was deine Zunge spricht, das mu\u00df von Herzen", "tokens": ["Was", "dei\u00b7ne", "Zun\u00b7ge", "spricht", ",", "das", "mu\u00df", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und stimmet Herz und Mund im Reden \u00fcberein:", "tokens": ["Und", "stim\u00b7met", "Herz", "und", "Mund", "im", "Re\u00b7den", "\u00fc\u00b7be\u00b7re\u00b7in", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "So richtest du dich recht nach deines Sch\u00f6pfers", "tokens": ["So", "rich\u00b7test", "du", "dich", "recht", "nach", "dei\u00b7nes", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Was von der Zunge fliest, sol aus dem Herzen", "tokens": ["Was", "von", "der", "Zun\u00b7ge", "fliest", ",", "sol", "aus", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}