{"textgrid.poem.44919": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Postulata", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pre\u00dffreiheit steht denn oben an,", "tokens": ["Pre\u00df\u00b7frei\u00b7heit", "steht", "denn", "o\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo \u2013 welch absurdes Treiben! \u2013", "tokens": ["Wo", "\u2013", "welch", "ab\u00b7sur\u00b7des", "Trei\u00b7ben", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "$(", "PWAT", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Das halbe Land nicht lesen kann,", "tokens": ["Das", "hal\u00b7be", "Land", "nicht", "le\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das andere nicht schreiben.", "tokens": ["Das", "an\u00b7de\u00b7re", "nicht", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}