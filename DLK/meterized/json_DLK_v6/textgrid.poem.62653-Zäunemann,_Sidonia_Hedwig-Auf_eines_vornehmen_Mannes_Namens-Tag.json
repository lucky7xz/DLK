ambic.hexa"}, "line.25": {"text": "Der Rath, die Policey, die B\u00fcrger insgesamt", "tokens": ["Der", "Rath", ",", "die", "Po\u00b7li\u00b7cey", ",", "die", "B\u00fcr\u00b7ger", "ins\u00b7ge\u00b7samt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bekr\u00e4ftigen mein Wort. Sie m\u00fcssen frey gestehen,", "tokens": ["Be\u00b7kr\u00e4f\u00b7ti\u00b7gen", "mein", "Wort", ".", "Sie", "m\u00fcs\u00b7sen", "frey", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df sie ein Tugend-Bild an ihrem ", "tokens": ["Da\u00df", "sie", "ein", "Tu\u00b7gen\u00b7dBild", "an", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Drum ist dein Rathschlu\u00df gut, drum ist dein Urtheil recht,", "tokens": ["Drum", "ist", "dein", "Rath\u00b7schlu\u00df", "gut", ",", "drum", "ist", "dein", "Ur\u00b7theil", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "PAV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer kennet dich noch mehr? Ihr Waysen! ja ihr sprecht,", "tokens": ["Wer", "ken\u00b7net", "dich", "noch", "mehr", "?", "Ihr", "Way\u00b7sen", "!", "ja", "ihr", "sprecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "pu