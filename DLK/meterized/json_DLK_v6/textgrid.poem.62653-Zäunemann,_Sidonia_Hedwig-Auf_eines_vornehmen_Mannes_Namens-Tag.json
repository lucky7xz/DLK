"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die\u00df hast du, ", "tokens": ["Die\u00df", "hast", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Die Tugend hat man stets an deinem Thun versp\u00fchrt.", "tokens": ["Die", "Tu\u00b7gend", "hat", "man", "stets", "an", "dei\u00b7nem", "Thun", "ver\u00b7sp\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein Geist stieg \u00fcber sich nach jenen Himmels-Zinnen,", "tokens": ["Dein", "Geist", "stieg", "\u00fc\u00b7ber", "sich", "nach", "je\u00b7nen", "Him\u00b7mels\u00b7Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du gabst auch dein Gem\u00fcth, dein Herz und deine Sinnen,", "tokens": ["Du", "gabst", "auch", "dein", "Ge\u00b7m\u00fcth", ",", "dein", "Herz", "und", "dei\u00b7ne", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Tugend g\u00e4nzlich hin, und richtest deinen Stand", "tokens": ["Der", "Tu\u00b7gend", "g\u00e4nz\u00b7lich", "hin", 