{"textgrid.poem.62653": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf eines vornehmen Mannes Namens-Tag", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Allmacht Meisterst\u00fcck, der Menschen die kleine Welt,", "tokens": ["Der", "All\u00b7macht", "Meis\u00b7ter\u00b7st\u00fcck", ",", "der", "Men\u00b7schen", "die", "klei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Viel Gaben der Natur und Sch\u00f6nheit in sich h\u00e4lt,", "tokens": ["Viel", "Ga\u00b7ben", "der", "Na\u00b7tur", "und", "Sch\u00f6n\u00b7heit", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Woraus die Lieb und Huld des Sch\u00f6pfers zu erkennen,", "tokens": ["Wo\u00b7raus", "die", "Lieb", "und", "Huld", "des", "Sch\u00f6p\u00b7fers", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weswegen wir uns auch nicht wenig gl\u00fccklich nennen.", "tokens": ["Wes\u00b7we\u00b7gen", "wir", "uns", "auch", "nicht", "we\u00b7nig", "gl\u00fcck\u00b7lich", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein was hilft es uns, wenn wir auch noch so sch\u00f6n,", "tokens": ["Al\u00b7lein", "was", "hilft", "es", "uns", ",", "wenn", "wir", "auch", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und lieblich ausgeschm\u00fcckt der Welt vor Augen stehn;", "tokens": ["Und", "lieb\u00b7lich", "aus\u00b7ge\u00b7schm\u00fcckt", "der", "Welt", "vor", "Au\u00b7gen", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Woferne nicht der Herr zu unsern Leib und Leben,", "tokens": ["Wo\u00b7fer\u00b7ne", "nicht", "der", "Herr", "zu", "un\u00b7sern", "Leib", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Uns eine feine Seel und die Vernunft gegeben?", "tokens": ["Uns", "ei\u00b7ne", "fei\u00b7ne", "Seel", "und", "die", "Ver\u00b7nunft", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was n\u00fctzet uns der Leib, wenn uns Verstand und Licht;", "tokens": ["Was", "n\u00fct\u00b7zet", "uns", "der", "Leib", ",", "wenn", "uns", "Ver\u00b7stand", "und", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu unsern Amt und Werk hier mangelt und gebricht?", "tokens": ["Zu", "un\u00b7sern", "Amt", "und", "Werk", "hier", "man\u00b7gelt", "und", "ge\u00b7bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie elend ist der Mensch, dem Geist und Kr\u00e4fte fehlen,", "tokens": ["Wie", "e\u00b7lend", "ist", "der", "Mensch", ",", "dem", "Geist", "und", "Kr\u00e4f\u00b7te", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An statt der Laster-Bruth die Tugend zu erwehlen.", "tokens": ["An", "statt", "der", "Las\u00b7ter\u00b7Bruth", "die", "Tu\u00b7gend", "zu", "er\u00b7weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Giebt aber uns der Herr bey unserm Erden-Trit", "tokens": ["Giebt", "a\u00b7ber", "uns", "der", "Herr", "bey", "un\u00b7serm", "Er\u00b7den\u00b7Trit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch eine feine Seel nach seiner Liebe mit;", "tokens": ["Auch", "ei\u00b7ne", "fei\u00b7ne", "Seel", "nach", "sei\u00b7ner", "Lie\u00b7be", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sucht er in unsre Brust Verstand und sch\u00f6ne Gaben,", "tokens": ["Sucht", "er", "in", "uns\u00b7re", "Brust", "Ver\u00b7stand", "und", "sch\u00f6\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Licht, Weisheit und Vernunft zu legen und zu graben;", "tokens": ["Licht", ",", "Weis\u00b7heit", "und", "Ver\u00b7nunft", "zu", "le\u00b7gen", "und", "zu", "gra\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sind wir sch\u00f6n geziert. Denn hat Witz und Vernunft,", "tokens": ["So", "sind", "wir", "sch\u00f6n", "ge\u00b7ziert", ".", "Denn", "hat", "Witz", "und", "Ver\u00b7nunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$.", "KON", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Bey uns die Oberhand, so sehen wir die Zunft", "tokens": ["Bey", "uns", "die", "O\u00b7ber\u00b7hand", ",", "so", "se\u00b7hen", "wir", "die", "Zunft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Th\u00f6richten nicht an. Ist die Vernunft gel\u00e4utert,", "tokens": ["Der", "Th\u00f6\u00b7rich\u00b7ten", "nicht", "an", ".", "Ist", "die", "Ver\u00b7nunft", "ge\u00b7l\u00e4u\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "PTKVZ", "$.", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-++---+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ist der Verstand erleucht, gekl\u00e4rt und ausgeheitert,", "tokens": ["Ist", "der", "Ver\u00b7stand", "er\u00b7leucht", ",", "ge\u00b7kl\u00e4rt", "und", "aus\u00b7ge\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So ist der Sinn auch sch\u00f6n, so ist der Wille gut.", "tokens": ["So", "ist", "der", "Sinn", "auch", "sch\u00f6n", ",", "so", "ist", "der", "Wil\u00b7le", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Wesen so man treibt, die Werke so man thut,", "tokens": ["Das", "We\u00b7sen", "so", "man", "treibt", ",", "die", "Wer\u00b7ke", "so", "man", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VVFIN", "$,", "ART", "NN", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die richt man l\u00f6blich ein. Sie f\u00fchret uns zu Sachen,", "tokens": ["Die", "richt", "man", "l\u00f6b\u00b7lich", "ein", ".", "Sie", "f\u00fch\u00b7ret", "uns", "zu", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wodurch wir uns ber\u00fchmt und gl\u00fccklich k\u00f6nnen machen.", "tokens": ["Wo\u00b7durch", "wir", "uns", "be\u00b7r\u00fchmt", "und", "gl\u00fcck\u00b7lich", "k\u00f6n\u00b7nen", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es l\u00e4\u00dft uns die Vernunft was mehr als Irdsches sehn,", "tokens": ["Es", "l\u00e4\u00dft", "uns", "die", "Ver\u00b7nunft", "was", "mehr", "als", "Ird\u00b7sches", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PWS", "PIS", "KOKOM", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie lockt und reitzet uns der Tugend nachzugehn.", "tokens": ["Sie", "lockt", "und", "reit\u00b7zet", "uns", "der", "Tu\u00b7gend", "nach\u00b7zu\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein niedertr\u00e4chtig Herz labt sich an eitlen Werken,", "tokens": ["Ein", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "Herz", "labt", "sich", "an", "eit\u00b7len", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und sucht sein Aug und Herz an solcher Lust zu st\u00e4rken,", "tokens": ["Und", "sucht", "sein", "Aug", "und", "Herz", "an", "sol\u00b7cher", "Lust", "zu", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die reine Seelen fliehn; da dort ein kluger Geist", "tokens": ["Die", "rei\u00b7ne", "See\u00b7len", "fliehn", ";", "da", "dort", "ein", "klu\u00b7ger", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und aufgekl\u00e4rter Sinn sich alle dem entreist,", "tokens": ["Und", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ter", "Sinn", "sich", "al\u00b7le", "dem", "en\u00b7treist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PRF", "PIAT", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was nach der Thorheit schmeckt. Er schwingt sich von der Erden,", "tokens": ["Was", "nach", "der", "Thor\u00b7heit", "schmeckt", ".", "Er", "schwingt", "sich", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und trachtet durch Vernunft und Tugend gro\u00df zu werden.", "tokens": ["Und", "trach\u00b7tet", "durch", "Ver\u00b7nunft", "und", "Tu\u00b7gend", "gro\u00df", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er wirds auch in der That. Sein Name bleibt und steht,", "tokens": ["Er", "wirds", "auch", "in", "der", "That", ".", "Sein", "Na\u00b7me", "bleibt", "und", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn schon der Leib zerbricht und er von hinnen geht.", "tokens": ["Wenn", "schon", "der", "Leib", "zer\u00b7bricht", "und", "er", "von", "hin\u00b7nen", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "KON", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von diesem zeugen ja so viel ber\u00fchmte M\u00e4nner,", "tokens": ["Von", "die\u00b7sem", "zeu\u00b7gen", "ja", "so", "viel", "be\u00b7r\u00fchm\u00b7te", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ADV", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die\u00df lehrst du ebenfalls/ ", "tokens": ["Die\u00df", "lehrst", "du", "e\u00b7ben\u00b7falls", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der zierte deinen Leib mit einer feinen Seel:", "tokens": ["Der", "zier\u00b7te", "dei\u00b7nen", "Leib", "mit", "ei\u00b7ner", "fei\u00b7nen", "Seel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Jahre nahmen zu, so wie der Geist in dir,", "tokens": ["Die", "Jah\u00b7re", "nah\u00b7men", "zu", ",", "so", "wie", "der", "Geist", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "KOKOM", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hielt dir die Vernunft stets ihre Sch\u00f6nheit f\u00fcr:", "tokens": ["Es", "hielt", "dir", "die", "Ver\u00b7nunft", "stets", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ADV", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und durch derselben Trieb trugst du ein gro\u00df Verlangen,", "tokens": ["Und", "durch", "der\u00b7sel\u00b7ben", "Trieb", "trugst", "du", "ein", "gro\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Tugend nachzugehn, und nur durch sie zu prangen.", "tokens": ["Der", "Tu\u00b7gend", "nach\u00b7zu\u00b7gehn", ",", "und", "nur", "durch", "sie", "zu", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,", "KON", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es sah dein tiefer Witz der Tugend W\u00fcrde an,", "tokens": ["Es", "sah", "dein", "tie\u00b7fer", "Witz", "der", "Tu\u00b7gend", "W\u00fcr\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und wie man sich durch sie gef\u00e4llig machen kan.", "tokens": ["Und", "wie", "man", "sich", "durch", "sie", "ge\u00b7f\u00e4l\u00b7lig", "ma\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PIS", "PRF", "APPR", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du wu\u00dftest, da\u00df sie Neid, Zeit, Tod und Moder h\u00f6hnet,", "tokens": ["Du", "wu\u00df\u00b7test", ",", "da\u00df", "sie", "Neid", ",", "Zeit", ",", "Tod", "und", "Mo\u00b7der", "h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und sich allein durch sich ver