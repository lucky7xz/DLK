{"textgrid.poem.61434": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Unterwegs", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun zieh' ich hin, du liebes Kind,", "tokens": ["Nun", "zieh'", "ich", "hin", ",", "du", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch vor mir f\u00e4hrt der Morgenwind", "tokens": ["Frisch", "vor", "mir", "f\u00e4hrt", "der", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und r\u00fchrt mit sanftem Schauder leis", "tokens": ["Und", "r\u00fchrt", "mit", "sanf\u00b7tem", "Schau\u00b7der", "leis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wipfel, die vom Fr\u00fchrot gl\u00fchen. \u2013", "tokens": ["Die", "Wip\u00b7fel", ",", "die", "vom", "Fr\u00fch\u00b7rot", "gl\u00fc\u00b7hen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, seit ich dich mein eigen wei\u00df,", "tokens": ["Ach", ",", "seit", "ich", "dich", "mein", "ei\u00b7gen", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PRF", "PPOSAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie reich d\u00fcnkt mir die Welt zu bl\u00fchen!", "tokens": ["Wie", "reich", "d\u00fcnkt", "mir", "die", "Welt", "zu", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "All\u00fcberall, im Schmelz der Auen,", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", ",", "im", "Schmelz", "der", "Au\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im zarten Lichtgew\u00f6lk, im Wald,", "tokens": ["Im", "zar\u00b7ten", "Licht\u00b7ge\u00b7w\u00f6lk", ",", "im", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Glaub' ich dich, liebliche Gestalt,", "tokens": ["Glaub'", "ich", "dich", ",", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Gleich wie durch Nebel noch zu schauen.", "tokens": ["Gleich", "wie", "durch", "Ne\u00b7bel", "noch", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sonne hebt aus dunkelm Bach", "tokens": ["Die", "Son\u00b7ne", "hebt", "aus", "dun\u00b7kelm", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein l\u00e4chelnd Auge mir entgegen;", "tokens": ["Dein", "l\u00e4\u00b7chelnd", "Au\u00b7ge", "mir", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es t\u00e4uscht der Glieder anmutvoll Bewegen", "tokens": ["Es", "t\u00e4uscht", "der", "Glie\u00b7der", "an\u00b7mut\u00b7voll", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Schattentanz des Laubes nach.", "tokens": ["Der", "Schat\u00b7ten\u00b7tanz", "des", "Lau\u00b7bes", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn urpl\u00f6tzlich dann im Wind", "tokens": ["Und", "wenn", "ur\u00b7pl\u00f6tz\u00b7lich", "dann", "im", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das holde Gaukelspiel zerrinnt,", "tokens": ["Das", "hol\u00b7de", "Gau\u00b7kel\u00b7spiel", "zer\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann schlie\u00df' ich rastend wohl die Augenlider;", "tokens": ["Dann", "schlie\u00df'", "ich", "ras\u00b7tend", "wohl", "die", "Au\u00b7gen\u00b7li\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sieh, ein neues Wunder tut sich kund:", "tokens": ["Und", "sieh", ",", "ein", "neu\u00b7es", "Wun\u00b7der", "tut", "sich", "kund", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich find' in meines Herzens Grund", "tokens": ["Ich", "find'", "in", "mei\u00b7nes", "Her\u00b7zens", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich klarer nur und sch\u00f6ner nur dich wieder.", "tokens": ["Dich", "kla\u00b7rer", "nur", "und", "sch\u00f6\u00b7ner", "nur", "dich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADJD", "ADV", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}