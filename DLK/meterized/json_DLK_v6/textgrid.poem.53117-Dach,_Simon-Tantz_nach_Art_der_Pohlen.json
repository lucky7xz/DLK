{"textgrid.poem.53117": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Tantz nach Art der Pohlen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Ihr jetzt seyt erschienen", "tokens": ["Die", "Ihr", "jetzt", "seyt", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu vnsrer Fr\u00f6ligkeit,", "tokens": ["Zu", "vns\u00b7rer", "Fr\u00f6\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was kan euch besser dienen", "tokens": ["Was", "kan", "euch", "bes\u00b7ser", "die\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bey dieser kalten Zeit,", "tokens": ["Bey", "die\u00b7ser", "kal\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Al\u00df da\u00df Ihr theils im Tantzen", "tokens": ["Al\u00df", "da\u00df", "Ihr", "theils", "im", "Tant\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Euch \u00fcbt, wie ich zwar thue,", "tokens": ["Euch", "\u00fcbt", ",", "wie", "ich", "zwar", "thue", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Theils auch mit Gl\u00e4ser schantzen", "tokens": ["Theils", "auch", "mit", "Gl\u00e4\u00b7ser", "schant\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Setzt auff einander zu?", "tokens": ["Setzt", "auff", "ein\u00b7an\u00b7der", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Jungfern vnd Gesellen,", "tokens": ["Ihr", "Jung\u00b7fern", "vnd", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man fordert euch hervor,", "tokens": ["Man", "for\u00b7dert", "euch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommt, kommt euch ein-zustellen,", "tokens": ["Kommt", ",", "kommt", "euch", "ein\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es winckt der gantze Chor,", "tokens": ["Es", "winckt", "der", "gant\u00b7ze", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd sagen die Schalmeyen", "tokens": ["Vnd", "sa\u00b7gen", "die", "Schal\u00b7me\u00b7yen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Da\u00df die\u00df der Braut-Tantz sey,", "tokens": ["Da\u00df", "die\u00df", "der", "Braut\u00b7Tantz", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr steht im ersten Reyen,", "tokens": ["Ihr", "steht", "im", "ers\u00b7ten", "Re\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.8": {"text": "Kommt, findet euch herbey!", "tokens": ["Kommt", ",", "fin\u00b7det", "euch", "her\u00b7bey", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hat jemand nun im Hertzen", "tokens": ["Hat", "je\u00b7mand", "nun", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beschlossen, die er liebt,", "tokens": ["Be\u00b7schlos\u00b7sen", ",", "die", "er", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der thu' er kundt die Schmertzen", "tokens": ["Der", "thu'", "er", "kundt", "die", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd was Ihn nur betr\u00fcbt,", "tokens": ["Vnd", "was", "Ihn", "nur", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hie mag er sich besprechen", "tokens": ["Hie", "mag", "er", "sich", "be\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So gut er immer kan,", "tokens": ["So", "gut", "er", "im\u00b7mer", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sage sein Gebrechen", "tokens": ["Er", "sa\u00b7ge", "sein", "Ge\u00b7bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Getrost der Liebsten an;", "tokens": ["Ge\u00b7trost", "der", "Liebs\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er rede mit den Augen,", "tokens": ["Er", "re\u00b7de", "mit", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Seuftzen ohne Ziel,", "tokens": ["Mit", "Seuft\u00b7zen", "oh\u00b7ne", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd was zum Vor-trab taugen", "tokens": ["Vnd", "was", "zum", "Vor\u00b7trab", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mag in dem Liebes Spiel:", "tokens": ["Mag", "in", "dem", "Lie\u00b7bes", "Spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch s\u00fcsses H\u00e4nde-K\u00fcssen,", "tokens": ["Durch", "s\u00fcs\u00b7ses", "H\u00e4n\u00b7de\u00b7K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd was jhm sonst bekandt,", "tokens": ["Vnd", "was", "jhm", "sonst", "be\u00b7kandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lass' er der Liebsten wissen", "tokens": ["Lass'", "er", "der", "Liebs\u00b7ten", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Der Liebe grossen Brandt.", "tokens": ["Der", "Lie\u00b7be", "gros\u00b7sen", "Brandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann auch ihr Herrn vnd Frawen,", "tokens": ["Dann", "auch", "ihr", "Herrn", "vnd", "Fra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Ihr vns gutes g\u00f6nnt,", "tokens": ["Die", "Ihr", "vns", "gu\u00b7tes", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kompt, lasset jetzund schawen,", "tokens": ["Kompt", ",", "las\u00b7set", "je\u00b7tzund", "scha\u00b7wen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ihr auch tantzen k\u00f6nnt!", "tokens": ["Da\u00df", "ihr", "auch", "tant\u00b7zen", "k\u00f6nnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Legt ewren Gram was nieder,", "tokens": ["Legt", "ew\u00b7ren", "Gram", "was", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PWS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den schlawen Lebens-Dieb!", "tokens": ["Den", "schla\u00b7wen", "Le\u00b7bens\u00b7Dieb", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Offt haben alte Glieder", "tokens": ["Offt", "ha\u00b7ben", "al\u00b7te", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Noch junge Freiheit lieb.", "tokens": ["Noch", "jun\u00b7ge", "Frei\u00b7heit", "lieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die aber nicht zu lencken", "tokens": ["Die", "a\u00b7ber", "nicht", "zu", "len\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch auff-zu-bringen seyn,", "tokens": ["Noch", "auf\u00b7fzu\u00b7brin\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lassen sich beschencken", "tokens": ["Die", "las\u00b7sen", "sich", "be\u00b7schen\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit gutem Bier vnd Wein:", "tokens": ["Mit", "gu\u00b7tem", "Bier", "vnd", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geht, Blasien, schenckt die Mandel", "tokens": ["Geht", ",", "Bla\u00b7si\u00b7en", ",", "schenckt", "die", "Man\u00b7del"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Gl\u00e4ser frisch vnd voll!", "tokens": ["Der", "Gl\u00e4\u00b7ser", "frisch", "vnd", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr wisst in diesem Handel", "tokens": ["Ihr", "wisst", "in", "die\u00b7sem", "Han\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Hofes Ordnung wol.", "tokens": ["Des", "Ho\u00b7fes", "Ord\u00b7nung", "wol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Verzeiht mir noch daneben,", "tokens": ["Ver\u00b7zeiht", "mir", "noch", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Herren, da\u00df ich geh'!", "tokens": ["Ihr", "Her\u00b7ren", ",", "da\u00df", "ich", "geh'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr seht, mir winckt mein Leben,", "tokens": ["Ihr", "seht", ",", "mir", "winckt", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil ich im Tantze steh'.", "tokens": ["Weil", "ich", "im", "Tant\u00b7ze", "steh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich geb' euch zu erkennen,", "tokens": ["Ich", "geb'", "euch", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nembt Ihr es ab an euch:", "tokens": ["Nembt", "Ihr", "es", "ab", "an", "euch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob nicht mein Hertz mag brennen", "tokens": ["Ob", "nicht", "mein", "Hertz", "mag", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Kattich-Fewer gleich?", "tokens": ["Dem", "Kat\u00b7tich\u00b7Fe\u00b7wer", "gleich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In der sich meine Seele", "tokens": ["In", "der", "sich", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat gantz vnd gar verirrt,", "tokens": ["Hat", "gantz", "vnd", "gar", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von der mich kaum die H\u00f6le", "tokens": ["Von", "der", "mich", "kaum", "die", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Grabes trennen wird:", "tokens": ["Des", "Gra\u00b7bes", "tren\u00b7nen", "wird", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Solt' ich mit der nicht tantzen?", "tokens": ["Solt'", "ich", "mit", "der", "nicht", "tant\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PRELS", "PTKNEG", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "So h\u00e4tt' es diesen Schein,", "tokens": ["So", "h\u00e4tt'", "es", "die\u00b7sen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als solte schon das Pflantzen", "tokens": ["Als", "sol\u00b7te", "schon", "das", "Pflant\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Lieb' erstorben seyn?", "tokens": ["Der", "Lieb'", "ers\u00b7tor\u00b7ben", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "So lang' es, meine Sonne,", "tokens": ["So", "lang'", "es", ",", "mei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir warm zum Hertzen geht,", "tokens": ["Mir", "warm", "zum", "Hert\u00b7zen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Solt Ihr seyn meine Wonne;", "tokens": ["Solt", "Ihr", "seyn", "mei\u00b7ne", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich hab' in mir erh\u00f6ht", "tokens": ["Ich", "hab'", "in", "mir", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Schloss f\u00fcr euch, darinnen", "tokens": ["Ein", "Schloss", "f\u00fcr", "euch", ",", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Ewig herrschen solt,", "tokens": ["Ihr", "E\u00b7wig", "herr\u00b7schen", "solt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hie k\u00f6nnt ihr meinen Sinnen", "tokens": ["Hie", "k\u00f6nnt", "ihr", "mei\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gebieten wie ihr wolt.", "tokens": ["Ge\u00b7bie\u00b7ten", "wie", "ihr", "wolt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So last euch nun zu Ehren", "tokens": ["So", "last", "euch", "nun", "zu", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vn\u00df vnd der gantzen Schaar,", "tokens": ["Vn\u00df", "vnd", "der", "gant\u00b7zen", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihr Musicanten, h\u00f6ren", "tokens": ["Ihr", "Mu\u00b7si\u00b7can\u00b7ten", ",", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd macht es offenbahr:", "tokens": ["Vnd", "macht", "es", "of\u00b7fen\u00b7bahr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df mich vor allen Leiden", "tokens": ["Da\u00df", "mich", "vor", "al\u00b7len", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Lieb' jetzt hat verschantzt,", "tokens": ["Die", "Lieb'", "jetzt", "hat", "ver\u00b7schantzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "