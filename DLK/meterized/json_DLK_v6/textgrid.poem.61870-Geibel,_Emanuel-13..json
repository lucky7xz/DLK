{"textgrid.poem.61870": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vieles lernt der Dichter tragen,", "tokens": ["Vie\u00b7les", "lernt", "der", "Dich\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch am schwersten das Entsagen,", "tokens": ["Doch", "am", "schwers\u00b7ten", "das", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wenn in Wolken unerreicht", "tokens": ["Wenn", "in", "Wol\u00b7ken", "un\u00b7er\u00b7reicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm sein Ideal entweicht.", "tokens": ["Ihm", "sein", "I\u00b7deal", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wenn er sp\u00fcrt: es ward dir eben", "tokens": ["Wenn", "er", "sp\u00fcrt", ":", "es", "ward", "dir", "e\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur dein Ma\u00df der Kraft gegeben,", "tokens": ["Nur", "dein", "Ma\u00df", "der", "Kraft", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Statt des Zaubers der Gestalt", "tokens": ["Statt", "des", "Zau\u00b7bers", "der", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur ein Ton, wie bald verhallt!", "tokens": ["Nur", "ein", "Ton", ",", "wie", "bald", "ver\u00b7hallt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dennoch gib dich, Herz, zufrieden,", "tokens": ["Den\u00b7noch", "gib", "dich", ",", "Herz", ",", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df dir dieser Ton beschieden,", "tokens": ["Da\u00df", "dir", "die\u00b7ser", "Ton", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dankbar unter Leid und Lust", "tokens": ["Dank\u00b7bar", "un\u00b7ter", "Leid", "und", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Reif' ihn aus in treuer Brust.", "tokens": ["Reif'", "ihn", "aus", "in", "treu\u00b7er", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Macht' er doch zur Zeit des Lenzen", "tokens": ["Macht'", "er", "doch", "zur", "Zeit", "des", "Len\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst der Liebsten Auge gl\u00e4nzen,", "tokens": ["Einst", "der", "Liebs\u00b7ten", "Au\u00b7ge", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heut im herbstlich k\u00fchlen Hauch,", "tokens": ["Heut", "im", "herbst\u00b7lich", "k\u00fch\u00b7len", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dich labt, erwarb er auch.", "tokens": ["Was", "dich", "labt", ",", "er\u00b7warb", "er", "auch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ist's kein Ruhm auf weiter Erde,", "tokens": ["Ist's", "kein", "Ruhm", "auf", "wei\u00b7ter", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist's ein Blumenkranz am Herde;", "tokens": ["Ist's", "ein", "Blu\u00b7men\u00b7kranz", "am", "Her\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist's kein jauchzend Volk, Poet,", "tokens": ["Ist's", "kein", "jauch\u00b7zend", "Volk", ",", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PIAT", "ADJD", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist's ein Freund, der dich versteht.", "tokens": ["Ist's", "ein", "Freund", ",", "der", "dich", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}