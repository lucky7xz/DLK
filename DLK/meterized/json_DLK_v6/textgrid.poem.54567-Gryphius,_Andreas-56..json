{"textgrid.poem.54567": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der weitber\u00fchmte Mann/ der Vater hat der Welt", "tokens": ["Der", "weit\u00b7be\u00b7r\u00fchm\u00b7te", "Mann", "/", "der", "Va\u00b7ter", "hat", "der", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kr\u00e4uter Eigenschafft/ der V\u00f6gel Art und Wesen/", "tokens": ["Der", "Kr\u00e4u\u00b7ter", "Ei\u00b7gen\u00b7schafft", "/", "der", "V\u00f6\u00b7gel", "Art", "und", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Thiere Zucht und was von Fischen auszulesen/", "tokens": ["Der", "Thie\u00b7re", "Zucht", "und", "was", "von", "Fi\u00b7schen", "aus\u00b7zu\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PWS", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Ertz und Holtz und S\u00e4fft und Steine vorgestellt/", "tokens": ["Und", "Ertz", "und", "Holtz", "und", "S\u00e4fft", "und", "Stei\u00b7ne", "vor\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er zeigt uns was den Leib/ der Seelen Hau\u00df/ erh\u00e4lt/", "tokens": ["Er", "zeigt", "uns", "was", "den", "Leib", "/", "der", "See\u00b7len", "Hau\u00df", "/", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ART", "NN", "$(", "ART", "NN", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bringt alle Zeiten vor: lehrt wie das Land genesen/", "tokens": ["Bringt", "al\u00b7le", "Zei\u00b7ten", "vor", ":", "lehrt", "wie", "das", "Land", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$.", "VVFIN", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und sich entziehen m\u00f6g' erhitzter Seuchen Besen;", "tokens": ["Und", "sich", "ent\u00b7zie\u00b7hen", "m\u00f6g'", "er\u00b7hitz\u00b7ter", "Seu\u00b7chen", "Be\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "VMFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie da\u00df aus allen ihr denn nur ein Schaf gef\u00e4llt?", "tokens": ["Wie", "da\u00df", "aus", "al\u00b7len", "ihr", "denn", "nur", "ein", "Schaf", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPR", "PIAT", "PPOSAT", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Holdselge K\u00f6nigin sch\u00e4tzt sie den Hirten-Stab", "tokens": ["Hold\u00b7sel\u00b7ge", "K\u00f6\u00b7ni\u00b7gin", "sch\u00e4tzt", "sie", "den", "Hir\u00b7ten\u00b7Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Vor allen/ was man sch\u00e4tzt/ denn vor die sch\u00f6nste Gab?", "tokens": ["Vor", "al\u00b7len", "/", "was", "man", "sch\u00e4tzt", "/", "denn", "vor", "die", "sch\u00f6ns\u00b7te", "Gab", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$(", "PWS", "PIS", "VVFIN", "$(", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist sonst nichts/ als ein Schaf/ das w\u00fcrdig sey zu lieben?", "tokens": ["Ist", "sonst", "nichts", "/", "als", "ein", "Schaf", "/", "das", "w\u00fcr\u00b7dig", "sey", "zu", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$(", "KOUS", "ART", "NN", "$(", "ART", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nein warlich/ weil nichts mehr den reinen Sitten gleicht/", "tokens": ["Nein", "war\u00b7lich", "/", "weil", "nichts", "mehr", "den", "rei\u00b7nen", "Sit\u00b7ten", "gleicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "$(", "KOUS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nochmehr weil dieses Schaf von Gott durch Bitt erreicht.", "tokens": ["Noch\u00b7mehr", "weil", "die\u00b7ses", "Schaf", "von", "Gott", "durch", "Bitt", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PDAT", "NN", "APPR", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dis bleibt das h\u00f6chste Gut was Gott uns selbst verschrieben.", "tokens": ["Dis", "bleibt", "das", "h\u00f6chs\u00b7te", "Gut", "was", "Gott", "uns", "selbst", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "PWS", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}