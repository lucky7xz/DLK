{"dta.poem.21328": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es herrschte im ungeheuren Raum", "tokens": ["Es", "herrschte", "im", "un\u00b7ge\u00b7heu\u00b7ren", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Tod und Nacht und Schweigen;", "tokens": ["Nur", "Tod", "und", "Nacht", "und", "Schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es brannten Ampeln hie und da,", "tokens": ["Es", "brann\u00b7ten", "Am\u00b7peln", "hie", "und", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um die Dunkelheit recht zu zeigen.", "tokens": ["Um", "die", "Dun\u00b7kel\u00b7heit", "recht", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich wandelte lange den Pfeilern entlang", "tokens": ["Ich", "wan\u00b7del\u00b7te", "lan\u00b7ge", "den", "Pfei\u00b7lern", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPO"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und h\u00f6rte nur die Tritte", "tokens": ["Und", "h\u00f6r\u00b7te", "nur", "die", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von meinem Begleiter, er folgte mir", "tokens": ["Von", "mei\u00b7nem", "Be\u00b7glei\u00b7ter", ",", "er", "folg\u00b7te", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Auch hier bey jedem Schritte.", "tokens": ["Auch", "hier", "bey", "je\u00b7dem", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir kamen endlich zu einem Ort,", "tokens": ["Wir", "ka\u00b7men", "end\u00b7lich", "zu", "ei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo funkelnde Kerzenhelle", "tokens": ["Wo", "fun\u00b7keln\u00b7de", "Ker\u00b7zen\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und blitzendes Gold und Edelstein;", "tokens": ["Und", "blit\u00b7zen\u00b7des", "Gold", "und", "E\u00b7del\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das war die Drey-K\u00f6nigs-Kapelle.", "tokens": ["Das", "war", "die", "Drey\u00b7K\u00f6\u00b7nigs\u00b7Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Die heil\u2019gen drey K\u00f6nige jedoch,", "tokens": ["Die", "heil'\u00b7gen", "drey", "K\u00f6\u00b7ni\u00b7ge", "je\u00b7doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sonst so still dort lagen,", "tokens": ["Die", "sonst", "so", "still", "dort", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Wunder! sie sa\u00dfen aufrecht jetzt", "tokens": ["O", "Wun\u00b7der", "!", "sie", "sa\u00b7\u00dfen", "auf\u00b7recht", "jetzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "ADJD", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf ihren Sarkophagen.", "tokens": ["Auf", "ih\u00b7ren", "Sar\u00b7ko\u00b7pha\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}