{"textgrid.poem.49103": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "62. Billigung", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df der Deutsche diene dem Fremden getreulich, hat solchem", "tokens": ["Da\u00df", "der", "Deut\u00b7sche", "die\u00b7ne", "dem", "Frem\u00b7den", "ge\u00b7treu\u00b7lich", ",", "hat", "sol\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$,", "VAFIN", "PIAT"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er verpf\u00e4ndet sein Wort, la\u00df ich gefallen mir wol;", "tokens": ["Er", "ver\u00b7pf\u00e4n\u00b7det", "sein", "Wort", ",", "la\u00df", "ich", "ge\u00b7fal\u00b7len", "mir", "wol", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "VVPP", "PPER", "ADV", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nur verkaufen zum Sclaven soll nie er lassen sich, hetzen", "tokens": ["Nur", "ver\u00b7kau\u00b7fen", "zum", "Scla\u00b7ven", "soll", "nie", "er", "las\u00b7sen", "sich", ",", "het\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "APPRART", "NN", "VMFIN", "ADV", "PPER", "VVFIN", "PRF", "$,", "VVINF"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Gegen den Bruder als Hund \u2013 dies verabscheut mein Herz.", "tokens": ["Ge\u00b7gen", "den", "Bru\u00b7der", "als", "Hund", "\u2013", "dies", "ver\u00b7ab\u00b7scheut", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOUS", "NN", "$(", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}