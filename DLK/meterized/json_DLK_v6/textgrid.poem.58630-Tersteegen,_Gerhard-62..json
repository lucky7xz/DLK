{"textgrid.poem.58630": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein B\u00f6ses allzumal allein daher entspringet,", "tokens": ["Mein", "B\u00f6\u00b7ses", "all\u00b7zu\u00b7mal", "al\u00b7lein", "da\u00b7her", "ent\u00b7sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich verlasse dich, du aller Gutheit Quell;", "tokens": ["Da\u00df", "ich", "ver\u00b7las\u00b7se", "dich", ",", "du", "al\u00b7ler", "Gut\u00b7heit", "Quell", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "$,", "PPER", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach, welche Bitterkeit es meiner Seele bringet,", "tokens": ["Ach", ",", "wel\u00b7che", "Bit\u00b7ter\u00b7keit", "es", "mei\u00b7ner", "See\u00b7le", "brin\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAT", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ich mir deine Furcht nicht stets vor Augen stell'!", "tokens": ["Wenn", "ich", "mir", "dei\u00b7ne", "Furcht", "nicht", "stets", "vor", "Au\u00b7gen", "stell'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dies hab' ich l\u00e4ngst erkannt, nun seh' ich, was es gebe,", "tokens": ["Dies", "hab'", "ich", "l\u00e4ngst", "er\u00b7kannt", ",", "nun", "seh'", "ich", ",", "was", "es", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie du verl\u00e4ssest den, der dich zuerst verlie\u00df;", "tokens": ["Wie", "du", "ver\u00b7l\u00e4s\u00b7sest", "den", ",", "der", "dich", "zu\u00b7erst", "ver\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Herr, halt mich nah bei dir, da\u00df ich in Ehrfurcht lebe", "tokens": ["Herr", ",", "halt", "mich", "nah", "bei", "dir", ",", "da\u00df", "ich", "in", "Ehr\u00b7furcht", "le\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vor deinen Augen stets, nur dies ist gut und s\u00fc\u00df!", "tokens": ["Vor", "dei\u00b7nen", "Au\u00b7gen", "stets", ",", "nur", "dies", "ist", "gut", "und", "s\u00fc\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "ADV", "PDS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}