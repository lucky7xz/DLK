{"textgrid.poem.42113": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Oft aus den tiefen Sch\u00e4chten der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft aus den tiefen Sch\u00e4chten der Nacht", "tokens": ["Oft", "aus", "den", "tie\u00b7fen", "Sch\u00e4ch\u00b7ten", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Steigst Du empor: Dir trieft", "tokens": ["Steigst", "Du", "em\u00b7por", ":", "Dir", "trieft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dunkel Schweigens Gew\u00e4sser", "tokens": ["Dun\u00b7kel", "Schwei\u00b7gens", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Aus dem verworrenen Haar.", "tokens": ["Aus", "dem", "ver\u00b7wor\u00b7re\u00b7nen", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Oft vor der R\u00f6te des Morgens", "tokens": ["Oft", "vor", "der", "R\u00f6\u00b7te", "des", "Mor\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Steht das Gebirg, Deine Stirn,", "tokens": ["Steht", "das", "Ge\u00b7birg", ",", "Dei\u00b7ne", "Stirn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Schweifender Hang der Wange um", "tokens": ["Schwei\u00b7fen\u00b7der", "Hang", "der", "Wan\u00b7ge", "um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "APPR"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Deines Mundes vereisten Grat.", "tokens": ["Dei\u00b7nes", "Mun\u00b7des", "ver\u00b7eis\u00b7ten", "Grat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Abends d\u00e4mmert Dein Schatten oft.", "tokens": ["A\u00b7bends", "d\u00e4m\u00b7mert", "Dein", "Schat\u00b7ten", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Tief aus dem Spiegel des Sees,", "tokens": ["Tief", "aus", "dem", "Spie\u00b7gel", "des", "Sees", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Blicklosen Blickes hebt", "tokens": ["Blick\u00b7lo\u00b7sen", "Bli\u00b7ckes", "hebt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sich Dein goldenes Auge auf.", "tokens": ["Sich", "Dein", "gol\u00b7de\u00b7nes", "Au\u00b7ge", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Allein im Mittag seh ich Dich", "tokens": ["Al\u00b7lein", "im", "Mit\u00b7tag", "seh", "ich", "Dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beweglich, fl\u00fcchtigen Fu\u00dfes", "tokens": ["Be\u00b7weg\u00b7lich", ",", "fl\u00fcch\u00b7ti\u00b7gen", "Fu\u00b7\u00dfes"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ziehn wie die Zeit. Du sch\u00fcttelst", "tokens": ["Ziehn", "wie", "die", "Zeit", ".", "Du", "sch\u00fct\u00b7telst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "L\u00e4chelnd H\u00e4nde und Haupt.", "tokens": ["L\u00e4\u00b7chelnd", "H\u00e4n\u00b7de", "und", "Haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}