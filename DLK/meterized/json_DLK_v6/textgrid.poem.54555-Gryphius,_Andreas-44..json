{"textgrid.poem.54555": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du auch mein ander Ich/ du Lust in meinen Schmertzen/", "tokens": ["Du", "auch", "mein", "an\u00b7der", "Ich", "/", "du", "Lust", "in", "mei\u00b7nen", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "ADJD", "PPER", "$(", "PPER", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Seelen Auffenthalt/ du Spiegel reiner Zucht/", "tokens": ["Mein", "See\u00b7len", "Auf\u00b7fent\u00b7halt", "/", "du", "Spie\u00b7gel", "rei\u00b7ner", "Zucht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ich bi\u00dfher umsonst im Krei\u00df der Welt gesucht/", "tokens": ["Die", "ich", "bi\u00df\u00b7her", "um\u00b7sonst", "im", "Krei\u00df", "der", "Welt", "ge\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPRART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du scheidest und zureist dein Band in meinem Hertzen/", "tokens": ["Du", "schei\u00b7dest", "und", "zu\u00b7reist", "dein", "Band", "in", "mei\u00b7nem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch nein/ das Band ist gantz/ ob schon die Trauer-Kertzen", "tokens": ["Doch", "nein", "/", "das", "Band", "ist", "gantz", "/", "ob", "schon", "die", "Trau\u00b7e\u00b7rKert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$(", "ART", "NN", "VAFIN", "ADV", "$(", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verlodern \u00fcber dir/ und der betr\u00fcbten Frucht/", "tokens": ["Ver\u00b7lo\u00b7dern", "\u00fc\u00b7ber", "dir", "/", "und", "der", "be\u00b7tr\u00fcb\u00b7ten", "Frucht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$(", "KON", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein Lieben brennet noch/ und l\u00e4st bey deiner Flucht/", "tokens": ["Mein", "Lie\u00b7ben", "bren\u00b7net", "noch", "/", "und", "l\u00e4st", "bey", "dei\u00b7ner", "Flucht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich durch die Grabes-Nacht des Todes nicht verschertzen/", "tokens": ["Sich", "durch", "die", "Gra\u00b7bes\u00b7Nacht", "des", "To\u00b7des", "nicht", "ver\u00b7schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ob schon dein C\u00f6rper fault/ dein Leben lebt in mir.", "tokens": ["Ob", "schon", "dein", "C\u00f6r\u00b7per", "fault", "/", "dein", "Le\u00b7ben", "lebt", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich irre/ mein Gem\u00fcth ist ausser dir bey dir/", "tokens": ["Ich", "ir\u00b7re", "/", "mein", "Ge\u00b7m\u00fcth", "ist", "aus\u00b7ser", "dir", "bey", "dir", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Indem mein mattes Fleisch verschmeltzt in heissen Thr\u00e4nen.", "tokens": ["In\u00b7dem", "mein", "mat\u00b7tes", "Fleisch", "ver\u00b7schmeltzt", "in", "heis\u00b7sen", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie? l\u00e4ssest du mich hier? Wie? giebst du gute Nacht/", "tokens": ["Wie", "?", "l\u00e4s\u00b7sest", "du", "mich", "hier", "?", "Wie", "?", "giebst", "du", "gu\u00b7te", "Nacht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PRF", "ADV", "$.", "PWAV", "$.", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Indem die Flamme sinckt/ indem der Fried erwacht?", "tokens": ["In\u00b7dem", "die", "Flam\u00b7me", "sinckt", "/", "in\u00b7dem", "der", "Fried", "er\u00b7wacht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$(", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie? oder hast du dort wornach wir hier uns sehnen.", "tokens": ["Wie", "?", "o\u00b7der", "hast", "du", "dort", "wor\u00b7nach", "wir", "hier", "uns", "seh\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VAFIN", "PPER", "ADV", "PWAV", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}