{"textgrid.poem.33734": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "16. Mumie", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Leichnam sprach der Priester: Schlafend Leben!", "tokens": ["Zum", "Leich\u00b7nam", "sprach", "der", "Pries\u00b7ter", ":", "Schla\u00b7fend", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir h\u00fcllen dich in Bissus ein und D\u00fcfte,", "tokens": ["Wir", "h\u00fcl\u00b7len", "dich", "in", "Bis\u00b7sus", "ein", "und", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NE", "PTKVZ", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit dir wird hinunter in die Gr\u00fcfte", "tokens": ["Und", "mit", "dir", "wird", "hin\u00b7un\u00b7ter", "in", "die", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Larve und das Saatenkorn gegeben.", "tokens": ["Die", "Lar\u00b7ve", "und", "das", "Saa\u00b7ten\u00b7korn", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zum Zeichen, wie die Saat sich wird erheben", "tokens": ["Zum", "Zei\u00b7chen", ",", "wie", "die", "Saat", "sich", "wird", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wie der K\u00e4fer einst sich schwingt in L\u00fcfte,", "tokens": ["Und", "wie", "der", "K\u00e4\u00b7fer", "einst", "sich", "schwingt", "in", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wird der Geist auch durch die Grabeskl\u00fcfte", "tokens": ["So", "wird", "der", "Geist", "auch", "durch", "die", "Gra\u00b7bes\u00b7kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Licht empor in neue K\u00f6rper schweben.", "tokens": ["Zum", "Licht", "em\u00b7por", "in", "neu\u00b7e", "K\u00f6r\u00b7per", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Priester sprach's, die Mumie sank, verklungen", "tokens": ["Der", "Pries\u00b7ter", "sprach's", ",", "die", "Mu\u00b7mie", "sank", ",", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind zwei Jahrtausend \u00fcber seinem Worte.", "tokens": ["Sind", "zwei", "Jahr\u00b7tau\u00b7send", "\u00fc\u00b7ber", "sei\u00b7nem", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fragst du, ob Wahrheit uns sein Mund gesungen?", "tokens": ["Fragst", "du", ",", "ob", "Wahr\u00b7heit", "uns", "sein", "Mund", "ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schau hin! Die Saat durchbrach die H\u00fclsenpforte,", "tokens": ["Schau", "hin", "!", "Die", "Saat", "durch\u00b7brach", "die", "H\u00fcl\u00b7sen\u00b7pfor\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der K\u00e4fer hat in Freiheit sich geschwungen,", "tokens": ["Der", "K\u00e4\u00b7fer", "hat", "in", "Frei\u00b7heit", "sich", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Mumie nur schl\u00e4ft noch am alten Orte.", "tokens": ["Die", "Mu\u00b7mie", "nur", "schl\u00e4ft", "noch", "am", "al\u00b7ten", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}