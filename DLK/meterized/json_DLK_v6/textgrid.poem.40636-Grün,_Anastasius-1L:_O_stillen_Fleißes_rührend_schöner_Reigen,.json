{"textgrid.poem.40636": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: O stillen Flei\u00dfes r\u00fchrend sch\u00f6ner Reigen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O stillen Flei\u00dfes r\u00fchrend sch\u00f6ner Reigen,", "tokens": ["O", "stil\u00b7len", "Flei\u00b7\u00dfes", "r\u00fch\u00b7rend", "sch\u00f6\u00b7ner", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn zarte Frauen hier mit schweren Lasten", "tokens": ["Wenn", "zar\u00b7te", "Frau\u00b7en", "hier", "mit", "schwe\u00b7ren", "Las\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinan, hinab die Inseltreppe hasten,", "tokens": ["Hi\u00b7nan", ",", "hin\u00b7ab", "die", "In\u00b7sel\u00b7trep\u00b7pe", "has\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie ab und auf am Bronn die Eimer steigen!", "tokens": ["Wie", "ab", "und", "auf", "am", "Bronn", "die", "Ei\u00b7mer", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKVZ", "KON", "APPR", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Hochsinn ging in Dienstbarkeit sich neigen,", "tokens": ["Der", "Hoch\u00b7sinn", "ging", "in", "Dienst\u00b7bar\u00b7keit", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Thatkraft und Schw\u00e4che sich so hold umfa\u00dften,", "tokens": ["That\u00b7kraft", "und", "Schw\u00e4\u00b7che", "sich", "so", "hold", "um\u00b7fa\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herkulisch Tagwerk \u00fcbend ohne Rasten", "tokens": ["Her\u00b7ku\u00b7lisch", "Tag\u00b7werk", "\u00fc\u00b7bend", "oh\u00b7ne", "Ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und magdlich fromm es bergend tief in Schweigen.", "tokens": ["Und", "magd\u00b7lich", "fromm", "es", "ber\u00b7gend", "tief", "in", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJD", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sinnvoll, ihr Frauen, sprecht ihr's aus im Kleide:", "tokens": ["Sinn\u00b7voll", ",", "ihr", "Frau\u00b7en", ",", "sprecht", "ih\u00b7r's", "aus", "im", "Klei\u00b7de", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PIS", "APPR", "APPRART", "NN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des Hauptes schwarze H\u00fclle sagt von Leide,", "tokens": ["Des", "Haup\u00b7tes", "schwar\u00b7ze", "H\u00fcl\u00b7le", "sagt", "von", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das euch in Dunkelheit die Tage spinnen;", "tokens": ["Das", "euch", "in", "Dun\u00b7kel\u00b7heit", "die", "Ta\u00b7ge", "spin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch f\u00fcrstlich schwebt der Fu\u00df hinan die Treppe", "tokens": ["Doch", "f\u00fcrst\u00b7lich", "schwebt", "der", "Fu\u00df", "hi\u00b7nan", "die", "Trep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im sch\u00f6nverbr\u00e4mten Roth der Purpurschleppe:", "tokens": ["Im", "sch\u00f6n\u00b7ver\u00b7br\u00e4m\u00b7ten", "Roth", "der", "Pur\u00b7pur\u00b7schlep\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem\u00fcth'ge M\u00e4gde, hohe K\u00f6niginnen!", "tokens": ["De\u00b7m\u00fcth'\u00b7ge", "M\u00e4g\u00b7de", ",", "ho\u00b7he", "K\u00f6\u00b7ni\u00b7gin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}