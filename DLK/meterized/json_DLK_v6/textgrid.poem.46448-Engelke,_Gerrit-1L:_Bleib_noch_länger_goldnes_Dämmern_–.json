{"textgrid.poem.46448": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bleib noch l\u00e4nger goldnes D\u00e4mmern \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1904, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleib noch l\u00e4nger goldnes D\u00e4mmern \u2013", "tokens": ["Bleib", "noch", "l\u00e4n\u00b7ger", "gold\u00b7nes", "D\u00e4m\u00b7mern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie wird der Tag schon matt und blauer \u2013", "tokens": ["Wie", "wird", "der", "Tag", "schon", "matt", "und", "blau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verstummt ist L\u00e4rm und Werkstatth\u00e4mmern.", "tokens": ["Ver\u00b7stummt", "ist", "L\u00e4rm", "und", "Werk\u00b7statt\u00b7h\u00e4m\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Nacht liegt auf der Lauer \u2013", "tokens": ["Die", "Nacht", "liegt", "auf", "der", "Lau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Schl\u00fcssel schlie\u00dft die H\u00e4usertore.", "tokens": ["Der", "Schl\u00fcs\u00b7sel", "schlie\u00dft", "die", "H\u00e4u\u00b7ser\u00b7to\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun Wandrer meide die dunkle Mauer \u2013", "tokens": ["Nun", "Wand\u00b7rer", "mei\u00b7de", "die", "dunk\u00b7le", "Mau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Licht ist aus \u2013 es klingt im Ohre \u2013", "tokens": ["Das", "Licht", "ist", "aus", "\u2013", "es", "klingt", "im", "Oh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$(", "PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegen Strolche auf der Lauer?4 \u2013", "tokens": ["Lie\u00b7gen", "Strol\u00b7che", "auf", "der", "Lau\u00b7er", "?", "4", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "number", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$.", "CARD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinauf die knarrenden Windeltritte.", "tokens": ["Hin\u00b7auf", "die", "knar\u00b7ren\u00b7den", "Win\u00b7del\u00b7trit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Gasse w\u00e4scht ein Regenschauer.", "tokens": ["Die", "Gas\u00b7se", "w\u00e4scht", "ein", "Re\u00b7gen\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald nahen im Schlafe weiche Schritte:", "tokens": ["Bald", "na\u00b7hen", "im", "Schla\u00b7fe", "wei\u00b7che", "Schrit\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Traum liegt auf der Lauer \u2013", "tokens": ["Der", "Traum", "liegt", "auf", "der", "Lau\u00b7er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}