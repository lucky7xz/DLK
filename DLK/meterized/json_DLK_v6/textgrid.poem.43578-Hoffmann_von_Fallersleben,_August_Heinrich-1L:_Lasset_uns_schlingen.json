{"textgrid.poem.43578": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lasset uns schlingen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset uns schlingen", "tokens": ["Las\u00b7set", "uns", "schlin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Fr\u00fchling Bl\u00fcmelein zum Kranz!", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "Bl\u00fc\u00b7me\u00b7lein", "zum", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lasset uns springen,", "tokens": ["Las\u00b7set", "uns", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hei\u00dfa, zum Tanz!", "tokens": ["Hei\u00b7\u00dfa", ",", "zum", "Tanz", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Blumenpracht, Laubesduft, reges Gewimmel,", "tokens": ["Blu\u00b7men\u00b7pracht", ",", "Lau\u00b7bes\u00b7duft", ",", "re\u00b7ges", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Sang und Klang, Sonnenschein, heiterer Himmel!", "tokens": ["Sang", "und", "Klang", ",", "Son\u00b7nen\u00b7schein", ",", "hei\u00b7te\u00b7rer", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dank dem Gef\u00e4hrten", "tokens": ["Dank", "dem", "Ge\u00b7f\u00e4hr\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bei unserm Spiel und Scherz und Tand!", "tokens": ["Bei", "un\u00b7serm", "Spiel", "und", "Scherz", "und", "Tand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wir begehrten,", "tokens": ["Was", "wir", "be\u00b7gehr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Hat er gesandt.", "tokens": ["Hat", "er", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hat er doch wieder", "tokens": ["Hat", "er", "doch", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ein neues Leben angefacht,", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frohsinn und Lieder", "tokens": ["Froh\u00b7sinn", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wieder gebracht.", "tokens": ["Wie\u00b7der", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Lasset uns weilen", "tokens": ["Las\u00b7set", "uns", "wei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nun auch nicht l\u00e4nger mehr zu Haus!", "tokens": ["Nun", "auch", "nicht", "l\u00e4n\u00b7ger", "mehr", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lasset uns eilen,", "tokens": ["Las\u00b7set", "uns", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Eilen hinaus!", "tokens": ["Ei\u00b7len", "hin\u00b7aus", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Lasset uns schlingen", "tokens": ["Las\u00b7set", "uns", "schlin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Fr\u00fchling Bl\u00fcmelein zum Kranz!", "tokens": ["Dem", "Fr\u00fch\u00b7ling", "Bl\u00fc\u00b7me\u00b7lein", "zum", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lasset uns springen,", "tokens": ["Las\u00b7set", "uns", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hei\u00dfa, zum Tanz!", "tokens": ["Hei\u00b7\u00dfa", ",", "zum", "Tanz", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}