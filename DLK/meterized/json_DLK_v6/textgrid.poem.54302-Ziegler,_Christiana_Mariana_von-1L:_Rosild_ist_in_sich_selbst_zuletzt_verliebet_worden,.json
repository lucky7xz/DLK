{"textgrid.poem.54302": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rosild ist in sich selbst zuletzt verliebet worden,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosild ist in sich selbst zuletzt verliebet worden,", "tokens": ["Ro\u00b7sild", "ist", "in", "sich", "selbst", "zu\u00b7letzt", "ver\u00b7lie\u00b7bet", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PRF", "ADV", "ADV", "VVFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie lobt und r\u00fchmt an sich den himmlischen Verstand,", "tokens": ["Sie", "lobt", "und", "r\u00fchmt", "an", "sich", "den", "himm\u00b7li\u00b7schen", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und schreibt sich selber mit in der Sybillen Orden.", "tokens": ["Und", "schreibt", "sich", "sel\u00b7ber", "mit", "in", "der", "Sy\u00b7bil\u00b7len", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nechst hat sie sonder Schaam in Compagnie bekandt,", "tokens": ["Nechst", "hat", "sie", "son\u00b7der", "Schaam", "in", "Com\u00b7pag\u00b7nie", "be\u00b7kandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie h\u00fclffe k\u00fcnfftighin auch mit Calender machen,", "tokens": ["Sie", "h\u00fclf\u00b7fe", "k\u00fcnff\u00b7tig\u00b7hin", "auch", "mit", "Ca\u00b7len\u00b7der", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Weil ihr gescheider Kopf das Firmament versteht.", "tokens": ["Weil", "ihr", "ge\u00b7schei\u00b7der", "Kopf", "das", "Fir\u00b7ma\u00b7ment", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Rosildgen dein Verstand ist warlich zu belachen,", "tokens": ["Ro\u00b7sild\u00b7gen", "dein", "Ver\u00b7stand", "ist", "war\u00b7lich", "zu", "be\u00b7la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der \u00fcber Himmel, Meer und alle Pf\u00fctzen geht.", "tokens": ["Der", "\u00fc\u00b7ber", "Him\u00b7mel", ",", "Meer", "und", "al\u00b7le", "Pf\u00fct\u00b7zen", "geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer dich von ferne nur h\u00f6rt, freche N\u00e4rrin, sprechen,", "tokens": ["Wer", "dich", "von", "fer\u00b7ne", "nur", "h\u00f6rt", ",", "fre\u00b7che", "N\u00e4r\u00b7rin", ",", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADV", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der braucht kein Vomotiv, dieweil der Eckel macht", "tokens": ["Der", "braucht", "kein", "Vo\u00b7mo\u00b7tiv", ",", "die\u00b7weil", "der", "E\u00b7ckel", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df man sich von sich selbst mu\u00df von dem Prahlen brechen,", "tokens": ["Da\u00df", "man", "sich", "von", "sich", "selbst", "mu\u00df", "von", "dem", "Prah\u00b7len", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PRF", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das alle Welt an dir, wie billig ist, belacht.", "tokens": ["Das", "al\u00b7le", "Welt", "an", "dir", ",", "wie", "bil\u00b7lig", "ist", ",", "be\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPER", "$,", "PWAV", "ADJD", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Armselge, schweige doch, sonst wirst du noch der Erden", "tokens": ["Arm\u00b7sel\u00b7ge", ",", "schwei\u00b7ge", "doch", ",", "sonst", "wirst", "du", "noch", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Wofern dein Fratzen-Kopff die Schellen nicht versteckt,", "tokens": ["Wo\u00b7fern", "dein", "Frat\u00b7zen\u00b7Kopff", "die", "Schel\u00b7len", "nicht", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein Charletan des Volcks und Spott der Kinder werden,", "tokens": ["Ein", "Char\u00b7le\u00b7tan", "des", "Volcks", "und", "Spott", "der", "Kin\u00b7der", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein Vogel, wie du bist, wird am Gesang entdeckt.", "tokens": ["Ein", "Vo\u00b7gel", ",", "wie", "du", "bist", ",", "wird", "am", "Ge\u00b7sang", "ent\u00b7deckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PWAV", "PPER", "VAFIN", "$,", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}