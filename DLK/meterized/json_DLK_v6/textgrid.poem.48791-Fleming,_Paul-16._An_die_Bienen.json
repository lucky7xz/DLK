{"textgrid.poem.48791": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16. An die Bienen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlagt eure Werkstatt auf in dieser Linden hier,", "tokens": ["Schlagt", "eu\u00b7re", "Werk\u00b7statt", "auf", "in", "die\u00b7ser", "Lin\u00b7den", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "APPR", "PDAT", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die hohl ist von Natur, ihr Honigmeisterinnen.", "tokens": ["die", "hohl", "ist", "von", "Na\u00b7tur", ",", "ihr", "Ho\u00b7nig\u00b7meis\u00b7te\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Aue hier, durchn\u00e4\u00dft mit so viel kalter Br\u00fcnnen,", "tokens": ["Die", "Au\u00b7e", "hier", ",", "durc\u00b7hn\u00e4\u00dft", "mit", "so", "viel", "kal\u00b7ter", "Br\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "VVFIN", "APPR", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die bringt gesundes Gras und feisten Klee herf\u00fcr.", "tokens": ["die", "bringt", "ge\u00b7sun\u00b7des", "Gras", "und", "feis\u00b7ten", "Klee", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier wirket euer Werk, das s\u00fc\u00dfe, nach Begier.", "tokens": ["Hier", "wir\u00b7ket", "eu\u00b7er", "Werk", ",", "das", "s\u00fc\u00b7\u00dfe", ",", "nach", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier pfleget of zu gehn der Preis der Venusinnen,", "tokens": ["Hier", "pfle\u00b7get", "of", "zu", "gehn", "der", "Preis", "der", "Ve\u00b7nu\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ich sie lassen mu\u00df, so wachet ihr bei ihr.", "tokens": ["Weil", "ich", "sie", "las\u00b7sen", "mu\u00df", ",", "so", "wa\u00b7chet", "ihr", "bei", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Geschieht es, da\u00df vielleicht ein Ander ihr schleicht nach,", "tokens": ["Ge\u00b7schieht", "es", ",", "da\u00df", "viel\u00b7leicht", "ein", "An\u00b7der", "ihr", "schleicht", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "indem sie bei euch ist und diesen sch\u00f6nen Fl\u00fcssen,", "tokens": ["in\u00b7dem", "sie", "bei", "euch", "ist", "und", "die\u00b7sen", "sch\u00f6\u00b7nen", "Fl\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VAFIN", "KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und will mit Hinterlist ihr s\u00fc\u00dfes M\u00fcndlein k\u00fcssen,", "tokens": ["und", "will", "mit", "Hin\u00b7ter\u00b7list", "ihr", "s\u00fc\u00b7\u00dfes", "M\u00fcnd\u00b7lein", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "das euch auch s\u00fc\u00dfer macht, so sollt ihr meine Schmach,", "tokens": ["das", "euch", "auch", "s\u00fc\u00b7\u00dfer", "macht", ",", "so", "sollt", "ihr", "mei\u00b7ne", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "$,", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ihr Feinde der Gewalt, aus rechtem Eifer r\u00e4chen", "tokens": ["ihr", "Fein\u00b7de", "der", "Ge\u00b7walt", ",", "aus", "rech\u00b7tem", "Ei\u00b7fer", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und diesen frechen Mund alsbald zu Tode stechen.", "tokens": ["und", "die\u00b7sen", "fre\u00b7chen", "Mund", "als\u00b7bald", "zu", "To\u00b7de", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}