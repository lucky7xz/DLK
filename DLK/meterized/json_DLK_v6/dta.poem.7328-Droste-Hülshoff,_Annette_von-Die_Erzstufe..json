{"dta.poem.7328": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Erzstufe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, Blitze, Blitze! der Schwaden dr\u00e4ngt", "tokens": ["Ja", ",", "Blit\u00b7ze", ",", "Blit\u00b7ze", "!", "der", "Schwa\u00b7den", "dr\u00e4ngt"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Giftiges Gas am Risse hinaus,", "tokens": ["Gif\u00b7ti\u00b7ges", "Gas", "am", "Ris\u00b7se", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "APZR", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Auf einem Blitze bin ich gesprengt", "tokens": ["Auf", "ei\u00b7nem", "Blit\u00b7ze", "bin", "ich", "ge\u00b7sprengt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Aus meinem funkelnden Kellerhaus.", "tokens": ["Aus", "mei\u00b7nem", "fun\u00b7keln\u00b7den", "Kel\u00b7ler\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O, wie war ich zerbrochen und krank,", "tokens": ["O", ",", "wie", "war", "ich", "zer\u00b7bro\u00b7chen", "und", "krank", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie rieselt's mir \u00fcber die blanke Haut,", "tokens": ["Wie", "rie\u00b7selt's", "mir", "\u00fc\u00b7ber", "die", "blan\u00b7ke", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Wenn langsam schwellend der Tropfen sank,", "tokens": ["Wenn", "lang\u00b7sam", "schwel\u00b7lend", "der", "Trop\u00b7fen", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Des Zuges Schneide mich angegraut!", "tokens": ["Des", "Zu\u00b7ges", "Schnei\u00b7de", "mich", "an\u00b7ge\u00b7graut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Kennst du den Bergm\u00f6nch, den braunen Schelm,", "tokens": ["Kennst", "du", "den", "Berg\u00b7m\u00f6nch", ",", "den", "brau\u00b7nen", "Schelm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dem auf der Schulter das Antlitz krei\u00dft?", "tokens": ["Dem", "auf", "der", "Schul\u00b7ter", "das", "Ant\u00b7litz", "krei\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schwarz und rauh wie ein rostiger Helm,", "tokens": ["Schwarz", "und", "rauh", "wie", "ein", "ros\u00b7ti\u00b7ger", "Helm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wie die Grubenlampe sein Auge glei\u00dft.", "tokens": ["Wie", "die", "Gru\u00b7ben\u00b7lam\u00b7pe", "sein", "Au\u00b7ge", "glei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O, er ist b\u00f6se, t\u00fcckisch und schlimm!", "tokens": ["O", ",", "er", "ist", "b\u00f6\u00b7se", ",", "t\u00fc\u00b7ckisch", "und", "schlimm", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mit dem Gez\u00e4hn ", "tokens": ["Mit", "dem", "Ge\u00b7z\u00e4hn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Bis das schwefelnde Wetter im Grimm", "tokens": ["Bis", "das", "schwe\u00b7feln\u00b7de", "Wet\u00b7ter", "im", "Grimm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NE"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Gegen die weichende Rinde schwallt.", "tokens": ["Ge\u00b7gen", "die", "wei\u00b7chen\u00b7de", "Rin\u00b7de", "schwallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Steiger bete! du armer Knapp',", "tokens": ["Stei\u00b7ger", "be\u00b7te", "!", "du", "ar\u00b7mer", "Knapp'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "ADJA", "NN", "$,"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Dem in der H\u00fctte das Kindlein zart,", "tokens": ["Dem", "in", "der", "H\u00fct\u00b7te", "das", "Kin\u00b7dlein", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Betet! betet! eh ihr hinab,", "tokens": ["Be\u00b7tet", "!", "be\u00b7tet", "!", "eh", "ihr", "hin\u00b7ab", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "VVFIN", "$.", "KOUS", "PPER", "PTKVZ", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Eh zum letzten Male vor Ort ihr fahrt.", "tokens": ["Eh", "zum", "letz\u00b7ten", "Ma\u00b7le", "vor", "Ort", "ihr", "fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sieben N\u00e4chte hab' ich gesehn", "tokens": ["Sie\u00b7ben", "N\u00e4ch\u00b7te", "hab'", "ich", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie eine Walze rollen den Nacken,", "tokens": ["Wie", "ei\u00b7ne", "Wal\u00b7ze", "rol\u00b7len", "den", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und die Augen funkeln und drehn,", "tokens": ["Und", "die", "Au\u00b7gen", "fun\u00b7keln", "und", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und das Gez\u00e4hn sch\u00fcrfen und hacken.", "tokens": ["Und", "das", "Ge\u00b7z\u00e4hn", "sch\u00fcr\u00b7fen", "und", "ha\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort, dort hinter dem reichen Gang", "tokens": ["Dort", ",", "dort", "hin\u00b7ter", "dem", "rei\u00b7chen", "Gang"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Lauert der giftige Brodem; da", "tokens": ["Lau\u00b7ert", "der", "gif\u00b7ti\u00b7ge", "Bro\u00b7dem", ";", "da"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "KOUS"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wo der Kobold den Hammer schwang,", "tokens": ["Wo", "der", "Ko\u00b7bold", "den", "Ham\u00b7mer", "schwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich am Bruche ihn schnuppern sah.", "tokens": ["Wo", "ich", "am", "Bru\u00b7che", "ihn", "schnup\u00b7pern", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gleich dem Molche von Dunste trunken", "tokens": ["Gleich", "dem", "Mol\u00b7che", "von", "Duns\u00b7te", "trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Schwoll und wackelt' der Gnom am Grund,", "tokens": ["Schwoll", "und", "wa\u00b7ckelt'", "der", "Gnom", "am", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und des Gases knisternde Funken", "tokens": ["Und", "des", "Ga\u00b7ses", "knis\u00b7tern\u00b7de", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zogen in seinen saugenden Schlund.", "tokens": ["Zo\u00b7gen", "in", "sei\u00b7nen", "sau\u00b7gen\u00b7den", "Schlund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Bete, Steiger, den Morgenpsalm", "tokens": ["Be\u00b7te", ",", "Stei\u00b7ger", ",", "den", "Mor\u00b7gen\u00b7psalm"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Einmal noch, und dein \u201ewalt's Gott\u201c,", "tokens": ["Ein\u00b7mal", "noch", ",", "und", "dein", "\u201e", "walt's", "Gott", "\u201c", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "PPOSAT", "$(", "VVFIN", "NN", "$(", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinen Segen gen Wetters Qualm,", "tokens": ["Dei\u00b7nen", "Se\u00b7gen", "gen", "Wet\u00b7ters", "Qualm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "G\u00e4h' Verscheiden und Teufelsrott'.", "tokens": ["G\u00e4h'", "Ver\u00b7schei\u00b7den", "und", "Teu\u00b7fels\u00b7rott'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Schau noch einmal in's Angesicht", "tokens": ["Schau", "noch", "ein\u00b7mal", "in's", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Deinem T\u00f6chterchen, deinem Weib,", "tokens": ["Dei\u00b7nem", "T\u00f6ch\u00b7ter\u00b7chen", ",", "dei\u00b7nem", "Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und dann z\u00fcnde das Grubenlicht.", "tokens": ["Und", "dann", "z\u00fcn\u00b7de", "das", "Gru\u00b7ben\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "\u201egott die Seele, dem Schacht der Leib!\u201c", "tokens": ["\u201e", "gott", "die", "See\u00b7le", ",", "dem", "Schacht", "der", "Leib", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Sie sind vor Ort, die L\u00e4mpchen rund", "tokens": ["Sie", "sind", "vor", "Ort", ",", "die", "L\u00e4mp\u00b7chen", "rund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Irrwischfl\u00e4mmchen aufgestellt.", "tokens": ["Wie", "Irr\u00b7wischfl\u00e4mm\u00b7chen", "auf\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Winde keucht, es rollt der Hund, ", "tokens": ["Die", "Win\u00b7de", "keucht", ",", "es", "rollt", "der", "Hund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hammer pickt, die Stufe f\u00e4llt,", "tokens": ["Der", "Ham\u00b7mer", "pickt", ",", "die", "Stu\u00b7fe", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An Bleigew\u00fcrfel, Glimmerspath", "tokens": ["An", "Blei\u00b7ge\u00b7w\u00fcr\u00b7fel", ",", "Glim\u00b7mer\u00b7spath"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zerrinnend, malt der kleine Stral", "tokens": ["Zer\u00b7rin\u00b7nend", ",", "malt", "der", "klei\u00b7ne", "Stral"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In seiner Glorie schwimmend Rad", "tokens": ["In", "sei\u00b7ner", "Glo\u00b7rie", "schwim\u00b7mend", "Rad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich Regenbogen und Opal.", "tokens": ["Sich", "Re\u00b7gen\u00b7bo\u00b7gen", "und", "O\u00b7pal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Winde keucht, es rollt der Hund. \u2014", "tokens": ["Die", "Win\u00b7de", "keucht", ",", "es", "rollt", "der", "Hund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rst du des Schwadens Sausen nicht?", "tokens": ["H\u00f6rst", "du", "des", "Schwa\u00b7dens", "Sau\u00b7sen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Hagel br\u00f6ckelt es zum Grund \u2014", "tokens": ["Wie", "Ha\u00b7gel", "br\u00f6\u00b7ckelt", "es", "zum", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Hammer pickt, die Stufe bricht; \u2014", "tokens": ["Der", "Ham\u00b7mer", "pickt", ",", "die", "Stu\u00b7fe", "bricht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weh, weh! es z\u00fcndet, flammt hinein!", "tokens": ["Weh", ",", "weh", "!", "es", "z\u00fcn\u00b7det", ",", "flammt", "hin\u00b7ein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos