{"textgrid.poem.47929": {"metadata": {"author": {"name": "Stieler, Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1669, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vorraht in Saturnus Welt", "tokens": ["Der", "Vor\u00b7raht", "in", "Sa\u00b7tur\u00b7nus", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "war Korn und reiche Wolle/", "tokens": ["war", "Korn", "und", "rei\u00b7che", "Wol\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "ein gr\u00fcner Busch/ ein Brunn ein breites Feld:", "tokens": ["ein", "gr\u00fc\u00b7ner", "Busch", "/", "ein", "Brunn", "ein", "brei\u00b7tes", "Feld", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "dar lebte man ohn allen Neid und Grolle.", "tokens": ["dar", "leb\u00b7te", "man", "ohn", "al\u00b7len", "Neid", "und", "Grol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "VVFIN", "PIS", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sint da\u00df der Geiz und Hoffart kahm", "tokens": ["Sint", "da\u00df", "der", "Geiz", "und", "Hof\u00b7fart", "kahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und Herrschafft nahm:", "tokens": ["und", "Herr\u00b7schafft", "nahm", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "entstund' ein Reich des Eisens.", "tokens": ["ent\u00b7stund'", "ein", "Reich", "des", "Ei\u00b7sens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Man wolte Gold und Sammet tragen/", "tokens": ["Man", "wol\u00b7te", "Gold", "und", "Sam\u00b7met", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "die Einfalt samt der Tugend golte nicht.", "tokens": ["die", "Ein\u00b7falt", "samt", "der", "Tu\u00b7gend", "gol\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Sucht h\u00e4lt nu die Jungfern auch gefangen", "tokens": ["Die", "Sucht", "h\u00e4lt", "nu", "die", "Jung\u00b7fern", "auch", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "darum werd' ich vorbey gegangen.", "tokens": ["da\u00b7rum", "werd'", "ich", "vor\u00b7bey", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Man liebt die Runzel-haut den Husten und die Gicht.", "tokens": ["Man", "liebt", "die", "Run\u00b7zel\u00b7haut", "den", "Hus\u00b7ten", "und", "die", "Gicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}