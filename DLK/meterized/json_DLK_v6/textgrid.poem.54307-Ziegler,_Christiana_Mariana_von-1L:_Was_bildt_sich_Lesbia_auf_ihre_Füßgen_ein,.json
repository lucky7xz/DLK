{"textgrid.poem.54307": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was bildt sich Lesbia auf ihre F\u00fc\u00dfgen ein,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bildt sich Lesbia auf ihre F\u00fc\u00dfgen ein,", "tokens": ["Was", "bildt", "sich", "Les\u00b7bia", "auf", "ih\u00b7re", "F\u00fc\u00df\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "NE", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die gro\u00df und ungeschickt? es geht ihr wie den Pfauen,", "tokens": ["Die", "gro\u00df", "und", "un\u00b7ge\u00b7schickt", "?", "es", "geht", "ihr", "wie", "den", "Pfau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die gantz vortrefflich sch\u00f6n nach ihren Federn seyn;", "tokens": ["Die", "gantz", "vor\u00b7treff\u00b7lich", "sch\u00f6n", "nach", "ih\u00b7ren", "Fe\u00b7dern", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch darff man selbgen nicht nach ihren F\u00fc\u00dfen schauen,", "tokens": ["Doch", "darff", "man", "selb\u00b7gen", "nicht", "nach", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn diese sehen plump, beschmutzt und he\u00dflich aus.", "tokens": ["Denn", "die\u00b7se", "se\u00b7hen", "plump", ",", "be\u00b7schmutzt", "und", "he\u00df\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "$,", "VVPP", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr ungestalter Schein entziehrt ihr gantzes Prangen;", "tokens": ["Ihr", "un\u00b7ge\u00b7stal\u00b7ter", "Schein", "ent\u00b7ziehrt", "ihr", "gant\u00b7zes", "Pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mich d\u00fcnckt, es kommt bey dir auch eben so heraus,", "tokens": ["Mich", "d\u00fcnckt", ",", "es", "kommt", "bey", "dir", "auch", "e\u00b7ben", "so", "he\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Haut von Elffenbein, der Purpur deiner Wangen", "tokens": ["Die", "Haut", "von", "Elf\u00b7fen\u00b7bein", ",", "der", "Pur\u00b7pur", "dei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "F\u00e4llt ieden ins Gesicht, dir selbsten, ists nicht wahr?", "tokens": ["F\u00e4llt", "ie\u00b7den", "ins", "Ge\u00b7sicht", ",", "dir", "selbs\u00b7ten", ",", "ists", "nicht", "wahr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPRART", "NN", "$,", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Du kanst dich nimmer satt in deinen Spiegel sehen.", "tokens": ["Du", "kanst", "dich", "nim\u00b7mer", "satt", "in", "dei\u00b7nen", "Spie\u00b7gel", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch schau nach deinen Fu\u00df, wie stellt sich dieser dar?", "tokens": ["Doch", "schau", "nach", "dei\u00b7nen", "Fu\u00df", ",", "wie", "stellt", "sich", "die\u00b7ser", "dar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PRF", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So, da\u00df ihn iederman mit rechte mu\u00df verschm\u00e4hen.", "tokens": ["So", ",", "da\u00df", "ihn", "ie\u00b7der\u00b7man", "mit", "rech\u00b7te", "mu\u00df", "ver\u00b7schm\u00e4\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PIS", "APPR", "ADJA", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Inde\u00dfen bildst du dir doch recht was grosses ein,", "tokens": ["In\u00b7de\u00b7\u00dfen", "bildst", "du", "dir", "doch", "recht", "was", "gros\u00b7ses", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PWS", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Schrittgen seyn gewi\u00df, wie nach dem Tact, geme\u00dfen.", "tokens": ["Die", "Schritt\u00b7gen", "seyn", "ge\u00b7wi\u00df", ",", "wie", "nach", "dem", "Tact", ",", "ge\u00b7me\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die M\u00e4nner m\u00fcsten dir, meynst du, gest\u00e4ndig seyn,", "tokens": ["Die", "M\u00e4n\u00b7ner", "m\u00fcs\u00b7ten", "dir", ",", "meynst", "du", ",", "ge\u00b7st\u00e4n\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "VAINF", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Es habe die Natur gar nichts an dir verge\u00dfen.", "tokens": ["Es", "ha\u00b7be", "die", "Na\u00b7tur", "gar", "nichts", "an", "dir", "ver\u00b7ge\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nein! du betr\u00fcgest dich, denn es ergehet dir,", "tokens": ["Nein", "!", "du", "be\u00b7tr\u00fc\u00b7gest", "dich", ",", "denn", "es", "er\u00b7ge\u00b7het", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie jenen Bilde dort, das Babels K\u00f6nig sahe:", "tokens": ["Wie", "je\u00b7nen", "Bil\u00b7de", "dort", ",", "das", "Ba\u00b7bels", "K\u00f6\u00b7nig", "sa\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "$,", "PRELS", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die F\u00fc\u00dfe stellten Thon und kahles Eisen f\u00fcr;", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "stell\u00b7ten", "Thon", "und", "kah\u00b7les", "Ei\u00b7sen", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jedoch der C\u00f6rper kam Gold, Ertz und Silber nahe.", "tokens": ["Je\u00b7doch", "der", "C\u00f6r\u00b7per", "kam", "Gold", ",", "Ertz", "und", "Sil\u00b7ber", "na\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}}}}}