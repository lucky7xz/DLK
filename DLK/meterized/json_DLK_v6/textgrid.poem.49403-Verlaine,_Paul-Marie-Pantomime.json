{"textgrid.poem.49403": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "Pantomime", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pierrot, der so ungleich Clitander,", "tokens": ["Pier\u00b7rot", ",", "der", "so", "un\u00b7gleich", "Cli\u00b7tan\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADV", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Langt zu und vertilgt nacheinander", "tokens": ["Langt", "zu", "und", "ver\u00b7tilgt", "na\u00b7ch\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "ADV"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ohne Zaudern Pastete und Wein.", "tokens": ["Oh\u00b7ne", "Zau\u00b7dern", "Pas\u00b7te\u00b7te", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Cassander seh' ich dort stehen,", "tokens": ["Cas\u00b7san\u00b7der", "seh'", "ich", "dort", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Eine Tr\u00e4ne im Grund der Alleen", "tokens": ["Ei\u00b7ne", "Tr\u00e4\u00b7ne", "im", "Grund", "der", "Al\u00b7leen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dem enterbten Neffen zu weihn.", "tokens": ["Dem", "en\u00b7terb\u00b7ten", "Nef\u00b7fen", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Zur Entf\u00fchrung von Colombine", "tokens": ["Zur", "Ent\u00b7f\u00fch\u00b7rung", "von", "Co\u00b7lom\u00b7bi\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NE"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Macht der Schelm von Harlekin Miene,", "tokens": ["Macht", "der", "Schelm", "von", "Har\u00b7le\u00b7kin", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der sein Rad hier viermal schl\u00e4gt.", "tokens": ["Der", "sein", "Rad", "hier", "vier\u00b7mal", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Colombine staunt, dass im Winde", "tokens": ["Co\u00b7lom\u00b7bi\u00b7ne", "staunt", ",", "dass", "im", "Win\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ein Herz sie tr\u00e4umend empfinde,", "tokens": ["Ein", "Herz", "sie", "tr\u00e4u\u00b7mend", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ihr Herz ein Fl\u00fcstern bewegt.", "tokens": ["Und", "ihr", "Herz", "ein", "Fl\u00fcs\u00b7tern", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}