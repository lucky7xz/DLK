{"textgrid.poem.49742": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Sittlichkeitskonferenz zu Magdeburg", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich euch wieder, hochehrw\u00fcrd'ge Herren,", "tokens": ["Seh'", "ich", "euch", "wie\u00b7der", ",", "hoch\u00b7ehr\u00b7w\u00fcrd'\u00b7ge", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nachdem f\u00fcr euch mich etwas einzusperren", "tokens": ["Nach\u00b7dem", "f\u00fcr", "euch", "mich", "et\u00b7was", "ein\u00b7zu\u00b7sper\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "PRF", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In herber Strenge suchten fromme Schwaben?", "tokens": ["In", "her\u00b7ber", "Stren\u00b7ge", "such\u00b7ten", "from\u00b7me", "Schwa\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 Bis jetzt nur \u00bbsuchten\u00ab, weil sie mich nicht haben.", "tokens": ["\u2013", "Bis", "jetzt", "nur", "\u00bb", "such\u00b7ten", "\u00ab", ",", "weil", "sie", "mich", "nicht", "ha\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "ADV", "$(", "VVFIN", "$(", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So hat nun Gott der Herr mich heimgesuchet,", "tokens": ["So", "hat", "nun", "Gott", "der", "Herr", "mich", "heim\u00b7ge\u00b7su\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz offenbarlich, weil ihr mich verfluchet.", "tokens": ["Ganz", "of\u00b7fen\u00b7bar\u00b7lich", ",", "weil", "ihr", "mich", "ver\u00b7flu\u00b7chet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er mu\u00dfte wohl, und war's nur, um euch Biedern", "tokens": ["Er", "mu\u00df\u00b7te", "wohl", ",", "und", "wa\u00b7r's", "nur", ",", "um", "euch", "Bie\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KON", "VAFIN", "ADV", "$,", "KOUI", "PPER", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die vielen Dienste freundlich zu erwidern.", "tokens": ["Die", "vie\u00b7len", "Diens\u00b7te", "freund\u00b7lich", "zu", "er\u00b7wi\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hall\u00e4h! und Lujah! singt jetzt, frohe Sieger!", "tokens": ["Hal\u00b7l\u00e4h", "!", "und", "Lu\u00b7jah", "!", "singt", "jetzt", ",", "fro\u00b7he", "Sie\u00b7ger", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "NE", "$.", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und B\u00e4h! und Muh! geliebte Kinderkrieger!", "tokens": ["Und", "B\u00e4h", "!", "und", "Muh", "!", "ge\u00b7lieb\u00b7te", "Kin\u00b7der\u00b7krie\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "KON", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch eure Frauen singen um die Wette,", "tokens": ["Auch", "eu\u00b7re", "Frau\u00b7en", "sin\u00b7gen", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das hei\u00dft: die paar, die nicht im Wochenbette.", "tokens": ["Das", "hei\u00dft", ":", "die", "paar", ",", "die", "nicht", "im", "Wo\u00b7chen\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "ART", "PIAT", "$,", "PRELS", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich ehre sie, seitdem Herr Bohn beschrieben,", "tokens": ["Ich", "eh\u00b7re", "sie", ",", "seit\u00b7dem", "Herr", "Bohn", "be\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "NN", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie z\u00e4rtlich sie in stiller Kammer lieben,", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "sie", "in", "stil\u00b7ler", "Kam\u00b7mer", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie ihn selbst so oft die Mannheit zierte.", "tokens": ["Und", "wie", "ihn", "selbst", "so", "oft", "die", "Mann\u00b7heit", "zier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das hei\u00dft: wenn Bohn nicht etwa renommierte.", "tokens": ["Das", "hei\u00dft", ":", "wenn", "Bohn", "nicht", "et\u00b7wa", "re\u00b7nom\u00b7mier\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "KOUS", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und nun verleih' der Herr euch allen St\u00e4rke!", "tokens": ["Und", "nun", "ver\u00b7leih'", "der", "Herr", "euch", "al\u00b7len", "St\u00e4r\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Nutzen seh' ich nicht von eurem Werke.", "tokens": ["Den", "Nut\u00b7zen", "seh'", "ich", "nicht", "von", "eu\u00b7rem", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr k\u00f6nnt euch jedes Jahr aufs neu' versammeln,", "tokens": ["Ihr", "k\u00f6nnt", "euch", "je\u00b7des", "Jahr", "aufs", "neu'", "ver\u00b7sam\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Menschen lieben, und die Hasen rammeln.", "tokens": ["Die", "Men\u00b7schen", "lie\u00b7ben", ",", "und", "die", "Ha\u00b7sen", "ram\u00b7meln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und auch ihr Frommen \u2013 \u2013 ja, was wollt' ich sagen? \u2013", "tokens": ["Und", "auch", "ihr", "From\u00b7men", "\u2013", "\u2013", "ja", ",", "was", "wollt'", "ich", "sa\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "$(", "$(", "PTKANT", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr solltet euch nicht allzusehr beklagen,", "tokens": ["Ihr", "soll\u00b7tet", "euch", "nicht", "all\u00b7zu\u00b7sehr", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hall\u00e4h! und Lujah! B\u00e4h! und Muh! Die K\u00e4lber,", "tokens": ["Hal\u00b7l\u00e4h", "!", "und", "Lu\u00b7jah", "!", "B\u00e4h", "!", "und", "Muh", "!", "Die", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "NE", "$.", "NN", "$.", "KON", "NN", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie kommen auf die Welt nicht ganz von selber.", "tokens": ["Sie", "kom\u00b7men", "auf", "die", "Welt", "nicht", "ganz", "von", "sel\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKNEG", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}