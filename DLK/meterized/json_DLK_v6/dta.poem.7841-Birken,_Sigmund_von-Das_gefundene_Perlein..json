{"dta.poem.7841": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Das gefundene Perlein.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Sch\u00e4fer/ wohlbekandt", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "/", "wohl\u00b7be\u00b7kandt"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NE", "$(", "PWAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "am gr\u00fcnen Pegnitzrand/", "tokens": ["am", "gr\u00fc\u00b7nen", "Peg\u00b7nitz\u00b7rand", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "gieng in dem Feld spaziren/", "tokens": ["gieng", "in", "dem", "Feld", "spa\u00b7zi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "und ite\u00df den Weg sich f\u00fchren", "tokens": ["und", "i\u00b7te\u00df", "den", "Weg", "sich", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PRF", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "in einen Buchenh\u00e4yn/", "tokens": ["in", "ei\u00b7nen", "Bu\u00b7chen\u00b7h\u00e4yn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "der ihn geladen ein.", "tokens": ["der", "ihn", "ge\u00b7la\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier wolt er diesesmahl/", "tokens": ["Hier", "wolt", "er", "die\u00b7ses\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "von solcher St\u00e4mme Zahl/", "tokens": ["von", "sol\u00b7cher", "St\u00e4m\u00b7me", "Zahl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ein Paar ihm auserwehlen:", "tokens": ["ein", "Paar", "ihm", "au\u00b7ser\u00b7weh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "gewillt/ davon zu scheelen", "tokens": ["ge\u00b7willt", "/", "da\u00b7von", "zu", "schee\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$(", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "ein Rinden oder zwey/", "tokens": ["ein", "Rin\u00b7den", "o\u00b7der", "zwey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "CARD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "zu seiner Liederey.", "tokens": ["zu", "sei\u00b7ner", "Lie\u00b7de\u00b7rey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er sahe hin und her/", "tokens": ["Er", "sa\u00b7he", "hin", "und", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "lie\u00df in die l\u00e4ng und qu\u00e4r", "tokens": ["lie\u00df", "in", "die", "l\u00e4ng", "und", "qu\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die Augen gehn spaziren/", "tokens": ["die", "Au\u00b7gen", "gehn", "spa\u00b7zi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "vor sich was aus zusp\u00fcren.", "tokens": ["vor", "sich", "was", "aus", "zu\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "PRELS", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er fand der Buchen viel.", "tokens": ["Er", "fand", "der", "Bu\u00b7chen", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der H\u00e4yn ihm wohlgefiel.", "tokens": ["Der", "H\u00e4yn", "ihm", "wohl\u00b7ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": " so reich an B\u00e4umen war", "tokens": ["so", "reich", "an", "B\u00e4u\u00b7men", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "di\u00df W\u00e4ldlein hier und dar:", "tokens": ["di\u00df", "W\u00e4ld\u00b7lein", "hier", "und", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df ihn/ in seinem Denken/", "tokens": ["da\u00df", "ihn", "/", "in", "sei\u00b7nem", "Den\u00b7ken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die Wahl sieng an zukr\u00e4nken.", "tokens": ["die", "Wahl", "sieng", "an", "zu\u00b7kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er w\u00fcnschte/ da\u00df der H\u00e4yn", "tokens": ["Er", "w\u00fcnschte", "/", "da\u00df", "der", "H\u00e4yn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "ganz m\u00f6chte werden sein.", "tokens": ["ganz", "m\u00f6ch\u00b7te", "wer\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VAINF", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}