{"dta.poem.13051": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Mond erhob sich einst die Sonne zu betrachten,", "tokens": ["\u201e", "der", "Mond", "er\u00b7hob", "sich", "einst", "die", "Son\u00b7ne", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "440\u201dSo fieng er an, sich selbst erstaunend zu verachten.", "tokens": ["\"", "So", "fi\u00b7eng", "er", "an", ",", "sich", "selbst", "er\u00b7stau\u00b7nend", "zu", "ver\u00b7ach\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PRF", "ADV", "VVPP", "PTKZU", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u201eer sah mit tr\u00fcbem Aug an ihr die Pracht des Lichts,", "tokens": ["\u201e", "er", "sah", "mit", "tr\u00fc\u00b7bem", "Aug", "an", "ihr", "die", "Pracht", "des", "Lichts", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ean seinem blassen Rund und Umkrei\u00df aber nichts", "tokens": ["\u201e", "an", "sei\u00b7nem", "blas\u00b7sen", "Rund", "und", "Um\u00b7krei\u00df", "a\u00b7ber", "nichts"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "ADV", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eals graue Dunckelheit. Er wandt sich nach der Ferne,", "tokens": ["\u201e", "als", "grau\u00b7e", "Dun\u00b7ckel\u00b7heit", ".", "Er", "wandt", "sich", "nach", "der", "Fer\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADJA", "NN", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ein welcher das Gebliz der allerkl\u00e4rsten Sterne", "tokens": ["\u201e", "in", "wel\u00b7cher", "das", "Ge\u00b7bliz", "der", "al\u00b7ler\u00b7kl\u00e4rs\u00b7ten", "Ster\u00b7ne"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PRELS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "445\u201dDie blauen B\u00f6gen ziert. Er nennte sie zwar sch\u00f6n,", "tokens": ["\"", "Die", "blau\u00b7en", "B\u00f6\u00b7gen", "ziert", ".", "Er", "nenn\u00b7te", "sie", "zwar", "sch\u00f6n", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eallein es schien ihm mehr am Sonnen-Licht zu sehn.", "tokens": ["\u201e", "al\u00b7lein", "es", "schien", "ihm", "mehr", "am", "Son\u00b7nen\u00b7Licht", "zu", "sehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eer merckte, da\u00df die Luft und Erde spieglend gl\u00fche,", "tokens": ["\u201e", "er", "merck\u00b7te", ",", "da\u00df", "die", "Luft", "und", "Er\u00b7de", "spieg\u00b7lend", "gl\u00fc\u00b7he", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewann kaum ein Strahl davon im Fr\u00fchling in der Fr\u00fche", "tokens": ["\u201e", "wann", "kaum", "ein", "Strahl", "da\u00b7von", "im", "Fr\u00fch\u00b7ling", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "PAV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edieselbigen bemahlt; wie sehr, wann selbst das Rund", "tokens": ["\u201e", "die\u00b7sel\u00b7bi\u00b7gen", "be\u00b7mahlt", ";", "wie", "sehr", ",", "wann", "selbst", "das", "Rund"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VVFIN", "$.", "PWAV", "ADV", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "450\u201dDer Sonne sich erh\u00f6ht, und jene goldne Stund", "tokens": ["\"", "Der", "Son\u00b7ne", "sich", "er\u00b7h\u00f6ht", ",", "und", "je\u00b7ne", "gold\u00b7ne", "Stund"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PRF", "VVPP", "$,", "KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eder Welt verk\u00fcndiget, die selbst den Tag besch\u00e4met,", "tokens": ["\u201e", "der", "Welt", "ver\u00b7k\u00fcn\u00b7di\u00b7get", ",", "die", "selbst", "den", "Tag", "be\u00b7sch\u00e4\u00b7met", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eda sie das bunte Feld mit ihrem Licht bes\u00e4met.", "tokens": ["\u201e", "da", "sie", "das", "bun\u00b7te", "Feld", "mit", "ih\u00b7rem", "Licht", "be\u00b7s\u00e4\u00b7met", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201eso fuhr er auf und sprach: wer ist dann hier das Haupt?", "tokens": ["\u201e", "so", "fuhr", "er", "auf", "und", "sprach", ":", "wer", "ist", "dann", "hier", "das", "Haupt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$.", "PWS", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201ein dieser Gegenwart bin ich des Rangs beraubt!", "tokens": ["\u201e", "in", "die\u00b7ser", "Ge\u00b7gen\u00b7wart", "bin", "ich", "des", "Rangs", "be\u00b7raubt", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "455\u201dNoch mehr ein blasser Stern, von dessen Eigenschaften", "tokens": ["\"", "Noch", "mehr", "ein", "blas\u00b7ser", "Stern", ",", "von", "des\u00b7sen", "Ei\u00b7gen\u00b7schaf\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201edie meisten an der Sonn und ihrer Wirckung hafften.", "tokens": ["\u201e", "die", "meis\u00b7ten", "an", "der", "Sonn", "und", "ih\u00b7rer", "Wir\u00b7ckung", "haff\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "APPR", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eich dencke was ich will (fuhr er verwundert fort)", "tokens": ["\u201e", "ich", "den\u00b7cke", "was", "ich", "will", "(", "fuhr", "er", "ver\u00b7wun\u00b7dert", "fort", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PWS", "PPER", "VMFIN", "$(", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201eich sehe kein Gesch\u00f6pf, kein Wesen, keinen Ort", "tokens": ["\u201e", "ich", "se\u00b7he", "kein", "Ge\u00b7sch\u00f6pf", ",", "kein", "We\u00b7sen", ",", "kei\u00b7nen", "Ort"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201ewohin nicht ihre Macht mit Licht und Strahlen dringet,", "tokens": ["\u201e", "wo\u00b7hin", "nicht", "ih\u00b7re", "Macht", "mit", "Licht", "und", "Strah\u00b7len", "drin\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "460\u201dSo sie nicht f\u00e4rbt, belebt, erw\u00e4rmt, in Wachsthum bringet.", "tokens": ["\"", "So", "sie", "nicht", "f\u00e4rbt", ",", "be\u00b7lebt", ",", "er\u00b7w\u00e4rmt", ",", "in", "Wach\u00b7sthum", "brin\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "PTKNEG", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201esie schwingt sich durch die Welt mit mehrer Pracht und Schein,", "tokens": ["\u201e", "sie", "schwingt", "sich", "durch", "die", "Welt", "mit", "meh\u00b7rer", "Pracht", "und", "Schein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eals aller Sterne Glanz in eins verkn\u00fcpft kann seyn.", "tokens": ["\u201e", "als", "al\u00b7ler", "Ster\u00b7ne", "Glanz", "in", "eins", "ver\u00b7kn\u00fcpft", "kann", "seyn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIAT", "NN", "NN", "APPR", "PIS", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201ewer mag dahero nicht aus allen Wercken sp\u00fchren,", "tokens": ["\u201e", "wer", "mag", "da\u00b7he\u00b7ro", "nicht", "aus", "al\u00b7len", "Wer\u00b7cken", "sp\u00fch\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PAV", "PTKNEG", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u201eda\u00df wir vor ihr die Kraft, mithin den Rang verliehren?", "tokens": ["\u201e", "da\u00df", "wir", "vor", "ihr", "die", "Kraft", ",", "mi\u00b7thin", "den", "Rang", "ver\u00b7lieh\u00b7ren", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PPER", "ART", "NN", "$,", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}