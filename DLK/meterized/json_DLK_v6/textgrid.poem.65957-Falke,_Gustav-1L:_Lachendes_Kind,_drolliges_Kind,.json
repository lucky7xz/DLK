{"textgrid.poem.65957": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lachendes Kind, drolliges Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lachendes Kind, drolliges Kind,", "tokens": ["La\u00b7chen\u00b7des", "Kind", ",", "drol\u00b7li\u00b7ges", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Blitzblick und Gr\u00fcbchen in Wangen,", "tokens": ["Blitz\u00b7blick", "und", "Gr\u00fcb\u00b7chen", "in", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Nur einen Walzer noch. Nicht zu geschwind.", "tokens": ["Nur", "ei\u00b7nen", "Wal\u00b7zer", "noch", ".", "Nicht", "zu", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$.", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Seliges Wiegen so, la la la la la la", "tokens": ["Se\u00b7li\u00b7ges", "Wie\u00b7gen", "so", ",", "la", "la", "la", "la", "la", "la"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "$,", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Will es im Himmel nicht besser verlangen.", "tokens": ["Will", "es", "im", "Him\u00b7mel", "nicht", "bes\u00b7ser", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Munter im Kreise. Bald sind verstummt", "tokens": ["Mun\u00b7ter", "im", "Krei\u00b7se", ".", "Bald", "sind", "ver\u00b7stummt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$.", "ADV", "VAFIN", "VVPP"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Brummbass und Fiedel und Fl\u00f6ten.", "tokens": ["Brumm\u00b7bass", "und", "Fie\u00b7del", "und", "Fl\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eh' uns der Werkeltag wieder umsummt,", "tokens": ["Eh'", "uns", "der", "Wer\u00b7kel\u00b7tag", "wie\u00b7der", "um\u00b7summt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur einen Walzer noch, la la la la la la", "tokens": ["Nur", "ei\u00b7nen", "Wal\u00b7zer", "noch", ",", "la", "la", "la", "la", "la", "la"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Warum unschuldige Fr\u00f6hlichkeit t\u00f6ten.", "tokens": ["Wa\u00b7rum", "un\u00b7schul\u00b7di\u00b7ge", "Fr\u00f6h\u00b7lich\u00b7keit", "t\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Mutter, bevor sie den Vater nahm,", "tokens": ["Mut\u00b7ter", ",", "be\u00b7vor", "sie", "den", "Va\u00b7ter", "nahm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hat es nicht anders getrieben.", "tokens": ["Hat", "es", "nicht", "an\u00b7ders", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wenn nach der Arbeit der Sonntag kam,", "tokens": ["Wenn", "nach", "der", "Ar\u00b7beit", "der", "Sonn\u00b7tag", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ach, einen Walzer nur, la la la la la la", "tokens": ["Ach", ",", "ei\u00b7nen", "Wal\u00b7zer", "nur", ",", "la", "la", "la", "la", "la", "la"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "ADV", "$,", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und nun sollt' es die Tochter nicht lieben.", "tokens": ["Und", "nun", "sollt'", "es", "die", "Toch\u00b7ter", "nicht", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Taschen voll Lebenslust, Geld grad' genug,", "tokens": ["Ta\u00b7schen", "voll", "Le\u00b7bens\u00b7lust", ",", "Geld", "grad'", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "NN", "ADV", "ADV", "$,"], "meter": "+--++--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Gilt noch ein Zaudern, ein Fragen?", "tokens": ["Gilt", "noch", "ein", "Zau\u00b7dern", ",", "ein", "Fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fangen wir heute die Freuden im Flug,", "tokens": ["Fan\u00b7gen", "wir", "heu\u00b7te", "die", "Freu\u00b7den", "im", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Nur einen Walzer noch, la la la la la la", "tokens": ["Nur", "ei\u00b7nen", "Wal\u00b7zer", "noch", ",", "la", "la", "la", "la", "la", "la"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "$,", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es", "FM.es"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Morgen hei\u00dft's wieder sich placken und plagen.", "tokens": ["Mor\u00b7gen", "hei\u00dft's", "wie\u00b7der", "sich", "pla\u00b7cken", "und", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}