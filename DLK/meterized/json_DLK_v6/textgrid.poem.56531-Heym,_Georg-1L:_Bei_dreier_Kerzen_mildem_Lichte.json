{"textgrid.poem.56531": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei dreier Kerzen mildem Lichte", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei dreier Kerzen mildem Lichte", "tokens": ["Bei", "drei\u00b7er", "Ker\u00b7zen", "mil\u00b7dem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Leiche schl\u00e4ft. Und hohe M\u00f6nche gehen", "tokens": ["Die", "Lei\u00b7che", "schl\u00e4ft", ".", "Und", "ho\u00b7he", "M\u00f6n\u00b7che", "ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um sie herum, und legen ihre Finger", "tokens": ["Um", "sie", "he\u00b7rum", ",", "und", "le\u00b7gen", "ih\u00b7re", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Manchmal \u00fcber ihr Angesicht.", "tokens": ["Manch\u00b7mal", "\u00fc\u00b7ber", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Froh sind die Toten, die zur Ruhe kehren", "tokens": ["Froh", "sind", "die", "To\u00b7ten", ",", "die", "zur", "Ru\u00b7he", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und strecken ihre wei\u00dfen H\u00e4nde aus,", "tokens": ["Und", "stre\u00b7cken", "ih\u00b7re", "wei\u00b7\u00dfen", "H\u00e4n\u00b7de", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Engeln zu, die gro\u00df und schattig gehen", "tokens": ["Den", "En\u00b7geln", "zu", ",", "die", "gro\u00df", "und", "schat\u00b7tig", "ge\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Fl\u00fcgelschlagen durch das hohe Haus.", "tokens": ["Mit", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen", "durch", "das", "ho\u00b7he", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur manchmal schallt ein Weinen durch die W\u00e4nde,", "tokens": ["Nur", "manch\u00b7mal", "schallt", "ein", "Wei\u00b7nen", "durch", "die", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein tiefes Schluchzen w\u00e4lzt sich in der Lust.", "tokens": ["Ein", "tie\u00b7fes", "Schluch\u00b7zen", "w\u00e4lzt", "sich", "in", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man kreuzet ihre hageren Finger-H\u00e4nde", "tokens": ["Man", "kreu\u00b7zet", "ih\u00b7re", "ha\u00b7ge\u00b7ren", "Fin\u00b7ger\u00b7H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zum Frieden sanft auf die verhaarte Brust.", "tokens": ["Zum", "Frie\u00b7den", "sanft", "auf", "die", "ver\u00b7ha\u00b7ar\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}}}}}