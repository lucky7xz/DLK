{"textgrid.poem.60186": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du, dem durch drey lange schwarze N\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, dem durch drey lange schwarze N\u00e4chte", "tokens": ["O", "du", ",", "dem", "durch", "drey", "lan\u00b7ge", "schwar\u00b7ze", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "APPR", "CARD", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Todes-Engel offenbahrt", "tokens": ["Ein", "To\u00b7des\u00b7En\u00b7gel", "of\u00b7fen\u00b7bahrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Abgrund, wo f\u00fcr Ungerechte", "tokens": ["Den", "Ab\u00b7grund", ",", "wo", "f\u00fcr", "Un\u00b7ge\u00b7rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Quaal zubereitet ward.", "tokens": ["Qua\u00b7al", "zu\u00b7be\u00b7rei\u00b7tet", "ward", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund, sprich wann sagt der Himmel seine Fl\u00fcche", "tokens": ["Freund", ",", "sprich", "wann", "sagt", "der", "Him\u00b7mel", "sei\u00b7ne", "Fl\u00fc\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "PWAV", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wuth, die aus der H\u00f6lle flog,", "tokens": ["Der", "Wuth", ",", "die", "aus", "der", "H\u00f6l\u00b7le", "flog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch Gewalt der Friedensbr\u00fcche", "tokens": ["Und", "durch", "Ge\u00b7walt", "der", "Frie\u00b7dens\u00b7br\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ins Herz des Kriegers zog?", "tokens": ["Ins", "Herz", "des", "Krie\u00b7gers", "zog", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie kam und hauchte pestisches Verderben", "tokens": ["Sie", "kam", "und", "hauch\u00b7te", "pes\u00b7ti\u00b7sches", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf ganzer V\u00f6lkerschaften Gl\u00fcck,", "tokens": ["Auf", "gan\u00b7zer", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah' gest\u00fcrzter Menschen Sterben,", "tokens": ["Und", "sah'", "ge\u00b7st\u00fcrz\u00b7ter", "Men\u00b7schen", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit nimmer sattem Blick.", "tokens": ["Mit", "nim\u00b7mer", "sat\u00b7tem", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Tief in die Schlacht flog sie von Heer zu Heere,", "tokens": ["Tief", "in", "die", "Schlacht", "flog", "sie", "von", "Heer", "zu", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Und weit verbreitet ward der Tod.", "tokens": ["Und", "weit", "ver\u00b7brei\u00b7tet", "ward", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An allen Ufern bis zum Meere,", "tokens": ["An", "al\u00b7len", "U\u00b7fern", "bis", "zum", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward das Gew\u00e4sser roth.", "tokens": ["Ward", "das", "Ge\u00b7w\u00e4s\u00b7ser", "roth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "O! von den Gr\u00e4bern ungez\u00e4hlter Leichen", "tokens": ["O", "!", "von", "den", "Gr\u00e4\u00b7bern", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Lei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Verwendet oft der Tag sein Haupt,", "tokens": ["Ver\u00b7wen\u00b7det", "oft", "der", "Tag", "sein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieht halb todte Menschen schleichen,", "tokens": ["Und", "sieht", "halb", "tod\u00b7te", "Men\u00b7schen", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganz der Krieg beraubt.", "tokens": ["Die", "ganz", "der", "Krieg", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dort wankt auf seiner unbefl\u00fcgten Erde", "tokens": ["Dort", "wankt", "auf", "sei\u00b7ner", "un\u00b7be\u00b7fl\u00fcg\u00b7ten", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Landmann traurig fort, und tritt", "tokens": ["Der", "Land\u00b7mann", "trau\u00b7rig", "fort", ",", "und", "tritt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch in den Hufschlag von dem Pferde", "tokens": ["Noch", "in", "den", "Huf\u00b7schlag", "von", "dem", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Worauf sein Pl\u00fcndrer ritt.", "tokens": ["Wo\u00b7rauf", "sein", "Pl\u00fcnd\u00b7rer", "ritt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn Hunger aus der bleich gewordnen Wange,", "tokens": ["Wenn", "Hun\u00b7ger", "aus", "der", "bleich", "ge\u00b7word\u00b7nen", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gram aus des Mundes Seufzer spricht;", "tokens": ["Gram", "aus", "des", "Mun\u00b7des", "Seuf\u00b7zer", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dann reizet ihn zum Lobgesange,", "tokens": ["Dann", "rei\u00b7zet", "ihn", "zum", "Lob\u00b7ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die satte Lerche nicht.", "tokens": ["Die", "sat\u00b7te", "Ler\u00b7che", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihn reizet nicht im buntgestreiften Kleide", "tokens": ["Ihn", "rei\u00b7zet", "nicht", "im", "bunt\u00b7ge\u00b7streif\u00b7ten", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Tulpe, die sich stolz erhebt \u2013 \u2013", "tokens": ["Die", "Tul\u00b7pe", ",", "die", "sich", "stolz", "er\u00b7hebt", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Rock ward ohne Hand und Seide", "tokens": ["Ihr", "Rock", "ward", "oh\u00b7ne", "Hand", "und", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geordnet und gewebt", "tokens": ["Ge\u00b7ord\u00b7net", "und", "ge\u00b7webt"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Von Gott, erhaben \u00fcber alle Thronen,", "tokens": ["Von", "Gott", ",", "er\u00b7ha\u00b7ben", "\u00fc\u00b7ber", "al\u00b7le", "Thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der tausend Welten ausgeschm\u00fcckt,", "tokens": ["Der", "tau\u00b7send", "Wel\u00b7ten", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mehr als tausend Nationen", "tokens": ["Und", "mehr", "als", "tau\u00b7send", "Na\u00b7ti\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einmahl \u00fcberblickt.", "tokens": ["Auf", "ein\u00b7mahl", "\u00fc\u00b7berb\u00b7lickt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Er sah' herab, zerstreute Frankreichs Flotten,", "tokens": ["Er", "sah'", "her\u00b7ab", ",", "zer\u00b7streu\u00b7te", "Fran\u00b7kreichs", "Flot\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ri\u00df viel Inseln aus der Hand", "tokens": ["Und", "ri\u00df", "viel", "In\u00b7seln", "aus", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Ludewigs, der sein zu spotten", "tokens": ["Des", "Lu\u00b7de\u00b7wigs", ",", "der", "sein", "zu", "spot\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Heer hat ausgesandt.", "tokens": ["Ein", "Heer", "hat", "aus\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Er fuhr herab, in einer Feuerwolke,", "tokens": ["Er", "fuhr", "her\u00b7ab", ",", "in", "ei\u00b7ner", "Feu\u00b7er\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ihn an Braunschweigs Mauren trug,", "tokens": ["Die", "ihn", "an", "Braun\u00b7schweigs", "Mau\u00b7ren", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als mit viel Muth und wenig Volke,", "tokens": ["Als", "mit", "viel", "Muth", "und", "we\u00b7nig", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Heer ein J\u00fcngling schlug.", "tokens": ["Das", "Heer", "ein", "J\u00fcng\u00b7ling", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "O Freund, er warf drey grosse Gottesblicke", "tokens": ["O", "Freund", ",", "er", "warf", "drey", "gros\u00b7se", "Got\u00b7tes\u00b7bli\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf uns. Wir zitterten voll Schmerz;", "tokens": ["Auf", "uns", ".", "Wir", "zit\u00b7ter\u00b7ten", "voll", "Schmerz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PPER", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da rief er schnell ein Weh zur\u00fccke,", "tokens": ["Da", "rief", "er", "schnell", "ein", "Weh", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sandt' ein F\u00fcrsten-Herz.", "tokens": ["Und", "sandt'", "ein", "F\u00fcrs\u00b7ten\u00b7Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Mit seinem Finger wirft er alles nieder,", "tokens": ["Mit", "sei\u00b7nem", "Fin\u00b7ger", "wirft", "er", "al\u00b7les", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was den Gerechten noch bedr\u00e4ngt;", "tokens": ["Was", "den", "Ge\u00b7rech\u00b7ten", "noch", "be\u00b7dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er eilt, da\u00df bald die H\u00f6lle wieder", "tokens": ["Er", "eilt", ",", "da\u00df", "bald", "die", "H\u00f6l\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Krieges Wuth empf\u00e4ngt!", "tokens": ["Des", "Krie\u00b7ges", "Wuth", "emp\u00b7f\u00e4ngt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}