{"textgrid.poem.46663": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[noch bis in die letzten Z\u00fcge]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch bis in die letzten Z\u00fcge", "tokens": ["Noch", "bis", "in", "die", "letz\u00b7ten", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hielt ichs f\u00fcr unm\u00f6glich,", "tokens": ["Hielt", "ichs", "f\u00fcr", "un\u00b7m\u00f6g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df Gott meinem Herzen schl\u00fcge", "tokens": ["Da\u00df", "Gott", "mei\u00b7nem", "Her\u00b7zen", "schl\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Diese Wunde t\u00f6dtlich.", "tokens": ["Die\u00b7se", "Wun\u00b7de", "t\u00f6dt\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "F\u00fchlen sollt' ich's zur Gen\u00fcge,", "tokens": ["F\u00fch\u00b7len", "sollt'", "ich's", "zur", "Ge\u00b7n\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchl' es doch nicht v\u00f6llig;", "tokens": ["F\u00fchl'", "es", "doch", "nicht", "v\u00f6l\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn ich nicht die Wunde tr\u00fcge,", "tokens": ["Wenn", "ich", "nicht", "die", "Wun\u00b7de", "tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hielt' ich's noch unm\u00f6glich.", "tokens": ["Hielt'", "ich's", "noch", "un\u00b7m\u00f6g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}