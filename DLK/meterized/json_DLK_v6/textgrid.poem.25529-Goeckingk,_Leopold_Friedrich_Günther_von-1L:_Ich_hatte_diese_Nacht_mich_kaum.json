{"textgrid.poem.25529": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hatte diese Nacht mich kaum", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte diese Nacht mich kaum", "tokens": ["Ich", "hat\u00b7te", "die\u00b7se", "Nacht", "mich", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Schlummer hingestreckt,", "tokens": ["Zum", "Schlum\u00b7mer", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward ich, ach! aus s\u00fc\u00dfem Traum'", "tokens": ["Da", "ward", "ich", ",", "ach", "!", "aus", "s\u00fc\u00b7\u00dfem", "Traum'"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ITJ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schon wieder aufgeschreckt.", "tokens": ["Schon", "wie\u00b7der", "auf\u00b7ge\u00b7schreckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Trommel ging, die Glocke klang,", "tokens": ["Die", "Trom\u00b7mel", "ging", ",", "die", "Glo\u00b7cke", "klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der W\u00e4chter stie\u00df ins Rohr,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "stie\u00df", "ins", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus jeder Th\u00fcr' und Fenster sprang", "tokens": ["Aus", "je\u00b7der", "Th\u00fcr'", "und", "Fens\u00b7ter", "sprang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein blo\u00dfes Hemd' hervor.", "tokens": ["Ein", "blo\u00b7\u00dfes", "Hemd'", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wie stob ich aus dem Bett' heraus!", "tokens": ["Wie", "stob", "ich", "aus", "dem", "Bett'", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein s\u00fc\u00dfer Traum verschwand,", "tokens": ["Mein", "s\u00fc\u00b7\u00dfer", "Traum", "ver\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Mein Muth dazu, des Nachbars Haus", "tokens": ["Mein", "Muth", "da\u00b7zu", ",", "des", "Nach\u00b7bars", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PAV", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Stand lichterloh in Brand.", "tokens": ["Stand", "lich\u00b7ter\u00b7loh", "in", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Bild, Lock' und Lieder! bleibt nur mein!", "tokens": ["Bild", ",", "Lock'", "und", "Lie\u00b7der", "!", "bleibt", "nur", "mein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$.", "VVFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Kommt! folgt mir bis ins Grab!", "tokens": ["Kommt", "!", "folgt", "mir", "bis", "ins", "Grab", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und nun, mein H\u00e4uschen, mu\u00df es seyn,", "tokens": ["Und", "nun", ",", "mein", "H\u00e4usc\u00b7hen", ",", "mu\u00df", "es", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Nun wohl! so brenn' itzt ab!", "tokens": ["Nun", "wohl", "!", "so", "brenn'", "itzt", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Auf unsern Kirchhof lief ich da", "tokens": ["Auf", "un\u00b7sern", "Kirch\u00b7hof", "lief", "ich", "da"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mit meinem Schatz', und stand", "tokens": ["Mit", "mei\u00b7nem", "Schatz'", ",", "und", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und k\u00fc\u00dfte dein Portrait, und sah", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "dein", "Por\u00b7trait", ",", "und", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Gelassen in den Brand.", "tokens": ["Ge\u00b7las\u00b7sen", "in", "den", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Dein Schutzgeist, welcher \u00fcber mir", "tokens": ["Dein", "Schutz\u00b7geist", ",", "wel\u00b7cher", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dein Bild mich k\u00fcssen sah,", "tokens": ["Dein", "Bild", "mich", "k\u00fcs\u00b7sen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Sprach zu der Flamme: Stehe hier!", "tokens": ["Sprach", "zu", "der", "Flam\u00b7me", ":", "Ste\u00b7he", "hier", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.24": {"text": "Und pl\u00f6tzlich stand sie da!", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "stand", "sie", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}