{"textgrid.poem.53185": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Epitaphium Ambrosii Lobwa\u00dferi quod sibi ipse posuit", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ich erkant, die Welt sey nichts vnd nichs ihr Pracht,", "tokens": ["Der", "ich", "er\u00b7kant", ",", "die", "Welt", "sey", "nichts", "vnd", "nichs", "ihr", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ART", "NN", "VAFIN", "PIS", "KON", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lieg auch nun hie zu Staub, Sch\u00e4m, Asch vnd nichts gemacht.", "tokens": ["Lieg", "auch", "nun", "hie", "zu", "Staub", ",", "Sch\u00e4m", ",", "Asch", "vnd", "nichts", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "KON", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch der au\u00df nichts erbawt den Himmel vnd die Erden,", "tokens": ["Doch", "der", "au\u00df", "nichts", "er\u00b7bawt", "den", "Him\u00b7mel", "vnd", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PIS", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wirdt mich sampt meinem Fleisch zu nichs nicht lassen werden.", "tokens": ["Wirdt", "mich", "sampt", "mei\u00b7nem", "Fleisch", "zu", "nichs", "nicht", "las\u00b7sen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIS", "PTKNEG", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum hab ich nach dem Tod vnd allem nichts gefragt,", "tokens": ["Drum", "hab", "ich", "nach", "dem", "Tod", "vnd", "al\u00b7lem", "nichts", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "APPR", "ART", "NN", "KON", "PIS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wa\u00df nichts ist, wei\u00df ich, bleibt von W\u00fcrmern ungenagt.", "tokens": ["Wa\u00df", "nichts", "ist", ",", "wei\u00df", "ich", ",", "bleibt", "von", "W\u00fcr\u00b7mern", "un\u00b7ge\u00b7nagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}