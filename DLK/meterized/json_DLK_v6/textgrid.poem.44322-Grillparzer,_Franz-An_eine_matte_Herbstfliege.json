{"textgrid.poem.44322": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An eine matte Herbstfliege", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wanken dir die matten F\u00fc\u00dfe,", "tokens": ["Wan\u00b7ken", "dir", "die", "mat\u00b7ten", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ist der Fl\u00fcgel Schwung erlahmt?", "tokens": ["ist", "der", "Fl\u00fc\u00b7gel", "Schwung", "er\u00b7lahmt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traurig schleichst du an dem Fenster,", "tokens": ["Trau\u00b7rig", "schleichst", "du", "an", "dem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "das einst deine Spiele sah:", "tokens": ["das", "einst", "dei\u00b7ne", "Spie\u00b7le", "sah", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach, der Sommer ist verronnen,", "tokens": ["Ach", ",", "der", "Som\u00b7mer", "ist", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und der rauhe Winter naht.", "tokens": ["und", "der", "rau\u00b7he", "Win\u00b7ter", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sieh meine welken Kniee,", "tokens": ["Doch", "sieh", "mei\u00b7ne", "wel\u00b7ken", "Kni\u00b7ee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "sieh das Antlitz totenbleich,", "tokens": ["sieh", "das", "Ant\u00b7litz", "to\u00b7ten\u00b7bleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "sieh der Augen mutges Feuer,", "tokens": ["sieh", "der", "Au\u00b7gen", "mut\u00b7ges", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "von der Krankheit Hauch gel\u00f6scht;", "tokens": ["von", "der", "Krank\u00b7heit", "Hauch", "ge\u00b7l\u00f6scht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "ist denn schon mein Herbst gekommen,", "tokens": ["ist", "denn", "schon", "mein", "Herbst", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eh mein Sommer noch erschien?", "tokens": ["eh", "mein", "Som\u00b7mer", "noch", "er\u00b7schien", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}