{"textgrid.poem.63418": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Sacht Rat!", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sacht Rat! Min oll l\u00fctt Fahlen,", "tokens": ["Sacht", "Rat", "!", "Min", "oll", "l\u00fctt", "Fah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "NN", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wat b\u00fcstz du schier un glatt!", "tokens": ["wat", "b\u00fcstz", "du", "schier", "un", "glatt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dor wa\u00dft woll sch\u00f6nen Klewer,", "tokens": ["Dor", "wa\u00dft", "woll", "sch\u00f6\u00b7nen", "Kle\u00b7wer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dor inne Koppel, wat?", "tokens": ["dor", "in\u00b7ne", "Kop\u00b7pel", ",", "wat", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wu stur kannst du di steilen!", "tokens": ["Wu", "stur", "kannst", "du", "di", "stei\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wat settst du, flink un blind,", "tokens": ["Wat", "settst", "du", ",", "flink", "un", "blind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "FM", "FM", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "hen \u0153wer Sleet un Rick un Tun,", "tokens": ["hen", "\u0153wer", "Sleet", "un", "Rick", "un", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "so licht as Fr\u00fchjohrswind!", "tokens": ["so", "licht", "as", "Fr\u00fch\u00b7johrs\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wat hest f\u00fcr dralle Been du,", "tokens": ["Wat", "hest", "f\u00fcr", "dral\u00b7le", "Be\u00b7en", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "wu dull sleihst achter ut!", "tokens": ["wu", "dull", "sleihst", "ach\u00b7ter", "ut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wat b\u00fcst du kr\u0153nsch und w\u00e4hlig", "tokens": ["Wat", "b\u00fcst", "du", "kr\u0153nsch", "und", "w\u00e4h\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un prust so mit din Snut!", "tokens": ["un", "prust", "so", "mit", "din", "Snut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sacht Rat! min oll l\u00fctt Fahlen,", "tokens": ["Sacht", "Rat", "!", "min", "oll", "l\u00fctt", "Fah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPOSAT", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sacht Rat! Wu ball, wu ball \u2013", "tokens": ["sacht", "Rat", "!", "Wu", "ball", ",", "wu", "ball", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "NE", "ADV", "$,", "PWAV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "s\u00fch, denn so k\u00fcmmt de Winter,", "tokens": ["s\u00fch", ",", "denn", "so", "k\u00fcmmt", "de", "Win\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ADV", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un denn k\u00fcmmst uppen Stall.", "tokens": ["un", "denn", "k\u00fcmmst", "up\u00b7pen", "Stall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Nahst kam se mit de \u00c4gen,", "tokens": ["Nahst", "kam", "se", "mit", "de", "\u00c4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "mit Haken un mit Plog,", "tokens": ["mit", "Ha\u00b7ken", "un", "mit", "Plog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sw\u00e4p, Sadel, Tom un S\u00e4len,", "tokens": ["Sw\u00e4p", ",", "Sa\u00b7del", ",", "Tom", "un", "S\u00e4\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "denn warst du fram genog.", "tokens": ["denn", "warst", "du", "fram", "ge\u00b7nog", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn geiht di't anne Knaken,", "tokens": ["Denn", "geiht", "di't", "an\u00b7ne", "Kna\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gifft't man'n str\u00e4ken Fatt;", "tokens": ["gifft't", "man'n", "str\u00e4\u00b7ken", "Fatt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "denn kamen Kropp un Schibel", "tokens": ["denn", "ka\u00b7men", "Kropp", "un", "Schi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "FM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un Muk un Gall un Spatt.", "tokens": ["un", "Muk", "un", "Gall", "un", "Spatt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Sacht Rat! min oll l\u00fctt Fahlen,", "tokens": ["Sacht", "Rat", "!", "min", "oll", "l\u00fctt", "Fah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPOSAT", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sacht Rat! Wu ball, wu ball \u2013", "tokens": ["sacht", "Rat", "!", "Wu", "ball", ",", "wu", "ball", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "NE", "ADV", "$,", "PWAV", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "s\u00fch, denn so k\u00fcmmst de Winter,", "tokens": ["s\u00fch", ",", "denn", "so", "k\u00fcmmst", "de", "Win\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "ADV", "VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "un denn warst up du stallt!", "tokens": ["un", "denn", "warst", "up", "du", "stallt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "KON", "VAFIN", "NE", "PPER", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}