{"textgrid.poem.51996": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Staub aus Cypern mu\u00df auff deine Haare schneyn,", "tokens": ["Der", "Staub", "aus", "Cy\u00b7pern", "mu\u00df", "auff", "dei\u00b7ne", "Haa\u00b7re", "schneyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles nach Zibeth an deinen Kleidern schmecken:", "tokens": ["Und", "al\u00b7les", "nach", "Zi\u00b7beth", "an", "dei\u00b7nen", "Klei\u00b7dern", "schme\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es pflegt, indem du gehst, umb dich ein Dampff zu seyn,", "tokens": ["Es", "pflegt", ",", "in\u00b7dem", "du", "gehst", ",", "umb", "dich", "ein", "Dampff", "zu", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUI", "PRF", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den nicht der Nebel kan an Moschus Reh erwecken.", "tokens": ["Den", "nicht", "der", "Ne\u00b7bel", "kan", "an", "Mo\u00b7schus", "Reh", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "VMFIN", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sieht, da\u00df deine Hand Pomamber Kugeln tr\u00e4gt,", "tokens": ["Man", "sieht", ",", "da\u00df", "dei\u00b7ne", "Hand", "Po\u00b7mam\u00b7ber", "Ku\u00b7geln", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da\u00df die Wangen dir wie frische Rosen bl\u00fchen,", "tokens": ["Und", "da\u00df", "die", "Wan\u00b7gen", "dir", "wie", "fri\u00b7sche", "Ro\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "KOKOM", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn ihres Purpurs Schein die Schmincke dr\u00fcber legt,", "tokens": ["Wenn", "ih\u00b7res", "Pur\u00b7purs", "Schein", "die", "Schmin\u00b7cke", "dr\u00fc\u00b7ber", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So fangen wie Corall die Lippen an zu gl\u00fcen:", "tokens": ["So", "fan\u00b7gen", "wie", "Co\u00b7rall", "die", "Lip\u00b7pen", "an", "zu", "gl\u00fcen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Wasser, welches fr\u00fch aus deinem Becken fleust:", "tokens": ["Das", "Was\u00b7ser", ",", "wel\u00b7ches", "fr\u00fch", "aus", "dei\u00b7nem", "Be\u00b7cken", "fleust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Reucht nach Pari\u00df, die Seiff ist von Venedig kommen:", "tokens": ["Reucht", "nach", "Pa\u00b7ri\u00df", ",", "die", "Seiff", "ist", "von", "Ve\u00b7ne\u00b7dig", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "ART", "NN", "VAFIN", "APPR", "NE", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Die Kohlen schmeltzen dir, was der Chineser geust,", "tokens": ["Die", "Koh\u00b7len", "schmelt\u00b7zen", "dir", ",", "was", "der", "Chi\u00b7ne\u00b7ser", "geust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.12": {"text": "Und die Pastillen sind aus Spanien geschwommen.", "tokens": ["Und", "die", "Pas\u00b7til\u00b7len", "sind", "aus", "Spa\u00b7ni\u00b7en", "ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wei\u00df nicht, was es hilfft, Humande, deine Zier,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "es", "hilfft", ",", "Hu\u00b7man\u00b7de", ",", "dei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wiewol [du] deinen Leib mit Balsam hast bestrichen,", "tokens": ["Wie\u00b7wol", "du", "dei\u00b7nen", "Leib", "mit", "Bal\u00b7sam", "hast", "be\u00b7stri\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PPER", "$(", "PPOSAT", "NN", "APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Dann gienge gleich dein Ruff den besten Salben f\u00fcr,", "tokens": ["Dann", "gien\u00b7ge", "gleich", "dein", "Ruff", "den", "bes\u00b7ten", "Sal\u00b7ben", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch wirstu immer zu nach einem Todten riechen.", "tokens": ["Doch", "wirs\u00b7tu", "im\u00b7mer", "zu", "nach", "ei\u00b7nem", "Tod\u00b7ten", "rie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}