{"dta.poem.8603": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der geldgierige Pfaffe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Der Pfaffe wei\u00df mit Dampf\u201a Gesang und Glocken\u201a", "tokens": ["Der", "Pfaf\u00b7fe", "wei\u00df", "mit", "Dampf", "\u201a", "Ge\u00b7sang", "und", "Glo\u00b7cken", "\u201a"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Mummerei, Geberd' und schlauem Segen", "tokens": ["Mit", "Mum\u00b7me\u00b7rei", ",", "Ge\u00b7berd'", "und", "schlau\u00b7em", "Se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den P\u00f6bel zum Guckkasten hinzulocken,", "tokens": ["Den", "P\u00f6\u00b7bel", "zum", "Guck\u00b7kas\u00b7ten", "hin\u00b7zu\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Worin sich H\u00f6ll' und Himmel bunt bewegen.", "tokens": ["Wo\u00b7rin", "sich", "H\u00f6ll'", "und", "Him\u00b7mel", "bunt", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Derweil, entz\u00fcckt, der P\u00f6bel, und erschrocken", "tokens": ["Der\u00b7weil", ",", "ent\u00b7z\u00fcckt", ",", "der", "P\u00f6\u00b7bel", ",", "und", "er\u00b7schro\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "VVPP", "$,", "ART", "NN", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ans Wunderloch nun thut das Auge legen,", "tokens": ["Ans", "Wun\u00b7der\u00b7loch", "nun", "thut", "das", "Au\u00b7ge", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Umschleichet ihn der Pfaffe, aus den Taschen", "tokens": ["Um\u00b7schlei\u00b7chet", "ihn", "der", "Pfaf\u00b7fe", ",", "aus", "den", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die schwei\u00dfgetr\u00e4nkten Kreuzer ihm zu haschen.", "tokens": ["Die", "schwei\u00df\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Kreu\u00b7zer", "ihm", "zu", "ha\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}