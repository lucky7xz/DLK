{"textgrid.poem.38880": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der edle Herzog gro\u00df", "tokens": ["Der", "ed\u00b7le", "Her\u00b7zog", "gro\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von dem Burgunder Lande", "tokens": ["Von", "dem", "Bur\u00b7gun\u00b7der", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Litt manchen Feindessto\u00df", "tokens": ["Litt", "man\u00b7chen", "Fein\u00b7des\u00b7sto\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wohl auf dem ebnen Sande.", "tokens": ["Wohl", "auf", "dem", "eb\u00b7nen", "San\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sprach: mich schl\u00e4gt der Feind,", "tokens": ["Er", "sprach", ":", "mich", "schl\u00e4gt", "der", "Feind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Muth ist mir entwichen,", "tokens": ["Mein", "Muth", "ist", "mir", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Freunde sind erblichen,", "tokens": ["Die", "Freun\u00b7de", "sind", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Knecht' geflohen seynd!", "tokens": ["Die", "Knecht'", "ge\u00b7flo\u00b7hen", "seynd", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich kann mich nicht mehr regen,", "tokens": ["Ich", "kann", "mich", "nicht", "mehr", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht Waffen f\u00fchren kann:", "tokens": ["Nicht", "Waf\u00b7fen", "f\u00fch\u00b7ren", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo bleibt der edle Degen,", "tokens": ["Wo", "bleibt", "der", "ed\u00b7le", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eckart der treue Mann?", "tokens": ["E\u00b7ckart", "der", "treu\u00b7e", "Mann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er war mir sonst zur Seite", "tokens": ["Er", "war", "mir", "sonst", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In jedem harten Strau\u00df,", "tokens": ["In", "je\u00b7dem", "har\u00b7ten", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch leider blieb er heute", "tokens": ["Doch", "lei\u00b7der", "blieb", "er", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Daheim bei sich zu Haus.", "tokens": ["Da\u00b7heim", "bei", "sich", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es mehren sich die Haufen,", "tokens": ["Es", "meh\u00b7ren", "sich", "die", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mu\u00df gefangen sein,", "tokens": ["Ich", "mu\u00df", "ge\u00b7fan\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag nicht wie Knecht entlaufen,", "tokens": ["Mag", "nicht", "wie", "Knecht", "ent\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "KOKOM", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drum will sterben fein! \u2013", "tokens": ["Drum", "will", "ster\u00b7ben", "fein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VMFIN", "VVINF", "ADJD", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "So klagt der von Burgund,", "tokens": ["So", "klagt", "der", "von", "Bur\u00b7gund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Will sein Schwerdt in sich stechen:", "tokens": ["Will", "sein", "Schwerdt", "in", "sich", "ste\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da kommt zur selben Stund", "tokens": ["Da", "kommt", "zur", "sel\u00b7ben", "Stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Eckart, den Feind zu brechen.", "tokens": ["E\u00b7ckart", ",", "den", "Feind", "zu", "bre\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Geharnischt reit't der Degen", "tokens": ["Ge\u00b7har\u00b7nischt", "reit't", "der", "De\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Keck in den Feind hinein,", "tokens": ["Keck", "in", "den", "Feind", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ihm folgt die Schaar verwegen", "tokens": ["Ihm", "folgt", "die", "Schaar", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und auch der Sohne sein.", "tokens": ["Und", "auch", "der", "Soh\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Burgund erkennt die Zeichen,", "tokens": ["Bur\u00b7gund", "er\u00b7kennt", "die", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ruft: Gott sei gelobt!", "tokens": ["Und", "ruft", ":", "Gott", "sei", "ge\u00b7lobt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Feinde mu\u00dften weichen", "tokens": ["Die", "Fein\u00b7de", "mu\u00df\u00b7ten", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die w\u00fcthend erst getobt.", "tokens": ["Die", "w\u00fct\u00b7hend", "erst", "ge\u00b7tobt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da schlug mit treuem Muthe", "tokens": ["Da", "schlug", "mit", "treu\u00b7em", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eckart ins Volk hinein,", "tokens": ["E\u00b7ckart", "ins", "Volk", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch schwamm im rothen Blute", "tokens": ["Doch", "schwamm", "im", "ro\u00b7then", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein zartes S\u00f6hnelein.", "tokens": ["Sein", "zar\u00b7tes", "S\u00f6h\u00b7nel\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Als nun der Feind bezwungen,", "tokens": ["Als", "nun", "der", "Feind", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sprach der Herzog laut:", "tokens": ["Da", "sprach", "der", "Her\u00b7zog", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist dir wohl gelungen,", "tokens": ["Es", "ist", "dir", "wohl", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch so, da\u00df es mir graut;", "tokens": ["Doch", "so", ",", "da\u00df", "es", "mir", "graut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du hast viel Mann geworben,", "tokens": ["Du", "hast", "viel", "Mann", "ge\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu retten Reich und Leben", "tokens": ["Zu", "ret\u00b7ten", "Reich", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein S\u00f6hnlein liegt erstorben,", "tokens": ["Dein", "S\u00f6hn\u00b7lein", "liegt", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kanns's dir nicht wieder geben. \u2013", "tokens": ["Kanns's", "dir", "nicht", "wie\u00b7der", "ge\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Eckart weinet fast,", "tokens": ["Der", "E\u00b7ckart", "wei\u00b7net", "fast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "B\u00fcckt sich der starke Held,", "tokens": ["B\u00fcckt", "sich", "der", "star\u00b7ke", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und nimmt die theure Last,", "tokens": ["Und", "nimmt", "die", "theu\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den Sohn in Armen h\u00e4lt.", "tokens": ["Den", "Sohn", "in", "Ar\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wie starbst du, Heinz, so fr\u00fche,", "tokens": ["Wie", "starbst", "du", ",", "Heinz", ",", "so", "fr\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NE", "$,", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und warst noch kaum ein Mann?", "tokens": ["Und", "warst", "noch", "kaum", "ein", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich reut nicht meine M\u00fche,", "tokens": ["Mich", "reut", "nicht", "mei\u00b7ne", "M\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich seh' dich gerne an,", "tokens": ["Ich", "seh'", "dich", "ger\u00b7ne", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Weil wir dich, F\u00fcrst, erl\u00f6sten,", "tokens": ["Weil", "wir", "dich", ",", "F\u00fcrst", ",", "er\u00b7l\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus deiner Feinde Hohn,", "tokens": ["Aus", "dei\u00b7ner", "Fein\u00b7de", "Hohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und drum will ich mich tr\u00f6sten,", "tokens": ["Und", "drum", "will", "ich", "mich", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich schenke dir den Sohn.", "tokens": ["Ich", "schen\u00b7ke", "dir", "den", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Da ward dem Burgund tr\u00fcbe", "tokens": ["Da", "ward", "dem", "Bur\u00b7gund", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor seiner Augen Licht,", "tokens": ["Vor", "sei\u00b7ner", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil diese gro\u00dfe Liebe", "tokens": ["Weil", "die\u00b7se", "gro\u00b7\u00dfe", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein edles Herze bricht.", "tokens": ["Sein", "ed\u00b7les", "Her\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er weint die hellen Z\u00e4hren", "tokens": ["Er", "weint", "die", "hel\u00b7len", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und f\u00e4llt ihm an die Brust:", "tokens": ["Und", "f\u00e4llt", "ihm", "an", "die", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich, Held, mu\u00df ich verehren,", "tokens": ["Dich", ",", "Held", ",", "mu\u00df", "ich", "ver\u00b7eh\u00b7ren", ","], "token_info": ["w