{"dta.poem.18103": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "An seine Buhlschaft.  \n Auf die Weise: Angelica die Edle.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Asterie du Edle Sch\u00e4ferin", "tokens": ["As\u00b7te\u00b7rie", "du", "Ed\u00b7le", "Sch\u00e4\u00b7fe\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Werd ich dich sehen schier?", "tokens": ["Werd", "ich", "dich", "se\u00b7hen", "schier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In deiner Huld ich gantz verschlossen bin,", "tokens": ["In", "dei\u00b7ner", "Huld", "ich", "gantz", "ver\u00b7schlos\u00b7sen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und lebe weit von dir.", "tokens": ["Und", "le\u00b7be", "weit", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur bey den wilden Thieren,", "tokens": ["Nur", "bey", "den", "wil\u00b7den", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und in dem w\u00fcsten Wald,", "tokens": ["Und", "in", "dem", "w\u00fcs\u00b7ten", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df ich mein Leben f\u00fchren,", "tokens": ["Mu\u00df", "ich", "mein", "Le\u00b7ben", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist mein Ausenthalt.", "tokens": ["Das", "ist", "mein", "Au\u00b7sent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Kein sch\u00f6ner Baum, kein zartes Bl\u00fcmelein,", "tokens": ["Kein", "sch\u00f6\u00b7ner", "Baum", ",", "kein", "zar\u00b7tes", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kein Ort mich tr\u00f6sten mag,", "tokens": ["Kein", "Ort", "mich", "tr\u00f6s\u00b7ten", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Kein kalter Brunn mit springender Fontein,", "tokens": ["Kein", "kal\u00b7ter", "Brunn", "mit", "sprin\u00b7gen\u00b7der", "Fon\u00b7tein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Erleschet meine Plag,", "tokens": ["Er\u00b7le\u00b7schet", "mei\u00b7ne", "Plag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Mein Augen auch wie Brunnen", "tokens": ["Mein", "Au\u00b7gen", "auch", "wie", "Brun\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sind gantz von Thr\u00e4nen na\u00df,", "tokens": ["Sind", "gantz", "von", "Thr\u00e4\u00b7nen", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Auch fast gar ausgerunnen", "tokens": ["Auch", "fast", "gar", "aus\u00b7ge\u00b7run\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Durch Weinen ohne Maa\u00df.", "tokens": ["Durch", "Wei\u00b7nen", "oh\u00b7ne", "Maa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Kein Rath noch H\u00fclff ohn dich mein Hertz erfreut,", "tokens": ["Kein", "Rath", "noch", "H\u00fclff", "ohn", "dich", "mein", "Hertz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Kein edler Lauten-Klang,", "tokens": ["Kein", "ed\u00b7ler", "Lau\u00b7ten\u00b7Klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Kein gr\u00fcner Platz erquicket mich in Leyd,", "tokens": ["Kein", "gr\u00fc\u00b7ner", "Platz", "er\u00b7quic\u00b7ket", "mich", "in", "Leyd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Kein lieblicher Gesang,", "tokens": ["Kein", "lieb\u00b7li\u00b7cher", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Voll Zittern, Furcht und Zagen", "tokens": ["Voll", "Zit\u00b7tern", ",", "Furcht", "und", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Ist mir die gantze Welt,", "tokens": ["Ist", "mir", "die", "gant\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Nur trauren, seufftzen, Klagen", "tokens": ["Nur", "trau\u00b7ren", ",", "seufft\u00b7zen", ",", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVINF", "$,", "VVINF", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Alleine mir gef\u00e4llt.", "tokens": ["Al\u00b7lei\u00b7ne", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Ach komm, ach komm du sehr gew\u00fcnschter Tag,", "tokens": ["Ach", "komm", ",", "ach", "komm", "du", "sehr", "ge\u00b7w\u00fcnschter", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "ITJ", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Jhr Stunden eilet fort,", "tokens": ["Ihr", "Stun\u00b7den", "ei\u00b7let", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Da\u00df ich doch bald mit Freuden kommen mag", "tokens": ["Da\u00df", "ich", "doch", "bald", "mit", "Freu\u00b7den", "kom\u00b7men", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Zu meines Lebens Hort!", "tokens": ["Zu", "mei\u00b7nes", "Le\u00b7bens", "Hort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "La\u00df Eolus die Winde", "tokens": ["La\u00df", "E\u00b7o\u00b7lus", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "NE", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Mich f\u00fchren von dem Land,", "tokens": ["Mich", "f\u00fch\u00b7ren", "von", "dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Neptunus gieb geschwinde", "tokens": ["Nep\u00b7tu\u00b7nus", "gieb", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVIMP", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Mich in der Liebsten Hand!", "tokens": ["Mich", "in", "der", "Liebs\u00b7ten", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Gehabt euch wol ihr Nymphen in der Heyd!", "tokens": ["Ge\u00b7habt", "euch", "wol", "ihr", "Nym\u00b7phen", "in", "der", "Heyd", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "O Pan ich mu\u00df von dir!", "tokens": ["O", "Pan", "ich", "mu\u00df", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "VMFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Gehabt euch wohl, mein Schiff ist schon bereit,", "tokens": ["Ge\u00b7habt", "euch", "wohl", ",", "mein", "Schiff", "ist", "schon", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Das mich von hinnen f\u00fchr,", "tokens": ["Das", "mich", "von", "hin\u00b7nen", "f\u00fchr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Adie ich will verlassen", "tokens": ["A\u00b7die", "ich", "will", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Der Weisheit Lob und Ehr,", "tokens": ["Der", "Weis\u00b7heit", "Lob", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Minerva mag mich hassen,", "tokens": ["Mi\u00b7ner\u00b7va", "mag", "mich", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.40": {"text": "Mein Augentrost ist mehr.", "tokens": ["Mein", "Au\u00b7gen\u00b7trost", "ist", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}