{"textgrid.poem.62011": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[es ist mir so federleicht um's Herz]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mir so federleicht um's Herz,", "tokens": ["Es", "ist", "mir", "so", "fe\u00b7der\u00b7leicht", "um's", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Versunken ist der wilde Schmerz,", "tokens": ["Ver\u00b7sun\u00b7ken", "ist", "der", "wil\u00b7de", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn's mir so im Innern gl\u00fcht,", "tokens": ["Und", "wenn's", "mir", "so", "im", "In\u00b7nern", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sing' ich euch bald ein neues Lied:", "tokens": ["Sing'", "ich", "euch", "bald", "ein", "neu\u00b7es", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein blaues Aug', ein goldner Stern,", "tokens": ["Ein", "blau\u00b7es", "Aug'", ",", "ein", "gold\u00b7ner", "Stern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein rotes W\u00f6lkchen himmelsfern,", "tokens": ["Ein", "ro\u00b7tes", "W\u00f6lk\u00b7chen", "him\u00b7mels\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Freundes Herz, ein treuer Blick,", "tokens": ["Ein", "Freun\u00b7des", "Herz", ",", "ein", "treu\u00b7er", "Blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein menschlich r\u00fchmliches Geschick.", "tokens": ["Ein", "menschlich", "r\u00fchm\u00b7li\u00b7ches", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Welle steigt, die Welle sinkt,", "tokens": ["Die", "Wel\u00b7le", "steigt", ",", "die", "Wel\u00b7le", "sinkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein brauner Nachen freundlich winkt,", "tokens": ["Ein", "brau\u00b7ner", "Na\u00b7chen", "freund\u00b7lich", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein frischer Ruderschlag ert\u00f6nt!", "tokens": ["Ein", "fri\u00b7scher", "Ru\u00b7der\u00b7schlag", "er\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie man sich dort nach S\u00e4ngern sehnt!", "tokens": ["Wie", "man", "sich", "dort", "nach", "S\u00e4n\u00b7gern", "sehnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ade, ade, Du gr\u00fcne Welt!", "tokens": ["A\u00b7de", ",", "a\u00b7de", ",", "Du", "gr\u00fc\u00b7ne", "Welt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Der S\u00e4nger ist der wahre Held,", "tokens": ["Der", "S\u00e4n\u00b7ger", "ist", "der", "wah\u00b7re", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Greift er in seine Saiten ein,", "tokens": ["Greift", "er", "in", "sei\u00b7ne", "Sai\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stimmt bald die ganze Seele ein!", "tokens": ["Stimmt", "bald", "die", "gan\u00b7ze", "See\u00b7le", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die ganze Welt, sie stimmt mit ein,", "tokens": ["Die", "gan\u00b7ze", "Welt", ",", "sie", "stimmt", "mit", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist sein, die Menschen sein,", "tokens": ["Die", "Welt", "ist", "sein", ",", "die", "Men\u00b7schen", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VAINF", "$,", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ade, ade, Du gr\u00fcne Welt,", "tokens": ["A\u00b7de", ",", "a\u00b7de", ",", "Du", "gr\u00fc\u00b7ne", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Der S\u00e4nger ist der wahre Held!", "tokens": ["Der", "S\u00e4n\u00b7ger", "ist", "der", "wah\u00b7re", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die ganze Welt, sie stimmt mit ein,", "tokens": ["Die", "gan\u00b7ze", "Welt", ",", "sie", "stimmt", "mit", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ist sein, die Menschen sein,", "tokens": ["Die", "Welt", "ist", "sein", ",", "die", "Men\u00b7schen", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VAINF", "$,", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ade, ade, Du gr\u00fcne Welt,", "tokens": ["A\u00b7de", ",", "a\u00b7de", ",", "Du", "gr\u00fc\u00b7ne", "Welt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4"