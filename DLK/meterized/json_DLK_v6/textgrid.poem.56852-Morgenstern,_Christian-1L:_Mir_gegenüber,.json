{"textgrid.poem.56852": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir gegen\u00fcber,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir gegen\u00fcber,", "tokens": ["Mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "dicht unterm Dach,", "tokens": ["dicht", "un\u00b7term", "Dach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "sitzt ein Weib", "tokens": ["sitzt", "ein", "Weib"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "am geduckten Fenster", "tokens": ["am", "ge\u00b7duck\u00b7ten", "Fens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "und n\u00e4ht.", "tokens": ["und", "n\u00e4ht", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Fr\u00fch", "tokens": ["Fr\u00fch"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "in das steigende Licht,", "tokens": ["in", "das", "stei\u00b7gen\u00b7de", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "sp\u00e4t", "tokens": ["sp\u00e4t"], "token_info": ["word"], "pos": ["ADJD"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "in die fallende Nacht.", "tokens": ["in", "die", "fal\u00b7len\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Manchmal", "tokens": ["Manch\u00b7mal"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "blickt es vom Scho\u00dfe auf", "tokens": ["blickt", "es", "vom", "Scho\u00b7\u00dfe", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "und verloren hinaus", "tokens": ["und", "ver\u00b7lo\u00b7ren", "hin\u00b7aus"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVPP", "APZR"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "auf die D\u00e4cher \u2013", "tokens": ["auf", "die", "D\u00e4\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "die Wolken \u2013", "tokens": ["die", "Wol\u00b7ken", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "die Ewigkeit.", "tokens": ["die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich kann", "tokens": ["Ich", "kann"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "sein Auge nicht sehn,", "tokens": ["sein", "Au\u00b7ge", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "aber ich ", "tokens": ["a\u00b7ber", "ich"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "ich blicke ihn mit,", "tokens": ["ich", "bli\u00b7cke", "ihn", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "den zehrenden Blick", "tokens": ["den", "zeh\u00b7ren\u00b7den", "Blick"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "auf die D\u00e4cher \u2013", "tokens": ["auf", "die", "D\u00e4\u00b7cher", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "die Wolken \u2013", "tokens": ["die", "Wol\u00b7ken", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "die Ewigkeit ...", "tokens": ["die", "E\u00b7wig\u00b7keit", "..."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}