{"textgrid.poem.49553": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Hymen und die Truppen Amors", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hymen stand im Hinterhalte:", "tokens": ["Hy\u00b7men", "stand", "im", "Hin\u00b7ter\u00b7hal\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein Heer von Amuretten", "tokens": ["Als", "ein", "Heer", "von", "A\u00b7mu\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seines Reiches Grenzen nahte!", "tokens": ["Sei\u00b7nes", "Rei\u00b7ches", "Gren\u00b7zen", "nah\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer da! rief er halberschrocken,", "tokens": ["Wer", "da", "!", "rief", "er", "hal\u00b7ber\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer da! \u2013 oder soll ich schiesen?", "tokens": ["Wer", "da", "!", "\u2013", "o\u00b7der", "soll", "ich", "schie\u00b7sen", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "$(", "KON", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Holder Bruder, sprach ihr F\u00fchrer,", "tokens": ["Hol\u00b7der", "Bru\u00b7der", ",", "sprach", "ihr", "F\u00fch\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcrchte nichts von Amors Truppen.", "tokens": ["F\u00fcrch\u00b7te", "nichts", "von", "A\u00b7mors", "Trup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unser Endzweck ist nicht dieser,", "tokens": ["Un\u00b7ser", "End\u00b7zweck", "ist", "nicht", "die\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deine Lande zu verheeren", "tokens": ["Dei\u00b7ne", "Lan\u00b7de", "zu", "ver\u00b7hee\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Oder in Besitz zu halten:", "tokens": ["O\u00b7der", "in", "Be\u00b7sitz", "zu", "hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wir verlangen nur den Durchzug!", "tokens": ["Wir", "ver\u00b7lan\u00b7gen", "nur", "den", "Durch\u00b7zug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}