{"textgrid.poem.43912": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Engel, nimm von mir so viel getreue Gr\u00fc\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Engel, nimm von mir so viel getreue Gr\u00fc\u00dfe", "tokens": ["Mein", "En\u00b7gel", ",", "nimm", "von", "mir", "so", "viel", "ge\u00b7treu\u00b7e", "Gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "APPR", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Tropfen mir anjezt aus Kiel und Augen gehn,", "tokens": ["Als", "Trop\u00b7fen", "mir", "an\u00b7jezt", "aus", "Kiel", "und", "Au\u00b7gen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "APPR", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Seufzer ich vor dich in diesen Brief verschlie\u00dfe,", "tokens": ["Als", "Seuf\u00b7zer", "ich", "vor", "dich", "in", "die\u00b7sen", "Brief", "ver\u00b7schlie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "PRF", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als Thr\u00e4nen dir vielleicht auf deinen Rosen stehn.", "tokens": ["Als", "Thr\u00e4\u00b7nen", "dir", "viel\u00b7leicht", "auf", "dei\u00b7nen", "Ro\u00b7sen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Erde schl\u00e4ft und ruht, ich aber wach und tr\u00e4ume,", "tokens": ["Die", "Er\u00b7de", "schl\u00e4ft", "und", "ruht", ",", "ich", "a\u00b7ber", "wach", "und", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil deine Liebe mich mit ofnen Augen wiegt.", "tokens": ["Weil", "dei\u00b7ne", "Lie\u00b7be", "mich", "mit", "of\u00b7nen", "Au\u00b7gen", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schreib und weis nicht was; du siehst es aus dem Reime,", "tokens": ["Ich", "schreib", "und", "weis", "nicht", "was", ";", "du", "siehst", "es", "aus", "dem", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PTKVZ", "PTKNEG", "PWS", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nun aus Schweidniz kommt und lahme F\u00fc\u00dfe kriegt.", "tokens": ["Der", "nun", "aus", "Schweid\u00b7niz", "kommt", "und", "lah\u00b7me", "F\u00fc\u00b7\u00dfe", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Joch der Einsamkeit schl\u00e4gt meinen Leib darnieder,", "tokens": ["Das", "Joch", "der", "Ein\u00b7sam\u00b7keit", "schl\u00e4gt", "mei\u00b7nen", "Leib", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Nacht und Finstern\u00fc\u00df die m\u00fcde Seite schleust,", "tokens": ["Dem", "Nacht", "und", "Fins\u00b7ter\u00b7n\u00fc\u00df", "die", "m\u00fc\u00b7de", "Sei\u00b7te", "schleust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ungewohnte Streu f\u00fchlt selbst den Schmerz der Glieder,", "tokens": ["Die", "un\u00b7ge\u00b7wohn\u00b7te", "Streu", "f\u00fchlt", "selbst", "den", "Schmerz", "der", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ein verborgnes Weh von ihrem Lager rei\u00dft.", "tokens": ["Die", "ein", "ver\u00b7borg\u00b7nes", "Weh", "von", "ih\u00b7rem", "La\u00b7ger", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Mangel deiner Brust best\u00fcrmet mein Gem\u00fcthe", "tokens": ["Der", "Man\u00b7gel", "dei\u00b7ner", "Brust", "be\u00b7st\u00fcr\u00b7met", "mein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist ein scharfes Schwerd, das durch die Seele dringt;", "tokens": ["Und", "ist", "ein", "schar\u00b7fes", "Schwerd", ",", "das", "durch", "die", "See\u00b7le", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gedenck ich deiner Treu, so wallt mir das Gebl\u00fcte", "tokens": ["Ge\u00b7denck", "ich", "dei\u00b7ner", "Treu", ",", "so", "wallt", "mir", "das", "Ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie ein zerflo\u00dfnes Erz, das mit dem Feuer ringt.", "tokens": ["Wie", "ein", "zer\u00b7flo\u00df\u00b7nes", "Erz", ",", "das", "mit", "dem", "Feu\u00b7er", "ringt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Bald schw\u00e4cht die Ungedult die Tapferkeit der Sinnen,", "tokens": ["Bald", "schw\u00e4cht", "die", "Un\u00b7ge\u00b7dult", "die", "Tap\u00b7fer\u00b7keit", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis der verwegne Mund auf den Geburthsstern flucht,", "tokens": ["Bis", "der", "ver\u00b7weg\u00b7ne", "Mund", "auf", "den", "Ge\u00b7burths\u00b7stern", "flucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald strafet die Vernunft mein th\u00f6richtes Beginnen,", "tokens": ["Bald", "stra\u00b7fet", "die", "Ver\u00b7nunft", "mein", "th\u00f6\u00b7rich\u00b7tes", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+---+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bald seh ich was von dir, das mich zu tr\u00f6sten sucht.", "tokens": ["Bald", "seh", "ich", "was", "von", "dir", ",", "das", "mich", "zu", "tr\u00f6s\u00b7ten", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "PPER", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ach wie vergn\u00fcgt mich nicht die Arbeit deiner H\u00e4nde,", "tokens": ["Ach", "wie", "ver\u00b7gn\u00fcgt", "mich", "nicht", "die", "Ar\u00b7beit", "dei\u00b7ner", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir in dem Horaz die Verse abgezehlt!", "tokens": ["Die", "mir", "in", "dem", "Ho\u00b7raz", "die", "Ver\u00b7se", "ab\u00b7ge\u00b7zehlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Sehnsucht schildert mir dein Bildn\u00fc\u00df an die W\u00e4nde,", "tokens": ["Die", "Sehn\u00b7sucht", "schil\u00b7dert", "mir", "dein", "Bild\u00b7n\u00fc\u00df", "an", "die", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem zu der \u00c4hnligkeit nichts als das Leben fehlt.", "tokens": ["Dem", "zu", "der", "\u00c4hn\u00b7lig\u00b7keit", "nichts", "als", "das", "Le\u00b7ben", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PIS", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Dies ist der Lebenslauf, den ich anjezo f\u00fchre.", "tokens": ["Dies", "ist", "der", "Le\u00b7bens\u00b7lauf", ",", "den", "ich", "an\u00b7je\u00b7zo", "f\u00fch\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer mag wohl, werthes Kind, dein Zeitvertreiber seyn?", "tokens": ["Wer", "mag", "wohl", ",", "wert\u00b7hes", "Kind", ",", "dein", "Zeit\u00b7ver\u00b7trei\u00b7ber", "seyn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel ich rathen kan und aus der Neigung sp\u00fcre,", "tokens": ["So", "viel", "ich", "ra\u00b7then", "kan", "und", "aus", "der", "Nei\u00b7gung", "sp\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVINF", "VMFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So stimmt vielleicht dein Mund zu meinen Klagen ein.", "tokens": ["So", "stimmt", "viel\u00b7leicht", "dein", "Mund", "zu", "mei\u00b7nen", "Kla\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Mich deucht, du schickst den Fu\u00df zur Wehmuth in die Kammer,", "tokens": ["Mich", "deucht", ",", "du", "schickst", "den", "Fu\u00df", "zur", "Weh\u00b7muth", "in", "die", "Kam\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In welcher unser Ku\u00df oft sichre Zuflucht fand,", "tokens": ["In", "wel\u00b7cher", "un\u00b7ser", "Ku\u00df", "oft", "sich\u00b7re", "Zu\u00b7flucht", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPOSAT", "NN", "ADV", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich deucht, du klagst bereits dem Fenster deinen Jammer,", "tokens": ["Mich", "deucht", ",", "du", "klagst", "be\u00b7reits", "dem", "Fens\u00b7ter", "dei\u00b7nen", "Jam\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey dem dein erstes Ja mich an dein Herze band.", "tokens": ["Bey", "dem", "dein", "ers\u00b7tes", "Ja", "mich", "an", "dein", "Her\u00b7ze", "band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wie aber? L\u00e4st dich auch die Tadelsucht zufrieden?", "tokens": ["Wie", "a\u00b7ber", "?", "L\u00e4st", "dich", "auch", "die", "Ta\u00b7del\u00b7sucht", "zu\u00b7frie\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ahnt mir allerdings: Ihr Stachel wird nicht ruhn,", "tokens": ["Es", "ahnt", "mir", "al\u00b7ler\u00b7dings", ":", "Ihr", "Sta\u00b7chel", "wird", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VAFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir, da des Himmels Hand uns ihr zur Lust geschieden,", "tokens": ["Dir", ",", "da", "des", "Him\u00b7mels", "Hand", "uns", "ihr", "zur", "Lust", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "ART", "NN", "NN", "PPER", "PPOSAT", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch das Verleumdungsgift der L\u00e4strer weh zu thun.", "tokens": ["Durch", "das", "Ver\u00b7leum\u00b7dungs\u00b7gift", "der", "L\u00e4st\u00b7rer", "weh", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Allein Gedult, mein Kind, befiehl nur Gott die Rache.", "tokens": ["Al\u00b7lein", "Ge\u00b7dult", ",", "mein", "Kind", ",", "be\u00b7fiehl", "nur", "Gott", "die", "Ra\u00b7che", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Thu Recht und scheue nichts. Wen das Gewi\u00dfen sch\u00fczt,", "tokens": ["Thu", "Recht", "und", "scheu\u00b7e", "nichts", ".", "Wen", "das", "Ge\u00b7wi\u00b7\u00dfen", "sch\u00fczt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "PIS", "$.", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der spricht der Misgunst Hohn und hat die beste Sache,", "tokens": ["Der", "spricht", "der", "Mis\u00b7gunst", "Hohn", "und", "hat", "die", "bes\u00b7te", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "KON", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.