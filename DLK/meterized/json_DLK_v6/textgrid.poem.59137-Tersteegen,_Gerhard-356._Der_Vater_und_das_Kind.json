{"textgrid.poem.59137": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "356. Der Vater und das Kind", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott sorgt f\u00fcr gro\u00df und kleine Sachen,", "tokens": ["Gott", "sorgt", "f\u00fcr", "gro\u00df", "und", "klei\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kindlein schl\u00e4ft im Scho\u00df mit Lachen", "tokens": ["Das", "Kin\u00b7dlein", "schl\u00e4ft", "im", "Scho\u00df", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4\u00dft, wie Gott will, alles gehen,", "tokens": ["Und", "l\u00e4\u00dft", ",", "wie", "Gott", "will", ",", "al\u00b7les", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "VMFIN", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es denkt: Der Vater wird's versehen.", "tokens": ["Es", "denkt", ":", "Der", "Va\u00b7ter", "wird's", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}