{"textgrid.poem.60372": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Heimatklang", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als sie vom Paradiese ward gezwungen,", "tokens": ["Als", "sie", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "ward", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam jeder Seele eine Melodie", "tokens": ["Kam", "je\u00b7der", "See\u00b7le", "ei\u00b7ne", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Lebewohl s\u00fc\u00df schmerzlich nachgeklungen,", "tokens": ["Zum", "Le\u00b7be\u00b7wohl", "s\u00fc\u00df", "schmerz\u00b7lich", "nach\u00b7ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Darauf umschlo\u00df die Erdenh\u00fclle sie.", "tokens": ["Da\u00b7rauf", "um\u00b7schlo\u00df", "die", "Er\u00b7den\u00b7h\u00fcl\u00b7le", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch ist dies Lied nicht v\u00f6llig uns verdrungen,", "tokens": ["Noch", "ist", "dies", "Lied", "nicht", "v\u00f6l\u00b7lig", "uns", "ver\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "PTKNEG", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch t\u00f6nt es leiser stets auf Erden hie.", "tokens": ["Doch", "t\u00f6nt", "es", "lei\u00b7ser", "stets", "auf", "Er\u00b7den", "hie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gib acht, o Herz, da\u00df in den Sch\u00fctterungen", "tokens": ["Gib", "acht", ",", "o", "Herz", ",", "da\u00df", "in", "den", "Sch\u00fct\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "CARD", "$,", "FM", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dir nicht des Liedes letzter Hauch entflieh!", "tokens": ["Dir", "nicht", "des", "Lie\u00b7des", "letz\u00b7ter", "Hauch", "ent\u00b7flieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein Nachhall dieses Liedes ist entsprungen", "tokens": ["Ein", "Nach\u00b7hall", "die\u00b7ses", "Lie\u00b7des", "ist", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Des Morgenlandes s\u00fc\u00dfe Poesie,", "tokens": ["Des", "Mor\u00b7gen\u00b7lan\u00b7des", "s\u00fc\u00b7\u00dfe", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Von Jugendtr\u00e4umen wirds manchmal gesungen,", "tokens": ["Von", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7men", "wirds", "manch\u00b7mal", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Doch dunkel, unbewu\u00dft woher? und wie?", "tokens": ["Doch", "dun\u00b7kel", ",", "un\u00b7be\u00b7wu\u00dft", "wo\u00b7her", "?", "und", "wie", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "PWAV", "$.", "KON", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wem aber einmal klar und voll geklungen", "tokens": ["Wem", "a\u00b7ber", "ein\u00b7mal", "klar", "und", "voll", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die wunderbare Heimatmelodie,", "tokens": ["Die", "wun\u00b7der\u00b7ba\u00b7re", "Hei\u00b7mat\u00b7me\u00b7lo\u00b7die", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Der wird von bangem Heimweh tief durchdrungen,", "tokens": ["Der", "wird", "von", "ban\u00b7gem", "Heim\u00b7weh", "tief", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und er genest von seiner Sehnsucht nie.", "tokens": ["Und", "er", "ge\u00b7nest", "von", "sei\u00b7ner", "Sehn\u00b7sucht", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}