{"textgrid.poem.66919": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von eins am r\u00f6tlichen Heidestrecken,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von eins am r\u00f6tlichen Heidestrecken,", "tokens": ["Von", "eins", "am", "r\u00f6t\u00b7li\u00b7chen", "Hei\u00b7de\u00b7stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo schweigend Hirt mit der Herde zieht,", "tokens": ["Wo", "schwei\u00b7gend", "Hirt", "mit", "der", "Her\u00b7de", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zur schwarzen Stadt, wo sich Schlote recken", "tokens": ["Zur", "schwar\u00b7zen", "Stadt", ",", "wo", "sich", "Schlo\u00b7te", "re\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PWAV", "PRF", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und H\u00e4mmer dr\u00f6hnen ihr eisern Lied ...", "tokens": ["Und", "H\u00e4m\u00b7mer", "dr\u00f6h\u00b7nen", "ihr", "ei\u00b7sern", "Lied", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von Meeresh\u00e4fen, mastenreichen,", "tokens": ["Von", "Mee\u00b7res\u00b7h\u00e4\u00b7fen", ",", "mas\u00b7ten\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drin Pfeifen heulen und Kr\u00e4ne knarrn,", "tokens": ["Drin", "Pfei\u00b7fen", "heu\u00b7len", "und", "Kr\u00e4\u00b7ne", "knarrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zu schattenspiegelnden Bergesteichen", "tokens": ["Zu", "schat\u00b7ten\u00b7spie\u00b7geln\u00b7den", "Ber\u00b7ge\u00b7stei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und m\u00e4rchenwebendem Hochwaldfarn \u2013", "tokens": ["Und", "m\u00e4r\u00b7chen\u00b7we\u00b7ben\u00b7dem", "Hoch\u00b7wald\u00b7farn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wo Menschenh\u00e4nde sich wirkend r\u00fchren,", "tokens": ["Wo", "Men\u00b7schen\u00b7h\u00e4n\u00b7de", "sich", "wir\u00b7kend", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wo die Seele der Heimat sinnt,", "tokens": ["Und", "wo", "die", "See\u00b7le", "der", "Hei\u00b7mat", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O, all deine Adern und Fasern sp\u00fcren", "tokens": ["O", ",", "all", "dei\u00b7ne", "A\u00b7dern", "und", "Fa\u00b7sern", "sp\u00fc\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PIAT", "PPOSAT", "NN", "KON", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Strom des Schicksals, der dich durchrinnt,", "tokens": ["Den", "Strom", "des", "Schick\u00b7sals", ",", "der", "dich", "durch\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Deutschland, Mutter der Schaffensfrohen,", "tokens": ["Deutschland", ",", "Mut\u00b7ter", "der", "Schaf\u00b7fens\u00b7fro\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wiege der geistesk\u00e4mpfenden Kraft,", "tokens": ["Wie\u00b7ge", "der", "geis\u00b7tes\u00b7k\u00e4mp\u00b7fen\u00b7den", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Das unter dem Herzen tr\u00e4gt Heroen", "tokens": ["Das", "un\u00b7ter", "dem", "Her\u00b7zen", "tr\u00e4gt", "He\u00b7roen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Heiliger Kunst und Wissenschaft!", "tokens": ["Hei\u00b7li\u00b7ger", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Erst zuckt' es durch all deine Fasern und Adern,", "tokens": ["Erst", "zuckt'", "es", "durch", "all", "dei\u00b7ne", "Fa\u00b7sern", "und", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als schl\u00fcge der Blitz in dein innerstes Sein.", "tokens": ["Als", "schl\u00fc\u00b7ge", "der", "Blitz", "in", "dein", "in\u00b7ners\u00b7tes", "Sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann ruckt' es zusammen zu menschlichen Quadern,", "tokens": ["Dann", "ruckt'", "es", "zu\u00b7sam\u00b7men", "zu", "menschli\u00b7chen", "Qua\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dein Fu\u00df wurde Fels, dein Haupt wurde Stein.", "tokens": ["Dein", "Fu\u00df", "wur\u00b7de", "Fels", ",", "dein", "Haupt", "wur\u00b7de", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es zerrten und rissen die Fluten der L\u00fcge,", "tokens": ["Es", "zerr\u00b7ten", "und", "ris\u00b7sen", "die", "Flu\u00b7ten", "der", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der Sturm der Zerst\u00f6rung an deinem Gebein \u2013", "tokens": ["Der", "Sturm", "der", "Zer\u00b7st\u00f6\u00b7rung", "an", "dei\u00b7nem", "Ge\u00b7bein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Du hieltest gewaltig in deinem Gef\u00fcge", "tokens": ["Du", "hiel\u00b7test", "ge\u00b7wal\u00b7tig", "in", "dei\u00b7nem", "Ge\u00b7f\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und botst der Verleumdung ein ehernes Nein.", "tokens": ["Und", "botst", "der", "Ver\u00b7leum\u00b7dung", "ein", "e\u00b7her\u00b7nes", "Nein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Festgeschlossen, in Erz gegossen,", "tokens": ["Fest\u00b7ge\u00b7schlos\u00b7sen", ",", "in", "Erz", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Deutschland, menschheitsgl\u00e4ubiges, steh!", "tokens": ["Deutschland", ",", "menschheits\u00b7gl\u00e4u\u00b7bi\u00b7ges", ",", "steh", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "$,", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Eins im Wehrbann der Werkgenossen,", "tokens": ["Eins", "im", "Wehr\u00b7bann", "der", "Werk\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Eins im Wahrheitsbund der Idee.", "tokens": ["Eins", "im", "Wahr\u00b7heits\u00b7bund", "der", "I\u00b7dee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines verb\u00fcndenden Geistes Mauer", "tokens": ["Dei\u00b7nes", "ver\u00b7b\u00fcn\u00b7den\u00b7den", "Geis\u00b7tes", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Sei der Gerechtigkeit sch\u00fctzendes Tor \u2013", "tokens": ["Sei", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", "sch\u00fct\u00b7zen\u00b7des", "Tor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Unter dir der Vernichtung Schauer,", "tokens": ["Un\u00b7ter", "dir", "der", "Ver\u00b7nich\u00b7tung", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wachse zur Wartburg der Freiheit empor!", "tokens": ["Wach\u00b7se", "zur", "Wart\u00b7burg", "der", "Frei\u00b7heit", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}