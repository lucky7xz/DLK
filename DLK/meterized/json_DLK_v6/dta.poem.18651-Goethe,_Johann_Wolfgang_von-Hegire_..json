{"dta.poem.18651": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hegire .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nord und West und S\u00fcd zersplittern,", "tokens": ["Nord", "und", "West", "und", "S\u00fcd", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Throne bersten, Reiche zittern,", "tokens": ["Thro\u00b7ne", "bers\u00b7ten", ",", "Rei\u00b7che", "zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fl\u00fcchte du, im reinen Osten", "tokens": ["Fl\u00fcch\u00b7te", "du", ",", "im", "rei\u00b7nen", "Os\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Patriarchenluft zu kosten,", "tokens": ["Pat\u00b7ri\u00b7ar\u00b7chen\u00b7luft", "zu", "kos\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Lieben, Trinken, Singen,", "tokens": ["Un\u00b7ter", "Lie\u00b7ben", ",", "Trin\u00b7ken", ",", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll dich Chisers Quell verj\u00fcngen.", "tokens": ["Soll", "dich", "Chi\u00b7sers", "Quell", "ver\u00b7j\u00fcn\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "NE", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort, im Reinen und im Rechten,", "tokens": ["Dort", ",", "im", "Rei\u00b7nen", "und", "im", "Rech\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich menschlichen Geschlechten", "tokens": ["Will", "ich", "menschli\u00b7chen", "Ge\u00b7schlech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "In des Ursprungs Tiefe dringen,", "tokens": ["In", "des", "Ur\u00b7sprungs", "Tie\u00b7fe", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sie noch von Gott empfingen", "tokens": ["Wo", "sie", "noch", "von", "Gott", "emp\u00b7fin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelslehr\u2019 in Erdesprachen,", "tokens": ["Him\u00b7mels\u00b7lehr'", "in", "Er\u00b7de\u00b7spra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sich nicht den Kopf zerbrachen.", "tokens": ["Und", "sich", "nicht", "den", "Kopf", "zer\u00b7bra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo sie V\u00e4ter hoch verehrten,", "tokens": ["Wo", "sie", "V\u00e4\u00b7ter", "hoch", "ver\u00b7ehr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeden fremden Dienst verwehrten;", "tokens": ["Je\u00b7den", "frem\u00b7den", "Dienst", "ver\u00b7wehr\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will mich freun der Jugendschranke:", "tokens": ["Will", "mich", "freun", "der", "Ju\u00b7gend\u00b7schran\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaube weit, eng der Gedanke,", "tokens": ["Glau\u00b7be", "weit", ",", "eng", "der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wie das Wort so wichtig dort war,", "tokens": ["Wie", "das", "Wort", "so", "wich\u00b7tig", "dort", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil es ein gesprochen Wort war.", "tokens": ["Weil", "es", "ein", "ge\u00b7spro\u00b7chen", "Wort", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Will mich unter Hirten mischen,", "tokens": ["Will", "mich", "un\u00b7ter", "Hir\u00b7ten", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An Oasen mich erfrischen,", "tokens": ["An", "O\u00b7a\u00b7sen", "mich", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn mit Caravanen wandle,", "tokens": ["Wenn", "mit", "Ca\u00b7ra\u00b7va\u00b7nen", "wand\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schawl, Caffee und Moschus handle.", "tokens": ["Schawl", ",", "Caf\u00b7fee", "und", "Mo\u00b7schus", "hand\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NE", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Jeden Pfad will ich betreten", "tokens": ["Je\u00b7den", "Pfad", "will", "ich", "be\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der W\u00fcste zu den St\u00e4dten.", "tokens": ["Von", "der", "W\u00fcs\u00b7te", "zu", "den", "St\u00e4d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "B\u00f6sen Felsweg auf und nieder", "tokens": ["B\u00f6\u00b7sen", "Fels\u00b7weg", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00f6sten Hafis deine Lieder,", "tokens": ["Tr\u00f6s\u00b7ten", "Ha\u00b7fis", "dei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn der F\u00fchrer mit Entz\u00fccken,", "tokens": ["Wenn", "der", "F\u00fch\u00b7rer", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von des Maulthiers hohem R\u00fccken,", "tokens": ["Von", "des", "Mault\u00b7hiers", "ho\u00b7hem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Singt, die Sterne zu erwecken,", "tokens": ["Singt", ",", "die", "Ster\u00b7ne", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die R\u00e4uber zu erschrecken.", "tokens": ["Und", "die", "R\u00e4u\u00b7ber", "zu", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Will in B\u00e4dern und in Schenken", "tokens": ["Will", "in", "B\u00e4\u00b7dern", "und", "in", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil\u2019ger Hafis dein gedenken,", "tokens": ["Heil'\u00b7ger", "Ha\u00b7fis", "dein", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn den Schleyer Liebchen l\u00fcftet,", "tokens": ["Wenn", "den", "Schle\u00b7yer", "Lieb\u00b7chen", "l\u00fcf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttlend Ambralocken d\u00fcftet.", "tokens": ["Sch\u00fctt\u00b7lend", "A\u00b7mbra\u00b7lo\u00b7cken", "d\u00fcf\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja des Dichters Liebefl\u00fcstern", "tokens": ["Ja", "des", "Dich\u00b7ters", "Lie\u00b7be\u00b7fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mache selbst die Huris l\u00fcstern.", "tokens": ["Ma\u00b7che", "selbst", "die", "Hu\u00b7ris", "l\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wolltet ihr ihm dies beneiden,", "tokens": ["Woll\u00b7tet", "ihr", "ihm", "dies", "be\u00b7nei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder etwa gar verleiden;", "tokens": ["O\u00b7der", "et\u00b7wa", "gar", "ver\u00b7lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wisset nur, dass Dichterworte", "tokens": ["Wis\u00b7set", "nur", ",", "dass", "Dich\u00b7ter\u00b7wor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um des Paradieses Pforte", "tokens": ["Um", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer leise klopfend schweben,", "tokens": ["Im\u00b7mer", "lei\u00b7se", "klop\u00b7fend", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich erbittend ew\u2019ges Leben.", "tokens": ["Sich", "er\u00b7bit\u00b7tend", "ew'\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}