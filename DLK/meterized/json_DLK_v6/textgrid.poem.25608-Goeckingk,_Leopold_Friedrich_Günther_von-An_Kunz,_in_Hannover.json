{"textgrid.poem.25608": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "An Kunz, in Hannover", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ahmest l\u00e4cherlich in Kleidung und in Mienen", "tokens": ["Du", "ah\u00b7mest", "l\u00e4\u00b7cher\u00b7lich", "in", "Klei\u00b7dung", "und", "in", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Britten nach, und forschest nah und fern,", "tokens": ["Den", "Brit\u00b7ten", "nach", ",", "und", "for\u00b7schest", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was englisch sey! Ich kann mit etwas dienen:", "tokens": ["Was", "eng\u00b7lisch", "sey", "!", "Ich", "kann", "mit", "et\u00b7was", "die\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$.", "PPER", "VMFIN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Britt' entleibt sich gern.", "tokens": ["Der", "Britt'", "ent\u00b7leibt", "sich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}