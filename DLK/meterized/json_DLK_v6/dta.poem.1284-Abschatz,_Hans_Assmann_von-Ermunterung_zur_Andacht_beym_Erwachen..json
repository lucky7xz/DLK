{"dta.poem.1284": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ermunterung zur Andacht beym  \n Erwachen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "D Seele/ werde wach vom Schlaffe deiner S\u00fcnden/", "tokens": ["D", "See\u00b7le", "/", "wer\u00b7de", "wach", "vom", "Schlaf\u00b7fe", "dei\u00b7ner", "S\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "VAFIN", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dich in eine Nacht ohn Ende st\u00fcrtzen kan.", "tokens": ["Der", "dich", "in", "ei\u00b7ne", "Nacht", "ohn", "En\u00b7de", "st\u00fcrt\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm keinen falschen Schein zu deinem F\u00fchrer an/", "tokens": ["Nimm", "kei\u00b7nen", "fal\u00b7schen", "Schein", "zu", "dei\u00b7nem", "F\u00fch\u00b7rer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und m\u00fche dich das Licht der Ewigkeit zu finden.", "tokens": ["Und", "m\u00fc\u00b7he", "dich", "das", "Licht", "der", "E\u00b7wig\u00b7keit", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geh nicht den breiten Weg der weltgesinnten Blinden/", "tokens": ["Geh", "nicht", "den", "brei\u00b7ten", "Weg", "der", "welt\u00b7ge\u00b7sinn\u00b7ten", "Blin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die schn\u00f6der Ehre Blitz/ und falscher Freude Wahn", "tokens": ["Die", "schn\u00f6\u00b7der", "Eh\u00b7re", "Blitz", "/", "und", "fal\u00b7scher", "Freu\u00b7de", "Wahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vom Tugend-Steige f\u00fchrt auff glatter Wollust Bahn/", "tokens": ["Vom", "Tu\u00b7gen\u00b7dStei\u00b7ge", "f\u00fchrt", "auff", "glat\u00b7ter", "Wol\u00b7lust", "Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Liebe/ Lust und Schertz die Augen pflegt zu binden.", "tokens": ["Den", "Lie\u00b7be", "/", "Lust", "und", "Schertz", "die", "Au\u00b7gen", "pflegt", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erhebe dich im Geist bey das gestirnte Hau\u00df/", "tokens": ["Er\u00b7he\u00b7be", "dich", "im", "Geist", "bey", "das", "ge\u00b7stirn\u00b7te", "Hau\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00df Schatten/ Nebel/ Nacht seyn unter deinen F\u00fcssen/", "tokens": ["La\u00df", "Schat\u00b7ten", "/", "Ne\u00b7bel", "/", "Nacht", "seyn", "un\u00b7ter", "dei\u00b7nen", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "NN", "$(", "NN", "VAINF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und kl\u00e4re deinen Sinn mit reiner Andacht aus.", "tokens": ["Und", "kl\u00e4\u00b7re", "dei\u00b7nen", "Sinn", "mit", "rei\u00b7ner", "An\u00b7dacht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird das Licht/ dem Licht und Sonne folgen m\u00fcssen/", "tokens": ["So", "wird", "das", "Licht", "/", "dem", "Licht", "und", "Son\u00b7ne", "fol\u00b7gen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit Strahlen seiner Gnad erleuchten deine Sinnen/", "tokens": ["Mit", "Strah\u00b7len", "sei\u00b7ner", "Gnad", "er\u00b7leuch\u00b7ten", "dei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df du durch finstres Thal wirst sicher wandeln k\u00fcnnen.", "tokens": ["Da\u00df", "du", "durch", "finst\u00b7res", "Thal", "wirst", "si\u00b7cher", "wan\u00b7deln", "k\u00fcn\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}