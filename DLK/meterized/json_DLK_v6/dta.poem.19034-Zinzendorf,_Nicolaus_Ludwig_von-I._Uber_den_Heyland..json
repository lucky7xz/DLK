{"dta.poem.19034": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "I.   Uber den Heyland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Wohlan der Bundes-GOtt hat sich recht treu erzeigt, ", "tokens": ["Wo\u00b7hlan", "der", "Bun\u00b7des\u00b7GOtt", "hat", "sich", "recht", "treu", "er\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PRF", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir wissen was er j\u00fcngst vor neue Wunder \u00fcbte,", "tokens": ["Wir", "wis\u00b7sen", "was", "er", "j\u00fcngst", "vor", "neu\u00b7e", "Wun\u00b7der", "\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie hoch hat er erh\u00f6ht die er so tieff gebeugt,", "tokens": ["Wie", "hoch", "hat", "er", "er\u00b7h\u00f6ht", "die", "er", "so", "tieff", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie hat er die erfreut, die er zuvor betr\u00fcbte.", "tokens": ["Wie", "hat", "er", "die", "er\u00b7freut", ",", "die", "er", "zu\u00b7vor", "be\u00b7tr\u00fcb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geliebte! da Euch GOtt den sch\u00f6nen Vorzug g\u00f6nnt,", "tokens": ["Ge\u00b7lieb\u00b7te", "!", "da", "Euch", "Gott", "den", "sch\u00f6\u00b7nen", "Vor\u00b7zug", "g\u00f6nnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df Jhr in seiner Furcht die Sache angefangen,", "tokens": ["Da\u00df", "Ihr", "in", "sei\u00b7ner", "Furcht", "die", "Sa\u00b7che", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df Euch sein Gnaden-Ruff zur heilgen Eh ernennt,", "tokens": ["Da\u00df", "Euch", "sein", "Gna\u00b7den\u00b7Ruff", "zur", "heil\u00b7gen", "Eh", "er\u00b7nennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So suchet ewiglich in diesem Schmuck zu prangen.", "tokens": ["So", "su\u00b7chet", "e\u00b7wig\u00b7lich", "in", "die\u00b7sem", "Schmuck", "zu", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum nehmet Euch doch nichts in eurem Leben f\u00fcr,", "tokens": ["Drum", "neh\u00b7met", "Euch", "doch", "nichts", "in", "eu\u00b7rem", "Le\u00b7ben", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PIS", "APPR", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dabey der treue HErr nicht Rath und That gewesen:", "tokens": ["Da\u00b7bey", "der", "treu\u00b7e", "Herr", "nicht", "Rath", "und", "That", "ge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PTKNEG", "NN", "KON", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der HErr er\u00f6ffne Euch des Gl\u00fccks und Creutzes Th\u00fcr,", "tokens": ["Der", "Herr", "er\u00b7\u00f6ff\u00b7ne", "Euch", "des", "Gl\u00fccks", "und", "Creut\u00b7zes", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er leg Euch Kranckheit auf, er lasse euch genesen.", "tokens": ["Er", "leg", "Euch", "Kran\u00b7ck\u00b7heit", "auf", ",", "er", "las\u00b7se", "euch", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Er selber mach es Euch ietzt leicht, und morgen schwer,", "tokens": ["Er", "sel\u00b7ber", "mach", "es", "Euch", "ietzt", "leicht", ",", "und", "mor\u00b7gen", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So, wie ", "tokens": ["So", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Was Euch begegnen kan, das alles schaffe ", "tokens": ["Was", "Euch", "be\u00b7geg\u00b7nen", "kan", ",", "das", "al\u00b7les", "schaf\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "PRELS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Damit Jhr Lieb und Leid von Jhm zu fordern wisset.", "tokens": ["Da\u00b7mit", "Ihr", "Lieb", "und", "Leid", "von", "Jhm", "zu", "for\u00b7dern", "wis\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "KON", "NN", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er sey der erste Grund, der Mittel-Punct, und Schlu\u00df", "tokens": ["Er", "sey", "der", "ers\u00b7te", "Grund", ",", "der", "Mit\u00b7tel\u00b7Punct", ",", "und", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von allem Euren Thun: sein Stecken m\u00fc\u00df Euch weyden:", "tokens": ["Von", "al\u00b7lem", "Eu\u00b7ren", "Thun", ":", "sein", "Ste\u00b7cken", "m\u00fc\u00df", "Euch", "wey\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er zuckre jeden Kampff mit seiner Gunst Genu\u00df,", "tokens": ["Er", "zuck\u00b7re", "je\u00b7den", "Kampff", "mit", "sei\u00b7ner", "Gunst", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und woll Euch allen Trost der Creatur verleiden.", "tokens": ["Und", "woll", "Euch", "al\u00b7len", "Trost", "der", "Crea\u00b7tur", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Wie s\u00fcsse schmeckt das Brod, das ", "tokens": ["Wie", "s\u00fcs\u00b7se", "schmeckt", "das", "Brod", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Wie labet uns der Tranck, den ", "tokens": ["Wie", "la\u00b7bet", "uns", "der", "Tranck", ",", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Wohl einer jeden Seel, die ", "tokens": ["Wohl", "ei\u00b7ner", "je\u00b7den", "Seel", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "PIAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Mein Bruder, deine Ruh wird meinen Geist erfreun,", "tokens": ["Mein", "Bru\u00b7der", ",", "dei\u00b7ne", "Ruh", "wird", "mei\u00b7nen", "Geist", "er\u00b7freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und die Zufriedenheit der werthen ", "tokens": ["Und", "die", "Zu\u00b7frie\u00b7den\u00b7heit", "der", "wert\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wird mir ein Gnaden-Lohn von GOttes G\u00fcte seyn,", "tokens": ["Wird", "mir", "ein", "Gna\u00b7den\u00b7Lohn", "von", "Got\u00b7tes", "G\u00fc\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Euer Eheband im W\u00e4chter-Rath erkohren,", "tokens": ["Die", "Eu\u00b7er", "E\u00b7he\u00b7band", "im", "W\u00e4ch\u00b7ter\u00b7Rath", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Erinnert Euch nur offt, was von ", "tokens": ["E\u00b7rin\u00b7nert", "Euch", "nur", "offt", ",", "was", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Was von der ", "tokens": ["Was", "von", "der"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.31": {"text": "Und von ", "tokens": ["Und", "von"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Auf Reminiscere Das war der Verl\u00f6bni\u00df-Tag, und jenes der Inhalt damaliger\nReden. ist ansgesprochen worden.", "tokens": ["Auf", "Re\u00b7mi\u00b7nis\u00b7ce\u00b7re", "Das", "war", "der", "Ver\u00b7l\u00f6b\u00b7ni\u00df\u00b7Tag", ",", "und", "je\u00b7nes", "der", "In\u00b7halt", "da\u00b7ma\u00b7li\u00b7ger", "Re\u00b7den", ".", "ist", "ans\u00b7ge\u00b7spro\u00b7chen", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDS", "VAFIN", "ART", "NN", "$,", "KON", "PDS", "ART", "NN", "ADJA", "NN", "$.", "VAFIN", "VVPP", "VAPP", "$."], "meter": "-+--+--+--+-+-+--++-+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.33": {"text": "Herr h\u00f6re mein Gebet, und siehe gn\u00e4dig an", "tokens": ["Herr", "h\u00f6\u00b7re", "mein", "Ge\u00b7bet", ",", "und", "sie\u00b7he", "gn\u00e4\u00b7dig", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVIMP", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Kinder, die Jhr Gl\u00fcck in deiner Hand gefunden:", "tokens": ["Die", "Kin\u00b7der", ",", "die", "Ihr", "Gl\u00fcck", "in", "dei\u00b7ner", "Hand", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Brich Jhnen \u00fcberall die sch\u00f6ne ", "tokens": ["Brich", "Jh\u00b7nen", "\u00fc\u00b7be\u00b7rall", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Verm\u00e4hle Sie mit Dir, so sind sie fest verbunden.", "tokens": ["Ver\u00b7m\u00e4h\u00b7le", "Sie", "mit", "Dir", ",", "so", "sind", "sie", "fest", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}