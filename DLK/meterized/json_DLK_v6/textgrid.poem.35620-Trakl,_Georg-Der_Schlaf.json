{"textgrid.poem.35620": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Schlaf", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verflucht ihr dunklen Gifte,", "tokens": ["Ver\u00b7flucht", "ihr", "dunk\u00b7len", "Gif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00dfer Schlaf!", "tokens": ["Wei\u00b7\u00dfer", "Schlaf", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dieser h\u00f6chst seltsame Garten", "tokens": ["Die\u00b7ser", "h\u00f6chst", "selt\u00b7sa\u00b7me", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00e4mmernder B\u00e4ume", "tokens": ["D\u00e4m\u00b7mern\u00b7der", "B\u00e4u\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Erf\u00fcllt von Schlangen, Nachtfaltern,", "tokens": ["Er\u00b7f\u00fcllt", "von", "Schlan\u00b7gen", ",", "Nacht\u00b7fal\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Spinnen, Flederm\u00e4usen.", "tokens": ["Spin\u00b7nen", ",", "Fle\u00b7der\u00b7m\u00e4u\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Fremdling! Dein verlorner Schatten", "tokens": ["Fremd\u00b7ling", "!", "Dein", "ver\u00b7lor\u00b7ner", "Schat\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Im Abendrot,", "tokens": ["Im", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ein finsterer Korsar", "tokens": ["Ein", "fins\u00b7te\u00b7rer", "Kor\u00b7sar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Im salzigen Meer der Tr\u00fcbsal.", "tokens": ["Im", "sal\u00b7zi\u00b7gen", "Meer", "der", "Tr\u00fcb\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Aufflattern wei\u00dfe V\u00f6gel am Nachtsaum", "tokens": ["Auf\u00b7flat\u00b7tern", "wei\u00b7\u00dfe", "V\u00f6\u00b7gel", "am", "Nacht\u00b7saum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "\u00dcber st\u00fcrzenden St\u00e4dten", "tokens": ["\u00dc\u00b7ber", "st\u00fcr\u00b7zen\u00b7den", "St\u00e4d\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Von Stahl.", "tokens": ["Von", "Stahl", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}