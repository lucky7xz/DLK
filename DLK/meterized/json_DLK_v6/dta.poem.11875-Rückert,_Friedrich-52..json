{"dta.poem.11875": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gew\u00f6hne Sch\u00f6nes dich zu sinnen und zu denken,", "tokens": ["Ge\u00b7w\u00f6h\u00b7ne", "Sch\u00f6\u00b7nes", "dich", "zu", "sin\u00b7nen", "und", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lerne jeden Sinn aufs Sch\u00f6ne hinzulenken,", "tokens": ["Und", "ler\u00b7ne", "je\u00b7den", "Sinn", "aufs", "Sch\u00f6\u00b7ne", "hin\u00b7zu\u00b7len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und strebe jeden Sinn ins Sch\u00f6ne einzusenken,", "tokens": ["Und", "stre\u00b7be", "je\u00b7den", "Sinn", "ins", "Sch\u00f6\u00b7ne", "ein\u00b7zu\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Sch\u00f6nes m\u00f6ge hold dir jede Stunde schenken,", "tokens": ["Und", "Sch\u00f6\u00b7nes", "m\u00f6\u00b7ge", "hold", "dir", "je\u00b7de", "Stun\u00b7de", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ADJD", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In Sch\u00f6nes h\u00fcllen dich, dein Herz mit Sch\u00f6nem tr\u00e4nken,", "tokens": ["In", "Sch\u00f6\u00b7nes", "h\u00fcl\u00b7len", "dich", ",", "dein", "Herz", "mit", "Sch\u00f6\u00b7nem", "tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit dem Anblick nie dich des Unsch\u00f6nen kr\u00e4nken.", "tokens": ["Und", "mit", "dem", "An\u00b7blick", "nie", "dich", "des", "Un\u00b7sch\u00f6\u00b7nen", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Wer mit entschlossnem Blick das Sch\u00f6ne liebt und sucht,", "tokens": ["Wer", "mit", "ent\u00b7schloss\u00b7nem", "Blick", "das", "Sch\u00f6\u00b7ne", "liebt", "und", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor dessen Auge nimmt das H\u00e4\u00dfliche die Flucht.", "tokens": ["Vor", "des\u00b7sen", "Au\u00b7ge", "nimmt", "das", "H\u00e4\u00df\u00b7li\u00b7che", "die", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der G\u00f6tter h\u00f6chste Gunst ist aber dem bewahrt,", "tokens": ["Der", "G\u00f6t\u00b7ter", "h\u00f6chs\u00b7te", "Gunst", "ist", "a\u00b7ber", "dem", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der im Unsch\u00f6nen selbst das Sch\u00f6ne nur gewahrt;", "tokens": ["Der", "im", "Un\u00b7sch\u00f6\u00b7nen", "selbst", "das", "Sch\u00f6\u00b7ne", "nur", "ge\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sei's auch, Unsch\u00f6nes nur, das seyn will, zu vernichten,", "tokens": ["Sei's", "auch", ",", "Un\u00b7sch\u00f6\u00b7nes", "nur", ",", "das", "seyn", "will", ",", "zu", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPOSAT", "ADV", "$,", "PRELS", "VAINF", "VMFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Sch\u00f6nes an der Statt, das seyn sollt', aufzurichten.", "tokens": ["Und", "Sch\u00f6\u00b7nes", "an", "der", "Statt", ",", "das", "seyn", "sollt'", ",", "auf\u00b7zu\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,", "PRELS", "VAINF", "VMFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein zartes Auge wird beleidigt vom Unsch\u00f6nen,", "tokens": ["Ein", "zar\u00b7tes", "Au\u00b7ge", "wird", "be\u00b7lei\u00b7digt", "vom", "Un\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alswie ein feines Ohr verletzt von falschen T\u00f6nen.", "tokens": ["Als\u00b7wie", "ein", "fei\u00b7nes", "Ohr", "ver\u00b7letzt", "von", "fal\u00b7schen", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Feinzarter Sinn ist gut, nicht gut der z\u00e4rtlich schwache;", "tokens": ["Fein\u00b7zar\u00b7ter", "Sinn", "ist", "gut", ",", "nicht", "gut", "der", "z\u00e4rt\u00b7lich", "schwa\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADJD", "$,", "PTKNEG", "ADJD", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du h\u00e4rte deinen so da\u00df es nicht stumpf ihn mache.", "tokens": ["Du", "h\u00e4r\u00b7te", "dei\u00b7nen", "so", "da\u00df", "es", "nicht", "stumpf", "ihn", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "KOUS", "PPER", "PTKNEG", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}