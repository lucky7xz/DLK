{"textgrid.poem.26286": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am H\u00fcgel schlummert der Klapperbaum", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am H\u00fcgel schlummert der Klapperbaum", "tokens": ["Am", "H\u00fc\u00b7gel", "schlum\u00b7mert", "der", "Klap\u00b7per\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und das Lalanggras an des Weges Saum.", "tokens": ["Und", "das", "La\u00b7lang\u00b7gras", "an", "des", "We\u00b7ges", "Saum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Halbmond wacht im Himmelsraum.", "tokens": ["Der", "Halb\u00b7mond", "wacht", "im", "Him\u00b7mels\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Sehnsuchtgedanken sind mein Geleit.", "tokens": ["Die", "Sehn\u00b7sucht\u00b7ge\u00b7dan\u00b7ken", "sind", "mein", "Ge\u00b7leit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr Atem ist warm. Ihr Weg war weit.", "tokens": ["Ihr", "A\u00b7tem", "ist", "warm", ".", "Ihr", "Weg", "war", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie kommen von dir \u00fcber Urbusch und Meer.", "tokens": ["Sie", "kom\u00b7men", "von", "dir", "\u00fc\u00b7ber", "Ur\u00b7busch", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie trinken mein Blut. Sie trennen sich schwer.", "tokens": ["Sie", "trin\u00b7ken", "mein", "Blut", ".", "Sie", "tren\u00b7nen", "sich", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}