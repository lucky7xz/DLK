{"textgrid.poem.40171": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie das Meer", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das Meer", "tokens": ["Wie", "das", "Meer"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "ist die Liebe \u2013", "tokens": ["ist", "die", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Woge um Woge", "tokens": ["Wo\u00b7ge", "um", "Wo\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "dr\u00e4ngend getrieben,", "tokens": ["dr\u00e4n\u00b7gend", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Woge in Woge", "tokens": ["Wo\u00b7ge", "in", "Wo\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "w\u00fchlend verschlungen,", "tokens": ["w\u00fch\u00b7lend", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "sturm-und-wettergeworfen nun,", "tokens": ["stur\u00b7mun\u00b7dwet\u00b7ter\u00b7ge\u00b7wor\u00b7fen", "nun", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "sonnelachend nun,", "tokens": ["son\u00b7ne\u00b7la\u00b7chend", "nun", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "bebend nun dem Mond", "tokens": ["be\u00b7bend", "nun", "dem", "Mond"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "die rastlos wechselnde ", "tokens": ["die", "rast\u00b7los", "wech\u00b7seln\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.11": {"text": "leises Fluten ewiger Ruhe,", "tokens": ["lei\u00b7ses", "Flu\u00b7ten", "e\u00b7wi\u00b7ger", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "unersch\u00fcttert,", "tokens": ["un\u00b7er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "undurchdringlich dem suchenden Blick,", "tokens": ["un\u00b7durch\u00b7dring\u00b7lich", "dem", "su\u00b7chen\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "matt verd\u00e4mmernd in n\u00e4chtiges Dunkel, \u2013", "tokens": ["matt", "ver\u00b7d\u00e4m\u00b7mernd", "in", "n\u00e4ch\u00b7ti\u00b7ges", "Dun\u00b7kel", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "sanftes Wallen ewiger Ruhe,", "tokens": ["sanf\u00b7tes", "Wal\u00b7len", "e\u00b7wi\u00b7ger", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "unbewegt,", "tokens": ["un\u00b7be\u00b7wegt", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "unerfa\u00dflich dem suchenden Blick,", "tokens": ["un\u00b7er\u00b7fa\u00df\u00b7lich", "dem", "su\u00b7chen\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "still verschwimmend in Himmelsl\u00fcfte, \u2013", "tokens": ["still", "ver\u00b7schwim\u00b7mend", "in", "Him\u00b7mel\u00b7sl\u00fcf\u00b7te", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$,", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "ist das Meer,", "tokens": ["ist", "das", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "ist die Liebe.", "tokens": ["ist", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}