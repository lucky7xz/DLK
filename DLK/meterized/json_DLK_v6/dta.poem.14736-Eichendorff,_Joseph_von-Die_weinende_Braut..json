{"dta.poem.14736": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die weinende Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Kr\u00e4nzlein trag' ich nun im Haare,", "tokens": ["Ein", "Kr\u00e4nz\u00b7lein", "trag'", "ich", "nun", "im", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In reichen Kleidern sch\u00f6n geschm\u00fcckt,", "tokens": ["In", "rei\u00b7chen", "Klei\u00b7dern", "sch\u00f6n", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fchrt mich ein andrer zum Altare,", "tokens": ["F\u00fchrt", "mich", "ein", "an\u00b7drer", "zum", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Aeltern sind so hoch begl\u00fcckt.", "tokens": ["Die", "A\u00b7el\u00b7tern", "sind", "so", "hoch", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und fr\u00f6hlich kann ich mich wohl zeigen,", "tokens": ["Und", "fr\u00f6h\u00b7lich", "kann", "ich", "mich", "wohl", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sonne hell wie damals scheint,", "tokens": ["Die", "Son\u00b7ne", "hell", "wie", "da\u00b7mals", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und vor dem Jauchzen und dem Geigen", "tokens": ["Und", "vor", "dem", "Jauch\u00b7zen", "und", "dem", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00f6rt Keiner, wie die Braut still weint.", "tokens": ["H\u00f6rt", "Kei\u00b7ner", ",", "wie", "die", "Braut", "still", "weint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}