{"textgrid.poem.49017": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du schwebest vom H\u00fcgel", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schwebest vom H\u00fcgel", "tokens": ["Du", "schwe\u00b7best", "vom", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit tauigem Fl\u00fcgel,", "tokens": ["Mit", "tau\u00b7i\u00b7gem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mit blumigem Kleid!", "tokens": ["Mit", "blu\u00b7mi\u00b7gem", "Kleid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "O Fr\u00fchling, hernieder,", "tokens": ["O", "Fr\u00fch\u00b7ling", ",", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "PTKVZ", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und weckest uns Lieder,", "tokens": ["Und", "we\u00b7ckest", "uns", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und weckest uns Freud'", "tokens": ["Und", "we\u00b7ckest", "uns", "Freud'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und f\u00fchrest gelinde", "tokens": ["Und", "f\u00fch\u00b7rest", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Umschmeichelnde Winde", "tokens": ["Um\u00b7schmei\u00b7cheln\u00b7de", "Win\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Zum schilfigen Bord,", "tokens": ["Zum", "schil\u00b7fi\u00b7gen", "Bord", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Und fesselst geschwinde", "tokens": ["Und", "fes\u00b7selst", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Den schnaubenden Nord.", "tokens": ["Den", "schnau\u00b7ben\u00b7den", "Nord", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Du kleidest die Haiden", "tokens": ["Du", "klei\u00b7dest", "die", "Hai\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und nackenden Weiden,", "tokens": ["Und", "na\u00b7cken\u00b7den", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du schw\u00e4ngerst die Luft", "tokens": ["Du", "schw\u00e4n\u00b7gerst", "die", "Luft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Mit Balsamger\u00fcchen", "tokens": ["Mit", "Bal\u00b7sam\u00b7ge\u00b7r\u00fc\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und lieblichem Duft,", "tokens": ["Und", "lieb\u00b7li\u00b7chem", "Duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und gibest den Quellen", "tokens": ["Und", "gi\u00b7best", "den", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Belebende Wellen", "tokens": ["Be\u00b7le\u00b7ben\u00b7de", "Wel\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit l\u00e4chelndem Blick,", "tokens": ["Mit", "l\u00e4\u00b7cheln\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Dem schmeichelnden Bache", "tokens": ["Dem", "schmei\u00b7cheln\u00b7den", "Ba\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die freundliche Sprache", "tokens": ["Die", "freund\u00b7li\u00b7che", "Spra\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und Stimme zur\u00fcck.", "tokens": ["Und", "Stim\u00b7me", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Dich gr\u00fc\u00dfet der Himmel,", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfet", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dich gr\u00fc\u00dfet die Welt,", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfet", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im frohen Get\u00fcmmel", "tokens": ["Im", "fro\u00b7hen", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Tal, Wiesen und Feld.", "tokens": ["Tal", ",", "Wie\u00b7sen", "und", "Feld", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Dich gr\u00fc\u00dfet durch Lieder", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfet", "durch", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das bunte Gefieder,", "tokens": ["Das", "bun\u00b7te", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Das B\u00fcsche durchzieht;", "tokens": ["Das", "B\u00fc\u00b7sche", "durch\u00b7zieht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Dich gr\u00fc\u00dfen die Hirten", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfen", "die", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Bei schattigen Myrthen,", "tokens": ["Bei", "schat\u00b7ti\u00b7gen", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Dich gr\u00fc\u00dfet mein Lied!", "tokens": ["Dich", "gr\u00fc\u00b7\u00dfet", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Mit blendenden F\u00fc\u00dfen", "tokens": ["Mit", "blen\u00b7den\u00b7den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Entschl\u00fcpfen den Fl\u00fcssen", "tokens": ["Ent\u00b7schl\u00fcp\u00b7fen", "den", "Fl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Nun Paar an Paar;", "tokens": ["Nun", "Paar", "an", "Paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die frohen Najaden", "tokens": ["Die", "fro\u00b7hen", "Na\u00b7ja\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sie ruhn an Gestaden", "tokens": ["Sie", "ruhn", "an", "Ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und trocknen ihr Haar:", "tokens": ["Und", "trock\u00b7nen", "ihr", "Haar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie eilen, Violen", "tokens": ["Sie", "ei\u00b7len", ",", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und Rosen zu holen", "tokens": ["Und", "Ro\u00b7sen", "zu", "ho\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Vom schattigen Hain,", "tokens": ["Vom", "schat\u00b7ti\u00b7gen", "Hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Und gr\u00fc\u00dfen dich singend", "tokens": ["Und", "gr\u00fc\u00b7\u00dfen", "dich", "sin\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und k\u00fcssen dich schlingend", "tokens": ["Und", "k\u00fcs\u00b7sen", "dich", "schlin\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "In l\u00e4chelnden Reihn.", "tokens": ["In", "l\u00e4\u00b7cheln\u00b7den", "Reihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Mit fr\u00f6hlichem Spotte", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", "Spot\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Steigt aus der Grotte", "tokens": ["Steigt", "aus", "der", "Grot\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der Satyr herf\u00fcr:", "tokens": ["Der", "Sa\u00b7tyr", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Treibt L\u00e4mmer und Gei\u00dfen,", "tokens": ["Treibt", "L\u00e4m\u00b7mer", "und", "Gei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und locket den wei\u00dfen,", "tokens": ["Und", "lo\u00b7cket", "den", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wildbr\u00fcllenden Stier.", "tokens": ["Wild\u00b7br\u00fcl\u00b7len\u00b7den", "Stier", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nun trinkt er und singet,", "tokens": ["Nun", "trinkt", "er", "und", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und gr\u00fc\u00dft dich und springet", "tokens": ["Und", "gr\u00fc\u00dft", "dich", "und", "sprin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mit fr\u00f6hlichem Mut;", "tokens": ["Mit", "fr\u00f6h\u00b7li\u00b7chem", "Mut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Und wirfet sich nieder,", "tokens": ["Und", "wir\u00b7fet", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Und w\u00e4lzet die Glieder", "tokens": ["Und", "w\u00e4l\u00b7zet", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "In sonniger Glut.", "tokens": ["In", "son\u00b7ni\u00b7ger", "Glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Auch Amor, der kleine,", "tokens": ["Auch", "A\u00b7mor", ",", "der", "klei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durchtanzet die Haine,", "tokens": ["Durch\u00b7tan\u00b7zet", "die", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den Satyr sieht er;", "tokens": ["Den", "Sa\u00b7tyr", "sieht", "er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Er winkt den Najaden", "tokens": ["Er", "winkt", "den", "Na\u00b7ja\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und blauen Dryaden", "tokens": ["Und", "blau\u00b7en", "Dry\u00b7a\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Vom Fr\u00fchlingsfest her.", "tokens": ["Vom", "Fr\u00fch\u00b7lings\u00b7fest", "her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Da eilen von T\u00e4nzen", "tokens": ["Da", "ei\u00b7len", "von", "T\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Nymphen hervor,", "tokens": ["Die", "Nym\u00b7phen", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Und schm\u00fccken mit Kr\u00e4nzen", "tokens": ["Und", "schm\u00fc\u00b7cken", "mit", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Des schlummernden Ohr.", "tokens": ["Des", "schlum\u00b7mern\u00b7den", "Ohr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}