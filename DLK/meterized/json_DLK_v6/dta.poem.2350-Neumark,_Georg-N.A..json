{"dta.poem.2350": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "So reissestu dich ab von dieser eitlen Erden/", "tokens": ["So", "reis\u00b7se\u00b7stu", "dich", "ab", "von", "die\u00b7ser", "eit\u00b7len", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "DuO mein andres Hertz du teutscher Neumark du?", "tokens": ["DuO", "mein", "and\u00b7res", "Hertz", "du", "teut\u00b7scher", "Neu\u00b7mark", "du", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Un\u0303 schwingest dich mit Macht nach dem Gew\u00f6lke zu?", "tokens": ["U\u00f1", "schwin\u00b7gest", "dich", "mit", "Macht", "nach", "dem", "Ge\u00b7w\u00f6l\u00b7ke", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll dein ber\u00fchmter Nahm? auch noch unsterblich werden?", "tokens": ["Soll", "dein", "be\u00b7r\u00fchm\u00b7ter", "Nahm", "?", "auch", "noch", "uns\u00b7terb\u00b7lich", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Recht so du \u00e4dler Geist! setz immer frisch hinan", "tokens": ["Recht", "so", "du", "\u00e4d\u00b7ler", "Geist", "!", "setz", "im\u00b7mer", "frisch", "hi\u00b7nan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "ADJA", "NN", "$.", "VVFIN", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Tugend und die Kunst die weisen der die Bahn", "tokens": ["Die", "Tu\u00b7gend", "und", "die", "Kunst", "die", "wei\u00b7sen", "der", "die", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "ADJA", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zur Unverg\u00e4nglichkeit. Fahr mit den wakkren Pferden/", "tokens": ["Zur", "Un\u00b7ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", ".", "Fahr", "mit", "den", "wak\u00b7kren", "Pfer\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der unverdrosnen M\u00fch/ und schwing dich allgenach.", "tokens": ["Der", "un\u00b7ver\u00b7dros\u00b7nen", "M\u00fch", "/", "und", "schwing", "dich", "all\u00b7ge\u00b7nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der blaue Neid stehe dir mit scheelen Augen nach.", "tokens": ["Der", "blau\u00b7e", "Neid", "ste\u00b7he", "dir", "mit", "schee\u00b7len", "Au\u00b7gen", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}