{"textgrid.poem.28441": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist ein g\u00fctig Gott, der zehnfach H\u00fclffe sendet,", "tokens": ["Gott", "ist", "ein", "g\u00fc\u00b7tig", "Gott", ",", "der", "zehn\u00b7fach", "H\u00fclf\u00b7fe", "sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJD", "NN", "$,", "PRELS", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh einmal sich der Mensch zu seinem Dancke wendet;", "tokens": ["Eh", "ein\u00b7mal", "sich", "der", "Mensch", "zu", "sei\u00b7nem", "Dan\u00b7cke", "wen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch schau, da\u00df dich nicht wo der Welt ihr Brauch beth\u00f6rt,", "tokens": ["Doch", "schau", ",", "da\u00df", "dich", "nicht", "wo", "der", "Welt", "ihr", "Brauch", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df, zehnmal wann du ruffst, nicht einmal Gott dich h\u00f6rt.", "tokens": ["Da\u00df", ",", "zehn\u00b7mal", "wann", "du", "ruffst", ",", "nicht", "ein\u00b7mal", "Gott", "dich", "h\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "PWAV", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}