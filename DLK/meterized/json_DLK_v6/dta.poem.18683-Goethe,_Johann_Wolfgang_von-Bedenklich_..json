{"dta.poem.18683": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Bedenklich .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.71", "sv:0.14", "nl:0.14"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich von Smaragden reden,", "tokens": ["Soll", "ich", "von", "Sma\u00b7rag\u00b7den", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Die dein Finger niedlich zeigt?", "tokens": ["Die", "dein", "Fin\u00b7ger", "nied\u00b7lich", "zeigt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manchmal ist ein Wort vonn\u00f6then,", "tokens": ["Manch\u00b7mal", "ist", "ein", "Wort", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oft ist\u2019s besser dass man schweigt.", "tokens": ["Oft", "ist's", "bes\u00b7ser", "dass", "man", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also sag\u2019 ich: dass die Farbe", "tokens": ["Al\u00b7so", "sag'", "ich", ":", "dass", "die", "Far\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcn und augerquicklich sey!", "tokens": ["Gr\u00fcn", "und", "au\u00b7ger\u00b7quick\u00b7lich", "sey", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sage nicht dass Schmerz und Narbe", "tokens": ["Sa\u00b7ge", "nicht", "dass", "Schmerz", "und", "Nar\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "KOUS", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu bef\u00fcrchten nah dabey.", "tokens": ["Zu", "be\u00b7f\u00fcrch\u00b7ten", "nah", "da\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Immerhin! du magst es lesen!", "tokens": ["Im\u00b7mer\u00b7hin", "!", "du", "magst", "es", "le\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Warum \u00fcbst du solche Macht!", "tokens": ["Wa\u00b7rum", "\u00fcbst", "du", "sol\u00b7che", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201eso gef\u00e4hrlich ist dein Wesen", "tokens": ["\u201e", "so", "ge\u00b7f\u00e4hr\u00b7lich", "ist", "dein", "We\u00b7sen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als erquicklich der Smaragd.\u201c", "tokens": ["Als", "er\u00b7quick\u00b7lich", "der", "Sma\u00b7ragd", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}