{"dta.poem.16212": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "266.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist begl\u00fcckt, wenn dir gegeben ist, zusammen", "tokens": ["Du", "bist", "be\u00b7gl\u00fcckt", ",", "wenn", "dir", "ge\u00b7ge\u00b7ben", "ist", ",", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit vielen wirkend, dich mit ihnen zu entflammen.", "tokens": ["Mit", "vie\u00b7len", "wir\u00b7kend", ",", "dich", "mit", "ih\u00b7nen", "zu", "ent\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "$,", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wenn du stehst allein, so la\u00df dich's nicht verdrie\u00dfen,", "tokens": ["Doch", "wenn", "du", "stehst", "al\u00b7lein", ",", "so", "la\u00df", "dich's", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "$,", "ADV", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt Menschen mu\u00dft du nur der Menschheit dich erschlie\u00dfen.", "tokens": ["Statt", "Men\u00b7schen", "mu\u00dft", "du", "nur", "der", "Menschheit", "dich", "er\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus jeder Raumesweit', aus allen Zeitenfernen,", "tokens": ["Aus", "je\u00b7der", "Rau\u00b7mes\u00b7weit'", ",", "aus", "al\u00b7len", "Zei\u00b7ten\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gr\u00fc\u00dft den der Menschheit Geist, der von ihm wei\u00df zu lernen.", "tokens": ["Gr\u00fc\u00dft", "den", "der", "Menschheit", "Geist", ",", "der", "von", "ihm", "wei\u00df", "zu", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Gedanken steigen aus vermorschter B\u00fcchergruft,", "tokens": ["Ge\u00b7dan\u00b7ken", "stei\u00b7gen", "aus", "ver\u00b7morschter", "B\u00fc\u00b7cher\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und andre schwimmen in der Luft wie Bl\u00fctenduft.", "tokens": ["Und", "and\u00b7re", "schwim\u00b7men", "in", "der", "Luft", "wie", "Bl\u00fc\u00b7ten\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Noch kein gedachter je gieng Denkenden verloren,", "tokens": ["Noch", "kein", "ge\u00b7dach\u00b7ter", "je", "gieng", "Den\u00b7ken\u00b7den", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungeahnet wird kein neuer auch geboren.", "tokens": ["Und", "un\u00b7ge\u00b7ah\u00b7net", "wird", "kein", "neu\u00b7er", "auch", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PIAT", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum tr\u00f6sten magst du dich, wenn aufgieng dir ein Licht,", "tokens": ["Drum", "tr\u00f6s\u00b7ten", "magst", "du", "dich", ",", "wenn", "auf\u00b7gieng", "dir", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "PPER", "PRF", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Theilst du's auch keinem mit, der Welt entgeht es nicht.", "tokens": ["Theilst", "du's", "auch", "kei\u00b7nem", "mit", ",", "der", "Welt", "ent\u00b7geht", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PIS", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie streiten, wer zuerst dis habe vorgebracht;", "tokens": ["Sie", "strei\u00b7ten", ",", "wer", "zu\u00b7erst", "dis", "ha\u00b7be", "vor\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "ADV", "PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist der Menschheit hats gemeinschaftlich erdacht.", "tokens": ["Der", "Geist", "der", "Menschheit", "hats", "ge\u00b7mein\u00b7schaft\u00b7lich", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}