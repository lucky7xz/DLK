{"textgrid.poem.24007": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Nebenbei", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit dem F\u00e4cher spielen,", "tokens": ["Mit", "dem", "F\u00e4\u00b7cher", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit den Augen zielen,", "tokens": ["Mit", "den", "Au\u00b7gen", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jede kann ", "tokens": ["Je\u00b7de", "kann"], "token_info": ["word", "word"], "pos": ["PIS", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Jede lernt das schnell,", "tokens": ["Je\u00b7de", "lernt", "das", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die kleinste N\u00e4hmamsell", "tokens": ["Die", "kleins\u00b7te", "N\u00e4h\u00b7mam\u00b7sell"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist auf diesem Gebiete gro\u00df.", "tokens": ["Ist", "auf", "die\u00b7sem", "Ge\u00b7bie\u00b7te", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}