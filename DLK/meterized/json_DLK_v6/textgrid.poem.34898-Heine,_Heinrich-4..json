{"textgrid.poem.34898": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Besser hat es sich gewendet,", "tokens": ["Bes\u00b7ser", "hat", "es", "sich", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Geschick, das dich bedroht' \u2013", "tokens": ["Das", "Ge\u00b7schick", ",", "das", "dich", "be\u00b7droht'", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott sei Dank, du hast geendet,", "tokens": ["Gott", "sei", "Dank", ",", "du", "hast", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott sei Dank, und du bist tot.", "tokens": ["Gott", "sei", "Dank", ",", "und", "du", "bist", "tot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In der Dachstub' deiner armen", "tokens": ["In", "der", "Dach\u00b7stub'", "dei\u00b7ner", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alten Mutter starbest du,", "tokens": ["Al\u00b7ten", "Mut\u00b7ter", "star\u00b7best", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie schlo\u00df dir mit Erbarmen", "tokens": ["Und", "sie", "schlo\u00df", "dir", "mit", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine sch\u00f6nen Augen zu.", "tokens": ["Dei\u00b7ne", "sch\u00f6\u00b7nen", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kaufte dir ein gutes Leilich,", "tokens": ["Kauf\u00b7te", "dir", "ein", "gu\u00b7tes", "Lei\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Sarg, ein Grab sogar.", "tokens": ["Ei\u00b7nen", "Sarg", ",", "ein", "Grab", "so\u00b7gar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Begr\u00e4bnisfeier freilich", "tokens": ["Die", "Be\u00b7gr\u00e4b\u00b7nis\u00b7fei\u00b7er", "frei\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Etwas kahl und \u00e4rmlich war.", "tokens": ["Et\u00b7was", "kahl", "und", "\u00e4rm\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keinen Pfaffen h\u00f6rt' man singen,", "tokens": ["Kei\u00b7nen", "Pfaf\u00b7fen", "h\u00f6rt'", "man", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Glocke klagte schwer;", "tokens": ["Kei\u00b7ne", "Glo\u00b7cke", "klag\u00b7te", "schwer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hinter deiner Bahre gingen", "tokens": ["Hin\u00b7ter", "dei\u00b7ner", "Bah\u00b7re", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur dein Hund und dein Friseur.", "tokens": ["Nur", "dein", "Hund", "und", "dein", "Fri\u00b7seur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbach, ich habe der Pomare\u00ab,", "tokens": ["\u00bb", "ach", ",", "ich", "ha\u00b7be", "der", "Po\u00b7ma\u00b7re", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seufzte dieser, \u00bboft gek\u00e4mmt", "tokens": ["Seufz\u00b7te", "die\u00b7ser", ",", "\u00bb", "oft", "ge\u00b7k\u00e4mmt"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "PDS", "$,", "$(", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre langen schwarzen Haare,", "tokens": ["Ih\u00b7re", "lan\u00b7gen", "schwar\u00b7zen", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie vor mir sa\u00df im Hemd.\u00ab", "tokens": ["Wenn", "sie", "vor", "mir", "sa\u00df", "im", "Hemd", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Was den Hund betrifft, so rannt er", "tokens": ["Was", "den", "Hund", "be\u00b7tr\u00b7ifft", ",", "so", "rannt", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Schon am Kirchhofstor davon,", "tokens": ["Schon", "am", "Kirch\u00b7hofs\u00b7tor", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Unterkommen fand er", "tokens": ["Und", "ein", "Un\u00b7ter\u00b7kom\u00b7men", "fand", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00e4terhin bei Ros' Pompon,", "tokens": ["Sp\u00e4\u00b7ter\u00b7hin", "bei", "Ros'", "Pom\u00b7pon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ros' Pompon, der Provenzalin,", "tokens": ["Ros'", "Pom\u00b7pon", ",", "der", "Pro\u00b7ven\u00b7za\u00b7lin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Die den Namen K\u00f6nigin", "tokens": ["Die", "den", "Na\u00b7men", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir mi\u00dfg\u00f6nnt und als Rivalin", "tokens": ["Dir", "mi\u00df\u00b7g\u00f6nnt", "und", "als", "Ri\u00b7va\u00b7lin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "KOUS", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dich verklatscht mit niederm Sinn.", "tokens": ["Dich", "ver\u00b7klatscht", "mit", "nie\u00b7derm", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Arme K\u00f6nigin des Spottes,", "tokens": ["Ar\u00b7me", "K\u00f6\u00b7ni\u00b7gin", "des", "Spot\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Diadem von Kot,", "tokens": ["Mit", "dem", "Di\u00b7a\u00b7dem", "von", "Kot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bist gerettet jetzt durch Gottes", "tokens": ["Bist", "ge\u00b7ret\u00b7tet", "jetzt", "durch", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ew'ge G\u00fcte, du bist tot.", "tokens": ["Ew'\u00b7ge", "G\u00fc\u00b7te", ",", "du", "bist", "tot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie die Mutter, so der Vater", "tokens": ["Wie", "die", "Mut\u00b7ter", ",", "so", "der", "Va\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat Barmherzigkeit ge\u00fcbt,", "tokens": ["Hat", "Barm\u00b7her\u00b7zig\u00b7keit", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich glaube, dieses tat er,", "tokens": ["Und", "ich", "glau\u00b7be", ",", "die\u00b7ses", "tat", "er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil auch du soviel geliebt.", "tokens": ["Weil", "auch", "du", "so\u00b7viel", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}