{"textgrid.poem.51201": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Revolution", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbes wird schon gehn!\u00ab ruft in den L\u00fcften", "tokens": ["\u00bb", "es", "wird", "schon", "gehn", "!", "\u00ab", "ruft", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "VVINF", "$.", "$(", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lerche, die am fr\u00fchsten wach;", "tokens": ["Die", "Ler\u00b7che", ",", "die", "am", "fr\u00fchs\u00b7ten", "wach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbes wird schon gehn!\u00ab rollt in den Gr\u00fcften", "tokens": ["\u00bb", "es", "wird", "schon", "gehn", "!", "\u00ab", "rollt", "in", "den", "Gr\u00fcf\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "VVINF", "$.", "$(", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein unterirdisch Wetter nach.", "tokens": ["Ein", "un\u00b7ter\u00b7ir\u00b7disch", "Wet\u00b7ter", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbes geht!\u00ab rauscht es in allen B\u00e4umen,", "tokens": ["\u00bb", "es", "geht", "!", "\u00ab", "rauscht", "es", "in", "al\u00b7len", "B\u00e4u\u00b7men", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "$(", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und lieblich wie Schalmeienton", "tokens": ["Und", "lieb\u00b7lich", "wie", "Schal\u00b7mei\u00b7en\u00b7ton"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NE"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "\u00bbes geht schon!\u00ab hallt es in den Tr\u00e4umen", "tokens": ["\u00bb", "es", "geht", "schon", "!", "\u00ab", "hallt", "es", "in", "den", "Tr\u00e4u\u00b7men"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$.", "$(", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der fieberkranken Nation.", "tokens": ["Der", "fie\u00b7ber\u00b7kran\u00b7ken", "Na\u00b7tion", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die St\u00e4dte werden reg und munter,", "tokens": ["Die", "St\u00e4d\u00b7te", "wer\u00b7den", "reg", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbes geht!\u00ab erschallt's von Haus zu Haus;", "tokens": ["\u00bb", "es", "geht", "!", "\u00ab", "er\u00b7schallt's", "von", "Haus", "zu", "Haus", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "$(", "PIS", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon steigt der Ruhm in sie hinunter", "tokens": ["Schon", "steigt", "der", "Ruhm", "in", "sie", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4hlt sich seine Kinder aus.", "tokens": ["Und", "w\u00e4hlt", "sich", "sei\u00b7ne", "Kin\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Morgensonne ruft: \u00bbErwache,", "tokens": ["Die", "Mor\u00b7gen\u00b7son\u00b7ne", "ruft", ":", "\u00bb", "Er\u00b7wa\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Volk, und eile auf den Markt!", "tokens": ["O", "Volk", ",", "und", "ei\u00b7le", "auf", "den", "Markt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bring auf das Forum deine Sache!", "tokens": ["Bring", "auf", "das", "Fo\u00b7rum", "dei\u00b7ne", "Sa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Freien nur ein Volk erstarkt!", "tokens": ["Im", "Frei\u00b7en", "nur", "ein", "Volk", "er\u00b7starkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Trag all dein Lieben und dein Hassen", "tokens": ["Trag", "all", "dein", "Lie\u00b7ben", "und", "dein", "Has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "PPOSAT", "ADJA", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Lust und Leid im Sturmesschritt,", "tokens": ["Und", "Lust", "und", "Leid", "im", "Stur\u00b7mes\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein schlagend Herz frei durch die Gassen,", "tokens": ["Dein", "schla\u00b7gend", "Herz", "frei", "durch", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja bring den ganzen Menschen mit!", "tokens": ["Ja", "bring", "den", "gan\u00b7zen", "Men\u00b7schen", "mit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df str\u00f6men all dein Sein und Denken", "tokens": ["La\u00df", "str\u00f6\u00b7men", "all", "dein", "Sein", "und", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVFIN", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kehr dein Innerstes zu Tag!", "tokens": ["Und", "kehr", "dein", "In\u00b7ners\u00b7tes", "zu", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Kindheit braucht dich nicht zu kr\u00e4nken,", "tokens": ["Die", "Kind\u00b7heit", "braucht", "dich", "nicht", "zu", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn du ein Kind von gutem Schlag!\u00ab", "tokens": ["Wenn", "du", "ein", "Kind", "von", "gu\u00b7tem", "Schlag", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Morgensonne ruft: \u00bbErwache!\u00ab", "tokens": ["Die", "Mor\u00b7gen\u00b7son\u00b7ne", "ruft", ":", "\u00bb", "Er\u00b7wa\u00b7che", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Klopft unterm Dach am Fenster an;", "tokens": ["Klopft", "un\u00b7term", "Dach", "am", "Fens\u00b7ter", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsteh auf und schau zu unsrer Sache,", "tokens": ["\u00bb", "steh", "auf", "und", "schau", "zu", "uns\u00b7rer", "Sa\u00b7che", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie geht, sie geht auf guter Bahn!", "tokens": ["Sie", "geht", ",", "sie", "geht", "auf", "gu\u00b7ter", "Bahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lege Gold auf deine Zunge!", "tokens": ["Ich", "le\u00b7ge", "Gold", "auf", "dei\u00b7ne", "Zun\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich lege Feuer in dein Wort!", "tokens": ["Ich", "le\u00b7ge", "Feu\u00b7er", "in", "dein", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So mach dich auf, mein lieber Junge,", "tokens": ["So", "mach", "dich", "auf", ",", "mein", "lie\u00b7ber", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schlag dich zu dem Volke dort!\u00ab", "tokens": ["Und", "schlag", "dich", "zu", "dem", "Vol\u00b7ke", "dort", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er eilt, und es empf\u00e4ngt die Menge", "tokens": ["Er", "eilt", ",", "und", "es", "emp\u00b7f\u00e4ngt", "die", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn hoffend auf dem weiten Plan;", "tokens": ["Ihn", "hof\u00b7fend", "auf", "dem", "wei\u00b7ten", "Plan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stolz tr\u00e4gt sein Kind des Volks Gedr\u00e4nge", "tokens": ["Stolz", "tr\u00e4gt", "sein", "Kind", "des", "Volks", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Rednerb\u00fchne hoch hinan.", "tokens": ["Zur", "Red\u00b7ner\u00b7b\u00fch\u00b7ne", "hoch", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun geht ein Leuchten und Gewittern", "tokens": ["Nun", "geht", "ein", "Leuch\u00b7ten", "und", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus seinem Mund durch jedes Herz;", "tokens": ["Aus", "sei\u00b7nem", "Mund", "durch", "je\u00b7des", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch goldne S\u00e4le weht ein Zittern \u2013", "tokens": ["Durch", "gold\u00b7ne", "S\u00e4\u00b7le", "weht", "ein", "Zit\u00b7tern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es wird schon gehn, schon flie\u00dft das Erz.", "tokens": ["Es", "wird", "schon", "gehn", ",", "schon", "flie\u00dft", "das", "Erz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie eine Braut am Hochzeitstage,", "tokens": ["Wie", "ei\u00b7ne", "Braut", "am", "Hoch\u00b7zeits\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So ist ein Volk, das sich erkennt;", "tokens": ["So", "ist", "ein", "Volk", ",", "das", "sich", "er\u00b7kennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie rosenrot vom hei\u00dfen Schlage,", "tokens": ["Wie", "ro\u00b7sen\u00b7rot", "vom", "hei\u00b7\u00dfen", "Schla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Liebespuls ihr Antlitz brennt!", "tokens": ["Vom", "Lie\u00b7be\u00b7spuls", "ihr", "Ant\u00b7litz", "brennt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum ersten Mal wird sie es inne,", "tokens": ["Zum", "ers\u00b7ten", "Mal", "wird", "sie", "es", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sch\u00f6n sie sei, und f\u00fchlt es ganz:", "tokens": ["Wie", "sch\u00f6n", "sie", "sei", ",", "und", "f\u00fchlt", "es", "ganz", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So stehet in der Freiheitsminne", "tokens": ["So", "ste\u00b7het", "in", "der", "Frei\u00b7heits\u00b7min\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Volk mit seinem Siegeskranz.", "tokens": ["Ein", "Volk", "mit", "sei\u00b7nem", "Sie\u00b7ges\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch wenn es nicht von G\u00fcte strahlet", "tokens": ["Doch", "wenn", "es", "nicht", "von", "G\u00fc\u00b7te", "strah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine hochbegl\u00fcckte Braut,", "tokens": ["Wie", "ei\u00b7ne", "hoch\u00b7be\u00b7gl\u00fcck\u00b7te", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist sein Lohn ihm ausgezahlet", "tokens": ["So", "ist", "sein", "Lohn", "ihm", "aus\u00b7ge\u00b7zah\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Freiheit f\u00e4hrt ins Kraut.", "tokens": ["Und", "sei\u00b7ne", "Frei\u00b7heit", "f\u00e4hrt", "ins", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punc