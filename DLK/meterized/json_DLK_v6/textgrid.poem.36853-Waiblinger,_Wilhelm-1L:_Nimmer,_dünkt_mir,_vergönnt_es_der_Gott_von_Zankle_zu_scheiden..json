{"textgrid.poem.36853": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nimmer, d\u00fcnkt mir, verg\u00f6nnt es der Gott von Zankle zu scheiden.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimmer, d\u00fcnkt mir, verg\u00f6nnt es der Gott von Zankle zu scheiden.", "tokens": ["Nim\u00b7mer", ",", "d\u00fcnkt", "mir", ",", "ver\u00b7g\u00f6nnt", "es", "der", "Gott", "von", "Zank\u00b7le", "zu", "schei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "K\u00f6nig der Winde, vernimm, K\u00f6nig der Wasser, mein Lied.", "tokens": ["K\u00f6\u00b7nig", "der", "Win\u00b7de", ",", "ver\u00b7nimm", ",", "K\u00f6\u00b7nig", "der", "Was\u00b7ser", ",", "mein", "Lied", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "$,", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wieder bin ich zum Strudel gekehrt der wilden Charybdis;", "tokens": ["Wie\u00b7der", "bin", "ich", "zum", "Stru\u00b7del", "ge\u00b7kehrt", "der", "wil\u00b7den", "Cha\u00b7ryb\u00b7dis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Meiner Wanderung Ziel schien der Peloro zu sein.", "tokens": ["Mei\u00b7ner", "Wan\u00b7de\u00b7rung", "Ziel", "schien", "der", "Pe\u00b7lo\u00b7ro", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Und was bracht' ich zur\u00fcck? Ein Herz voll Freuden und Wonnen,", "tokens": ["Und", "was", "bracht'", "ich", "zu\u00b7r\u00fcck", "?", "Ein", "Herz", "voll", "Freu\u00b7den", "und", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und ein Gl\u00fcck, wie es nur Wen'gen der Himmel geschenkt.", "tokens": ["Und", "ein", "Gl\u00fcck", ",", "wie", "es", "nur", "Wen'\u00b7gen", "der", "Him\u00b7mel", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Jubelnd str\u00f6mte das Wort mir auf die begeisterte Lippe,", "tokens": ["Ju\u00b7belnd", "str\u00f6m\u00b7te", "das", "Wort", "mir", "auf", "die", "be\u00b7geis\u00b7ter\u00b7te", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Als vom Gebirge zumal wieder die prangende Stadt,", "tokens": ["Als", "vom", "Ge\u00b7bir\u00b7ge", "zu\u00b7mal", "wie\u00b7der", "die", "pran\u00b7gen\u00b7de", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Hafen und Burg und das leuchtende Blau des wogenden Meeres,", "tokens": ["Ha\u00b7fen", "und", "Burg", "und", "das", "leuch\u00b7ten\u00b7de", "Blau", "des", "wo\u00b7gen\u00b7den", "Mee\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Wie ein gigantischer Strom zwischen die Ufer gedr\u00e4ngt,", "tokens": ["Wie", "ein", "gi\u00b7gan\u00b7ti\u00b7scher", "Strom", "zwi\u00b7schen", "die", "U\u00b7fer", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Als der Faro sich mir und Kalabriens s\u00fcdliche Zauber,", "tokens": ["Als", "der", "Fa\u00b7ro", "sich", "mir", "und", "Ka\u00b7la\u00b7bri\u00b7ens", "s\u00fcd\u00b7li\u00b7che", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "PPER", "KON", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Scilla und Apennin wieder dem Auge gezeigt,", "tokens": ["Scil\u00b7la", "und", "A\u00b7pen\u00b7nin", "wie\u00b7der", "dem", "Au\u00b7ge", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "++-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Und vergangener Monde, vergangener Freuden Erinnrung,", "tokens": ["Und", "ver\u00b7gan\u00b7ge\u00b7ner", "Mon\u00b7de", ",", "ver\u00b7gan\u00b7ge\u00b7ner", "Freu\u00b7den", "E\u00b7rinn\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Meer und Ufer und Stadt dankbar und z\u00e4rtlich begr\u00fc\u00dft.", "tokens": ["Meer", "und", "U\u00b7fer", "und", "Stadt", "dank\u00b7bar", "und", "z\u00e4rt\u00b7lich", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Aber, o Vater Neptun, dem eilenden Wandrer entgegen", "tokens": ["A\u00b7ber", ",", "o", "Va\u00b7ter", "Nep\u00b7tun", ",", "dem", "ei\u00b7len\u00b7den", "Wand\u00b7rer", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "FM", "NN", "NE", "$,", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+--+----+--+--+-", "measure": "dactylic.di.plus"}, "line.16": {"text": "F\u00fchrest du Wellen und Wind, f\u00fchrest du Aeolus Brut.", "tokens": ["F\u00fch\u00b7rest", "du", "Wel\u00b7len", "und", "Wind", ",", "f\u00fch\u00b7rest", "du", "A\u00b7e\u00b7o\u00b7lus", "Brut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "VVFIN", "PPER", "NE", "NE", "$."], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Und ein Gefangener bleib' ich zur\u00fcck; an jeglichem Morgen", "tokens": ["Und", "ein", "Ge\u00b7fan\u00b7ge\u00b7ner", "bleib'", "ich", "zu\u00b7r\u00fcck", ";", "an", "jeg\u00b7li\u00b7chem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Tret' ich ans Fenster, den Zug wandelnder Wolken zu schaun.", "tokens": ["Tret'", "ich", "ans", "Fens\u00b7ter", ",", "den", "Zug", "wan\u00b7deln\u00b7der", "Wol\u00b7ken", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Und den Schiffer erm\u00fcdet der Fragen l\u00e4st'ge Best\u00fcrmung;", "tokens": ["Und", "den", "Schif\u00b7fer", "er\u00b7m\u00fc\u00b7det", "der", "Fra\u00b7gen", "l\u00e4st'\u00b7ge", "Be\u00b7st\u00fcr\u00b7mung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Immer kehr' ich an Bord, immer nach Hause zur\u00fcck.", "tokens": ["Im\u00b7mer", "kehr'", "ich", "an", "Bord", ",", "im\u00b7mer", "nach", "Hau\u00b7se", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Wann erblick' ich die Segel? Es kommen und scheiden die Schiffe;", "tokens": ["Wann", "er\u00b7blick'", "ich", "die", "Se\u00b7gel", "?", "Es", "kom\u00b7men", "und", "schei\u00b7den", "die", "Schif\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Durch den emp\u00f6rten Kanal ziehen sie schwankend heran.", "tokens": ["Durch", "den", "em\u00b7p\u00f6r\u00b7ten", "Ka\u00b7nal", "zie\u00b7hen", "sie", "schwan\u00b7kend", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Nur das meine verweilt, und vergebens heftet die Sehnsucht", "tokens": ["Nur", "das", "mei\u00b7ne", "ver\u00b7weilt", ",", "und", "ver\u00b7ge\u00b7bens", "hef\u00b7tet", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PPOSAT", "VVFIN", "$,", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.24": {"text": "Nach dem Faro den Blick, w\u00fcnscht sich ins Weite hinaus.", "tokens": ["Nach", "dem", "Fa\u00b7ro", "den", "Blick", ",", "w\u00fcnscht", "sich", "ins", "Wei\u00b7te", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Wochen voll \u00e4ngstlicher Pein rollt so von der Spindel die Parze,", "tokens": ["Wo\u00b7chen", "voll", "\u00e4ngst\u00b7li\u00b7cher", "Pein", "rollt", "so", "von", "der", "Spin\u00b7del", "die", "Par\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--++-+-+--+-", "measure": "dactylic.di.plus"}, "line.26": {"text": "Und das neid'sche Geschick l\u00f6st mir die Fessel noch nicht.", "tokens": ["Und", "das", "nei\u00b7d'\u00b7sche", "Ge\u00b7schick", "l\u00f6st", "mir", "die", "Fes\u00b7sel", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "--+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Zwar es w\u00fcrzt mir die Stunden der Freundschaft reichste Bewirthung;", "tokens": ["Zwar", "es", "w\u00fcrzt", "mir", "die", "Stun\u00b7den", "der", "Freund\u00b7schaft", "reichs\u00b7te", "Be\u00b7wirt\u00b7hung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Aber, o G\u00f6tter, nach Rom treibt mich die Liebe zur\u00fcck.", "tokens": ["A\u00b7ber", ",", "o", "G\u00f6t\u00b7ter", ",", "nach", "Rom", "treibt", "mich", "die", "Lie\u00b7be", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$,", "APPR", "NE", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}