{"dta.poem.19315": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sturm der Vernichtung, sprich, wohin denn mich verschlagen,", "tokens": ["Sturm", "der", "Ver\u00b7nich\u00b7tung", ",", "sprich", ",", "wo\u00b7hin", "denn", "mich", "ver\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "$,", "PWAV", "ADV", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wohin denn willst du mich, wo Gott nicht w\u00e4re, tragen?", "tokens": ["Wo\u00b7hin", "denn", "willst", "du", "mich", ",", "wo", "Gott", "nicht", "w\u00e4\u00b7re", ",", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "$,", "PWAV", "NN", "PTKNEG", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von Gott ist alles Seyn umschlungen und umrungen,", "tokens": ["Von", "Gott", "ist", "al\u00b7les", "Seyn", "um\u00b7schlun\u00b7gen", "und", "um\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PIS", "PPOSAT", "ADJA", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ich bin sein, nicht mein, ich bin von ihm durchdrungen.", "tokens": ["Und", "ich", "bin", "sein", ",", "nicht", "mein", ",", "ich", "bin", "von", "ihm", "durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VAINF", "$,", "PTKNEG", "PPOSAT", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wohin ich sehe, seh' ich Gottes Schoo\u00df mir offen,", "tokens": ["Wo\u00b7hin", "ich", "se\u00b7he", ",", "seh'", "ich", "Got\u00b7tes", "Schoo\u00df", "mir", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "NN", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nur dem Zweifel ist verschlossen, nicht dem Hoffen.", "tokens": ["Der", "nur", "dem", "Zwei\u00b7fel", "ist", "ver\u00b7schlos\u00b7sen", ",", "nicht", "dem", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verschlossen ist er nur dem ihm verschlossnen Sinn;", "tokens": ["Ver\u00b7schlos\u00b7sen", "ist", "er", "nur", "dem", "ihm", "ver\u00b7schloss\u00b7nen", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum ist er offen mir, weil ich ihm offen bin.", "tokens": ["Drum", "ist", "er", "of\u00b7fen", "mir", ",", "weil", "ich", "ihm", "of\u00b7fen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "PPER", "$,", "KOUS", "PPER", "PPER", "ADJD", 