{"textgrid.poem.37016": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Beim Ober-Mufti", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dem zopf'gen Ober-Mufti", "tokens": ["Von", "dem", "zopf'\u00b7gen", "O\u00b7ber\u00b7Muf\u00b7ti"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lumpel-Lampel ", "tokens": ["Lum\u00b7pel\u00b7Lam\u00b7pel"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "(was mich h\u00f6chlich \u00fcberraschte)", "tokens": ["(", "was", "mich", "h\u00f6ch\u00b7lich", "\u00fc\u00b7berr\u00b7aschte", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines Tages vorgefordert:", "tokens": ["Ei\u00b7nes", "Ta\u00b7ges", "vor\u00b7ge\u00b7for\u00b7dert", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lie\u00df von meiner Gr\u00e4fin, welche", "tokens": ["Lie\u00df", "von", "mei\u00b7ner", "Gr\u00e4\u00b7fin", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch bei meiner Haus-Chatulle", "tokens": ["Auch", "bei", "mei\u00b7ner", "Haus\u00b7Cha\u00b7tul\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gern mir ihre Dienste weihte,", "tokens": ["Gern", "mir", "ih\u00b7re", "Diens\u00b7te", "weih\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich mir holen eine Pulle", "tokens": ["Ich", "mir", "ho\u00b7len", "ei\u00b7ne", "Pul\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbgr\u00fcneberger Schattenseite.\u00ab", "tokens": ["\u00bb", "gr\u00fc\u00b7ne\u00b7ber\u00b7ger", "Schat\u00b7ten\u00b7sei\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn es ist hier Pflicht und Sitte", "tokens": ["Denn", "es", "ist", "hier", "Pflicht", "und", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedem, den der Ober-Mufti", "tokens": ["Je\u00b7dem", ",", "den", "der", "O\u00b7ber\u00b7Muf\u00b7ti"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIAT", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lampel durch Audienz beehret,", "tokens": ["Lam\u00b7pel", "durch", "Au\u00b7di\u00b7enz", "be\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Eine solche theure Pulle", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "theu\u00b7re", "Pul\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem k\u00f6stlichsten der Weine", "tokens": ["Von", "dem", "k\u00f6st\u00b7lichs\u00b7ten", "der", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ART", "NN"], "meter": "--+---+-", "measure": "anapaest.init"}, "line.6": {"text": "Der gepre\u00dft hier wird und welchen", "tokens": ["Der", "ge\u00b7pre\u00dft", "hier", "wird", "und", "wel\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "VAFIN", "KON", "PWAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Eine goldne Etiquette", "tokens": ["Ei\u00b7ne", "gold\u00b7ne", "E\u00b7ti\u00b7quet\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Titelt \u00bbHerber Himmelssegen,\u00ab", "tokens": ["Ti\u00b7telt", "\u00bb", "Her\u00b7ber", "Him\u00b7mels\u00b7se\u00b7gen", ",", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "NN", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Auf den Altar des Pallastes", "tokens": ["Auf", "den", "Al\u00b7tar", "des", "Pal\u00b7las\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Seiner Zopfigkeit zu legen.", "tokens": ["Sei\u00b7ner", "Zop\u00b7fig\u00b7keit", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Das Portal des m\u00e4chtig-pr\u00e4cht'gen", "tokens": ["Das", "Por\u00b7tal", "des", "m\u00e4ch\u00b7tig\u00b7pr\u00e4cht'\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Marmorschlosses Lumpel-Lampels,", "tokens": ["Mar\u00b7mor\u00b7schlos\u00b7ses", "Lum\u00b7pel\u00b7Lam\u00b7pels", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das dem braven Dummdummdummer", "tokens": ["Das", "dem", "bra\u00b7ven", "Dumm\u00b7dumm\u00b7dum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Volk, bei dem man immer frommen", "tokens": ["Volk", ",", "bei", "dem", "man", "im\u00b7mer", "from\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELS", "PIS", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erbauungslust'gen Sinn trifft:", "tokens": ["Und", "er\u00b7bau\u00b7ungs\u00b7lust'\u00b7gen", "Sinn", "trifft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber eine Million", "tokens": ["Ue\u00b7ber", "ei\u00b7ne", "Mil\u00b7li\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Beutel Goldes einst gekostet,", "tokens": ["Beu\u00b7tel", "Gol\u00b7des", "einst", "ge\u00b7kos\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trug in Zeichen, schon verrostet,", "tokens": ["Trug", "in", "Zei\u00b7chen", ",", "schon", "ver\u00b7ros\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Diese demuthvolle Inschrift:", "tokens": ["Die\u00b7se", "de\u00b7muth\u00b7vol\u00b7le", "In\u00b7schrift", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "\u00bbhier Entbehrung, dort der Lohn!\u00ab", "tokens": ["\u00bb", "hier", "Ent\u00b7beh\u00b7rung", ",", "dort", "der", "Lohn", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "$,", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Als ich durch die hochgew\u00f6lbten", "tokens": ["Als", "ich", "durch", "die", "hoch\u00b7ge\u00b7w\u00f6lb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiten, kalten Hallen schritt,", "tokens": ["Wei\u00b7ten", ",", "kal\u00b7ten", "Hal\u00b7len", "schritt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nahm es Anfangs mich Erstaunen,", "tokens": ["Nahm", "es", "An\u00b7fangs", "mich", "Er\u00b7stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die kolossalen Statuen", "tokens": ["Da\u00df", "die", "ko\u00b7los\u00b7sa\u00b7len", "Sta\u00b7tu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "All' der fr\u00fchern Ober-Mufti's", "tokens": ["All'", "der", "fr\u00fc\u00b7hern", "O\u00b7ber\u00b7Muf\u00b7ti's"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.6": {"text": "Und der fr\u00fchern hohen Sultans", "tokens": ["Und", "der", "fr\u00fc\u00b7hern", "ho\u00b7hen", "Sul\u00b7tans"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "(andere Personen durften", "tokens": ["(", "an\u00b7de\u00b7re", "Per\u00b7so\u00b7nen", "durf\u00b7ten"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "VMFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Hier nicht ausgehauen werden),", "tokens": ["Hier", "nicht", "aus\u00b7ge\u00b7hau\u00b7en", "wer\u00b7den", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAINF", "$(", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Welche ich als Meisterwerke", "tokens": ["Wel\u00b7che", "ich", "als", "Meis\u00b7ter\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der Sculptur bewundern mu\u00dfte:", "tokens": ["Der", "Sculp\u00b7tur", "be\u00b7wun\u00b7dern", "mu\u00df\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sich hier s\u00e4mmtlich pr\u00e4sentirten", "tokens": ["Sich", "hier", "s\u00e4mmt\u00b7lich", "pr\u00e4\u00b7sen\u00b7tir\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit grellblauen oder schwarzen", "tokens": ["Mit", "grell\u00b7blau\u00b7en", "o\u00b7der", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Augen, lebensrothen Backen,", "tokens": ["Au\u00b7gen", ",", "le\u00b7bens\u00b7ro\u00b7then", "Ba\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dunkeln oder grauen Haaren,", "tokens": ["Dun\u00b7keln", "o\u00b7der", "grau\u00b7en", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Bunten M\u00e4nteln, Hosen, Jacken!", "tokens": ["Bun\u00b7ten", "M\u00e4n\u00b7teln", ",", "Ho\u00b7sen", ",", "Ja\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Kurzum: coloriret waren.", "tokens": ["Kur\u00b7zum", ":", "co\u00b7lo\u00b7ri\u00b7ret", "wa\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber bald erkannt ich freudigst,", "tokens": ["A\u00b7ber", "bald", "er\u00b7kannt", "ich", "freu\u00b7digst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die K\u00fcnstler dieses Sternes", "tokens": ["Da\u00df", "die", "K\u00fcnst\u00b7ler", "die\u00b7ses", "Ster\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre ird'schen Griechensch\u00fcler", "tokens": ["Uns\u00b7re", "ird'\u00b7schen", "Grie\u00b7chen\u00b7sch\u00fc\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So an \u00e4u\u00dferlicher Klarheit,", "tokens": ["So", "an", "\u00e4u\u00b7\u00dfer\u00b7li\u00b7cher", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie an Tiefe und an Wahrheit", "tokens": ["Wie", "an", "Tie\u00b7fe", "und", "an", "Wahr\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kirchthurmartig \u00fcberragen", "tokens": ["Kirch\u00b7thurm\u00b7ar\u00b7tig", "\u00fc\u00b7berr\u00b7a\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, was mehr noch, vom Gemeinen", "tokens": ["Und", ",", "was", "mehr", "noch", ",", "vom", "Ge\u00b7mei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PRELS", "ADV", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Fern sich haltend, nie den feinen", "tokens": ["Fern", "sich", "hal\u00b7tend", ",", "nie", "den", "fei\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PRF", "VVPP", "$,", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Anstand zu beleid'gen wagen.", "tokens": ["An\u00b7stand", "zu", "be\u00b7lei\u00b7d'\u00b7gen", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Denn da\u00df diese Griechensch\u00fcler", "tokens": ["Denn", "da\u00df", "die\u00b7se", "Grie\u00b7chen\u00b7sch\u00fc\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df uns machen m\u00f6chten: unsre", "tokens": ["Wei\u00df", "uns", "ma\u00b7chen", "m\u00f6ch\u00b7ten", ":", "uns\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "VVINF", "VMFIN", "$.", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00dfen und ber\u00fchmten M\u00e4nner,", "tokens": ["Gro\u00b7\u00dfen", "und", "be\u00b7r\u00fchm\u00b7ten", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sie durch den Mei\u00dfel schaffen,", "tokens": ["Die", "sie", "durch", "den", "Mei\u00b7\u00dfel", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4ren immer ohne Halstuch,", "tokens": ["W\u00e4\u00b7ren", "im\u00b7mer", "oh\u00b7ne", "Hal\u00b7stuch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mit so leichenblassem Antlitz", "tokens": ["Mit", "so", "lei\u00b7chen\u00b7blas\u00b7sem", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit so schneewei\u00dfem Schlafrock,", "tokens": ["Und", "mit", "so", "schnee\u00b7wei\u00b7\u00dfem", "Schla\u00b7frock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Stiefel, Beinkleid, Rock und Mantel", "tokens": ["Stie\u00b7fel", ",", "Bein\u00b7kleid", ",", "Rock", "und", "Man\u00b7tel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In der Welt umhergegangen:", "tokens": ["In", "der", "Welt", "um\u00b7her\u00b7ge\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Das mu\u00df doch der kleinste Junge", "tokens": ["Das", "mu\u00df", "doch", "der", "kleins\u00b7te", "Jun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "L\u00e4cherlich und albern finden!", "tokens": ["L\u00e4\u00b7cher\u00b7lich", "und", "al\u00b7bern", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und da\u00df gar die Griechensch\u00fcler", "tokens": ["Und", "da\u00df", "gar", "die", "Grie\u00b7chen\u00b7sch\u00fc\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sich die Bl\u00f6\u00dfe geben, gro\u00dfe", "tokens": ["Sich", "die", "Bl\u00f6\u00b7\u00dfe", "ge\u00b7ben", ",", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PRF", "ART", "NN", "VVINF", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Frau'n und M\u00e4nner so zu mei\u00dfeln,", "tokens": ["Frau'n", "und", "M\u00e4n\u00b7ner", "so", "zu", "mei\u00b7\u00dfeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Als ob sie stets alle Sitte", "tokens": ["Als", "ob", "sie", "stets", "al\u00b7le", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Und mit ihr zugleich bei Seite", "tokens": ["Und", "mit", "ihr", "zu\u00b7gleich", "bei", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Unterr\u00f6cke, Schuh' und Str\u00fcmpfe,", "tokens": ["Un\u00b7ter\u00b7r\u00f6\u00b7cke", ",", "Schuh'", "und", "Str\u00fcmp\u00b7fe", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Hosen, falsche Cul's, Manchetten,", "tokens": ["Ho\u00b7sen", ",", "fal\u00b7sche", "Cul's", ",", "Man\u00b7chet\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Hemden und Corsetts geworfen,", "tokens": ["Hem\u00b7den", "und", "Cor\u00b7setts", "ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Und zuletzt, just solche zeigend,", "tokens": ["Und", "zu\u00b7letzt", ",", "just", "sol\u00b7che", "zei\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Alle Scham vergessen h\u00e4tten:", "tokens": ["Al\u00b7le", "Scham", "ver\u00b7ges\u00b7sen", "h\u00e4t\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Das mu\u00df jeden J\u00fcngling, jede", "tokens": ["Das", "mu\u00df", "je\u00b7den", "J\u00fcng\u00b7ling", ",", "je\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "$,", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Dame, Demoiselle, jedes", "tokens": ["Da\u00b7me", ",", "De\u00b7mo\u00b7i\u00b7sel\u00b7le", ",", "je\u00b7des"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "PIAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Fr\u00e4ulein, darauf m\u00f6cht' ich schw\u00f6ren", "tokens": ["Fr\u00e4u\u00b7lein", ",", "da\u00b7rauf", "m\u00f6cht'", "ich", "schw\u00f6\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PAV", "VMFIN", "PPER", "VVINF"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "(nach geschehener Betrachtung)", "tokens": ["(", "nach", "ge\u00b7sche\u00b7he\u00b7ner", "Be\u00b7trach\u00b7tung", ")"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.26": {"text": "Jach erf\u00fcllen mit Verachtung", "tokens": ["Jach", "er\u00b7f\u00fcl\u00b7len", "mit", "Ver\u00b7ach\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und im Innersten emp\u00f6ren.", "tokens": ["Und", "im", "In\u00b7ners\u00b7ten", "em\u00b7p\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Selbst, da\u00df diese Steingestalten", "tokens": ["Selbst", ",", "da\u00df", "die\u00b7se", "Stein\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Todtbezwung'ner Herrschgewalten.", "tokens": ["Todt\u00b7be\u00b7zwung'\u00b7ner", "Herrschge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sich nicht hier im blo\u00dfen, wei\u00dfen", "tokens": ["Sich", "nicht", "hier", "im", "blo\u00b7\u00dfen", ",", "wei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PRF", "PTKNEG", "ADV", "APPRART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haupte zeigten, sondern Fezze", "tokens": ["Haup\u00b7te", "zeig\u00b7ten", ",", "son\u00b7dern", "Fez\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bequeme H\u00fcte trugen,", "tokens": ["Und", "be\u00b7que\u00b7me", "H\u00fc\u00b7te", "tru\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00dfte kritisch gut ich hei\u00dfen,", "tokens": ["Mu\u00df\u00b7te", "kri\u00b7tisch", "gut", "ich", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da nach h\u00f6chstem Kunstgesetze", "tokens": ["Da", "nach", "h\u00f6chs\u00b7tem", "Kunst\u00b7ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "All' sie sollten sein doch m\u00f6glichst", "tokens": ["All'", "sie", "soll\u00b7ten", "sein", "doch", "m\u00f6g\u00b7lichst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PPER", "VMFIN", "PPOSAT", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Aehnlich den Originalen,", "tokens": ["A\u00b7ehn\u00b7lich", "den", "O\u00b7rig\u00b7i\u00b7na\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Und sich diese, barhaupt weilend", "tokens": ["Und", "sich", "die\u00b7se", ",", "bar\u00b7haupt", "wei\u00b7lend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PRF", "PDS", "$,", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hier in diesen k\u00fchlen St\u00e4tten,", "tokens": ["Hier", "in", "die\u00b7sen", "k\u00fch\u00b7len", "St\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sicher einen kolossalen", "tokens": ["Si\u00b7cher", "ei\u00b7nen", "ko\u00b7los\u00b7sa\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schnupfen zugezogen h\u00e4tten.", "tokens": ["Schnup\u00b7fen", "zu\u00b7ge\u00b7zo\u00b7gen", "h\u00e4t\u00b