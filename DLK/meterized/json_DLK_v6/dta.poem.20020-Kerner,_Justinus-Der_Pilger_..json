{"dta.poem.20020": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Pilger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hieng mit hei\u00dfer Liebe", "tokens": ["Ich", "hieng", "mit", "hei\u00b7\u00dfer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An einem ird'schen Bild,", "tokens": ["An", "ei\u00b7nem", "ird'\u00b7schen", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ach! mit eitlem Triebe", "tokens": ["Das", "ach", "!", "mit", "eit\u00b7lem", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ITJ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das schwache Herz erf\u00fcllt,", "tokens": ["Das", "schwa\u00b7che", "Herz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es schwand des Lebens Frieden,", "tokens": ["Es", "schwand", "des", "Le\u00b7bens", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und matt blieb ich hienieden.", "tokens": ["Und", "matt", "blieb", "ich", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da blickt ich ach mit Thr\u00e4nen", "tokens": ["Da", "blickt", "ich", "ach", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus wol in die Welt,", "tokens": ["Hin\u00b7aus", "wol", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es stillte nicht mein Sehnen,", "tokens": ["Es", "still\u00b7te", "nicht", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was frommem Sinn gef\u00e4llt,", "tokens": ["Was", "from\u00b7mem", "Sinn", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wol standen Mond und Sterne", "tokens": ["Wol", "stan\u00b7den", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Kalt in der schwarzen Ferne.", "tokens": ["Kalt", "in", "der", "schwar\u00b7zen", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mich und die Welt zu fliehen,", "tokens": ["Mich", "und", "die", "Welt", "zu", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fa\u00dft' ich den Wanderstab,", "tokens": ["Fa\u00dft'", "ich", "den", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Blumen sah ich bl\u00fchen,", "tokens": ["Viel", "Blu\u00b7men", "sah", "ich", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch keine brach ich ab,", "tokens": ["Doch", "kei\u00b7ne", "brach", "ich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich trieb ein banges Ahnen", "tokens": ["Mich", "trieb", "ein", "ban\u00b7ges", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach ungewohnten Bahnen.", "tokens": ["Nach", "un\u00b7ge\u00b7wohn\u00b7ten", "Bah\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da blickt von Bergesh\u00f6hen", "tokens": ["Da", "blickt", "von", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir ach! so neu, so mild,", "tokens": ["Mir", "ach", "!", "so", "neu", ",", "so", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ITJ", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als h\u00e4tt' ich's nie gesehen,", "tokens": ["Als", "h\u00e4tt'", "ich's", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Kreuz des Mittlers Bild,", "tokens": ["Vom", "Kreuz", "des", "Mitt\u00b7lers", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht konnt' ich widerstreben,", "tokens": ["Nicht", "konnt'", "ich", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf zog es mich mit Beben.", "tokens": ["Auf", "zog", "es", "mich", "mit", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und was ich j\u00fcngst begehrte,", "tokens": ["Und", "was", "ich", "j\u00fcngst", "be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Ird'sche floh mein Herz,", "tokens": ["Das", "Ird'\u00b7sche", "floh", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinab sank Staub und Erde,", "tokens": ["Hin\u00b7ab", "sank", "Staub", "und", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonne flog himmelw\u00e4rts.", "tokens": ["Son\u00b7ne", "flog", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.5": {"text": "Hin kniet' ich im Entz\u00fccken,", "tokens": ["Hin", "kniet'", "ich", "im", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es an das Herz zu dr\u00fccken.", "tokens": ["Es", "an", "das", "Herz", "zu", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da str\u00f6mten Ruh' und Wonne", "tokens": ["Da", "str\u00f6m\u00b7ten", "Ruh'", "und", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus ihm in meine Brust,", "tokens": ["Aus", "ihm", "in", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als w\u00e4r' es eine Sonne,", "tokens": ["Als", "w\u00e4r'", "es", "ei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchz\u00fcckt es mich mit Lust;", "tokens": ["Durch\u00b7z\u00fcckt", "es", "mich", "mit", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es flogen Engel nieder,", "tokens": ["Es", "flo\u00b7gen", "En\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und gr\u00fc\u00dften mich als Br\u00fcder.", "tokens": ["Und", "gr\u00fc\u00df\u00b7ten", "mich", "als", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch sieh! zum s\u00fc\u00dfen Lohne", "tokens": ["Doch", "sieh", "!", "zum", "s\u00fc\u00b7\u00dfen", "Loh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Neigt mild das Bildni\u00df sich,", "tokens": ["Neigt", "mild", "das", "Bild\u00b7ni\u00df", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sinkt die Dornenkrone", "tokens": ["Es", "sinkt", "die", "Dor\u00b7nen\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von seinem Haupt auf mich.", "tokens": ["Von", "sei\u00b7nem", "Haupt", "auf", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Fest dr\u00fcckt' ich sie zum Herzen,", "tokens": ["Fest", "dr\u00fcckt'", "ich", "sie", "zum", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fchlend so s\u00fc\u00dfe Schmerzen.", "tokens": ["F\u00fch\u00b7lend", "so", "s\u00fc\u00b7\u00dfe", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Und bis zur Todesstunde,", "tokens": ["Und", "bis", "zur", "To\u00b7dess\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Weh und Lebenslust,", "tokens": ["In", "Weh", "und", "Le\u00b7bens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchl' ich die theure Wunde", "tokens": ["F\u00fchl'", "ich", "die", "theu\u00b7re", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun tief in meiner Brust;", "tokens": ["Nun", "tief", "in", "mei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchl', wie ein neues Leben", "tokens": ["F\u00fchl'", ",", "wie", "ein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir ganz in ihr gegeben.", "tokens": ["Mir", "ganz", "in", "ihr", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}