{"textgrid.poem.46123": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Von lieben h\u00e4nden", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach gib mir dise zarte hand", "tokens": ["Ach", "gib", "mir", "di\u00b7se", "zar\u00b7te", "hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "damit ich sie doch gnug m\u00f6g k\u00fcssen,", "tokens": ["da\u00b7mit", "ich", "sie", "doch", "gnug", "m\u00f6g", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "gib die hand, meiner hofnung pfand,", "tokens": ["gib", "die", "hand", ",", "mei\u00b7ner", "hof\u00b7nung", "pfand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "die aus verzweiflung mich gerissen;", "tokens": ["die", "aus", "ver\u00b7zwei\u00b7flung", "mich", "ge\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "darum gib mir sie her,", "tokens": ["da\u00b7rum", "gib", "mir", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVIMP", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "da\u00df ich sie k\u00fc\u00df mit ehr.", "tokens": ["da\u00df", "ich", "sie", "k\u00fc\u00df", "mit", "ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie! k\u00fcssen dise hand so frech,", "tokens": ["Wie", "!", "k\u00fcs\u00b7sen", "di\u00b7se", "hand", "so", "frech", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "so mein herz d\u00f6rfte mir ausrei\u00dfen?", "tokens": ["so", "mein", "herz", "d\u00f6rf\u00b7te", "mir", "aus\u00b7rei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "nein! es ist zeit, da\u00df ich mich r\u00e4ch,", "tokens": ["nein", "!", "es", "ist", "zeit", ",", "da\u00df", "ich", "mich", "r\u00e4ch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "darum will ich sie vilmehr bei\u00dfen,", "tokens": ["da\u00b7rum", "will", "ich", "sie", "vil\u00b7mehr", "bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df sie sich der untreu", "tokens": ["da\u00df", "sie", "sich", "der", "un\u00b7treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ART", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "und bosheit nunmehr reu.", "tokens": ["und", "bos\u00b7heit", "nun\u00b7mehr", "reu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was nutzet aber hie mein zorn", "tokens": ["Was", "nut\u00b7zet", "a\u00b7ber", "hie", "mein", "zorn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr disen s\u00fc\u00dfen gilg und rosen,", "tokens": ["f\u00fcr", "di\u00b7sen", "s\u00fc\u00b7\u00dfen", "gilg", "und", "ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die alle, stets frisch und ohn dorn,", "tokens": ["die", "al\u00b7le", ",", "stets", "frisch", "und", "ohn", "dorn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADV", "ADJD", "KON", "APPR", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "dem herzen und gesicht liebkosen?", "tokens": ["dem", "her\u00b7zen", "und", "ge\u00b7sicht", "lieb\u00b7ko\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "darum gib mir sie her,", "tokens": ["da\u00b7rum", "gib", "mir", "sie", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVIMP", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "da\u00df ich sie k\u00fc\u00df mit ehr.", "tokens": ["da\u00df", "ich", "sie", "k\u00fc\u00df", "mit", "ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O hand, warum k\u00fc\u00df ich dich lang,", "tokens": ["O", "hand", ",", "wa\u00b7rum", "k\u00fc\u00df", "ich", "dich", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "da ich mehr ursach, dich zu hassen,", "tokens": ["da", "ich", "mehr", "ur\u00b7sach", ",", "dich", "zu", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "zerdruckend mit so zartem zwang,", "tokens": ["zer\u00b7dru\u00b7ckend", "mit", "so", "zar\u00b7tem", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADV", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als du mein armes herz erfassen!", "tokens": ["als", "du", "mein", "ar\u00b7mes", "herz", "er\u00b7fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "da\u00df numehr der untreu", "tokens": ["da\u00df", "nu\u00b7mehr", "der", "un\u00b7treu"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "und bosheit dich auch reu.", "tokens": ["und", "bos\u00b7heit", "dich", "auch", "reu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O sch\u00f6ne hand, der augen weid,", "tokens": ["O", "sch\u00f6\u00b7ne", "hand", ",", "der", "au\u00b7gen", "weid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "la\u00df meinen zorn dich nicht betr\u00fcben,", "tokens": ["la\u00df", "mei\u00b7nen", "zorn", "dich", "nicht", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ob du mir schon thust vil zu leid", "tokens": ["ob", "du", "mir", "schon", "thust", "vil", "zu", "leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "ADV", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mu\u00df ich doch deine thaten lieben;", "tokens": ["mu\u00df", "ich", "doch", "dei\u00b7ne", "tha\u00b7ten", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "darum so kom nu her,", "tokens": ["da\u00b7rum", "so", "kom", "nu", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "da\u00df ich dich k\u00fc\u00df mit ehr.", "tokens": ["da\u00df", "ich", "dich", "k\u00fc\u00df", "mit", "ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Je mehr ich k\u00fc\u00df, je mehr dein schnee", "tokens": ["Je", "mehr", "ich", "k\u00fc\u00df", ",", "je", "mehr", "dein", "schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mein herz ganz wunderlich anz\u00fcndet,", "tokens": ["mein", "herz", "ganz", "wun\u00b7der\u00b7lich", "an\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "darum ich billich nu absteh,", "tokens": ["da\u00b7rum", "ich", "bil\u00b7lich", "nu", "ab\u00b7steh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "eh g\u00e4nzlich mich dein schein verblindet,", "tokens": ["eh", "g\u00e4nz\u00b7lich", "mich", "dein", "schein", "ver\u00b7blin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und da\u00df nicht der untreu", "tokens": ["und", "da\u00df", "nicht", "der", "un\u00b7treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "ART", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "zu spat uns beede reu.", "tokens": ["zu", "spat", "uns", "bee\u00b7de", "reu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVFIN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O da\u00df ich, unserm verdienst nach,", "tokens": ["O", "da\u00df", "ich", ",", "un\u00b7serm", "ver\u00b7dienst", "nach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "$,", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "m\u00f6g diser zarten hand mutwillen", "tokens": ["m\u00f6g", "di\u00b7ser", "zar\u00b7ten", "hand", "mut\u00b7wil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "VVINF"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "und meines herzens s\u00fc\u00dfe rach", "tokens": ["und", "mei\u00b7nes", "her\u00b7zens", "s\u00fc\u00b7\u00dfe", "rach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ganz uners\u00e4tlich k\u00fcssend stillen!", "tokens": ["ganz", "un\u00b7er\u00b7s\u00e4t\u00b7lich", "k\u00fcs\u00b7send", "stil\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und sie bu\u00df und reu lehr,", "tokens": ["und", "sie", "bu\u00df", "und", "reu", "lehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "sie k\u00fcssend mehr und mehr.", "tokens": ["sie", "k\u00fcs\u00b7send", "mehr", "und", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}