{"textgrid.poem.59952": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das Gewand um deine Glieder,", "tokens": ["Wie", "das", "Ge\u00b7wand", "um", "dei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schlingt sich der Reim um meine Lieder;", "tokens": ["Schlingt", "sich", "der", "Reim", "um", "mei\u00b7ne", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6n m\u00f6gen des Gewandes Falten sein:", "tokens": ["Sch\u00f6n", "m\u00f6\u00b7gen", "des", "Ge\u00b7wan\u00b7des", "Fal\u00b7ten", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch sch\u00f6ner mu\u00df, was sie enthalten, sein!", "tokens": ["Doch", "sch\u00f6\u00b7ner", "mu\u00df", ",", "was", "sie", "ent\u00b7hal\u00b7ten", ",", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "$,", "PRELS", "PPER", "VVPP", "$,", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}