{"textgrid.poem.65743": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "17", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer glaubt wohl k\u00fcnftig an mein Lied, erf\u00fcllet", "tokens": ["Wer", "glaubt", "wohl", "k\u00fcnf\u00b7tig", "an", "mein", "Lied", ",", "er\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von deinem h\u00f6chsten Wert? \u2013 Der Himmel zwar", "tokens": ["Von", "dei\u00b7nem", "h\u00f6chs\u00b7ten", "Wert", "?", "\u2013", "Der", "Him\u00b7mel", "zwar"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "$(", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00df, nur ein Grab ist's, das dein Leben h\u00fcllet,", "tokens": ["Wei\u00df", ",", "nur", "ein", "Grab", "ist's", ",", "das", "dein", "Le\u00b7ben", "h\u00fcl\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht halb dein Erbteil schildernd wie es war. \u2013", "tokens": ["Nicht", "halb", "dein", "Erb\u00b7teil", "schil\u00b7dernd", "wie", "es", "war", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "ADJD", "KOKOM", "PPER", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Schrieb ich die Sch\u00f6nheit deiner Augenlichter,", "tokens": ["Schrieb", "ich", "die", "Sch\u00f6n\u00b7heit", "dei\u00b7ner", "Au\u00b7gen\u00b7lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In frischen Weisen jeden Reiz von dir,", "tokens": ["In", "fri\u00b7schen", "Wei\u00b7sen", "je\u00b7den", "Reiz", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Nachwelt spr\u00e4ch: ein L\u00fcgner ist der Dichter,", "tokens": ["Die", "Nach\u00b7welt", "spr\u00e4ch", ":", "ein", "L\u00fcg\u00b7ner", "ist", "der", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So himmlisch blickt kein Erdenauge hier.", "tokens": ["So", "himm\u00b7lisch", "blickt", "kein", "Er\u00b7de\u00b7nau\u00b7ge", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So w\u00e4re dann, Greisen gleich von minder Witz als Worten,", "tokens": ["So", "w\u00e4\u00b7re", "dann", ",", "Grei\u00b7sen", "gleich", "von", "min\u00b7der", "Witz", "als", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "NN", "ADV", "APPR", "ADJA", "NN", "KOUS", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Mein Blatt, vergilbt an ihrem Alter, schon", "tokens": ["Mein", "Blatt", ",", "ver\u00b7gilbt", "an", "ih\u00b7rem", "Al\u00b7ter", ",", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Zu Spott, dein gutes Recht ein Dichterwahnsinn worden,", "tokens": ["Zu", "Spott", ",", "dein", "gu\u00b7tes", "Recht", "ein", "Dich\u00b7ter\u00b7wahn\u00b7sinn", "wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "ADJA", "NN", "ART", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Aus einem alten Sang ein \u00fcbertriebner Ton!", "tokens": ["Aus", "ei\u00b7nem", "al\u00b7ten", "Sang", "ein", "\u00fc\u00b7bert\u00b7rieb\u00b7ner", "Ton", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch, h\u00e4ttest du ein Kind um jene Zeit,", "tokens": ["Doch", ",", "h\u00e4t\u00b7test", "du", "ein", "Kind", "um", "je\u00b7ne", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zwiefach w\u00e4r'st du, in ihm, und meinem Reim erneut.", "tokens": ["Zwie\u00b7fach", "w\u00e4r'st", "du", ",", "in", "ihm", ",", "und", "mei\u00b7nem", "Reim", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}