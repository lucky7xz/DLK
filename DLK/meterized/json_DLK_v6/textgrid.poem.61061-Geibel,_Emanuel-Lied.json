{"textgrid.poem.61061": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne brannte hei\u00df am Tage,", "tokens": ["Die", "Son\u00b7ne", "brann\u00b7te", "hei\u00df", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun wird es auf den Abend k\u00fchl;", "tokens": ["Nun", "wird", "es", "auf", "den", "A\u00b7bend", "k\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wolken ziehn in dunkler Lage,", "tokens": ["Die", "Wol\u00b7ken", "ziehn", "in", "dunk\u00b7ler", "La\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und durch die Luft weht Harfenspiel.", "tokens": ["Und", "durch", "die", "Luft", "weht", "Har\u00b7fen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir ist so eigen, ist so tr\u00fcbe;", "tokens": ["Mir", "ist", "so", "ei\u00b7gen", ",", "ist", "so", "tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herz strebt in die Ferne fort,", "tokens": ["Mein", "Herz", "strebt", "in", "die", "Fer\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es denkt an seine alte Liebe", "tokens": ["Es", "denkt", "an", "sei\u00b7ne", "al\u00b7te", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sinnt auf ein verloren Wort.", "tokens": ["Und", "sinnt", "auf", "ein", "ver\u00b7lo\u00b7ren", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Umsonst! Ich werd' ihn nimmer finden,", "tokens": ["Um\u00b7sonst", "!", "Ich", "werd'", "ihn", "nim\u00b7mer", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Spruch, der Seelen binden mag;", "tokens": ["Den", "Spruch", ",", "der", "See\u00b7len", "bin\u00b7den", "mag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum auch gab ich ihn den Winden,", "tokens": ["Wa\u00b7rum", "auch", "gab", "ich", "ihn", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da er auf meinen Lippen lag?", "tokens": ["Da", "er", "auf", "mei\u00b7nen", "Lip\u00b7pen", "lag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! Immer finstrer wird der Schatten;", "tokens": ["Ach", "!", "Im\u00b7mer", "finst\u00b7rer", "wird", "der", "Schat\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich steh' allein in \u00f6der Nacht,", "tokens": ["Ich", "steh'", "al\u00b7lein", "in", "\u00f6\u00b7der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und keine St\u00e4tte harrt des Matten,", "tokens": ["Und", "kei\u00b7ne", "St\u00e4t\u00b7te", "harrt", "des", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und niemand ist, der mit mir wacht.", "tokens": ["Und", "nie\u00b7mand", "ist", ",", "der", "mit", "mir", "wacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}