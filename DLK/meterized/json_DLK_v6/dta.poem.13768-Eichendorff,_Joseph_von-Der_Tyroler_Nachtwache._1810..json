{"dta.poem.13768": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tyroler Nachtwache.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie die St\u00e4mme in dem Wald", "tokens": ["Gleich\u00b7wie", "die", "St\u00e4m\u00b7me", "in", "dem", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woll'n wir zusammenhalten,", "tokens": ["Woll'n", "wir", "zu\u00b7sam\u00b7men\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein' feste Burg, Trutz der Gewalt,", "tokens": ["Ein'", "fes\u00b7te", "Burg", ",", "Trutz", "der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Verbleiben treu die alten.", "tokens": ["Ver\u00b7blei\u00b7ben", "treu", "die", "al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Steig', Sonne, sch\u00f6n!", "tokens": ["Steig'", ",", "Son\u00b7ne", ",", "sch\u00f6n", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Wirf von den H\u00f6h'n", "tokens": ["Wirf", "von", "den", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nacht und die mit ihr kamen,", "tokens": ["Nacht", "und", "die", "mit", "ihr", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Hinab in Gottes Namen!", "tokens": ["Hin\u00b7ab", "in", "Got\u00b7tes", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}