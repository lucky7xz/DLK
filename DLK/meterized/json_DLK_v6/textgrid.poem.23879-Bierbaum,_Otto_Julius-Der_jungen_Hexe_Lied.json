{"textgrid.poem.23879": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der jungen Hexe Lied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als nachts ich \u00fcberm Gebirge ritt,", "tokens": ["Als", "nachts", "ich", "\u00fc\u00b7berm", "Ge\u00b7bir\u00b7ge", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rack, schack, schacke mein Pferdchen,", "tokens": ["Rack", ",", "schack", ",", "scha\u00b7cke", "mein", "Pferd\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da ritt ein seltsam Klingeln mit,", "tokens": ["Da", "ritt", "ein", "selt\u00b7sam", "Klin\u00b7geln", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kling, ling, klingelalei.", "tokens": ["Kling", ",", "ling", ",", "klin\u00b7ge\u00b7la\u00b7lei", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "ITJ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Es war ein schmeichlerisch bittend Get\u00f6n,", "tokens": ["Es", "war", "ein", "schmeich\u00b7le\u00b7risch", "bit\u00b7tend", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es war wie Kinderstimmen sch\u00f6n.", "tokens": ["Es", "war", "wie", "Kin\u00b7der\u00b7stim\u00b7men", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir wars, ich streichelt' ein lindes Haar,", "tokens": ["Mir", "wars", ",", "ich", "strei\u00b7chelt'", "ein", "lin\u00b7des", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir war so weh und wunderbar.", "tokens": ["Mir", "war", "so", "weh", "und", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da schwand das Klingeln mit einemmal,", "tokens": ["Da", "schwand", "das", "Klin\u00b7geln", "mit", "ei\u00b7nem\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sah hinunter ins tiefe Thal.", "tokens": ["Ich", "sah", "hin\u00b7un\u00b7ter", "ins", "tie\u00b7fe", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da sah ich Licht in meinem Haus,", "tokens": ["Da", "sah", "ich", "Licht", "in", "mei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rack, schack, schacke mein Pferdchen,", "tokens": ["Rack", ",", "schack", ",", "scha\u00b7cke", "mein", "Pferd\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Mein B\u00fcbchen sah nach der Mutter aus,", "tokens": ["Mein", "B\u00fcb\u00b7chen", "sah", "nach", "der", "Mut\u00b7ter", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kling, ling, klingelalei.", "tokens": ["Kling", ",", "ling", ",", "klin\u00b7ge\u00b7la\u00b7lei", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "ITJ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}