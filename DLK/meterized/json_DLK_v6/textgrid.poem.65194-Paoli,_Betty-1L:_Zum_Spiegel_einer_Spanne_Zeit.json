{"textgrid.poem.65194": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Spiegel einer Spanne Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Spiegel einer Spanne Zeit", "tokens": ["Zum", "Spie\u00b7gel", "ei\u00b7ner", "Span\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt ihr die heil'ge Kunst erniedern?", "tokens": ["Wollt", "ihr", "die", "heil'\u00b7ge", "Kunst", "er\u00b7nie\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts als die bare Wirklichkeit", "tokens": ["Nichts", "als", "die", "ba\u00b7re", "Wirk\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll sie euch schildern und zergliedern?", "tokens": ["Soll", "sie", "euch", "schil\u00b7dern", "und", "zer\u00b7glie\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "D\u00fcnkt euch die Wirklichkeit so viel?", "tokens": ["D\u00fcnkt", "euch", "die", "Wirk\u00b7lich\u00b7keit", "so", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So w\u00fcrdig aller h\u00f6chsten Ehren", "tokens": ["So", "w\u00fcr\u00b7dig", "al\u00b7ler", "h\u00f6chs\u00b7ten", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des blinden Zufalls fl\u00fcchtig Spiel,", "tokens": ["Des", "blin\u00b7den", "Zu\u00b7falls", "fl\u00fcch\u00b7tig", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebund'ner Kr\u00e4fte tr\u00fcbes G\u00e4ren?", "tokens": ["Ge\u00b7bun\u00b7d'\u00b7ner", "Kr\u00e4f\u00b7te", "tr\u00fc\u00b7bes", "G\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Wirrsal ist sie, in der Macht", "tokens": ["Ein", "Wirr\u00b7sal", "ist", "sie", ",", "in", "der", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Widerspruchs, der sie durchw\u00fctet!", "tokens": ["Des", "Wi\u00b7der\u00b7spruchs", ",", "der", "sie", "durch\u00b7w\u00fc\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Chaos, \u00fcber dem die Nacht", "tokens": ["Ein", "Chaos", ",", "\u00fc\u00b7ber", "dem", "die", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der dumpfen Unbewu\u00dftheit br\u00fctet!", "tokens": ["Der", "dum\u00b7pfen", "Un\u00b7be\u00b7wu\u00df\u00b7theit", "br\u00fc\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll diesem Chaos, w\u00fcst und fahl,", "tokens": ["Soll", "die\u00b7sem", "Chaos", ",", "w\u00fcst", "und", "fahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sich eine bl\u00fch'nde Welt entringen,", "tokens": ["Sich", "ei\u00b7ne", "bl\u00fch'n\u00b7de", "Welt", "ent\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df mit sch\u00f6pferischem Strahl", "tokens": ["So", "mu\u00df", "mit", "sch\u00f6p\u00b7fe\u00b7ri\u00b7schem", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Poesie es erst durchdringen.", "tokens": ["Die", "Poe\u00b7sie", "es", "erst", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Denn sie nur wei\u00df das L\u00f6sungswort", "tokens": ["Denn", "sie", "nur", "wei\u00df", "das", "L\u00f6\u00b7sungs\u00b7wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verworr'ner R\u00e4tsel aufzufinden,", "tokens": ["Ver\u00b7worr'\u00b7ner", "R\u00e4t\u00b7sel", "auf\u00b7zu\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das St\u00fcckwerk hie, das St\u00fcckwerk dort", "tokens": ["Das", "St\u00fcck\u00b7werk", "hie", ",", "das", "St\u00fcck\u00b7werk", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem Ganzen zu verbinden.", "tokens": ["Zu", "ei\u00b7nem", "Gan\u00b7zen", "zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn ihr nur ward der Seherblick,", "tokens": ["Denn", "ihr", "nur", "ward", "der", "Se\u00b7her\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wesenheit vom Scheine trennet,", "tokens": ["Der", "We\u00b7sen\u00b7heit", "vom", "Schei\u00b7ne", "tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in dem einzelnen Geschick", "tokens": ["Und", "in", "dem", "ein\u00b7zel\u00b7nen", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ein allgemein Gesetz erkennet.", "tokens": ["Ein", "all\u00b7ge\u00b7mein", "Ge\u00b7setz", "er\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wollt ihr von ihrem Sternenlauf", "tokens": ["Wollt", "ihr", "von", "ih\u00b7rem", "Ster\u00b7nen\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht froh vertrau'nd euch leiten lassen,", "tokens": ["Nicht", "froh", "ver\u00b7trau'nd", "euch", "lei\u00b7ten", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann, teure Freunde! gebt es auf", "tokens": ["Dann", ",", "teu\u00b7re", "Freun\u00b7de", "!", "gebt", "es", "auf"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Welt Mysterien zu fassen.", "tokens": ["Der", "Welt", "Mys\u00b7te\u00b7ri\u00b7en", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Dann bleibt auch k\u00fcnftig wie zuvor", "tokens": ["Dann", "bleibt", "auch", "k\u00fcnf\u00b7tig", "wie", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "KOKOM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Truge der Erscheinung kleben!", "tokens": ["Am", "Tru\u00b7ge", "der", "Er\u00b7schei\u00b7nung", "kle\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Wahrheit f\u00fchrt nur sie empor,", "tokens": ["Zur", "Wahr\u00b7heit", "f\u00fchrt", "nur", "sie", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie, die der Weg, das Licht, das Leben!", "tokens": ["Sie", ",", "die", "der", "Weg", ",", "das", "Licht", ",", "das", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}