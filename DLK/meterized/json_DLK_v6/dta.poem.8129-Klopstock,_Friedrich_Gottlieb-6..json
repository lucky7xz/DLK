{"dta.poem.8129": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr lieft nicht die Laufbahn des Erdulders,", "tokens": ["Ihr", "lieft", "nicht", "die", "Lauf\u00b7bahn", "des", "E\u00b7rdul\u00b7ders", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Pilgers, da hinab nicht, wo der Tod war;", "tokens": ["Des", "Pil\u00b7gers", ",", "da", "hin\u00b7ab", "nicht", ",", "wo", "der", "Tod", "war", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADV", "PTKNEG", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jhr Unsterblichen, sahet das Grab", "tokens": ["Ihr", "U\u00b7nsterb\u00b7li\u00b7chen", ",", "sa\u00b7het", "das", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Nicht er\u00f6fnet, und gef\u00fcllt mit Gebein!", "tokens": ["Nicht", "er\u00b7\u00f6f\u00b7net", ",", "und", "ge\u00b7f\u00fcllt", "mit", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KON", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}