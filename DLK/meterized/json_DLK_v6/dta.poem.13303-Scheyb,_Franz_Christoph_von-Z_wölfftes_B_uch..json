{"dta.poem.13303": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ees deutet mir mein Herz noch gr\u00f6ssre Sachen an,", "tokens": ["\u201e", "es", "deu\u00b7tet", "mir", "mein", "Herz", "noch", "gr\u00f6ss\u00b7re", "Sa\u00b7chen", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edie mein erstaunter Mund euch nicht beschreiben kann.", "tokens": ["\u201e", "die", "mein", "er\u00b7staun\u00b7ter", "Mund", "euch", "nicht", "be\u00b7schrei\u00b7ben", "kann", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PPOSAT", "ADJA", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edie Weisheit rede nur; ich lasse sie beschliefsen:", "tokens": ["\u201e", "die", "Weis\u00b7heit", "re\u00b7de", "nur", ";", "ich", "las\u00b7se", "sie", "be\u00b7schlief\u00b7sen", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder Adler heisse nichts. Dort werden wir es wissen;", "tokens": ["\u201e", "der", "Ad\u00b7ler", "heis\u00b7se", "nichts", ".", "Dort", "wer\u00b7den", "wir", "es", "wis\u00b7sen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PIS", "$.", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "245\u201dDort will ich sehn, was sie bey dieser Ankunft spricht.", "tokens": ["\"", "Dort", "will", "ich", "sehn", ",", "was", "sie", "bey", "die\u00b7ser", "An\u00b7kunft", "spricht", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "PPER", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eich trage zum Geschrey der Leute Zuversicht;", "tokens": ["\u201e", "ich", "tra\u00b7ge", "zum", "Ge\u00b7schrey", "der", "Leu\u00b7te", "Zu\u00b7ver\u00b7sicht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eein treuer Adler-Schwarm wird dieses Schiff begleiten,", "tokens": ["\u201e", "ein", "treu\u00b7er", "Ad\u00b7ler\u00b7Schwarm", "wird", "die\u00b7ses", "Schiff", "be\u00b7glei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund um den Rang des Diensts, der Ehrerbietung streiten.", "tokens": ["\u201e", "und", "um", "den", "Rang", "des", "Diensts", ",", "der", "Ehr\u00b7er\u00b7bie\u00b7tung", "strei\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ekein Fenster, keine Th\u00fcr der Stadt, ja kein Ger\u00e4th", "tokens": ["\u201e", "kein", "Fens\u00b7ter", ",", "kei\u00b7ne", "Th\u00fcr", "der", "Stadt", ",", "ja", "kein", "Ge\u00b7r\u00e4\u00b7th"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PIAT", "NN", "$,", "PIAT", "NN", "ART", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "250\u201dWird mehr zu finden seyn, wo nicht ein Adler steht:", "tokens": ["\"", "Wird", "mehr", "zu", "fin\u00b7den", "seyn", ",", "wo", "nicht", "ein", "Ad\u00b7ler", "steht", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "PTKZU", "VVINF", "VAINF", "$,", "PWAV", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eso auf dem tiefsten Dach, als auf den h\u00f6chsten Spizen", "tokens": ["\u201e", "so", "auf", "dem", "tiefs\u00b7ten", "Dach", ",", "als", "auf", "den", "h\u00f6chs\u00b7ten", "Spi\u00b7zen"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eder Th\u00fcrne wird man ihn frolockend sehen sizen.", "tokens": ["\u201e", "der", "Th\u00fcr\u00b7ne", "wird", "man", "ihn", "fro\u00b7lo\u00b7ckend", "se\u00b7hen", "si\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PIS", "PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewelch unerlebtes Fest! glorreiche K\u00f6niginn!", "tokens": ["\u201e", "welch", "un\u00b7er\u00b7leb\u00b7tes", "Fest", "!", "glor\u00b7rei\u00b7che", "K\u00f6\u00b7ni\u00b7ginn", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAT", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ethron-werther Ehgemahl! mir sagen Herz und Sinn:", "tokens": ["\u201e", "thron\u00b7wer\u00b7ther", "Eh\u00b7ge\u00b7mahl", "!", "mir", "sa\u00b7gen", "Herz", "und", "Sinn", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "255\u201dWie ihr mit jenem Schmuck des deutschen Reiches pranget;", "tokens": ["\"", "Wie", "ihr", "mit", "je\u00b7nem", "Schmuck", "des", "deut\u00b7schen", "Rei\u00b7ches", "pran\u00b7get", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "APPR", "PDAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201emit was vor Hulden ihr der V\u00f6lcker Herz erlanget.", "tokens": ["\u201e", "mit", "was", "vor", "Hul\u00b7den", "ihr", "der", "V\u00f6l\u00b7cker", "Herz", "er\u00b7lan\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "APPR", "NN", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201ekomm O gekr\u00f6nter Tag! ..... Indessen nahm ich wahr", "tokens": ["\u201e", "komm", "O", "ge\u00b7kr\u00f6n\u00b7ter", "Tag", "!", ".....", "In\u00b7des\u00b7sen", "nahm", "ich", "wahr"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "NE", "ADJA", "NN", "$.", "$(", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df dieses Wunsch-Gespr\u00e4ch Verwunderung gebar.", "tokens": ["Da\u00df", "die\u00b7ses", "WunschGe\u00b7spr\u00e4ch", "Ver\u00b7wun\u00b7de\u00b7rung", "ge\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Mir war die Sache neu; bi\u00df wieder andre kamen", "tokens": ["Mir", "war", "die", "Sa\u00b7che", "neu", ";", "bi\u00df", "wie\u00b7der", "and\u00b7re", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$.", "ADV", "ADV", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "260Die die\u00df erkl\u00e4reten und mir den Zweifel nahmen.", "tokens": ["die\u00df", "er\u00b7kl\u00e4\u00b7re\u00b7ten", "und", "mir", "den", "Zwei\u00b7fel", "nah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+---+-+-+-", "measure": "unknown.measure.penta"}}}}}