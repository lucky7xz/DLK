{"textgrid.poem.66092": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der rechte Ort", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein stiller Pfad", "tokens": ["Es", "ist", "ein", "stil\u00b7ler", "Pfad"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Entlang an Klee und Korn,", "tokens": ["Ent\u00b7lang", "an", "Klee", "und", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Furchen grub das schwere Rad;", "tokens": ["Wo", "Fur\u00b7chen", "grub", "das", "schwe\u00b7re", "Rad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gaisblatt wuchert am Rand, und Dorn.", "tokens": ["Gais\u00b7blatt", "wu\u00b7chert", "am", "Rand", ",", "und", "Dorn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Rings Farben, juliwarm,", "tokens": ["Rings", "Far\u00b7ben", ",", "ju\u00b7li\u00b7warm", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und reifer Roggenduft!", "tokens": ["Und", "rei\u00b7fer", "Rog\u00b7gen\u00b7duft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein tanzender M\u00fcckenschwarm", "tokens": ["Ein", "tan\u00b7zen\u00b7der", "M\u00fc\u00b7cken\u00b7schwarm"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Schwalben in zitternder Luft.", "tokens": ["Und", "Schwal\u00b7ben", "in", "zit\u00b7tern\u00b7der", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und um die gl\u00fche Mittagszeit", "tokens": ["Und", "um", "die", "gl\u00fc\u00b7he", "Mit\u00b7tags\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Bett im Heckenkraut,", "tokens": ["Ein", "Bett", "im", "He\u00b7cken\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weit", "tokens": ["Und", "weit"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Kein Menschenlaut.", "tokens": ["Kein", "Men\u00b7schen\u00b7laut", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}