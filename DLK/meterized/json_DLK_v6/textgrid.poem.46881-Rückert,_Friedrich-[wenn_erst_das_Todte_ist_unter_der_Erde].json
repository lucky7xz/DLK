{"textgrid.poem.46881": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn erst das Todte ist unter der Erde]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn erst das Todte ist unter der Erde,", "tokens": ["Wenn", "erst", "das", "Tod\u00b7te", "ist", "un\u00b7ter", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat das Lebende halbe Beschwerde.", "tokens": ["Hat", "das", "Le\u00b7ben\u00b7de", "hal\u00b7be", "Be\u00b7schwer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df es ruhn, es ist gut bedeckt,", "tokens": ["La\u00df", "es", "ruhn", ",", "es", "ist", "gut", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und glaube, da\u00df es wird aufgeweckt.", "tokens": ["Und", "glau\u00b7be", ",", "da\u00df", "es", "wird", "auf\u00b7ge\u00b7weckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist kein Schatten mehr im Raume,", "tokens": ["Es", "ist", "kein", "Schat\u00b7ten", "mehr", "im", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist ein Licht in deinem Traume.", "tokens": ["Es", "ist", "ein", "Licht", "in", "dei\u00b7nem", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und bist du ein Dichter, so kannst du's sehn", "tokens": ["Und", "bist", "du", "ein", "Dich\u00b7ter", ",", "so", "kannst", "du's", "sehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VMFIN", "PIS", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Jeden Fr\u00fchling in Blumen stehn.", "tokens": ["Je\u00b7den", "Fr\u00fch\u00b7ling", "in", "Blu\u00b7men", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}