{"textgrid.poem.57788": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Erwartung", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter der Linde", "tokens": ["Un\u00b7ter", "der", "Lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da ist mein allerliebster Platz,", "tokens": ["Da", "ist", "mein", "al\u00b7ler\u00b7liebs\u00b7ter", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da will ich warten", "tokens": ["Da", "will", "ich", "war\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Auf meinen Schatz.", "tokens": ["Auf", "mei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Liljen und Rosen", "tokens": ["Lil\u00b7jen", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die sind so wunderwundersch\u00f6n", "tokens": ["Die", "sind", "so", "wun\u00b7der\u00b7wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Gartentore", "tokens": ["Am", "Gar\u00b7ten\u00b7to\u00b7re"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da mu\u00df ich stehn.", "tokens": ["Da", "mu\u00df", "ich", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Nachtigallen", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die schlagen immerimmerzu,", "tokens": ["Die", "schla\u00b7gen", "im\u00b7me\u00b7rim\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es klopft mein Herze,", "tokens": ["Es", "klopft", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Gibt keine Ruh.", "tokens": ["Gibt", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Warten, ach warten", "tokens": ["War\u00b7ten", ",", "ach", "war\u00b7ten"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "XY", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Das kann ich nimmernimmermehr,", "tokens": ["Das", "kann", "ich", "nim\u00b7mer\u00b7nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach meinem Schatze", "tokens": ["Nach", "mei\u00b7nem", "Schat\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sehn' ich mich sehr.", "tokens": ["Sehn'", "ich", "mich", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}