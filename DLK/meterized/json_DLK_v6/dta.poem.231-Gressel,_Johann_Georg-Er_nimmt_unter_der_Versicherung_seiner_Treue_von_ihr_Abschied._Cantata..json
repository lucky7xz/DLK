{"dta.poem.231": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er nimmt unter der Versicherung seiner  \n Treue von ihr Abschied.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Adjeu Sch\u00f6nste! lebe wohl!", "tokens": ["Ad\u00b7jeu", "Sch\u00f6ns\u00b7te", "!", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibe meiner eingedenck.", "tokens": ["Blei\u00b7be", "mei\u00b7ner", "ein\u00b7ge\u00b7denck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "K\u00fcsse mich offt in Gedancken/", "tokens": ["K\u00fcs\u00b7se", "mich", "offt", "in", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Meine Treu/ die soll nicht wancken.", "tokens": ["Mei\u00b7ne", "Treu", "/", "die", "soll", "nicht", "wan\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So lang als ich athmen kan", "tokens": ["So", "lang", "als", "ich", "ath\u00b7men", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VVINF", "VMFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bett\u2019 ich deine Gottheit an", "tokens": ["Bett'", "ich", "dei\u00b7ne", "Got\u00b7theit", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Adjeu Sch\u00f6nste! lebe wohl!", "tokens": ["Ad\u00b7jeu", "Sch\u00f6ns\u00b7te", "!", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bleibe meiner eingedenck.", "tokens": ["Blei\u00b7be", "mei\u00b7ner", "ein\u00b7ge\u00b7denck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird schon der Leib entfernt", "tokens": ["Wird", "schon", "der", "Leib", "ent\u00b7fernt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bleibt doch die Seele da;", "tokens": ["Bleibt", "doch", "die", "See\u00b7le", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wo solche Sch\u00f6nheit sternt/", "tokens": ["Wo", "sol\u00b7che", "Sch\u00f6n\u00b7heit", "sternt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Bleiben/ ob der Leib von hinnen/", "tokens": ["Blei\u00b7ben", "/", "ob", "der", "Leib", "von", "hin\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "APPR", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Doch die Sinnen", "tokens": ["Doch", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Jhrem Angel-Sterne nah.", "tokens": ["Ih\u00b7rem", "An\u00b7gel\u00b7S\u00b7ter\u00b7ne", "nah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch Abwesend will ich brennen", "tokens": ["Auch", "Ab\u00b7we\u00b7send", "will", "ich", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sch\u00f6nste in der hellen Gluht;", "tokens": ["Sch\u00f6ns\u00b7te", "in", "der", "hel\u00b7len", "Gluht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Treue soll bestehen/", "tokens": ["Mei\u00b7ne", "Treu\u00b7e", "soll", "be\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nach steten Wachsthum sehen", "tokens": ["Und", "nach", "ste\u00b7ten", "Wach\u00b7sthum", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitten in der Neider-Wuht", "tokens": ["Mit\u00b7ten", "in", "der", "Nei\u00b7der\u00b7Wuht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch Abwesend will ich brennen", "tokens": ["Auch", "Ab\u00b7we\u00b7send", "will", "ich", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Sch\u00f6nste in der hellen Gluht.", "tokens": ["Sch\u00f6ns\u00b7te", "in", "der", "hel\u00b7len", "Gluht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}