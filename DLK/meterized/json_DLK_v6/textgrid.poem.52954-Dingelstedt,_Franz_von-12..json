{"textgrid.poem.52954": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sei geleert die bittre Abschieds-Schale,", "tokens": ["Nun", "sei", "ge\u00b7leert", "die", "bitt\u00b7re", "Ab\u00b7schieds\u00b7Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das harte Wort sei schonungslos gesprochen:", "tokens": ["Das", "har\u00b7te", "Wort", "sei", "scho\u00b7nungs\u00b7los", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Leb wohl, leb wohl! Auf Nimmerwiedersehen!", "tokens": ["Leb", "wohl", ",", "leb", "wohl", "!", "Auf", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "VVFIN", "ADV", "$.", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier k\u00fc\u00dft' ich Deinen Mund zum ersten Male,", "tokens": ["Hier", "k\u00fc\u00dft'", "ich", "Dei\u00b7nen", "Mund", "zum", "ers\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier werde auch der letzte Ku\u00df zerbrochen,", "tokens": ["Hier", "wer\u00b7de", "auch", "der", "letz\u00b7te", "Ku\u00df", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du bleib auf dieser Schwelle einsam stehen,", "tokens": ["Du", "bleib", "auf", "die\u00b7ser", "Schwel\u00b7le", "ein\u00b7sam", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mich lasse einsam gehen!", "tokens": ["Mich", "las\u00b7se", "ein\u00b7sam", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ja, Du bist gro\u00df! \u2013 Du hei\u00dfest ohne Z\u00e4hre", "tokens": ["Ja", ",", "Du", "bist", "gro\u00df", "!", "\u2013", "Du", "hei\u00b7\u00dfest", "oh\u00b7ne", "Z\u00e4h\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$.", "$(", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und ohne Klage mich von dannen ziehen;", "tokens": ["Und", "oh\u00b7ne", "Kla\u00b7ge", "mich", "von", "dan\u00b7nen", "zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PRF", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "O M\u00e4dchen, M\u00e4dchen, wenn es m\u00f6glich w\u00e4re \u2013 \u2013", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "M\u00e4d\u00b7chen", ",", "wenn", "es", "m\u00f6g\u00b7lich", "w\u00e4\u00b7re", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nein, es ist nicht. Du wei\u00dft es, ich mu\u00df fliehen.", "tokens": ["Nein", ",", "es", "ist", "nicht", ".", "Du", "wei\u00dft", "es", ",", "ich", "mu\u00df", "flie\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und dies das Letzte was ich Dir geschrieben:", "tokens": ["Und", "dies", "das", "Letz\u00b7te", "was", "ich", "Dir", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ART", "ADJA", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Du hast geliebt \u2013 Ich werde nimmer lieben!", "tokens": ["Du", "hast", "ge\u00b7liebt", "\u2013", "Ich", "wer\u00b7de", "nim\u00b7mer", "lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}