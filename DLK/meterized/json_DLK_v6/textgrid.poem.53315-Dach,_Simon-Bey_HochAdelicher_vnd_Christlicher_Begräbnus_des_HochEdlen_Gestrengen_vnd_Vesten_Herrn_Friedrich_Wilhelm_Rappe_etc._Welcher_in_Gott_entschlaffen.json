{"textgrid.poem.53315": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Bey HochAdelicher vnd Christlicher Begr\u00e4bnus des HochEdlen Gestrengen vnd Vesten Herrn Friedrich Wilhelm Rappe etc. Welcher in Gott entschlaffen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich das Elend vnd Beschwehr", "tokens": ["Soll", "ich", "das", "E\u00b7lend", "vnd", "Be\u00b7schwehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens satt beweinen,", "tokens": ["Des", "Le\u00b7bens", "satt", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo nehm' ich alle Thr\u00e4nen her?", "tokens": ["Wo", "nehm'", "ich", "al\u00b7le", "Thr\u00e4\u00b7nen", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer ist es, ich weis keinen,", "tokens": ["Wer", "ist", "es", ",", "ich", "weis", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PPER", "PTKVZ", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der nicht von Hoffnung, furcht vnd noth", "tokens": ["Der", "nicht", "von", "Hoff\u00b7nung", ",", "furcht", "vnd", "noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PTKNEG", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verfolget sey bis in den Todt.", "tokens": ["Ver\u00b7fol\u00b7get", "sey", "bis", "in", "den", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind unser unsre M\u00fctter nicht", "tokens": ["Sind", "un\u00b7ser", "uns\u00b7re", "M\u00fct\u00b7ter", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Angst vnd Weh genesen?", "tokens": ["In", "Angst", "vnd", "Weh", "ge\u00b7ne\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ja unser erstes Werck vnd Pflicht", "tokens": ["Ja", "un\u00b7ser", "ers\u00b7tes", "Werck", "vnd", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Weinen nur gewesen.", "tokens": ["Ist", "Wei\u00b7nen", "nur", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Kindheit wird gantz ohn bedacht", "tokens": ["Die", "Kind\u00b7heit", "wird", "gantz", "ohn", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In lauter Thorheit zugebracht.", "tokens": ["In", "lau\u00b7ter", "Thor\u00b7heit", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Jugend l\u00e4st die Zucht nicht Rhue,", "tokens": ["Der", "Ju\u00b7gend", "l\u00e4st", "die", "Zucht", "nicht", "Rhue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Mann ist von dem Morgen", "tokens": ["Ein", "Mann", "ist", "von", "dem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis auff den sp\u00e4ten Abend zu", "tokens": ["Bis", "auff", "den", "sp\u00e4\u00b7ten", "A\u00b7bend", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Arbeit, M\u00fch' vnd Sorgen.", "tokens": ["In", "Ar\u00b7beit", ",", "M\u00fch'", "vnd", "Sor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Alter wohnet mancherley", "tokens": ["Dem", "Al\u00b7ter", "woh\u00b7net", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Furcht, Argwohn, Geitz vnd Kranckheit bey.", "tokens": ["Furcht", ",", "Arg\u00b7wohn", ",", "Geitz", "vnd", "Kran\u00b7ck\u00b7heit", "bey", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Vnd \u00fcber alles Vngemach", "tokens": ["Vnd", "\u00fc\u00b7ber", "al\u00b7les", "Vn\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sind wir keiner Zeiten", "tokens": ["So", "sind", "wir", "kei\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Todte frey, der stellt uns nach", "tokens": ["Vom", "Tod\u00b7te", "frey", ",", "der", "stellt", "uns", "nach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch List von allen Seiten,", "tokens": ["Durch", "List", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "W\u00fcrgt Alte, Kinder, Jugend, Mann,", "tokens": ["W\u00fcrgt", "Al\u00b7te", ",", "Kin\u00b7der", ",", "Ju\u00b7gend", ",", "Mann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ohn Vnterscheid vnd wie er kan.", "tokens": ["Ohn", "Vn\u00b7ter\u00b7scheid", "vnd", "wie", "er", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was sag' ich von der S\u00fcnden viel?", "tokens": ["Was", "sag'", "ich", "von", "der", "S\u00fcn\u00b7den", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die liegt uns im Gewissen,", "tokens": ["Die", "liegt", "uns", "im", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie werden wir ohn Maa\u00df vnd Ziel", "tokens": ["Wie", "wer\u00b7den", "wir", "ohn", "Maa\u00df", "vnd", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch jhren Mord gebissen!", "tokens": ["Durch", "jhren", "Mord", "ge\u00b7bis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "So wild wird keine See bewegt", "tokens": ["So", "wild", "wird", "kei\u00b7ne", "See", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wir, im fall ihr Wurm sich regt.", "tokens": ["Als", "wir", ",", "im", "fall", "ihr", "Wurm", "sich", "reg\u00b7t."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "PPOSAT", "NN", "PRF", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wer dieses wol zu Hertzen fasst,", "tokens": ["Wer", "die\u00b7ses", "wol", "zu", "Hert\u00b7zen", "fasst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie solt' er nicht der Erden,", "tokens": ["Wie", "solt'", "er", "nicht", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Angst vnd Pein vnd aller Last", "tokens": ["Der", "Angst", "vnd", "Pein", "vnd", "al\u00b7ler", "Last"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Befreyet wollen werden,", "tokens": ["Be\u00b7fre\u00b7yet", "wol\u00b7len", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu kriegen fur dies HertzeLeid", "tokens": ["Zu", "krie\u00b7gen", "fur", "dies", "Hert\u00b7ze", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die wahre Rhue vnd Seeligkeit:", "tokens": ["Die", "wah\u00b7re", "Rhue", "vnd", "See\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort, wo kein Vnmuth hingelangt,", "tokens": ["Dort", ",", "wo", "kein", "Vn\u00b7muth", "hin\u00b7ge\u00b7langt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Gn\u00fcg' vnd Wollust schweben", "tokens": ["Wo", "Gn\u00fcg'", "vnd", "Wol\u00b7lust", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wo die Schaar der Frommen prangt", "tokens": ["Vnd", "wo", "die", "Schaar", "der", "From\u00b7men", "prangt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Herrlichheit umbgeben,", "tokens": ["Mit", "Herr\u00b7lich\u00b7heit", "umb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo Freundschaft ohn Betrug vnd List", "tokens": ["Wo", "Freund\u00b7schaft", "ohn", "Be\u00b7trug", "vnd", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd GOTT in allem alles ist?", "tokens": ["Vnd", "GoTT", "in", "al\u00b7lem", "al\u00b7les", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PIS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da stehen umb des H\u00f6chsten Thron", "tokens": ["Da", "ste\u00b7hen", "umb", "des", "H\u00f6chs\u00b7ten", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00e4ter vnd Propheten,", "tokens": ["Die", "V\u00e4\u00b7ter", "vnd", "Pro\u00b7phe\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor allen, Isai, dein Sohn,", "tokens": ["Vor", "al\u00b7len", ",", "I\u00b7sai", ",", "dein", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Der Vater der Poeten,", "tokens": ["Der", "Va\u00b7ter", "der", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die stimmen s\u00e4mptlich hell vnd rein", "tokens": ["Die", "stim\u00b7men", "s\u00e4mpt\u00b7lich", "hell", "vnd", "rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit vielen tausent Engeln ein.", "tokens": ["Mit", "vie\u00b7len", "tau\u00b7sent", "En\u00b7geln", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ihr Lob-Spruch ist: Herr Zebaoth,", "tokens": ["Ihr", "Lob\u00b7Spruch", "ist", ":", "Herr", "Ze\u00b7bao\u00b7th", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "NN", "NE", "$,"], "meter": "-+++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "O wer kan Dich ergr\u00fcnden?", "tokens": ["O", "wer", "kan", "Dich", "er\u00b7gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Heilig, Heilig, Heilig, GOTT", "tokens": ["Wie", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "Hei\u00b7lig", ",", "GoTT"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "$,", "ADJA", "$,", "ADJA", "$,", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist Du doch zu befinden!", "tokens": ["Bist", "Du", "doch", "zu", "be\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Himmels vnd der Erden Krei\u00df", "tokens": ["Des", "Him\u00b7mels", "vnd", "der", "Er\u00b7den", "Krei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist viel zu klein f\u00fcr deinen Preis.", "tokens": ["Ist", "viel", "zu", "klein", "f\u00fcr", "dei\u00b7nen", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wer etwas von Erk\u00e4ntn\u00fcs hat,", "tokens": ["Wer", "et\u00b7was", "von", "Er\u00b7k\u00e4nt\u00b7n\u00fcs", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sich aus diesen Thr\u00e4nen", "tokens": ["Wird", "sich", "aus", "die\u00b7sen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort hin, in Gottes wehrte Stadt,", "tokens": ["Dort", "hin", ",", "in", "Got\u00b7tes", "wehr\u00b7te", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das wahre Zion, sehnen,", "tokens": ["Das", "wah\u00b7re", "Zi\u00b7on", ",", "seh\u00b7nen", ","], "token_info