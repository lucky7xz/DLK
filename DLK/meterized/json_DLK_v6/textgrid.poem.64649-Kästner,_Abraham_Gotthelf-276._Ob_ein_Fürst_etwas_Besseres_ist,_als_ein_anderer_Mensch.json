{"textgrid.poem.64649": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "276. Ob ein F\u00fcrst etwas Besseres ist, als ein anderer Mensch", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihn mit Weisheit und mit Macht,", "tokens": ["Wenn", "ihn", "mit", "Weis\u00b7heit", "und", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Gl\u00fcck von Tausenden sein Leben ganz zu weyh'n,", "tokens": ["Dem", "Gl\u00fcck", "von", "Tau\u00b7sen\u00b7den", "sein", "Le\u00b7ben", "ganz", "zu", "wey\u00b7h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Vorsicht w\u00e4hlt: sollt' er nichts Bessers seyn,", "tokens": ["Die", "Vor\u00b7sicht", "w\u00e4hlt", ":", "sollt'", "er", "nichts", "Bes\u00b7sers", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VMFIN", "PPER", "PIAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als einer nur von den'n, f\u00fcr die er wacht?", "tokens": ["Als", "ei\u00b7ner", "nur", "von", "den'n", ",", "f\u00fcr", "die", "er", "wacht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "APPR", "NE", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}