{"textgrid.poem.46247": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ihre sch\u00f6nheit von rosen und gilgen", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In lieblichem geruch auf frischem gr\u00fcnen thron,", "tokens": ["In", "lieb\u00b7li\u00b7chem", "ge\u00b7ruch", "auf", "fri\u00b7schem", "gr\u00fc\u00b7nen", "thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den tausend Liebelein (aufwartend) allzeit zieren,", "tokens": ["den", "tau\u00b7send", "Lie\u00b7be\u00b7lein", "(", "auf\u00b7war\u00b7tend", ")", "all\u00b7zeit", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$(", "VVPP", "$(", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "erhube sich die ros mit l\u00e4chelnd s\u00fc\u00dfem hohn,", "tokens": ["er\u00b7hu\u00b7be", "sich", "die", "ros", "mit", "l\u00e4\u00b7chelnd", "s\u00fc\u00b7\u00dfem", "hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NE", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als blumenkeiserin fr\u00f6lich zu triumfieren;", "tokens": ["als", "blu\u00b7men\u00b7kei\u00b7se\u00b7rin", "fr\u00f6\u00b7lich", "zu", "tri\u00b7um\u00b7fie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "In reicher mayestet, gleichlos in ihrem wohn,", "tokens": ["In", "rei\u00b7cher", "ma\u00b7ye\u00b7stet", ",", "gleich\u00b7los", "in", "ih\u00b7rem", "wohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "mit unbeflecktem pracht lie\u00df sich die gilg auff\u00fchren,", "tokens": ["mit", "un\u00b7be\u00b7fleck\u00b7tem", "pracht", "lie\u00df", "sich", "die", "gilg", "auf\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "vermeinend, demnach ihr allein geb\u00fchr die kron", "tokens": ["ver\u00b7mei\u00b7nend", ",", "dem\u00b7nach", "ihr", "al\u00b7lein", "ge\u00b7b\u00fchr", "die", "kron"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "als k\u00f6nigin das land der blumen zu regieren;", "tokens": ["als", "k\u00f6\u00b7ni\u00b7gin", "das", "land", "der", "blu\u00b7men", "zu", "re\u00b7gie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Alsbald bewegte sich beeder princessin scho\u00df", "tokens": ["Als\u00b7bald", "be\u00b7weg\u00b7te", "sich", "bee\u00b7der", "prin\u00b7ces\u00b7sin", "scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "durch eifer und hochmut, der oft die sch\u00f6nheit qu\u00e4let;", "tokens": ["durch", "ei\u00b7fer", "und", "hoch\u00b7mut", ",", "der", "oft", "die", "sch\u00f6n\u00b7heit", "qu\u00e4\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJD", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "sie fangen an den streit und sparen kein gescho\u00df:", "tokens": ["sie", "fan\u00b7gen", "an", "den", "streit", "und", "spa\u00b7ren", "kein", "ge\u00b7scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jedoch ihr ha\u00df in lieb (weil Amors rat nicht fehlet)", "tokens": ["Je\u00b7doch", "ihr", "ha\u00df", "in", "lieb", "(", "weil", "A\u00b7mors", "rat", "nicht", "feh\u00b7let", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "ADJD", "$(", "KOUS", "NE", "VVFIN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "verkehret, hat zuletzt zugleich die gilg und ros", "tokens": ["ver\u00b7keh\u00b7ret", ",", "hat", "zu\u00b7letzt", "zu\u00b7gleich", "die", "gilg", "und", "ros"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "auf euerm angesicht zu prachtieren verm\u00e4hlet.", "tokens": ["auf", "eu\u00b7erm", "an\u00b7ge\u00b7sicht", "zu", "prach\u00b7tie\u00b7ren", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVPP", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}