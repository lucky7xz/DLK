{"textgrid.poem.40368": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Zum Geburtstag", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebe Mutter! Du z\u00e4hlst sie gerne,", "tokens": ["Lie\u00b7be", "Mut\u00b7ter", "!", "Du", "z\u00e4hlst", "sie", "ger\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "deine dreiunddrei\u00dfig Geburtstagssterne.", "tokens": ["dei\u00b7ne", "drei\u00b7und\u00b7drei\u00b7\u00dfig", "Ge\u00b7burts\u00b7tags\u00b7ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "CARD", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Hier stehn sie strahlend; und daneben", "tokens": ["Hier", "stehn", "sie", "strah\u00b7lend", ";", "und", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$.", "KON", "PAV"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "siehst du zwei silberne Halbmonde schweben.", "tokens": ["siehst", "du", "zwei", "sil\u00b7ber\u00b7ne", "Halb\u00b7mon\u00b7de", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Das sind zwei Lampen f\u00fcrs Klavier,", "tokens": ["Das", "sind", "zwei", "Lam\u00b7pen", "f\u00fcrs", "Kla\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.6": {"text": "eine von Vater, die andre von mir.", "tokens": ["ei\u00b7ne", "von", "Va\u00b7ter", ",", "die", "and\u00b7re", "von", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "PRELS", "PIS", "APPR", "PPER", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Kommt nun der Abend mit m\u00fcden Beinen,", "tokens": ["Kommt", "nun", "der", "A\u00b7bend", "mit", "m\u00fc\u00b7den", "Bei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "dann l\u00e4\u00dft du deine Monde scheinen", "tokens": ["dann", "l\u00e4\u00dft", "du", "dei\u00b7ne", "Mon\u00b7de", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "und spielst; und wir, wir h\u00f6ren und tr\u00e4umen", "tokens": ["und", "spielst", ";", "und", "wir", ",", "wir", "h\u00f6\u00b7ren", "und", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KON", "PPER", "$,", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "von den hohen himmlischen R\u00e4umen,", "tokens": ["von", "den", "ho\u00b7hen", "himm\u00b7li\u00b7schen", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "von deinem Sternenringelreihn \u2013", "tokens": ["von", "dei\u00b7nem", "Ster\u00b7nen\u00b7rin\u00b7gel\u00b7reihn", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vater wacht noch, ich schlafe ein.", "tokens": ["Va\u00b7ter", "wacht", "noch", ",", "ich", "schla\u00b7fe", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}