{"textgrid.poem.58214": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "252. Lasset euer Licht leuchten!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in sich selbst entbl\u00f6\u00dft auf Gott alleine sicht,", "tokens": ["Wer", "in", "sich", "selbst", "ent\u00b7bl\u00f6\u00dft", "auf", "Gott", "al\u00b7lei\u00b7ne", "sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "ADV", "VVFIN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann als ein Tugendlicht zu Gottes Ehre prangen;", "tokens": ["Kann", "als", "ein", "Tu\u00b7gend\u00b7licht", "zu", "Got\u00b7tes", "Eh\u00b7re", "pran\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOKOM", "ART", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer seine Tugend schaut, der ist der Fr\u00f6mmste nicht,", "tokens": ["Wer", "sei\u00b7ne", "Tu\u00b7gend", "schaut", ",", "der", "ist", "der", "Fr\u00f6mms\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus unserm Nichts kann Gott den besten Ruhm erlangen.", "tokens": ["Aus", "un\u00b7serm", "Nichts", "kann", "Gott", "den", "bes\u00b7ten", "Ruhm", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}