{"dta.poem.646": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eines Flohes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich lange Zeit die weisse Brust bewacht/", "tokens": ["Nach\u00b7dem", "ich", "lan\u00b7ge", "Zeit", "die", "weis\u00b7se", "Brust", "be\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kahm ich an den Ort/ woselbst die Anmuth lacht/", "tokens": ["So", "kahm", "ich", "an", "den", "Ort", "/", "wo\u00b7selbst", "die", "An\u00b7muth", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN", "$(", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein den Vorwitz must ich mit dem Leben b\u00fcssen", "tokens": ["Al\u00b7lein", "den", "Vor\u00b7witz", "must", "ich", "mit", "dem", "Le\u00b7ben", "b\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn allda wil man nichts als grosse Stachel wissen.", "tokens": ["Denn", "all\u00b7da", "wil", "man", "nichts", "als", "gros\u00b7se", "Sta\u00b7chel", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "PIS", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}