{"textgrid.poem.26526": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das aber ist das Traurigste: zu sehen,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das aber ist das Traurigste: zu sehen,", "tokens": ["Das", "a\u00b7ber", "ist", "das", "Trau\u00b7rigs\u00b7te", ":", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$.", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie tief die Menschheit wurzelt im Gemeinen,", "tokens": ["Wie", "tief", "die", "Menschheit", "wur\u00b7zelt", "im", "Ge\u00b7mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Thaten, die uns hier die h\u00f6chsten scheinen,", "tokens": ["Wie", "Tha\u00b7ten", ",", "die", "uns", "hier", "die", "h\u00f6chs\u00b7ten", "schei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zumeist aus nied'rem Antrieb nur geschehen.", "tokens": ["Zu\u00b7meist", "aus", "nie\u00b7d'\u00b7rem", "An\u00b7trieb", "nur", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie es die Besten selbst so schwer verstehen,", "tokens": ["Wie", "es", "die", "Bes\u00b7ten", "selbst", "so", "schwer", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df man nur sch\u00f6pfen d\u00fcrfe aus dem Reinen,", "tokens": ["Da\u00df", "man", "nur", "sch\u00f6p\u00b7fen", "d\u00fcr\u00b7fe", "aus", "dem", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVINF", "VMFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie es giebt von Tausenden kaum Einen,", "tokens": ["Und", "wie", "es", "giebt", "von", "Tau\u00b7sen\u00b7den", "kaum", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "NN", "ADV", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sich den eig'nen Vortheil l\u00e4\u00dft entgehen.", "tokens": ["Der", "sich", "den", "eig'\u00b7nen", "Vor\u00b7theil", "l\u00e4\u00dft", "ent\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und so geschieht es, da\u00df in diesem Leben", "tokens": ["Und", "so", "ge\u00b7schieht", "es", ",", "da\u00df", "in", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein hoher Sinn gereicht zu Hohn und Schande,", "tokens": ["Ein", "ho\u00b7her", "Sinn", "ge\u00b7reicht", "zu", "Hohn", "und", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ward des Erfolges Glanz ihm nicht gegeben.", "tokens": ["Ward", "des", "Er\u00b7fol\u00b7ges", "Glanz", "ihm", "nicht", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und so geschieht's auch, da\u00df sich bis zum Rande", "tokens": ["Und", "so", "ge\u00b7schieht's", "auch", ",", "da\u00df", "sich", "bis", "zum", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NE", "ADV", "$,", "KOUS", "PRF", "APPR", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gewinnsucht f\u00fcllt, inde\u00df ein selbstlos Streben", "tokens": ["Ge\u00b7winn\u00b7sucht", "f\u00fcllt", ",", "in\u00b7de\u00df", "ein", "selbst\u00b7los", "Stre\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versiegen mu\u00df, so wie der Quell im Sande.", "tokens": ["Ver\u00b7sie\u00b7gen", "mu\u00df", ",", "so", "wie", "der", "Quell", "im", "San\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}