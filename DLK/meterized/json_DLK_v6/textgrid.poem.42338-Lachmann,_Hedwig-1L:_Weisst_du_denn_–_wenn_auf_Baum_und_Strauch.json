{"textgrid.poem.42338": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weisst du denn \u2013 wenn auf Baum und Strauch", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weisst du denn \u2013 wenn auf Baum und Strauch", "tokens": ["Weisst", "du", "denn", "\u2013", "wenn", "auf", "Baum", "und", "Strauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$(", "KOUS", "APPR", "NE", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Das Astwerk zittert und sich str\u00e4ubt,", "tokens": ["Das", "Ast\u00b7werk", "zit\u00b7tert", "und", "sich", "str\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn der leicht gewellte Rauch", "tokens": ["Und", "wenn", "der", "leicht", "ge\u00b7well\u00b7te", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An einer Wetterwand zerst\u00e4ubt \u2013", "tokens": ["An", "ei\u00b7ner", "Wet\u00b7ter\u00b7wand", "zer\u00b7st\u00e4ubt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein scheuer Vogel ohne Laut", "tokens": ["Ein", "scheu\u00b7er", "Vo\u00b7gel", "oh\u00b7ne", "Laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dir vorbei die Fl\u00fcgel schl\u00e4gt,", "tokens": ["An", "dir", "vor\u00b7bei", "die", "Fl\u00fc\u00b7gel", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Wolke sich an Wolke baut \u2013", "tokens": ["Und", "Wol\u00b7ke", "sich", "an", "Wol\u00b7ke", "baut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin dein wilder Wunsch dich tr\u00e4gt?", "tokens": ["Wo\u00b7hin", "dein", "wil\u00b7der", "Wunsch", "dich", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Weisst du denn, wenn nun alle Welt", "tokens": ["Weisst", "du", "denn", ",", "wenn", "nun", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich eng an Hof und Heimstatt schmiegt,", "tokens": ["Sich", "eng", "an", "Hof", "und", "Heims\u00b7tatt", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deine Sehnsucht dich bef\u00e4llt, \u2013", "tokens": ["Und", "dei\u00b7ne", "Sehn\u00b7sucht", "dich", "be\u00b7f\u00e4llt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo deine eigne Heimat liegt?", "tokens": ["Wo", "dei\u00b7ne", "eig\u00b7ne", "Hei\u00b7mat", "liegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}