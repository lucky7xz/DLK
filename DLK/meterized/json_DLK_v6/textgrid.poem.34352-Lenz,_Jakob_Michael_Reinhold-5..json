{"textgrid.poem.34352": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verge\u00dfen Constantin von dir! \u2013", "tokens": ["Ver\u00b7ge\u00b7\u00dfen", "Con\u00b7stan\u00b7tin", "von", "dir", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "APPR", "PPER", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ja ja, die Sonne schien heut hier,", "tokens": ["Ja", "ja", ",", "die", "Son\u00b7ne", "schien", "heut", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PTKANT", "$,", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lie\u00df keine Spur zur\u00fccke!", "tokens": ["Lie\u00df", "kei\u00b7ne", "Spur", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach deiner Liebe Schw\u00e4rmereyn", "tokens": ["Ach", "dei\u00b7ner", "Lie\u00b7be", "Schw\u00e4r\u00b7me\u00b7reyn"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gehn unter wie der Sonnenschein,", "tokens": ["Gehn", "un\u00b7ter", "wie", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihm all mein Gl\u00fccke!", "tokens": ["Mit", "ihm", "all", "mein", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}