{"dta.poem.19999": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Auf Rosas Tod im Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie waren Ros' und Lilie deine Freude,", "tokens": ["Wie", "wa\u00b7ren", "Ros'", "und", "Li\u00b7lie", "dei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NE", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und all' die Kinder stiller Blumenauen!", "tokens": ["Und", "all'", "die", "Kin\u00b7der", "stil\u00b7ler", "Blu\u00b7me\u00b7nau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warst selbsten eine Rose anzuschauen,", "tokens": ["Warst", "selbs\u00b7ten", "ei\u00b7ne", "Ro\u00b7se", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einfach erbl\u00fcht auf dufterf\u00fcllter Haide.", "tokens": ["Ein\u00b7fach", "er\u00b7bl\u00fcht", "auf", "duf\u00b7ter\u00b7f\u00fcll\u00b7ter", "Hai\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Blume in der Unschuld holdem Kleide!", "tokens": ["O", "Blu\u00b7me", "in", "der", "Un\u00b7schuld", "hol\u00b7dem", "Klei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O zartes Bildni\u00df lieber deutscher Frauen!", "tokens": ["O", "zar\u00b7tes", "Bild\u00b7ni\u00df", "lie\u00b7ber", "deut\u00b7scher", "Frau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit andern Blumen, angeweht von rauhen", "tokens": ["Mit", "an\u00b7dern", "Blu\u00b7men", ",", "an\u00b7ge\u00b7weht", "von", "rau\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herbstl\u00fcften, schiedest du, weh! uns zum Leide!", "tokens": ["Herbst\u00b7l\u00fcf\u00b7ten", ",", "schie\u00b7dest", "du", ",", "weh", "!", "uns", "zum", "Lei\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "PTKVZ", "$.", "PPER", "APPRART", "NN", "$."], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Wohl kommt der Lenz mit neuen Blumen wieder,", "tokens": ["Wohl", "kommt", "der", "Lenz", "mit", "neu\u00b7en", "Blu\u00b7men", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch solche Blumen bringt er, ach nur selten!", "tokens": ["Doch", "sol\u00b7che", "Blu\u00b7men", "bringt", "er", ",", "ach", "nur", "sel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "$,", "ITJ", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihre Bl\u00fcthe ist von kurzer Dauer.", "tokens": ["Und", "ih\u00b7re", "Bl\u00fc\u00b7the", "ist", "von", "kur\u00b7zer", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Aus seel'gen Sternen tr\u00e4gt er sie hernieder,", "tokens": ["Aus", "seel'\u00b7gen", "Ster\u00b7nen", "tr\u00e4gt", "er", "sie", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entkeimt dem Morgenrothe bessrer Welten,", "tokens": ["Ent\u00b7keimt", "dem", "Mor\u00b7gen\u00b7ro\u00b7the", "bess\u00b7rer", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erstickt sie bald der Erde kalter Schauer.", "tokens": ["Er\u00b7stickt", "sie", "bald", "der", "Er\u00b7de", "kal\u00b7ter", "Schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}