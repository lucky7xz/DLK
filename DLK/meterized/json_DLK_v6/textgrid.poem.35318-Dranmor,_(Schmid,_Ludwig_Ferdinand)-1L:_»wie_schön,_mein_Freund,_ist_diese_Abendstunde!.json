{"textgrid.poem.35318": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwie sch\u00f6n, mein Freund, ist diese Abendstunde!", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie sch\u00f6n, mein Freund, ist diese Abendstunde!", "tokens": ["\u00bb", "wie", "sch\u00f6n", ",", "mein", "Freund", ",", "ist", "die\u00b7se", "A\u00b7bends\u00b7tun\u00b7de", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O komm und h\u00e4nge keinen Grillen nach;", "tokens": ["O", "komm", "und", "h\u00e4n\u00b7ge", "kei\u00b7nen", "Gril\u00b7len", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Feld und Garten machen wir die Runde.\u00ab", "tokens": ["Durch", "Feld", "und", "Gar\u00b7ten", "ma\u00b7chen", "wir", "die", "Run\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie fa\u00dfte l\u00e4chelnd seine Hand und sprach:", "tokens": ["Sie", "fa\u00df\u00b7te", "l\u00e4\u00b7chelnd", "sei\u00b7ne", "Hand", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u00bbwie sch\u00f6n, mein Freund, ist diese Abendstunde!\u00ab", "tokens": ["\u00bb", "wie", "sch\u00f6n", ",", "mein", "Freund", ",", "ist", "die\u00b7se", "A\u00b7bends\u00b7tun\u00b7de", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er dachte: Was sind Stunden, Tage, Wochen?", "tokens": ["Er", "dach\u00b7te", ":", "Was", "sind", "Stun\u00b7den", ",", "Ta\u00b7ge", ",", "Wo\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was hoffen wir mit jedem Atemzug?", "tokens": ["Was", "hof\u00b7fen", "wir", "mit", "je\u00b7dem", "A\u00b7tem\u00b7zug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Herz, ein liebend Herz ist bald gebrochen,", "tokens": ["Ein", "Herz", ",", "ein", "lie\u00b7bend", "Herz", "ist", "bald", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tod gewi\u00df und rasch der Zeiten Flug.", "tokens": ["Der", "Tod", "ge\u00b7wi\u00df", "und", "rasch", "der", "Zei\u00b7ten", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er dachte: Was sind Stunden, Tage, Wochen?", "tokens": ["Er", "dach\u00b7te", ":", "Was", "sind", "Stun\u00b7den", ",", "Ta\u00b7ge", ",", "Wo\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wen trifft das Los zuerst, wen von uns beiden?", "tokens": ["Wen", "trifft", "das", "Los", "zu\u00b7erst", ",", "wen", "von", "uns", "bei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$,", "PWS", "APPR", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann sehn wir uns zum allerletzten mal?", "tokens": ["Wann", "sehn", "wir", "uns", "zum", "al\u00b7ler\u00b7letz\u00b7ten", "mal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer tr\u00f6stet dich in deinen Todesleiden?", "tokens": ["Wer", "tr\u00f6s\u00b7tet", "dich", "in", "dei\u00b7nen", "To\u00b7des\u00b7lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer tr\u00f6stet mich? \u2013 O R\u00e4tsel voller Qual! \u2013", "tokens": ["Wer", "tr\u00f6s\u00b7tet", "mich", "?", "\u2013", "O", "R\u00e4t\u00b7sel", "vol\u00b7ler", "Qual", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$(", "NE", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wen trifft das Los zuerst, wen von uns beiden?", "tokens": ["Wen", "trifft", "das", "Los", "zu\u00b7erst", ",", "wen", "von", "uns", "bei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$,", "PWS", "APPR", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn ich, Geliebte, dir die Augen schl\u00f6sse,", "tokens": ["Wenn", "ich", ",", "Ge\u00b7lieb\u00b7te", ",", "dir", "die", "Au\u00b7gen", "schl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die treuen Augen, holde Dulderin,", "tokens": ["Die", "treu\u00b7en", "Au\u00b7gen", ",", "hol\u00b7de", "Dul\u00b7de\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft es wohl, mit meinen Thr\u00e4nen fl\u00f6sse", "tokens": ["Du", "wei\u00dft", "es", "wohl", ",", "mit", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "fl\u00f6s\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch jede Hoffnung, jeder Trost dahin, \u2013", "tokens": ["Auch", "je\u00b7de", "Hoff\u00b7nung", ",", "je\u00b7der", "Trost", "da\u00b7hin", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "NN", "PAV", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ich, Geliebte, dir die Augen schl\u00f6sse.", "tokens": ["Wenn", "ich", ",", "Ge\u00b7lieb\u00b7te", ",", "dir", "die", "Au\u00b7gen", "schl\u00f6s\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NN", "$,", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch bringen sie des Gatten Totenbahre,", "tokens": ["Doch", "brin\u00b7gen", "sie", "des", "Gat\u00b7ten", "To\u00b7ten\u00b7bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df du, mein armes, schwaches Weib, entsetzt", "tokens": ["Da\u00df", "du", ",", "mein", "ar\u00b7mes", ",", "schwa\u00b7ches", "Weib", ",", "ent\u00b7setzt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich schicken mu\u00dft in \u00f6de Witwenjahre,", "tokens": ["Dich", "schi\u00b7cken", "mu\u00dft", "in", "\u00f6\u00b7de", "Wit\u00b7wen\u00b7jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich schluchzend fragen mu\u00dft: Und jetzt? Und jetzt? \u2013", "tokens": ["Dich", "schluch\u00b7zend", "fra\u00b7gen", "mu\u00dft", ":", "Und", "jetzt", "?", "Und", "jetzt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$.", "KON", "ADV", "$.", "KON", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch bringen sie des Gatten Totenbahre ...", "tokens": ["Doch", "brin\u00b7gen", "sie", "des", "Gat\u00b7ten", "To\u00b7ten\u00b7bah\u00b7re", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nein! Gott der Gnade, la\u00df es nicht geschehen!", "tokens": ["Nein", "!", "Gott", "der", "Gna\u00b7de", ",", "la\u00df", "es", "nicht", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "ART", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Himmel dringe meines Herzens Schrei:", "tokens": ["Zum", "Him\u00b7mel", "drin\u00b7ge", "mei\u00b7nes", "Her\u00b7zens", "Schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df, Vater, sie zuerst hin\u00fcbergehen!", "tokens": ["La\u00df", ",", "Va\u00b7ter", ",", "sie", "zu\u00b7erst", "hin\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch da\u00df sie elend und verlassen sei,", "tokens": ["Doch", "da\u00df", "sie", "e\u00b7lend", "und", "ver\u00b7las\u00b7sen", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Gott der Gnade, la\u00df es nicht geschehen!", "tokens": ["O", "Gott", "der", "Gna\u00b7de", ",", "la\u00df", "es", "nicht", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}