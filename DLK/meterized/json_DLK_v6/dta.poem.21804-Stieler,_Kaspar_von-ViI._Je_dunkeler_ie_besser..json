{"dta.poem.21804": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n  Je dunkeler/ ie besser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Hab' ich was der Nacht zu danken/", "tokens": ["Hab'", "ich", "was", "der", "Nacht", "zu", "dan\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PWS", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "gilt es dir drum/ F\u00f6be/ nicht.", "tokens": ["gilt", "es", "dir", "drum", "/", "F\u00f6\u00b7be", "/", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PAV", "$(", "NN", "$(", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deinetwegen/ Gramhafft Licht/", "tokens": ["Dei\u00b7net\u00b7we\u00b7gen", "/", "Gram\u00b7hafft", "Licht", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "h\u00e4tt\u2019 ich ewig m\u00fcssen kranken.", "tokens": ["h\u00e4tt'", "ich", "e\u00b7wig", "m\u00fcs\u00b7sen", "kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein verr\u00e4htrisch Silber-feuer", "tokens": ["Dein", "ver\u00b7r\u00e4h\u00b7trisch", "Sil\u00b7ber\u00b7feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hat mir offt geschnitten ab/", "tokens": ["hat", "mir", "offt", "ge\u00b7schnit\u00b7ten", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "was mir Venus willig gab/", "tokens": ["was", "mir", "Ve\u00b7nus", "wil\u00b7lig", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mir/ mir sonst verla\u00dfnem Freyer.", "tokens": ["mir", "/", "mir", "sonst", "ver\u00b7la\u00df\u00b7nem", "Frey\u00b7er", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Buhler suchen ihr Vergn\u00fcgen", "tokens": ["Buh\u00b7ler", "su\u00b7chen", "ihr", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in der stillen Finstern\u00fc\u00df/", "tokens": ["in", "der", "stil\u00b7len", "Fins\u00b7ter\u00b7n\u00fc\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "durch dich h\u00e4tt\u2019 ich nimmer di\u00df/", "tokens": ["durch", "dich", "h\u00e4tt'", "ich", "nim\u00b7mer", "di\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "PDS", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "was ich kriegte/ k\u00f6nnen kriegen.", "tokens": ["was", "ich", "krieg\u00b7te", "/", "k\u00f6n\u00b7nen", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$(", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nu du deinen Straal verborgen/", "tokens": ["Nu", "du", "dei\u00b7nen", "Straal", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und der Nebel dich umschlo\u00df\u2019", "tokens": ["und", "der", "Ne\u00b7bel", "dich", "um\u00b7schlo\u00df'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "hielt mich meiner Liebsten Scho\u00df", "tokens": ["hielt", "mich", "mei\u00b7ner", "Liebs\u00b7ten", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "eingeh\u00fcllet bi\u00df an Morgen.", "tokens": ["ein\u00b7ge\u00b7h\u00fcl\u00b7let", "bi\u00df", "an", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In den wild- und w\u00fcsten Gr\u00fcnden/", "tokens": ["In", "den", "wild", "und", "w\u00fcs\u00b7ten", "Gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "VVFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wo kein Mensche dich verrieth/", "tokens": ["wo", "kein", "Men\u00b7sche", "dich", "ver\u00b7rieth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "durffstu wol/ wie man dich sieht/", "tokens": ["durffs\u00b7tu", "wol", "/", "wie", "man", "dich", "sieht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "$(", "PWAV", "PIS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "beym Endimion dich finden.", "tokens": ["beym", "En\u00b7di\u00b7mi\u00b7on", "dich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wo die Neider Wache stehen/", "tokens": ["Wo", "die", "Nei\u00b7der", "Wa\u00b7che", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "k\u00f6mmt der Schatten mehr zu pa\u00df/", "tokens": ["k\u00f6mmt", "der", "Schat\u00b7ten", "mehr", "zu", "pa\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKZU", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "will ein Reisender/ so la\u00df", "tokens": ["will", "ein", "Rei\u00b7sen\u00b7der", "/", "so", "la\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "NN", "$(", "ADV", "VVIMP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ihn durch deine Blizze gehen.", "tokens": ["ihn", "durch", "dei\u00b7ne", "Bliz\u00b7ze", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Was ich w\u00fcntsche zu erjagen", "tokens": ["Was", "ich", "w\u00fcnt\u00b7sche", "zu", "er\u00b7ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kan ich fangen sonder Licht.", "tokens": ["kan", "ich", "fan\u00b7gen", "son\u00b7der", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinetwegen d\u00fcrffstu nicht", "tokens": ["Mei\u00b7net\u00b7we\u00b7gen", "d\u00fcrffs\u00b7tu", "nicht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gold an deinen Wangen tragen.", "tokens": ["Gold", "an", "dei\u00b7nen", "Wan\u00b7gen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wirstu aber ferner funkeln/", "tokens": ["Wirs\u00b7tu", "a\u00b7ber", "fer\u00b7ner", "fun\u00b7keln", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die soll dich an deiner Bahn", "tokens": ["die", "soll", "dich", "an", "dei\u00b7ner", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "auch in einem Hui verdunkeln.", "tokens": ["auch", "in", "ei\u00b7nem", "Hui", "ver\u00b7dun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "FM", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nacht/ du s\u00fcsse Nacht/ mein Leben/", "tokens": ["Nacht", "/", "du", "s\u00fcs\u00b7se", "Nacht", "/", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "ADJA", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leben/ Nacht/ du s\u00fcsse Nacht/", "tokens": ["Le\u00b7ben", "/", "Nacht", "/", "du", "s\u00fcs\u00b7se", "Nacht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "du hast mich vergn\u00fcgt gemacht/", "tokens": ["du", "hast", "mich", "ver\u00b7gn\u00fcgt", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ewig sey dir Dank gegeben", "tokens": ["e\u00b7wig", "sey", "dir", "Dank", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}