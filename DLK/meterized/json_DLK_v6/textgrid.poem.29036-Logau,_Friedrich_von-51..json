{"textgrid.poem.29036": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alsbald der Herr mir lacht, so lacht mir iederman;", "tokens": ["Als\u00b7bald", "der", "Herr", "mir", "lacht", ",", "so", "lacht", "mir", "ie\u00b7der\u00b7man", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siht sauer er mir zu, siht ieder so mich an.", "tokens": ["Siht", "sau\u00b7er", "er", "mir", "zu", ",", "siht", "ie\u00b7der", "so", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PPER", "PTKVZ", "$,", "VVFIN", "PIS", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Pupen machens so, die fremde Faust regirt;", "tokens": ["Die", "Pu\u00b7pen", "ma\u00b7chens", "so", ",", "die", "frem\u00b7de", "Faust", "re\u00b7girt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie stellen sich nach dem, nach dem sie einer f\u00fchrt.", "tokens": ["Sie", "stel\u00b7len", "sich", "nach", "dem", ",", "nach", "dem", "sie", "ei\u00b7ner", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "$,", "APPR", "PRELS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}