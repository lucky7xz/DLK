{"dta.poem.9094": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meinem gl\u00fchendsten Tulpenbaum", "tokens": ["In", "mei\u00b7nem", "gl\u00fc\u00b7hends\u00b7ten", "Tul\u00b7pen\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "tausend Bl\u00fcten!", "tokens": ["tau\u00b7send", "Bl\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Eine s\u00fcsse Stimme singt:", "tokens": ["Ei\u00b7ne", "s\u00fcs\u00b7se", "Stim\u00b7me", "singt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eblaue Fl\u00fcgel aus Perlmutter,", "tokens": ["\u201e", "blau\u00b7e", "Fl\u00fc\u00b7gel", "aus", "Perl\u00b7mut\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "als Hochzeitsbett ein Lilienblatt,", "tokens": ["als", "Hoch\u00b7zeits\u00b7bett", "ein", "Li\u00b7li\u00b7en\u00b7blatt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "eine ganz kleine Prinzessin!", "tokens": ["ei\u00b7ne", "ganz", "klei\u00b7ne", "Prin\u00b7zes\u00b7sin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Keiner kennt mich.", "tokens": ["Kei\u00b7ner", "kennt", "mich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Niemand weiss,", "tokens": ["Nie\u00b7mand", "weiss", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "wo mein Haus steht.", "tokens": ["wo", "mein", "Haus", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Sieben Regenbogenbr\u00fccken", "tokens": ["Sie\u00b7ben", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7br\u00fc\u00b7cken"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "funkeln zu ihm durch meinen Garten.", "tokens": ["fun\u00b7keln", "zu", "ihm", "durch", "mei\u00b7nen", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Wenn in deine Seele die Sonne scheint,", "tokens": ["Wenn", "in", "dei\u00b7ne", "See\u00b7le", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "besuch mich mal.", "tokens": ["be\u00b7such", "mich", "mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "H\u00f6rst du?\u201c", "tokens": ["H\u00f6rst", "du", "?", "\u201c"], "token_info": ["word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.9": {"line.1": {"text": "Starr,", "tokens": ["Starr", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "aus Schlangen gewunden,", "tokens": ["aus", "Schlan\u00b7gen", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "steht der Baum.", "tokens": ["steht", "der", "Baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Ein Windstoss r\u00fcttelt,", "tokens": ["Ein", "Winds\u00b7toss", "r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "wie tanzende Flammen wehn seine Bl\u00fcten!", "tokens": ["wie", "tan\u00b7zen\u00b7de", "Flam\u00b7men", "wehn", "sei\u00b7ne", "Bl\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}}}}}