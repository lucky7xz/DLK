{"textgrid.poem.28806": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sitzer und ein altes Weib (wie mu\u00df doch dieses kummen?)", "tokens": ["Der", "Sit\u00b7zer", "und", "ein", "al\u00b7tes", "Weib", "(", "wie", "mu\u00df", "doch", "die\u00b7ses", "kum\u00b7men", "?", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$(", "PWAV", "VMFIN", "ADV", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Sind auff lateinisch einerley, weil beyde gerne brummen.", "tokens": ["Sind", "auff", "la\u00b7tei\u00b7nisch", "ei\u00b7ner\u00b7ley", ",", "weil", "bey\u00b7de", "ger\u00b7ne", "brum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJD", "PTKVZ", "$,", "KOUS", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Iedoch ob di\u00df bedencklich ist, geschiehts vielleicht, dieweil", "tokens": ["Ie\u00b7doch", "ob", "di\u00df", "be\u00b7denck\u00b7lich", "ist", ",", "ge\u00b7schiehts", "viel\u00b7leicht", ",", "die\u00b7weil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "KOUS", "PDS", "ADJD", "VAFIN", "$,", "ADV", "ADV", "$,", "KOUS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Das garstig Alter billich ist der Jugend Hintertheil.", "tokens": ["Das", "gars\u00b7tig", "Al\u00b7ter", "bil\u00b7lich", "ist", "der", "Ju\u00b7gend", "Hin\u00b7ter\u00b7theil", "."], "token_info": ["word", "word", "word", "word", "wor