{"dta.poem.6264": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von einer solchen C\u00f6rper Menge,", "tokens": ["Von", "ei\u00b7ner", "sol\u00b7chen", "C\u00f6r\u00b7per", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nicht zu biegen,", "tokens": ["Die", "nicht", "zu", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und sich jedoch durch ihre L\u00e4nge", "tokens": ["Und", "sich", "je\u00b7doch", "durch", "ih\u00b7re", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Verwickeln und sich fest zusammen f\u00fcgen.", "tokens": ["Ver\u00b7wi\u00b7ckeln", "und", "sich", "fest", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PRF", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jhr starck gemeinsam Band giebt die Beschaffenheit,", "tokens": ["Ihr", "starck", "ge\u00b7mein\u00b7sam", "Band", "giebt", "die", "Be\u00b7schaf\u00b7fen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es kriegt ihr Glantz dadurch so Krafft, als H\u00e4rtigkeit:", "tokens": ["Es", "kriegt", "ihr", "Glantz", "da\u00b7durch", "so", "Krafft", ",", "als", "H\u00e4r\u00b7tig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PAV", "ADV", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es schneidet, theilt, durchdringet;", "tokens": ["Es", "schnei\u00b7det", ",", "theilt", ",", "durch\u00b7drin\u00b7get", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nichts widerstehet ihm. Cristall ist harte zwar,", "tokens": ["Nichts", "wi\u00b7der\u00b7ste\u00b7het", "ihm", ".", "Cris\u00b7tall", "ist", "har\u00b7te", "zwar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$.", "NN", "VAFIN", "ADJA", "ADV", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Allein, wenn man es stosst, zerbricht es leicht und springet.", "tokens": ["Al\u00b7lein", ",", "wenn", "man", "es", "stosst", ",", "zer\u00b7bricht", "es", "leicht", "und", "sprin\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Auch die Zerbrechlichkeit betrifft den Stahl so gar:", "tokens": ["Auch", "die", "Zer\u00b7brech\u00b7lich\u00b7keit", "be\u00b7tr\u00b7ifft", "den", "Stahl", "so", "gar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wie denn die Theil\u2019an ihm sich gantz geschwinde scheiden.", "tokens": ["Wie", "denn", "die", "Theil'\u00b7an", "ihm", "sich", "gantz", "ge\u00b7schwin\u00b7de", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NE", "PPER", "PRF", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die spr\u00f6de H\u00e4rte steckt in beyden,", "tokens": ["Die", "spr\u00f6\u00b7de", "H\u00e4r\u00b7te", "steckt", "in", "bey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "In langen C\u00f6rperchen, die auf einander liegen,", "tokens": ["In", "lan\u00b7gen", "C\u00f6r\u00b7per\u00b7chen", ",", "die", "auf", "ein\u00b7an\u00b7der", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "In glatten, die sich nicht so starck, so feste f\u00fcgen.", "tokens": ["In", "glat\u00b7ten", ",", "die", "sich", "nicht", "so", "starck", ",", "so", "fes\u00b7te", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "PRELS", "PRF", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}