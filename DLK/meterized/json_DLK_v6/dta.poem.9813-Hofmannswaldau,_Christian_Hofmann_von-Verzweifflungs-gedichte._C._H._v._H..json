{"dta.poem.9813": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Verzweifflungs-gedichte.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die augen schlo\u00df ich traurig zu/", "tokens": ["Die", "au\u00b7gen", "schlo\u00df", "ich", "trau\u00b7rig", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die h\u00e4nde deckten meine stirne/", "tokens": ["Die", "h\u00e4n\u00b7de", "deck\u00b7ten", "mei\u00b7ne", "stir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich war entbl\u00f6st von lust und ruh/", "tokens": ["Ich", "war", "ent\u00b7bl\u00f6st", "von", "lust", "und", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kummer f\u00fchlte das gehirne/", "tokens": ["Der", "kum\u00b7mer", "f\u00fchl\u00b7te", "das", "ge\u00b7hir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bald wacht ich auff/ bald schlieff ich ein/", "tokens": ["Bald", "wacht", "ich", "auff", "/", "bald", "schlieff", "ich", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "$(", "ADV", "VVFIN", "PPER", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bald wolt ich tod und asche seyn/", "tokens": ["Bald", "wolt", "ich", "tod", "und", "asc\u00b7he", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bald w\u00fcnscht ich weit von hier zu leben;", "tokens": ["Bald", "w\u00fcnscht", "ich", "weit", "von", "hier", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und da\u00df ja nichts sey unbekant/", "tokens": ["Und", "da\u00df", "ja", "nichts", "sey", "un\u00b7be\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIS", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So hat die thorheit/ meiner hand", "tokens": ["So", "hat", "die", "thor\u00b7heit", "/", "mei\u00b7ner", "hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Papier und feder \u00fcbergeben.", "tokens": ["Pa\u00b7pier", "und", "fe\u00b7der", "\u00fc\u00b7ber\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf auf mein sinn und du mein fu\u00df/", "tokens": ["Auf", "auf", "mein", "sinn", "und", "du", "mein", "fu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "KON", "PPER", "PPOSAT", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kan nicht l\u00e4nger hier verziehen/", "tokens": ["Ich", "kan", "nicht", "l\u00e4n\u00b7ger", "hier", "ver\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein warten bringet mir verdru\u00df/", "tokens": ["Mein", "war\u00b7ten", "brin\u00b7get", "mir", "ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00fcnsche von der welt zu fliehen.", "tokens": ["Ich", "w\u00fcn\u00b7sche", "von", "der", "welt", "zu", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich spey auf scepter und auff gold/", "tokens": ["Ich", "spey", "auf", "scep\u00b7ter", "und", "auff", "gold", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "KON", "APPR", "NN", "$("], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.6": {"text": "Man sey mir feind/ man sey mir hold/", "tokens": ["Man", "sey", "mir", "feind", "/", "man", "sey", "mir", "hold", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "NN", "$(", "PIS", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es soll mich beydes gleich erquicken;", "tokens": ["Es", "soll", "mich", "bey\u00b7des", "gleich", "er\u00b7qui\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die liebe/ so uns n\u00e4rrisch macht", "tokens": ["Die", "lie\u00b7be", "/", "so", "uns", "n\u00e4r\u00b7risch", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "$(", "ADV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und uns bezwingt mit dicker nacht/", "tokens": ["Und", "uns", "be\u00b7zwingt", "mit", "di\u00b7cker", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Soll mir nicht den compa\u00df verr\u00fccken.", "tokens": ["Soll", "mir", "nicht", "den", "com\u00b7pa\u00df", "ver\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich lache/ wenn ich \u00fcberhin", "tokens": ["Ich", "la\u00b7che", "/", "wenn", "ich", "\u00fc\u00b7berh\u00b7in"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein tummes leben \u00fcberlege/", "tokens": ["Mein", "tum\u00b7mes", "le\u00b7ben", "\u00fc\u00b7berl\u00b7e\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und di\u00df/ worauf ich kommen bin/", "tokens": ["Und", "di\u00df", "/", "wo\u00b7rauf", "ich", "kom\u00b7men", "bin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$(", "PWAV", "PPER", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In den gedancken recht erwege/", "tokens": ["In", "den", "ge\u00b7dan\u00b7cken", "recht", "er\u00b7we\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir zittern beydes marck und bein/", "tokens": ["Mir", "zit\u00b7tern", "bey\u00b7des", "marck", "und", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die stirne wird wie ey\u00df und stein/", "tokens": ["Die", "stir\u00b7ne", "wird", "wie", "ey\u00df", "und", "stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOKOM", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es will gebl\u00fct und geist erstarren;", "tokens": ["Es", "will", "ge\u00b7bl\u00fct", "und", "geist", "er\u00b7star\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Genug geirrt/ genung geklagt/", "tokens": ["Ge\u00b7nug", "ge\u00b7irrt", "/", "ge\u00b7nung", "ge\u00b7klagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$(", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den irrthum hat die zeit verjagt/", "tokens": ["Den", "irr\u00b7thum", "hat", "die", "zeit", "ver\u00b7jagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich will nicht l\u00e4nger hier verharren.", "tokens": ["Ich", "will", "nicht", "l\u00e4n\u00b7ger", "hier", "ver\u00b7har\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich eil in eine weisse gruft/", "tokens": ["Ich", "eil", "in", "ei\u00b7ne", "weis\u00b7se", "gruft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die keine sonne hat ber\u00fchret/", "tokens": ["Die", "kei\u00b7ne", "son\u00b7ne", "hat", "be\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da die eingesperrte luft", "tokens": ["Und", "da", "die", "ein\u00b7ge\u00b7sperr\u00b7te", "luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns zeitlich zu dem tode f\u00fchret/", "tokens": ["Uns", "zeit\u00b7lich", "zu", "dem", "to\u00b7de", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der schlangen gifft und drachen rauch/", "tokens": ["Der", "schlan\u00b7gen", "gifft", "und", "dra\u00b7chen", "rauch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der f\u00fclle nase/ brust und bauch/", "tokens": ["Der", "f\u00fcl\u00b7le", "na\u00b7se", "/", "brust", "und", "bauch", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und endlich meinen geist vertreibe/", "tokens": ["Und", "end\u00b7lich", "mei\u00b7nen", "geist", "ver\u00b7trei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf da\u00df die ausgedorrte brust", "tokens": ["Auf", "da\u00df", "die", "aus\u00b7ge\u00b7dorr\u00b7te", "brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als eine recht bestimmte kost", "tokens": ["Als", "ei\u00b7ne", "recht", "be\u00b7stimm\u00b7te", "kost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "F\u00fcr junger drachen z\u00e4hne bleibe.", "tokens": ["F\u00fcr", "jun\u00b7ger", "dra\u00b7chen", "z\u00e4h\u00b7ne", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und werd ich ja nicht hingericht/", "tokens": ["Und", "werd", "ich", "ja", "nicht", "hin\u00b7ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch schlangengift geschickt zu t\u00f6dten/", "tokens": ["Durch", "schlan\u00b7gen\u00b7gift", "ge\u0