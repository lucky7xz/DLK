{"textgrid.poem.32510": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift auf Voltairen", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt \u2013 wenn man euch glauben wollte,", "tokens": ["Hier", "liegt", "\u2013", "wenn", "man", "euch", "glau\u00b7ben", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr frommen Herr'n! \u2013 der l\u00e4ngst hier liegen sollte.", "tokens": ["Ihr", "from\u00b7men", "Herr'n", "!", "\u2013", "der", "l\u00e4ngst", "hier", "lie\u00b7gen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "ART", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der liebe Gott verzeih aus Gnade", "tokens": ["Der", "lie\u00b7be", "Gott", "ver\u00b7zeih", "aus", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm seine Henriade,", "tokens": ["Ihm", "sei\u00b7ne", "Hen\u00b7ria\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und seine Trauerspiele,", "tokens": ["Und", "sei\u00b7ne", "Trau\u00b7er\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und seiner Verschen viele:", "tokens": ["Und", "sei\u00b7ner", "Ver\u00b7schen", "vie\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Denn was er sonst ans Licht gebracht,", "tokens": ["Denn", "was", "er", "sonst", "ans", "Licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das hat er ziemlich gut gemacht.", "tokens": ["Das", "hat", "er", "ziem\u00b7lich", "gut", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}