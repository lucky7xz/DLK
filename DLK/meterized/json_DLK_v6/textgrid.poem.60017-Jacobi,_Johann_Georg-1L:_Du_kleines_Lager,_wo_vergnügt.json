{"textgrid.poem.60017": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kleines Lager, wo vergn\u00fcgt", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleines Lager, wo vergn\u00fcgt", "tokens": ["Du", "klei\u00b7nes", "La\u00b7ger", ",", "wo", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sch\u00f6nheit mit der Unschuld liegt!", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "mit", "der", "Un\u00b7schuld", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begl\u00fccktes Heiligthum der Liebe,", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Hei\u00b7lig\u00b7thum", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey dem, gew\u00f6hnt an frechen Raub,", "tokens": ["Bey", "dem", ",", "ge\u00b7w\u00f6hnt", "an", "fre\u00b7chen", "Raub", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ein roher Satyr sch\u00fcchtern bliebe!", "tokens": ["Ein", "ro\u00b7her", "Sa\u00b7tyr", "sch\u00fcch\u00b7tern", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dir will ich noch das letzte Laub", "tokens": ["Dir", "will", "ich", "noch", "das", "letz\u00b7te", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der l\u00e4ngst gestorbnen Rose streuen;", "tokens": ["Der", "l\u00e4ngst", "ge\u00b7storb\u00b7nen", "Ro\u00b7se", "streu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dich soll ein Dichter nicht entweihen,", "tokens": ["Dich", "soll", "ein", "Dich\u00b7ter", "nicht", "ent\u00b7wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der gerne mit dem Amor spielt,", "tokens": ["Der", "ger\u00b7ne", "mit", "dem", "A\u00b7mor", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und doch den Werth der Weisheit f\u00fchlt.", "tokens": ["Und", "doch", "den", "Werth", "der", "Weis\u00b7heit", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geheimer Schauder! Stille Lust!", "tokens": ["Ge\u00b7hei\u00b7mer", "Schau\u00b7der", "!", "Stil\u00b7le", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bem\u00e4chtigt euch des J\u00fcnglings Brust.", "tokens": ["Be\u00b7m\u00e4ch\u00b7tigt", "euch", "des", "J\u00fcng\u00b7lings", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Schlummerst\u00e4tte meiner Sch\u00f6nen!", "tokens": ["Du", "Schlum\u00b7mer\u00b7st\u00e4t\u00b7te", "mei\u00b7ner", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O zeige mir Belindens Bild;", "tokens": ["O", "zei\u00b7ge", "mir", "Be\u00b7lin\u00b7dens", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier siehst du jeden Reiz enth\u00fcllt;", "tokens": ["Hier", "siehst", "du", "je\u00b7den", "Reiz", "ent\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier sagt sie dir mit halben T\u00f6nen", "tokens": ["Hier", "sagt", "sie", "dir", "mit", "hal\u00b7ben", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vielleicht, was ihren W\u00fcnschen fehlt,", "tokens": ["Viel\u00b7leicht", ",", "was", "ih\u00b7ren", "W\u00fcn\u00b7schen", "fehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was sie noch selber sich verhehlt.", "tokens": ["Was", "sie", "noch", "sel\u00b7ber", "sich", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Vorhang rauscht, und Tr\u00e4ume schl\u00fcpfen", "tokens": ["Dein", "Vor\u00b7hang", "rauscht", ",", "und", "Tr\u00e4u\u00b7me", "schl\u00fcp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ihn: ein allerliebstes Heer!", "tokens": ["Durch", "ihn", ":", "ein", "al\u00b7ler\u00b7liebs\u00b7tes", "Heer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sch\u00f6n, wie der Venus Kinder, h\u00fcpfen", "tokens": ["Sch\u00f6n", ",", "wie", "der", "Ve\u00b7nus", "Kin\u00b7der", ",", "h\u00fcp\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie um das fromme M\u00e4dchen her.", "tokens": ["Sie", "um", "das", "from\u00b7me", "M\u00e4d\u00b7chen", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Belinde z\u00fcrnt: auf ihren Wangen", "tokens": ["Be\u00b7lin\u00b7de", "z\u00fcrnt", ":", "auf", "ih\u00b7ren", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist Keuschheit, Jugend, und Verlangen.", "tokens": ["Ist", "Keuschheit", ",", "Ju\u00b7gend", ",", "und", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn sie nun z\u00e4rtlicher erwacht;", "tokens": ["Wenn", "sie", "nun", "z\u00e4rt\u00b7li\u00b7cher", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie, nach ungeno\u00dfnen Freuden,", "tokens": ["Wenn", "sie", ",", "nach", "un\u00b7ge\u00b7no\u00df\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Morgensonn' entgegen lacht,", "tokens": ["Der", "Mor\u00b7gen\u00b7sonn'", "ent\u00b7ge\u00b7gen", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in verr\u00e4therische Tracht", "tokens": ["Und", "in", "ver\u00b7r\u00e4\u00b7the\u00b7ri\u00b7sche", "Tracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Behende Grazien sie kleiden:", "tokens": ["Be\u00b7hen\u00b7de", "Gra\u00b7zi\u00b7en", "sie", "klei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dann, o dann mu\u00df ich dich beneiden!", "tokens": ["Dann", ",", "o", "dann", "mu\u00df", "ich", "dich", "be\u00b7nei\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ungest\u00fcme W\u00fcnsche nicht", "tokens": ["Doch", "un\u00b7ge\u00b7st\u00fc\u00b7me", "W\u00fcn\u00b7sche", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll dieser kleine Tempel h\u00f6ren;", "tokens": ["Soll", "die\u00b7ser", "klei\u00b7ne", "Tem\u00b7pel", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Seufzer darf ich mir gew\u00e4hren,", "tokens": ["Nur", "Seuf\u00b7zer", "darf", "ich", "mir", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bescheiden, wie ein Amor spricht", "tokens": ["Be\u00b7schei\u00b7den", ",", "wie", "ein", "A\u00b7mor", "spricht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In einem W\u00e4ldchen mit Cytheren.", "tokens": ["In", "ei\u00b7nem", "W\u00e4ld\u00b7chen", "mit", "Cy\u00b7the\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr, die, von wilder Gluth entbrannt,", "tokens": ["Ihr", ",", "die", ",", "von", "wil\u00b7der", "Gluth", "ent\u00b7brannt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gott der Liebe nie gekannt,", "tokens": ["Der", "Gott", "der", "Lie\u00b7be", "nie", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zerrei\u00dfet mit verwegner Hand", "tokens": ["Zer\u00b7rei\u00b7\u00dfet", "mit", "ver\u00b7weg\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sch\u00f6nheit heiliges Gewand,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "hei\u00b7li\u00b7ges", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Huldg\u00f6ttinnen ihr gewebet,", "tokens": ["Das", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "ihr", "ge\u00b7we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Inde\u00df ein sanfter Hirt erbebet,", "tokens": ["In\u00b7de\u00df", "ein", "sanf\u00b7ter", "Hirt", "er\u00b7be\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn er Belindens Lager sieht,", "tokens": ["Wenn", "er", "Be\u00b7lin\u00b7dens", "La\u00b7ger", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Voll Ehrfurcht ihre Zelle flieht;", "tokens": ["Voll", "Ehr\u00b7furcht", "ih\u00b7re", "Zel\u00b7le", "flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und auf verschwiegnen gr\u00fcnen Heiden,", "tokens": ["Und", "auf", "ver\u00b7schwieg\u00b7nen", "gr\u00fc\u00b7nen", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wo G\u00f6tter mit dem M\u00e4dchen weiden,", "tokens": ["Wo", "G\u00f6t\u00b7ter", "mit", "dem", "M\u00e4d\u00b7chen", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Auf Blumen es verfolgt und k\u00fc\u00dft,", "tokens": ["Auf", "Blu\u00b7men", "es", "ver\u00b7folgt", "und", "k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und ohne Reu begl\u00fcckter ist,", "tokens": ["Und", "oh\u00b7ne", "Reu", "be\u00b7gl\u00fcck\u00b7ter", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Als ihr im Taumel eurer Freuden.", "tokens": ["Als", "ihr", "im", "Tau\u00b7mel", "eu\u00b7rer", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}