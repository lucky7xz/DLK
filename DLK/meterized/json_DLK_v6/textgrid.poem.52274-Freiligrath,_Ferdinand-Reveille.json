{"textgrid.poem.52274": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Reveille", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf zur Weise von Marseille,", "tokens": ["Frisch", "auf", "zur", "Wei\u00b7se", "von", "Mar\u00b7seil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch auf ein Lied mit hellem Ton!", "tokens": ["Frisch", "auf", "ein", "Lied", "mit", "hel\u00b7lem", "Ton", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sing es hinaus als die Reveille", "tokens": ["Sing", "es", "hin\u00b7aus", "als", "die", "Re\u00b7veil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APZR", "KOUS", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Der neuen Revolution!", "tokens": ["Der", "neu\u00b7en", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der neuen Revolution!", "tokens": ["Der", "neu\u00b7en", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der neuen, die mit Schwert und Lanze", "tokens": ["Der", "neu\u00b7en", ",", "die", "mit", "Schwert", "und", "Lan\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die letzte Fessel bald zerbricht \u2013", "tokens": ["Die", "letz\u00b7te", "Fes\u00b7sel", "bald", "zer\u00b7bricht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der alten, halben singt es nicht!", "tokens": ["Der", "al\u00b7ten", ",", "hal\u00b7ben", "singt", "es", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Uns gilt die neue nur, die ganze!", "tokens": ["Uns", "gilt", "die", "neu\u00b7e", "nur", ",", "die", "gan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die neue Rebellion!", "tokens": ["Die", "neu\u00b7e", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die ganze Rebellion!", "tokens": ["Die", "gan\u00b7ze", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Marsch, marsch!", "tokens": ["Marsch", ",", "marsch", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Marsch, marsch!", "tokens": ["Marsch", ",", "marsch", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Marsch \u2013 w\u00e4r's zum Tod!", "tokens": ["Marsch", "\u2013", "w\u00e4r's", "zum", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VAFIN", "APPRART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Und unsre Fahn' ist rot (", "tokens": ["Und", "uns\u00b7re", "Fahn'", "ist", "rot", "("], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sommer reift des Fr\u00fchlings Saaten,", "tokens": ["Der", "Som\u00b7mer", "reift", "des", "Fr\u00fch\u00b7lings", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum folgt der Juni auf den M\u00e4rz.", "tokens": ["Drum", "folgt", "der", "Ju\u00b7ni", "auf", "den", "M\u00e4rz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Juni, komm und bring' uns Taten!", "tokens": ["O", "Ju\u00b7ni", ",", "komm", "und", "bring'", "uns", "Ta\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach frischen Taten lechzt das Herz!", "tokens": ["Nach", "fri\u00b7schen", "Ta\u00b7ten", "lechzt", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach frischen Taten lechzt das Herz!", "tokens": ["Nach", "fri\u00b7schen", "Ta\u00b7ten", "lechzt", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df deine Wolken schwarz sich ballen,", "tokens": ["La\u00df", "dei\u00b7ne", "Wol\u00b7ken", "schwarz", "sich", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bring' uns Gewitter Schlag auf Schlag!", "tokens": ["Bring'", "uns", "Ge\u00b7wit\u00b7ter", "Schlag", "auf", "Schlag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df in die unges\u00fchnte Schmach", "tokens": ["La\u00df", "in", "die", "un\u00b7ge\u00b7s\u00fchn\u00b7te", "Schmach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Rache Donnerkeile fallen!", "tokens": ["Der", "Ra\u00b7che", "Don\u00b7ner\u00b7kei\u00b7le", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die neue Rebellion!", "tokens": ["Die", "neu\u00b7e", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die ganze Rebellion!", "tokens": ["Die", "gan\u00b7ze", "Re\u00b7bel\u00b7lion", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", 