{"textgrid.poem.67147": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr edeln Br\u00fcder droben, unsterbliches", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr edeln Br\u00fcder droben, unsterbliches", "tokens": ["Ihr", "e\u00b7deln", "Br\u00fc\u00b7der", "dro\u00b7ben", ",", "uns\u00b7terb\u00b7li\u00b7ches"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gestirn, euch frag ich, Helden, woher es ist,", "tokens": ["Ge\u00b7stirn", ",", "euch", "frag", "ich", ",", "Hel\u00b7den", ",", "wo\u00b7her", "es", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich so untertan ihm bin und", "tokens": ["Da\u00df", "ich", "so", "un\u00b7ter\u00b7tan", "ihm", "bin", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VAFIN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So der Gewaltige sein mich nennet.", "tokens": ["So", "der", "Ge\u00b7wal\u00b7ti\u00b7ge", "sein", "mich", "nen\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "PPER", "VVFIN", "$."], "meter": "+--+-+-+--", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Denn wenig, aber ", "tokens": ["Denn", "we\u00b7nig", ",", "a\u00b7ber"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "PIS", "$,", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da niemand mag, soll tauschen, ein gutes Gl\u00fcck,", "tokens": ["Da", "nie\u00b7mand", "mag", ",", "soll", "tau\u00b7schen", ",", "ein", "gu\u00b7tes", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "$,", "VMFIN", "VVINF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein lichtes, reines, zum Ged\u00e4chtnis", "tokens": ["Ein", "lich\u00b7tes", ",", "rei\u00b7nes", ",", "zum", "Ge\u00b7d\u00e4cht\u00b7nis"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebender Tage zur\u00fcckgeblieben.", "tokens": ["Le\u00b7ben\u00b7der", "Ta\u00b7ge", "zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "So aber er gebietet, dies ", "tokens": ["So", "a\u00b7ber", "er", "ge\u00b7bie\u00b7tet", ",", "dies"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "PPER", "VVPP", "$,", "PDS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohin ers wollte, wagt ich mein Saitenspiel,", "tokens": ["Wo\u00b7hin", "ers", "woll\u00b7te", ",", "wagt", "ich", "mein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Samt dem Gesange folgt ich, selbst ins", "tokens": ["Samt", "dem", "Ge\u00b7san\u00b7ge", "folgt", "ich", ",", "selbst", "ins"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "$,", "ADV", "APPRART"], "meter": "+--+-+---", "measure": "iambic.tri.invert"}, "line.4": {"text": "Dunkel der Tapferen, ihm hinunter.", "tokens": ["Dun\u00b7kel", "der", "Tap\u00b7fe\u00b7ren", ",", "ihm", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbmit Wolken\u00ab, s\u00e4ng ich, \u00bbtr\u00e4nkt das Gewitter dich,", "tokens": ["\u00bb", "mit", "Wol\u00b7ken", "\u00ab", ",", "s\u00e4ng", "ich", ",", "\u00bb", "tr\u00e4nkt", "das", "Ge\u00b7wit\u00b7ter", "dich", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du sp\u00f6ttischer Boden, aber mit Blut der Mensch,", "tokens": ["Du", "sp\u00f6t\u00b7ti\u00b7scher", "Bo\u00b7den", ",", "a\u00b7ber", "mit", "Blut", "der", "Mensch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So schweigt, so heiligt, der sein Gleiches", "tokens": ["So", "schweigt", ",", "so", "hei\u00b7ligt", ",", "der", "sein", "Glei\u00b7ches"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Droben und drunten umsonst erfragte.\u00ab", "tokens": ["Dro\u00b7ben", "und", "drun\u00b7ten", "um\u00b7sonst", "er\u00b7frag\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "KON", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}