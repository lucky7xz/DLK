{"dta.poem.19865": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Danklied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Alg\u00fctiger, mein Hochgesang               ", "tokens": ["Al\u00b7g\u00fc\u00b7ti\u00b7ger", ",", "mein", "Hoch\u00b7ge\u00b7sang"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frohlocke dir mein Leben lang!", "tokens": ["Froh\u00b7lo\u00b7cke", "dir", "mein", "Le\u00b7ben", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Namen sey gebenedeit,", "tokens": ["Dein", "Na\u00b7men", "sey", "ge\u00b7be\u00b7ne\u00b7deit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von nun an bis in Ewigkeit!", "tokens": ["Von", "nun", "an", "bis", "in", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "KON", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O Gott, an meiner Mira Brust", "tokens": ["O", "Gott", ",", "an", "mei\u00b7ner", "Mi\u00b7ra", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchschauert mich die fromme Lust.", "tokens": ["Durch\u00b7schau\u00b7ert", "mich", "die", "from\u00b7me", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den du erschufst, der Traube Saft,", "tokens": ["Den", "du", "er\u00b7schufst", ",", "der", "Trau\u00b7be", "Saft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Giebt meinem Liede Schwung und Kraft.", "tokens": ["Giebt", "mei\u00b7nem", "Lie\u00b7de", "Schwung", "und", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Wonnetaumel thut mein Mund,", "tokens": ["Im", "Won\u00b7ne\u00b7tau\u00b7mel", "thut", "mein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Geber, deine Gaben kund.", "tokens": ["Du", "Ge\u00b7ber", ",", "dei\u00b7ne", "Ga\u00b7ben", "kund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kus, Freudenmal und Becherklang", "tokens": ["Kus", ",", "Freu\u00b7den\u00b7mal", "und", "Be\u00b7cher\u00b7klang"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entweihen keinen frommen Sang. \u2014", "tokens": ["Ent\u00b7wei\u00b7hen", "kei\u00b7nen", "from\u00b7men", "Sang", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dies f\u00fcsse M\u00e4dchen, welches mir", "tokens": ["Dies", "f\u00fcs\u00b7se", "M\u00e4d\u00b7chen", ",", "wel\u00b7ches", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Himmel k\u00fcsset, danket dir,", "tokens": ["Den", "Him\u00b7mel", "k\u00fcs\u00b7set", ",", "dan\u00b7ket", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir dankt es feurig mein Gesang!", "tokens": ["Dir", "dankt", "es", "feu\u00b7rig", "mein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie meine Liebe flamt mein Dank!", "tokens": ["Wie", "mei\u00b7ne", "Lie\u00b7be", "flamt", "mein", "Dank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Tenne zolt mir ihre Gift;", "tokens": ["Die", "Ten\u00b7ne", "zolt", "mir", "ih\u00b7re", "Gift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir zinsen Garten, Forst und Trift;", "tokens": ["Mir", "zin\u00b7sen", "Gar\u00b7ten", ",", "Forst", "und", "Trift", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von mancher edlen Kelter fleust", "tokens": ["Von", "man\u00b7cher", "ed\u00b7len", "Kel\u00b7ter", "fleust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr mich der Traube Feuergeist.", "tokens": ["F\u00fcr", "mich", "der", "Trau\u00b7be", "Feu\u00b7er\u00b7geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf Rebenbergen, fern und nah,", "tokens": ["Auf", "Re\u00b7ben\u00b7ber\u00b7gen", ",", "fern", "und", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am hohen Kap, zu Malaga,", "tokens": ["Am", "ho\u00b7hen", "Kap", ",", "zu", "Ma\u00b7la\u00b7ga", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu Hochheim, Cypern und Burgund", "tokens": ["Zu", "Hoch\u00b7heim", ",", "Cy\u00b7pern", "und", "Bur\u00b7gund"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Trof Nektar schon f\u00fcr meinen Mund.", "tokens": ["Trof", "Nek\u00b7tar", "schon", "f\u00fcr", "mei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auch mir f\u00fchrt, unter Tausenden,", "tokens": ["Auch", "mir", "f\u00fchrt", ",", "un\u00b7ter", "Tau\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das reiche Schif aus Indien", "tokens": ["Das", "rei\u00b7che", "Schif", "aus", "In\u00b7di\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gew\u00fcrz und edle Spezerei", "tokens": ["Ge\u00b7w\u00fcrz", "und", "ed\u00b7le", "Spe\u00b7ze\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Saba\u2019s Bonen mit herbei. \u2014", "tokens": ["Und", "Sa\u00b7ba's", "Bo\u00b7nen", "mit", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "NN", "APPR", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer z\u00e4lt die Gaben alle? Wer?", "tokens": ["Wer", "z\u00e4lt", "die", "Ga\u00b7ben", "al\u00b7le", "?", "Wer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PIS", "$.", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Z\u00e4lt Jemand auch den Sand am Meer?", "tokens": ["Z\u00e4lt", "Je\u00b7mand", "auch", "den", "Sand", "am", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer ist, der an dem Firmament", "tokens": ["Wer", "ist", ",", "der", "an", "dem", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Summe der Gestirne nent? \u2014", "tokens": ["Die", "Sum\u00b7me", "der", "Ge\u00b7stir\u00b7ne", "nent", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Von dieser Unzal weg der Blik!", "tokens": ["Von", "die\u00b7ser", "Un\u00b7zal", "weg", "der", "Blik", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fck, mein Geist, in dich zur\u00fck!", "tokens": ["Zu\u00b7r\u00fck", ",", "mein", "Geist", ",", "in", "dich", "zu\u00b7r\u00fck", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In diesem engumschr\u00e4nkten Bau,", "tokens": ["In", "die\u00b7sem", "en\u00b7gum\u00b7schr\u00e4nk\u00b7ten", "Bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott! welcher Gaben Wunderschau!", "tokens": ["Gott", "!", "wel\u00b7cher", "Ga\u00b7ben", "Wun\u00b7der\u00b7schau", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du fl\u00f6ssest Geist den Nerven ein,", "tokens": ["Du", "fl\u00f6s\u00b7sest", "Geist", "den", "Ner\u00b7ven", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Kraft erf\u00fclst du mein Gebein,", "tokens": ["Mit", "Kraft", "er\u00b7f\u00fclst", "du", "mein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Str\u00f6mst in die Adern reines Blut,", "tokens": ["Str\u00f6mst", "in", "die", "A\u00b7dern", "rei\u00b7nes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in die Brust gesunden Mut.", "tokens": ["Und", "in", "die", "Brust", "ge\u00b7sun\u00b7den", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich f\u00fchle deinen sch\u00f6nen Mai,", "tokens": ["Ich", "f\u00fch\u00b7le", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Philomelens Melodei,", "tokens": ["Und", "Phi\u00b7lo\u00b7me\u00b7lens", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Sommers wollustvolle Luft,", "tokens": ["Des", "Som\u00b7mers", "wol\u00b7lust\u00b7vol\u00b7le", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Blumen Farbenglanz und Duft.", "tokens": ["Der", "Blu\u00b7men", "Far\u00b7ben\u00b7glanz", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Vor Tausenden gab deine Gunst", "tokens": ["Vor", "Tau\u00b7sen\u00b7den", "gab", "dei\u00b7ne", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Des Liedes und der Harfe Kunst", "tokens": ["Des", "Lie\u00b7des", "und", "der", "Har\u00b7fe", "Kunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meine Kehle, meine Hand;", "tokens": ["In", "mei\u00b7ne", "Keh\u00b7le", ",", "mei\u00b7ne", "Hand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht zur Schande f\u00fcr mein Land!", "tokens": ["Und", "nicht", "zur", "Schan\u00b7de", "f\u00fcr", "mein", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Da\u00df meine Fantasei, vol Kraft,", "tokens": ["Da\u00df", "mei\u00b7ne", "Fan\u00b7ta\u00b7sei", ",", "vol", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vernichtet Welten, Welten schaft,", "tokens": ["Ver\u00b7nich\u00b7tet", "Wel\u00b7ten", ",", "Wel\u00b7ten", "schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00f6llenab, und himmelan,", "tokens": ["Und", "h\u00f6l\u00b7len\u00b7ab", ",", "und", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$,", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich senken und erheben kan;", "tokens": ["Sich", "sen\u00b7ken", "und", "er\u00b7he\u00b7ben", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Da\u00df meines Geistes Auge hell", "tokens": ["Da\u00df", "mei\u00b7nes", "Geis\u00b7tes", "Au\u00b7ge", "hell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Dinge Wirwar, leicht und schnell,", "tokens": ["Der", "Din\u00b7ge", "Wir\u00b7war", ",", "leicht", "und", "schnell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie nicht ein jeder Erdenman,", "tokens": ["Wie", "nicht", "ein", "je\u00b7der", "Er\u00b7den\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchsp\u00e4hen und entwickeln kan;", "tokens": ["Durch\u00b7sp\u00e4\u00b7hen", "und", "ent\u00b7wi\u00b7ckeln", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.