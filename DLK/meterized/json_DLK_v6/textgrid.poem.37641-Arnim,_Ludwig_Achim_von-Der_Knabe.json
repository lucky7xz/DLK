{"textgrid.poem.37641": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sonnte sich ein kranker Knabe", "tokens": ["Es", "sonn\u00b7te", "sich", "ein", "kran\u00b7ker", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf seiner armen Mutter Gruft,", "tokens": ["Auf", "sei\u00b7ner", "ar\u00b7men", "Mut\u00b7ter", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fasset ihn der Ahndung Gabe,", "tokens": ["Da", "fas\u00b7set", "ihn", "der", "Ahn\u00b7dung", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er wittert einer Blume Duft,", "tokens": ["Er", "wit\u00b7tert", "ei\u00b7ner", "Blu\u00b7me", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ferne schwebet in dem Meere,", "tokens": ["Die", "fer\u00b7ne", "schwe\u00b7bet", "in", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Weit von dem Ende aller Welt,", "tokens": ["Weit", "von", "dem", "En\u00b7de", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "In die aus hoher lust'ger Leere", "tokens": ["In", "die", "aus", "ho\u00b7her", "lust'\u00b7ger", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sonne wie ein Same f\u00e4llt.", "tokens": ["Die", "Son\u00b7ne", "wie", "ein", "Sa\u00b7me", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es gl\u00fcht auf seiner blassen Wange", "tokens": ["Es", "gl\u00fcht", "auf", "sei\u00b7ner", "blas\u00b7sen", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun eine R\u00f6the wunderbar,", "tokens": ["Nun", "ei\u00b7ne", "R\u00f6\u00b7the", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schwebt sein Ohr in tiefem Klange.", "tokens": ["Es", "schwebt", "sein", "Ohr", "in", "tie\u00b7fem", "Klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird sein Auge ihm so klar,", "tokens": ["Es", "wird", "sein", "Au\u00b7ge", "ihm", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es gl\u00e4nzt auf seinem stillen Herzen", "tokens": ["Es", "gl\u00e4nzt", "auf", "sei\u00b7nem", "stil\u00b7len", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Regenbogen wie ein Strau\u00df,", "tokens": ["Ein", "Re\u00b7gen\u00b7bo\u00b7gen", "wie", "ein", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der hat verk\u00fcndet seine Schmerzen", "tokens": ["Der", "hat", "ver\u00b7k\u00fcn\u00b7det", "sei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hoch in des Himmels sel'gem Haus.", "tokens": ["Hoch", "in", "des", "Him\u00b7mels", "sel'\u00b7gem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem Himmel hat er ihn verbunden,", "tokens": ["Dem", "Him\u00b7mel", "hat", "er", "ihn", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt ihm das offne Himmelsthor,", "tokens": ["Zeigt", "ihm", "das", "off\u00b7ne", "Him\u00b7mel\u00b7sthor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schauet nun in Schmerzensstunden,", "tokens": ["Er", "schau\u00b7et", "nun", "in", "Schmer\u00b7zenss\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Lust ihm nie gezeigt zuvor,", "tokens": ["Wo", "Lust", "ihm", "nie", "ge\u00b7zeigt", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "ADV", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie kann er nun die Welt verschmerzen", "tokens": ["Wie", "kann", "er", "nun", "die", "Welt", "ver\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm ist verschwunden aller Graus,", "tokens": ["Ihm", "ist", "ver\u00b7schwun\u00b7den", "al\u00b7ler", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Herz, gebrochen einst in Schmerzen,", "tokens": ["Sein", "Herz", ",", "ge\u00b7bro\u00b7chen", "einst", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sieht froh die Witterung voraus.", "tokens": ["Sieht", "froh", "die", "Wit\u00b7te\u00b7rung", "vo\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sieht voraus die Liebestage,", "tokens": ["Er", "sieht", "vo\u00b7raus", "die", "Lie\u00b7bes\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Hand in Hand sich gern ergeht,", "tokens": ["Wo", "Hand", "in", "Hand", "sich", "gern", "er\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch M\u00e4dchen zeigt die Hand zur Frage,", "tokens": ["Manch", "M\u00e4d\u00b7chen", "zeigt", "die", "Hand", "zur", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil Er die Linien jetzt versteht;", "tokens": ["Weil", "Er", "die", "Li\u00b7ni\u00b7en", "jetzt", "ver\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Des Knaben Ruf ist weit erschollen,", "tokens": ["Des", "Kna\u00b7ben", "Ruf", "ist", "weit", "er\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn jeder fr\u00e4gt nach Witterung,", "tokens": ["Denn", "je\u00b7der", "fr\u00e4gt", "nach", "Wit\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Alten, weil sie ernten wollen,", "tokens": ["Die", "Al\u00b7ten", ",", "weil", "sie", "ern\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und weil sich lieben, die noch jung.", "tokens": ["Und", "weil", "sich", "lie\u00b7ben", ",", "die", "noch", "jung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "VVINF", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzt hat der Schlaf ihn fest umfangen,", "tokens": ["Jetzt", "hat", "der", "Schlaf", "ihn", "fest", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da nimmt die Mutter seine Hand,", "tokens": ["Da", "nimmt", "die", "Mut\u00b7ter", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieht er all, was ihm vergangen,", "tokens": ["Da", "sieht", "er", "all", ",", "was", "ihm", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und keine Zukunft er drin fand:", "tokens": ["Und", "kei\u00b7ne", "Zu\u00b7kunft", "er", "drin", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Liebe, wo du gegenw\u00e4rtig,", "tokens": ["O", "Lie\u00b7be", ",", "wo", "du", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da ist das eigne Leben aus,", "tokens": ["Da", "ist", "das", "eig\u00b7ne", "Le\u00b7ben", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Seele ist dann reisefertig,", "tokens": ["Die", "See\u00b7le", "ist", "dann", "rei\u00b7se\u00b7fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du tr\u00e4gst sie in ein andres Haus.", "tokens": ["Du", "tr\u00e4gst", "sie", "in", "ein", "and\u00b7res", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbo Muttererde la\u00df dich gr\u00fc\u00dfen,", "tokens": ["\u00bb", "o", "Mut\u00b7ter\u00b7er\u00b7de", "la\u00df", "dich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du trugst mich treu in stiller Qual,", "tokens": ["Du", "trugst", "mich", "treu", "in", "stil\u00b7ler", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df deine k\u00fchlen Lippen k\u00fcssen,", "tokens": ["La\u00df", "dei\u00b7ne", "k\u00fch\u00b7len", "Lip\u00b7pen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast andre Kinder ohne Zahl,", "tokens": ["Hast", "and\u00b7re", "Kin\u00b7der", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ich geh\u00f6r dem Vaterlande,", "tokens": ["Doch", "ich", "ge\u00b7h\u00f6r", "dem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem Vater in dem Himmelreich,", "tokens": ["Dem", "Va\u00b7ter", "in", "dem", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es l\u00f6sen sich die alten Bande,", "tokens": ["Es", "l\u00f6\u00b7sen", "sich", "die", "al\u00b7ten", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zum letztenmal die Hand mir reich.\u00ab", "tokens": ["Zum", "letz\u00b7ten\u00b7mal", "die", "Hand", "mir", "reich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADV", "ART", "NN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er kann sich selber nicht begreifen,", "tokens": ["Er", "kann", "sich", "sel\u00b7ber", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird ihm wohl, so auf einmal,", "tokens": ["Es", "wird", "ihm", "wohl", ",", "so", "auf", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sieht er dann die Engel schweifen", "tokens": ["Da", "sieht", "er", "dann", "die", "En\u00b7gel", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seines Thr\u00e4nenbogens Strahl,", "tokens": ["Auf", "sei\u00b7nes", "Thr\u00e4\u00b7nen\u00b7bo\u00b7gens", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie sie die bunten Fl\u00fcgel schlagen,", "tokens": ["Wie", "sie", "die", "bun\u00b7ten", "Fl\u00fc\u00b7gel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df jede Farbe klingt im Glanz,", "tokens": ["Da\u00df", "je\u00b7de", "Far\u00b7be", "klingt", "im", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er f\u00fchlt von ihnen sich getragen,", "tokens": ["Er", "f\u00fchlt", "von", "ih\u00b7nen", "sich", "ge\u00b7tra\u00b