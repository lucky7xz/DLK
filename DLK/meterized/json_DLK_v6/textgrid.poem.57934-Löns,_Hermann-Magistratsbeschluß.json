{"textgrid.poem.57934": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Magistratsbeschlu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am gr\u00fcnen Donnerstage des Jahres", "tokens": ["Am", "gr\u00fc\u00b7nen", "Don\u00b7ners\u00b7ta\u00b7ge", "des", "Jah\u00b7res"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Heils ein Tausend neun Hundert und Sechs", "tokens": ["Des", "Heils", "ein", "Tau\u00b7send", "neun", "Hun\u00b7dert", "und", "Sechs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "CARD", "NN", "KON", "CARD"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Beschlossen die st\u00e4dtischen Collegia", "tokens": ["Be\u00b7schlos\u00b7sen", "die", "st\u00e4d\u00b7ti\u00b7schen", "Col\u00b7le\u00b7gia"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nachfolgende hochwohlweise Lex:", "tokens": ["Nach\u00b7fol\u00b7gen\u00b7de", "hoch\u00b7wohl\u00b7wei\u00b7se", "Lex", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Wasma\u00dfen in Folge der allzugro\u00dfen", "tokens": ["Was\u00b7ma\u00b7\u00dfen", "in", "Fol\u00b7ge", "der", "all\u00b7zu\u00b7gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ART", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00dcberhandnahme der dritten Kind'", "tokens": ["\u00dc\u00b7berh\u00b7and\u00b7nah\u00b7me", "der", "drit\u00b7ten", "Kind'"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Unserer getreven Stadt Finanzen", "tokens": ["Un\u00b7se\u00b7rer", "ge\u00b7tre\u00b7ven", "Stadt", "Fi\u00b7nan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In perturbationem geraten sind,", "tokens": ["In", "per\u00b7tur\u00b7ba\u00b7ti\u00b7o\u00b7nem", "ge\u00b7ra\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVPP", "VAFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wasma\u00dfen ferner und alldieweilen", "tokens": ["Was\u00b7ma\u00b7\u00dfen", "fer\u00b7ner", "und", "all\u00b7die\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Kosten der Repraesentatio", "tokens": ["Die", "Kos\u00b7ten", "der", "Re\u00b7prae\u00b7sen\u00b7ta\u00b7tio"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In urbis gloriam erheblich wachsen,", "tokens": ["In", "ur\u00b7bis", "glo\u00b7ri\u00b7am", "er\u00b7heb\u00b7lich", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beschlossen die Collegia so:", "tokens": ["Be\u00b7schlos\u00b7sen", "die", "Col\u00b7le\u00b7gia", "so", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die dritten Kindlein sind hinf\u00fcro", "tokens": ["Die", "drit\u00b7ten", "Kin\u00b7dlein", "sind", "hin\u00b7f\u00fc\u00b7ro"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Rechts der Schulgeldfreiheit bar,", "tokens": ["Des", "Rechts", "der", "Schul\u00b7geld\u00b7frei\u00b7heit", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuwiderhandlung geschieht von jetzt ab", "tokens": ["Zu\u00b7wi\u00b7der\u00b7hand\u00b7lung", "ge\u00b7schieht", "von", "jetzt", "ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADV", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auf eigene Rechnung und Gefahr.", "tokens": ["Auf", "ei\u00b7ge\u00b7ne", "Rech\u00b7nung", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Dies tuen kund wir und zu wissen", "tokens": ["Dies", "tu\u00b7en", "kund", "wir", "und", "zu", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVINF", "VMFIN", "PPER", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den B\u00fcrgern Unserer lieben Stadt,", "tokens": ["Den", "B\u00fcr\u00b7gern", "Un\u00b7se\u00b7rer", "lie\u00b7ben", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Inquilinis ebenfallsig;", "tokens": ["Den", "In\u00b7qui\u00b7li\u00b7nis", "e\u00b7ben\u00b7fall\u00b7sig", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wonach sich zu richten! Der Magistrat.", "tokens": ["Wo\u00b7nach", "sich", "zu", "rich\u00b7ten", "!", "Der", "Ma\u00b7gist\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PTKZU", "VVINF", "$.", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}