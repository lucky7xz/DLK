{"textgrid.poem.39355": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Jugend flieht, die Hoffnung ist zerronnen,", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jugend flieht, die Hoffnung ist zerronnen,", "tokens": ["Die", "Ju\u00b7gend", "flieht", ",", "die", "Hoff\u00b7nung", "ist", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens Bl\u00fcthen fallen welkend ab,", "tokens": ["Des", "Le\u00b7bens", "Bl\u00fc\u00b7then", "fal\u00b7len", "wel\u00b7kend", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unerreichbar fern sind meine Wonnen,", "tokens": ["Und", "un\u00b7er\u00b7reich\u00b7bar", "fern", "sind", "mei\u00b7ne", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stumm und einsam bin ich wie ein Grab.", "tokens": ["Und", "stumm", "und", "ein\u00b7sam", "bin", "ich", "wie", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im ganzen weiten Reich der Wesen", "tokens": ["Im", "gan\u00b7zen", "wei\u00b7ten", "Reich", "der", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du allein die Zaubermacht,", "tokens": ["Hast", "du", "al\u00b7lein", "die", "Zau\u00b7ber\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich von dem Gram zu l\u00f6sen,", "tokens": ["Mich", "von", "dem", "Gram", "zu", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der jeden Trost verlacht.", "tokens": ["Der", "je\u00b7den", "Trost", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ach! ich mu\u00df vor deinem Willen schweigen;", "tokens": ["Und", "ach", "!", "ich", "mu\u00df", "vor", "dei\u00b7nem", "Wil\u00b7len", "schwei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was er verh\u00e4ngt, wird hoch von mir geehrt.", "tokens": ["Was", "er", "ver\u00b7h\u00e4ngt", ",", "wird", "hoch", "von", "mir", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was h\u00fclf' es auch zu reden? Ihn zu beugen?", "tokens": ["Was", "h\u00fcl\u00b7f'", "es", "auch", "zu", "re\u00b7den", "?", "Ihn", "zu", "beu\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "So k\u00fchner Wahn hat nimmer mich beth\u00f6rt.", "tokens": ["So", "k\u00fch\u00b7ner", "Wahn", "hat", "nim\u00b7mer", "mich", "be\u00b7th\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du kennst das h\u00f6chste Ziel des Lebens,", "tokens": ["Du", "kennst", "das", "h\u00f6chs\u00b7te", "Ziel", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zeichnest deine Bahn dir vor.", "tokens": ["Und", "zeich\u00b7nest", "dei\u00b7ne", "Bahn", "dir", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Flehen schlug vergebens", "tokens": ["Mein", "Fle\u00b7hen", "schlug", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Voll Inbrunst an dein Ohr.", "tokens": ["Voll", "In\u00b7brunst", "an", "dein", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Zwar giengest du nicht taub vor mir vor\u00fcber,", "tokens": ["Zwar", "gien\u00b7gest", "du", "nicht", "taub", "vor", "mir", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist ein Weib, und Weichheit ist dein Stolz.", "tokens": ["Du", "bist", "ein", "Weib", ",", "und", "Weich\u00b7heit", "ist", "dein", "Stolz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Busen bebte mir in jeder Fiber,", "tokens": ["Mein", "Bu\u00b7sen", "beb\u00b7te", "mir", "in", "je\u00b7der", "Fi\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als nun dein Blick um mich in Thr\u00e4nen schmolz.", "tokens": ["Als", "nun", "dein", "Blick", "um", "mich", "in", "Thr\u00e4\u00b7nen", "schmolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "PRF", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den s\u00fc\u00dfen Thau der holden Augen", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Thau", "der", "hol\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verschlang mein Herz, wie d\u00fcrres Land.", "tokens": ["Ver\u00b7schlang", "mein", "Herz", ",", "wie", "d\u00fcr\u00b7res", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weh mir! ihn einzusaugen,", "tokens": ["Weh", "mir", "!", "ihn", "ein\u00b7zu\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das n\u00e4hrte nur den Brand.", "tokens": ["Das", "n\u00e4hr\u00b7te", "nur", "den", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich k\u00e4mpfte mich empor und wollte fl\u00fcchten;", "tokens": ["Ich", "k\u00e4mpf\u00b7te", "mich", "em\u00b7por", "und", "woll\u00b7te", "fl\u00fcch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich stie\u00df die dargebotne Hand zur\u00fcck.", "tokens": ["Ich", "stie\u00df", "die", "dar\u00b7ge\u00b7bot\u00b7ne", "Hand", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbo z\u00fcrne mir, sonst wirst du mich vernichten,", "tokens": ["\u00bb", "o", "z\u00fcr\u00b7ne", "mir", ",", "sonst", "wirst", "du", "mich", "ver\u00b7nich\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich peinigt dieser g\u00f6ttlich milde Blick.", "tokens": ["Mich", "pei\u00b7nigt", "die\u00b7ser", "g\u00f6tt\u00b7lich", "mil\u00b7de", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "War's Frevel, da\u00df ich so entgl\u00fchte?", "tokens": ["Wa\u00b7r's", "Fre\u00b7vel", ",", "da\u00df", "ich", "so", "ent\u00b7gl\u00fch\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "O du bist edel! gieb mich los!", "tokens": ["O", "du", "bist", "e\u00b7del", "!", "gieb", "mich", "los", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df ab mit deiner G\u00fcte!", "tokens": ["La\u00df", "ab", "mit", "dei\u00b7ner", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo nicht: sei minder gro\u00df!\u00ab", "tokens": ["Wo", "nicht", ":", "sei", "min\u00b7der", "gro\u00df", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "VAFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So rief ich aus. Was half mein Widerstreben?", "tokens": ["So", "rief", "ich", "aus", ".", "Was", "half", "mein", "Wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchlte mich von unsichtbarer Kraft,", "tokens": ["Ich", "f\u00fchl\u00b7te", "mich", "von", "un\u00b7sicht\u00b7ba\u00b7rer", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Schicksal selbst in deine Hand gegeben,", "tokens": ["Vom", "Schick\u00b7sal", "selbst", "in", "dei\u00b7ne", "Hand", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die, was sie will, aus meinem Wesen schafft.", "tokens": ["Die", ",", "was", "sie", "will", ",", "aus", "mei\u00b7nem", "We\u00b7sen", "schafft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich klage nicht; ich will es tragen.", "tokens": ["Ich", "kla\u00b7ge", "nicht", ";", "ich", "will", "es", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dank dir! Mich adelt dieses Leid.", "tokens": ["Dank", "dir", "!", "Mich", "a\u00b7delt", "die\u00b7ses", "Leid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gest\u00e4hlt durch mein Entsagen", "tokens": ["Ge\u00b7st\u00e4hlt", "durch", "mein", "Ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Besteh' ich jeden Streit.", "tokens": ["Be\u00b7steh'", "ich", "je\u00b7den", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Jugend Flur voll heller Gaukelscenen,", "tokens": ["Der", "Ju\u00b7gend", "Flur", "voll", "hel\u00b7ler", "Gau\u00b7kel\u00b7sce\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der W\u00fcnsch' und Tr\u00e4ume l\u00e4chelndes Revier,", "tokens": ["Der", "W\u00fcn\u00b7sch'", "und", "Tr\u00e4u\u00b7me", "l\u00e4\u00b7cheln\u00b7des", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wohin ich sonst mit hoffnungsvollem Sehnen", "tokens": ["Wo\u00b7hin", "ich", "sonst", "mit", "hoff\u00b7nungs\u00b7vol\u00b7lem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich oft verirrt, liegt \u00f6de hinter mir.", "tokens": ["Mich", "oft", "ver\u00b7irrt", ",", "liegt", "\u00f6\u00b7de", "hin\u00b7ter", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gleichg\u00fcltig steh' ich im Get\u00fcmmel,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "steh'", "ich", "im", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das nach Genu\u00df sich dr\u00e4ngt; f\u00fcr mich", "tokens": ["Das", "nach", "Ge\u00b7nu\u00df", "sich", "dr\u00e4ngt", ";", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "APPR", "NN", "PRF", "VVFIN", "$.", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "W\u00e4r' auch der Sel'gen Himmel", "tokens": ["W\u00e4r'", "auch", "der", "Sel'\u00b7gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Chaos ohne dich.", "tokens": ["Ein", "Chaos", "oh\u00b7ne", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Das Gl\u00fcck ist arm; ich spotte seiner Gaben;", "tokens": ["Das", "Gl\u00fcck", "ist", "arm", ";", "ich", "spot\u00b7te", "sei\u00b7ner", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In mir ist mehr, als es mir bieten kann.", "tokens": ["In", "mir", "ist", "mehr", ",", "als", "es", "mir", "bie\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter