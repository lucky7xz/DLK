{"textgrid.poem.59102": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "321. Dem Absalom durchs Herz", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vorwand, die Vern\u00fcnftelei", "tokens": ["Der", "Vor\u00b7wand", ",", "die", "Ver\u00b7n\u00fcnf\u00b7te\u00b7lei"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist heimlich, fein und mancherlei.", "tokens": ["Ist", "heim\u00b7lich", ",", "fein", "und", "man\u00b7cher\u00b7lei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJD", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Folg du gerad' dem innern Leiter!", "tokens": ["Folg", "du", "ge\u00b7rad'", "dem", "in\u00b7nern", "Lei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer sich verschont, der kommt nicht weiter.", "tokens": ["Wer", "sich", "ver\u00b7schont", ",", "der", "kommt", "nicht", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVPP", "$,", "PRELS", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}