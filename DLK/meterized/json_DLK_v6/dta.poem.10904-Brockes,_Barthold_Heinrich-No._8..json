{"dta.poem.10904": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "No.  8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm verwachsenen Gefilde, zwischen dick - beb\u00fcschten H\u00fcgeln,", "tokens": ["Jm", "ver\u00b7wach\u00b7se\u00b7nen", "Ge\u00b7fil\u00b7de", ",", "zwi\u00b7schen", "dick", "be\u00b7b\u00fcschten", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "ADJD", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Jm mit Schilf bekr\u00e4nzten Bach, der im Widerschein stets gr\u00fcn,", "tokens": ["Jm", "mit", "Schilf", "be\u00b7kr\u00e4nz\u00b7ten", "Bach", ",", "der", "im", "Wi\u00b7der\u00b7schein", "stets", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "APPR", "NN", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Durch der gr\u00fcnen Bl\u00e4tter Schatten, in polirter Klarheit", "tokens": ["Durch", "der", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7ter", "Schat\u00b7ten", ",", "in", "po\u00b7lir\u00b7ter", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Sieht man hier den edlen Hirsch weiden, und zugleich sich", "tokens": ["Sieht", "man", "hier", "den", "ed\u00b7len", "Hirsch", "wei\u00b7den", ",", "und", "zu\u00b7gleich", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVINF", "$,", "KON", "ADV", "PRF"], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.5": {"text": "In des offnen Maules Stellung sieht man deutlich, da\u00df er", "tokens": ["In", "des", "off\u00b7nen", "Mau\u00b7les", "Stel\u00b7lung", "sieht", "man", "deut\u00b7lich", ",", "da\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "PIS", "ADJD", "$,", "KOUS", "PPER"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Wie die feucht und frische Kost ihn mit Anmuth n\u00e4hr und", "tokens": ["Wie", "die", "feucht", "und", "fri\u00b7sche", "Kost", "ihn", "mit", "An\u00b7muth", "n\u00e4hr", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJD", "KON", "ADJA", "NN", "PPER", "APPR", "NN", "ADJD", "KON"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Doch sein Auge zeigt zugleich, da\u00df sein pr\u00e4chtiges Geweih,", "tokens": ["Doch", "sein", "Au\u00b7ge", "zeigt", "zu\u00b7gleich", ",", "da\u00df", "sein", "pr\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7weih", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "So der Wiederschein ihm zeiget, seiner Blicke Vorwurf sey.", "tokens": ["So", "der", "Wie\u00b7der\u00b7schein", "ihm", "zei\u00b7get", ",", "sei\u00b7ner", "Bli\u00b7cke", "Vor\u00b7wurf", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wer bewundert, der die\u00df siehet, nicht des K\u00fcnstlers kluge", "tokens": ["Wer", "be\u00b7wun\u00b7dert", ",", "der", "die\u00df", "sie\u00b7het", ",", "nicht", "des", "K\u00fcnst\u00b7lers", "klu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVPP", "$,", "PRELS", "PDS", "VVFIN", "$,", "PTKNEG", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.10": {"text": "Jeder Punkt zeigt einen Geist, jede Linie Verstand.", "tokens": ["Je\u00b7der", "Punkt", "zeigt", "ei\u00b7nen", "Geist", ",", "je\u00b7de", "Li\u00b7nie", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+---+-+", "measure": "unknown.measure.hexa"}, "line.11": {"text": "Aber h\u00f6rt! erkennt dabey, wenn euch sein Gem\u00e4lde r\u00fchret,", "tokens": ["A\u00b7ber", "h\u00f6rt", "!", "er\u00b7kennt", "da\u00b7bey", ",", "wenn", "euch", "sein", "Ge\u00b7m\u00e4l\u00b7de", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PAV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Da\u00df er uns, durch die Copie, zum weit sch\u00f6nern Urbild f\u00fchret.", "tokens": ["Da\u00df", "er", "uns", ",", "durch", "die", "Co\u00b7pie", ",", "zum", "weit", "sch\u00f6\u00b7nern", "Ur\u00b7bild", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "APPR", "ART", "NN", "$,", "APPRART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}