{"textgrid.poem.61171": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Meiden", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schleicht ein zehrend Feuer", "tokens": ["Es", "schleicht", "ein", "zeh\u00b7rend", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch mein Gebein;", "tokens": ["Durch", "mein", "Ge\u00b7bein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Schatt' ist mir nicht treuer", "tokens": ["Mein", "Schatt'", "ist", "mir", "nicht", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie diese Pein.", "tokens": ["Wie", "die\u00b7se", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich h\u00f6re die Stunden ziehen", "tokens": ["Ich", "h\u00f6\u00b7re", "die", "Stun\u00b7den", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Tr\u00fcben Gesichts;", "tokens": ["Tr\u00fc\u00b7ben", "Ge\u00b7sichts", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sie kommen, weilen, fliehen -", "tokens": ["Sie", "kom\u00b7men", ",", "wei\u00b7len", ",", "flie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und \u00e4ndern nichts.", "tokens": ["Und", "\u00e4n\u00b7dern", "nichts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Sommer kommt gegangen,", "tokens": ["Der", "Som\u00b7mer", "kommt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir ist's wie Traum;", "tokens": ["Mir", "ist's", "wie", "Traum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Am Busch Wildr\u00f6slein hangen,", "tokens": ["Am", "Busch", "Wild\u00b7r\u00f6s\u00b7lein", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich acht' es kaum.", "tokens": ["Ich", "acht'", "es", "kaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es schlagen die Nachtigallen", "tokens": ["Es", "schla\u00b7gen", "die", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "In Wald und Plan,", "tokens": ["In", "Wald", "und", "Plan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "La\u00df schallen, la\u00df verhallen!", "tokens": ["La\u00df", "schal\u00b7len", ",", "la\u00df", "ver\u00b7hal\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was geht's mich an?", "tokens": ["Was", "geht's", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchle nur das eine", "tokens": ["Ich", "f\u00fch\u00b7le", "nur", "das", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In meinem Sinn:", "tokens": ["In", "mei\u00b7nem", "Sinn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df ich von dir, du reine,", "tokens": ["Da\u00df", "ich", "von", "dir", ",", "du", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geschieden bin.", "tokens": ["Ge\u00b7schie\u00b7den", "bin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein Schatt' ist mir nicht treuer", "tokens": ["Mein", "Schatt'", "ist", "mir", "nicht", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie diese Pein;", "tokens": ["Wie", "die\u00b7se", "Pein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und zehrend schleicht das Feuer", "tokens": ["Und", "zeh\u00b7rend", "schleicht", "das", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch mein Gebein.", "tokens": ["Durch", "mein", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}