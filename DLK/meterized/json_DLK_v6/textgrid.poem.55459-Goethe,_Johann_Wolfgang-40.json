{"textgrid.poem.55459": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "40", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib acht! es wird dir allerlei begegnen,", "tokens": ["Gib", "acht", "!", "es", "wird", "dir", "al\u00b7ler\u00b7lei", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$.", "PPER", "VAFIN", "PPER", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bist du im Trocknen, wird es regnen,", "tokens": ["Bist", "du", "im", "Trock\u00b7nen", ",", "wird", "es", "reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Schwimmen wird die Welle sich versagen,", "tokens": ["Zum", "Schwim\u00b7men", "wird", "die", "Wel\u00b7le", "sich", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wen aber hast du deshalb anzuklagen?", "tokens": ["Wen", "a\u00b7ber", "hast", "du", "des\u00b7halb", "an\u00b7zu\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "PAV", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Merkst du nicht eifers\u00fccht'gen Zorn?", "tokens": ["Merkst", "du", "nicht", "ei\u00b7fer\u00b7s\u00fccht'\u00b7gen", "Zorn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein L\u00e4cheln wird er wohl verdienen;", "tokens": ["Ein", "L\u00e4\u00b7cheln", "wird", "er", "wohl", "ver\u00b7die\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und du verzeihst dem Onkel K\u00fchleborn,", "tokens": ["Und", "du", "ver\u00b7zeihst", "dem", "On\u00b7kel", "K\u00fch\u00b7le\u00b7born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Man sagt ihm nach, er liebe selbst Undinen.", "tokens": ["Man", "sagt", "ihm", "nach", ",", "er", "lie\u00b7be", "selbst", "Un\u00b7di\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}