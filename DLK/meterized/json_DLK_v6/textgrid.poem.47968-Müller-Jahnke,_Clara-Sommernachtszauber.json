{"textgrid.poem.47968": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Sommernachtszauber", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam in der Julinacht bin ich tr\u00e4umend heimgegangen;", "tokens": ["Ein\u00b7sam", "in", "der", "Ju\u00b7li\u00b7nacht", "bin", "ich", "tr\u00e4u\u00b7mend", "heim\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "schmeichelnd hielt Resedenduft meine Sinne s\u00fc\u00df umfangen.", "tokens": ["schmei\u00b7chelnd", "hielt", "Re\u00b7se\u00b7den\u00b7duft", "mei\u00b7ne", "Sin\u00b7ne", "s\u00fc\u00df", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.3": {"text": "Durch die Lindenzweige ging fl\u00fcsternd ein geheimes Sehnen,", "tokens": ["Durch", "die", "Lin\u00b7den\u00b7zwei\u00b7ge", "ging", "fl\u00fcs\u00b7ternd", "ein", "ge\u00b7hei\u00b7mes", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "von den Bl\u00fcten fiel der Tau leis und lind wie Liebestr\u00e4nen.", "tokens": ["von", "den", "Bl\u00fc\u00b7ten", "fiel", "der", "Tau", "leis", "und", "lind", "wie", "Lie\u00b7be\u00b7str\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "KOKOM", "NN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Einsam durch die Julinacht irrten Mandolinenkl\u00e4nge,", "tokens": ["Ein\u00b7sam", "durch", "die", "Ju\u00b7li\u00b7nacht", "irr\u00b7ten", "Man\u00b7do\u00b7li\u00b7nen\u00b7kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "ach, als ob aus Fernen weit deine Stimme zu mir dr\u00e4nge,", "tokens": ["ach", ",", "als", "ob", "aus", "Fer\u00b7nen", "weit", "dei\u00b7ne", "Stim\u00b7me", "zu", "mir", "dr\u00e4n\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "KOKOM", "KOUS", "APPR", "ADJA", "ADJD", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "deine Stimme, die mir einst weich in wogenden Akkorden", "tokens": ["dei\u00b7ne", "Stim\u00b7me", ",", "die", "mir", "einst", "weich", "in", "wo\u00b7gen\u00b7den", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "wie Musik ert\u00f6nt \u2013 und jetzt klanglos, unstet, fremd geworden . . .", "tokens": ["wie", "Mu\u00b7sik", "er\u00b7t\u00f6nt", "\u2013", "und", "jetzt", "klang\u00b7los", ",", "un\u00b7stet", ",", "fremd", "ge\u00b7wor\u00b7den", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$(", "KON", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAPP", "$.", "$.", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Fern aus S\u00fcd ein Windhauch kam; heimlich durch das Lindendunkel", "tokens": ["Fern", "aus", "S\u00fcd", "ein", "Wind\u00b7hauch", "kam", ";", "heim\u00b7lich", "durch", "das", "Lin\u00b7den\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ART", "NN", "VVFIN", "$.", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "blitzte, deinen Augen gleich, tr\u00e4umerisches Lichtgefunkel.", "tokens": ["blitz\u00b7te", ",", "dei\u00b7nen", "Au\u00b7gen", "gleich", ",", "tr\u00e4u\u00b7me\u00b7ri\u00b7sches", "Licht\u00b7ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Leuchtend fiel ein Stern herab \u2013 ach, wo mocht' sein Strahl sich senken?!", "tokens": ["Leuch\u00b7tend", "fiel", "ein", "Stern", "her\u00b7ab", "\u2013", "ach", ",", "wo", "mocht'", "sein", "Strahl", "sich", "sen\u00b7ken", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "$(", "XY", "$,", "PWAV", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Einsam in der Julinacht, weinend mu\u00dft ich dein gedenken. \u2013", "tokens": ["Ein\u00b7sam", "in", "der", "Ju\u00b7li\u00b7nacht", ",", "wei\u00b7nend", "mu\u00dft", "ich", "dein", "ge\u00b7den\u00b7ken", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "ADJD", "VMFIN", "PPER", "PPOSAT", "VVINF", "$.", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}