{"dta.poem.9769": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria/ im nahmen des Herrn Br\u00e4utigams.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So giebst du dich mein kind in deines dieners armen/", "tokens": ["So", "giebst", "du", "dich", "mein", "kind", "in", "dei\u00b7nes", "die\u00b7ners", "ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und siehest meinen schmertz mit milden augen an!", "tokens": ["Und", "sie\u00b7hest", "mei\u00b7nen", "schmertz", "mit", "mil\u00b7den", "au\u00b7gen", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du pflagst dem himmel gleich dich endlich zu erbarmen;", "tokens": ["Du", "pflagst", "dem", "him\u00b7mel", "gleich", "dich", "end\u00b7lich", "zu", "er\u00b7bar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach dem du mir das joch der fessel angethan.", "tokens": ["Nach", "dem", "du", "mir", "das", "joch", "der", "fes\u00b7sel", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch la\u00df dich scham und furcht nur l\u00e4nger nicht verweilen:", "tokens": ["Doch", "la\u00df", "dich", "scham", "und", "furcht", "nur", "l\u00e4n\u00b7ger", "nicht", "ver\u00b7wei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "KON", "NN", "ADV", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist ein himmlisch werck: die matten seelen heilen.", "tokens": ["Es", "ist", "ein", "himm\u00b7lisch", "werck", ":", "die", "mat\u00b7ten", "see\u00b7len", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mein engel r\u00f6the nicht die unentweihte wangen/", "tokens": ["Mein", "en\u00b7gel", "r\u00f6\u00b7the", "nicht", "die", "un\u00b7ent\u00b7weih\u00b7te", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und lieffre willig mir den werthen jungfern-crantz.", "tokens": ["Und", "lieff\u00b7re", "wil\u00b7lig", "mir", "den", "wert\u00b7hen", "jung\u00b7fern\u00b7crantz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ART", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach w\u00fcnsche l\u00e4nger nicht in solcher zier zu prangen/", "tokens": ["Ach", "w\u00fcn\u00b7sche", "l\u00e4n\u00b7ger", "nicht", "in", "sol\u00b7cher", "zier", "zu", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADJD", "PTKNEG", "APPR", "PIAT", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es ist ein falscher schmuck/ und ein geschminckter glantz.", "tokens": ["Es", "ist", "ein", "fal\u00b7scher", "schmuck", "/", "und", "ein", "ge\u00b7schminck\u00b7ter", "glantz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verwirff die m\u00fcrbe pracht/ die zwar die scheitel schm\u00fccket/", "tokens": ["Ver\u00b7wirff", "die", "m\u00fcr\u00b7be", "pracht", "/", "die", "zwar", "die", "schei\u00b7tel", "schm\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$(", "PRELS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und doch die freyheit nur mit centner-lasten dr\u00fccket.", "tokens": ["Und", "doch", "die", "frey\u00b7heit", "nur", "mit", "cent\u00b7ner\u00b7las\u00b7ten", "dr\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bedencke nur vielmehr du/ meine werthe taube/", "tokens": ["Be\u00b7den\u00b7cke", "nur", "viel\u00b7mehr", "du", "/", "mei\u00b7ne", "wert\u00b7he", "tau\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPER", "$(", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie du der Venus magst geschickt entgegen gehn.", "tokens": ["Wie", "du", "der", "Ve\u00b7nus", "magst", "ge\u00b7schickt", "ent\u00b7ge\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "ADJD", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es wird dir/ liebster schatz/ die s\u00fcsse frauen-haube/", "tokens": ["Es", "wird", "dir", "/", "liebs\u00b7ter", "schatz", "/", "die", "s\u00fcs\u00b7se", "frau\u00b7en\u00b7hau\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mehr als du selber denckst/ gesichert/ artig stehn;", "tokens": ["Mehr", "als", "du", "sel\u00b7ber", "denckst", "/", "ge\u00b7si\u00b7chert", "/", "ar\u00b7tig", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPER", "ADV", "VVFIN", "$(", "VVPP", "$(", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum schaue nur getrost die zarten blumen beugen/", "tokens": ["Drum", "schau\u00b7e", "nur", "ge\u00b7trost", "die", "zar\u00b7ten", "blu\u00b7men", "beu\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ADJD", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil doch ein Ph\u00f6nix soll aus ihrer asche steigen.", "tokens": ["Weil", "doch", "ein", "Ph\u00f6\u00b7nix", "soll", "aus", "ih\u00b7rer", "asc\u00b7he", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Verwirff das rauhe pfand der unerfahrnen sitten/", "tokens": ["Ver\u00b7wirff", "das", "rau\u00b7he", "pfand", "der", "un\u00b7er\u00b7fahr\u00b7nen", "sit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und lerne was die glut der warmen adern will.", "tokens": ["Und", "ler\u00b7ne", "was", "die", "glut", "der", "war\u00b7men", "a\u00b7dern", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ART", "NN", "ART", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es schaut der strenge crantz genau nach allen tritten/", "tokens": ["Es", "schaut", "der", "stren\u00b7ge", "crantz", "ge\u00b7nau", "nach", "al\u00b7len", "trit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und setzt der jungferschafft ein gar zu enges ziel.", "tokens": ["Und", "setzt", "der", "jung\u00b7fer\u00b7schafft", "ein", "gar", "zu", "en\u00b7ges", "ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie kan ein wespen-stich die blumen leicht verletzen/", "tokens": ["Wie", "kan", "ein", "wes\u00b7pen\u00b7stich", "die", "blu\u00b7men", "leicht", "ver\u00b7let\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "ADJD", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ein erhitzter strahl sie aller zier entsetzen!", "tokens": ["Und", "ein", "er\u00b7hitz\u00b7ter", "strahl", "sie", "al\u00b7ler", "zier", "ent\u00b7set\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Drum ach beseuffze nicht du engel meine liebe/", "tokens": ["Drum", "ach", "be\u00b7seuff\u00b7ze", "nicht", "du", "en\u00b7gel", "mei\u00b7ne", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PTKNEG", "PPER", "NN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und achte meinen ku\u00df vor keine galle nicht;", "tokens": ["Und", "ach\u00b7te", "mei\u00b7nen", "ku\u00df", "vor", "kei\u00b7ne", "gal\u00b7le", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPOSAT", "NN", "APPR", "PIAT", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der beste nectar-safft schmeckt in der ersten tr\u00fcbe/", "tokens": ["Der", "bes\u00b7te", "nec\u00b7ta\u00b7r\u00b7safft", "schmeckt", "in", "der", "ers\u00b7ten", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Gnug/ da\u00df man auch zugleich von dornen rosen bricht.", "tokens": ["Gnug", "/", "da\u00df", "man", "auch", "zu\u00b7gleich", "von", "dor\u00b7nen", "ro\u00b7sen", "bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PIS", "ADV", "ADV", "APPR", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dann pflegt das gr\u00f6ste leid die beste lust zu machen/", "tokens": ["Dann", "pflegt", "das", "gr\u00f6s\u00b7te", "leid", "die", "bes\u00b7te", "lust", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn man nach kurtzem weh die seuffzer kan belachen.", "tokens": ["Wenn", "man", "nach", "kurt\u00b7zem", "weh", "die", "seuff\u00b7zer", "kan", "be\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Nun schatz/ der abend-stern tritt allgemach zur wache/", "tokens": ["Nun", "schatz", "/", "der", "a\u00b7ben\u00b7ds\u00b7tern", "tritt", "all\u00b7ge\u00b7mach", "zur", "wa\u00b7che", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Und die empfindligkeit wil schon zu bette seyn.", "tokens": ["Und", "die", "emp\u00b7find\u00b7lig\u00b7keit", "wil", "schon", "zu", "bet\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "ADV", "PTKZU", "VVFIN", "VAINF", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Cupido steht und lauscht vor unserm schlaffgemache/", "tokens": ["Cu\u00b7pi\u00b7do", "steht", "und", "lauscht", "vor", "un\u00b7serm", "schlaff\u00b7ge\u00b7ma\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und fordert selbst den schmuck von deiner scheitel ein;", "tokens": ["Und", "for\u00b7dert", "selbst", "den", "schmuck", "von", "dei\u00b7ner", "schei\u00b7tel", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er rufft: verliebtes paar/ komm/ schleu\u00df den m\u00fcden reihen/", "tokens": ["Er", "rufft", ":", "ver\u00b7lieb\u00b7tes", "paar", "/", "komm", "/", "schleu\u00df", "den", "m\u00fc\u00b7den", "rei\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADJA", "NN", "$(", "VVFIN", "$(", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die glut ist schon bereit/ das opffer einzuweihen.", "tokens": ["Die", "glut", "ist", "schon", "be\u00b7reit", "/", "das", "opf\u00b7fer", "ein\u00b7zu\u00b7wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mich deucht/ ich breche schon die himmel-s\u00fcsse k\u00fcsse", "tokens": ["Mich", "deucht", "/", "ich", "bre\u00b7che", "schon", "die", "him\u00b7mel\u00b7s\u00fcs\u00b7se", "k\u00fcs\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Von deinen lippen ab: drum engel eile doch;", "tokens": ["Von", "dei\u00b7nen", "lip\u00b7pen", "ab", ":", "drum", "en\u00b7gel", "ei\u00b7le", "doch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "PAV", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die seele wallet schon auff die verliebten bisse/", "tokens": ["Die", "see\u00b7le", "wal\u00b7let", "schon", "auff", "die", "ver\u00b7lieb\u00b7ten", "bis\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Ach komm und la\u00df den crantz/ das kleine kummer-joch;", "tokens": ["Ach", "komm", "und", "la\u00df", "den", "crantz", "/", "das", "klei\u00b7ne", "kum\u00b7mer\u00b7joch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "KON", "VVIMP", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measu