{"dta.poem.6705": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Dummer ist sowol, als der, so klug, ger\u00fchrt.", "tokens": ["Ein", "Dum\u00b7mer", "ist", "so\u00b7wol", ",", "als", "der", ",", "so", "klug", ",", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "KOUS", "ART", "$,", "ADV", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In aller C\u00f6rper Meng, die wir bey uns erblicken,", "tokens": ["In", "al\u00b7ler", "C\u00f6r\u00b7per", "Meng", ",", "die", "wir", "bey", "uns", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NE", "$,", "PRELS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In allen, die den Bau des weiten Himmels schm\u00fccken,", "tokens": ["In", "al\u00b7len", ",", "die", "den", "Bau", "des", "wei\u00b7ten", "Him\u00b7mels", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird von dem Geiste, DER, so sie gemacht, versp\u00fcrt.", "tokens": ["Wird", "von", "dem", "Geis\u00b7te", ",", "DeR", ",", "so", "sie", "ge\u00b7macht", ",", "ver\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "NE", "$,", "ADV", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Ordnungen, die unzerst\u00f6rlich,", "tokens": ["Die", "Ord\u00b7nun\u00b7gen", ",", "die", "un\u00b7zer\u00b7st\u00f6r\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und welchen selbst der Ur-Stoff unterthan, ", "tokens": ["Und", "wel\u00b7chen", "selbst", "der", "Ur\u00b7Stoff", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die reden alle unaufh\u00f6rlich", "tokens": ["Die", "re\u00b7den", "al\u00b7le", "un\u00b7auf\u00b7h\u00f6r\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Menschen Augen an.", "tokens": ["Der", "Men\u00b7schen", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Der sichtbare Zusammenklang", "tokens": ["Der", "sicht\u00b7ba\u00b7re", "Zu\u00b7sam\u00b7men\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ist allenthalben zu verstehen,", "tokens": ["Ist", "al\u00b7len\u00b7thal\u00b7ben", "zu", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}