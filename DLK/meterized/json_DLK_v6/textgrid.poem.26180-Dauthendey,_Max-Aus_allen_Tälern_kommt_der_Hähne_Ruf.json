{"textgrid.poem.26180": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Aus allen T\u00e4lern kommt der H\u00e4hne Ruf", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus allen T\u00e4lern kommt der H\u00e4hne Ruf,", "tokens": ["Aus", "al\u00b7len", "T\u00e4\u00b7lern", "kommt", "der", "H\u00e4h\u00b7ne", "Ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als ob sich jede Meile eine Stimme schuf,", "tokens": ["Als", "ob", "sich", "je\u00b7de", "Mei\u00b7le", "ei\u00b7ne", "Stim\u00b7me", "schuf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PRF", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weckt die Erde in der grauen Runde", "tokens": ["Und", "weckt", "die", "Er\u00b7de", "in", "der", "grau\u00b7en", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Augen\u00f6ffnen in der Morgenstunde.", "tokens": ["Zum", "Au\u00b7ge\u00b7n\u00f6ff\u00b7nen", "in", "der", "Mor\u00b7gen\u00b7stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dazwischen noch ein K\u00e4uzchen leise klagt,", "tokens": ["Da\u00b7zwi\u00b7schen", "noch", "ein", "K\u00e4uz\u00b7chen", "lei\u00b7se", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dem Dunkel es nur \u00e4ngstlich Abschied sagt.", "tokens": ["Dem", "Dun\u00b7kel", "es", "nur", "\u00e4ngst\u00b7lich", "Ab\u00b7schied", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Manchem die Nacht mehr als der Tag behagt,", "tokens": ["Man\u00b7chem", "die", "Nacht", "mehr", "als", "der", "Tag", "be\u00b7hagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PIAT", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wenn er im Finstern sich zur Liebsten wagt,", "tokens": ["Wenn", "er", "im", "Fins\u00b7tern", "sich", "zur", "Liebs\u00b7ten", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Weil ihm das Blut zur Lieb' am Tag verzagt.", "tokens": ["Weil", "ihm", "das", "Blut", "zur", "Lieb'", "am", "Tag", "ver\u00b7zagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}