{"textgrid.poem.62682": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Ohne Thema", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder h\u00fcte sich vor Netze, Strick und \u2013 \u2013 Fall,", "tokens": ["Ein", "je\u00b7der", "h\u00fc\u00b7te", "sich", "vor", "Net\u00b7ze", ",", "Strick", "und", "\u2013", "\u2013", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "NN", "$,", "NN", "KON", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Tugend rufet ihn, und zwar mit starkem \u2013 \u2013 Schall", "tokens": ["Die", "Tu\u00b7gend", "ru\u00b7fet", "ihn", ",", "und", "zwar", "mit", "star\u00b7kem", "\u2013", "\u2013", "Schall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "ADV", "APPR", "ADJA", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fast alle Stunden zu: Wer nur zu mir will \u2013 \u2013 kommen,", "tokens": ["Fast", "al\u00b7le", "Stun\u00b7den", "zu", ":", "Wer", "nur", "zu", "mir", "will", "\u2013", "\u2013", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$.", "PWS", "ADV", "APPR", "PPER", "VMFIN", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja wer sich meinen Dienst mit Ernste \u2013 \u2013 vorgenommen,", "tokens": ["Ja", "wer", "sich", "mei\u00b7nen", "Dienst", "mit", "Erns\u00b7te", "\u2013", "\u2013", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PTKANT", "PWS", "PRF", "PPOSAT", "NN", "APPR", "NN", "$(", "$(", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ist sehr hoch begl\u00fcckt, und hat den besten \u2013 \u2013 Schatz,", "tokens": ["Der", "ist", "sehr", "hoch", "be\u00b7gl\u00fcckt", ",", "und", "hat", "den", "bes\u00b7ten", "\u2013", "\u2013", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "KON", "VAFIN", "ART", "ADJA", "$(", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wahre Ruh erwehlt. Der Tugend Sammel \u2013 \u2013 Platz", "tokens": ["Und", "wah\u00b7re", "Ruh", "er\u00b7wehlt", ".", "Der", "Tu\u00b7gend", "Sam\u00b7mel", "\u2013", "\u2013", "Platz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "ADJA", "NN", "VVPP", "$.", "ART", "NN", "NN", "$(", "$(", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan nicht in Seelen seyn, die wie die Thoren \u2013 \u2013 rasen,", "tokens": ["Kan", "nicht", "in", "See\u00b7len", "seyn", ",", "die", "wie", "die", "Tho\u00b7ren", "\u2013", "\u2013", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "NN", "VAINF", "$,", "PRELS", "KOKOM", "ART", "NN", "$(", "$(", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mit der Unvernunft und Aberglauben \u2013 \u2013 spasen.", "tokens": ["Und", "mit", "der", "Un\u00b7ver\u00b7nunft", "und", "A\u00b7berg\u00b7lau\u00b7ben", "\u2013", "\u2013", "spa\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "$(", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}