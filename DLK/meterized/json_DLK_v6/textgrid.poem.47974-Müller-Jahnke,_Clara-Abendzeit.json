{"textgrid.poem.47974": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Abendzeit", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df es noch, so manches Mal,", "tokens": ["Ich", "wei\u00df", "es", "noch", ",", "so", "man\u00b7ches", "Mal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wenn still der Tag zur Ruhe ging,", "tokens": ["wenn", "still", "der", "Tag", "zur", "Ru\u00b7he", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wenn sich der Sonne letzter Strahl", "tokens": ["wenn", "sich", "der", "Son\u00b7ne", "letz\u00b7ter", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in gr\u00fcnen Baumeswipfeln fing,", "tokens": ["in", "gr\u00fc\u00b7nen", "Bau\u00b7mes\u00b7wip\u00b7feln", "fing", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "da ward mir wunderbar zu Sinn", "tokens": ["da", "ward", "mir", "wun\u00b7der\u00b7bar", "zu", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "in duftverkl\u00e4rter Abendzeit,", "tokens": ["in", "duft\u00b7ver\u00b7kl\u00e4r\u00b7ter", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "als wehten durch die Seele hin", "tokens": ["als", "weh\u00b7ten", "durch", "die", "See\u00b7le", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "die Tr\u00e4ume meiner Kinderzeit.", "tokens": ["die", "Tr\u00e4u\u00b7me", "mei\u00b7ner", "Kin\u00b7der\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vergangenheit! Die Sonne sinkt", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "!", "Die", "Son\u00b7ne", "sinkt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und f\u00e4rbt der Wolke blassen Saum", "tokens": ["und", "f\u00e4rbt", "der", "Wol\u00b7ke", "blas\u00b7sen", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und mir im Herzen webt und singt", "tokens": ["und", "mir", "im", "Her\u00b7zen", "webt", "und", "singt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein letztes Lied, ein letzter Traum \u2013", "tokens": ["ein", "letz\u00b7tes", "Lied", ",", "ein", "letz\u00b7ter", "Traum", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "die Rosen welkten allzumal,", "tokens": ["die", "Ro\u00b7sen", "welk\u00b7ten", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die goldne Zeit zu R\u00fcste ging \u2013", "tokens": ["die", "gold\u00b7ne", "Zeit", "zu", "R\u00fcs\u00b7te", "ging", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "kaum, da\u00df ein letzter Sonnenstrahl \u2013", "tokens": ["kaum", ",", "da\u00df", "ein", "letz\u00b7ter", "Son\u00b7nen\u00b7strahl", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "sich in der m\u00fcden Seele fing. \u2013", "tokens": ["sich", "in", "der", "m\u00fc\u00b7den", "See\u00b7le", "fing", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}