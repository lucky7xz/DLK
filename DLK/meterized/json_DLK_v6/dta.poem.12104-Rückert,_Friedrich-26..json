{"dta.poem.12104": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sanskrit, das einen Satz gern in Ein Wort verbindet,", "tokens": ["Sans\u00b7krit", ",", "das", "ei\u00b7nen", "Satz", "gern", "in", "Ein", "Wort", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nennt, wer zu Haus ist da, wo ihn der Abend findet,", "tokens": ["Nennt", ",", "wer", "zu", "Haus", "ist", "da", ",", "wo", "ihn", "der", "A\u00b7bend", "fin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "APPR", "NN", "VAFIN", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es nennt ihn \u201eAbendheim\u201c den Mann vom Bettlerorden,", "tokens": ["Es", "nennt", "ihn", "\u201e", "A\u00b7bend\u00b7heim", "\u201c", "den", "Mann", "vom", "Bett\u00b7le\u00b7ror\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "NN", "$(", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der keine Heimat hat, wie ich nun bin geworden,", "tokens": ["Der", "kei\u00b7ne", "Hei\u00b7mat", "hat", ",", "wie", "ich", "nun", "bin", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "PWAV", "PPER", "ADV", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nicht auf Lebenszeit, dem strengen Jogi gleich,", "tokens": ["Doch", "nicht", "auf", "Le\u00b7bens\u00b7zeit", ",", "dem", "stren\u00b7gen", "Jo\u00b7gi", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "$,", "ART", "ADJA", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der arm an jedem Gut, und nur an Stolz ist reich.", "tokens": ["Der", "arm", "an", "je\u00b7dem", "Gut", ",", "und", "nur", "an", "Stolz", "ist", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PIAT", "NN", "$,", "KON", "ADV", "APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich habe nicht wie er die Heimat aufgegeben,", "tokens": ["Ich", "ha\u00b7be", "nicht", "wie", "er", "die", "Hei\u00b7mat", "auf\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich f\u00fchle ihren Hauch mich \u00fcberall umschweben.", "tokens": ["Ich", "f\u00fch\u00b7le", "ih\u00b7ren", "Hauch", "mich", "\u00fc\u00b7be\u00b7rall", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df an jedem Tag, wo meine Heimat ist,", "tokens": ["Ich", "wei\u00df", "an", "je\u00b7dem", "Tag", ",", "wo", "mei\u00b7ne", "Hei\u00b7mat", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bin am Abend dort, o Liebe, wo du bist.", "tokens": ["Und", "bin", "am", "A\u00b7bend", "dort", ",", "o", "Lie\u00b7be", ",", "wo", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADV", "$,", "FM", "NN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}