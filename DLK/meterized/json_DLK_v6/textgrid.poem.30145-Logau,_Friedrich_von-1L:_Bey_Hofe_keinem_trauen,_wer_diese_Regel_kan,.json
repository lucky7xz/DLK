{"textgrid.poem.30145": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bey Hofe keinem trauen, wer diese Regel kan,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey Hofe keinem trauen, wer diese Regel kan,", "tokens": ["Bey", "Ho\u00b7fe", "kei\u00b7nem", "trau\u00b7en", ",", "wer", "die\u00b7se", "Re\u00b7gel", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "VVINF", "$,", "PWS", "PDAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der kan den Hofe-Glauben und ist ein Hofe-Mann;", "tokens": ["Der", "kan", "den", "Ho\u00b7fe\u00b7Glau\u00b7ben", "und", "ist", "ein", "Ho\u00b7fe\u00b7Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "KON", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Hofe-Catechismus steht meistens drauff und dran.", "tokens": ["Der", "Ho\u00b7fe\u00b7Ca\u00b7te\u00b7chis\u00b7mus", "steht", "meis\u00b7tens", "drauff", "und", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "KON", "PAV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}