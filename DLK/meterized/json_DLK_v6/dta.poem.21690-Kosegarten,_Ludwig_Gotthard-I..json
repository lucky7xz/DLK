{"dta.poem.21690": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem t\u00f6nt des Hymnus Feyerklang?", "tokens": ["Wem", "t\u00f6nt", "des", "Hym\u00b7nus", "Fe\u00b7yer\u00b7klang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wem rauscht der Lyra Hochgesang,", "tokens": ["Wem", "rauscht", "der", "Ly\u00b7ra", "Hoch\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich Nachtigallenschlag itzt schmetternd und itzt", "tokens": ["Gleich", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", "itzt", "schmet\u00b7ternd", "und", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "leise?", "tokens": ["lei\u00b7se", "?"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wen meinen unsers P\u00e4ans Preise?", "tokens": ["Wen", "mei\u00b7nen", "un\u00b7sers", "P\u00e4\u00b7ans", "Prei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dich meinen sie,", "tokens": ["Dich", "mei\u00b7nen", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Allm\u00e4chtige Harmonie.", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Heil dir, dir huldigt die Natur!", "tokens": ["Heil", "dir", ",", "dir", "hul\u00b7digt", "die", "Na\u00b7tur", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Beschworen, Himmlische, von deinem Zauberschwur", "tokens": ["Be\u00b7schwo\u00b7ren", ",", "Himm\u00b7li\u00b7sche", ",", "von", "dei\u00b7nem", "Zau\u00b7ber\u00b7schwur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Tanzt lustberauscht der Wald, h\u00fcpft l\u00fcstern rings die", "tokens": ["Tanzt", "lust\u00b7be\u00b7rauscht", "der", "Wald", ",", "h\u00fcpft", "l\u00fcs\u00b7tern", "rings", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD", "ADV", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Flur \u2014", "tokens": ["Flur"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Und jene, die in nimmer m\u00fcden Kreisen", "tokens": ["Und", "je\u00b7ne", ",", "die", "in", "nim\u00b7mer", "m\u00fc\u00b7den", "Krei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Rings um das Herz des All's melodisch t\u00f6nend reisen,", "tokens": ["Rings", "um", "das", "Herz", "des", "All's", "me\u00b7lo\u00b7disch", "t\u00f6\u00b7nend", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer lenkt, wer z\u00e4umt, wer b\u00e4ndigt sie?", "tokens": ["Wer", "lenkt", ",", "wer", "z\u00e4umt", ",", "wer", "b\u00e4n\u00b7digt", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du thust's, allm\u00e4chtige Harmonie!", "tokens": ["Du", "thust's", ",", "all\u00b7m\u00e4ch\u00b7ti\u00b7ge", "Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dein s\u00fcsses T\u00f6nen, Harmonie, erscholl", "tokens": ["Dein", "s\u00fcs\u00b7ses", "T\u00f6\u00b7nen", ",", "Har\u00b7mo\u00b7nie", ",", "er\u00b7scholl"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und laut aufklangen des Abyssus Schr\u00fcnde.", "tokens": ["Und", "laut", "auf\u00b7klan\u00b7gen", "des", "A\u00b7bys\u00b7sus", "Schr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Lichtes nie erforschter Quell erschwoll", "tokens": ["Des", "Lich\u00b7tes", "nie", "er\u00b7forschter", "Quell", "er\u00b7schwoll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die alte Nacht erschrack; tief in des Abgrunds", "tokens": ["Die", "al\u00b7te", "Nacht", "er\u00b7schrack", ";", "tief", "in", "des", "Ab\u00b7grunds"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Versteckte, jeder Kraft beraubt,", "tokens": ["Ver\u00b7steck\u00b7te", ",", "je\u00b7der", "Kraft", "be\u00b7raubt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Chaos sein ge\u00e4ngstet Haupt.", "tokens": ["Das", "Chaos", "sein", "ge\u00b7\u00e4ngs\u00b7tet", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Hervor, o Harmonie, auf dein melodisch Werde", "tokens": ["Her\u00b7vor", ",", "o", "Har\u00b7mo\u00b7nie", ",", "auf", "dein", "me\u00b7lo\u00b7disch", "Wer\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sprang l\u00e4chelnd der Olymp, sprang thaubeperlt die", "tokens": ["Sprang", "l\u00e4\u00b7chelnd", "der", "O\u00b7lymp", ",", "sprang", "thau\u00b7be\u00b7perlt", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "$,", "VVFIN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Rings reihte sich der Sterne g\u00fcldner Kranz.", "tokens": ["Rings", "reih\u00b7te", "sich", "der", "Ster\u00b7ne", "g\u00fcld\u00b7ner", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In nimmer-lassen, nimmer-stummen Ch\u00f6ren", "tokens": ["In", "nim\u00b7mer\u00b7las\u00b7sen", ",", "nim\u00b7mer\u00b7stum\u00b7men", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Begannen ihren Reigentanz", "tokens": ["Be\u00b7gan\u00b7nen", "ih\u00b7ren", "Rei\u00b7gen\u00b7tanz"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Rund um den Quell des Lichts die ewig-", "tokens": ["Rund", "um", "den", "Quell", "des", "Lichts", "die", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Dein hehtes T\u00f6nen, Harmonie, erscholl,", "tokens": ["Dein", "heh\u00b7tes", "T\u00f6\u00b7nen", ",", "Har\u00b7mo\u00b7nie", ",", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Nacht und Chaos floh und Lieb' und", "tokens": ["Und", "Nacht", "und", "Chaos", "floh", "und", "Lieb'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "KON", "NN", "KON"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}