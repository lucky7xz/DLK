{"textgrid.poem.25545": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein treuer Hengst! du wei\u00dft, ich liebe dich;", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein treuer Hengst! du wei\u00dft, ich liebe dich;", "tokens": ["Mein", "treu\u00b7er", "Hengst", "!", "du", "wei\u00dft", ",", "ich", "lie\u00b7be", "dich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du sollst auch alt in meinem Stalle sterben;", "tokens": ["Du", "sollst", "auch", "alt", "in", "mei\u00b7nem", "Stal\u00b7le", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft, nicht Zorn, nicht Wettlauf reitzte mich,", "tokens": ["Du", "wei\u00dft", ",", "nicht", "Zorn", ",", "nicht", "Wett\u00b7lauf", "reitz\u00b7te", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit deinem Blut' die Sporen roth zu f\u00e4rben.", "tokens": ["Mit", "dei\u00b7nem", "Blut'", "die", "Spo\u00b7ren", "roth", "zu", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich will nicht reich durch deine F\u00fc\u00dfe werden,", "tokens": ["Ich", "will", "nicht", "reich", "durch", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mehr bist du mir als Gold der Wetten werth,", "tokens": ["Mehr", "bist", "du", "mir", "als", "Gold", "der", "Wet\u00b7ten", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "KOUS", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und warest doch von allen schnellen Pferden", "tokens": ["Und", "wa\u00b7rest", "doch", "von", "al\u00b7len", "schnel\u00b7len", "Pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Ach! gutes Thier, was sind f\u00fcnf tausend Pfund,", "tokens": ["Ach", "!", "gu\u00b7tes", "Thier", ",", "was", "sind", "f\u00fcnf", "tau\u00b7send", "Pfund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "$,", "PWS", "VAFIN", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die so geschwind dein leichter Huf errennet?", "tokens": ["Die", "so", "ge\u00b7schwind", "dein", "leich\u00b7ter", "Huf", "er\u00b7ren\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mich machten sie nicht gl\u00fccklich, nicht gesund,", "tokens": ["Mich", "mach\u00b7ten", "sie", "nicht", "gl\u00fcck\u00b7lich", ",", "nicht", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mich Kranken, der ein einzig Gut nur kennet.", "tokens": ["Mich", "Kran\u00b7ken", ",", "der", "ein", "ein\u00b7zig", "Gut", "nur", "ken\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "ADJD", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die\u00df ist das Ziel, zu dem wir heute fliegen,", "tokens": ["Die\u00df", "ist", "das", "Ziel", ",", "zu", "dem", "wir", "heu\u00b7te", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und dieses Ziel, mein Alles in der Welt;", "tokens": ["Und", "die\u00b7ses", "Ziel", ",", "mein", "Al\u00b7les", "in", "der", "Welt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPOSAT", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Ruhm, o Ro\u00df! hat dich gelehrt zu siegen,", "tokens": ["Der", "Ruhm", ",", "o", "Ro\u00df", "!", "hat", "dich", "ge\u00b7lehrt", "zu", "sie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "NN", "$.", "VAFIN", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Die Liebe lehrt allein, wie man gef\u00e4llt.", "tokens": ["Die", "Lie\u00b7be", "lehrt", "al\u00b7lein", ",", "wie", "man", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Kein Wasser sey zu tief, schwimm du hin\u00fcber,", "tokens": ["Kein", "Was\u00b7ser", "sey", "zu", "tief", ",", "schwimm", "du", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Kein Schlagbaum sey zu hoch, kein Steg zu schmal,", "tokens": ["Kein", "Schlag\u00b7baum", "sey", "zu", "hoch", ",", "kein", "Steg", "zu", "schmal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PIAT", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Kein Graben dir zu breit, spring rasch dar\u00fcber,", "tokens": ["Kein", "Gra\u00b7ben", "dir", "zu", "breit", ",", "spring", "rasch", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PTKA", "ADJD", "$,", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Sey nirgend, Ro\u00df! und sey doch \u00fcberall!", "tokens": ["Sey", "nir\u00b7gend", ",", "Ro\u00df", "!", "und", "sey", "doch", "\u00fc\u00b7be\u00b7rall", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "NN", "$.", "KON", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Sieh auf, mein Pferd! auf halbem Wege schreitet", "tokens": ["Sieh", "auf", ",", "mein", "Pferd", "!", "auf", "hal\u00b7bem", "We\u00b7ge", "schrei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die Sonne schon, doch eh' ihr letzter Schein", "tokens": ["Die", "Son\u00b7ne", "schon", ",", "doch", "eh'", "ihr", "letz\u00b7ter", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Noch Purpurfarb' auf mein Gesicht verbreitet,", "tokens": ["Noch", "Pur\u00b7pur\u00b7fa\u00b7rb'", "auf", "mein", "Ge\u00b7sicht", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Mu\u00df ich im Arm' von meinem ", "tokens": ["Mu\u00df", "ich", "im", "Arm'", "von", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Nun biege dich, und nimm geschwind mich auf!", "tokens": ["Nun", "bie\u00b7ge", "dich", ",", "und", "nimm", "ge\u00b7schwind", "mich", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVIMP", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Rasch! tummle dich! die\u00df Ziel noch zu erreichen.", "tokens": ["Rasch", "!", "tumm\u00b7le", "dich", "!", "die\u00df", "Ziel", "noch", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "VVFIN", "PPER", "$.", "PDS", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wie wird sie dir, zum Preis f\u00fcr deinen Lauf,", "tokens": ["Wie", "wird", "sie", "dir", ",", "zum", "Preis", "f\u00fcr", "dei\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "$,", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Den Schwanenhals mit sanften H\u00e4nden streichen!", "tokens": ["Den", "Schwa\u00b7nen\u00b7hals", "mit", "sanf\u00b7ten", "H\u00e4n\u00b7den", "strei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}