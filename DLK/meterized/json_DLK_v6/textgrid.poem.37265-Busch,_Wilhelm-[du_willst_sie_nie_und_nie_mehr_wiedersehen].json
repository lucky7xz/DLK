{"textgrid.poem.37265": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[du willst sie nie und nie mehr wiedersehen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du willst sie nie und nie mehr wiedersehen?", "tokens": ["Du", "willst", "sie", "nie", "und", "nie", "mehr", "wie\u00b7der\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besinne dich, mein Herz, noch ist es Zeit.", "tokens": ["Be\u00b7sin\u00b7ne", "dich", ",", "mein", "Herz", ",", "noch", "ist", "es", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie war so lieb. Verzeih, was auch geschehen.", "tokens": ["Sie", "war", "so", "lieb", ".", "Ver\u00b7zeih", ",", "was", "auch", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "NN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sonst nimmt dich wohl beim Wort die Ewigkeit", "tokens": ["Sonst", "nimmt", "dich", "wohl", "beim", "Wort", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und zwingt dich mit Gewalt zum Weitergehen", "tokens": ["Und", "zwingt", "dich", "mit", "Ge\u00b7walt", "zum", "Wei\u00b7ter\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ins \u00f6de Reich der Allvergessenheit.", "tokens": ["Ins", "\u00f6\u00b7de", "Reich", "der", "All\u00b7ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du rufst und rufst; vergebens sind die Worte;", "tokens": ["Du", "rufst", "und", "rufst", ";", "ver\u00b7ge\u00b7bens", "sind", "die", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ins feste Schlo\u00df dumpfdr\u00f6hnend schl\u00e4gt die Pforte.", "tokens": ["Ins", "fes\u00b7te", "Schlo\u00df", "dumpf\u00b7dr\u00f6h\u00b7nend", "schl\u00e4gt", "die", "Pfor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}