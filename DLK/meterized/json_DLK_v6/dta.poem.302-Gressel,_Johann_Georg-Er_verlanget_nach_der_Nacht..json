{"dta.poem.302": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er verlanget nach der Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Brich doch an du dunckles Wesen!", "tokens": ["Brich", "doch", "an", "du", "dunck\u00b7les", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eile fort du schwartze Nacht!", "tokens": ["Ei\u00b7le", "fort", "du", "schwart\u00b7ze", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mich vor sich auserlesen", "tokens": ["Die", "mich", "vor", "sich", "au\u00b7ser\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist auf meine Lust bedacht.", "tokens": ["Ist", "auf", "mei\u00b7ne", "Lust", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhr Lichter an den Himmels-B\u00fchnen", "tokens": ["Ihr", "Lich\u00b7ter", "an", "den", "Him\u00b7mels\u00b7B\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Solt uns zu Hochzeits-Fackeln dienen.", "tokens": ["Solt", "uns", "zu", "Hoch\u00b7zeits\u00b7Fa\u00b7ckeln", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schw\u00e4rtzet euch ihr hellen L\u00fcffte/", "tokens": ["Schw\u00e4rt\u00b7zet", "euch", "ihr", "hel\u00b7len", "L\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht den weissen Schimmer ein/", "tokens": ["Zieht", "den", "weis\u00b7sen", "Schim\u00b7mer", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scharrt uns in die Feder Gr\u00fcffte/", "tokens": ["Scharrt", "uns", "in", "die", "Fe\u00b7der", "Gr\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und last uns verkn\u00fcpffet seyn/", "tokens": ["Und", "last", "uns", "ver\u00b7kn\u00fcpf\u00b7fet", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVPP", "VAINF", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Macht/ machet/ da\u00df mit Ku\u00df und Dr\u00fccken", "tokens": ["Macht", "/", "ma\u00b7chet", "/", "da\u00df", "mit", "Ku\u00df", "und", "Dr\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "VVFIN", "$(", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Geister sich gar bald erquicken.", "tokens": ["Die", "Geis\u00b7ter", "sich", "gar", "bald", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Schauet wie ", "tokens": ["Schau\u00b7et", "wie"], "token_info": ["word", "word"], "pos": ["VVFIN", "KOKOM"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Die ein irrdisch Eden sind/", "tokens": ["Die", "ein", "irr\u00b7disch", "E\u00b7den", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich erheben durch die L\u00fcste/", "tokens": ["Sich", "er\u00b7he\u00b7ben", "durch", "die", "L\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So uns ", "tokens": ["So", "uns"], "token_info": ["word", "word"], "pos": ["ADV", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Ach! lasset doch eur Licht verschwinden/", "tokens": ["Ach", "!", "las\u00b7set", "doch", "eur", "Licht", "ver\u00b7schwin\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kan mein Gl\u00fcck im Finstern finden.", "tokens": ["Ich", "kan", "mein", "Gl\u00fcck", "im", "Fins\u00b7tern", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6rt das Seuffzen/ so wir senden/", "tokens": ["H\u00f6rt", "das", "Seuff\u00b7zen", "/", "so", "wir", "sen\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$(", "ADV", "PPER", "VVINF", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Last uns nicht vergebens flehn/", "tokens": ["Last", "uns", "nicht", "ver\u00b7ge\u00b7bens", "flehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft den langen Tag sich enden/", "tokens": ["La\u00dft", "den", "lan\u00b7gen", "Tag", "sich", "en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Sonne untergehn/", "tokens": ["Und", "die", "Son\u00b7ne", "un\u00b7ter\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach flieh\u2019 doch Tag! eilt fort ihr Schatten/", "tokens": ["Ach", "flieh'", "doch", "Tag", "!", "eilt", "fort", "ihr", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "NN", "$.", "VVFIN", "PTKVZ", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich will mich mit ", "tokens": ["Ich", "will", "mich", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR"], "meter": "-+-+", "measure": "iambic.di"}}}}}