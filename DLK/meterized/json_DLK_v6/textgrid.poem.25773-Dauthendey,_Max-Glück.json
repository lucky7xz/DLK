{"textgrid.poem.25773": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was suchst du?", "tokens": ["Was", "suchst", "du", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Warte und wache so laut du kannst.", "tokens": ["War\u00b7te", "und", "wa\u00b7che", "so", "laut", "du", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wache und horche.", "tokens": ["Wa\u00b7che", "und", "hor\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das Gl\u00fcck, das berauschende, wonnezitternde Gl\u00fcck,", "tokens": ["Das", "Gl\u00fcck", ",", "das", "be\u00b7rau\u00b7schen\u00b7de", ",", "won\u00b7ne\u00b7zit\u00b7tern\u00b7de", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Es kommt nie. Es ist.", "tokens": ["Es", "kommt", "nie", ".", "Es", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Es umarmt dich j\u00e4h,", "tokens": ["Es", "um\u00b7armt", "dich", "j\u00e4h", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Aus der pochenden Ahnung geboren.", "tokens": ["Aus", "der", "po\u00b7chen\u00b7den", "Ah\u00b7nung", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Rosen, starke schwellende Rosen h\u00e4ufen ihren Duft.", "tokens": ["Ro\u00b7sen", ",", "star\u00b7ke", "schwel\u00b7len\u00b7de", "Ro\u00b7sen", "h\u00e4u\u00b7fen", "ih\u00b7ren", "Duft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Das ist sein Atem.", "tokens": ["Das", "ist", "sein", "A\u00b7tem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und sein Lachen?", "tokens": ["Und", "sein", "La\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Es gibt nur ", "tokens": ["Es", "gibt", "nur"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und das Lachen hei\u00dft \u00bbGl\u00fcck.\u00ab", "tokens": ["Und", "das", "La\u00b7chen", "hei\u00dft", "\u00bb", "Gl\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$(", "NN", "$.", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Und seine Augen! O diese Augen,", "tokens": ["Und", "sei\u00b7ne", "Au\u00b7gen", "!", "O", "die\u00b7se", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "NE", "PDAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Strahlenblume des Himmels,", "tokens": ["Die", "Strah\u00b7len\u00b7blu\u00b7me", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Sternentau silberner N\u00e4chte,", "tokens": ["Der", "Ster\u00b7nen\u00b7tau", "sil\u00b7ber\u00b7ner", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Schrill und melodisch.", "tokens": ["Schrill", "und", "me\u00b7lo\u00b7disch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Aber so ist es nicht immer.", "tokens": ["A\u00b7ber", "so", "ist", "es", "nicht", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Es kriegt in sich,", "tokens": ["Es", "kriegt", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Lustsaugend an der Erinnerung.", "tokens": ["Lust\u00b7sau\u00b7gend", "an", "der", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Und dann leben die blendenden Tr\u00e4ume,", "tokens": ["Und", "dann", "le\u00b7ben", "die", "blen\u00b7den\u00b7den", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Versteinert, stumpf und hart,", "tokens": ["Ver\u00b7stei\u00b7nert", ",", "stumpf", "und", "hart", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie des Mondlichts marmorne Lilien.", "tokens": ["Wie", "des", "Mond\u00b7lichts", "mar\u00b7mor\u00b7ne", "Li\u00b7li\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber nicht lange.", "tokens": ["A\u00b7ber", "nicht", "lan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "W\u00fchlende Glockenlaute,", "tokens": ["W\u00fch\u00b7len\u00b7de", "Glo\u00b7cken\u00b7lau\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Taumelnd, schwelgend,", "tokens": ["Tau\u00b7melnd", ",", "schwel\u00b7gend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Von Freude gewiegt,", "tokens": ["Von", "Freu\u00b7de", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In Freude schwingend und sch\u00e4umend", "tokens": ["In", "Freu\u00b7de", "schwin\u00b7gend", "und", "sch\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das ist seine Stimme,", "tokens": ["Das", "ist", "sei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Seine all\u00fcberflutende Stimme.", "tokens": ["Sei\u00b7ne", "al\u00b7l\u00fc\u00b7berf\u00b7lu\u00b7ten\u00b7de", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wird es nie m\u00fcde?", "tokens": ["Wird", "es", "nie", "m\u00fc\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "M\u00fcde! Todesm\u00fcde.", "tokens": ["M\u00fc\u00b7de", "!", "To\u00b7des\u00b7m\u00fc\u00b7de", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aber dann ist es nicht mehr,", "tokens": ["A\u00b7ber", "dann", "ist", "es", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Und wird nie mehr sein.", "tokens": ["Und", "wird", "nie", "mehr", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Es flackert noch rot,", "tokens": ["Es", "fla\u00b7ckert", "noch", "rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Rot, purpurrot,", "tokens": ["Rot", ",", "pur\u00b7pur\u00b7rot", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Aber ohne gl\u00fchende Kraft,", "tokens": ["A\u00b7ber", "oh\u00b7ne", "gl\u00fc\u00b7hen\u00b7de", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nur noch die Farbe von Flammen und Rosen.", "tokens": ["Nur", "noch", "die", "Far\u00b7be", "von", "Flam\u00b7men", "und", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Stockend kalt ekelgeronnenes Blut.", "tokens": ["Sto\u00b7ckend", "kalt", "e\u00b7kel\u00b7ge\u00b7ron\u00b7ne\u00b7nes", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "So ernst wird es dann,", "tokens": ["So", "ernst", "wird", "es", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und so angstfromm,", "tokens": ["Und", "so", "angst\u00b7fromm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und Weihrauch kriecht ihm zu F\u00fc\u00dfen.", "tokens": ["Und", "Weih\u00b7rauch", "kriecht", "ihm", "zu", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Tief im Dunkel,", "tokens": ["Tief", "im", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "In modernder Einsamkeit", "tokens": ["In", "mo\u00b7dern\u00b7der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tasten die blassen welken Gedanken.", "tokens": ["Tas\u00b7ten", "die", "blas\u00b7sen", "wel\u00b7ken", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Horch! Harfen, ferne, ferne Harfen ...", "tokens": ["Horch", "!", "Har\u00b7fen", ",", "fer\u00b7ne", ",", "fer\u00b7ne", "Har\u00b7fen", "..."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da breitet die Sehnsucht", "tokens": ["Da", "brei\u00b7tet", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schluchzend die Arme:", "tokens": ["Schluch\u00b7zend", "die", "Ar\u00b7me", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Gl\u00fcck! Gl\u00fcck!", "tokens": ["O", "Gl\u00fcck", "!", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "O Gl\u00fcck!", "tokens": ["O", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}