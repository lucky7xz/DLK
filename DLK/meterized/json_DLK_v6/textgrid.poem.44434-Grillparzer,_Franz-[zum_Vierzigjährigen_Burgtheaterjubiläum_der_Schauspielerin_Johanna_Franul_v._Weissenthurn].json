{"textgrid.poem.44434": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[zum Vierzigj\u00e4hrigen Burgtheaterjubil\u00e4um der Schauspielerin Johanna Franul v. Weissenthurn]", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei\u00dfig Jahr ein Menschenleben!", "tokens": ["Drei\u00b7\u00dfig", "Jahr", "ein", "Men\u00b7schen\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nun ", "tokens": ["Und", "nun"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Z\u00e4hlest du sie zu den Jahren,", "tokens": ["Z\u00e4h\u00b7lest", "du", "sie", "zu", "den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo du Schmerz und Not erfahren,", "tokens": ["Wo", "du", "Schmerz", "und", "Not", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Starrt das Auge tr\u00fcb und weit!", "tokens": ["Starrt", "das", "Au\u00b7ge", "tr\u00fcb", "und", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber vierzig Jahr dem Spiele", "tokens": ["A\u00b7ber", "vier\u00b7zig", "Jahr", "dem", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holder Musenkunst geweiht,", "tokens": ["Hol\u00b7der", "Mu\u00b7sen\u00b7kunst", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind nur Fl\u00fcgel zu dem Ziele,", "tokens": ["Sind", "nur", "Fl\u00fc\u00b7gel", "zu", "dem", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Teile einer Ewigkeit. \u2013", "tokens": ["Tei\u00b7le", "ei\u00b7ner", "E\u00b7wig\u00b7keit", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}