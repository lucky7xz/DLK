{"textgrid.poem.56551": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ik weer mal junk un sch\u00f6n,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ik weer mal junk un sch\u00f6n,", "tokens": ["Ik", "weer", "mal", "junk", "un", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dats nu ni mehr to sehn.", "tokens": ["Dats", "nu", "ni", "mehr", "to", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ik harr de Rosen op de Back,", "tokens": ["Ik", "harr", "de", "Ro\u00b7sen", "op", "de", "Back", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ik harr de Lucken um de Nack;", "tokens": ["Ik", "harr", "de", "Lu\u00b7cken", "um", "de", "Nack", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wa weer ik junk un sch\u00f6n!", "tokens": ["Wa", "weer", "ik", "junk", "un", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wa weer ik junk un sch\u00f6n!", "tokens": ["Wa", "weer", "ik", "junk", "un", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ik sung v\u0153r Lust un Moth,", "tokens": ["Ik", "sung", "v\u0153r", "Lust", "un", "Moth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Ik sung v\u0153r Kleen un Grot,", "tokens": ["Ik", "sung", "v\u0153r", "Kleen", "un", "Grot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Un Alle, de mi h\u00f6rn un sehn,", "tokens": ["Un", "Al\u00b7le", ",", "de", "mi", "h\u00f6rn", "un", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "$,", "FM", "FM", "FM", "FM", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "De s\u00e4n, ik weer so junk un sch\u00f6n.", "tokens": ["De", "s\u00e4n", ",", "ik", "weer", "so", "junk", "un", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "FM", "FM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wa harr ik Lust un Moth!", "tokens": ["Wa", "harr", "ik", "Lust", "un", "Moth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wa harr ik Lust un Moth!", "tokens": ["Wa", "harr", "ik", "Lust", "un", "Moth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ik dach ni an de Noth,", "tokens": ["Ik", "dach", "ni", "an", "de", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ik dach ni an den Dod.", "tokens": ["Ik", "dach", "ni", "an", "den", "Dod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vun Mark to Mark, vun Hus to Hus,", "tokens": ["Vun", "Mark", "to", "Mark", ",", "vun", "Hus", "to", "Hus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wo ik keem, dar weer't en Lust:", "tokens": ["Und", "wo", "ik", "keem", ",", "dar", "weer't", "en", "Lust", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer dach wul anne Noth?", "tokens": ["Wer", "dach", "wul", "an\u00b7ne", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wer dach wul an den Dod?", "tokens": ["Wer", "dach", "wul", "an", "den", "Dod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ik sing noch j\u00fcmmer fort,", "tokens": ["Ik", "sing", "noch", "j\u00fcm\u00b7mer", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Un krup vun Ort to Ort,", "tokens": ["Un", "krup", "vun", "Ort", "to", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Un wenn ik sing vun Lust un Lev,", "tokens": ["Un", "wenn", "ik", "sing", "vun", "Lust", "un", "Lev", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer fragt mi nu, warum ik b\u0119v?", "tokens": ["Wer", "fragt", "mi", "nu", ",", "wa\u00b7rum", "ik", "b\u0119v", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "ADV", "$,", "PWAV", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ik sing man j\u00fcmmer fort,", "tokens": ["Ik", "sing", "man", "j\u00fcm\u00b7mer", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ik sing man j\u00fcmmer fort.", "tokens": ["Ik", "sing", "man", "j\u00fcm\u00b7mer", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}