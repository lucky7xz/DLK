{"textgrid.poem.40185": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlings Einzug", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling stieg zur Erde nieder:", "tokens": ["Der", "Fr\u00fch\u00b7ling", "stieg", "zur", "Er\u00b7de", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "o helle Lust, o stille Scheu!", "tokens": ["o", "hel\u00b7le", "Lust", ",", "o", "stil\u00b7le", "Scheu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Herzen regt sich Alles wieder,", "tokens": ["Im", "Her\u00b7zen", "regt", "sich", "Al\u00b7les", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "so alt vertraut, so selig neu!", "tokens": ["so", "alt", "ver\u00b7traut", ",", "so", "se\u00b7lig", "neu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So schnell geschah's \u2013 kaum kann ich's fassen,", "tokens": ["So", "schnell", "ge\u00b7scha\u00b7h's", "\u2013", "kaum", "kann", "ich's", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$(", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der graue Winter sei nun tot!", "tokens": ["der", "grau\u00b7e", "Win\u00b7ter", "sei", "nun", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man f\u00fchlte sich so gl\u00fcckverlassen,", "tokens": ["Man", "f\u00fchl\u00b7te", "sich", "so", "gl\u00fcck\u00b7ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "man ", "tokens": ["man"], "token_info": ["word"], "pos": ["PIS"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Ach, Seligkeit: es scheint die Sonne!", "tokens": ["Ach", ",", "Se\u00b7lig\u00b7keit", ":", "es", "scheint", "die", "Son\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und schickt der Lenz auch ", "tokens": ["und", "schickt", "der", "Lenz", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "er weint sich aus von seiner Wonne,", "tokens": ["er", "weint", "sich", "aus", "von", "sei\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da\u00df er so klarer l\u00e4cheln kann!", "tokens": ["da\u00df", "er", "so", "kla\u00b7rer", "l\u00e4\u00b7cheln", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich m\u00f6chte alle Menschen fragen,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "al\u00b7le", "Men\u00b7schen", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ob sie nicht jauchzen woll'n mit mir!", "tokens": ["ob", "sie", "nicht", "jauch\u00b7zen", "woll'n", "mit", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nein, ganz verschwiegen will ich's tragen!", "tokens": ["Nein", ",", "ganz", "ver\u00b7schwie\u00b7gen", "will", "ich's", "tra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVINF", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, Liebchen, nein! ich sag' es Dir!", "tokens": ["Ach", ",", "Lieb\u00b7chen", ",", "nein", "!", "ich", "sag'", "es", "Dir", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}