{"dta.poem.15860": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Lieb' ohn' Eigennutz freu dich, die du gewannst,", "tokens": ["Der", "Lieb'", "ohn'", "Ei\u00b7gen\u00b7nutz", "freu", "dich", ",", "die", "du", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der freien Gab', um die du Dank nur geben kannst.", "tokens": ["Der", "frei\u00b7en", "Gab'", ",", "um", "die", "du", "Dank", "nur", "ge\u00b7ben", "kannst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was du dir sagen darfst, darf sich kein K\u00f6nig sagen:", "tokens": ["Was", "du", "dir", "sa\u00b7gen", "darfst", ",", "darf", "sich", "kein", "K\u00f6\u00b7nig", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$,", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ganz reine Neigung ists, was dir die Herzen schlagen.", "tokens": ["Ganz", "rei\u00b7ne", "Nei\u00b7gung", "ists", ",", "was", "dir", "die", "Her\u00b7zen", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$,", "PWS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Man sucht nicht deine Huld, man scheut nicht deine Macht,", "tokens": ["Man", "sucht", "nicht", "dei\u00b7ne", "Huld", ",", "man", "scheut", "nicht", "dei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "PIS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an den Menschen nur hat hier der Mensch gedacht.", "tokens": ["Und", "an", "den", "Men\u00b7schen", "nur", "hat", "hier", "der", "Mensch", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}