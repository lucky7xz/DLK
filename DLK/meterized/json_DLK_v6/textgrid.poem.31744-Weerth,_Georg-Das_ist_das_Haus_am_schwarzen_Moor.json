{"textgrid.poem.31744": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das ist das Haus am schwarzen Moor", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist das Haus am schwarzen Moor!", "tokens": ["Das", "ist", "das", "Haus", "am", "schwar\u00b7zen", "Moor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer dort im letzten Winter fror,", "tokens": ["Wer", "dort", "im", "letz\u00b7ten", "Win\u00b7ter", "fror", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der friert dort nicht in diesem Jahr \u2013", "tokens": ["Der", "friert", "dort", "nicht", "in", "die\u00b7sem", "Jahr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sank schon l\u00e4ngst auf die Totenbahr.", "tokens": ["Er", "sank", "schon", "l\u00e4ngst", "auf", "die", "To\u00b7ten\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das ist das Haus am schwarzen Moor,", "tokens": ["Das", "ist", "das", "Haus", "am", "schwar\u00b7zen", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haus, wo der alte Jan erfror.", "tokens": ["Das", "Haus", ",", "wo", "der", "al\u00b7te", "Jan", "er\u00b7fror", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zur T\u00fcr gewandt das wei\u00dfe Gesicht,", "tokens": ["Zur", "T\u00fcr", "ge\u00b7wandt", "das", "wei\u00b7\u00dfe", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Starb er und wu\u00dft es selber nicht.", "tokens": ["Starb", "er", "und", "wu\u00dft", "es", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er starb. \u2013 Da kam, wie ein scheues Reh,", "tokens": ["Er", "starb", ".", "\u2013", "Da", "kam", ",", "wie", "ein", "scheu\u00b7es", "Reh", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Tag und h\u00fcpfte \u00fcber den Schnee.", "tokens": ["Der", "Tag", "und", "h\u00fcpf\u00b7te", "\u00fc\u00b7ber", "den", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbguten Morgen, Jan! Guten Morgen, Jan!\u00ab \u2013", "tokens": ["\u00bb", "gu\u00b7ten", "Mor\u00b7gen", ",", "Jan", "!", "Gu\u00b7ten", "Mor\u00b7gen", ",", "Jan", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NE", "$.", "ADJA", "NN", "$,", "NE", "$.", "$(", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Jan keine Antwort geben kann.", "tokens": ["Der", "Jan", "kei\u00b7ne", "Ant\u00b7wort", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Da erhuben die Glocken ihr hell Gel\u00e4ut,", "tokens": ["Da", "er\u00b7hu\u00b7ben", "die", "Glo\u00b7cken", "ihr", "hell", "Ge\u00b7l\u00e4ut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sie sangen und klangen und riefen so weit:", "tokens": ["Sie", "san\u00b7gen", "und", "klan\u00b7gen", "und", "rie\u00b7fen", "so", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u00bbguten Morgen, Jan! Guten Morgen, Jan!\u00ab \u2013", "tokens": ["\u00bb", "gu\u00b7ten", "Mor\u00b7gen", ",", "Jan", "!", "Gu\u00b7ten", "Mor\u00b7gen", ",", "Jan", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NE", "$.", "ADJA", "NN", "$,", "NE", "$.", "$(", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Jan keine Antwort geben kann.", "tokens": ["Der", "Jan", "kei\u00b7ne", "Ant\u00b7wort", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da kamen die Kinder aus der Stadt:", "tokens": ["Da", "ka\u00b7men", "die", "Kin\u00b7der", "aus", "der", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbwir wissen, wie lieb er uns alle hat;", "tokens": ["\u00bb", "wir", "wis\u00b7sen", ",", "wie", "lieb", "er", "uns", "al\u00b7le", "hat", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVINF", "$,", "PWAV", "ADJD", "PPER", "PPER", "PIS", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Guten Morgen, Jan! Guten Morgen, Jan!\u00ab \u2013", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", ",", "Jan", "!", "Gu\u00b7ten", "Mor\u00b7gen", ",", "Jan", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "$.", "ADJA", "NN", "$,", "NE", "$.", "$(", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der Jan keine Antwort geben kann.", "tokens": ["Der", "Jan", "kei\u00b7ne", "Ant\u00b7wort", "ge\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Tag, Glocken und Kinder er nicht verstund.", "tokens": ["Tag", ",", "Glo\u00b7cken", "und", "Kin\u00b7der", "er", "nicht", "ver\u00b7stund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da nahte die sonnige Mittagsstund,", "tokens": ["Da", "nah\u00b7te", "die", "son\u00b7ni\u00b7ge", "Mit\u00b7tags\u00b7stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Da nahte ein armes Weib: \u00bbMein Jan,", "tokens": ["Da", "nah\u00b7te", "ein", "ar\u00b7mes", "Weib", ":", "\u00bb", "Mein", "Jan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Willst essen und trinken nicht, alter Mann?", "tokens": ["Willst", "es\u00b7sen", "und", "trin\u00b7ken", "nicht", ",", "al\u00b7ter", "Mann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "KON", "VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Sieh, was ich brachte dir aus der Stadt;", "tokens": ["Sieh", ",", "was", "ich", "brach\u00b7te", "dir", "aus", "der", "Stadt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sollst froh nun werden und warm und satt!\u00ab \u2013", "tokens": ["Sollst", "froh", "nun", "wer\u00b7den", "und", "warm", "und", "satt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VMFIN", "ADJD", "ADV", "VAFIN", "KON", "ADJD", "KON", "ADJD", "$.", "$(", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Alte sah lange auf ihren Jan,", "tokens": ["Die", "Al\u00b7te", "sah", "lan\u00b7ge", "auf", "ih\u00b7ren", "Jan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da fing sie bitter zu weinen an.", "tokens": ["Da", "fing", "sie", "bit\u00b7ter", "zu", "wei\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Da weinte sie an dem schwarzen Moor,", "tokens": ["Da", "wein\u00b7te", "sie", "an", "dem", "schwar\u00b7zen", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Moor, wo der alte Jan erfror;", "tokens": ["Am", "Moor", ",", "wo", "der", "al\u00b7te", "Jan", "er\u00b7fror", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da weinte sie ihr brennend Weh", "tokens": ["Da", "wein\u00b7te", "sie", "ihr", "bren\u00b7nend", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinunter in den kalten Schnee.", "tokens": ["Hin\u00b7un\u00b7ter", "in", "den", "kal\u00b7ten", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}