{"textgrid.poem.34009": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die beiden Zecher", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei'm Weine sah ich einst zwei Zecher sitzen;", "tokens": ["Bei'm", "Wei\u00b7ne", "sah", "ich", "einst", "zwei", "Ze\u00b7cher", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Eine rief: kein Tropfen wird vergossen,", "tokens": ["Der", "Ei\u00b7ne", "rief", ":", "kein", "Trop\u00b7fen", "wird", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bevor sich das Geheimni\u00df mir erschlossen,", "tokens": ["Be\u00b7vor", "sich", "das", "Ge\u00b7heim\u00b7ni\u00df", "mir", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Woher es kommt, dies Perlen und dies Blitzen!", "tokens": ["Wo\u00b7her", "es", "kommt", ",", "dies", "Per\u00b7len", "und", "dies", "Blit\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PDS", "NN", "KON", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der And're sprach: er wird mein Blut erhitzen,", "tokens": ["Der", "An\u00b7d'\u00b7re", "sprach", ":", "er", "wird", "mein", "Blut", "er\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und daraus ist mir nie noch Heil entsprossen,", "tokens": ["Und", "da\u00b7raus", "ist", "mir", "nie", "noch", "Heil", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie w\u00e4r' mir's, wenn ich nach dem Rausch verdrossen", "tokens": ["Wie", "w\u00e4r'", "mir's", ",", "wenn", "ich", "nach", "dem", "Rausch", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "NE", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich f\u00e4nde auf den schroffsten Felsenspitzen!", "tokens": ["Mich", "f\u00e4n\u00b7de", "auf", "den", "schroffs\u00b7ten", "Fel\u00b7sen\u00b7spit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sa\u00dfen sie und gr\u00fcbelten auf's Beste,", "tokens": ["So", "sa\u00b7\u00dfen", "sie", "und", "gr\u00fc\u00b7bel\u00b7ten", "auf's", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Inde\u00df umsonst die Goldpocale lachten,", "tokens": ["In\u00b7de\u00df", "um\u00b7sonst", "die", "Gold\u00b7po\u00b7ca\u00b7le", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu ihres g\u00fctigen Bewirthers Qualen;", "tokens": ["Zu", "ih\u00b7res", "g\u00fc\u00b7ti\u00b7gen", "Be\u00b7wirt\u00b7hers", "Qua\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Inzwischen kam ein Haufen frischer G\u00e4ste,", "tokens": ["I\u00b7nzwi\u00b7schen", "kam", "ein", "Hau\u00b7fen", "fri\u00b7scher", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da sah'n sie sich vertrieben, eh' sie's dachten,", "tokens": ["Da", "sah'n", "sie", "sich", "ver\u00b7trie\u00b7ben", ",", "eh'", "sie's", "dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und m\u00fcssen nun mit ew'gem Durst bezahlen!", "tokens": ["Und", "m\u00fcs\u00b7sen", "nun", "mit", "ew'\u00b7gem", "Durst", "be\u00b7zah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}