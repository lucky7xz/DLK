{"textgrid.poem.59252": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Meisten", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist denn alles ganz vergebens?", "tokens": ["Ist", "denn", "al\u00b7les", "ganz", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freiheit, Ruhm und treue Sitte,", "tokens": ["Frei\u00b7heit", ",", "Ruhm", "und", "treu\u00b7e", "Sit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ritterbild des alten Lebens,", "tokens": ["Rit\u00b7ter\u00b7bild", "des", "al\u00b7ten", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog im Lied durch eure Mitte", "tokens": ["Zog", "im", "Lied", "durch", "eu\u00b7re", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hohnverlacht als Don Quijote;", "tokens": ["Hohn\u00b7ver\u00b7lacht", "als", "Don", "Qui\u00b7jo\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch deckt Schlaf mit plumper Pfote,", "tokens": ["Euch", "deckt", "Schlaf", "mit", "plum\u00b7per", "Pfo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Ehre ist euch Zote.", "tokens": ["Und", "die", "Eh\u00b7re", "ist", "euch", "Zo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob sich Kampf erneut', vergliche,", "tokens": ["Ob", "sich", "Kampf", "er\u00b7neut'", ",", "ver\u00b7gli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VVFIN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sich roh Gebirgsvolk raufe,", "tokens": ["Ob", "sich", "roh", "Ge\u00b7birgs\u00b7volk", "rau\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht der Kl\u00fcgre Weg' und Schliche,", "tokens": ["Sucht", "der", "Kl\u00fcg\u00b7re", "Weg'", "und", "Schli\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie er nur sein Haus erlaufe.", "tokens": ["Wie", "er", "nur", "sein", "Haus", "er\u00b7lau\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhet, st\u00fctzet nur und haltet!", "tokens": ["Ru\u00b7het", ",", "st\u00fct\u00b7zet", "nur", "und", "hal\u00b7tet", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Untersinkt, was ihr gestaltet,", "tokens": ["Un\u00b7ter\u00b7sinkt", ",", "was", "ihr", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der Mutterboden spaltet.", "tokens": ["Wenn", "der", "Mut\u00b7ter\u00b7bo\u00b7den", "spal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie so lustig, ihr Poeten,", "tokens": ["Wie", "so", "lus\u00b7tig", ",", "ihr", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den blumenreichen Hagen", "tokens": ["An", "den", "blu\u00b7men\u00b7rei\u00b7chen", "Ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Abendgold zu fl\u00f6ten,", "tokens": ["In", "dem", "A\u00b7bend\u00b7gold", "zu", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Quellen, Nymphen nachzujagen!", "tokens": ["Quel\u00b7len", ",", "Nym\u00b7phen", "nach\u00b7zu\u00b7ja\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn erst mut'ge Sch\u00fcsse fallen,", "tokens": ["Wenn", "erst", "mut'\u00b7ge", "Sch\u00fcs\u00b7se", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den sch\u00f6nen Widerhallen", "tokens": ["Von", "den", "sch\u00f6\u00b7nen", "Wi\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00dft ihr zart Sonette schallen.", "tokens": ["La\u00dft", "ihr", "zart", "So\u00b7net\u00b7te", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohlfeil Ruhm sich zu erringen,", "tokens": ["Wohl\u00b7feil", "Ruhm", "sich", "zu", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder \u00e4ngstlich schreibt und treibet;", "tokens": ["Je\u00b7der", "\u00e4ngst\u00b7lich", "schreibt", "und", "trei\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinem m\u00f6cht das Herz zerspringen,", "tokens": ["Kei\u00b7nem", "m\u00f6cht", "das", "Herz", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glaubt sich selbst nicht, was er schreibet.", "tokens": ["Glaubt", "sich", "selbst", "nicht", ",", "was", "er", "schrei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seid ihr M\u00e4nner, seid ihr Christen?", "tokens": ["Seid", "ihr", "M\u00e4n\u00b7ner", ",", "seid", "ihr", "Chris\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glaubt ihr, Gott zu \u00fcberlisten,", "tokens": ["Glaubt", "ihr", ",", "Gott", "zu", "\u00fc\u00b7berl\u00b7is\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So in Selbstsucht feig zu nisten?", "tokens": ["So", "in", "Selbst\u00b7sucht", "feig", "zu", "nis\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Einen Wald doch kenn ich droben,", "tokens": ["Ei\u00b7nen", "Wald", "doch", "kenn", "ich", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauschend mit den gr\u00fcnen Kronen,", "tokens": ["Rau\u00b7schend", "mit", "den", "gr\u00fc\u00b7nen", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4mme br\u00fcderlich verwoben,", "tokens": ["St\u00e4m\u00b7me", "br\u00fc\u00b7der\u00b7lich", "ver\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo das alte Recht mag wohnen.", "tokens": ["Wo", "das", "al\u00b7te", "Recht", "mag", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche auf sein Rauschen merken,", "tokens": ["Man\u00b7che", "auf", "sein", "Rau\u00b7schen", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein neu Geschlecht wird st\u00e4rken", "tokens": ["Und", "ein", "neu", "Ge\u00b7schlecht", "wird", "st\u00e4r\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "VAFIN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Dieser Wald zu deutschen Werken.", "tokens": ["Die\u00b7ser", "Wald", "zu", "deut\u00b7schen", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}