{"textgrid.poem.37931": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Werd ein Kind", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klein und arm an Herz und Munde", "tokens": ["Klein", "und", "arm", "an", "Herz", "und", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00dft du seyn, wenn Christus soll", "tokens": ["Mu\u00dft", "du", "seyn", ",", "wenn", "Chris\u00b7tus", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VAINF", "$,", "KOUS", "NE", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gehen auf in deinem Grunde:", "tokens": ["Ge\u00b7hen", "auf", "in", "dei\u00b7nem", "Grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die Rose und Viol", "tokens": ["Denn", "die", "Ro\u00b7se", "und", "Vi\u00b7ol"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4chst im Thal der niedern Seelen,", "tokens": ["W\u00e4chst", "im", "Thal", "der", "nie\u00b7dern", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die nichts hohes hier erw\u00e4hlen!", "tokens": ["Die", "nichts", "ho\u00b7hes", "hier", "er\u00b7w\u00e4h\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJA", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00f6gst du nur so seyn dem\u00fcthig,", "tokens": ["M\u00f6gst", "du", "nur", "so", "seyn", "de\u00b7m\u00fct\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die niedre Sarons Blum,", "tokens": ["Wie", "die", "nied\u00b7re", "Sa\u00b7rons", "Blum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dennoch stehen ehrerbietig", "tokens": ["Den\u00b7noch", "ste\u00b7hen", "ehr\u00b7er\u00b7bie\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vor Gott geb\u00fccket krumm:", "tokens": ["Und", "vor", "Gott", "ge\u00b7b\u00fc\u00b7cket", "krumm", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also m\u00f6gst du bald die Gaben", "tokens": ["Al\u00b7so", "m\u00f6gst", "du", "bald", "die", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seines Geistes in dir haben.", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "in", "dir", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn dich aber hoch beflecket", "tokens": ["Wenn", "dich", "a\u00b7ber", "hoch", "be\u00b7fle\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Weisheit stolzer Witz,", "tokens": ["Dei\u00b7ner", "Weis\u00b7heit", "stol\u00b7zer", "Witz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich alsdann vor dir verstecket", "tokens": ["Sich", "als\u00b7dann", "vor", "dir", "ver\u00b7ste\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wahrer Wahrheit klarer Blitz:", "tokens": ["Wah\u00b7rer", "Wahr\u00b7heit", "kla\u00b7rer", "Blitz", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Buchstab dich gefangen,", "tokens": ["Wenn", "der", "Buch\u00b7stab", "dich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kannst du nicht zum Geist gelangen.", "tokens": ["Kannst", "du", "nicht", "zum", "Geist", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Werd ein Kind, werd arm und kleine,", "tokens": ["Werd", "ein", "Kind", ",", "werd", "arm", "und", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey nicht hoch noch weis' bei dir,", "tokens": ["Sey", "nicht", "hoch", "noch", "weis'", "bei", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ADV", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Setze dich in Staub und weine,", "tokens": ["Set\u00b7ze", "dich", "in", "Staub", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis dich Gott zur Schule f\u00fchrt,", "tokens": ["Bis", "dich", "Gott", "zur", "Schu\u00b7le", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da sein Geist die Arm' und Bl\u00f6den", "tokens": ["Da", "sein", "Geist", "die", "Arm'", "und", "Bl\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weislich lehret von ihm reden.", "tokens": ["Weis\u00b7lich", "leh\u00b7ret", "von", "ihm", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}