{"dta.poem.8580": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hinter hohen Mauern", "tokens": ["Hin\u00b7ter", "ho\u00b7hen", "Mau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "hinter mir", "tokens": ["hin\u00b7ter", "mir"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "liegt ein Paradies.", "tokens": ["liegt", "ein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcne, glitzernde Stachelbeerstr\u00e4ucher,", "tokens": ["Gr\u00fc\u00b7ne", ",", "glit\u00b7zern\u00b7de", "Sta\u00b7chel\u00b7beer\u00b7str\u00e4u\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "eine Strohbude", "tokens": ["ei\u00b7ne", "Stroh\u00b7bu\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "und B\u00e4ume mit Glaskirschen.", "tokens": ["und", "B\u00e4u\u00b7me", "mit", "Glas\u00b7kir\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Niemand weiss von ihm.", "tokens": ["Nie\u00b7mand", "weiss", "von", "ihm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "An einem Halm", "tokens": ["An", "ei\u00b7nem", "Halm"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "klettert ein Marienk\u00e4ferchen,", "tokens": ["klet\u00b7tert", "ein", "Ma\u00b7ri\u00b7en\u00b7k\u00e4\u00b7fer\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "plumps, und f\u00e4llt in goldgelbe Butterblumen.", "tokens": ["plumps", ",", "und", "f\u00e4llt", "in", "gold\u00b7gel\u00b7be", "But\u00b7ter\u00b7blu\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.5": {"line.1": {"text": "Hilfreich neigen sich Tausendsch\u00f6nchen,", "tokens": ["Hilf\u00b7reich", "nei\u00b7gen", "sich", "Tau\u00b7send\u00b7sch\u00f6n\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Stiefm\u00fctterchen machen ein b\u00f6ses Gesicht.", "tokens": ["Stief\u00b7m\u00fct\u00b7ter\u00b7chen", "ma\u00b7chen", "ein", "b\u00f6\u00b7ses", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Verschollen", "tokens": ["Ver\u00b7schol\u00b7len"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "gl\u00e4nzen die Beete!", "tokens": ["gl\u00e4n\u00b7zen", "die", "Bee\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}