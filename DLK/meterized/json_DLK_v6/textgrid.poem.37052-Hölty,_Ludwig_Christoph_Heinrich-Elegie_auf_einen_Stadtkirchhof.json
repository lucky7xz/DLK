{"textgrid.poem.37052": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Elegie auf einen Stadtkirchhof", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hespers Lampe funkelt schon im Westen,", "tokens": ["Hes\u00b7pers", "Lam\u00b7pe", "fun\u00b7kelt", "schon", "im", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Abendl\u00fcftchen buhlen", "tokens": ["A\u00b7ben\u00b7dl\u00fcft\u00b7chen", "buh\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit den Wiesenblumen, mit den B\u00fcschen,", "tokens": ["Mit", "den", "Wie\u00b7sen\u00b7blu\u00b7men", ",", "mit", "den", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die der Fr\u00fchling r\u00f6thet.", "tokens": ["Die", "der", "Fr\u00fch\u00b7ling", "r\u00f6\u00b7thet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Zephyr scherzet mit des M\u00e4dchens Busen,", "tokens": ["Ze\u00b7phyr", "scher\u00b7zet", "mit", "des", "M\u00e4d\u00b7chens", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das der Stadt entfliehet,", "tokens": ["Das", "der", "Stadt", "ent\u00b7flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und am Arme eines bunten Stutzers", "tokens": ["Und", "am", "Ar\u00b7me", "ei\u00b7nes", "bun\u00b7ten", "Stut\u00b7zers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch die Wiese gaukelt.", "tokens": ["Durch", "die", "Wie\u00b7se", "gau\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gaukelt nur, ihr bunten Schmetterlinge!", "tokens": ["Gau\u00b7kelt", "nur", ",", "ihr", "bun\u00b7ten", "Schmet\u00b7ter\u00b7lin\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Andre Scenen laden", "tokens": ["And\u00b7re", "Sce\u00b7nen", "la\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mich zur Grotte, wo die Schwermuth lauschet,", "tokens": ["Mich", "zur", "Grot\u00b7te", ",", "wo", "die", "Schwer\u00b7muth", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Der Betrachtung Mutter.", "tokens": ["Der", "Be\u00b7trach\u00b7tung", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wie der Fackeln goldne Locken wallen!", "tokens": ["Wie", "der", "Fa\u00b7ckeln", "gold\u00b7ne", "Lo\u00b7cken", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie der Leichenwagen", "tokens": ["Wie", "der", "Lei\u00b7chen\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch das Stadtthor rollet! Wie die Wehmuth", "tokens": ["Durch", "das", "Stadt\u00b7thor", "rol\u00b7let", "!", "Wie", "die", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus den Augen tr\u00e4ufelt!", "tokens": ["Aus", "den", "Au\u00b7gen", "tr\u00e4u\u00b7felt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Mische dich, o Muse, in die Reihen,", "tokens": ["Mi\u00b7sche", "dich", ",", "o", "Mu\u00b7se", ",", "in", "die", "Rei\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die der Bahre folgen,", "tokens": ["Die", "der", "Bah\u00b7re", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wandle auf den Kirchhof, wo der St\u00e4dter", "tokens": ["Wand\u00b7le", "auf", "den", "Kirch\u00b7hof", ",", "wo", "der", "St\u00e4d\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unter Marmor schlummert.", "tokens": ["Un\u00b7ter", "Mar\u00b7mor", "schlum\u00b7mert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Weile bey den goldnen Mausol\u00e4en,", "tokens": ["Wei\u00b7le", "bey", "den", "gold\u00b7nen", "Mau\u00b7so\u00b7l\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bey den Aschenkr\u00fcgen,", "tokens": ["Bey", "den", "A\u00b7schen\u00b7kr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die den Vorhang vor die bunte Scene", "tokens": ["Die", "den", "Vor\u00b7hang", "vor", "die", "bun\u00b7te", "Sce\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dieses Lebens ziehen.", "tokens": ["Die\u00b7ses", "Le\u00b7bens", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Rosenwangen liegen hier im Arme", "tokens": ["Ro\u00b7sen\u00b7wan\u00b7gen", "lie\u00b7gen", "hier", "im", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Verwesung, H\u00e4nde,", "tokens": ["Der", "Ver\u00b7we\u00b7sung", ",", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die so sch\u00f6n den F\u00e4cher schwangen, F\u00fc\u00dfe,", "tokens": ["Die", "so", "sch\u00f6n", "den", "F\u00e4\u00b7cher", "schwan\u00b7gen", ",", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie des Windes Fl\u00fcgel.", "tokens": ["Wie", "des", "Win\u00b7des", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Arme Sch\u00f6nen! wo sind nun die Gr\u00fcbchen,", "tokens": ["Ar\u00b7me", "Sch\u00f6\u00b7nen", "!", "wo", "sind", "nun", "die", "Gr\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWAV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Scherze sa\u00dfen", "tokens": ["Wo", "die", "Scher\u00b7ze", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Herzen raubten, wo Gott Amor", "tokens": ["Und", "die", "Her\u00b7zen", "raub\u00b7ten", ",", "wo", "Gott", "A\u00b7mor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "PWAV", "NN", "NE"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seine Pfeile spitzte?", "tokens": ["Sei\u00b7ne", "Pfei\u00b7le", "spitz\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Moder naget an dem sch\u00f6nen Schleyer,", "tokens": ["Mo\u00b7der", "na\u00b7get", "an", "dem", "sch\u00f6\u00b7nen", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Den ein Geist bewohnte,", "tokens": ["Den", "ein", "Geist", "be\u00b7wohn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der nur Moden, Opern, A\u00dfambleen", "tokens": ["Der", "nur", "Mo\u00b7den", ",", "O\u00b7pern", ",", "A\u00df\u00b7am\u00b7bleen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADV", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und Redouten dachte.", "tokens": ["Und", "Re\u00b7dou\u00b7ten", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Nacht verschlie\u00dft die gro\u00dfen blauen Augen,", "tokens": ["Nacht", "ver\u00b7schlie\u00dft", "die", "gro\u00b7\u00dfen", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deren Blick den Himmel", "tokens": ["De\u00b7ren", "Blick", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oefnete, die schmelzende Entz\u00fcckung", "tokens": ["Oef\u00b7ne\u00b7te", ",", "die", "schmel\u00b7zen\u00b7de", "Ent\u00b7z\u00fc\u00b7ckung"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "In die Seelen blitzten.", "tokens": ["In", "die", "See\u00b7len", "blitz\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Zauberische Augen! Sie erblickten", "tokens": ["Zau\u00b7be\u00b7ri\u00b7sche", "Au\u00b7gen", "!", "Sie", "er\u00b7blick\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nie die Morgenr\u00f6the,", "tokens": ["Nie", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hiengen lieber an der goldnen Weste,", "tokens": ["Hien\u00b7gen", "lie\u00b7ber", "an", "der", "gold\u00b7nen", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als an Fr\u00fchlingsblumen.", "tokens": ["Als", "an", "Fr\u00fch\u00b7lings\u00b7blu\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Auch der bunte Schmetterling, der Stutzer,", "tokens": ["Auch", "der", "bun\u00b7te", "Schmet\u00b7ter\u00b7ling", ",", "der", "Stut\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der die kleine Spanne", "tokens": ["Der", "die", "klei\u00b7ne", "Span\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Seines Lebens durchgeflattert hatte,", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "durch\u00b7ge\u00b7flat\u00b7tert", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "H\u00f6rt hier auf zu flattern.", "tokens": ["H\u00f6rt", "hier", "auf", "zu", "flat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Todesschlummer sitzt auf seiner Scheitel,", "tokens": ["To\u00b7des\u00b7schlum\u00b7mer", "sitzt", "auf", "sei\u00b7ner", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die er jeden Morgen", "tokens": ["Die", "er", "je\u00b7den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit Romanenscherzen, seine Sch\u00f6nen", "tokens": ["Mit", "Ro\u00b7ma\u00b7nen\u00b7scher\u00b7zen", ",", "sei\u00b7ne", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Zu bezaubern, f\u00fcllte.", "tokens": ["Zu", "be\u00b7zau\u00b7bern", ",", "f\u00fcll\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Welch Gew\u00fchl von H\u00fcgeln! Woll\u00fcstlinge,", "tokens": ["Welch", "Ge\u00b7w\u00fchl", "von", "H\u00fc\u00b7geln", "!", "Wol\u00b7l\u00fcst\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$.", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deren Blumentage", "tokens": ["De\u00b7ren", "Blu\u00b7men\u00b7ta\u00b7ge"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Lauter Spiele, lauter Scherze waren,", "tokens": ["Lau\u00b7ter", "Spie\u00b7le", ",", "lau\u00b7ter", "Scher\u00b7ze", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruhen unter Wuchrern.", "tokens": ["Ru\u00b7hen", "un\u00b7ter", "Wuc\u00b7hrern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Lieblinge Ly\u00e4ens, welche Rosen", "tokens": ["Lieb\u00b7lin\u00b7ge", "Ly\u00b7\u00e4\u00b7ens", ",", "wel\u00b7che", "Ro\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "PWAT", "NN"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Um die Becher wanden,", "tokens": ["Um", "die", "Be\u00b7cher", "wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ruhen unter Kr\u00e4mern und Pedanten,", "tokens": ["Ru\u00b7hen", "un\u00b7ter", "Kr\u00e4\u00b7mer