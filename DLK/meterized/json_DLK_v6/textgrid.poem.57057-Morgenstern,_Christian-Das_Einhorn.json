{"textgrid.poem.57057": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Einhorn", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Einhorn lebt von Ort zu Ort", "tokens": ["Das", "Ein\u00b7horn", "lebt", "von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "nur noch als Wirtshaus fort.", "tokens": ["nur", "noch", "als", "Wirts\u00b7haus", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Man geht hinein zur Abendstund", "tokens": ["Man", "geht", "hin\u00b7ein", "zur", "A\u00b7bends\u00b7tund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sitzt den Stammtisch rund.", "tokens": ["und", "sitzt", "den", "Stamm\u00b7tisch", "rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer wei\u00df! Nach Jahr und Tag sind wir", "tokens": ["Wer", "wei\u00df", "!", "Nach", "Jahr", "und", "Tag", "sind", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "APPR", "NN", "KON", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "auch ganz wie jenes Tier", "tokens": ["auch", "ganz", "wie", "je\u00b7nes", "Tier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hotels nur noch, darin man speist \u2013", "tokens": ["Ho\u00b7tels", "nur", "noch", ",", "da\u00b7rin", "man", "speist", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PAV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(so v\u00f6llig wurden wir zu Geist).", "tokens": ["(", "so", "v\u00f6l\u00b7lig", "wur\u00b7den", "wir", "zu", "Geist", ")", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "PPER", "APPR", "NN", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Im \u00bbGoldnen Menschen\u00ab sitzt man dann", "tokens": ["Im", "\u00bb", "Gold\u00b7nen", "Men\u00b7schen", "\u00ab", "sitzt", "man", "dann"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "$(", "ADJA", "NN", "$(", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sagt sein Solo an ...", "tokens": ["und", "sagt", "sein", "So\u00b7lo", "an", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}