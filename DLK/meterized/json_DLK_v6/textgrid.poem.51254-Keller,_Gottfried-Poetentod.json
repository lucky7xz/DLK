{"textgrid.poem.51254": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Poetentod", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herbstwind rauscht; der Dichter liegt im Sterben,", "tokens": ["Der", "Herbst\u00b7wind", "rauscht", ";", "der", "Dich\u00b7ter", "liegt", "im", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bl\u00e4tterschatten fallen an der Wand;", "tokens": ["Die", "Bl\u00e4t\u00b7ter\u00b7schat\u00b7ten", "fal\u00b7len", "an", "der", "Wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An seinem Lager knien die zarten Erben,", "tokens": ["An", "sei\u00b7nem", "La\u00b7ger", "kni\u00b7en", "die", "zar\u00b7ten", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des Weibes Stirn ruht hei\u00df auf seiner Hand.", "tokens": ["Des", "Wei\u00b7bes", "Stirn", "ruht", "hei\u00df", "auf", "sei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit dunklem Purpurwein, darin ertrunken", "tokens": ["Mit", "dunk\u00b7lem", "Pur\u00b7pur\u00b7wein", ",", "da\u00b7rin", "er\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der letzten Sonne Strahl, netzt er den Mund;", "tokens": ["Der", "letz\u00b7ten", "Son\u00b7ne", "Strahl", ",", "netzt", "er", "den", "Mund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann wieder r\u00fcckw\u00e4rts auf den Pf\u00fchl gesunken,", "tokens": ["Dann", "wie\u00b7der", "r\u00fcck\u00b7w\u00e4rts", "auf", "den", "Pf\u00fchl", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tut er den letzten Willen also kund:", "tokens": ["Tut", "er", "den", "letz\u00b7ten", "Wil\u00b7len", "al\u00b7so", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbdie ich aus luft'gen Kl\u00e4ngen aufgerichtet,", "tokens": ["\u00bb", "die", "ich", "aus", "luft'\u00b7gen", "Kl\u00e4n\u00b7gen", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorbei ist dieses Hauses Herrlichkeit;", "tokens": ["Vor\u00b7bei", "ist", "die\u00b7ses", "Hau\u00b7ses", "Herr\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe ausgelebt und ausgedichtet", "tokens": ["Ich", "ha\u00b7be", "aus\u00b7ge\u00b7lebt", "und", "aus\u00b7ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Tagewerk und meine Erdenzeit.", "tokens": ["Mein", "Ta\u00b7ge\u00b7werk", "und", "mei\u00b7ne", "Er\u00b7den\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das keck und sicher seine Welt regierte,", "tokens": ["Das", "keck", "und", "si\u00b7cher", "sei\u00b7ne", "Welt", "re\u00b7gier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es bricht mein Herz, mit ihm das K\u00f6nigshaus;", "tokens": ["Es", "bricht", "mein", "Herz", ",", "mit", "ihm", "das", "K\u00f6\u00b7nigs\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Hungerschlucker, der die Tafel zierte:", "tokens": ["Der", "Hun\u00b7ger\u00b7schlu\u00b7cker", ",", "der", "die", "Ta\u00b7fel", "zier\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ruhm, er flattert mit den Schwalben aus.", "tokens": ["Der", "Ruhm", ",", "er", "flat\u00b7tert", "mit", "den", "Schwal\u00b7ben", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So l\u00f6schet meines Herdes Weihrauchflamme", "tokens": ["So", "l\u00f6\u00b7schet", "mei\u00b7nes", "Her\u00b7des", "Weih\u00b7rauch\u00b7flam\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und z\u00fcndet wieder schlechte Kohlen an,", "tokens": ["Und", "z\u00fcn\u00b7det", "wie\u00b7der", "schlech\u00b7te", "Koh\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie's Sitte war bei meiner V\u00e4ter Stamme,", "tokens": ["Wie's", "Sit\u00b7te", "war", "bei", "mei\u00b7ner", "V\u00e4\u00b7ter", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor ich den Schritt auf dieses Rund getan!", "tokens": ["Vor", "ich", "den", "Schritt", "auf", "die\u00b7ses", "Rund", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und was den Herd bescheidnen Schmuckes kr\u00e4nzte,", "tokens": ["Und", "was", "den", "Herd", "be\u00b7scheid\u00b7nen", "Schmu\u00b7ckes", "kr\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was sich an alter Weisheit um ihn fand,", "tokens": ["Was", "sich", "an", "al\u00b7ter", "Weis\u00b7heit", "um", "ihn", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Weihgef\u00e4\u00dfen auf Gesimsen gl\u00e4nzte,", "tokens": ["In", "Weih\u00b7ge\u00b7f\u00e4\u00b7\u00dfen", "auf", "Ge\u00b7sim\u00b7sen", "gl\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Streut in den Wind, gebt in der Juden Hand:", "tokens": ["Streut", "in", "den", "Wind", ",", "gebt", "in", "der", "Ju\u00b7den", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.7": {"line.1": {"text": "Da\u00df meines Sinnes unbekannter Erbe", "tokens": ["Da\u00df", "mei\u00b7nes", "Sin\u00b7nes", "un\u00b7be\u00b7kann\u00b7ter", "Er\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit find'ger Hand, vielleicht im Sch\u00fclerkleid,", "tokens": ["Mit", "fin\u00b7d'\u00b7ger", "Hand", ",", "viel\u00b7leicht", "im", "Sch\u00fc\u00b7ler\u00b7kleid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf offnem Markte ahnungsvoll erwerbe", "tokens": ["Auf", "off\u00b7nem", "Mark\u00b7te", "ah\u00b7nungs\u00b7voll", "er\u00b7wer\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Heilkraft wider der Vernachtung Leid.", "tokens": ["Die", "Heil\u00b7kraft", "wi\u00b7der", "der", "Ver\u00b7nach\u00b7tung", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Werft jenen Wust verblichner Schrift ins Feuer,", "tokens": ["Werft", "je\u00b7nen", "Wust", "ver\u00b7blich\u00b7ner", "Schrift", "ins", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Staub der Werkstatt mag zugrunde gehn!", "tokens": ["Der", "Staub", "der", "Werk\u00b7statt", "mag", "zu\u00b7grun\u00b7de", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Reich der Kunst, wo Raum und Licht so teuer,", "tokens": ["Im", "Reich", "der", "Kunst", ",", "wo", "Raum", "und", "Licht", "so", "teu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PWAV", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll nicht der Schutt dem Werk im Wege stehn!", "tokens": ["Soll", "nicht", "der", "Schutt", "dem", "Werk", "im", "We\u00b7ge", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Dann la\u00dft des Gartens Zierde niederm\u00e4hen,", "tokens": ["Dann", "la\u00dft", "des", "Gar\u00b7tens", "Zier\u00b7de", "nie\u00b7der\u00b7m\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil unfruchtbar; die Lauben brechet ab!", "tokens": ["Weil", "un\u00b7frucht\u00b7bar", ";", "die", "Lau\u00b7ben", "bre\u00b7chet", "ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwei junge Rosenb\u00e4umchen lasset stehen", "tokens": ["Zwei", "jun\u00b7ge", "Ro\u00b7sen\u00b7b\u00e4um\u00b7chen", "las\u00b7set", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr mein und meiner lieben Frauen Grab!", "tokens": ["F\u00fcr", "mein", "und", "mei\u00b7ner", "lie\u00b7ben", "Frau\u00b7en", "Grab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "KON", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Mein Lied mag auf des Volkes Wegen klingen,", "tokens": ["Mein", "Lied", "mag", "auf", "des", "Vol\u00b7kes", "We\u00b7gen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo seine Banner von den T\u00fcrmen wehn;", "tokens": ["Wo", "sei\u00b7ne", "Ban\u00b7ner", "von", "den", "T\u00fcr\u00b7men", "wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch ungekannt, mit m\u00fchsalschwerem Ringen", "tokens": ["Doch", "un\u00b7ge\u00b7kannt", ",", "mit", "m\u00fch\u00b7sal\u00b7schwe\u00b7rem", "Rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird meine Sippschaft dran vor\u00fcbergehn!\u00ab", "tokens": ["Wird", "mei\u00b7ne", "Sipp\u00b7schaft", "dran", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PAV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Noch \u00fcberl\u00e4uft sein Angesicht, das reine,", "tokens": ["Noch", "\u00fc\u00b7berl\u00b7\u00e4uft", "sein", "An\u00b7ge\u00b7sicht", ",", "das", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit einem Strahl das sinkende Gestirn;", "tokens": ["Mit", "ei\u00b7nem", "Strahl", "das", "sin\u00b7ken\u00b7de", "Ge\u00b7stirn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gl\u00fcht' noch eben in dem Purpurscheine,", "tokens": ["So", "gl\u00fcht'", "noch", "e\u00b7ben", "in", "dem", "Pur\u00b7pur\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun starret kalt und wei\u00df des Berges Firn.", "tokens": ["Nun", "star\u00b7ret", "kalt", "und", "wei\u00df", "des", "Ber\u00b7ges", "Firn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und wie durch Alpend\u00e4mmerung das Rauschen", "tokens": ["Und", "wie", "durch", "Al\u00b7pen\u00b7d\u00e4m\u00b7me\u00b7rung", "das", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von eines sp\u00e4ten Adlers Schwingen webt,", "tokens": ["Von", "ei\u00b7nes", "sp\u00e4\u00b7ten", "Ad\u00b7lers", "Schwin\u00b7gen", "webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist in der Todesstille zu erlauschen,", "tokens": ["Ist", "in", "der", "To\u00b7des\u00b7stil\u00b7le", "zu", "er\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie eine Geisterschar von hinnen schwebt.", "tokens": ["Wie", "ei\u00b7ne", "Geis\u00b7ter\u00b7schar", "von", "hin\u00b7nen", "schwebt", "."], "token_info": ["word", "word", "word", "word", "word", "word",