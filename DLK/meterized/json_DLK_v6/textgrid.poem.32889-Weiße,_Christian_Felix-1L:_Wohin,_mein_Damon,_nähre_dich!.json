{"textgrid.poem.32889": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin, mein Damon, n\u00e4hre dich!", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, mein Damon, n\u00e4hre dich!", "tokens": ["Wo\u00b7hin", ",", "mein", "Da\u00b7mon", ",", "n\u00e4h\u00b7re", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du w\u00fcnschst mir einen Ku\u00df zu rauben?", "tokens": ["Du", "w\u00fcnschst", "mir", "ei\u00b7nen", "Ku\u00df", "zu", "rau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gut! einen will ich dir erlauben:", "tokens": ["Gut", "!", "ei\u00b7nen", "will", "ich", "dir", "er\u00b7lau\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PIS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sey auch klug! verstehst du mich?", "tokens": ["Doch", "sey", "auch", "klug", "!", "ver\u00b7stehst", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$.", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Strau\u00df am Busen reizet dich?", "tokens": ["Der", "Strau\u00df", "am", "Bu\u00b7sen", "rei\u00b7zet", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh, du w\u00fcnschst mir ihn zu rauben?", "tokens": ["Ich", "seh", ",", "du", "w\u00fcnschst", "mir", "ihn", "zu", "rau\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Komm nur, ich will es dir erlauben:", "tokens": ["Komm", "nur", ",", "ich", "will", "es", "dir", "er\u00b7lau\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sey auch klug! verstehst du mich?", "tokens": ["Doch", "sey", "auch", "klug", "!", "ver\u00b7stehst", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$.", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du sehnest nach den Schatten dich,", "tokens": ["Du", "seh\u00b7nest", "nach", "den", "Schat\u00b7ten", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sagst, nichts gliche jenen Lauben:", "tokens": ["Und", "sagst", ",", "nichts", "gli\u00b7che", "je\u00b7nen", "Lau\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PIS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Komm, f\u00fchr mich hin, ich will dirs glauben:", "tokens": ["Komm", ",", "f\u00fchr", "mich", "hin", ",", "ich", "will", "dirs", "glau\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sey auch klug! verstehst du mich?", "tokens": ["Doch", "sey", "auch", "klug", "!", "ver\u00b7stehst", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$.", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}