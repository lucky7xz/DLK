{"dta.poem.6815": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die C\u00f6rper sind gedehnt, sie haben ein Bewegen,", "tokens": ["Die", "C\u00f6r\u00b7per", "sind", "ge\u00b7dehnt", ",", "sie", "ha\u00b7ben", "ein", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind unterschiedentlich gelegt, sie hegen", "tokens": ["Sind", "un\u00b7ter\u00b7schie\u00b7dent\u00b7lich", "ge\u00b7legt", ",", "sie", "he\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "VVPP", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So mancherley Figur. Allein,", "tokens": ["So", "man\u00b7cher\u00b7ley", "Fi\u00b7gur", ".", "Al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$.", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Farben sch\u00f6ner Glantz, der Sonnen heller Schein,", "tokens": ["Der", "Far\u00b7ben", "sch\u00f6\u00b7ner", "Glantz", ",", "der", "Son\u00b7nen", "hel\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der scharff und s\u00fcsse Druck vom Riechen und von Schmecken,", "tokens": ["Der", "scharff", "und", "s\u00fcs\u00b7se", "Druck", "vom", "Rie\u00b7chen", "und", "von", "Schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schall, mit welchem uns die Wolcken schrecken,", "tokens": ["Der", "Schall", ",", "mit", "wel\u00b7chem", "uns", "die", "Wol\u00b7cken", "schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zusam\u0303t des Feuers herben Pein,", "tokens": ["Zu\u00b7sam\u0303t", "des", "Feu\u00b7ers", "her\u00b7ben", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geh\u00f6ren uns nun gantz allein.", "tokens": ["Ge\u00b7h\u00f6\u00b7ren", "uns", "nun", "gantz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}