{"dta.poem.21597": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wohin Du horchst \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin Du horchst, vernimmst Du den H\u00fclferuf", "tokens": ["Wo\u00b7hin", "Du", "horchst", ",", "ver\u00b7nimmst", "Du", "den", "H\u00fcl\u00b7fe\u00b7ruf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Noth! Wohin Du blickest, erschrecken Dich", "tokens": ["Der", "Noth", "!", "Wo\u00b7hin", "Du", "bli\u00b7ckest", ",", "er\u00b7schre\u00b7cken", "Dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gerungne H\u00e4nde, bleiche Lippen,", "tokens": ["Ge\u00b7rung\u00b7ne", "H\u00e4n\u00b7de", ",", "blei\u00b7che", "Lip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welche des Todes Beschw\u00f6rung murmeln!", "tokens": ["Wel\u00b7che", "des", "To\u00b7des", "Be\u00b7schw\u00f6\u00b7rung", "mur\u00b7meln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Wohin Du helfend schreitest, versinkt Dein Fu\u00df", "tokens": ["Wo\u00b7hin", "Du", "hel\u00b7fend", "schrei\u00b7test", ",", "ver\u00b7sinkt", "Dein", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Koth der L\u00fcgen. \u2014 Selbstischer Dummheit voll", "tokens": ["Im", "Koth", "der", "L\u00fc\u00b7gen", ".", "Selbs\u00b7ti\u00b7scher", "Dumm\u00b7heit", "voll"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$(", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schreit dort ein Protz nach \u201eOrdnung\u201c, ihm ja", "tokens": ["Schreit", "dort", "ein", "Protz", "nach", "\u201e", "Ord\u00b7nung", "\u201c", ",", "ihm", "ja"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "$(", "NN", "$(", "$,", "PPER", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00fcllte der \u201eg\u00fctige Gott\u201c den Fleischtopf.", "tokens": ["F\u00fcll\u00b7te", "der", "\u201e", "g\u00fc\u00b7ti\u00b7ge", "Gott", "\u201c", "den", "Fleischtopf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "$(", "ADJA", "NN", "$(", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "\u201ereformation\u201c, so heulen die Pfaffen rings.", "tokens": ["\u201e", "re\u00b7for\u00b7ma\u00b7ti\u00b7on", "\u201c", ",", "so", "heu\u00b7len", "die", "Pfaf\u00b7fen", "rings", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$(", "$,", "ADV", "ADV", "ART", "NN", "ADV", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201ees mu\u00df die Kirche wieder im Geisterreich", "tokens": ["\u201e", "es", "mu\u00df", "die", "Kir\u00b7che", "wie\u00b7der", "im", "Geis\u00b7ter\u00b7reich"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als Herrin thronen: ihre Lehren", "tokens": ["Als", "Her\u00b7rin", "thro\u00b7nen", ":", "ih\u00b7re", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "VVINF", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheuchen das Sorgen um ", "tokens": ["Scheu\u00b7chen", "das", "Sor\u00b7gen", "um"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Des Staates Herren hoffen des Staates Heil", "tokens": ["Des", "Staa\u00b7tes", "Her\u00b7ren", "hof\u00b7fen", "des", "Staa\u00b7tes", "Heil"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vom sichren Maulkorb, welcher das Bei\u00dfen wehrt,", "tokens": ["Vom", "sich\u00b7ren", "Maul\u00b7korb", ",", "wel\u00b7cher", "das", "Bei\u00b7\u00dfen", "wehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sogar das unbequeme Bellen", "tokens": ["So\u00b7gar", "das", "un\u00b7be\u00b7que\u00b7me", "Bel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wissen sie knebelgewandt zu d\u00e4mpfen \u2026", "tokens": ["Wis\u00b7sen", "sie", "kne\u00b7bel\u00b7ge\u00b7wandt", "zu", "d\u00e4mp\u00b7fen", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "In diesem dunkelflutenden Wogenschwall", "tokens": ["In", "die\u00b7sem", "dun\u00b7kel\u00b7flu\u00b7ten\u00b7den", "Wo\u00b7gen\u00b7schwall"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo ist der Boden, welcher den Anker h\u00e4lt?", "tokens": ["Wo", "ist", "der", "Bo\u00b7den", ",", "wel\u00b7cher", "den", "An\u00b7ker", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wann naht der Gott im Sturm fahrend,", "tokens": ["Wann", "naht", "der", "Gott", "im", "Sturm", "fah\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der die verpesteten L\u00fcfte reinigt?", "tokens": ["Der", "die", "ver\u00b7pes\u00b7te\u00b7ten", "L\u00fcf\u00b7te", "rei\u00b7nigt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Wo blitzt ein Lichtstrahl kommenden Morgenroths", "tokens": ["Wo", "blitzt", "ein", "Licht\u00b7strahl", "kom\u00b7men\u00b7den", "Mor\u00b7gen\u00b7roths"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An diesem nachtbelasteten Horizont?", "tokens": ["An", "die\u00b7sem", "nacht\u00b7be\u00b7las\u00b7te\u00b7ten", "Ho\u00b7ri\u00b7zont", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo sieht der Jugend Thatensehnsucht", "tokens": ["Wo", "sieht", "der", "Ju\u00b7gend", "Tha\u00b7ten\u00b7sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flattern die Wimpel des fernen Zieles?", "tokens": ["Flat\u00b7tern", "die", "Wim\u00b7pel", "des", "fer\u00b7nen", "Zie\u00b7les", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}