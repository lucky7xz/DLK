{"textgrid.poem.41392": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Philippus, K\u00f6nig in Macedonien, und Alster", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft ist der Witz ein scharfes Schwert,", "tokens": ["Oft", "ist", "der", "Witz", "ein", "schar\u00b7fes", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das pl\u00f6tzlich aus der Scheide f\u00e4hrt,", "tokens": ["Das", "pl\u00f6tz\u00b7lich", "aus", "der", "Schei\u00b7de", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, den es sch\u00fctzen soll, verletzet.", "tokens": ["Und", ",", "den", "es", "sch\u00fct\u00b7zen", "soll", ",", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Einfalt offnes Maul bleibt, ihr zum Vortheil, stumm;", "tokens": ["Der", "Ein\u00b7falt", "off\u00b7nes", "Maul", "bleibt", ",", "ihr", "zum", "Vor\u00b7theil", ",", "stumm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,", "PPER", "APPRART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Schweigen nutzet, und erg\u00f6tzet;", "tokens": ["Ihr", "Schwei\u00b7gen", "nut\u00b7zet", ",", "und", "er\u00b7g\u00f6t\u00b7zet", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und jener Amme Wunsch wird billig hochgesch\u00e4tzet,", "tokens": ["Und", "je\u00b7ner", "Am\u00b7me", "Wunsch", "wird", "bil\u00b7lig", "hoch\u00b7ge\u00b7sch\u00e4t\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "NN", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die zu dem S\u00e4ugling sprach: Mein liebstes Kind, sei dumm!", "tokens": ["Die", "zu", "dem", "S\u00e4ug\u00b7ling", "sprach", ":", "Mein", "liebs\u00b7tes", "Kind", ",", "sei", "dumm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Philippus Beispiel macht den Satz der Klugheit wahr:", "tokens": ["Phil\u00b7ip\u00b7pus", "Bei\u00b7spiel", "macht", "den", "Satz", "der", "Klug\u00b7heit", "wahr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sinnreich sein bringt oft Gefahr.", "tokens": ["Zu", "sinn\u00b7reich", "sein", "bringt", "oft", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAINF", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie strafte diesen gro\u00dfen K\u00f6nig", "tokens": ["Wie", "straf\u00b7te", "die\u00b7sen", "gro\u00b7\u00dfen", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Scherz, der ihm zu schnell entfiel!", "tokens": ["Ein", "Scherz", ",", "der", "ihm", "zu", "schnell", "ent\u00b7fiel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein einz'ger Feind ist schon zu viel,", "tokens": ["Ein", "einz'\u00b7ger", "Feind", "ist", "schon", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und hundert Freunde sind zu wenig.", "tokens": ["Und", "hun\u00b7dert", "Freun\u00b7de", "sind", "zu", "we\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "PTKA", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Philippus war bem\u00fcht, in Thracien zu dringen,", "tokens": ["Phil\u00b7ip\u00b7pus", "war", "be\u00b7m\u00fcht", ",", "in", "Thra\u00b7ci\u00b7en", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in dem Hinzug noch Methone zu bezwingen,", "tokens": ["Und", "in", "dem", "Hin\u00b7zug", "noch", "Me\u00b7tho\u00b7ne", "zu", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Alster, den man dort den besten Sch\u00fctzen hie\u00df,", "tokens": ["Als", "Als\u00b7ter", ",", "den", "man", "dort", "den", "bes\u00b7ten", "Sch\u00fct\u00b7zen", "hie\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PIS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich diesem K\u00f6nige zum Dienst entbieten lie\u00df.", "tokens": ["Sich", "die\u00b7sem", "K\u00f6\u00b7ni\u00b7ge", "zum", "Dienst", "ent\u00b7bie\u00b7ten", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PDAT", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihn r\u00fchmten Hof und Land; von allen ward erz\u00e4hlet,", "tokens": ["Ihn", "r\u00fchm\u00b7ten", "Hof", "und", "Land", ";", "von", "al\u00b7len", "ward", "er\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "APPR", "PIS", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur dieser habe nie der Sch\u00fcsse Ziel verfehlet,", "tokens": ["Nur", "die\u00b7ser", "ha\u00b7be", "nie", "der", "Sch\u00fcs\u00b7se", "Ziel", "ver\u00b7feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil sein geschwinder Pfeil, dem er die Kraft ertheilt,", "tokens": ["Weil", "sein", "ge\u00b7schwin\u00b7der", "Pfeil", ",", "dem", "er", "die", "Kraft", "er\u00b7theilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Oft V\u00f6gel in der Luft im st\u00e4rksten Flug ereilt.", "tokens": ["Oft", "V\u00f6\u00b7gel", "in", "der", "Luft", "im", "st\u00e4rks\u00b7ten", "Flug", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wohl! sprach Amynta's Sohn, wann wir mit Staaren streiten,", "tokens": ["Wohl", "!", "sprach", "A\u00b7myn\u00b7ta's", "Sohn", ",", "wann", "wir", "mit", "Staa\u00b7ren", "strei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "NE", "NN", "$,", "PWAV", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So soll er ganz gewi\u00df beim Angriff uns begleiten.", "tokens": ["So", "soll", "er", "ganz", "ge\u00b7wi\u00df", "beim", "An\u00b7griff", "uns", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das scheint f\u00fcrtrefflich sch\u00f6n; denn wer bewundert nicht", "tokens": ["Das", "scheint", "f\u00fcr\u00b7treff\u00b7lich", "sch\u00f6n", ";", "denn", "wer", "be\u00b7wun\u00b7dert", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "ADJD", "$.", "KON", "PWS", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den g\u00f6ttlichen Verstand, so oft ein K\u00f6nig spricht?", "tokens": ["Den", "g\u00f6tt\u00b7li\u00b7chen", "Ver\u00b7stand", ",", "so", "oft", "ein", "K\u00f6\u00b7nig", "spricht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Sch\u00fctze, seine Kunst nicht mehr verh\u00f6hnt zu sehen,", "tokens": ["Der", "Sch\u00fct\u00b7ze", ",", "sei\u00b7ne", "Kunst", "nicht", "mehr", "ver\u00b7h\u00f6hnt", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "ADV", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eilt, den Belagerten rachs\u00fcchtig beizustehen.", "tokens": ["Eilt", ",", "den", "Be\u00b7la\u00b7ger\u00b7ten", "rach\u00b7s\u00fcch\u00b7tig", "bei\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Er flieht in ihre Stadt, verst\u00e4rkt die Gegenwehr,", "tokens": ["Er", "flieht", "in", "ih\u00b7re", "Stadt", ",", "ver\u00b7st\u00e4rkt", "die", "Ge\u00b7gen\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und machet Sturm und Sieg dem stolzen Heere schwer,", "tokens": ["Und", "ma\u00b7chet", "Sturm", "und", "Sieg", "dem", "stol\u00b7zen", "Hee\u00b7re", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das pl\u00f6tzlich sich gescheucht und voll Best\u00fcrzung f\u00fchlet,", "tokens": ["Das", "pl\u00f6tz\u00b7lich", "sich", "ge\u00b7scheucht", "und", "voll", "Be\u00b7st\u00fcr\u00b7zung", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PRF", "VVPP", "KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil Alsters scharfer Pfeil, der auf den K\u00f6nig zielet,", "tokens": ["Weil", "Als\u00b7ters", "schar\u00b7fer", "Pfeil", ",", "der", "auf", "den", "K\u00f6\u00b7nig", "zie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den ihm bestimmten Flug mit dieser Aufschrift nimmt:", "tokens": ["Den", "ihm", "be\u00b7stimm\u00b7ten", "Flug", "mit", "die\u00b7ser", "Auf\u00b7schrift", "nimmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Philippus rechtem Aug' ist dieser Schu\u00df bestimmt.", "tokens": ["Phil\u00b7ip\u00b7pus", "rech\u00b7tem", "Aug'", "ist", "die\u00b7ser", "Schu\u00df", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der K\u00f6nig, der ihn nicht so f\u00fcrchterlich geglaubet,", "tokens": ["Der", "K\u00f6\u00b7nig", ",", "der", "ihn", "nicht", "so", "f\u00fcrch\u00b7ter\u00b7lich", "ge\u00b7glau\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bereut den Hechelscherz, der ihm sein Auge raubet,", "tokens": ["Be\u00b7reut", "den", "He\u00b7chel\u00b7scherz", ",", "der", "ihm", "sein", "Au\u00b7ge", "rau\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und schie\u00dft den Pfeil zur\u00fcck, mit dieser Gegenschrift:", "tokens": ["Und", "schie\u00dft", "den", "Pfeil", "zu\u00b7r\u00fcck", ",", "mit", "die\u00b7ser", "Ge\u00b7gen\u00b7schrift", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du, Alster, k\u00f6mmst ans Kreuz, sobald man dich betrifft.", "tokens": ["Du", ",", "Als\u00b7ter", ",", "k\u00f6mmst", "ans", "Kreuz", ",", "so\u00b7bald", "man", "dich", "be\u00b7tr\u00b7ifft", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}}, "stanza.7": {"line.1": {"text": "Kaum ward der Friede drauf der frohen Stadt versprochen,", "tokens": ["Kaum", "ward", "der", "Frie\u00b7de", "drauf", "der", "fro\u00b7hen", "Stadt", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ward auch Alsters Scherz durch seinen Tod gerochen.", "tokens": ["So", "ward", "auch", "Als\u00b7ters", "Scherz", "durch", "sei\u00b7nen", "Tod", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}