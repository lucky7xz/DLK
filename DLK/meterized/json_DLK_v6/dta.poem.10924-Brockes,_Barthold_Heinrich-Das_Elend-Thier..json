{"dta.poem.10924": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Elend-Thier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein sonderbar Gesch\u00f6pf! halb ein Hirsch und halb\nein Pferd,", "tokens": ["Welch", "ein", "son\u00b7der\u00b7bar", "Ge\u00b7sch\u00f6pf", "!", "halb", "ein", "Hirsch", "und", "halb", "ein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "$.", "ADJD", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wovon das Original, auch die k\u00fcnstliche Copie,", "tokens": ["Wo\u00b7von", "das", "O\u00b7rig\u00b7i\u00b7nal", ",", "auch", "die", "k\u00fcnst\u00b7li\u00b7che", "Co\u00b7pie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Beyde, da\u00df man sie bewundert, und, mit ernsten Blicken, sie,", "tokens": ["Bey\u00b7de", ",", "da\u00df", "man", "sie", "be\u00b7wun\u00b7dert", ",", "und", ",", "mit", "erns\u00b7ten", "Bli\u00b7cken", ",", "sie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PIS", "PPER", "VVPP", "$,", "KON", "$,", "APPR", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Der Natur und Geister Urquell blo\u00df zum Ruhm, betrachtet,", "tokens": ["Der", "Na\u00b7tur", "und", "Geis\u00b7ter", "Ur\u00b7quell", "blo\u00df", "zum", "Ruhm", ",", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NE", "ADV", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Sein Geweih ist ganz besonders, und als sonst kein Thier", "tokens": ["Sein", "Ge\u00b7weih", "ist", "ganz", "be\u00b7son\u00b7ders", ",", "und", "als", "sonst", "kein", "Thier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "$,", "KON", "KOUS", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Eines Adlers Schwingen gleich, wenn er sie herunter schl\u00e4get.", "tokens": ["Ei\u00b7nes", "Ad\u00b7lers", "Schwin\u00b7gen", "gleich", ",", "wenn", "er", "sie", "her\u00b7un\u00b7ter", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Von der Klaue saget man, da\u00df sie gro\u00dfe Kr\u00e4fte heget,", "tokens": ["Von", "der", "Klau\u00b7e", "sa\u00b7get", "man", ",", "da\u00df", "sie", "gro\u00b7\u00dfe", "Kr\u00e4f\u00b7te", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Und im Krampf und Nerven-Schmerzen, Linderung und H\u00fcl-", "tokens": ["Und", "im", "Krampf", "und", "Ner\u00b7ven\u00b7Schmer\u00b7zen", ",", "Lin\u00b7de\u00b7rung", "und", "H\u00fcl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "KON", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Da die Dicke seiner Haut weder Stich noch Hieb durchdringet:", "tokens": ["Da", "die", "Di\u00b7cke", "sei\u00b7ner", "Haut", "we\u00b7der", "Stich", "noch", "Hieb", "durch\u00b7drin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Braucht man sie an Panzers statt, da sie Schirm und Nutzen", "tokens": ["Braucht", "man", "sie", "an", "Pan\u00b7zers", "statt", ",", "da", "sie", "Schirm", "und", "Nut\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "PTKVZ", "$,", "KOUS", "PPER", "NN", "KON", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Seines K\u00f6rpers Schwere gleichet einer ziemlich starken Kuh.", "tokens": ["Sei\u00b7nes", "K\u00f6r\u00b7pers", "Schwe\u00b7re", "glei\u00b7chet", "ei\u00b7ner", "ziem\u00b7lich", "star\u00b7ken", "Kuh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Vorn am Halse ist es zottigt, aber glatt nach hinten zu.", "tokens": ["Vorn", "am", "Hal\u00b7se", "ist", "es", "zot\u00b7tigt", ",", "a\u00b7ber", "glatt", "nach", "hin\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "VVFIN", "$,", "ADV", "ADJD", "APPR", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Um sein langes Ober-Maul, soll man es, nicht vorwerts gehen,", "tokens": ["Um", "sein", "lan\u00b7ges", "O\u00b7ber\u00b7Maul", ",", "soll", "man", "es", ",", "nicht", "vor\u00b7werts", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,", "VMFIN", "PIS", "PPER", "$,", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "(da\u00df es ihm nicht hindr\u2019 im Grasen,) sondern r\u00fcckwerts wei-", "tokens": ["(", "da\u00df", "es", "ihm", "nicht", "hindr'", "im", "Gra\u00b7sen", ",", ")", "son\u00b7dern", "r\u00fcck\u00b7werts", "wei"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "$,", "$(", "KON", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.15": {"text": "Langen Durst und schwere Arbeit, ist es t\u00fcchtig zu ertragen.", "tokens": ["Lan\u00b7gen", "Durst", "und", "schwe\u00b7re", "Ar\u00b7beit", ",", "ist", "es", "t\u00fcch\u00b7tig", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Doch da ich die\u00df Kupfer seh, zwingt mich fast die Kunst zu", "tokens": ["Doch", "da", "ich", "die\u00df", "Kup\u00b7fer", "seh", ",", "zwingt", "mich", "fast", "die", "Kunst", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDS", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU"], "meter": "--+-+-++-+-+-", "measure": "anapaest.init"}, "line.17": {"text": "Ob die kluge Schw\u00e4rz am Thier mehr die Landschaft wei-", "tokens": ["Ob", "die", "klu\u00b7ge", "Schw\u00e4rz", "am", "Thier", "mehr", "die", "Land\u00b7schaft", "wei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "ART", "NN", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Oder ob die linde Landschaft es noch mehr herausgebracht.", "tokens": ["O\u00b7der", "ob", "die", "lin\u00b7de", "Land\u00b7schaft", "es", "noch", "mehr", "her\u00b7aus\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}