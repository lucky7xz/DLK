{"textgrid.poem.48807": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "32. Er betrachtet ihre Sch\u00f6nheit und Treue", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du treue Sch\u00f6nheit du und auch du sch\u00f6ne Treue,", "tokens": ["Du", "treu\u00b7e", "Sch\u00f6n\u00b7heit", "du", "und", "auch", "du", "sch\u00f6\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "KON", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die ihr den zarten Leib und edlen Geist besitzt,", "tokens": ["die", "ihr", "den", "zar\u00b7ten", "Leib", "und", "ed\u00b7len", "Geist", "be\u00b7sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ihr Schwestern gleicher Kraft, die ihr mir das besch\u00fctzt,", "tokens": ["ihr", "Schwes\u00b7tern", "glei\u00b7cher", "Kraft", ",", "die", "ihr", "mir", "das", "be\u00b7sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wor\u00fcber ich mich-stets mit h\u00f6chsten Freuden freue,", "tokens": ["wo\u00b7r\u00fc\u00b7ber", "ich", "mich\u00b7stets", "mit", "h\u00f6chs\u00b7ten", "Freu\u00b7den", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "was sag' ich doch von euch, da\u00df euch und mich nicht reue?", "tokens": ["was", "sag'", "ich", "doch", "von", "euch", ",", "da\u00df", "euch", "und", "mich", "nicht", "reu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "KON", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr starke G\u00f6ttinnen, habt mir den Sinn erhitzt,", "tokens": ["Ihr", "star\u00b7ke", "G\u00f6t\u00b7tin\u00b7nen", ",", "habt", "mir", "den", "Sinn", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "da\u00df mir auf dieser Welt nichts als nur Eine n\u00fctzt.", "tokens": ["da\u00df", "mir", "auf", "die\u00b7ser", "Welt", "nichts", "als", "nur", "Ei\u00b7ne", "n\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "PIS", "KOKOM", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie ists, an der ich mich ohn' Unterla\u00df verneue.", "tokens": ["Sie", "ists", ",", "an", "der", "ich", "mich", "ohn'", "Un\u00b7ter\u00b7la\u00df", "ver\u00b7neu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PRELS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die zarte Sch\u00f6nheit folgt der Flucht der sch\u00f6nen Zeit,", "tokens": ["Die", "zar\u00b7te", "Sch\u00f6n\u00b7heit", "folgt", "der", "Flucht", "der", "sch\u00f6\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die feste Treue geht den Weg der Ewigkeit,", "tokens": ["die", "fes\u00b7te", "Treu\u00b7e", "geht", "den", "Weg", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Sch\u00f6nheit macht mir Lust, die Treue Trost zu leben.", "tokens": ["die", "Sch\u00f6n\u00b7heit", "macht", "mir", "Lust", ",", "die", "Treu\u00b7e", "Trost", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O wie ein g\u00f6ttlichs Mensch ist diese, die euch hat!", "tokens": ["O", "wie", "ein", "g\u00f6tt\u00b7lichs", "Mensch", "ist", "die\u00b7se", ",", "die", "euch", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ART", "ADJA", "NN", "VAFIN", "PDS", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O wie ein Menschgott auch wird der, dem in der Tat", "tokens": ["O", "wie", "ein", "Menschgott", "auch", "wird", "der", ",", "dem", "in", "der", "Tat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "NN", "ADV", "VAFIN", "ART", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+--+-+----+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "wird diese sch\u00f6ne Treu' und treue Sch\u00f6nheit geben!", "tokens": ["wird", "die\u00b7se", "sch\u00f6\u00b7ne", "Treu'", "und", "treu\u00b7e", "Sch\u00f6n\u00b7heit", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}