{"dta.poem.8576": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch einen schwarzen, schwehlenden Schneckengang", "tokens": ["Durch", "ei\u00b7nen", "schwar\u00b7zen", ",", "schweh\u00b7len\u00b7den", "Schne\u00b7cken\u00b7gang"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "stinken Pechfackeln.", "tokens": ["stin\u00b7ken", "Pech\u00b7fa\u00b7ckeln", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Gr\u00fcne, johlende Meerkater", "tokens": ["Gr\u00fc\u00b7ne", ",", "joh\u00b7len\u00b7de", "Meer\u00b7ka\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "mit Eisenklauen und geringelten Schw\u00e4nzen", "tokens": ["mit", "Ei\u00b7sen\u00b7klau\u00b7en", "und", "ge\u00b7rin\u00b7gel\u00b7ten", "Schw\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "schieben, schleppen, zerren, beissen mich", "tokens": ["schie\u00b7ben", ",", "schlep\u00b7pen", ",", "zer\u00b7ren", ",", "beis\u00b7sen", "mich"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "vor die boshaften Greise.", "tokens": ["vor", "die", "bos\u00b7haf\u00b7ten", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die hocken, Strohkronen auf ihren Sch\u00e4deln, und blinzeln.", "tokens": ["Die", "ho\u00b7cken", ",", "Stroh\u00b7kro\u00b7nen", "auf", "ih\u00b7ren", "Sch\u00e4\u00b7deln", ",", "und", "blin\u00b7zeln", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ihre langen Geierh\u00e4lse recken sich,", "tokens": ["Ih\u00b7re", "lan\u00b7gen", "Gei\u00b7er\u00b7h\u00e4l\u00b7se", "re\u00b7cken", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "aus ihren Froschm\u00e4ulern quillt Geifer.", "tokens": ["aus", "ih\u00b7ren", "Frosc\u00b7hm\u00e4u\u00b7lern", "quillt", "Gei\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du hast Unsre Tropfsteinst\u00fchle bespien! Du hast \u00fcber Unsre Ges\u00e4ssschwielen gelacht!", "tokens": ["Du", "hast", "Uns\u00b7re", "Tropf\u00b7stein\u00b7st\u00fch\u00b7le", "be\u00b7spi\u00b7en", "!", "Du", "hast", "\u00fc\u00b7ber", "Uns\u00b7re", "Ge\u00b7s\u00e4ss\u00b7schwie\u00b7len", "ge\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVINF", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+-+-+---+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Du hast Unsre Excremente nicht verehrt!", "tokens": ["Du", "hast", "Uns\u00b7re", "Ex\u00b7cre\u00b7men\u00b7te", "nicht", "ver\u00b7ehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Schon hebt der Henker, eine Mandril, seinen riesigen Pl\u00e4ttbolzen.", "tokens": ["Schon", "hebt", "der", "Hen\u00b7ker", ",", "ei\u00b7ne", "Mand\u00b7ril", ",", "sei\u00b7nen", "rie\u00b7si\u00b7gen", "Pl\u00e4tt\u00b7bol\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Der gl\u00fcht!", "tokens": ["Der", "gl\u00fcht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "Die Bestien br\u00fcllen, das Eisen zischt,", "tokens": ["Die", "Be\u00b7sti\u00b7en", "br\u00fcl\u00b7len", ",", "das", "Ei\u00b7sen", "zischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "rotes, berstendes Blutlicht zersprengt die H\u00f6hle.", "tokens": ["ro\u00b7tes", ",", "bers\u00b7ten\u00b7des", "Blut\u00b7licht", "zer\u00b7sprengt", "die", "H\u00f6h\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Pestkanaillen!!", "tokens": ["Pest\u00b7ka\u00b7nail\u00b7len", "!!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Ich strample, stosse, sch\u00e4ume, schreie, schlage w\u00fctend um mich.", "tokens": ["Ich", "stramp\u00b7le", ",", "stos\u00b7se", ",", "sch\u00e4u\u00b7me", ",", "schrei\u00b7e", ",", "schla\u00b7ge", "w\u00fc\u00b7tend", "um", "mich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.11": {"line.1": {"text": "St\u00fcrzen die Sterne zusammen,", "tokens": ["St\u00fcr\u00b7zen", "die", "Ster\u00b7ne", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "bricht die Welt ein?", "tokens": ["bricht", "die", "Welt", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.12": {"line.1": {"text": "Auf meinem Bettvorleger,", "tokens": ["Auf", "mei\u00b7nem", "Bett\u00b7vor\u00b7le\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "in kleinen T\u00fcmpeln,", "tokens": ["in", "klei\u00b7nen", "T\u00fcm\u00b7peln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "zwischen den blauen, blitzenden Scherben meiner Karaffe,", "tokens": ["zwi\u00b7schen", "den", "blau\u00b7en", ",", "blit\u00b7zen\u00b7den", "Scher\u00b7ben", "mei\u00b7ner", "Ka\u00b7raf\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "glitzert die Morgensonne.", "tokens": ["glit\u00b7zert", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}