{"dta.poem.16067": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "121.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wenig wissen doch die Menschen sich zu sagen", "tokens": ["Wie", "we\u00b7nig", "wis\u00b7sen", "doch", "die", "Men\u00b7schen", "sich", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des sagenswerthen, die sich in Gesellschaft plagen.", "tokens": ["Des", "sa\u00b7gens\u00b7wert\u00b7hen", ",", "die", "sich", "in", "Ge\u00b7sell\u00b7schaft", "pla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Alsob ertr\u00e4glicher dadurch die Langeweile", "tokens": ["Al\u00b7sob", "er\u00b7tr\u00e4g\u00b7li\u00b7cher", "da\u00b7durch", "die", "Lan\u00b7ge\u00b7wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem einen sei, da\u00df er sie mit den andern theile.", "tokens": ["Dem", "ei\u00b7nen", "sei", ",", "da\u00df", "er", "sie", "mit", "den", "an\u00b7dern", "thei\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo Ungelehrte unertr\u00e4glich thun gelehrt,", "tokens": ["Wo", "Un\u00b7ge\u00b7lehr\u00b7te", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "thun", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da thun Gelehrte nun gar kl\u00e4glich ungelehrt.", "tokens": ["Da", "thun", "Ge\u00b7lehr\u00b7te", "nun", "gar", "kl\u00e4g\u00b7lich", "un\u00b7ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur selten im Gespr\u00e4ch entwischt ein guter Spruch,", "tokens": ["Nur", "sel\u00b7ten", "im", "Ge\u00b7spr\u00e4ch", "ent\u00b7wischt", "ein", "gu\u00b7ter", "Spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil jeder, was er wei\u00df, spart lieber f\u00fcr ein Buch.", "tokens": ["Weil", "je\u00b7der", ",", "was", "er", "wei\u00df", ",", "spart", "lie\u00b7ber", "f\u00fcr", "ein", "Buch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}