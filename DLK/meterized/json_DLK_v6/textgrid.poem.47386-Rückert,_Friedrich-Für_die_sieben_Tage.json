{"textgrid.poem.47386": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr die sieben Tage", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprich, liebes Herz, in deines Tempels Mitten,", "tokens": ["Sprich", ",", "lie\u00b7bes", "Herz", ",", "in", "dei\u00b7nes", "Tem\u00b7pels", "Mit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr sieben Wochentage sieben Bitten.", "tokens": ["F\u00fcr", "sie\u00b7ben", "Wo\u00b7chen\u00b7ta\u00b7ge", "sie\u00b7ben", "Bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum ersten Tag: La\u00df deine Sonne tagen", "tokens": ["Zum", "ers\u00b7ten", "Tag", ":", "La\u00df", "dei\u00b7ne", "Son\u00b7ne", "ta\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Licht verleihn der Erd' und meinen Schritten.", "tokens": ["Und", "Licht", "ver\u00b7leihn", "der", "Erd'", "und", "mei\u00b7nen", "Schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zum zweiten Tag: O la\u00df nach dir mich wandeln,", "tokens": ["Zum", "zwei\u00b7ten", "Tag", ":", "O", "la\u00df", "nach", "dir", "mich", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "VVFIN", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie Mond der Sonne nach mit leisen Tritten.", "tokens": ["Wie", "Mond", "der", "Son\u00b7ne", "nach", "mit", "lei\u00b7sen", "Trit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zum dritten Tag: Lehr' deinen Dienst mich kennen", "tokens": ["Zum", "drit\u00b7ten", "Tag", ":", "Lehr'", "dei\u00b7nen", "Dienst", "mich", "ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "NN", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wie ich dienen soll mit rechten Sitten.", "tokens": ["Und", "wie", "ich", "die\u00b7nen", "soll", "mit", "rech\u00b7ten", "Sit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zum vierten Tag: Du wollst mich nicht verlassen", "tokens": ["Zum", "vier\u00b7ten", "Tag", ":", "Du", "wollst", "mich", "nicht", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In meiner Woch', in meines Tagwerks Mitten.", "tokens": ["In", "mei\u00b7ner", "Wo\u00b7ch'", ",", "in", "mei\u00b7nes", "Tag\u00b7werks", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Zum f\u00fcnften Tag: O donnr' ins Herz mir deine", "tokens": ["Zum", "f\u00fcnf\u00b7ten", "Tag", ":", "O", "donn\u00b7r'", "ins", "Herz", "mir", "dei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "NE", "APPRART", "NN", "PPER", "PPOSAT"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Gebote, wann sie meinem Sinn entglitten.", "tokens": ["Ge\u00b7bo\u00b7te", ",", "wann", "sie", "mei\u00b7nem", "Sinn", "ent\u00b7glit\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zum sechsten Tag: O la\u00df mich freudig f\u00fchlen,", "tokens": ["Zum", "sechs\u00b7ten", "Tag", ":", "O", "la\u00df", "mich", "freu\u00b7dig", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "NE", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wodurch du mir die Freiheit hast erstritten.", "tokens": ["Wo\u00b7durch", "du", "mir", "die", "Frei\u00b7heit", "hast", "er\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Zum siebenten: Die Sonne sinkt am Abend;", "tokens": ["Zum", "sie\u00b7ben\u00b7ten", ":", "Die", "Son\u00b7ne", "sinkt", "am", "A\u00b7bend", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "O d\u00fcrft' ich mir so hellen Tod erbitten!", "tokens": ["O", "d\u00fcrft'", "ich", "mir", "so", "hel\u00b7len", "Tod", "er\u00b7bit\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}