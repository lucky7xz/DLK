{"textgrid.poem.58005": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "43. Wer's recht greift an, hat's halb getan", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst, es sei dir schwer, Lust, Sinn und Willen zwingen;", "tokens": ["Du", "sprichst", ",", "es", "sei", "dir", "schwer", ",", "Lust", ",", "Sinn", "und", "Wil\u00b7len", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja, zwingen tut auch weh, doch greif's mit Gott nur an,", "tokens": ["Ja", ",", "zwin\u00b7gen", "tut", "auch", "weh", ",", "doch", "greif's", "mit", "Gott", "nur", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVINF", "VVFIN", "ADV", "PTKVZ", "$,", "ADV", "NE", "APPR", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So wirst du bald mit Lust ganz leicht und frei vollbringen,", "tokens": ["So", "wirst", "du", "bald", "mit", "Lust", "ganz", "leicht", "und", "frei", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was du vorhin mit Zwang in eigner Kraft getan!", "tokens": ["Was", "du", "vor\u00b7hin", "mit", "Zwang", "in", "eig\u00b7ner", "Kraft", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}