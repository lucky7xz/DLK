{"textgrid.poem.57894": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Elektrisch ist heute fast alles", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Elektrisch ist heute fast alles", "tokens": ["E\u00b7lekt\u00b7risch", "ist", "heu\u00b7te", "fast", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PIS"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf dieser Erde Rund,", "tokens": ["Auf", "die\u00b7ser", "Er\u00b7de", "Rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Elektrisch macht tot man die Menschen,", "tokens": ["E\u00b7lekt\u00b7risch", "macht", "tot", "man", "die", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Elektrisch macht man sie gesund.", "tokens": ["E\u00b7lekt\u00b7risch", "macht", "man", "sie", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Elektrisch kann man schon n\u00e4hen,", "tokens": ["E\u00b7lekt\u00b7risch", "kann", "man", "schon", "n\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Elektrisch pl\u00e4tten man kann,", "tokens": ["E\u00b7lekt\u00b7risch", "pl\u00e4t\u00b7ten", "man", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "VMFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Elektrisch kann man sich schreiben,", "tokens": ["E\u00b7lekt\u00b7risch", "kann", "man", "sich", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Elektrisch ruft man sich an.", "tokens": ["E\u00b7lekt\u00b7risch", "ruft", "man", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PRF", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Elektrisch beim Sch\u00fctzenfeste", "tokens": ["E\u00b7lekt\u00b7risch", "beim", "Sch\u00fct\u00b7zen\u00b7fes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Rundteil beleuchtet war,", "tokens": ["Das", "Rund\u00b7teil", "be\u00b7leuch\u00b7tet", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Elektrisch lassen die Damen", "tokens": ["E\u00b7lekt\u00b7risch", "las\u00b7sen", "die", "Da\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entfernen das Schnurrbartshaar.", "tokens": ["Ent\u00b7fer\u00b7nen", "das", "Schnurr\u00b7barts\u00b7haar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Elektrisch ist heute fast alles,", "tokens": ["E\u00b7lekt\u00b7risch", "ist", "heu\u00b7te", "fast", "al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PIS", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und was das Neueste ist,", "tokens": ["Und", "was", "das", "Neu\u00b7es\u00b7te", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In Hannover f\u00e4hrt man elektrisch", "tokens": ["In", "Han\u00b7no\u00b7ver", "f\u00e4hrt", "man", "e\u00b7lekt\u00b7risch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PIS", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auf der Georgstra\u00dfe Mist.", "tokens": ["Auf", "der", "Ge\u00b7orgs\u00b7tra\u00b7\u00dfe", "Mist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}