{"dta.poem.12597": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ziel des menschlichen Bestrebens,", "tokens": ["Ziel", "des", "menschli\u00b7chen", "Be\u00b7stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ziel, das man mit M\u00fch erreicht!", "tokens": ["Ziel", ",", "das", "man", "mit", "M\u00fch", "er\u00b7reicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nste Beute dieses Lebens!", "tokens": ["Sch\u00f6ns\u00b7te", "Beu\u00b7te", "die\u00b7ses", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kleinod, dem kein Reichthum gleicht!", "tokens": ["Klei\u00b7nod", ",", "dem", "kein", "Reicht\u00b7hum", "gleicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tugend! dich, dich, unbefleckte Sch\u00f6ne!", "tokens": ["Tu\u00b7gend", "!", "dich", ",", "dich", ",", "un\u00b7be\u00b7fleck\u00b7te", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "$,", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lieben Griechen-Landes S\u00f6hne.", "tokens": ["Lie\u00b7ben", "Grie\u00b7chen\u00b7Lan\u00b7des", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhnen heissen alle Plagen", "tokens": ["Jh\u00b7nen", "heis\u00b7sen", "al\u00b7le", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das grausamste Geschick,", "tokens": ["Und", "das", "grau\u00b7sams\u00b7te", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wenn sie es f\u00fcr dich ertragen,", "tokens": ["Wenn", "sie", "es", "f\u00fcr", "dich", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein beneidenswehrtes Gl\u00fcck.", "tokens": ["Ein", "be\u00b7nei\u00b7dens\u00b7wehr\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Quaal und Tod f\u00fcr dich geduldig leiden,", "tokens": ["Qua\u00b7al", "und", "Tod", "f\u00fcr", "dich", "ge\u00b7dul\u00b7dig", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist ein Theil von ihren Freuden.", "tokens": ["Ist", "ein", "Theil", "von", "ih\u00b7ren", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dies zeigt deines Samens Bl\u00fcte,", "tokens": ["Dies", "zeigt", "dei\u00b7nes", "Sa\u00b7mens", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fr\u00fcchte der Unsterblichkeit,", "tokens": ["Fr\u00fcch\u00b7te", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Fr\u00fcchte, welche deine G\u00fcte", "tokens": ["Fr\u00fcch\u00b7te", ",", "wel\u00b7che", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Menschen Herze streut.", "tokens": ["In", "der", "Men\u00b7schen", "Her\u00b7ze", "streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eltern, Gold, der s\u00fcsse Schlaf gefallen;", "tokens": ["El\u00b7tern", ",", "Gold", ",", "der", "s\u00fcs\u00b7se", "Schlaf", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aber du gef\u00e4llst vor allen.", "tokens": ["A\u00b7ber", "du", "ge\u00b7f\u00e4llst", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Herculs, Castors, Pollux Werke,", "tokens": ["Her\u00b7culs", ",", "Cas\u00b7tors", ",", "Pol\u00b7lux", "Wer\u00b7ke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so viel f\u00fcr dich gethan,", "tokens": ["Die", "so", "viel", "f\u00fcr", "dich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Waren Zeugen deiner St\u00e4rke,", "tokens": ["Wa\u00b7ren", "Zeu\u00b7gen", "dei\u00b7ner", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fcndigten dein Daseyn an.", "tokens": ["K\u00fcn\u00b7dig\u00b7ten", "dein", "Da\u00b7seyn", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und warum starb Ajax mit Achillen?", "tokens": ["Und", "wa\u00b7rum", "starb", "A\u00b7jax", "mit", "A\u00b7chil\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "NE", "APPR", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Tugend! nur um deinetwillen.", "tokens": ["Tu\u00b7gend", "!", "nur", "um", "dei\u00b7net\u00b7wil\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine Sch\u00f6nheit zeugt die Triebe,", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "zeugt", "die", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Womit du gesuchet wirst.", "tokens": ["Wo\u00b7mit", "du", "ge\u00b7su\u00b7chet", "wirst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Sch\u00f6nheit blos zu Liebe", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "blos", "zu", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blendet sich Atarnens F\u00fcrst,", "tokens": ["Blen\u00b7det", "sich", "A\u00b7tar\u00b7nens", "F\u00fcrst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieser F\u00fcrst, den sein Verdienst und Leben", "tokens": ["Die\u00b7ser", "F\u00fcrst", ",", "den", "sein", "Ver\u00b7dienst", "und", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Ewigkeit erheben.", "tokens": ["Und", "die", "E\u00b7wig\u00b7keit", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Des Ged\u00e4chtnisses Geschlechte,", "tokens": ["Des", "Ge\u00b7d\u00e4cht\u00b7nis\u00b7ses", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Musen! wollt ihr Zevs erh\u00f6hn,", "tokens": ["Mu\u00b7sen", "!", "wollt", "ihr", "Zevs", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "NE", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter dessen Schutz die Rechte", "tokens": ["Un\u00b7ter", "des\u00b7sen", "Schutz", "die", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Gast-Freyheit sicher stehn:", "tokens": ["Der", "Gast\u00b7Frey\u00b7heit", "si\u00b7cher", "stehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O so la\u00dft stets unter eurem Singen", "tokens": ["O", "so", "la\u00dft", "stets", "un\u00b7ter", "eu\u00b7rem", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dieses F\u00fcrsten Lob erklingen.", "tokens": ["Die\u00b7ses", "F\u00fcrs\u00b7ten", "Lob", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und so oft als eure Leyer", "tokens": ["Und", "so", "oft", "als", "eu\u00b7re", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von der Freundschaft Alter spielt,", "tokens": ["Von", "der", "Freund\u00b7schaft", "Al\u00b7ter", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die das jugendliche Feuer", "tokens": ["Die", "das", "ju\u00b7gend\u00b7li\u00b7che", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die erste Treu noch f\u00fchlt,", "tokens": ["Und", "die", "ers\u00b7te", "Treu", "noch", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "O so oft la\u00dft unter euren Ch\u00f6ren", "tokens": ["O", "so", "oft", "la\u00dft", "un\u00b7ter", "eu\u00b7ren", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dieses F\u00fcrsten Lob-Lied h\u00f6ren.", "tokens": ["Die\u00b7ses", "F\u00fcrs\u00b7ten", "Lob\u00b7Lied", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}