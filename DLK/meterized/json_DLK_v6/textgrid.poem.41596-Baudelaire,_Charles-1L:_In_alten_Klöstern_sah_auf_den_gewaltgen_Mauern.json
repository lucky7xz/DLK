{"textgrid.poem.41596": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: In alten Kl\u00f6stern sah auf den gewaltgen Mauern", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In alten Kl\u00f6stern sah auf den gewaltgen Mauern", "tokens": ["In", "al\u00b7ten", "Kl\u00f6s\u00b7tern", "sah", "auf", "den", "ge\u00b7walt\u00b7gen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wahrheit man gemalt in heilgem Strahlenkleid,", "tokens": ["Die", "Wahr\u00b7heit", "man", "ge\u00b7malt", "in", "heil\u00b7gem", "Strah\u00b7len\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Herz erw\u00e4rmte sie den b\u00fc\u00dfenden Beschauern", "tokens": ["Das", "Herz", "er\u00b7w\u00e4rm\u00b7te", "sie", "den", "b\u00fc\u00b7\u00dfen\u00b7den", "Be\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und milderte den Frost der strengen Fr\u00f6mmigkeit.", "tokens": ["Und", "mil\u00b7der\u00b7te", "den", "Frost", "der", "stren\u00b7gen", "Fr\u00f6m\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als damals Christi Saat gespro\u00dft aus Segensschauern,", "tokens": ["Als", "da\u00b7mals", "Chris\u00b7ti", "Saat", "ge\u00b7spro\u00dft", "aus", "Se\u00b7gens\u00b7schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nahm mancher M\u00f6nch, des Ruhm verl\u00f6scht ist durch die Zeit,", "tokens": ["Nahm", "man\u00b7cher", "M\u00f6nch", ",", "des", "Ruhm", "ver\u00b7l\u00f6scht", "ist", "durch", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "ART", "NN", "VVPP", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu seiner Werkstatt sich des Grabfelds ernstes Trauern", "tokens": ["Zu", "sei\u00b7ner", "Werk\u00b7statt", "sich", "des", "Grab\u00b7felds", "erns\u00b7tes", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und feierte den Tod mit schlichter Einfachheit.", "tokens": ["Und", "fei\u00b7er\u00b7te", "den", "Tod", "mit", "schlich\u00b7ter", "Ein\u00b7fach\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein Herz ist eine Gruft. Ein schlechter M\u00f6nch durcheile", "tokens": ["Mein", "Herz", "ist", "ei\u00b7ne", "Gruft", ".", "Ein", "schlech\u00b7ter", "M\u00f6nch", "durc\u00b7hei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seit Ewigkeiten ich den Raum, wo tr\u00fcb ich weile,", "tokens": ["Seit", "E\u00b7wig\u00b7kei\u00b7ten", "ich", "den", "Raum", ",", "wo", "tr\u00fcb", "ich", "wei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ART", "NN", "$,", "PWAV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Bild versch\u00f6nt mir des verha\u00dften Klosters Wand.", "tokens": ["Kein", "Bild", "ver\u00b7sch\u00f6nt", "mir", "des", "ver\u00b7ha\u00df\u00b7ten", "Klos\u00b7ters", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O tatenloser M\u00f6nch! Wann wird es mir gelingen,", "tokens": ["O", "ta\u00b7ten\u00b7lo\u00b7ser", "M\u00f6nch", "!", "Wann", "wird", "es", "mir", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PWAV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem schmerzensreichen Spiel des Lebens abzuringen", "tokens": ["Dem", "schmer\u00b7zens\u00b7rei\u00b7chen", "Spiel", "des", "Le\u00b7bens", "ab\u00b7zu\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Augen Labsal und die Arbeit meiner Hand!", "tokens": ["Der", "Au\u00b7gen", "Lab\u00b7sal", "und", "die", "Ar\u00b7beit", "mei\u00b7ner", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}