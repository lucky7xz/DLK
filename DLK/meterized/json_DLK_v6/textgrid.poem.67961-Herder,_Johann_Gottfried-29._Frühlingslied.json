{"textgrid.poem.67961": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "29. Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schnee zerschmilzt, der Fr\u00fchling kommt", "tokens": ["Der", "Schnee", "zer\u00b7schmilzt", ",", "der", "Fr\u00fch\u00b7ling", "kommt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seiner Blumen Schaar,", "tokens": ["Mit", "sei\u00b7ner", "Blu\u00b7men", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Busch und Baum ist jung und gr\u00fcn,", "tokens": ["Und", "Busch", "und", "Baum", "ist", "jung", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NE", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bl\u00fchend wie er war,", "tokens": ["Und", "bl\u00fc\u00b7hend", "wie", "er", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Bergen rauscht der Strom nicht mehr", "tokens": ["Von", "Ber\u00b7gen", "rauscht", "der", "Strom", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit wilder Fluthen Fall;", "tokens": ["Mit", "wil\u00b7der", "Flut\u00b7hen", "Fall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In seinen Ufern murmelt er,", "tokens": ["In", "sei\u00b7nen", "U\u00b7fern", "mur\u00b7melt", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein schleichender Krystall.", "tokens": ["Ein", "schlei\u00b7chen\u00b7der", "Krys\u00b7tall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ob Ewigkeit hienieden sey?", "tokens": ["Ob", "E\u00b7wig\u00b7keit", "hien\u00b7ie\u00b7den", "sey", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt Jahr- und Tageslauf:", "tokens": ["Zeigt", "Jahr", "und", "Ta\u00b7ges\u00b7lauf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne, die jezt niedergeht", "tokens": ["Die", "Son\u00b7ne", ",", "die", "jezt", "nie\u00b7der\u00b7geht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geht morgen wieder auf.", "tokens": ["Geht", "mor\u00b7gen", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was steiget, f\u00e4llt; in kurzer Frist", "tokens": ["Was", "stei\u00b7get", ",", "f\u00e4llt", ";", "in", "kur\u00b7zer", "Frist"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt wieder auf, was f\u00e4llt;", "tokens": ["Kommt", "wie\u00b7der", "auf", ",", "was", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Mensch der einmal drunten ist,", "tokens": ["Der", "Mensch", "der", "ein\u00b7mal", "drun\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sieht nimmermehr die Welt.", "tokens": ["Sieht", "nim\u00b7mer\u00b7mehr", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und was sein Gut hienieden sey,", "tokens": ["Und", "was", "sein", "Gut", "hien\u00b7ie\u00b7den", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist, ders ihm sichern kann?", "tokens": ["Ist", ",", "ders", "ihm", "si\u00b7chern", "kann", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schnitt Lachesis nicht heute ab,", "tokens": ["Schnitt", "La\u00b7che\u00b7sis", "nicht", "heu\u00b7te", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Klotho gestern spann?", "tokens": ["Was", "Klo\u00b7tho", "ge\u00b7stern", "spann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O Elend, o Gebrechlichkeit,", "tokens": ["O", "E\u00b7lend", ",", "o", "Ge\u00b7brech\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf Tand und Nebel baun!", "tokens": ["Auf", "Tand", "und", "Ne\u00b7bel", "baun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Des Todes zu gewissen Streich", "tokens": ["Des", "To\u00b7des", "zu", "ge\u00b7wis\u00b7sen", "Streich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Ungewissen traun!", "tokens": ["Im", "Un\u00b7ge\u00b7wis\u00b7sen", "traun", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur Traum, nur Traumgl\u00fcckseligkeit", "tokens": ["Nur", "Traum", ",", "nur", "Traum\u00b7gl\u00fcck\u00b7se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nieden unser Theil!", "tokens": ["Ist", "nie\u00b7den", "un\u00b7ser", "Theil", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "M\u00fch' ist das Leben, ach und fleucht", "tokens": ["M\u00fch'", "ist", "das", "Le\u00b7ben", ",", "ach", "und", "fleucht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "XY", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein verscho\u00dfner Pfeil.", "tokens": ["Wie", "ein", "ver\u00b7scho\u00df\u00b7ner", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Himmels Wohnungen, o ihr,", "tokens": ["Des", "Him\u00b7mels", "Woh\u00b7nun\u00b7gen", ",", "o", "ihr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "FM", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Mein ewges Vaterland", "tokens": ["Mein", "ew\u00b7ges", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein matter Fremdling auf der Welt,", "tokens": ["Ein", "mat\u00b7ter", "Fremd\u00b7ling", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Streck' ich nach Euch die Hand.", "tokens": ["Streck'", "ich", "nach", "Euch", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer leiht mir Fl\u00fcgel? ach wer gibt", "tokens": ["Wer", "leiht", "mir", "Fl\u00fc\u00b7gel", "?", "ach", "wer", "gibt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$.", "XY", "PWS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu schwingen mich von hier,", "tokens": ["Zu", "schwin\u00b7gen", "mich", "von", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem kranken Geiste neuen Muth,", "tokens": ["Dem", "kran\u00b7ken", "Geis\u00b7te", "neu\u00b7en", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und neue Kr\u00e4fte mir?", "tokens": ["Und", "neu\u00b7e", "Kr\u00e4f\u00b7te", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohlan, kein Erdgedanke mehr", "tokens": ["Wo\u00b7hlan", ",", "kein", "Erd\u00b7ge\u00b7dan\u00b7ke", "mehr"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Keim' auf in dir, o Herz!", "tokens": ["Keim'", "auf", "in", "dir", ",", "o", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "APPR", "APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zeit ists, aufs Veste nun zu schaun,", "tokens": ["Zeit", "ists", ",", "aufs", "Ves\u00b7te", "nun", "zu", "schaun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Zu denken Himmelw\u00e4rts.", "tokens": ["Zu", "den\u00b7ken", "Him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}