{"dta.poem.2461": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Es beklaget sich einsmals einer/ da\u00df  \n ihm so grosser Schimpff und Wiederwill von  \n einem vornehmen Mann wiederf\u00fchre/ da  \n ward ihm zur Antwort:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn nur die Tugend gl\u00e4ntzt und ihre Strahlen fun-", "tokens": ["Wenn", "nur", "die", "Tu\u00b7gend", "gl\u00e4ntzt", "und", "ih\u00b7re", "Strah\u00b7len", "fun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "keln/", "tokens": ["keln", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "So mu\u00df durch ihren Schein/ Gold/ Gut und Geld ver-", "tokens": ["So", "mu\u00df", "durch", "ih\u00b7ren", "Schein", "/", "Gold", "/", "Gut", "und", "Geld", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN", "$(", "NN", "$(", "ADJD", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "dunkeln.", "tokens": ["dun\u00b7keln", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}