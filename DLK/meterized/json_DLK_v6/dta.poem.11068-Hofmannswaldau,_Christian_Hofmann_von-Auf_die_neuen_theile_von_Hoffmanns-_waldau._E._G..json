{"dta.poem.11068": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die neuen theile von Hoffmanns-  \n waldau.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die leute k\u00fcmmern sich,", "tokens": ["Die", "leu\u00b7te", "k\u00fcm\u00b7mern", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was doch bey neuer tichter sachen", "tokens": ["Was", "doch", "bey", "neu\u00b7er", "tich\u00b7ter", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Was gilts: Mein\u2019 antwort h\u00e4lt den stich:", "tokens": ["Was", "gilts", ":", "Mein'", "ant\u00b7wort", "h\u00e4lt", "den", "stich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er zeigt, da\u00df seines geistes gaben", "tokens": ["Er", "zeigt", ",", "da\u00df", "sei\u00b7nes", "geis\u00b7tes", "ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht g\u00e4ntzlich ausgestorben seyn.", "tokens": ["Nicht", "g\u00e4ntz\u00b7lich", "aus\u00b7ge\u00b7stor\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und trifft auch dieser schlu\u00df nicht richtig ein;", "tokens": ["Und", "trifft", "auch", "die\u00b7ser", "schlu\u00df", "nicht", "rich\u00b7tig", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "NN", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So h\u00f6rt, ihr solt was anders haben:", "tokens": ["So", "h\u00f6rt", ",", "ihr", "solt", "was", "an\u00b7ders", "ha\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VMFIN", "PIS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "\u201cwenn Musen auf den berg der anmuth spielen", "tokens": ["\u201c", "wenn", "Mu\u00b7sen", "auf", "den", "berg", "der", "an\u00b7muth", "spie\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "NN", "APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "gehn;", "tokens": ["gehn", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+", "measure": "single.up"}, "line.11": {"text": "\u201eso mu\u00df ", "tokens": ["\u201e", "so", "mu\u00df"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ADV", "VMFIN"], "meter": "-+", "measure": "iambic.single"}}}}}