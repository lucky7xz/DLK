{"textgrid.poem.52275": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Abschiedswort", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein offner Hieb in offner Schlacht \u2013", "tokens": ["Kein", "off\u00b7ner", "Hieb", "in", "off\u00b7ner", "Schlacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es f\u00e4llen die N\u00fccken und T\u00fccken,", "tokens": ["Es", "f\u00e4l\u00b7len", "die", "N\u00fc\u00b7cken", "und", "T\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es f\u00e4llt mich die schleichende Niedetracht", "tokens": ["Es", "f\u00e4llt", "mich", "die", "schlei\u00b7chen\u00b7de", "Nie\u00b7de\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der schmutzigen Westkalm\u00fccken!", "tokens": ["Der", "schmut\u00b7zi\u00b7gen", "West\u00b7kal\u00b7m\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Aus dem Dunkel flog der t\u00f6tende Schaft,", "tokens": ["Aus", "dem", "Dun\u00b7kel", "flog", "der", "t\u00f6\u00b7ten\u00b7de", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Aus dem Hinterhalt fielen die Streiche \u2013", "tokens": ["Aus", "dem", "Hin\u00b7ter\u00b7halt", "fie\u00b7len", "die", "Strei\u00b7che", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Und so lieg' ich nun da in meiner Kraft,", "tokens": ["Und", "so", "lieg'", "ich", "nun", "da", "in", "mei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Eine stolze Rebelleinleiche!", "tokens": ["Ei\u00b7ne", "stol\u00b7ze", "Re\u00b7bel\u00b7lein\u00b7lei\u00b7che", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf der Lippe den Trotz und den zuckenden Hohn,", "tokens": ["Auf", "der", "Lip\u00b7pe", "den", "Trotz", "und", "den", "zu\u00b7cken\u00b7den", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "In der Hand den blitzenden Degen,", "tokens": ["In", "der", "Hand", "den", "blit\u00b7zen\u00b7den", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch im Sterben rufend: \u00bbDie Rebellion!\u00ab \u2013", "tokens": ["Noch", "im", "Ster\u00b7ben", "ru\u00b7fend", ":", "\u00bb", "Die", "Re\u00b7bel\u00b7lion", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$.", "$(", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So bin ich mit Ehren erlegen.", "tokens": ["So", "bin", "ich", "mit", "Eh\u00b7ren", "er\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "O, gern wohl bestreuten mein Grab mit Salz", "tokens": ["O", ",", "gern", "wohl", "be\u00b7streu\u00b7ten", "mein", "Grab", "mit", "Salz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Der Preu\u00dfe zusamt der Zare \u2013", "tokens": ["Der", "Preu\u00b7\u00dfe", "zu\u00b7samt", "der", "Za\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch es schicken die Ungarn, es schickt die Pfalz", "tokens": ["Doch", "es", "schi\u00b7cken", "die", "Un\u00b7garn", ",", "es", "schickt", "die", "Pfalz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NE"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Drei Salven mir \u00fcber die Bahre!", "tokens": ["Drei", "Sal\u00b7ven", "mir", "\u00fc\u00b7ber", "die", "Bah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und der arme Mann im zerri\u00dfnen Gewand,", "tokens": ["Und", "der", "ar\u00b7me", "Mann", "im", "zer\u00b7ri\u00df\u00b7nen", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er wirft auf mein Haupt die Schollen!", "tokens": ["Er", "wirft", "auf", "mein", "Haupt", "die", "Schol\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er wirft sie hinab mit der flei\u00dfigen Hand,", "tokens": ["Er", "wirft", "sie", "hin\u00b7ab", "mit", "der", "flei\u00b7\u00dfi\u00b7gen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit der harten, der schwielenvollen.", "tokens": ["Mit", "der", "har\u00b7ten", ",", "der", "schwie\u00b7len\u00b7vol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "PRELS", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Einen Kranz bringt er aus Blumen und Main,", "tokens": ["Ei\u00b7nen", "Kranz", "bringt", "er", "aus", "Blu\u00b7men", "und", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Zu ruhn auf meinen Wunden;", "tokens": ["Zu", "ruhn", "auf", "mei\u00b7nen", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den haben sein Weib und sein T\u00f6chterlein", "tokens": ["Den", "ha\u00b7ben", "sein", "Weib", "und", "sein", "T\u00f6ch\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Nach der Arbeit f\u00fcr mich gewunden.", "tokens": ["Nach", "der", "Ar\u00b7beit", "f\u00fcr", "mich", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun ade, ihr ringenden Heere!", "tokens": ["Nun", "a\u00b7de", ",", "ihr", "rin\u00b7gen\u00b7den", "Hee\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nun ade, du pulvergeschw\u00e4rztes feld!", "tokens": ["Nun", "a\u00b7de", ",", "du", "pul\u00b7ver\u00b7ge\u00b7schw\u00e4rz\u00b7tes", "feld", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Nun ade, ihr Schwerter und Speere!", "tokens": ["Nun", "a\u00b7de", ",", "ihr", "Schwer\u00b7ter", "und", "Spee\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun ade \u2013 doch nicht f\u00fcr immer ade!", "tokens": ["Nun", "a\u00b7de", "\u2013", "doch", "nicht", "f\u00fcr", "im\u00b7mer", "a\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADV", "PTKNEG", "APPR", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denn sie t\u00f6ten den Geist nicht, ihr Br\u00fcder!", "tokens": ["Denn", "sie", "t\u00f6\u00b7ten", "den", "Geist", "nicht", ",", "ihr", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Bald richt' ich mich rasselnd in die H\u00f6h',", "tokens": ["Bald", "richt'", "ich", "mich", "ras\u00b7selnd", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bald kehr' ich reisiger wieder!", "tokens": ["Bald", "kehr'", "ich", "rei\u00b7si\u00b7ger", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wenn die letzte Krone wie Glas zerbricht,", "tokens": ["Wenn", "die", "letz\u00b7te", "Kro\u00b7ne", "wie", "Glas", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KOKOM", "NN", "VVPP", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In des Kampfes Wettern und Flammen,", "tokens": ["In", "des", "Kamp\u00b7fes", "Wet\u00b7tern", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn das Volk sein letztes \u00bbSchuldig!\u00ab spricht,", "tokens": ["Wenn", "das", "Volk", "sein", "letz\u00b7tes", "\u00bb", "Schul\u00b7dig", "!", "\u00ab", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "ADJA", "$(", "ADJD", "$.", "$(", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dann stehn wir wieder zusammen!", "tokens": ["Dann", "stehn", "wir", "wie\u00b7der", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mit dem Wort, mit dem Schwert, an der Donau, am Rhein", "tokens": ["Mit", "dem", "Wort", ",", "mit", "dem", "Schwert", ",", "an", "der", "Do\u00b7nau", ",", "am", "Rhein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "ART", "NE", "$,", "APPRART", "NE"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Eine allzeit treue Gesellin", "tokens": ["Ei\u00b7ne", "all\u00b7zeit", "treu\u00b7e", "Ge\u00b7sel\u00b7lin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wird dem Throne zerschmetternden Volke sein", "tokens": ["Wird", "dem", "Thro\u00b7ne", "zer\u00b7schmet\u00b7tern\u00b7den", "Vol\u00b7ke", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "PPOSAT"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Die Ge\u00e4chtete, die Rebellin!", "tokens": ["Die", "Ge\u00e4ch\u00b7te\u00b7te", ",", "die", "Re\u00b7bel\u00b7lin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}