{"textgrid.poem.64123": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der H\u00fcgel birgt den K\u00f6nig Stein:", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der H\u00fcgel birgt den K\u00f6nig Stein:", "tokens": ["Der", "H\u00fc\u00b7gel", "birgt", "den", "K\u00f6\u00b7nig", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vier S\u00f6hne sind die Erben;", "tokens": ["Vier", "S\u00f6h\u00b7ne", "sind", "die", "Er\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In der Halle sitzen sie nun allein:", "tokens": ["In", "der", "Hal\u00b7le", "sit\u00b7zen", "sie", "nun", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um das Erbe die Erben werben.", "tokens": ["Um", "das", "Er\u00b7be", "die", "Er\u00b7ben", "wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Der blonde Halfdan streicht den Bart", "tokens": ["Der", "blon\u00b7de", "Half\u00b7dan", "streicht", "den", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und spiegelt sich im Schilde;", "tokens": ["Und", "spie\u00b7gelt", "sich", "im", "Schil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der schwarze Helgi, von d\u00fcstrer Art,", "tokens": ["Der", "schwar\u00b7ze", "Hel\u00b7gi", ",", "von", "d\u00fcst\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sinnt stolze Taten und wilde;", "tokens": ["Sinnt", "stol\u00b7ze", "Ta\u00b7ten", "und", "wil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Der rote Hako erw\u00e4gt, wie den Wert", "tokens": ["Der", "ro\u00b7te", "Ha\u00b7ko", "er\u00b7w\u00e4gt", ",", "wie", "den", "Wert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Von des Reiches Hort zu verwenden;", "tokens": ["Von", "des", "Rei\u00b7ches", "Hort", "zu", "ver\u00b7wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Der J\u00fcngste h\u00e4lt des Vaters Schwert", "tokens": ["Der", "J\u00fcngs\u00b7te", "h\u00e4lt", "des", "Va\u00b7ters", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "In tr\u00e4nenbetr\u00e4uften H\u00e4nden. \u2013", "tokens": ["In", "tr\u00e4\u00b7nen\u00b7be\u00b7tr\u00e4uf\u00b7ten", "H\u00e4n\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf sprang von selbst da die eichene T\u00fcr:", "tokens": ["Auf", "sprang", "von", "selbst", "da", "die", "ei\u00b7che\u00b7ne", "T\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Nicht wagten die R\u00fcden Gebelle,", "tokens": ["Nicht", "wag\u00b7ten", "die", "R\u00fc\u00b7den", "Ge\u00b7bel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und vor den Br\u00fcdern stand Wegaf\u00fcr,", "tokens": ["Und", "vor", "den", "Br\u00fc\u00b7dern", "stand", "We\u00b7ga\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Vaters vertrauter Geselle.", "tokens": ["Des", "Va\u00b7ters", "ver\u00b7trau\u00b7ter", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Alte im Mantel und Wandrerhut,", "tokens": ["Der", "Al\u00b7te", "im", "Man\u00b7tel", "und", "Wand\u00b7rer\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Er sprach: \u00bbNun h\u00f6ret, ihr F\u00fcrsten:", "tokens": ["Er", "sprach", ":", "\u00bb", "Nun", "h\u00f6\u00b7ret", ",", "ihr", "F\u00fcrs\u00b7ten", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nicht soll eurer k\u00fchnsten W\u00fcnsche Mut", "tokens": ["Nicht", "soll", "eu\u00b7rer", "k\u00fchns\u00b7ten", "W\u00fcn\u00b7sche", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Umsonst nach Erf\u00fcllung d\u00fcrsten.", "tokens": ["Um\u00b7sonst", "nach", "Er\u00b7f\u00fcl\u00b7lung", "d\u00fcrs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VMFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ihr wi\u00dft es: mancher Zauber ist mein,", "tokens": ["Ihr", "wi\u00dft", "es", ":", "man\u00b7cher", "Zau\u00b7ber", "ist", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PIAT", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Ich war des K\u00f6nigs Berater:", "tokens": ["Ich", "war", "des", "K\u00f6\u00b7nigs", "Be\u00b7ra\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Euch sollen vier W\u00fcnsche verstattet sein,", "tokens": ["Euch", "sol\u00b7len", "vier", "W\u00fcn\u00b7sche", "ver\u00b7stat\u00b7tet", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "CARD", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Das versprach ich dem sterbenden Vater.", "tokens": ["Das", "ver\u00b7sprach", "ich", "dem", "ster\u00b7ben\u00b7den", "Va\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Und der weiseste Wunsch, der wird gew\u00e4hrt. \u2013", "tokens": ["Und", "der", "wei\u00b7ses\u00b7te", "Wunsch", ",", "der", "wird", "ge\u00b7w\u00e4hrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "VAFIN", "VVPP", "$.", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Nun w\u00fcnscht nach des Herzens Triebe.\u00ab", "tokens": ["Nun", "w\u00fcnscht", "nach", "des", "Her\u00b7zens", "Trie\u00b7be", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Halfdan rief: \u00bbAuf weiter Erd'", "tokens": ["Und", "Half\u00b7dan", "rief", ":", "\u00bb", "Auf", "wei\u00b7ter", "Erd'"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "APPR", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist das S\u00fc\u00dfeste Weibesliebe!", "tokens": ["Ist", "das", "S\u00fc\u00b7\u00dfes\u00b7te", "Wei\u00b7bes\u00b7lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weichwangiger Weiber wonnige Gunst,", "tokens": ["Weich\u00b7wan\u00b7gi\u00b7ger", "Wei\u00b7ber", "won\u00b7ni\u00b7ge", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die sollst du mir, Alter, gew\u00e4hren!\u00ab", "tokens": ["Die", "sollst", "du", "mir", ",", "Al\u00b7ter", ",", "ge\u00b7w\u00e4h\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPER", "$,", "NN", "$,", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbdie Lieb' ist Wahn und Weh und Brunst,\u00ab", "tokens": ["\u00bb", "die", "Lieb'", "ist", "Wahn", "und", "Weh", "und", "Brunst", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "NN", "KON", "NN", "KON", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach Helgi, \u00bbmich d\u00fcrstet nach Ehren!", "tokens": ["Sprach", "Hel\u00b7gi", ",", "\u00bb", "mich", "d\u00fcrs\u00b7tet", "nach", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "$(", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Gib mir vor allen K\u00f6nigen Ruhm.\u00ab", "tokens": ["Gib", "mir", "vor", "al\u00b7len", "K\u00f6\u00b7ni\u00b7gen", "Ruhm", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Doch Hako h\u00f6hnte, der rote:", "tokens": ["Doch", "Ha\u00b7ko", "h\u00f6hn\u00b7te", ",", "der", "ro\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbruhm ist gar windiges Eigentum!", "tokens": ["\u00bb", "ruhm", "ist", "gar", "win\u00b7di\u00b7ges", "Ei\u00b7gen\u00b7tum", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mir spende, du Wunschesbote,", "tokens": ["Mir", "spen\u00b7de", ",", "du", "Wun\u00b7sches\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des roten Goldes unendlichen Hort!\u00ab \u2013", "tokens": ["Des", "ro\u00b7ten", "Gol\u00b7des", "un\u00b7end\u00b7li\u00b7chen", "Hort", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Da sprach der Alte mit Sinnen:", "tokens": ["Da", "sprach", "der", "Al\u00b7te", "mit", "Sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "\u00bbnun, Harald, Braunkopf, du findest kein Wort?", "tokens": ["\u00bb", "nun", ",", "Ha\u00b7rald", ",", "Braun\u00b7kopf", ",", "du", "fin\u00b7dest", "kein", "Wort", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NE", "$,", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie? \u2013 Tr\u00e4nen seh' ich dir rinnen?\u00ab", "tokens": ["Wie", "?", "\u2013", "Tr\u00e4\u00b7nen", "seh'", "ich", "dir", "rin\u00b7nen", "?", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "$.", "$(", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbich w\u00fcnsche nur meines Vaters Schwert,", "tokens": ["\u00bb", "ich", "w\u00fcn\u00b7sche", "nur", "mei\u00b7nes", "Va\u00b7ters", "Schwert", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das hier in H\u00e4nden ich halte.\u00ab", "tokens": ["Das", "hier", "in", "H\u00e4n\u00b7den", "ich", "hal\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "APPR", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbdu wirst es f\u00fchren des Vaters wert!", "tokens": ["\u00bb", "du", "wirst", "es", "f\u00fch\u00b7ren", "des", "Va\u00b7ters", "wert", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nichts weiter?\u00ab forschte der Alte.", "tokens": ["Und", "nichts", "wei\u00b7ter", "?", "\u00ab", "forschte", "der", "Al\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "$.", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbnichts! Ich hoffe nur, da\u00df zuweilen ", "tokens": ["\u00bb", "nichts", "!", "Ich", "hof\u00b7fe", "nur", ",", "da\u00df", "zu\u00b7wei\u00b7len"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PIS", "$.", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In meiner Halle dich zeigest,", "tokens": ["In", "mei\u00b7ner", "Hal\u00b7le", "dich", "zei\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Schweigen der Nacht, in des Abends Ruh'", "tokens": ["Im", "Schwei\u00b7gen", "der", "Nacht", ",", "in", "des", "A\u00b7bends", "Ruh'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Antlitz zu mir neigest.", "tokens": ["Das", "Ant\u00b7litz", "zu", "mir", "nei\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn Unausdenkliches liegt geh\u00e4uft", "tokens": ["Denn", "Un\u00b7aus\u00b7denk\u00b7li\u00b7ches", "liegt", "ge\u00b7h\u00e4uft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auf deiner Stirne, der hohen,", "tokens": ["Auf", "dei\u00b7ner", "Stir\u00b7ne", ",", "der", "ho\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und vom Mund dir ersch\u00fctternde Weisheit tr\u00e4uft", "tokens": ["Und", "vom", "Mund", "dir", "er\u00b7sch\u00fct\u00b7tern\u00b7de", "Weis\u00b7heit", "tr\u00e4uft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPER", "ADJA", "NN", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Bei des grauen Auges Lohen.", "tokens": ["Bei", "des", "grau\u00b7en", "Au\u00b7ges", "Lo\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dir will ich mich weihn mit des Vaters Schwert!", "tokens": ["Dir", "will", "ich", "mich", "weihn", "mit", "des", "Va\u00b7ters", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Nichts andres heisch' ich auf Erden!\u00ab", "tokens": ["Nichts", "and\u00b7res", "heisch'", "ich", "auf", "Er\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "PIS", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbheil dir, jung Harald! Dir ist gew\u00e4hrt,", "tokens": ["\u00bb", "heil", "dir", ",", "jung", "Ha\u00b7rald", "!", "Dir", "ist", "ge\u00b7w\u00e4hrt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$,", "ADJD", "NE", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und das Herrlichste soll dir werden!", "tokens": ["Und", "das", "Herr\u00b7lichs\u00b7te", "soll", "dir", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein erprobtes Schwert in treuer Hand, \u2013", "tokens": ["Ein", "er\u00b7prob\u00b7tes", "Schwert", "in", "treu\u00b7er", "Hand", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nach dem H\u00f6chsten ein ahnendes Sehnen, \u2013", "tokens": ["Nach", "dem", "H\u00f6chs\u00b7ten", "ein", "ah\u00b7nen\u00b7des", "Seh\u00b7nen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Ein Geist, zu Adlerfluge gespannt,", "tokens": ["Ein", "Geist", ",", "zu", "Ad\u00b7ler\u00b7flu\u00b7ge", "ge\u00b7spannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und im Auge kindliche Tr\u00e4nen: \u2013 \u2013", "tokens": ["Und", "im", "Au\u00b7ge", "kind\u00b7li\u00b7che", "Tr\u00e4\u00b7nen", ":", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du sollst gewinnen des Weibes Ku\u00df", "tokens": ["Du", "sollst", "ge\u00b7win\u00b7nen", "des", "Wei\u00b7bes", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und des Ruhmes Harfenschallen", "tokens": ["Und", "des", "Ruh\u00b7mes", "Har\u00b7fen\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und des glei\u00dfenden Goldes \u00dcberflu\u00df", "tokens": ["Und", "des", "glei\u00b7\u00dfen\u00b7den", "Gol\u00b7des", "\u00dc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und mich, jung Harald, vor allen.", "tokens": ["Und", "mich", ",", "jung", "Ha\u00b7rald", ",", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "NE", "$,", "APPR", "PIAT", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.11": {"text": "Ich, Odhin von Asgardh, k\u00fcsse dich jetzt,", "tokens": ["Ich", ",", "Od\u00b7hin", "von", "As\u00b7gardh", ",", "k\u00fcs\u00b7se", "dich", "jetzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "APPR", "NE", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Zum Wunschsohn dich mir zu k\u00fcren,", "tokens": ["Zum", "Wunschsohn", "dich", "mir", "zu", "k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und nach tausend Siegen sollen zuletzt", "tokens": ["Und", "nach", "tau\u00b7send", "Sie\u00b7gen", "sol\u00b7len", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "CARD", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Die Walk\u00fcren zu mir dich f\u00fchren! \u2013 \u2013\u00ab", "tokens": ["Die", "Wal\u00b7k\u00fc\u00b7ren", "zu", "mir", "dich", "f\u00fch\u00b7ren", "!", "\u2013", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PRF", "VVINF", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}