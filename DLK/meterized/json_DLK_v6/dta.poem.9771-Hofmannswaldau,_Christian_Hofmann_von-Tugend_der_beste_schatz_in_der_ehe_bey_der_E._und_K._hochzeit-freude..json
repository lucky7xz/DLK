{"dta.poem.9771": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tugend der beste schatz in der ehe/ bey der E.  \n und K. hochzeit-freude.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der menschen wachsamkeit/ das nimmer m\u00fcde sorgen/", "tokens": ["Der", "men\u00b7schen", "wach\u00b7sam\u00b7keit", "/", "das", "nim\u00b7mer", "m\u00fc\u00b7de", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PDS", "ADV", "ADJD", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die arbeit sonder ziel/ die ruhe sonder ruh/", "tokens": ["Die", "ar\u00b7beit", "son\u00b7der", "ziel", "/", "die", "ru\u00b7he", "son\u00b7der", "ruh", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$(", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bi\u00df zum abend w\u00e4chst und steigt vom hellen morgen/", "tokens": ["So", "bi\u00df", "zum", "a\u00b7bend", "w\u00e4chst", "und", "steigt", "vom", "hel\u00b7len", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADV", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df alles sucht gewinn und rennet sch\u00e4tzen zu.", "tokens": ["Di\u00df", "al\u00b7les", "sucht", "ge\u00b7winn", "und", "ren\u00b7net", "sch\u00e4t\u00b7zen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "NN", "KON", "VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der bergmann gr\u00e4bet sich lebendig in die gr\u00fcffte/", "tokens": ["Der", "berg\u00b7mann", "gr\u00e4\u00b7bet", "sich", "le\u00b7ben\u00b7dig", "in", "die", "gr\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Warum? der todte schatz des silbers locket ihn.", "tokens": ["Wa\u00b7rum", "?", "der", "tod\u00b7te", "schatz", "des", "sil\u00b7bers", "lo\u00b7cket", "ihn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was scheuet ein soldat die donner-schwangern l\u00fcffte/", "tokens": ["Was", "scheu\u00b7et", "ein", "sol\u00b7dat", "die", "don\u00b7ner\u00b7schwan\u00b7gern", "l\u00fcff\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann er an sch\u00e4tzen reich kan von der wahlstatt ziehn?", "tokens": ["Wann", "er", "an", "sch\u00e4t\u00b7zen", "reich", "kan", "von", "der", "wahl\u00b7statt", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADJD", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein kauffmann f\u00e4hrt mit lust von westen bi\u00df zu osten/", "tokens": ["Ein", "kauf\u00b7fmann", "f\u00e4hrt", "mit", "lust", "von", "wes\u00b7ten", "bi\u00df", "zu", "os\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN", "APPR", "ADJA", "KON", "PTKZU", "VVINF", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Zu sammlen geld auff geld/ zu h\u00e4uffen gold auff gold;", "tokens": ["Zu", "samm\u00b7len", "geld", "auff", "geld", "/", "zu", "h\u00e4uf\u00b7fen", "gold", "auff", "gold", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "APPR", "NN", "$(", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er l\u00e4st sichs flei\u00df und schwei\u00df und saure m\u00fche kosten/", "tokens": ["Er", "l\u00e4st", "sichs", "flei\u00df", "und", "schwei\u00df", "und", "sau\u00b7re", "m\u00fc\u00b7he", "kos\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVFIN", "KON", "ADJD", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bi\u00df ihm das gl\u00fcck gelacht/ das schicksal wohlgewolt.", "tokens": ["Bi\u00df", "ihm", "das", "gl\u00fcck", "ge\u00b7lacht", "/", "das", "schick\u00b7sal", "wohl\u00b7ge\u00b7wolt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$(", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie aber sucht nicht auch die liebe theure g\u00fcter/", "tokens": ["Wie", "a\u00b7ber", "sucht", "nicht", "auch", "die", "lie\u00b7be", "theu\u00b7re", "g\u00fc\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und pflegt sie sich nicht auch nach sch\u00e4tzen umzusehn?", "tokens": ["Und", "pflegt", "sie", "sich", "nicht", "auch", "nach", "sch\u00e4t\u00b7zen", "um\u00b7zu\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "APPR", "VVINF", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nachdem die regung treibt der liebenden gem\u00fcther/", "tokens": ["Nach\u00b7dem", "die", "re\u00b7gung", "treibt", "der", "lie\u00b7ben\u00b7den", "ge\u00b7m\u00fc\u00b7ther", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wird bald auff di\u00df/ bald das derselben wahl geschehn.", "tokens": ["Wird", "bald", "auff", "di\u00df", "/", "bald", "das", "der\u00b7sel\u00b7ben", "wahl", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDS", "$(", "ADV", "ART", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die sch\u00f6nheit zeigt sich erst in kostbarem gepr\u00e4ge;", "tokens": ["Die", "sch\u00f6n\u00b7heit", "zeigt", "sich", "erst", "in", "kost\u00b7ba\u00b7rem", "ge\u00b7pr\u00e4\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die m\u00fcntze solcher art gilt durch die gantze welt.", "tokens": ["Die", "m\u00fcnt\u00b7ze", "sol\u00b7cher", "art", "gilt", "durch", "die", "gant\u00b7ze", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das hertze wird erhitzt/ die sinnen werden rege/", "tokens": ["Das", "hert\u00b7ze", "wird", "er\u00b7hitzt", "/", "die", "sin\u00b7nen", "wer\u00b7den", "re\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ART", "VVINF", "VAFIN", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So bald dergleichen st\u00fcck uns in die augen f\u00e4llt.", "tokens": ["So", "bald", "derg\u00b7lei\u00b7chen", "st\u00fcck", "uns", "in", "die", "au\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jedoch/ wie manchem bleibt nur kupffer in den h\u00e4nden/", "tokens": ["Je\u00b7doch", "/", "wie", "man\u00b7chem", "bleibt", "nur", "kupf\u00b7fer", "in", "den", "h\u00e4n\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOKOM", "PIAT", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er wehlet ertz vor gold/ vor silber wasserbley;", "tokens": ["Er", "weh\u00b7let", "ertz", "vor", "gold", "/", "vor", "sil\u00b7ber", "was\u00b7ser\u00b7bley", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$(", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wird er die m\u00fcntze nur auff jene seite wenden/", "tokens": ["Wird", "er", "die", "m\u00fcnt\u00b7ze", "nur", "auff", "je\u00b7ne", "sei\u00b7te", "wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ADV", "APPR", "PDAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So sieht er/ da\u00df sie nicht vom besten schlage sey.", "tokens": ["So", "sieht", "er", "/", "da\u00df", "sie", "nicht", "vom", "bes\u00b7ten", "schla\u00b7ge", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das bild der phantasey/ der menschen grosser g\u00f6tze/", "tokens": ["Das", "bild", "der", "phan\u00b7ta\u00b7sey", "/", "der", "men\u00b7schen", "gros\u00b7ser", "g\u00f6t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die ehre/ machet auch allhier die l\u00e4den auff/", "tokens": ["Die", "eh\u00b7re", "/", "ma\u00b7chet", "auch", "all\u00b7hier", "die", "l\u00e4\u00b7den", "auff", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "VVFIN", "ADV", "ADV", "ART", "ADJA", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und da\u00df ich deutlicher die rechte meynung setze:", "tokens": ["Und", "da\u00df", "ich", "deut\u00b7li\u00b7cher", "die", "rech\u00b7te", "mey\u00b7nung", "set\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Man schliest offt mit dem amt/ nicht mit der braut den kauff.", "tokens": ["Man", "schliest", "offt", "mit", "dem", "amt", "/", "nicht", "mit", "der", "braut", "den", "kauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$(", "PTKNEG", "APPR", "ART", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wiewohl die reue pflegt sich offt mit einzudingen/", "tokens": ["Wie\u00b7wohl", "die", "reu\u00b7e", "pflegt", "sich", "offt", "mit", "ein\u00b7zu\u00b7din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "VVFIN", "PRF", "ADV", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wann zwar des Jovis stand/ doch Juno nicht gef\u00e4llt.", "tokens": ["Wann", "zwar", "des", "Jo\u00b7vis", "stand", "/", "doch", "Ju\u00b7no", "nicht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NE", "VVFIN", "$(", "ADV", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zu dem: zu ehren soll man nicht durch ehen dringen/", "tokens": ["Zu", "dem", ":", "zu", "eh\u00b7ren", "soll", "man", "nicht", "durch", "e\u00b7hen", "drin\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$.", "PTKZU", "VVINF", "VMFIN", "PIS", "PTKNEG", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Weil diese stuffe doch gar selten feste h\u00e4lt.", "tokens": ["Weil", "die\u00b7se", "stuf\u00b7fe", "doch", "gar", "sel\u00b7ten", "fes\u00b7te", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "ADV", "ADV", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und endlich findet man/ die sich dem mammon weihen/", "tokens": ["Und", "end\u00b7lich", "fin\u00b7det", "man", "/", "die", "sich", "dem", "mam\u00b7mon", "wei\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$(", "PRELS", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In meynung; bey metall da wache sanffte ruh;", "tokens": ["In", "mey\u00b7nung", ";", "bey", "me\u00b7tall", "da", "wa\u00b7che", "sanff\u00b7te", "ruh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NE", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die mit den h\u00e4nden mehr/ als mit den augen freyen/", "tokens": ["Die", "mit", "den", "h\u00e4n\u00b7den", "mehr", "/", "als", "mit", "den", "au\u00b7gen", "frey\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "ADV", "$(", "KOKOM", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "O blinde! dr\u00fcckt doch nicht die augen beyde zu!", "tokens": ["O", "blin\u00b7de", "!", "dr\u00fcckt", "doch", "nicht", "die", "au\u00b7gen", "bey\u00b7de", "zu", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jhr fesselt hertz und sinn/ wer kan euch also retten?", "tokens": ["Ihr", "fes\u00b7selt", "hertz", "und", "sinn", "/", "wer", "kan", "euch", "al\u00b7so", "ret\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$(", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wer will ein sclave doch des schn\u00f6den geldes seyn?", "tokens": ["Wer", "will", "ein", "scla\u00b7ve", "doch", "des", "schn\u00f6\u00b7den", "gel\u00b7des", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Jhr legt die ducatons an schl\u00f6sser und an ketten/", "tokens": ["Ihr", "legt", "die", "du\u00b7ca\u00b7tons", "an", "schl\u00f6s\u00b7ser", "und", "an", "ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "ADJA", "KON", "APPR", "VVINF", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Und schli