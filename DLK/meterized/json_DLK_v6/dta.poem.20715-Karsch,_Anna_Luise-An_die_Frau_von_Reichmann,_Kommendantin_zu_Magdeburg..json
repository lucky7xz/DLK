{"dta.poem.20715": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  die Frau von Reichmann,  \n Kommendantin zu Magdeburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Winter hauchet Frost an diese d\u00fcnne Wand;                 ", "tokens": ["Der", "Win\u00b7ter", "hau\u00b7chet", "Frost", "an", "die\u00b7se", "d\u00fcn\u00b7ne", "Wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich aber troz ihm in dem Bette.", "tokens": ["Ich", "a\u00b7ber", "troz", "ihm", "in", "dem", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier sitz ich, und hier schreibt die kaltgewordne Hand", "tokens": ["Hier", "sitz", "ich", ",", "und", "hier", "schreibt", "die", "kalt\u00b7ge\u00b7word\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "$,", "KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An Dich, und wenn der Nord durch meine Fenster", "tokens": ["An", "Dich", ",", "und", "wenn", "der", "Nord", "durch", "mei\u00b7ne", "Fens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "redte", "tokens": ["red\u00b7te"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Gewaltiger als sonst, wenn dieser Finger krumm", "tokens": ["Ge\u00b7wal\u00b7ti\u00b7ger", "als", "sonst", ",", "wenn", "die\u00b7ser", "Fin\u00b7ger", "krumm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADV", "$,", "KOUS", "PDAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von Frost geworden w\u00e4r, so w\u00fcrd ich doch nicht stumm,", "tokens": ["Von", "Frost", "ge\u00b7wor\u00b7den", "w\u00e4r", ",", "so", "w\u00fcrd", "ich", "doch", "nicht", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAPP", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich s\u00e4nge Dir ein Lied, Dir! der ich alle Tage", "tokens": ["Ich", "s\u00e4n\u00b7ge", "Dir", "ein", "Lied", ",", "Dir", "!", "der", "ich", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "$.", "PRELS", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des Herzens ersten Gru\u00df in einem Liede sage.", "tokens": ["Des", "Her\u00b7zens", "ers\u00b7ten", "Gru\u00df", "in", "ei\u00b7nem", "Lie\u00b7de", "sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Viel K\u00e4lte stand ich aus, als Armuth mich gejagt", "tokens": ["Viel", "K\u00e4l\u00b7te", "stand", "ich", "aus", ",", "als", "Ar\u00b7muth", "mich", "ge\u00b7jagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Fr\u00fch Morgens aus dem Bett, sobald es nur getagt,", "tokens": ["Fr\u00fch", "Mor\u00b7gens", "aus", "dem", "Bett", ",", "so\u00b7bald", "es", "nur", "ge\u00b7tagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie \u00e4ngstlich lief ich da nach Holze,", "tokens": ["Wie", "\u00e4ngst\u00b7lich", "lief", "ich", "da", "nach", "Hol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Bei mir vor\u00fcber ging das stolze", "tokens": ["Bei", "mir", "vor\u00b7\u00fc\u00b7ber", "ging", "das", "stol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und reiche B\u00fcrger-Volk, nicht vornehm, aber doch", "tokens": ["Und", "rei\u00b7che", "B\u00fcr\u00b7ger\u00b7Volk", ",", "nicht", "vor\u00b7nehm", ",", "a\u00b7ber", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "PTKNEG", "ADJD", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sehr aufgebl\u00e4ht durch kluges Wissen", "tokens": ["Sehr", "auf\u00b7ge\u00b7bl\u00e4ht", "durch", "klu\u00b7ges", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Des Geldbesitzens, das sie noch", "tokens": ["Des", "Geld\u00b7be\u00b7sit\u00b7zens", ",", "das", "sie", "noch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Vielleicht, wie Rauch, verlieren m\u00fcssen.", "tokens": ["Viel\u00b7leicht", ",", "wie", "Rauch", ",", "ver\u00b7lie\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "$,", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Du h\u00e4ttest laut geweint, von Mitleid hingerissen,", "tokens": ["Du", "h\u00e4t\u00b7test", "laut", "ge\u00b7weint", ",", "von", "Mit\u00b7leid", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn ich bei Dir vorbeigegangen w\u00e4r!", "tokens": ["Wenn", "ich", "bei", "Dir", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Der arme K\u00f6rper war inwendig kalt und leer,", "tokens": ["Der", "ar\u00b7me", "K\u00f6r\u00b7per", "war", "in\u00b7wen\u00b7dig", "kalt", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Von au\u00dfen war er schlecht behangen;", "tokens": ["Von", "au\u00b7\u00dfen", "war", "er", "schlecht", "be\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ein B\u00fcndel Holz trug unter jedem Arm,", "tokens": ["Ein", "B\u00fcn\u00b7del", "Holz", "trug", "un\u00b7ter", "je\u00b7dem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Das Weib, nach welcher izt so warm,", "tokens": ["Das", "Weib", ",", "nach", "wel\u00b7cher", "izt", "so", "warm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So eifrig Alt und Jung verlangen.", "tokens": ["So", "eif\u00b7rig", "Alt", "und", "Jung", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ich schleppte m\u00fchsam mich und brach das Reisicht klein,", "tokens": ["Ich", "schlepp\u00b7te", "m\u00fch\u00b7sam", "mich", "und", "brach", "das", "Rei\u00b7sicht", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sa\u00df vor dem Ofen hin, und heitzte zitternd ein,", "tokens": ["Sa\u00df", "vor", "dem", "O\u00b7fen", "hin", ",", "und", "heitz\u00b7te", "zit\u00b7ternd", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Kinder vor dem Frost zu sch\u00fctzen;", "tokens": ["Die", "Kin\u00b7der", "vor", "dem", "Frost", "zu", "sch\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "O Dein Gedanke sieht mich bei dem Ofen sitzen,", "tokens": ["O", "Dein", "Ge\u00b7dan\u00b7ke", "sieht", "mich", "bei", "dem", "O\u00b7fen", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein Topf mit Wasser steht bei trockner Fichtengluth;", "tokens": ["Ein", "Topf", "mit", "Was\u00b7ser", "steht", "bei", "trock\u00b7ner", "Fich\u00b7ten\u00b7gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Er kocht, ich heb ihn ab, um Mehl darin zu sch\u00fctten,", "tokens": ["Er", "kocht", ",", "ich", "heb", "ihn", "ab", ",", "um", "Mehl", "da\u00b7rin", "zu", "sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUI", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Suppe, schlecht und ohne Schmalz, war gut.", "tokens": ["Die", "Sup\u00b7pe", ",", "schlecht", "und", "oh\u00b7ne", "Schmalz", ",", "war", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "APPR", "NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Izt d\u00fcrfte keiner mich darauf zu Gaste bitten.", "tokens": ["Izt", "d\u00fcrf\u00b7te", "kei\u00b7ner", "mich", "da\u00b7rauf", "zu", "Gas\u00b7te", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Seit dem ber\u00fchmten Sieg bei Leuthen tadle ich,", "tokens": ["Seit", "dem", "be\u00b7r\u00fchm\u00b7ten", "Sieg", "bei", "Leu\u00b7then", "tad\u00b7le", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den Kaffee, wenn er nicht so kr\u00e4ftig ist, da\u00df mich", "tokens": ["Den", "Kaf\u00b7fee", ",", "wenn", "er", "nicht", "so", "kr\u00e4f\u00b7tig", "ist", ",", "da\u00df", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sein w\u00e4rmend Oehl besch\u00fctzt vor einen b\u00f6sen Husten,", "tokens": ["Sein", "w\u00e4r\u00b7mend", "O\u00b7ehl", "be\u00b7sch\u00fctzt", "vor", "ei\u00b7nen", "b\u00f6\u00b7sen", "Hus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Den alle Suppen mir nicht zu vertreiben wusten;", "tokens": ["Den", "al\u00b7le", "Sup\u00b7pen", "mir", "nicht", "zu", "ver\u00b7trei\u00b7ben", "wus\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jezt eben komm ich, Frau, und wenn ich Dich gek\u00fc\u00dft,", "tokens": ["Jezt", "e\u00b7ben", "komm", "ich", ",", "Frau", ",", "und", "wenn", "ich", "Dich", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So fr\u00e4gt mein erster Blick: ob Kaffee fertig ist!", "tokens": ["So", "fr\u00e4gt", "mein", "ers\u00b7ter", "Blick", ":", "ob", "Kaf\u00b7fee", "fer\u00b7tig", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}