{"dta.poem.13756": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gebet   .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was hast Du mich blank ger\u00fcstet,", "tokens": ["Was", "hast", "Du", "mich", "blank", "ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn mein Volk mich nicht begehrt,", "tokens": ["Wenn", "mein", "Volk", "mich", "nicht", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinem mehr nach Freiheit l\u00fcstet,", "tokens": ["Kei\u00b7nem", "mehr", "nach", "Frei\u00b7heit", "l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df mein Herz, betr\u00fcbt, verw\u00fcstet,", "tokens": ["Da\u00df", "mein", "Herz", ",", "be\u00b7tr\u00fcbt", ",", "ver\u00b7w\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur dem Grabe zugekehrt? \u2014", "tokens": ["Nur", "dem", "Gra\u00b7be", "zu\u00b7ge\u00b7kehrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}