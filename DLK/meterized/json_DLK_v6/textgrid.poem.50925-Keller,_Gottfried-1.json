{"textgrid.poem.50925": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab in kalten Wintertagen,", "tokens": ["Ich", "hab", "in", "kal\u00b7ten", "Win\u00b7ter\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dunkler, hoffnungsarmer Zeit", "tokens": ["In", "dunk\u00b7ler", ",", "hoff\u00b7nungs\u00b7ar\u00b7mer", "Zeit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz aus dem Sinne dich geschlagen,", "tokens": ["Ganz", "aus", "dem", "Sin\u00b7ne", "dich", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Trugbild der Unsterblichkeit.", "tokens": ["O", "Trug\u00b7bild", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Nun, da der Sommer gl\u00fcht und gl\u00e4nzet,", "tokens": ["Nun", ",", "da", "der", "Som\u00b7mer", "gl\u00fcht", "und", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun seh ich, da\u00df ich wohlgetan!", "tokens": ["Nun", "seh", "ich", ",", "da\u00df", "ich", "wohl\u00b7ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufs neu hab ich das Haupt bekr\u00e4nzet,", "tokens": ["Aufs", "neu", "hab", "ich", "das", "Haupt", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VAFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Grabe aber ruht der Wahn.", "tokens": ["Im", "Gra\u00b7be", "a\u00b7ber", "ruht", "der", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich fahre auf dem klaren Strome,", "tokens": ["Ich", "fah\u00b7re", "auf", "dem", "kla\u00b7ren", "Stro\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er rinnt mir k\u00fchlend durch die Hand,", "tokens": ["Er", "rinnt", "mir", "k\u00fch\u00b7lend", "durch", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schau hinauf zum blauen Dome", "tokens": ["Ich", "schau", "hin\u00b7auf", "zum", "blau\u00b7en", "Do\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und such \u2013 kein be\u00dfres Vaterland.", "tokens": ["Und", "such", "\u2013", "kein", "be\u00df\u00b7res", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$(", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun erst versteh ich, die da bl\u00fchet,", "tokens": ["Nun", "erst", "ver\u00b7steh", "ich", ",", "die", "da", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Lilie, deinen stillen Gru\u00df:", "tokens": ["O", "Li\u00b7lie", ",", "dei\u00b7nen", "stil\u00b7len", "Gru\u00df", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, wie sehr das Herz auch gl\u00fchet,", "tokens": ["Ich", "wei\u00df", ",", "wie", "sehr", "das", "Herz", "auch", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich wie du vergehen mu\u00df!", "tokens": ["Da\u00df", "ich", "wie", "du", "ver\u00b7ge\u00b7hen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Seid mir gegr\u00fc\u00dft, ihr holden Rosen,", "tokens": ["Seid", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "hol\u00b7den", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In eures Daseins fl\u00fccht'gem Gl\u00fcck!", "tokens": ["In", "eu\u00b7res", "Da\u00b7seins", "fl\u00fccht'\u00b7gem", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wende mich vom Schrankenlosen", "tokens": ["Ich", "wen\u00b7de", "mich", "vom", "Schran\u00b7ken\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu eurer Anmut froh zur\u00fcck!", "tokens": ["Zu", "eu\u00b7rer", "An\u00b7mut", "froh", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zu gl\u00fchn, zu bl\u00fchn und ganz zu leben,", "tokens": ["Zu", "gl\u00fchn", ",", "zu", "bl\u00fchn", "und", "ganz", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das lehret euer Duft und Schein,", "tokens": ["Das", "leh\u00b7ret", "eu\u00b7er", "Duft", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und willig dann sich hinzugeben", "tokens": ["Und", "wil\u00b7lig", "dann", "sich", "hin\u00b7zu\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "PRF", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem ewigen Nimmerwiedersein!", "tokens": ["Dem", "e\u00b7wi\u00b7gen", "Nim\u00b7mer\u00b7wie\u00b7der\u00b7sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}