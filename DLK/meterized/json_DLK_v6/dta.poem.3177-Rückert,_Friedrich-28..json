{"dta.poem.3177": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Sprichwort auch ist wahr: wer sitzet in dem R\u00f6hricht", "tokens": ["Das", "Sprich\u00b7wort", "auch", "ist", "wahr", ":", "wer", "sit\u00b7zet", "in", "dem", "R\u00f6h\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$.", "PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keine Pfeife da sich schneidet, der ist th\u00f6richt.", "tokens": ["Und", "kei\u00b7ne", "Pfei\u00b7fe", "da", "sich", "schnei\u00b7det", ",", "der", "ist", "th\u00f6\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "PRF", "VVFIN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wer die g\u00fcnstige Gelegenheit verd\u00e4mmert,", "tokens": ["Und", "wer", "die", "g\u00fcns\u00b7ti\u00b7ge", "Ge\u00b7le\u00b7gen\u00b7heit", "ver\u00b7d\u00e4m\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ist es, der das kaltgewordne Eisen h\u00e4mmert.", "tokens": ["Der", "ist", "es", ",", "der", "das", "kalt\u00b7ge\u00b7word\u00b7ne", "Ei\u00b7sen", "h\u00e4m\u00b7mert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS