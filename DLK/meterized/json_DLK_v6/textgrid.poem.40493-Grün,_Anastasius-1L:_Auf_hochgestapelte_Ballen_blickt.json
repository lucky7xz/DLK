{"textgrid.poem.40493": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf hochgestapelte Ballen blickt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf hochgestapelte Ballen blickt", "tokens": ["Auf", "hoch\u00b7ges\u00b7ta\u00b7pel\u00b7te", "Bal\u00b7len", "blickt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Kaufherr mit Erg\u00f6tzen;", "tokens": ["Der", "Kauf\u00b7herr", "mit", "Er\u00b7g\u00f6t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein armer Fischer daneben flickt", "tokens": ["Ein", "ar\u00b7mer", "Fi\u00b7scher", "da\u00b7ne\u00b7ben", "flickt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Betr\u00fcbt an zerrissenen Netzen.", "tokens": ["Be\u00b7tr\u00fcbt", "an", "zer\u00b7ris\u00b7se\u00b7nen", "Net\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Manch r\u00fcstig stolzbewimpelt Schiff!", "tokens": ["Manch", "r\u00fcs\u00b7tig", "stolz\u00b7be\u00b7wim\u00b7pelt", "Schiff", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch morsches Wrack im Sande!", "tokens": ["Manch", "mor\u00b7sches", "Wrack", "im", "San\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Hafen hier, und dort das Riff,", "tokens": ["Der", "Ha\u00b7fen", "hier", ",", "und", "dort", "das", "Riff", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt Fluth, jetzt Ebb' am Strande.", "tokens": ["Jetzt", "Fluth", ",", "jetzt", "Ebb'", "am", "Stran\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier Sonnenblick, Sturmwolken dort;", "tokens": ["Hier", "Son\u00b7nen\u00b7blick", ",", "Sturm\u00b7wol\u00b7ken", "dort", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Hier Schweigen, dorten Lieder,", "tokens": ["Hier", "Schwei\u00b7gen", ",", "dor\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Heimkehr hier, dort Abschiedswort;", "tokens": ["Und", "Heim\u00b7kehr", "hier", ",", "dort", "Ab\u00b7schieds\u00b7wort", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Segel auf und nieder!", "tokens": ["Die", "Se\u00b7gel", "auf", "und", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zwei Jungfrauen sitzen am Meeresstrand;", "tokens": ["Zwei", "Jung\u00b7frau\u00b7en", "sit\u00b7zen", "am", "Mee\u00b7res\u00b7strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die eine weint in die Fluthen,", "tokens": ["Die", "ei\u00b7ne", "weint", "in", "die", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die andre mit dem Kranz in der Hand", "tokens": ["Die", "and\u00b7re", "mit", "dem", "Kranz", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wirft Rosen in die Fluthen.", "tokens": ["Wirft", "Ro\u00b7sen", "in", "die", "Flut\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die eine, tr\u00fcber Wehmuth Bild,", "tokens": ["Die", "ei\u00b7ne", ",", "tr\u00fc\u00b7ber", "Weh\u00b7muth", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "St\u00f6hnt mit geheimem Beben:", "tokens": ["St\u00f6hnt", "mit", "ge\u00b7hei\u00b7mem", "Be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo Meer, o Meer, so tr\u00fcb und wild,", "tokens": ["\u00bb", "o", "Meer", ",", "o", "Meer", ",", "so", "tr\u00fcb", "und", "wild", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gleichst du so ganz dem Leben!\u00ab", "tokens": ["Wie", "gleichst", "du", "so", "ganz", "dem", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die andre, lichter Freude Bild,", "tokens": ["Die", "and\u00b7re", ",", "lich\u00b7ter", "Freu\u00b7de", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kos't selig l\u00e4chelnd daneben:", "tokens": ["Kos't", "se\u00b7lig", "l\u00e4\u00b7chelnd", "da\u00b7ne\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJD", "PAV", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u00bbo Meer, o Meer, so licht und mild,", "tokens": ["\u00bb", "o", "Meer", ",", "o", "Meer", ",", "so", "licht", "und", "mild", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gleichst du so ganz dem Leben!\u00ab", "tokens": ["Wie", "gleichst", "du", "so", "ganz", "dem", "Le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Fortbraust das Meer und \u00fcberklingt", "tokens": ["Fort\u00b7braust", "das", "Meer", "und", "\u00fc\u00b7berk\u00b7lingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das St\u00f6hnen wie das Kosen;", "tokens": ["Das", "St\u00f6h\u00b7nen", "wie", "das", "Ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fortwogt das Meer, und, ach, verschlingt", "tokens": ["Fort\u00b7wogt", "das", "Meer", ",", "und", ",", "ach", ",", "ver\u00b7schlingt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "$,", "ITJ", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Thr\u00e4nen wie die Rosen.", "tokens": ["Die", "Thr\u00e4\u00b7nen", "wie", "die", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}