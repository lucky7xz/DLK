{"dta.poem.7056": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann nun die Herrlichkeit von diesen seel\u2019gen Schwellen", "tokens": ["Wann", "nun", "die", "Herr\u00b7lich\u00b7keit", "von", "die\u00b7sen", "seel'\u00b7gen", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschlichen Begriff unendlich \u00fcbersteiget;", "tokens": ["Den", "Menschli\u00b7chen", "Be\u00b7griff", "un\u00b7end\u00b7lich", "\u00fc\u00b7bers\u00b7tei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So f\u00e4llt es gar nicht schwer, zugleich uns vorzustellen,", "tokens": ["So", "f\u00e4llt", "es", "gar", "nicht", "schwer", ",", "zu\u00b7gleich", "uns", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$,", "ADV", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dieser Raum solch\u2019 eine Gr\u00f6sse zeigt,", "tokens": ["Da\u00df", "die\u00b7ser", "Raum", "solch'", "ei\u00b7ne", "Gr\u00f6s\u00b7se", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die, wo sie nicht unendlich; doch unendlich", "tokens": ["Die", ",", "wo", "sie", "nicht", "un\u00b7end\u00b7lich", ";", "doch", "un\u00b7end\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "$,", "PWAV", "PPER", "PTKNEG", "ADJD", "$.", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nach Menschlichem Begriff: Wir finden keine Schrancken,", "tokens": ["Nach", "Menschli\u00b7chem", "Be\u00b7griff", ":", "Wir", "fin\u00b7den", "kei\u00b7ne", "Schran\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Es wird allhier ein Ziel den forschenden Gedancken", "tokens": ["Es", "wird", "all\u00b7hier", "ein", "Ziel", "den", "for\u00b7schen\u00b7den", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ohn End\u2019 hinaus gesetzt. Ein End\u2019 ist unverst\u00e4ndlich.", "tokens": ["Ohn", "End'", "hin\u00b7aus", "ge\u00b7setzt", ".", "Ein", "End'", "ist", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "VVPP", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}