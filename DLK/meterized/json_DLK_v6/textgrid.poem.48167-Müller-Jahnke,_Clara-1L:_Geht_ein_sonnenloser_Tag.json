{"textgrid.poem.48167": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geht ein sonnenloser Tag", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht ein sonnenloser Tag", "tokens": ["Geht", "ein", "son\u00b7nen\u00b7lo\u00b7ser", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wiederum zur Neige,", "tokens": ["wie\u00b7de\u00b7rum", "zur", "Nei\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und der graue Nebel tropft", "tokens": ["und", "der", "grau\u00b7e", "Ne\u00b7bel", "tropft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "durch die kahlen Zweige.", "tokens": ["durch", "die", "kah\u00b7len", "Zwei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Leise atmend ruht die See,", "tokens": ["Lei\u00b7se", "at\u00b7mend", "ruht", "die", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "m\u00fcde, traumumsponnen . . .", "tokens": ["m\u00fc\u00b7de", ",", "trau\u00b7mum\u00b7spon\u00b7nen", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "eine Woge, schaumgekr\u00f6nt,", "tokens": ["ei\u00b7ne", "Wo\u00b7ge", ",", "schaum\u00b7ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ist im Sand zerronnen.", "tokens": ["ist", "im", "Sand", "zer\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}