{"textgrid.poem.35517": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz ist voll Sonne,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist voll Sonne,", "tokens": ["Mein", "Herz", "ist", "voll", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Voll Sonne so ganz ...", "tokens": ["Voll", "Son\u00b7ne", "so", "ganz", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kaum,", "tokens": ["Kaum", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Da\u00df einstiger Schmerzen", "tokens": ["Da\u00df", "eins\u00b7ti\u00b7ger", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "R\u00fcckf\u00fchrender Traum", "tokens": ["R\u00fcck\u00b7f\u00fch\u00b7ren\u00b7der", "Traum"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Leise mich streift ...", "tokens": ["Lei\u00b7se", "mich", "streift", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Es reift", "tokens": ["Es", "reift"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Alles, was ich besitze,", "tokens": ["Al\u00b7les", ",", "was", "ich", "be\u00b7sit\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Zu k\u00f6stlicher Nahrung,", "tokens": ["Zu", "k\u00f6st\u00b7li\u00b7cher", "Nah\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Kommender Blitze", "tokens": ["Kom\u00b7men\u00b7der", "Blit\u00b7ze"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Satte Bewahrung ...", "tokens": ["Sat\u00b7te", "Be\u00b7wah\u00b7rung", "..."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist voll Sonne,", "tokens": ["Mein", "Herz", "ist", "voll", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Voll Sonne so ganz ...", "tokens": ["Voll", "Son\u00b7ne", "so", "ganz", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADV", "ADV", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Um die Schl\u00e4fen mir schlingen", "tokens": ["Um", "die", "Schl\u00e4\u00b7fen", "mir", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PPER", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die gl\u00fccklichen G\u00f6tter", "tokens": ["Die", "gl\u00fcck\u00b7li\u00b7chen", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Den olympischen Kranz ...", "tokens": ["Den", "o\u00b7lym\u00b7pi\u00b7schen", "Kranz", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}