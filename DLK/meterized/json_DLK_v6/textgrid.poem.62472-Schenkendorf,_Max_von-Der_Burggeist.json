{"textgrid.poem.62472": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der Burggeist", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch auf dem Felsen, auf dem Thurm", "tokens": ["Hoch", "auf", "dem", "Fel\u00b7sen", ",", "auf", "dem", "Thurm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da steht ein alter Geist;", "tokens": ["Da", "steht", "ein", "al\u00b7ter", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er weht mich an, das ist ein Sturm,", "tokens": ["Er", "weht", "mich", "an", ",", "das", "ist", "ein", "Sturm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der mich von dannen rei\u00dft.", "tokens": ["Der", "mich", "von", "dan\u00b7nen", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist aus alter k\u00fchner Zeit", "tokens": ["Das", "ist", "aus", "al\u00b7ter", "k\u00fch\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein stolzes Riesenbild,", "tokens": ["Ein", "stol\u00b7zes", "Rie\u00b7sen\u00b7bild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es hat die Waffen mir gefeit,", "tokens": ["Es", "hat", "die", "Waf\u00b7fen", "mir", "ge\u00b7feit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat mich mit Muth erf\u00fcllt.", "tokens": ["Hat", "mich", "mit", "Muth", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist der W\u00e4chter, ist der Hort", "tokens": ["Es", "ist", "der", "W\u00e4ch\u00b7ter", ",", "ist", "der", "Hort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NE", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von diesem edlen Haus;", "tokens": ["Von", "die\u00b7sem", "ed\u00b7len", "Haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich gab ihm Handschlag, Ritterwort,", "tokens": ["Ich", "gab", "ihm", "Hand\u00b7schlag", ",", "Rit\u00b7ter\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ziehn ins Feld hinaus.", "tokens": ["Zu", "ziehn", "ins", "Feld", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die B\u00e4ume streben himmelan,", "tokens": ["Die", "B\u00e4u\u00b7me", "stre\u00b7ben", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach oben f\u00fchrt ein Pfad,", "tokens": ["Nach", "o\u00b7ben", "f\u00fchrt", "ein", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Haupt hebt jeder deutsche Mann,", "tokens": ["Sein", "Haupt", "hebt", "je\u00b7der", "deut\u00b7sche", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil die Erl\u00f6sung naht.", "tokens": ["Weil", "die", "Er\u00b7l\u00f6\u00b7sung", "naht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Viel hohe Zeichen sind geschehn,", "tokens": ["Viel", "ho\u00b7he", "Zei\u00b7chen", "sind", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Zeichen folgen nach,", "tokens": ["Viel", "Zei\u00b7chen", "fol\u00b7gen", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das kann kein wilder Sturm verwehn,", "tokens": ["Das", "kann", "kein", "wil\u00b7der", "Sturm", "ver\u00b7wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was Gott der Herr versprach.", "tokens": ["Was", "Gott", "der", "Herr", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der kann der Zeichen viele sehn,", "tokens": ["Der", "kann", "der", "Zei\u00b7chen", "vie\u00b7le", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer sie im Glauben sucht;", "tokens": ["Wer", "sie", "im", "Glau\u00b7ben", "sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen aus dem Kampf nicht gehn,", "tokens": ["Wir", "wol\u00b7len", "aus", "dem", "Kampf", "nicht", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis hier kein Welscher flucht.", "tokens": ["Bis", "hier", "kein", "Wel\u00b7scher", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wie sich durch der Erde Mark", "tokens": ["Und", "wie", "sich", "durch", "der", "Er\u00b7de", "Mark"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Felsenadern ziehn,", "tokens": ["Die", "Fel\u00b7se\u00b7na\u00b7dern", "ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So schw\u00f6ren wir als M\u00e4nner stark", "tokens": ["So", "schw\u00f6\u00b7ren", "wir", "als", "M\u00e4n\u00b7ner", "stark"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die V\u00f6lker zu durchgl\u00fchn.", "tokens": ["Die", "V\u00f6l\u00b7ker", "zu", "durch\u00b7gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das war es, was der alte Geist,", "tokens": ["Das", "war", "es", ",", "was", "der", "al\u00b7te", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der deutsche Geist gewollt,", "tokens": ["Der", "deut\u00b7sche", "Geist", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der dem, was welsch und knechtisch hei\u00dft,", "tokens": ["Der", "dem", ",", "was", "welsch", "und", "knech\u00b7tisch", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wol ewig flucht und grollt.", "tokens": ["Wol", "e\u00b7wig", "flucht", "und", "grollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}