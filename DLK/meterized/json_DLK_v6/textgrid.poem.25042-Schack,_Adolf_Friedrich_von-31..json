{"textgrid.poem.25042": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fliegt, durch die zitternden Reben", "tokens": ["Fliegt", ",", "durch", "die", "zit\u00b7tern\u00b7den", "Re\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ins St\u00fcbchen, ihr T\u00f6ne, fliegt,", "tokens": ["Ins", "St\u00fcb\u00b7chen", ",", "ihr", "T\u00f6\u00b7ne", ",", "fliegt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo hinter den Gitterst\u00e4ben", "tokens": ["Wo", "hin\u00b7ter", "den", "Git\u00b7ter\u00b7st\u00e4\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Kleine schlummernd liegt!", "tokens": ["Die", "Klei\u00b7ne", "schlum\u00b7mernd", "liegt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schon beim Klange der Saiten", "tokens": ["Schon", "beim", "Klan\u00b7ge", "der", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Regt sich die Schl\u00e4ferin;", "tokens": ["Regt", "sich", "die", "Schl\u00e4\u00b7fe\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Liebliche Tr\u00e4ume gleiten", "tokens": ["Lieb\u00b7li\u00b7che", "Tr\u00e4u\u00b7me", "glei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "F\u00fchlt sie durch Seele und Sinn!", "tokens": ["F\u00fchlt", "sie", "durch", "See\u00b7le", "und", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Web aus t\u00f6nenden Maschen,", "tokens": ["Web", "aus", "t\u00f6\u00b7nen\u00b7den", "Ma\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Webe ein Netz, mein Lied,", "tokens": ["We\u00b7be", "ein", "Netz", ",", "mein", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im Schlummer ihr Herz zu haschen,", "tokens": ["Im", "Schlum\u00b7mer", "ihr", "Herz", "zu", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das wachend scheu vor mir flieht.", "tokens": ["Das", "wa\u00b7chend", "scheu", "vor", "mir", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "L\u00e4nger mit Lachen und Necken", "tokens": ["L\u00e4n\u00b7ger", "mit", "La\u00b7chen", "und", "Ne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "H\u00f6hnen mich soll es nicht mehr;", "tokens": ["H\u00f6h\u00b7nen", "mich", "soll", "es", "nicht", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Wo es sich mag verstecken,", "tokens": ["Wo", "es", "sich", "mag", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fang es und bring's mir her.", "tokens": ["Fang", "es", "und", "bring's", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Nicht z\u00fcrnen wird sie dem Diebe,", "tokens": ["Nicht", "z\u00fcr\u00b7nen", "wird", "sie", "dem", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der es geraubt \u00fcber Nacht,", "tokens": ["Der", "es", "ge\u00b7raubt", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Wenn aus Tr\u00e4umen der Liebe", "tokens": ["Wenn", "aus", "Tr\u00e4u\u00b7men", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Beim Morgenrot sie erwacht.", "tokens": ["Beim", "Mor\u00b7gen\u00b7rot", "sie", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}