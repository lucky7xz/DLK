{"textgrid.poem.47081": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die wir nie mit unsern Klagen wecken,", "tokens": ["Du", ",", "die", "wir", "nie", "mit", "un\u00b7sern", "Kla\u00b7gen", "we\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Warum so fr\u00fch ruhst du von deinem Gange?", "tokens": ["Wa\u00b7rum", "so", "fr\u00fch", "ruhst", "du", "von", "dei\u00b7nem", "Gan\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War dir wohl vor des Mittags Schw\u00fclen bange?", "tokens": ["War", "dir", "wohl", "vor", "des", "Mit\u00b7tags", "Schw\u00fc\u00b7len", "ban\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schuf wohl des fernen Abends Frost dir Schrecken?", "tokens": ["Schuf", "wohl", "des", "fer\u00b7nen", "A\u00b7bends", "Frost", "dir", "Schre\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein! Mutig hobst du deinen Schritt, den kecken,", "tokens": ["Nein", "!", "Mu\u00b7tig", "hobst", "du", "dei\u00b7nen", "Schritt", ",", "den", "ke\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deiner Jugend vollstem \u00dcberschwange;", "tokens": ["In", "dei\u00b7ner", "Ju\u00b7gend", "volls\u00b7tem", "\u00dc\u00b7bersc\u00b7hwan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Blick in ungeduld'gem Hoffnungsdrange", "tokens": ["Dein", "Blick", "in", "un\u00b7ge\u00b7duld'\u00b7gem", "Hoff\u00b7nungs\u00b7dran\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Flog vorw\u00e4rts nach des Lebens blum'gen Strecken.", "tokens": ["Flog", "vor\u00b7w\u00e4rts", "nach", "des", "Le\u00b7bens", "blum'\u00b7gen", "Stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht wie ein zagend Kind, das grambeladen", "tokens": ["Nicht", "wie", "ein", "za\u00b7gend", "Kind", ",", "das", "gram\u00b7be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "KOKOM", "ART", "ADJD", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich nach der Mutter heimsehnt in die Ferne,", "tokens": ["Sich", "nach", "der", "Mut\u00b7ter", "heim\u00b7sehnt", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wardst du vom Wink der Mutter heimgeladen.", "tokens": ["Wardst", "du", "vom", "Wink", "der", "Mut\u00b7ter", "heim\u00b7ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein strenger Vater rief, wo du noch gerne", "tokens": ["Ein", "stren\u00b7ger", "Va\u00b7ter", "rief", ",", "wo", "du", "noch", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gegangen w\u00e4rst, dich ab von deinen Pfaden,", "tokens": ["Ge\u00b7gan\u00b7gen", "w\u00e4rst", ",", "dich", "ab", "von", "dei\u00b7nen", "Pfa\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Kindessinn vor ihm sich beugen lerne.", "tokens": ["Da\u00df", "Kin\u00b7des\u00b7sinn", "vor", "ihm", "sich", "beu\u00b7gen", "ler\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}