{"textgrid.poem.62359": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der verwandten Seele", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm' in den Garten, komm', es laden", "tokens": ["Komm'", "in", "den", "Gar\u00b7ten", ",", "komm'", ",", "es", "la\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "$,", "PPER", "VVINF"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling und die Nacht uns ein,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "und", "die", "Nacht", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kamen von des Wests Gestaden,", "tokens": ["Sie", "ka\u00b7men", "von", "des", "Wests", "Ge\u00b7sta\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um Zeugen unsres Gl\u00fccks zu sein.", "tokens": ["Um", "Zeu\u00b7gen", "uns\u00b7res", "Gl\u00fccks", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Harfe bebt von Fr\u00fchlingsl\u00fcften \u2013", "tokens": ["Die", "Har\u00b7fe", "bebt", "von", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh, wie der Mond ins Fenster winkt!", "tokens": ["Sieh", ",", "wie", "der", "Mond", "ins", "Fens\u00b7ter", "winkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm', da\u00df in jenen Blumend\u00fcften", "tokens": ["Komm'", ",", "da\u00df", "in", "je\u00b7nen", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele Himmelsahnung trinkt.", "tokens": ["Die", "See\u00b7le", "Him\u00b7mel\u00b7sah\u00b7nung", "trinkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In dem verschwiegnen Heiligthume,", "tokens": ["In", "dem", "ver\u00b7schwieg\u00b7nen", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um das die Nacht den Schleier legt,", "tokens": ["Um", "das", "die", "Nacht", "den", "Schlei\u00b7er", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entfaltet sich die stille Blume,", "tokens": ["Ent\u00b7fal\u00b7tet", "sich", "die", "stil\u00b7le", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die nur f\u00fcr sie den Balsam tr\u00e4gt.", "tokens": ["Die", "nur", "f\u00fcr", "sie", "den", "Bal\u00b7sam", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Wesen trennenden Gestalten", "tokens": ["Die", "We\u00b7sen", "tren\u00b7nen\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerflie\u00dfen in der D\u00e4mmerung", "tokens": ["Zer\u00b7flie\u00b7\u00dfen", "in", "der", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und Seelen, die zusammen wallten,", "tokens": ["Und", "See\u00b7len", ",", "die", "zu\u00b7sam\u00b7men", "wall\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfreu'n sich der Vereinigung.", "tokens": ["Er\u00b7freu'n", "sich", "der", "Ver\u00b7ei\u00b7ni\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Siehst du, wie dort im Sternenkreise,", "tokens": ["Siehst", "du", ",", "wie", "dort", "im", "Ster\u00b7nen\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit stillem, liebendem Gem\u00fcth", "tokens": ["Mit", "stil\u00b7lem", ",", "lie\u00b7ben\u00b7dem", "Ge\u00b7m\u00fcth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mond auf ewig gleiche Weise", "tokens": ["Der", "Mond", "auf", "e\u00b7wig", "glei\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach der verwandten Erde sieht?", "tokens": ["Nach", "der", "ver\u00b7wand\u00b7ten", "Er\u00b7de", "sieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der jedem Ding die Bahn gemessen,", "tokens": ["Der", "je\u00b7dem", "Ding", "die", "Bahn", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonnen einst an Sonnen band,", "tokens": ["Der", "Son\u00b7nen", "einst", "an", "Son\u00b7nen", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat seinen Liebling nicht vergessen,", "tokens": ["Hat", "sei\u00b7nen", "Lieb\u00b7ling", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Wesenkranz kn\u00fcpft Gottes Hand.", "tokens": ["Den", "We\u00b7sen\u00b7kranz", "kn\u00fcpft", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Hauch, in dem dem gro\u00dfen Geiste", "tokens": ["Der", "Hauch", ",", "in", "dem", "dem", "gro\u00b7\u00dfen", "Geis\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der schaffende Gedank' entflo\u00df,", "tokens": ["Der", "schaf\u00b7fen\u00b7de", "Ge\u00b7dank'", "ent\u00b7flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Von dem das alte Chaos kreiste,", "tokens": ["Von", "dem", "das", "al\u00b7te", "Chaos", "kreis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Er war's, der unsern Bund beschlo\u00df.", "tokens": ["Er", "wa\u00b7r's", ",", "der", "un\u00b7sern", "Bund", "be\u00b7schlo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "So komm' denn, du verwandte Seele,", "tokens": ["So", "komm'", "denn", ",", "du", "ver\u00b7wand\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In der sich meine Seele schaut,", "tokens": ["In", "der", "sich", "mei\u00b7ne", "See\u00b7le", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mich nicht w\u00e4hlt, die ich nicht w\u00e4hle,", "tokens": ["Die", "mich", "nicht", "w\u00e4hlt", ",", "die", "ich", "nicht", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir die Ewigkeit vertraut!", "tokens": ["Die", "mir", "die", "E\u00b7wig\u00b7keit", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nach ", "tokens": ["Nach"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Zerrinnen wir in ", "tokens": ["Zer\u00b7rin\u00b7nen", "wir", "in"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch nimmer wird der Kreis vollendet,", "tokens": ["Doch", "nim\u00b7mer", "wird", "der", "Kreis", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der magisch um die Welt sich zieht.", "tokens": ["Der", "ma\u00b7gisch", "um", "die", "Welt", "sich", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sprich, k\u00f6nnten wir ein Gl\u00fcck genie\u00dfen,", "tokens": ["Sprich", ",", "k\u00f6nn\u00b7ten", "wir", "ein", "Gl\u00fcck", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich nicht jedes Wesen freut,", "tokens": ["Wenn", "sich", "nicht", "je\u00b7des", "We\u00b7sen", "freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PTKNEG", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eng' uns in uns selbst verschlie\u00dfen", "tokens": ["Und", "eng'", "uns", "in", "uns", "selbst", "ver\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Angesicht der Ewigkeit?", "tokens": ["Im", "An\u00b7ge\u00b7sicht", "der", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Zu G\u00f6ttern macht uns der Gedanke,", "tokens": ["Zu", "G\u00f6t\u00b7tern", "macht", "uns", "der", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hier durch meine Seele gl\u00e4nzt;", "tokens": ["Der", "hier", "durch", "mei\u00b7ne", "See\u00b7le", "gl\u00e4nzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df keines Raumes enge Schranke", "tokens": ["Da\u00df", "kei\u00b7nes", "Rau\u00b7mes", "en\u00b7ge", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sel'ge G\u00f6tterreich begrenzt.", "tokens": ["Das", "sel'\u00b7ge", "G\u00f6t\u00b7ter\u00b7reich", "be\u00b7grenzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "So la\u00df uns denn auf unsern Pfaden", "tokens": ["So", "la\u00df", "uns", "denn", "auf", "un\u00b7sern", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umher nach Bundesgliedern sp\u00e4hn,", "tokens": ["Um\u00b7her", "nach", "Bun\u00b7des\u00b7glie\u00b7dern", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle mild und freundlich laden,", "tokens": ["Und", "al\u00b7le", "mild", "und", "freund\u00b7lich", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unser Paradies zu gehn.", "tokens": ["In", "un\u00b7ser", "Pa\u00b7ra\u00b7dies", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "So Manchen werden wir begegnen,", "tokens": ["So", "Man\u00b7chen", "wer\u00b7den", "wir", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einsam gehn den Dornengang,", "tokens": ["Die", "ein\u00b7sam", "gehn", "den", "Dor\u00b7nen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einst mit uns die Stunde segnen,", "tokens": ["Und", "einst", "mit", "uns", "die", "Stun\u00b7de", "seg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In der uns diese Glut durchdrang.", "tokens": ["In", "der", "uns", "die\u00b7se", "Glut", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Im Mondschein werden wir sie finden,", "tokens": ["Im", "Mond\u00b7schein", "wer\u00b7den", "wir", "sie", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Zeichen, das uns Gl\u00fcck verhei\u00dft,", "tokens": ["Im", "Zei\u00b7chen", ",", "das", "uns", "Gl\u00fcck", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dem verwandten Geiste k\u00fcnden", "tokens": ["Und", "dem", "ver\u00b7wand\u00b7ten", "Geis\u00b7te", "k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-