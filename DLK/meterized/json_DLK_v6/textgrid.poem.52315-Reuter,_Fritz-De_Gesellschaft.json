{"textgrid.poem.52315": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "De Gesellschaft", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De oll Kasprati is gewi\u00df", "tokens": ["De", "oll", "Ka\u00b7spra\u00b7ti", "is", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tau Rostock nich allein bekannt,", "tokens": ["Tau", "Ros\u00b7tock", "nich", "al\u00b7lein", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un m\u00e4nnig sch\u00f6nes L\u00e4uschen is", "tokens": ["Un", "m\u00e4n\u00b7nig", "sch\u00f6\u00b7nes", "L\u00e4u\u00b7schen", "is"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJA", "NN", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von em bericht't d\u00f6rch't ganze Land.", "tokens": ["Von", "em", "be\u00b7richt't", "d\u00f6rch't", "gan\u00b7ze", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Na, de hadd mal en l\u00fcttes Swin", "tokens": ["Na", ",", "de", "hadd", "mal", "en", "l\u00fct\u00b7tes", "Swin"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "FM", "FM", "FM", "FM", "FM", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Von Melmsen tau Sapshagen k\u00f6fft", "tokens": ["Von", "Melm\u00b7sen", "tau", "Saps\u00b7ha\u00b7gen", "k\u00f6fft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Un hadd, as wir de Nam' ok sin,", "tokens": ["Un", "hadd", ",", "as", "wir", "de", "Nam'", "ok", "sin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Dat Swin denn ok nah Melmsen d\u00f6fft.", "tokens": ["Dat", "Swin", "denn", "ok", "nah", "Melm\u00b7sen", "d\u00f6fft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NE", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "\u00bbdie kleine Melms\u00ab, so n\u00e4umt hei't blo\u00df,", "tokens": ["\u00bb", "die", "klei\u00b7ne", "Melms", "\u00ab", ",", "so", "n\u00e4umt", "hei't", "blo\u00df", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NE", "$(", "$,", "ADV", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Un kamm mal einer tau em rinner,", "tokens": ["Un", "kamm", "mal", "ei\u00b7ner", "tau", "em", "rin\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "XY", "XY", "XY", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Denn g\u00fcng ok dat Vertellen los,", "tokens": ["Denn", "g\u00fcng", "ok", "dat", "Ver\u00b7tel\u00b7len", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nich etwa von sin Fru un Kinner,", "tokens": ["Nich", "et\u00b7wa", "von", "sin", "Fru", "un", "Kin\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "FM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "So as dat s\u00fcs woll pleggt tau sin,", "tokens": ["So", "as", "dat", "s\u00fcs", "woll", "pleggt", "tau", "sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ne, von \u00bbdie kleine Melms\u00ab, von't l\u00fctte Swin.", "tokens": ["Ne", ",", "von", "\u00bb", "die", "klei\u00b7ne", "Melms", "\u00ab", ",", "von't", "l\u00fct\u00b7te", "Swin", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "$(", "ART", "ADJA", "NE", "$(", "$,", "ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "\u00bbdie kleine Melms\u00ab, de w\u00fcrd nu up den Kaben", "tokens": ["\u00bb", "die", "klei\u00b7ne", "Melms", "\u00ab", ",", "de", "w\u00fcrd", "nu", "up", "den", "Ka\u00b7ben"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NE", "$(", "$,", "NE", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mit eine olle S\u00e4g tausamen set't,", "tokens": ["Mit", "ei\u00b7ne", "ol\u00b7le", "S\u00e4g", "tau\u00b7sa\u00b7men", "set't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Un as dat kamm so gegen Fastelaben,", "tokens": ["Un", "as", "dat", "kamm", "so", "ge\u00b7gen", "Fas\u00b7te\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Dunn was de S\u00e4g ok riklich fett.", "tokens": ["Dunn", "was", "de", "S\u00e4g", "ok", "rik\u00b7lich", "fett", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "De w\u00fcrd nu slacht't, un as't gescheihn,", "tokens": ["De", "w\u00fcrd", "nu", "slacht't", ",", "un", "as't", "ge\u00b7scheihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVFIN", "$,", "FM", "FM", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.20": {"text": "Dunn satt \u00bbdie kleine Melms\u00ab allein,", "tokens": ["Dunn", "satt", "\u00bb", "die", "klei\u00b7ne", "Melms", "\u00ab", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ART", "ADJA", "NE", "$(", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Un't was, as ded sei sick d'r\u00fcm gr\u00e4men,", "tokens": ["Un't", "was", ",", "as", "ded", "sei", "sick", "d'\u00b7r\u00fcm", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Sei wull kein Fauder tau sick nemen.", "tokens": ["Sei", "wull", "kein", "Fau\u00b7der", "tau", "sick", "ne\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "VMFIN", "PIAT", "NN", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdie kleine Melms\u00ab ward d\u00e4glich slimmer,", "tokens": ["\u00bb", "die", "klei\u00b7ne", "Melms", "\u00ab", "ward", "d\u00e4g\u00b7lich", "slim\u00b7mer", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NE", "$(", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Un oll Kasprati, de geiht r\u00fcmmer", "tokens": ["Un", "oll", "Ka\u00b7spra\u00b7ti", ",", "de", "geiht", "r\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "NE", "$,", "NE", "VVFIN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Un schellt sin Knecht und schellt sin M\u00e4ten", "tokens": ["Un", "schellt", "sin", "Knecht", "und", "schellt", "sin", "M\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "V\u00f6r Sl\u00fcngels, Esels un v\u00f6r Schelms,", "tokens": ["V\u00f6r", "Sl\u00fcn\u00b7gels", ",", "E\u00b7sels", "un", "v\u00f6r", "Schelms", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "De em dat Swin verhungern leten,", "tokens": ["De", "em", "dat", "Swin", "ver\u00b7hun\u00b7gern", "le\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Un jammert \u00fcm \u00bbdie kleine Melms\u00ab.", "tokens": ["Un", "jam\u00b7mert", "\u00fcm", "\u00bb", "die", "klei\u00b7ne", "Melms", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$(", "ART", "ADJA", "NE", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Na, \u00fcm de Tid bes\u00f6cht ick em einmal,", "tokens": ["Na", ",", "\u00fcm", "de", "Tid", "be\u00b7s\u00f6cht", "ick", "em", "ein\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Hei gung in sine Gaststuw up un dal,", "tokens": ["Hei", "gung", "in", "si\u00b7ne", "Gast\u00b7stuw", "up", "un", "dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-++-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Schimpt un schandierte m\u00f6rderlich:", "tokens": ["Schimpt", "un", "schan\u00b7dier\u00b7te", "m\u00f6r\u00b7der\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "FM", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbi wei\u00df nich, was i mach bei die verfluchti Sach':", "tokens": ["\u00bb", "i", "wei\u00df", "nich", ",", "was", "i", "mach", "bei", "die", "ver\u00b7fluch\u00b7ti", "Sach'", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "VVFIN", "PTKNEG", "$,", "PRELS", "NE", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "---+-----+-+", "measure": "unknown.measure.tri"}, "line.11": {"text": "Die kleine Melms, die is ganz k\u00fcmmerlich,", "tokens": ["Die", "klei\u00b7ne", "Melms", ",", "die", "is", "ganz", "k\u00fcm\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "FM", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sie fre\u00dft mi un sie sauft mi nich.", "tokens": ["Sie", "fre\u00dft", "mi", "un", "sie", "sauft", "mi", "nich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "FM", "FM", "PPER", "VVFIN", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "I wei\u00df nich, was ich dabei mach!\u00ab", "tokens": ["I", "wei\u00df", "nich", ",", "was", "ich", "da\u00b7bei", "mach", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PAV", "VVFIN", "$.", "$("], "meter": "+----+-+", "measure": "dactylic.init"}, "line.14": {"text": "\u00bbde Schad'\u00ab, segg ick, \u00bbde is nich grot;", "tokens": ["\u00bb", "de", "Schad'", "\u00ab", ",", "segg", "ick", ",", "\u00bb", "de", "is", "nich", "grot", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "FM", "FM", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dat l\u00fctte Dirt, dat gr\u00e4mt sick blot,", "tokens": ["Dat", "l\u00fct\u00b7te", "Dirt", ",", "dat", "gr\u00e4mt", "sick", "blot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Sei m\u00f6ten em Gesellschaft gewen.\u00ab", "tokens": ["Sei", "m\u00f6\u00b7ten", "em", "Ge\u00b7sell\u00b7schaft", "ge\u00b7wen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADJA", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "\u00bbgesellschaft? Ja, das mein i eben!", "tokens": ["\u00bb", "ge\u00b7sell\u00b7schaft", "?", "Ja", ",", "das", "mein", "i", "e\u00b7ben", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "PTKANT", "$,", "PRELS", "PPOSAT", "NE", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da f\u00f6llt mir mit de Kellner ein.", "tokens": ["Da", "f\u00f6llt", "mir", "mit", "de", "Kell\u00b7ner", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Christ! \u2013 Junge! \u2013 Christ, komm doch mal rein!", "tokens": ["Christ", "!", "\u2013", "Jun\u00b7ge", "!", "\u2013", "Christ", ",", "komm", "doch", "mal", "rein", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "NN", "$.", "$(", "NN", "$,", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die kleine Melms, die gr\u00e4mt sich sehr,", "tokens": ["Die", "klei\u00b7ne", "Melms", ",", "die", "gr\u00e4mt", "sich", "sehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "PRELS", "ADJD", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sie sitzt allein auf ihre Kafen.", "tokens": ["Sie", "sitzt", "al\u00b7lein", "auf", "ih\u00b7re", "Ka\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Was meinst du, Christ, wie w\u00e4r,", "tokens": ["Was", "meinst", "du", ",", "Christ", ",", "wie", "w\u00e4r", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "PWAV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}}}}}