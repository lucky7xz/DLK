{"textgrid.poem.38550": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An ein Dorf", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flora kr\u00f6nt mit heitrer Blumenf\u00fclle", "tokens": ["Flo\u00b7ra", "kr\u00f6nt", "mit", "hei\u00b7trer", "Blu\u00b7men\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Rasenh\u00fcgel,", "tokens": ["Dei\u00b7ne", "Ra\u00b7sen\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ceres \u00fcberstr\u00f6mt mit goldnem Segen", "tokens": ["Ce\u00b7res", "\u00fc\u00b7ber\u00b7str\u00f6mt", "mit", "gold\u00b7nem", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dein Gefild, o D\u00f6rfchen!", "tokens": ["Dein", "Ge\u00b7fild", ",", "o", "D\u00f6rf\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Schwesterlich, in deiner B\u00e4ume Zwielicht,", "tokens": ["Schwes\u00b7ter\u00b7lich", ",", "in", "dei\u00b7ner", "B\u00e4u\u00b7me", "Zwie\u00b7licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wandeln, traut umschlungen,", "tokens": ["Wan\u00b7deln", ",", "traut", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie durch Ge\u00dfners Hirtenparadiese,", "tokens": ["Wie", "durch", "Ge\u00df\u00b7ners", "Hir\u00b7ten\u00b7pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Seelenruh' und Unschuld.", "tokens": ["See\u00b7len\u00b7ruh'", "und", "Un\u00b7schuld", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Sittsamkeit blieb deiner T\u00f6chter Erbe;", "tokens": ["Sitt\u00b7sam\u00b7keit", "blieb", "dei\u00b7ner", "T\u00f6ch\u00b7ter", "Er\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihrer Wangen Bl\u00fcthe", "tokens": ["Ih\u00b7rer", "Wan\u00b7gen", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Prangt in keuschem Jugendroth, wie Guidos", "tokens": ["Prangt", "in", "keu\u00b7schem", "Ju\u00b7gen\u00b7droth", ",", "wie", "Gui\u00b7dos"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "PWAV", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Himmlische Madonnen.", "tokens": ["Himm\u00b7li\u00b7sche", "Ma\u00b7don\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wacker sind und kraftvoll deine S\u00f6hne;", "tokens": ["Wa\u00b7cker", "sind", "und", "kraft\u00b7voll", "dei\u00b7ne", "S\u00f6h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mit wie mancher Wildni\u00df,", "tokens": ["Mit", "wie", "man\u00b7cher", "Wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KOKOM", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo die Distel herrschte, rang um Aehren", "tokens": ["Wo", "die", "Dis\u00b7tel", "herrschte", ",", "rang", "um", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon ihr Arm von Eisen!", "tokens": ["Schon", "ihr", "Arm", "von", "Ei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O da\u00df einst, o Dorf, in deinen Schatten,", "tokens": ["O", "da\u00df", "einst", ",", "o", "Dorf", ",", "in", "dei\u00b7nen", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bis zur letzten Woge,", "tokens": ["Bis", "zur", "letz\u00b7ten", "Wo\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mir der Strom des Lebens, rein wie jener", "tokens": ["Mir", "der", "Strom", "des", "Le\u00b7bens", ",", "rein", "wie", "je\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "ADJD", "KOKOM", "PDS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wiesenborn, entwallte!", "tokens": ["Wie\u00b7sen\u00b7born", ",", "ent\u00b7wall\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dort, wo Pappeln D\u00e4mmrung streun und K\u00fchle,", "tokens": ["Dort", ",", "wo", "Pap\u00b7peln", "D\u00e4mm\u00b7rung", "streun", "und", "K\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "NN", "VVINF", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo des Thals Geb\u00fcsche", "tokens": ["Wo", "des", "Thals", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In des M\u00fchlenteichs kristallner Klarheit", "tokens": ["In", "des", "M\u00fch\u00b7len\u00b7teichs", "kris\u00b7tall\u00b7ner", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Locken spiegeln:", "tokens": ["Ih\u00b7re", "Lo\u00b7cken", "spie\u00b7geln", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Winkte meine weinumrankte H\u00fctte,", "tokens": ["Wink\u00b7te", "mei\u00b7ne", "wei\u00b7num\u00b7rank\u00b7te", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gr\u00fcnte meine Laube,", "tokens": ["Gr\u00fcn\u00b7te", "mei\u00b7ne", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bl\u00fchten meines Blumengartens Beete,", "tokens": ["Bl\u00fch\u00b7ten", "mei\u00b7nes", "Blu\u00b7men\u00b7gar\u00b7tens", "Bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Reiften meine Saaten!", "tokens": ["Reif\u00b7ten", "mei\u00b7ne", "Saa\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Jenes Buchenhaines Fr\u00fchges\u00e4nge", "tokens": ["Je\u00b7nes", "Bu\u00b7chen\u00b7hai\u00b7nes", "Fr\u00fch\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weckten mich am Morgen;", "tokens": ["Weck\u00b7ten", "mich", "am", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dieses Apfelbaumes Nachtigallen", "tokens": ["Die\u00b7ses", "Ap\u00b7fel\u00b7bau\u00b7mes", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "T\u00f6nten mich in Schlummer!", "tokens": ["T\u00f6n\u00b7ten", "mich", "in", "Schlum\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Stern der Hoffnung! Doch du bist umschleiert;", "tokens": ["Stern", "der", "Hoff\u00b7nung", "!", "Doch", "du", "bist", "um\u00b7schlei\u00b7ert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ach! das Wonnel\u00e4cheln", "tokens": ["Ach", "!", "das", "Won\u00b7ne\u00b7l\u00e4\u00b7cheln"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meiner Grazie, der holden Freude,", "tokens": ["Mei\u00b7ner", "Gra\u00b7zie", ",", "der", "hol\u00b7den", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Starb an Lauras Grabe!", "tokens": ["Starb", "an", "Lau\u00b7ras", "Gra\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}