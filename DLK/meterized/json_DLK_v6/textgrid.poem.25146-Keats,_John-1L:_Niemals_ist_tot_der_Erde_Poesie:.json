{"textgrid.poem.25146": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: Niemals ist tot der Erde Poesie:", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemals ist tot der Erde Poesie:", "tokens": ["Nie\u00b7mals", "ist", "tot", "der", "Er\u00b7de", "Poe\u00b7sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn V\u00f6gel m\u00fcde sind von hei\u00dfen Sonnen,", "tokens": ["Wenn", "V\u00f6\u00b7gel", "m\u00fc\u00b7de", "sind", "von", "hei\u00b7\u00dfen", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann nimmt die F\u00fchrung in den Sommerwonnen", "tokens": ["Dann", "nimmt", "die", "F\u00fch\u00b7rung", "in", "den", "Som\u00b7mer\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Grash\u00fcpfers Stimme, und sie rastet nie.", "tokens": ["Gras\u00b7h\u00fcp\u00b7fers", "Stim\u00b7me", ",", "und", "sie", "ras\u00b7tet", "nie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Von Heck zu Hecke rennt die Melodie", "tokens": ["Von", "Heck", "zu", "He\u00b7cke", "rennt", "die", "Me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00e4lt die frischgem\u00e4hte Trift umsponnen;", "tokens": ["Und", "h\u00e4lt", "die", "frischge\u00b7m\u00e4h\u00b7te", "Trift", "um\u00b7spon\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Macht Lust ihn matt, so ruht er s\u00fc\u00df versonnen", "tokens": ["Macht", "Lust", "ihn", "matt", ",", "so", "ruht", "er", "s\u00fc\u00df", "ver\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei gr\u00fcnstem Halme, der f\u00fcr ihn gedieh.", "tokens": ["Bei", "gr\u00fcns\u00b7tem", "Hal\u00b7me", ",", "der", "f\u00fcr", "ihn", "ge\u00b7dieh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nie endet sie, die Poesie der Erde.", "tokens": ["Nie", "en\u00b7det", "sie", ",", "die", "Poe\u00b7sie", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am stillen Winterabend, wenn der grimme", "tokens": ["Am", "stil\u00b7len", "Win\u00b7ter\u00b7a\u00b7bend", ",", "wenn", "der", "grim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nachtfrost ein Schweigen breitet, schrillt vom Herde", "tokens": ["Nacht\u00b7frost", "ein", "Schwei\u00b7gen", "brei\u00b7tet", ",", "schrillt", "vom", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Des Heimchens Sang dem Tr\u00e4umer in die Ohren,", "tokens": ["Des", "Heim\u00b7chens", "Sang", "dem", "Tr\u00e4u\u00b7mer", "in", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als habe sich Grash\u00fcpfers Sommerstimme", "tokens": ["Als", "ha\u00b7be", "sich", "Gras\u00b7h\u00fcp\u00b7fers", "Som\u00b7mers\u00b7tim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PRF", "NE", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Aus gr\u00fcner Trift in seinen Traum verloren.", "tokens": ["Aus", "gr\u00fc\u00b7ner", "Trift", "in", "sei\u00b7nen", "Traum", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}