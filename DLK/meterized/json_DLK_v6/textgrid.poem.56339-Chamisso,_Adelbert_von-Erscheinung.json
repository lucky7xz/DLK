{"textgrid.poem.56339": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Erscheinung", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die zw\u00f6lfte Stunde war beim Klang der Becher", "tokens": ["Die", "zw\u00f6lf\u00b7te", "Stun\u00b7de", "war", "beim", "Klang", "der", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und w\u00fcstem Treiben schon herangewacht,", "tokens": ["Und", "w\u00fcs\u00b7tem", "Trei\u00b7ben", "schon", "her\u00b7an\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als ich hinaus mich stahl, ein m\u00fcder Zecher.", "tokens": ["Als", "ich", "hin\u00b7aus", "mich", "stahl", ",", "ein", "m\u00fc\u00b7der", "Ze\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APZR", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und um mich lag die kalte, finstre Nacht;", "tokens": ["Und", "um", "mich", "lag", "die", "kal\u00b7te", ",", "finst\u00b7re", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich h\u00f6rte durch die Stille widerhallen", "tokens": ["Ich", "h\u00f6r\u00b7te", "durch", "die", "Stil\u00b7le", "wi\u00b7der\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den eignen Tritt und fernen Ruf der Wacht.", "tokens": ["Den", "eig\u00b7nen", "Tritt", "und", "fer\u00b7nen", "Ruf", "der", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie aus den klangreich fest-erhellten Hallen", "tokens": ["Wie", "aus", "den", "klan\u00b7greich", "fest\u00b7er\u00b7hell\u00b7ten", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Einsamkeit sich meine Schritte wandten,", "tokens": ["In", "Ein\u00b7sam\u00b7keit", "sich", "mei\u00b7ne", "Schrit\u00b7te", "wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ward ich von seltsam tr\u00fcbem Mut befallen.", "tokens": ["Ward", "ich", "von", "selt\u00b7sam", "tr\u00fc\u00b7bem", "Mut", "be\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und meinem Hause nah, dem wohlbekannten,", "tokens": ["Und", "mei\u00b7nem", "Hau\u00b7se", "nah", ",", "dem", "wohl\u00b7be\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gewahrt ich, und ich stand versteinert fast,", "tokens": ["Ge\u00b7wahrt", "ich", ",", "und", "ich", "stand", "ver\u00b7stei\u00b7nert", "fast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df hinter meinen Fenstern Lichter brannten.", "tokens": ["Da\u00df", "hin\u00b7ter", "mei\u00b7nen", "Fens\u00b7tern", "Lich\u00b7ter", "brann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich pr\u00fcfte zweifelnd eine lange Rast,", "tokens": ["Ich", "pr\u00fcf\u00b7te", "zwei\u00b7felnd", "ei\u00b7ne", "lan\u00b7ge", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und fragte: macht es nur in mir der Wein?", "tokens": ["Und", "frag\u00b7te", ":", "macht", "es", "nur", "in", "mir", "der", "Wein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wie k\u00e4m zu dieser Stunde mir ein Gast?", "tokens": ["Wie", "k\u00e4m", "zu", "die\u00b7ser", "Stun\u00b7de", "mir", "ein", "Gast", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "PDAT", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ich trat hinzu, und konnte bei dem Schein", "tokens": ["Ich", "trat", "hin\u00b7zu", ",", "und", "konn\u00b7te", "bei", "dem", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Im wohlverschlo\u00dfnen Schlo\u00df den Schl\u00fcssel drehen,", "tokens": ["Im", "wohl\u00b7ver\u00b7schlo\u00df\u00b7nen", "Schlo\u00df", "den", "Schl\u00fcs\u00b7sel", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und \u00f6ffnete die T\u00fcr, und trat hinein.", "tokens": ["Und", "\u00f6ff\u00b7ne\u00b7te", "die", "T\u00fcr", ",", "und", "trat", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und, wie die Blicke nach dem Lichte sp\u00e4hen,", "tokens": ["Und", ",", "wie", "die", "Bli\u00b7cke", "nach", "dem", "Lich\u00b7te", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da ward mir ein Gesicht gar schreckenreich, \u2013", "tokens": ["Da", "ward", "mir", "ein", "Ge\u00b7sicht", "gar", "schre\u00b7cken\u00b7reich", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich sah mich selbst an meinem Pulte stehen.", "tokens": ["Ich", "sah", "mich", "selbst", "an", "mei\u00b7nem", "Pul\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ich rief: \u00bbWer bist du, Spuk?\u00ab \u2013 er rief sogleich:", "tokens": ["Ich", "rief", ":", "\u00bb", "Wer", "bist", "du", ",", "Spuk", "?", "\u00ab", "\u2013", "er", "rief", "sog\u00b7leich", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "VAFIN", "PPER", "$,", "NN", "$.", "$(", "$(", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "\u00bbwer st\u00f6rt mich auf in sp\u00e4ter Geisterstunde?\u00ab", "tokens": ["\u00bb", "wer", "st\u00f6rt", "mich", "auf", "in", "sp\u00e4\u00b7ter", "Geis\u00b7ter\u00b7stun\u00b7de", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und sah mich an, und ward, wie ich, auch bleich.", "tokens": ["Und", "sah", "mich", "an", ",", "und", "ward", ",", "wie", "ich", ",", "auch", "bleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und unerme\u00dflich wollte die Sekunde", "tokens": ["Und", "un\u00b7er\u00b7me\u00df\u00b7lich", "woll\u00b7te", "die", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Sich dehnen, da wir starrend wechselseitig", "tokens": ["Sich", "deh\u00b7nen", ",", "da", "wir", "star\u00b7rend", "wech\u00b7sel\u00b7sei\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PDS", "$,", "KOUS", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Uns ansahn, sprachberaubt mit offnem Munde.", "tokens": ["Uns", "an\u00b7sahn", ",", "sprach\u00b7be\u00b7raubt", "mit", "off\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und aus beklommner Brust zuerst befreit ich", "tokens": ["Und", "aus", "be\u00b7klomm\u00b7ner", "Brust", "zu\u00b7erst", "be\u00b7freit", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Das schnelle Wort: \u00bbDu grause Truggestalt,", "tokens": ["Das", "schnel\u00b7le", "Wort", ":", "\u00bb", "Du", "grau\u00b7se", "Trug\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Entweiche, mache mir den Platz nicht streitig!\u00ab", "tokens": ["Ent\u00b7wei\u00b7che", ",", "ma\u00b7che", "mir", "den", "Platz", "nicht", "strei\u00b7tig", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und er, als einer, \u00fcber den Gewalt", "tokens": ["Und", "er", ",", "als", "ei\u00b7ner", ",", "\u00fc\u00b7ber", "den", "Ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "KOUS", "PIS", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Die Furcht nur hat, erzwingend sich ein leises", "tokens": ["Die", "Furcht", "nur", "hat", ",", "er\u00b7zwin\u00b7gend", "sich", "ein", "lei\u00b7ses"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "VVPP", "PRF", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Und scheues L\u00e4cheln, sprach erwidernd: \u00bbHalt!", "tokens": ["Und", "scheu\u00b7es", "L\u00e4\u00b7cheln", ",", "sprach", "er\u00b7wi\u00b7dernd", ":", "\u00bb", "Halt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "VVFIN", "VVPP", "$.", "$(", "VVIMP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Ich bin's, du willst es sein; \u2013 um dieses Kreises,", "tokens": ["Ich", "bin's", ",", "du", "willst", "es", "sein", ";", "\u2013", "um", "die\u00b7ses", "Krei\u00b7ses", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VMFIN", "PPER", "VAINF", "$.", "$(", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Des wahnsinn-drohnden, Quadratur zu finden,", "tokens": ["Des", "wahn\u00b7sinn\u00b7drohn\u00b7den", ",", "Quad\u00b7ra\u00b7tur", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Bist du der rechte, wie du sagst, beweis es;", "tokens": ["Bist", "du", "der", "rech\u00b7te", ",", "wie", "du", "sagst", ",", "be\u00b7weis", "es", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Ins Wesenlose will ich dann verschwinden.", "tokens": ["Ins", "We\u00b7sen\u00b7lo\u00b7se", "will", "ich", "dann", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Du Spuk, wie du mich nennst, gehst du das ein,", "tokens": ["Du", "Spuk", ",", "wie", "du", "mich", "nennst", ",", "gehst", "du", "das", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und willst auch du zu Gleichem dich verbinden?\u00ab", "tokens": ["Und", "willst", "auch", "du", "zu", "Glei\u00b7chem", "dich", "ver\u00b7bin\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPER", "APPR", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Drauf ich entr\u00fcstet: \u00bbJa, so soll es sein!", "tokens": ["Drauf", "ich", "ent\u00b7r\u00fcs\u00b7tet", ":", "\u00bb", "Ja", ",", "so", "soll", "es", "sein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVPP", "$.", "$(", "PTKANT", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Es soll mein echtes Ich sich offenbaren,", "tokens": ["Es", "soll", "mein", "ech\u00b7tes", "Ich", "sich", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Zu Nichts zerflie\u00dfen dessen leerer Schein!\u00ab", "tokens": ["Zu", "Nichts", "zer\u00b7flie\u00b7\u00dfen", "des\u00b7sen", "lee\u00b7rer", "Schein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PDS", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Und er: \u00bbSo la\u00df uns, wer du seist, erfahren!\u00ab", "tokens": ["Und", "er", ":", "\u00bb", "So", "la\u00df", "uns", ",", "wer", "du", "seist", ",", "er\u00b7fah\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "$.", "$(", "ADV", "VVIMP", "PPER", "$,", "PWS", "PPER", "VAFIN", "$,", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Und ich: \u00bbEin solcher bin ich, der getrachtet", "tokens": ["Und", "ich", ":", "\u00bb", "Ein", "sol\u00b7cher", "bin", "ich", ",", "der", "ge\u00b7trach\u00b7tet"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$.", "$(", "ART", "PIAT", "VAFIN", "PPER", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Nur einzig nach dem Sch\u00f6nen, Guten, Wahren;", "tokens": ["Nur", "ein\u00b7zig", "nach", "dem", "Sch\u00f6\u00b7nen", ",", "Gu\u00b7ten", ",", "Wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Der Opfer nie dem G\u00f6tzendienst geschlachtet,", "tokens": ["Der", "Op\u00b7fer", "nie", "dem", "G\u00f6t\u00b7zen\u00b7dienst", "ge\u00b7schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Und nie gefr\u00f6nt dem weltlich eitlen Brauch,", "tokens": ["Und", "nie", "ge\u00b7fr\u00f6nt", "dem", "welt\u00b7lich", "eit\u00b7len", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Verkannt, verh\u00f6hnt, der Schmerzen nie geachtet;", "tokens": ["Ver\u00b7kannt", ",", "ver\u00b7h\u00f6hnt", ",", "der", "Schmer\u00b7zen", "nie", "ge\u00b7ach\u00b7tet", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Der irrend zwar und tr\u00e4umend oft den Rauch", "tokens": ["Der", "ir\u00b7rend", "zwar", "und", "tr\u00e4u\u00b7mend", "oft", "den", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "KON", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.50": {"text": "F\u00fcr Flamme hielt, doch mutig beim Erwachen", "tokens": ["F\u00fcr", "Flam\u00b7me", "hielt", ",", "doch", "mu\u00b7tig", "beim", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.51": {"text": "Das Rechte nur verfocht: \u2013 bist du das auch?\u00ab", "tokens": ["Das", "Rech\u00b7te", "nur", "ver\u00b7focht", ":", "\u2013", "bist", "du", "das", "auch", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$.", "$(", "VAFIN", "PPER", "PDS", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Und er mit wildem, kreischend lautem Lachen:", "tokens": ["Und", "er", "mit", "wil\u00b7dem", ",", "krei\u00b7schend", "lau\u00b7tem", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.53": {"text": "\u00bbder du dich r\u00fchmst zu sein, der bin ich nicht.", "tokens": ["\u00bb", "der", "du", "dich", "r\u00fchmst", "zu", "sein", ",", "der", "bin", "ich", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "PRF", "VVFIN", "PTKZU", "VAINF", "$,", "PRELS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.54": {"text": "Gar anders ist's bestellt um meine Sachen.", "tokens": ["Gar", "an\u00b7ders", "ist's", "be\u00b7stellt", "um", "mei\u00b7ne", "Sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.55": {"text": "Ich bin ein feiger, l\u00fcgenhafter Wicht,", "tokens": ["Ich", "bin", "ein", "fei\u00b7ger", ",", "l\u00fc\u00b7gen\u00b7haf\u00b7ter", "Wicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.56": {"text": "Ein Heuchler mir und andern, tief im Herzen", "tokens": ["Ein", "Heuch\u00b7ler", "mir", "und", "an\u00b7dern", ",", "tief", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "KON", "ADJA", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.57": {"text": "Nur Eigennutz, und Trug im Angesicht.", "tokens": ["Nur", "Ei\u00b7gen\u00b7nutz", ",", "und", "Trug", "im", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.58": {"text": "Verkannter Edler du mit deinen Schmerzen,", "tokens": ["Ver\u00b7kann\u00b7ter", "Ed\u00b7ler", "du", "mit", "dei\u00b7nen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.59": {"text": "Wer kennt sich nun? wer gab das rechte Zeichen?", "tokens": ["Wer", "kennt", "sich", "nun", "?", "wer", "gab", "das", "rech\u00b7te", "Zei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ADV", "$.", "PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.60": {"text": "Wer soll, ich oder du, sein Selbst verscherzen?", "tokens": ["Wer", "soll", ",", "ich", "o\u00b7der", "du", ",", "sein", "Selbst", "ver\u00b7scher\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$,", "PPER", "KON", "PPER", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.61": {"text": "Tritt her, so du es wagst, ich will dir weichen!\u00ab", "tokens": ["Tritt", "her", ",", "so", "du", "es", "wagst", ",", "ich", "will", "dir", "wei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADV", "PPER", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.62": {"text": "Drauf mit Entsetzen ich zu jenem Graus:", "tokens": ["Drauf", "mit", "Ent\u00b7set\u00b7zen", "ich", "zu", "je\u00b7nem", "Graus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.63": {"text": "\u00bbdu bist es, bleib, und la\u00df hinweg mich schleichen!\u00ab \u2013", "tokens": ["\u00bb", "du", "bist", "es", ",", "bleib", ",", "und", "la\u00df", "hin\u00b7weg", "mich", "schlei\u00b7chen", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "$,", "VVFIN", "$,", "KON", "VVIMP", "ADV", "PPER", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.64": {"text": "Und schlich, zu weinen, in die Nacht hinaus.", "tokens": ["Und", "schlich", ",", "zu", "wei\u00b7nen", ",", "in", "die", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}