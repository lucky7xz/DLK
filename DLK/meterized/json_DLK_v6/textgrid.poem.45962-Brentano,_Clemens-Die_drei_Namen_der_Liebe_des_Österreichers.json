{"textgrid.poem.45962": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Namen der Liebe des \u00d6sterreichers", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr wollt ein Lied, ein Lied der Freude", "tokens": ["Ihr", "wollt", "ein", "Lied", ",", "ein", "Lied", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur eines kann ich sagen heute.", "tokens": ["Nur", "ei\u00b7nes", "kann", "ich", "sa\u00b7gen", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rt still mich an, teilt meine Wonne,", "tokens": ["H\u00f6rt", "still", "mich", "an", ",", "teilt", "mei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns allen leuchtet eine Sonne.", "tokens": ["Uns", "al\u00b7len", "leuch\u00b7tet", "ei\u00b7ne", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Drei Namen nenn' ich euch segenreich,", "tokens": ["Drei", "Na\u00b7men", "nenn'", "ich", "euch", "se\u00b7gen\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie stehn mit der Freude im Bunde,", "tokens": ["Sie", "stehn", "mit", "der", "Freu\u00b7de", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Jed Herz bewegt sich in Wonne gleich,", "tokens": ["Jed", "Herz", "be\u00b7wegt", "sich", "in", "Won\u00b7ne", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn sie t\u00f6nen von Munde zu Munde:", "tokens": ["Wenn", "sie", "t\u00f6\u00b7nen", "von", "Mun\u00b7de", "zu", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "O liebet uns immer, wir lieben euch!", "tokens": ["O", "lie\u00b7bet", "uns", "im\u00b7mer", ",", "wir", "lie\u00b7ben", "euch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Unser Vater ist ", "tokens": ["Un\u00b7ser", "Va\u00b7ter", "ist"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Der Tag, der uns ihn gegeben,", "tokens": ["Der", "Tag", ",", "der", "uns", "ihn", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sieht heut ihn gekr\u00f6nt mit dem Siegeskranz,", "tokens": ["Sieht", "heut", "ihn", "ge\u00b7kr\u00f6nt", "mit", "dem", "Sie\u00b7ges\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der ewig wird \u00fcber ihm schweben,", "tokens": ["Der", "e\u00b7wig", "wird", "\u00fc\u00b7ber", "ihm", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Denn des Sieges Kranz in ewigem Licht,", "tokens": ["Denn", "des", "Sie\u00b7ges", "Kranz", "in", "e\u00b7wi\u00b7gem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ist der Kranz, den Liebe der V\u00f6lker flicht.", "tokens": ["Ist", "der", "Kranz", ",", "den", "Lie\u00b7be", "der", "V\u00f6l\u00b7ker", "flicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Unsre Mutter nenn' ich, die Huld umgiebt,", "tokens": ["Uns\u00b7re", "Mut\u00b7ter", "nenn'", "ich", ",", "die", "Huld", "um\u00b7giebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kein Kind lebt, das seine Mutter liebt,", "tokens": ["Kein", "Kind", "lebt", ",", "das", "sei\u00b7ne", "Mut\u00b7ter", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Das nicht ins Gebet sie schlie\u00dfe,", "tokens": ["Das", "nicht", "ins", "Ge\u00b7bet", "sie", "schlie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Denn es bl\u00fchet der Anmut, des Geistes Zier", "tokens": ["Denn", "es", "bl\u00fc\u00b7het", "der", "An\u00b7mut", ",", "des", "Geis\u00b7tes", "Zier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Im Krondiademe der Hoheit Ihr.", "tokens": ["Im", "Kron\u00b7di\u00b7a\u00b7de\u00b7me", "der", "Ho\u00b7heit", "Ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Unser Vaterland nenn' ich dich ", "tokens": ["Un\u00b7ser", "Va\u00b7ter\u00b7land", "nenn'", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PRF"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dich \u00d6streich in Siegesweihe;", "tokens": ["Dich", "\u00d6s\u00b7treich", "in", "Sie\u00b7ges\u00b7wei\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch Vaterland tut es dem meinen gleich", "tokens": ["Welch", "Va\u00b7ter\u00b7land", "tut", "es", "dem", "mei\u00b7nen", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PDS", "VVFIN", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "An St\u00e4rke, an Liebe, an Treue.", "tokens": ["An", "St\u00e4r\u00b7ke", ",", "an", "Lie\u00b7be", ",", "an", "Treu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Durch sie, durch sie nur strahlet allein", "tokens": ["Durch", "sie", ",", "durch", "sie", "nur", "strah\u00b7let", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "ADV", "VVFIN", "ADV"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "\u00d6streich in freudigem Siegesschein.", "tokens": ["\u00d6s\u00b7treich", "in", "freu\u00b7di\u00b7gem", "Sie\u00b7ges\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Und die St\u00e4rke, sie ist der Vater \u2013 ", "tokens": ["Und", "die", "St\u00e4r\u00b7ke", ",", "sie", "ist", "der", "Va\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Liebe ist die Mutter \u2013 ", "tokens": ["Die", "Lie\u00b7be", "ist", "die", "Mut\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und die Treue ist \u00d6streichs sch\u00f6nster Glanz,", "tokens": ["Und", "die", "Treu\u00b7e", "ist", "\u00d6s\u00b7treichs", "sch\u00f6ns\u00b7ter", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NE", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der ewig die Herrscher umflie\u00dfe.", "tokens": ["Der", "e\u00b7wig", "die", "Herr\u00b7scher", "um\u00b7flie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Denn wo sich die St\u00e4rke der Liebe verband,", "tokens": ["Denn", "wo", "sich", "die", "St\u00e4r\u00b7ke", "der", "Lie\u00b7be", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Hat Treue ihr ewiges Vaterland.", "tokens": ["Hat", "Treu\u00b7e", "ihr", "e\u00b7wi\u00b7ges", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Drei Namen nannt' ich euch segenreich,", "tokens": ["Drei", "Na\u00b7men", "nannt'", "ich", "euch", "se\u00b7gen\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie stehn mit der Freude im Bunde,", "tokens": ["Sie", "stehn", "mit", "der", "Freu\u00b7de", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Jed Herz bewegt sich in Wonne gleich,", "tokens": ["Jed", "Herz", "be\u00b7wegt", "sich", "in", "Won\u00b7ne", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn sie t\u00f6nen vom Munde zu Munde", "tokens": ["Wenn", "sie", "t\u00f6\u00b7nen", "vom", "Mun\u00b7de", "zu", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "O liebet uns immer, wir lieben euch!", "tokens": ["O", "lie\u00b7bet", "uns", "im\u00b7mer", ",", "wir", "lie\u00b7ben", "euch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Ich nannte die Namen, teilt all meine Wonne,", "tokens": ["Ich", "nann\u00b7te", "die", "Na\u00b7men", ",", "teilt", "all", "mei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Uns allen leuchtet nur eine Sonne!", "tokens": ["Uns", "al\u00b7len", "leuch\u00b7tet", "nur", "ei\u00b7ne", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}