{"dta.poem.7128": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich dieses schreib\u2019 und ernstlich \u00fcberdencke,", "tokens": ["In\u00b7dem", "ich", "die\u00b7ses", "schreib'", "und", "ernst\u00b7lich", "\u00fc\u00b7ber\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie leicht ein Sterblicher von Angst und Jammer frey,", "tokens": ["Wie", "leicht", "ein", "Sterb\u00b7li\u00b7cher", "von", "Angst", "und", "Jam\u00b7mer", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und aus dem Ungl\u00fccks-Pfuhl heraus zu ziehen sey,", "tokens": ["Und", "aus", "dem", "Un\u00b7gl\u00fccks\u00b7P\u00b7fuhl", "he\u00b7raus", "zu", "zie\u00b7hen", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APZR", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Blo\u00df durch den Schlaf allein, und da\u00df, so lang\u2019 er w\u00e4hret,", "tokens": ["Blo\u00df", "durch", "den", "Schlaf", "al\u00b7lein", ",", "und", "da\u00df", ",", "so", "lang'", "er", "w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,", "KON", "KOUS", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Leid f\u00fcr ihm so gut als w\u00fcrcklich aufgeh\u00f6ret;", "tokens": ["Sein", "Leid", "f\u00fcr", "ihm", "so", "gut", "als", "w\u00fcrck\u00b7lich", "auf\u00b7ge\u00b7h\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So denck\u2019 ich: wie vielmehr wird durch des Schlafes Bruder,", "tokens": ["So", "denck", "ich", ":", "wie", "viel\u00b7mehr", "wird", "durch", "des", "Schla\u00b7fes", "Bru\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$.", "PWAV", "ADV", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein hier Ge\u00e4ngsteter, durch Jammer, Schmach und Pein,", "tokens": ["Ein", "hier", "Ge\u00b7\u00e4ngs\u00b7te\u00b7ter", ",", "durch", "Jam\u00b7mer", ",", "Schmach", "und", "Pein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Von aller Noth befreyt, im Grabe ruhig seyn,", "tokens": ["Von", "al\u00b7ler", "Noth", "be\u00b7freyt", ",", "im", "Gra\u00b7be", "ru\u00b7hig", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "APPRART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn er von allen dem, wodurch er auf der Erden,", "tokens": ["Wenn", "er", "von", "al\u00b7len", "dem", ",", "wo\u00b7durch", "er", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "PDS", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit Armuth, Frost und Pein, mit Jammer und Beschwerden,", "tokens": ["Mit", "Ar\u00b7muth", ",", "Frost", "und", "Pein", ",", "mit", "Jam\u00b7mer", "und", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch wenn die Leidenschafft sein Innerstes durchw\u00fchlt,", "tokens": ["Auch", "wenn", "die", "Lei\u00b7den\u00b7schafft", "sein", "In\u00b7ners\u00b7tes", "durch\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf einmahl frey gemacht, nicht das geringste f\u00fchlt.", "tokens": ["Auf", "ein\u00b7mahl", "frey", "ge\u00b7macht", ",", "nicht", "das", "ge\u00b7rings\u00b7te", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "VVPP", "$,", "PTKNEG", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewie aber, f\u00e4llst du mir vielleicht, mein Leser, ein:", "tokens": ["\u201e", "wie", "a\u00b7ber", ",", "f\u00e4llst", "du", "mir", "viel\u00b7leicht", ",", "mein", "Le\u00b7ser", ",", "ein", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "$,", "VVFIN", "PPER", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201esoll einem Schlafenden ein Todter \u00e4hnlich seyn,", "tokens": ["\u201e", "soll", "ei\u00b7nem", "Schla\u00b7fen\u00b7den", "ein", "Tod\u00b7ter", "\u00e4hn\u00b7lich", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ART", "NN", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewo bleibt der Himmel denn? der Stand der seel\u2019 gen", "tokens": ["\u201e", "wo", "bleibt", "der", "Him\u00b7mel", "denn", "?", "der", "Stand", "der", "seel'", "gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VVFIN", "ART", "NN", "ADV", "$.", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Seelen;", "tokens": ["See\u00b7len", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "\u201esoll bis zum j\u00fcngsten Tag er uns denn etwann fehlen?", "tokens": ["\u201e", "soll", "bis", "zum", "j\u00fcng\u00b7sten", "Tag", "er", "uns", "denn", "et\u00b7wann", "feh\u00b7len", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "APPRART", "ADJA", "NN", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bef\u00fcrchte nicht, da\u00df sich dadurch dein Gl\u00fcck vers\u00e4ume,", "tokens": ["Be\u00b7f\u00fcrch\u00b7te", "nicht", ",", "da\u00df", "sich", "da\u00b7durch", "dein", "Gl\u00fcck", "ver\u00b7s\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PRF", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du denckest nicht daran, da\u00df man im Schlaf auch tr\u00e4ume.", "tokens": ["Du", "den\u00b7ckest", "nicht", "da\u00b7ran", ",", "da\u00df", "man", "im", "Schlaf", "auch", "tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$,", "KOUS", "PIS", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201esprich nicht, was Tr\u00e4umerey, es ist ein leichter Schaum", "tokens": ["\u201e", "sprich", "nicht", ",", "was", "Tr\u00e4u\u00b7me\u00b7rey", ",", "es", "ist", "ein", "leich\u00b7ter", "Schaum"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "PTKNEG", "$,", "PWS", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201eso eitel und so leer nicht einst, als wie ein Traum.", "tokens": ["\u201e", "so", "ei\u00b7tel", "und", "so", "leer", "nicht", "einst", ",", "als", "wie", "ein", "Traum", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "KON", "ADV", "ADJD", "PTKNEG", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nein, dencke besser nach: es fasst ja dein Verstand", "tokens": ["Nein", ",", "den\u00b7cke", "bes\u00b7ser", "nach", ":", "es", "fasst", "ja", "dein", "Ver\u00b7stand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nicht, was die Tr\u00e4ume sind.", "tokens": ["Nicht", ",", "was", "die", "Tr\u00e4u\u00b7me", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Dir ist, wie ihre Quell, ihr Wesen unbekannt,", "tokens": ["Dir", "ist", ",", "wie", "ih\u00b7re", "Quell", ",", "ihr", "We\u00b7sen", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch magst du, was du willt, von Tr\u00e4umen meynen;", "tokens": ["Doch", "magst", "du", ",", "was", "du", "willt", ",", "von", "Tr\u00e4u\u00b7men", "mey\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So wirst du hoffentlich doch dieses nicht verneinen,", "tokens": ["So", "wirst", "du", "hof\u00b7fent\u00b7lich", "doch", "die\u00b7ses", "nicht", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Da\u00df unsre Seel\u2019 im Traum, Be\u00e4ngstigung und Freude,", "tokens": ["Da\u00df", "uns\u00b7re", "Seel'", "im", "Traum", ",", "Be\u00b7\u00e4ngs\u00b7ti\u00b7gung", "und", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vergn\u00fcgen und Verdru\u00df versp\u00fcr\u2019, empfind\u2019 und leide.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "und", "Ver\u00b7dru\u00df", "ver\u00b7sp\u00fcr'", ",", "emp\u00b7find'", "und", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}