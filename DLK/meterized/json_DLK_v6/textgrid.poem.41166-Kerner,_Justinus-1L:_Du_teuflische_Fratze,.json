{"textgrid.poem.41166": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du teuflische Fratze,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du teuflische Fratze,", "tokens": ["Du", "teuf\u00b7li\u00b7sche", "Frat\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Halb Mensch und halb Katze!", "tokens": ["Halb", "Mensch", "und", "halb", "Kat\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Was willst du von mir?", "tokens": ["Was", "willst", "du", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ich klecksographier'", "tokens": ["Ich", "kleck\u00b7so\u00b7gra\u00b7phier'"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nicht Ritter vom Besen,", "tokens": ["Nicht", "Rit\u00b7ter", "vom", "Be\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das bist du gewesen,", "tokens": ["Das", "bist", "du", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VAPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Zum Teufel mit dir!", "tokens": ["Zum", "Teu\u00b7fel", "mit", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}