{"textgrid.poem.32791": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Einsame", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier im Scho\u00df der Einsamkeit,", "tokens": ["Hier", "im", "Scho\u00df", "der", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein wilder L\u00e4rm entweiht,", "tokens": ["Die", "kein", "wil\u00b7der", "L\u00e4rm", "ent\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Eichen,", "tokens": ["Un\u00b7ter", "Ei\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "An Gestr\u00e4uchen", "tokens": ["An", "Ge\u00b7str\u00e4u\u00b7chen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "F\u00fchl' ich meine Lebenszeit.", "tokens": ["F\u00fchl'", "ich", "mei\u00b7ne", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Still und ernsthaft, wie der Hain,", "tokens": ["Still", "und", "ernst\u00b7haft", ",", "wie", "der", "Hain", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll mein Herz in Zukunft sein.", "tokens": ["Soll", "mein", "Herz", "in", "Zu\u00b7kunft", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flieht, ihr Sorgen!", "tokens": ["Flieht", ",", "ihr", "Sor\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Jeder Morgen", "tokens": ["Je\u00b7der", "Mor\u00b7gen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ladet mich zur Freude ein.", "tokens": ["La\u00b7det", "mich", "zur", "Freu\u00b7de", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tr\u00e4g und weichlich hei\u00dft jetzt ", "tokens": ["Tr\u00e4g", "und", "weich\u00b7lich", "hei\u00dft", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Spott, des Geistes Gegenwart.", "tokens": ["Spott", ",", "des", "Geis\u00b7tes", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Heuchler,", "tokens": ["Je\u00b7der", "Heuch\u00b7ler", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Jeder Schmeichler", "tokens": ["Je\u00b7der", "Schmeich\u00b7ler"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Hei\u00dft ein Mann von Lebensart.", "tokens": ["Hei\u00dft", "ein", "Mann", "von", "Le\u00b7ben\u00b7sart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Tugend, Freundschaft, Freiheit, euch", "tokens": ["Tu\u00b7gend", ",", "Freund\u00b7schaft", ",", "Frei\u00b7heit", ",", "euch"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausch' ich um kein K\u00f6nigreich!", "tokens": ["Tausch'", "ich", "um", "kein", "K\u00f6\u00b7nig\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollust sch\u00e4ndet,", "tokens": ["Wol\u00b7lust", "sch\u00e4n\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hoheit blendet,", "tokens": ["Ho\u00b7heit", "blen\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Unschuld macht uns Engeln gleich.", "tokens": ["Un\u00b7schuld", "macht", "uns", "En\u00b7geln", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Trotz dem Sturme, der mir dr\u00e4ut,", "tokens": ["Trotz", "dem", "Stur\u00b7me", ",", "der", "mir", "dr\u00e4ut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schleich' ich durch das Thal der Zeit", "tokens": ["Schleich'", "ich", "durch", "das", "Thal", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Geleite", "tokens": ["Im", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Stiller Freude", "tokens": ["Stil\u00b7ler", "Freu\u00b7de"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Zu der nahen Ewigkeit.", "tokens": ["Zu", "der", "na\u00b7hen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}