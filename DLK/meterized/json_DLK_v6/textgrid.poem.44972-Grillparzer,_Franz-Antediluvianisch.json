{"textgrid.poem.44972": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Antediluvianisch", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fch, eh die Flut noch in die Welt gebrochen,", "tokens": ["Fr\u00fch", ",", "eh", "die", "Flut", "noch", "in", "die", "Welt", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gab es Gesch\u00f6pfe, ob zwar wunderlich,", "tokens": ["Gab", "es", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "ob", "zwar", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KOUS", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des zeugen noch fossile Mammutknochen", "tokens": ["Des", "zeu\u00b7gen", "noch", "fos\u00b7si\u00b7le", "Mam\u00b7mut\u00b7kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und das System des F\u00fcrsten Metternich.", "tokens": ["Und", "das", "Sys\u00b7tem", "des", "F\u00fcrs\u00b7ten", "Met\u00b7ter\u00b7nich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "NE", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}}}}