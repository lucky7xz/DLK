{"dta.poem.7993": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die sterbenden Helden .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es gl\u00e4nzet hoch und hehr im Sonnenstral", "tokens": ["Es", "gl\u00e4n\u00b7zet", "hoch", "und", "hehr", "im", "Son\u00b7nen\u00b7stral"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Allvaters Saal,", "tokens": ["All\u00b7va\u00b7ters", "Saal", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Sterne wandeln unter ihm, es ziehn", "tokens": ["Die", "Ster\u00b7ne", "wan\u00b7deln", "un\u00b7ter", "ihm", ",", "es", "ziehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die St\u00fcrme hin.", "tokens": ["Die", "St\u00fcr\u00b7me", "hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dort tafeln mit den V\u00e4tern wir in Ruh,", "tokens": ["Dort", "ta\u00b7feln", "mit", "den", "V\u00e4\u00b7tern", "wir", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erhebe dann dein Lied und end\u2019 es du!", "tokens": ["Er\u00b7he\u00b7be", "dann", "dein", "Lied", "und", "end'", "es", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}