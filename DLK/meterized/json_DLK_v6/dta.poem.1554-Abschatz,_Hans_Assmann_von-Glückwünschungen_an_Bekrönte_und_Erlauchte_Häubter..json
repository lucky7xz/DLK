{"dta.poem.1554": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungen  \n an  \n Bekr\u00f6nte  \n und  \n Erlauchte H\u00e4ubter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wann Tugend jederzeit ihr Gl\u00fccke k\u00f6nte machen/", "tokens": ["Wann", "Tu\u00b7gend", "je\u00b7der\u00b7zeit", "ihr", "Gl\u00fc\u00b7cke", "k\u00f6n\u00b7te", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So schriebe man von mir viel ungemeine Sachen:", "tokens": ["So", "schrie\u00b7be", "man", "von", "mir", "viel", "un\u00b7ge\u00b7mei\u00b7ne", "Sa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch mu\u00df der beste Ruhm das gr\u00f6ste Gl\u00fccke seyn/", "tokens": ["Doch", "mu\u00df", "der", "bes\u00b7te", "Ruhm", "das", "gr\u00f6s\u00b7te", "Gl\u00fc\u00b7cke", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn Gro\u00dfmutt und Verstand beysammen treten ein;", "tokens": ["Wenn", "Gro\u00df\u00b7mutt", "und", "Ver\u00b7stand", "bey\u00b7sam\u00b7men", "tre\u00b7ten", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "NN", "VVIZU", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo sich die Fr\u00f6mmigkeit/ wie hier/ damit verbindet/", "tokens": ["Wo", "sich", "die", "Fr\u00f6m\u00b7mig\u00b7keit", "/", "wie", "hier", "/", "da\u00b7mit", "ver\u00b7bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "$(", "KOKOM", "ADV", "$(", "PAV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So ist der Grund gelegt/ da\u00df kein Gel\u00fccke schwindet;", "tokens": ["So", "ist", "der", "Grund", "ge\u00b7legt", "/", "da\u00df", "kein", "Ge\u00b7l\u00fc\u00b7cke", "schwin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$(", "KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was in der Zeit nicht k\u00f6mmt/ mu\u00df nach der Zeit geschehn:", "tokens": ["Was", "in", "der", "Zeit", "nicht", "k\u00f6mmt", "/", "mu\u00df", "nach", "der", "Zeit", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PTKNEG", "VVFIN", "$(", "VMFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Welt wird dieses noch an meiner Nachkunfft sehn.", "tokens": ["Die", "Welt", "wird", "die\u00b7ses", "noch", "an", "mei\u00b7ner", "Nach\u00b7kunfft", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}