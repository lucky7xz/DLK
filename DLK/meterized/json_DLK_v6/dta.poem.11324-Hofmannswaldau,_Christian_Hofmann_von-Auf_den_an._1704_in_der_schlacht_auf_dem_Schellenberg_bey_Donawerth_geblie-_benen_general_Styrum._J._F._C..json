{"dta.poem.11324": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den  an.  1704 in der schlacht auf dem  \n Schellenberg bey Donawerth geblie-  \n benen general Styrum.  \n J. F. C.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um diesen Donau-straud, wo Bayern itzt erliegt,", "tokens": ["Um", "die\u00b7sen", "Do\u00b7nau\u00b7straud", ",", "wo", "Bay\u00b7ern", "itzt", "er\u00b7liegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$,", "PWAV", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward ich ein jahr zuvor nicht ohne schimpff befiegt;", "tokens": ["Ward", "ich", "ein", "jahr", "zu\u00b7vor", "nicht", "oh\u00b7ne", "schimpff", "be\u00b7fiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ich nun dazumahl geschlagen leben sollen,", "tokens": ["Weil", "ich", "nun", "da\u00b7zu\u00b7mahl", "ge\u00b7schla\u00b7gen", "le\u00b7ben", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVPP", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hab ich itzt allhier im siegen sterben wollen.", "tokens": ["So", "hab", "ich", "itzt", "all\u00b7hier", "im", "sie\u00b7gen", "ster\u00b7ben", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}