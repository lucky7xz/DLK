{"dta.poem.3074": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "120.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O f\u00fchle dich, du f\u00fchlst, du bist von allen Seiten", "tokens": ["O", "f\u00fch\u00b7le", "dich", ",", "du", "f\u00fchlst", ",", "du", "bist", "von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Abh\u00e4ngig, wo du stehn magst, liegen oder schreiten.", "tokens": ["Ab\u00b7h\u00e4n\u00b7gig", ",", "wo", "du", "stehn", "magst", ",", "lie\u00b7gen", "o\u00b7der", "schrei\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.2": {"line.1": {"text": "Vom Sto\u00df der \u00e4u\u00dfern Welt von jeder Seit' abh\u00e4ngig,", "tokens": ["Vom", "Sto\u00df", "der", "\u00e4u\u00b7\u00dfern", "Welt", "von", "je\u00b7der", "Seit'", "ab\u00b7h\u00e4n\u00b7gig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Kraft des Elements zug\u00e4ngig, ja durchg\u00e4ngig.", "tokens": ["Der", "Kraft", "des", "E\u00b7le\u00b7ments", "zu\u00b7g\u00e4n\u00b7gig", ",", "ja", "durch\u00b7g\u00e4n\u00b7gig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht einmal wie ein Erz dem Wasser undurchdringlich,", "tokens": ["Nicht", "ein\u00b7mal", "wie", "ein", "Erz", "dem", "Was\u00b7ser", "un\u00b7durch\u00b7dring\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht einmal wie ein Stein dem Feuer unbezwinglich.", "tokens": ["Nicht", "ein\u00b7mal", "wie", "ein", "Stein", "dem", "Feu\u00b7er", "un\u00b7be\u00b7zwing\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich trinkt der Hauch der Luft, dich i\u00dft der Wittrung Zahn,", "tokens": ["Dich", "trinkt", "der", "Hauch", "der", "Luft", ",", "dich", "i\u00dft", "der", "Witt\u00b7rung", "Zahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich wandelt Tag und Nacht, und wandelt deine Bahn.", "tokens": ["Dich", "wan\u00b7delt", "Tag", "und", "Nacht", ",", "und", "wan\u00b7delt", "dei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O f\u00fchle dich, und sprich, in deiner Engigkeit", "tokens": ["O", "f\u00fch\u00b7le", "dich", ",", "und", "sprich", ",", "in", "dei\u00b7ner", "En\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KON", "ADJD", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie kommst du zum Gef\u00fchl der Unabh\u00e4ngigkeit?", "tokens": ["Wie", "kommst", "du", "zum", "Ge\u00b7f\u00fchl", "der", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du f\u00fchlest, da\u00df ein Hauch dich jenes Odems tr\u00e4gt,", "tokens": ["Du", "f\u00fch\u00b7lest", ",", "da\u00df", "ein", "Hauch", "dich", "je\u00b7nes", "O\u00b7dems", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von dem im Gleichgewicht die Sch\u00f6pfung ist gew\u00e4gt;", "tokens": ["Von", "dem", "im", "Gleich\u00b7ge\u00b7wicht", "die", "Sch\u00f6p\u00b7fung", "ist", "ge\u00b7w\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Von dem im Gleichgewicht die Sch\u00f6pfung ist gew\u00e4gt,", "tokens": ["Von", "dem", "im", "Gleich\u00b7ge\u00b7wicht", "die", "Sch\u00f6p\u00b7fung", "ist", "ge\u00b7w\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPRART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So da\u00df nach keiner Seit' um eine Schale schl\u00e4gt.", "tokens": ["So", "da\u00df", "nach", "kei\u00b7ner", "Seit'", "um", "ei\u00b7ne", "Scha\u00b7le", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wie dich die Wage w\u00e4gt, wo dich die Schale tr\u00e4gt,", "tokens": ["Wie", "dich", "die", "Wa\u00b7ge", "w\u00e4gt", ",", "wo", "dich", "die", "Scha\u00b7le", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohin dich Element ins Element verschl\u00e4gt;", "tokens": ["Wo\u00b7hin", "dich", "E\u00b7le\u00b7ment", "ins", "E\u00b7le\u00b7ment", "ver\u00b7schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sag ihnen: Was verschl\u00e4gt es mir, wie ihr mich w\u00e4gt?", "tokens": ["Sag", "ih\u00b7nen", ":", "Was", "ver\u00b7schl\u00e4gt", "es", "mir", ",", "wie", "ihr", "mich", "w\u00e4gt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PWS", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich f\u00fchle mich ein Geist, mit Geist vom Geist gepr\u00e4gt.", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "ein", "Geist", ",", "mit", "Geist", "vom", "Geist", "ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Wer dis Gepr\u00e4ge tr\u00e4gt, der wei\u00df da\u00df man ihn w\u00e4gt,", "tokens": ["Wer", "dis", "Ge\u00b7pr\u00e4\u00b7ge", "tr\u00e4gt", ",", "der", "wei\u00df", "da\u00df", "man", "ihn", "w\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Pr\u00fcft, l\u00e4utert, umschmelzt, doch als unrecht nie verschl\u00e4gt.", "tokens": ["Pr\u00fcft", ",", "l\u00e4u\u00b7tert", ",", "um\u00b7schmelzt", ",", "doch", "als", "un\u00b7recht", "nie", "ver\u00b7schl\u00e4gt", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "ADV", "KOUS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}