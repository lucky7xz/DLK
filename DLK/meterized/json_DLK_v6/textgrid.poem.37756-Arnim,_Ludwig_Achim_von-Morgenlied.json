{"textgrid.poem.37756": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meint ihr, Sterne l\u00f6schen aus", "tokens": ["Meint", "ihr", ",", "Ster\u00b7ne", "l\u00f6\u00b7schen", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Morgen strahlt ins Haus?", "tokens": ["Wenn", "der", "Mor\u00b7gen", "strahlt", "ins", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6her spielen Herzensflammen", "tokens": ["H\u00f6\u00b7her", "spie\u00b7len", "Her\u00b7zens\u00b7flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der lichten Morgenblendung", "tokens": ["In", "der", "lich\u00b7ten", "Mor\u00b7gen\u00b7blen\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die schmelzen dann zusammen", "tokens": ["Und", "die", "schmel\u00b7zen", "dann", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die getrennt durch Himmelswendung,", "tokens": ["Die", "ge\u00b7trennt", "durch", "Him\u00b7mels\u00b7wen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die von ferne sich nur schauten,", "tokens": ["Die", "von", "fer\u00b7ne", "sich", "nur", "schau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu Vertrauten.", "tokens": ["Zu", "Ver\u00b7trau\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Meint ihr, Lampen l\u00f6schen aus,", "tokens": ["Meint", "ihr", ",", "Lam\u00b7pen", "l\u00f6\u00b7schen", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Sonne strahlt in's Haus?", "tokens": ["Wenn", "die", "Son\u00b7ne", "strahlt", "in's", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freier flammt das Herz im Morgen,", "tokens": ["Frei\u00b7er", "flammt", "das", "Herz", "im", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie die Lampe, die vergessen,", "tokens": ["Wie", "die", "Lam\u00b7pe", ",", "die", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil sie in dem Licht verborgen,", "tokens": ["Weil", "sie", "in", "dem", "Licht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dach und Haus entflammt vermessen,", "tokens": ["Dach", "und", "Haus", "ent\u00b7flammt", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "NN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach dann werdet ihr sie sehen,", "tokens": ["Ach", "dann", "wer\u00b7det", "ihr", "sie", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenns geschehen!", "tokens": ["Wenns", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}