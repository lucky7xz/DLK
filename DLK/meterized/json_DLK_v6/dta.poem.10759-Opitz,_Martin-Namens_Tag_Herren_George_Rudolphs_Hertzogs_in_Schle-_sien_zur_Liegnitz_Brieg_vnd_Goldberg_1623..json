{"dta.poem.10759": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Namens Tag  \n Herren George Rudolphs Hertzogs in Schle-  \n sien zur Liegnitz/ Brieg vnd Goldberg 1623.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist der Edle Tag/ an welchem billich m\u00fcssen", "tokens": ["Di\u00df", "ist", "der", "Ed\u00b7le", "Tag", "/", "an", "wel\u00b7chem", "bil\u00b7lich", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "APPR", "PRELS", "ADJD", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Violen mancherley vnd Blumen herf\u00fcr schiessen/", "tokens": ["Vi\u00b7o\u00b7len", "man\u00b7cher\u00b7ley", "vnd", "Blu\u00b7men", "her\u00b7f\u00fcr", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "KON", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An welchem billich auch auffstecket vnd au\u00dfbreit/", "tokens": ["An", "wel\u00b7chem", "bil\u00b7lich", "auch", "auffs\u00b7te\u00b7cket", "vnd", "au\u00df\u00b7breit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "ADV", "VVFIN", "KON", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Himmel seine Sonn/ die Erd\u2019 jhr gr\u00fcnes Kleid.", "tokens": ["Der", "Him\u00b7mel", "sei\u00b7ne", "Sonn", "/", "die", "Erd'", "jhr", "gr\u00fc\u00b7nes", "Kleid", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mehr Frieden/ Ehr vnd R\u00fchm seind vber einen hauffen", "tokens": ["Mehr", "Frie\u00b7den", "/", "Ehr", "vnd", "R\u00fchm", "seind", "vber", "ei\u00b7nen", "hauf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Auff Ewern Namens Tag zusammen nie gelauffen/", "tokens": ["Auff", "E\u00b7wern", "Na\u00b7mens", "Tag", "zu\u00b7sam\u00b7men", "nie", "ge\u00b7lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An welchem Tage Gott/ der K\u00e4yser/ vnser Land", "tokens": ["An", "wel\u00b7chem", "Ta\u00b7ge", "Gott", "/", "der", "K\u00e4y\u00b7ser", "/", "vn\u00b7ser", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "NN", "NN", "$(", "ART", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Befinden Lust vnd Frewd\u2019 an Ewerm F\u00fcrstenstand.", "tokens": ["Be\u00b7fin\u00b7den", "Lust", "vnd", "Fre\u00b7wd'", "an", "E\u00b7werm", "F\u00fcrs\u00b7ten\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Gott wegen Fr\u00f6migkeit/ der K\u00e4yser wegen trewen/", "tokens": ["Gott", "we\u00b7gen", "Fr\u00f6\u00b7mig\u00b7keit", "/", "der", "K\u00e4y\u00b7ser", "we\u00b7gen", "tre\u00b7wen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "ART", "NN", "APPR", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Das Land so sich nun gantz de\u00df friedens zuerfrewen", "tokens": ["Das", "Land", "so", "sich", "nun", "gantz", "de\u00df", "frie\u00b7dens", "zu\u00b7er\u00b7fre\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PRF", "ADV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von wegen welcher Gott/ der K\u00e4yser vnd auch wir", "tokens": ["Von", "we\u00b7gen", "wel\u00b7cher", "Gott", "/", "der", "K\u00e4y\u00b7ser", "vnd", "auch", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PWAT", "NN", "$(", "ART", "NN", "KON", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein jeder seine Gab Euch billich tr\u00e4get f\u00fcr.", "tokens": ["Ein", "je\u00b7der", "sei\u00b7ne", "Gab", "Euch", "bil\u00b7lich", "tr\u00e4\u00b7get", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "VVFIN", "PPER", "ADJD", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gott hat durch seine g\u00fctt\u2019 vnd sehr mildreichen Segen", "tokens": ["Gott", "hat", "durch", "sei\u00b7ne", "g\u00fctt'", "vnd", "sehr", "mil\u00b7drei\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PPOSAT", "NN", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Euch vberschwemmet gantz/ gleich wie ein fr\u00fcchtbar Regen", "tokens": ["Euch", "vber\u00b7schwem\u00b7met", "gantz", "/", "gleich", "wie", "ein", "fr\u00fccht\u00b7bar", "Re\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$(", "ADV", "KOKOM", "ART", "ADJD", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Nach grosser Sonnen hitz vons hohen Himmelsschlo\u00df", "tokens": ["Nach", "gros\u00b7ser", "Son\u00b7nen", "hitz", "vons", "ho\u00b7hen", "Him\u00b7mels\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NE", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sich miltiglich ergeust in seiner Erden scho\u00df.", "tokens": ["Sich", "mil\u00b7tig\u00b7lich", "er\u00b7geust", "in", "sei\u00b7ner", "Er\u00b7den", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der grosse K\u00e4yser auch/ damit man sehen m\u00f6chte", "tokens": ["Der", "gros\u00b7se", "K\u00e4y\u00b7ser", "auch", "/", "da\u00b7mit", "man", "se\u00b7hen", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "$(", "KOUS", "PIS", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was die Standhafftigkeit f\u00fcr gaben mit sich brechte/", "tokens": ["Was", "die", "Stand\u00b7haff\u00b7tig\u00b7keit", "f\u00fcr", "ga\u00b7ben", "mit", "sich", "brech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "VVFIN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Belohnet er die trew vnd dienst je mehr vnd mehr", "tokens": ["Be\u00b7loh\u00b7net", "er", "die", "trew", "vnd", "dienst", "je", "mehr", "vnd", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "KON", "ADV", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit hoher tewrer gnad/ mit newer wehrter Ehr.", "tokens": ["Mit", "ho\u00b7her", "tew\u00b7rer", "gnad", "/", "mit", "ne\u00b7wer", "wehr\u00b7ter", "Ehr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.21": {"text": "Im Lande wird gewi\u00df der Nahmen ewig bleiben", "tokens": ["Im", "Lan\u00b7de", "wird", "ge\u00b7wi\u00df", "der", "Nah\u00b7men", "e\u00b7wig", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den keine flucht der zeit kein Neid nicht kan vertreiben/", "tokens": ["Den", "kei\u00b7ne", "flucht", "der", "zeit", "kein", "Neid", "nicht", "kan", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "ART", "NN", "PIAT", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der g\u00fclden Fride selbst wird dessen H\u00fcter sein/", "tokens": ["Der", "g\u00fcl\u00b7den", "Fri\u00b7de", "selbst", "wird", "des\u00b7sen", "H\u00fc\u00b7ter", "sein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "PDS", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil wieder er durch Euch zu vns gekehret ein.", "tokens": ["Weil", "wie\u00b7der", "er", "durch", "Euch", "zu", "vns", "ge\u00b7keh\u00b7ret", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPR", "PPER", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wir alle/ die wir jetzt allhier in ruhe wohnen", "tokens": ["Wir", "al\u00b7le", "/", "die", "wir", "jetzt", "all\u00b7hier", "in", "ru\u00b7he", "woh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "$(", "PRELS", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Verm\u00f6gen anders nicht Euch dieses zubelohnen", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "an\u00b7ders", "nicht", "Euch", "die\u00b7ses", "zu\u00b7be\u00b7loh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "PPER", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Als mit den trewen Wunsch: Es mehre Ewern Stand", "tokens": ["Als", "mit", "den", "tre\u00b7wen", "Wunsch", ":", "Es", "meh\u00b7re", "E\u00b7wern", "Stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Von Gott die G\u00fctte/ Gnad vom K\u00e4yser/ Ruhm im Land.", "tokens": ["Von", "Gott", "die", "G\u00fct\u00b7te", "/", "Gnad", "vom", "K\u00e4y\u00b7ser", "/", "Ruhm", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$(", "NN", "APPRART", "NN", "$(", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}