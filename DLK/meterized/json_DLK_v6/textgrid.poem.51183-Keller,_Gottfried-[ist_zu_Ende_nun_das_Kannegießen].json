{"textgrid.poem.51183": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "[ist zu Ende nun das Kannegie\u00dfen]", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist zu Ende nun das Kannegie\u00dfen,", "tokens": ["Ist", "zu", "En\u00b7de", "nun", "das", "Kan\u00b7ne\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lasset euch das Trinken nicht verdrie\u00dfen;", "tokens": ["Las\u00b7set", "euch", "das", "Trin\u00b7ken", "nicht", "ver\u00b7drie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Braucht die Kannen! Ist erst Wein darin,", "tokens": ["Braucht", "die", "Kan\u00b7nen", "!", "Ist", "erst", "Wein", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VAFIN", "ADV", "NN", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wird zum alten auch das neue Zinn!", "tokens": ["Wird", "zum", "al\u00b7ten", "auch", "das", "neu\u00b7e", "Zinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}