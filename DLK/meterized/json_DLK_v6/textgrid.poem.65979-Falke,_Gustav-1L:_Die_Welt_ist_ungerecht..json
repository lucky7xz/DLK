{"textgrid.poem.65979": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Welt ist ungerecht.", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist ungerecht.", "tokens": ["Die", "Welt", "ist", "un\u00b7ge\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hans Schlau, der sich empfahl", "tokens": ["Hans", "Schlau", ",", "der", "sich", "emp\u00b7fahl"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seines Nachbars altem Tiegel,", "tokens": ["Mit", "sei\u00b7nes", "Nach\u00b7bars", "al\u00b7tem", "Tie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn setzt man, weil er stahl,", "tokens": ["Ihn", "setzt", "man", ",", "weil", "er", "stahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Flugs hinter Schloss und Riegel.", "tokens": ["Flugs", "hin\u00b7ter", "Schloss", "und", "Rie\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ihr, die ihr so oft", "tokens": ["Und", "ihr", ",", "die", "ihr", "so", "oft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit eurer Fingerfertigkeit", "tokens": ["Mit", "eu\u00b7rer", "Fin\u00b7ger\u00b7fer\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem lieben Gott die Zeit,", "tokens": ["Dem", "lie\u00b7ben", "Gott", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Den Nachbarn ihre Ruh' gestohlen,", "tokens": ["Den", "Nach\u00b7barn", "ih\u00b7re", "Ruh'", "ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ihr lauft noch frei umher.", "tokens": ["Ihr", "lauft", "noch", "frei", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "M\u00f6cht' euch der Satan holen.", "tokens": ["M\u00f6cht'", "euch", "der", "Sa\u00b7tan", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}