{"textgrid.poem.36901": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeines Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich komme vor dein Angesicht,", "tokens": ["Ich", "kom\u00b7me", "vor", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verwirf, o Gott, mein Flehen nicht;", "tokens": ["Ver\u00b7wirf", ",", "o", "Gott", ",", "mein", "Fle\u00b7hen", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergib mir alle meine Schuld,", "tokens": ["Ver\u00b7gib", "mir", "al\u00b7le", "mei\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du Gott der Gnaden und Geduld.", "tokens": ["Du", "Gott", "der", "Gna\u00b7den", "und", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schaff du ein reines Herz in mir,", "tokens": ["Schaff", "du", "ein", "rei\u00b7nes", "Herz", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Herz voll Lieb und Furcht zu dir,", "tokens": ["Ein", "Herz", "voll", "Lieb", "und", "Furcht", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herz voll Demut, Preis und Dank,", "tokens": ["Ein", "Herz", "voll", "De\u00b7mut", ",", "Preis", "und", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ruhig Herz mein Lebenlang.", "tokens": ["Ein", "ru\u00b7hig", "Herz", "mein", "Le\u00b7ben\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sei mein Besch\u00fctzer in Gefahr;", "tokens": ["Sei", "mein", "Be\u00b7sch\u00fct\u00b7zer", "in", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich harre deiner immerdar.", "tokens": ["Ich", "har\u00b7re", "dei\u00b7ner", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist wohl ein \u00dcbel, das mich schreckt,", "tokens": ["Ist", "wohl", "ein", "\u00dc\u00b7bel", ",", "das", "mich", "schreckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn deine Rechte mich bedeckt?", "tokens": ["Wenn", "dei\u00b7ne", "Rech\u00b7te", "mich", "be\u00b7deckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin ja, Herr, in deiner Hand.", "tokens": ["Ich", "bin", "ja", ",", "Herr", ",", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von dir empfing ich den Verstand;", "tokens": ["Von", "dir", "emp\u00b7fing", "ich", "den", "Ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhalt ihn mir, o Herr! mein Hort,", "tokens": ["Er\u00b7halt", "ihn", "mir", ",", "o", "Herr", "!", "mein", "Hort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und st\u00e4rk ihn durch dein g\u00f6ttlich Wort.", "tokens": ["Und", "st\u00e4rk", "ihn", "durch", "dein", "g\u00f6tt\u00b7lich", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df, deines Namens mich zu freun,", "tokens": ["La\u00df", ",", "dei\u00b7nes", "Na\u00b7mens", "mich", "zu", "freun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn stets vor meinen Augen sein.", "tokens": ["Ihn", "stets", "vor", "mei\u00b7nen", "Au\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df, meines Glaubens mich zu freun,", "tokens": ["La\u00df", ",", "mei\u00b7nes", "Glau\u00b7bens", "mich", "zu", "freun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn stets durch Liebe t\u00e4tig sein.", "tokens": ["Ihn", "stets", "durch", "Lie\u00b7be", "t\u00e4\u00b7tig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das ist mein Gl\u00fcck, was du mich lehrst.", "tokens": ["Das", "ist", "mein", "Gl\u00fcck", ",", "was", "du", "mich", "lehrst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PWS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sei mein Gl\u00fcck, da\u00df ich zuerst", "tokens": ["Das", "sei", "mein", "Gl\u00fcck", ",", "da\u00df", "ich", "zu\u00b7erst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach deinem Reiche tracht, und treu", "tokens": ["Nach", "dei\u00b7nem", "Rei\u00b7che", "tracht", ",", "und", "treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NE", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen meinen Pflichten sei!", "tokens": ["In", "al\u00b7len", "mei\u00b7nen", "Pflich\u00b7ten", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich bin zu schwach aus eigner Kraft", "tokens": ["Ich", "bin", "zu", "schwach", "aus", "eig\u00b7ner", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Siege meiner Leidenschaft;", "tokens": ["Zum", "Sie\u00b7ge", "mei\u00b7ner", "Lei\u00b7den\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber ziehst mit Kraft mich an,", "tokens": ["Du", "a\u00b7ber", "ziehst", "mit", "Kraft", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich den Sieg erlangen kann.", "tokens": ["Da\u00df", "ich", "den", "Sieg", "er\u00b7lan\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gib von den G\u00fctern dieser Welt", "tokens": ["Gib", "von", "den", "G\u00fc\u00b7tern", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir, Herr, so viel, als dir gef\u00e4llt;", "tokens": ["Mir", ",", "Herr", ",", "so", "viel", ",", "als", "dir", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "ADV", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gib deinem Knecht ein m\u00e4\u00dfig Teil,", "tokens": ["Gib", "dei\u00b7nem", "Knecht", "ein", "m\u00e4\u00b7\u00dfig", "Teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem Flei\u00dfe Gl\u00fcck und Heil.", "tokens": ["Zu", "sei\u00b7nem", "Flei\u00b7\u00dfe", "Gl\u00fcck", "und", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Schenkt deine Hand mir \u00dcberflu\u00df:", "tokens": ["Schenkt", "dei\u00b7ne", "Hand", "mir", "\u00dc\u00b7berf\u00b7lu\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df mich m\u00e4\u00dfig im Genu\u00df,", "tokens": ["So", "la\u00df", "mich", "m\u00e4\u00b7\u00dfig", "im", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und, d\u00fcrftge Br\u00fcder zu erfreun,", "tokens": ["Und", ",", "d\u00fcrft\u00b7ge", "Br\u00fc\u00b7der", "zu", "er\u00b7freun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich einen frohen Geber sein.", "tokens": ["Mich", "ei\u00b7nen", "fro\u00b7hen", "Ge\u00b7ber", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Gib mir Gesundheit, und verleih,", "tokens": ["Gib", "mir", "Ge\u00b7sund\u00b7heit", ",", "und", "ver\u00b7leih", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "KON", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich sie n\u00fctz, und dankbar sei,", "tokens": ["Da\u00df", "ich", "sie", "n\u00fctz", ",", "und", "dank\u00b7bar", "sei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nie, aus Liebe gegen sie,", "tokens": ["Und", "nie", ",", "aus", "Lie\u00b7be", "ge\u00b7gen", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich zaghaft einer Pflicht entzieh.", "tokens": ["Mich", "zag\u00b7haft", "ei\u00b7ner", "Pflicht", "ent\u00b7zieh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Erwecke mir stets einen Freund,", "tokens": ["Er\u00b7we\u00b7cke", "mir", "stets", "ei\u00b7nen", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der's treu mit meiner Wohlfahrt meint,", "tokens": ["Der's", "treu", "mit", "mei\u00b7ner", "Wohl\u00b7fahrt", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit mir in deiner Furcht sich \u00fcbt,", "tokens": ["Mit", "mir", "in", "dei\u00b7ner", "Furcht", "sich", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir Rat und Trost und Beispiel gibt.", "tokens": ["Mir", "Rat", "und", "Trost", "und", "Bei\u00b7spiel", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Bestimmst du mir ein l\u00e4ngres Ziel,", "tokens": ["Be\u00b7stimmst", "du", "mir", "ein", "l\u00e4ng\u00b7res", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und werden meiner Tage viel:", "tokens": ["Und", "wer\u00b7den", "mei\u00b7ner", "Ta\u00b7ge", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df, Gott, meine Zuversicht,", "tokens": ["So", "la\u00df", ",", "Gott", ",", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00df mich auch im Alter nicht.", "tokens": ["Ver\u00b7la\u00df", "mich", "auch", "im", "Al\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Und wird sich einst mein Ende nahn:", "tokens": ["Und", "wird", "sich", "einst", "mein", "En\u00b7de", "nahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So nimm dich meiner herzlich an,", "tokens": ["So", "nimm", "dich", "mei\u00b7ner", "herz\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sei durch Christum, deinen Sohn,", "tokens": ["Und", "sei", "durch", "Chris\u00b7tum", ",", "dei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schirm, mein Schild und gro\u00dfer Lohn!", "tokens": ["Mein", "Schirm", ",", "mein", "Schild", "und", "gro\u00b7\u00dfer", "Lohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}