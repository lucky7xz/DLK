{"dta.poem.996": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Drei weisen kennt vom dorf der bl\u00f6de knabe               ", "tokens": ["Drei", "wei\u00b7sen", "kennt", "vom", "dorf", "der", "bl\u00f6\u00b7de", "kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "VVINF", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die wenn er kommt sich st&#228;ndig wiederholen               ", "tokens": ["Die", "wenn", "er", "kommt", "sich", "st", "&#228;", "ndig", "wie\u00b7der\u00b7ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "$(", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die eine wie der v&#228;ter hauch vom grabe               ", "tokens": ["Die", "ei\u00b7ne", "wie", "der", "v", "&#228;", "ter", "hauch", "vom", "gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "PIS", "KOKOM", "ART", "XY", "$(", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die eh sie starben sich dem herrn befohlen             ", "tokens": ["Die", "eh", "sie", "star\u00b7ben", "sich", "dem", "herrn", "be\u00b7foh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "PPER", "VVFIN", "PRF", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die andre hat die tugendhafte weihe               ", "tokens": ["Die", "and\u00b7re", "hat", "die", "tu\u00b7gend\u00b7haf\u00b7te", "wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ob sie schwestern die beim spinnrad sassen               ", "tokens": ["Als", "ob", "sie", "schwes\u00b7tern", "die", "beim", "spinn\u00b7rad", "sas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "ART", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und m&#228;gde s&#228;ngen die in langer reihe               ", "tokens": ["Und", "m", "&#228;", "gde", "s", "&#228;", "ngen", "die", "in", "lan\u00b7ger", "rei\u00b7he"], "token_info": ["word", "word", "XML_entity", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "FM", "$(", "VVFIN", "ART", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vor zeiten zogen auf den abendstrassen             ", "tokens": ["Vor", "zei\u00b7ten", "zo\u00b7gen", "auf", "den", "a\u00b7bend\u00b7stras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die dritte droht &#8212; vers&#252;ndigung und rache               ", "tokens": ["Die", "drit\u00b7te", "droht", "&#8212;", "vers", "&#252;", "ndi\u00b7gung", "und", "ra\u00b7che"], "token_info": ["word", "word", "word", "XML_entity", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "$(", "NE", "$(", "NN", "KON", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit altem dolch in himmel-blauer scheide               ", "tokens": ["Mit", "al\u00b7tem", "dolch", "in", "him\u00b7mel\u00b7blau\u00b7er", "schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit mancher sippe angestammtem leide               ", "tokens": ["Mit", "man\u00b7cher", "sip\u00b7pe", "an\u00b7ge\u00b7stamm\u00b7tem", "lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit b\u00f6sen sternen &#252;ber manchem dache.             ", "tokens": ["Mit", "b\u00f6\u00b7sen", "ster\u00b7nen", "&#252;", "ber", "man\u00b7chem", "da\u00b7che", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "$(", "ADV", "PIAT", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}