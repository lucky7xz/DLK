{"textgrid.poem.23954": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Maikaterlied", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Maikater singt die ganze Nacht:", "tokens": ["Mai\u00b7ka\u00b7ter", "singt", "die", "gan\u00b7ze", "Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling ist erwacht, erwacht,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "er\u00b7wacht", ",", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Fr\u00fchling ist erwacht!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gleich einem Reis tr\u00e4gt er den Schwanz;", "tokens": ["Gleich", "ei\u00b7nem", "Reis", "tr\u00e4gt", "er", "den", "Schwanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "W\u00e4rn Bl\u00e4tter dran, so w\u00e4rs ein Kranz;", "tokens": ["W\u00e4rn", "Bl\u00e4t\u00b7ter", "dran", ",", "so", "w\u00e4rs", "ein", "Kranz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er fl\u00f6tet:", "tokens": ["Er", "fl\u00f6\u00b7tet", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Oh holde Mimamausamei,", "tokens": ["Oh", "hol\u00b7de", "Mi\u00b7ma\u00b7mau\u00b7sa\u00b7mei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wer dich zu lieben wagt, der sei", "tokens": ["Wer", "dich", "zu", "lie\u00b7ben", "wagt", ",", "der", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,", "PRELS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Get\u00f6tet!", "tokens": ["Ge\u00b7t\u00f6\u00b7tet", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Ich ganz alli-alla-allein,", "tokens": ["Ich", "ganz", "al\u00b7li\u00b7al\u00b7la\u00b7al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.11": {"text": "Nur ich darf dein Geschpusi sein,", "tokens": ["Nur", "ich", "darf", "dein", "Ge\u00b7schpu\u00b7si", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bis da\u00df es morgenr\u00f6tet.", "tokens": ["Bis", "da\u00df", "es", "mor\u00b7gen\u00b7r\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im Mai sind alle Bl\u00e4tter gr\u00fcn,", "tokens": ["Im", "Mai", "sind", "al\u00b7le", "Bl\u00e4t\u00b7ter", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Mai sind alle Kater k\u00fchn", "tokens": ["Im", "Mai", "sind", "al\u00b7le", "Ka\u00b7ter", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle J\u00fcngelinge.", "tokens": ["Und", "al\u00b7le", "J\u00fcn\u00b7ge\u00b7lin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wer ein Herz hat, fa\u00dft sich eins,", "tokens": ["Und", "wer", "ein", "Herz", "hat", ",", "fa\u00dft", "sich", "eins", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "$,", "VVFIN", "PRF", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00e4r sich keins fa\u00dft, hat auch keins;", "tokens": ["Und", "w\u00e4r", "sich", "keins", "fa\u00dft", ",", "hat", "auch", "keins", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "PIAT", "VVFIN", "$,", "VAFIN", "ADV", "PIAT", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Singe mein Kater, singe!", "tokens": ["Sin\u00b7ge", "mein", "Ka\u00b7ter", ",", "sin\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}