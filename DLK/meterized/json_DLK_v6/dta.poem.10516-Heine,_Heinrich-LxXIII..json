{"dta.poem.10516": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Auf deinen schneewei\u00dfen Busen", "tokens": ["Auf", "dei\u00b7nen", "schnee\u00b7wei\u00b7\u00dfen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hab' ich mein Haupt gelegt,", "tokens": ["Hab'", "ich", "mein", "Haupt", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und heimlich kann ich behorchen,", "tokens": ["Und", "heim\u00b7lich", "kann", "ich", "be\u00b7hor\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was dir dein Herz bewegt.", "tokens": ["Was", "dir", "dein", "Herz", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es blasen die blauen Husaren,", "tokens": ["Es", "bla\u00b7sen", "die", "blau\u00b7en", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und reiten zum Thor herein,", "tokens": ["Und", "rei\u00b7ten", "zum", "Thor", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und morgen will mich verlassen", "tokens": ["Und", "mor\u00b7gen", "will", "mich", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PRF", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Herzallerliebste mein.", "tokens": ["Die", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "mein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und willst du mich morgen verlassen", "tokens": ["Und", "willst", "du", "mich", "mor\u00b7gen", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "So bist du doch heute noch mein,", "tokens": ["So", "bist", "du", "doch", "heu\u00b7te", "noch", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und in deinen sch\u00f6nen Armen", "tokens": ["Und", "in", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich doppelt selig seyn.", "tokens": ["Will", "ich", "dop\u00b7pelt", "se\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}