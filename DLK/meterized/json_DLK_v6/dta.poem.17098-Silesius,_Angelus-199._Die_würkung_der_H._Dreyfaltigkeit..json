{"dta.poem.17098": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "199.  Die w\u00fcrkung der H. Dreyfaltigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Allmacht h\u00e4lt die Welt: die Wei\u00dfheit die regiert;", "tokens": ["Die", "All\u00b7macht", "h\u00e4lt", "die", "Welt", ":", "die", "Wei\u00df\u00b7heit", "die", "re\u00b7giert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die G\u00fctte segnet sie: wird hier nicht GOtt gesp\u00fcrt?", "tokens": ["Die", "G\u00fct\u00b7te", "seg\u00b7net", "sie", ":", "wird", "hier", "nicht", "Gott", "ge\u00b7sp\u00fcrt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "VAFIN", "ADV", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}