": ["NE", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Gefahr und alles Weh'", "tokens": ["Die", "Ge\u00b7fahr", "und", "al\u00b7les", "Weh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auff die Wellen und die See.", "tokens": ["Auff", "die", "Wel\u00b7len", "und", "die", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Sey gegr\u00fc\u00dft, du Trost in N\u00f6then,", "tokens": ["Sey", "ge\u00b7gr\u00fc\u00dft", ",", "du", "Trost", "in", "N\u00f6\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Zuflucht, unsre Ruh!", "tokens": ["Uns\u00b7re", "Zu\u00b7flucht", ",", "uns\u00b7re", "Ruh", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Mawer, Schild, Musqueten,", "tokens": ["Uns\u00b7re", "Ma\u00b7wer", ",", "Schild", ",", "Mus\u00b7que\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwerdt und Schirm sind Gott und du,", "tokens": ["Schwerdt", "und", "Schirm", "sind", "Gott", "und", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "NN", "KON", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hundert tausent helffen sehr,", "tokens": ["Hun\u00b7dert", "tau\u00b7sent", "helf\u00b7fen", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese Heeres-Krafft hilfft mehr.", "tokens": ["Die\u00b7se", "Hee\u00b7res\u00b7Krafft", "hilfft", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Komm zu uns mit solchen Waffen,", "tokens": ["Komm", "zu", "uns", "mit", "sol\u00b7chen", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche Gott gesegnet hat,", "tokens": ["Wel\u00b7che", "Gott", "ge\u00b7seg\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verheisse, du wirst schaffen", "tokens": ["Ich", "ver\u00b7heis\u00b7se", ",", "du", "wirst", "schaf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was dein Wunsch will oder Raht,", "tokens": ["Was", "dein", "Wunsch", "will", "o\u00b7der", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und f\u00fcrwahr kein Wiederstand", "tokens": ["Und", "f\u00fcr\u00b7wahr", "kein", "Wie\u00b7der\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird bestehn f\u00fcr deiner Hand,", "tokens": ["Wird", "be\u00b7stehn", "f\u00fcr", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Alles wird be\u00e4ngstigt fliehen,", "tokens": ["Al\u00b7les", "wird", "be\u00b7\u00e4ngs\u00b7tigt", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es wird vor deinem Heer", "tokens": ["Denn", "es", "wird", "vor", "dei\u00b7nem", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bleiche Furcht und Schrecken ziehen,", "tokens": ["Blei\u00b7che", "Furcht", "und", "Schre\u00b7cken", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur da\u00df deine Gegenwehr", "tokens": ["Nur", "da\u00df", "dei\u00b7ne", "Ge\u00b7gen\u00b7wehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wider alle Macht und List", "tokens": ["Wi\u00b7der", "al\u00b7le", "Macht", "und", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einig Gott sey, wie Er ist.", "tokens": ["Ei\u00b7nig", "Gott", "sey", ",", "wie", "Er", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word"