{"textgrid.poem.53242": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Bey h\u00f6chst- sehn- und erfrewlicher Ankunfft beider ChurF\u00fcrst. Durchl. in dero Hertzogthum Preussen und Residentz-Stadt K\u00f6nigsb.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warumb trug in diesen Tagen", "tokens": ["Wa\u00b7rumb", "trug", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich die Sonn' als eine Braut?", "tokens": ["Sich", "die", "Sonn'", "als", "ei\u00b7ne", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warumb ward ihr g\u00fcldner Wagen", "tokens": ["Wa\u00b7rumb", "ward", "ihr", "g\u00fcld\u00b7ner", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Ohn Gew\u00f6lcke fast geschawt,", "tokens": ["Ohn", "Ge\u00b7w\u00f6l\u00b7cke", "fast", "ge\u00b7schawt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da die Wage doch die Nacht", "tokens": ["Da", "die", "Wa\u00b7ge", "doch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Eben gleich dem Tage macht',", "tokens": ["E\u00b7ben", "gleich", "dem", "Ta\u00b7ge", "macht'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und gemein nur fauler Regen,", "tokens": ["Und", "ge\u00b7mein", "nur", "fau\u00b7ler", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trawigkeit und Unlust f\u00fchrt?", "tokens": ["Tra\u00b7wig\u00b7keit", "und", "Un\u00b7lust", "f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warumb ward da allerwegen", "tokens": ["Wa\u00b7rumb", "ward", "da", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Vorjahrs-Lufft gesp\u00fcrt?", "tokens": ["Ei\u00b7ne", "Vor\u00b7jahr\u00b7sLufft", "ge\u00b7sp\u00fcrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was verj\u00fcngte sich die Weldt?", "tokens": ["Was", "ver\u00b7j\u00fcng\u00b7te", "sich", "die", "Weldt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaffest du es nicht, O Held,", "tokens": ["Schaf\u00b7fest", "du", "es", "nicht", ",", "O", "Held", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Churf\u00fcrst, dieser Zeiten Sonne?", "tokens": ["Chur\u00b7f\u00fcrst", ",", "die\u00b7ser", "Zei\u00b7ten", "Son\u00b7ne", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses ist dir fast gemein,", "tokens": ["Die\u00b7ses", "ist", "dir", "fast", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn k\u00f6mpst Du, so k\u00f6mpt auch Wonne", "tokens": ["Denn", "k\u00f6mpst", "Du", ",", "so", "k\u00f6mpt", "auch", "Won\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ADV", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ein ungewohnter Schein,", "tokens": ["Und", "ein", "un\u00b7ge\u00b7wohn\u00b7ter", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie es war, als dieses Land", "tokens": ["Wie", "es", "war", ",", "als", "die\u00b7ses", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "KOUS", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Herrschaft erst empfand.", "tokens": ["Dei\u00b7ne", "Herr\u00b7schaft", "erst", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dabey noch in dreyen Wochen", "tokens": ["Da\u00b7bey", "noch", "in", "drey\u00b7en", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "APPR", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Durch des Herbsts betr\u00fcbte Zeit,", "tokens": ["Durch", "des", "Herbsts", "be\u00b7tr\u00fcb\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner Sonnen-Glantz gebrochen", "tokens": ["Kei\u00b7ner", "Son\u00b7nen\u00b7Glantz", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Gew\u00f6lcke dickes Kleid,", "tokens": ["Der", "Ge\u00b7w\u00f6l\u00b7cke", "di\u00b7ckes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Man ward deiner nur gewahr", "tokens": ["Man", "ward", "dei\u00b7ner", "nur", "ge\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPOSAT", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Stracks ward alles hell und klar.", "tokens": ["Stracks", "ward", "al\u00b7les", "hell", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Lufft und Himmel musten lachen,", "tokens": ["Lufft", "und", "Him\u00b7mel", "mus\u00b7ten", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Anzudeuten, da\u00df du hier", "tokens": ["An\u00b7zu\u00b7deu\u00b7ten", ",", "da\u00df", "du", "hier"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles w\u00fcrdest fr\u00f6lich machen,", "tokens": ["Al\u00b7les", "w\u00fcr\u00b7dest", "fr\u00f6\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da\u00df ihrem Bilde, dir,", "tokens": ["Und", "da\u00df", "ih\u00b7rem", "Bil\u00b7de", ",", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$,", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonn' und Himmel und sein Heer", "tokens": ["Sonn'", "und", "Him\u00b7mel", "und", "sein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mehr als andern g\u00fcnstig w\u00e4r.", "tokens": ["Mehr", "als", "an\u00b7dern", "g\u00fcns\u00b7tig", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PIS", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jetzt an dieser Sonnen-Kertzen,", "tokens": ["Jetzt", "an", "die\u00b7ser", "Son\u00b7nen\u00b7Kert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die gleich als verliebt gebrant,", "tokens": ["Die", "gleich", "als", "ver\u00b7liebt", "ge\u00b7brant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KOKOM", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Tag, der unsern Hertzen", "tokens": ["Wird", "der", "Tag", ",", "der", "un\u00b7sern", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auffgeht, weil du k\u00f6mpst, erkant,", "tokens": ["Auff\u00b7geht", ",", "weil", "du", "k\u00f6mpst", ",", "er\u00b7kant", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die bi\u00dfher gewesen sind", "tokens": ["Die", "bi\u00df\u00b7her", "ge\u00b7we\u00b7sen", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VAPP", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Finstern\u00fc\u00df, bew\u00f6lckt und blind,", "tokens": ["Fins\u00b7ter\u00b7n\u00fc\u00df", ",", "be\u00b7w\u00f6lckt", "und", "blind", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und mit einer Nacht umbgeben,", "tokens": ["Und", "mit", "ei\u00b7ner", "Nacht", "umb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die kein Mensch beschreiben kan.", "tokens": ["Die", "kein", "Mensch", "be\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwar O unser Stern und Leben,", "tokens": ["Zwar", "O", "un\u00b7ser", "Stern", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seit da\u00df du dich weg gethan,", "tokens": ["Seit", "da\u00df", "du", "dich", "weg", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind nur umb uns allezeit", "tokens": ["Sind", "nur", "umb", "uns", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schatten \u00f6der Trawrigkeit.", "tokens": ["Schat\u00b7ten", "\u00f6\u00b7der", "Traw\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O wie ist von allen Orden,", "tokens": ["O", "wie", "ist", "von", "al\u00b7len", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' uns noch der Krieg erschreckt,", "tokens": ["Eh'", "uns", "noch", "der", "Krieg", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch dir nachgeseufftzet worden,", "tokens": ["Doch", "dir", "nach\u00b7ge\u00b7seufft\u00b7zet", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts hat Lust bey uns erweckt,", "tokens": ["Nichts", "hat", "Lust", "bey", "uns", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts kam recht an unsern Sinn,", "tokens": ["Nichts", "kam", "recht", "an", "un\u00b7sern", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles war nur oben hin.", "tokens": ["Al\u00b7les", "war", "nur", "o\u00b7ben", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wie gro\u00df war mein Verlangen!", "tokens": ["Und", "wie", "gro\u00df", "war", "mein", "Ver\u00b7lan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie bin ich so mannig mal", "tokens": ["Wie", "bin", "ich", "so", "man\u00b7nig", "mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hie das Schlo\u00df hinauff gegangen,", "tokens": ["Hie", "das", "Schlo\u00df", "hin\u00b7auff", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trawrig war es \u00fcberall", "tokens": ["Traw\u00b7rig", "war", "es", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und von Spinnen gantz