{"textgrid.poem.26300": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und der Wind hat sich aufgemacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und der Wind hat sich aufgemacht", "tokens": ["Und", "der", "Wind", "hat", "sich", "auf\u00b7ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PRF", "VVPP"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er durchw\u00fchlt die B\u00e4ume in der Nacht,", "tokens": ["Er", "durch\u00b7w\u00fchlt", "die", "B\u00e4u\u00b7me", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Kommt dahergerannt gro\u00df entfacht,", "tokens": ["Kommt", "da\u00b7her\u00b7ge\u00b7rannt", "gro\u00df", "ent\u00b7facht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und es wankt der Boden unterm Wind.", "tokens": ["Und", "es", "wankt", "der", "Bo\u00b7den", "un\u00b7term", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00f6glich auch, da\u00df es meine Sorgen sind.", "tokens": ["M\u00f6g\u00b7lich", "auch", ",", "da\u00df", "es", "mei\u00b7ne", "Sor\u00b7gen", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ach, ich ward von langem Heimweh blind.", "tokens": ["Ach", ",", "ich", "ward", "von", "lan\u00b7gem", "Heim\u00b7weh", "blind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}