{"textgrid.poem.49612": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Pastor Klops", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war Herr Klops, der sich beugete", "tokens": ["Das", "war", "Herr", "Klops", ",", "der", "sich", "beu\u00b7ge\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "NE", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor dem Herrn und dabei erzeugete", "tokens": ["Vor", "dem", "Herrn", "und", "da\u00b7bei", "er\u00b7zeu\u00b7ge\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "PAV", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So viele Kinder wie ein Kaninchen", "tokens": ["So", "vie\u00b7le", "Kin\u00b7der", "wie", "ein", "Ka\u00b7nin\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Mit seinem Ehegespons Wilhelminchen.", "tokens": ["Mit", "sei\u00b7nem", "E\u00b7he\u00b7ge\u00b7spons", "Wil\u00b7hel\u00b7min\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er studierte die Theologie zu Kiel,", "tokens": ["Er", "stu\u00b7dier\u00b7te", "die", "Theo\u00b7lo\u00b7gie", "zu", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Und lernete dort gerade so viel,", "tokens": ["Und", "ler\u00b7ne\u00b7te", "dort", "ge\u00b7ra\u00b7de", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Was f\u00fcr einen Gottesmann gen\u00fcgt,", "tokens": ["Was", "f\u00fcr", "ei\u00b7nen", "Got\u00b7tes\u00b7mann", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Der die Fehler der Menschheit siehet und r\u00fcgt", "tokens": ["Der", "die", "Feh\u00b7ler", "der", "Menschheit", "sie\u00b7het", "und", "r\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und einmal die Woche als mahnende Stimme", "tokens": ["Und", "ein\u00b7mal", "die", "Wo\u00b7che", "als", "mah\u00b7nen\u00b7de", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "KOUS", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Seinen Schleim abf\u00fchrt mit heiligem Grimme.", "tokens": ["Sei\u00b7nen", "Schleim", "ab\u00b7f\u00fchrt", "mit", "hei\u00b7li\u00b7gem", "Grim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Doch au\u00dfer dem Zweck und Ursprung der S\u00fcnden", "tokens": ["Doch", "au\u00b7\u00dfer", "dem", "Zweck", "und", "Ur\u00b7sprung", "der", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Konnte er eigentlich nichts ergr\u00fcnden.", "tokens": ["Konn\u00b7te", "er", "ei\u00b7gent\u00b7lich", "nichts", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Was braucht auch der Mensch so viel zu wissen?", "tokens": ["Was", "braucht", "auch", "der", "Mensch", "so", "viel", "zu", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Als Pastor kann man es leicht vermissen.", "tokens": ["Als", "Pas\u00b7tor", "kann", "man", "es", "leicht", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PIS", "PPER", "ADJD", "VVINF", "$."], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.15": {"text": "Man ist vernagelt nach altem Brauch,", "tokens": ["Man", "ist", "ver\u00b7na\u00b7gelt", "nach", "al\u00b7tem", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Wie viele Pastoren sonst eben auch.", "tokens": ["Wie", "vie\u00b7le", "Pas\u00b7to\u00b7ren", "sonst", "e\u00b7ben", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Die Ehe aber gedieh mit Kindern;", "tokens": ["Die", "E\u00b7he", "a\u00b7ber", "ge\u00b7dieh", "mit", "Kin\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Des Frommen Flei\u00df l\u00e4\u00dft sich nicht verhindern,", "tokens": ["Des", "From\u00b7men", "Flei\u00df", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7hin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Denn dieser ist sich gar wohl bewu\u00dft,", "tokens": ["Denn", "die\u00b7ser", "ist", "sich", "gar", "wohl", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PRF", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Da\u00df die sch\u00e4ndlichen Werke der Fleischeslust", "tokens": ["Da\u00df", "die", "sch\u00e4nd\u00b7li\u00b7chen", "Wer\u00b7ke", "der", "Flei\u00b7sches\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Im Stande der Ehe gelten als Pflicht,", "tokens": ["Im", "Stan\u00b7de", "der", "E\u00b7he", "gel\u00b7ten", "als", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "KOKOM", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Und keineswegs als S\u00fcnde nicht.", "tokens": ["Und", "kei\u00b7nes\u00b7wegs", "als", "S\u00fcn\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Aus diesen Gr\u00fcnden kam es davon,", "tokens": ["Aus", "die\u00b7sen", "Gr\u00fcn\u00b7den", "kam", "es", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.24": {"text": "Da\u00df Klops erzeugte einen Sohn,", "tokens": ["Da\u00df", "Klops", "er\u00b7zeug\u00b7te", "ei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der jetzund wieder allda zu Kiel", "tokens": ["Der", "je\u00b7tzund", "wie\u00b7der", "all\u00b7da", "zu", "Kiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADV", "APPR", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Als Theologe nicht ganz soviel", "tokens": ["Als", "Theo\u00b7lo\u00b7ge", "nicht", "ganz", "so\u00b7viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PTKNEG", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wie sein guter Vater erlernet hat.", "tokens": ["Wie", "sein", "gu\u00b7ter", "Va\u00b7ter", "er\u00b7ler\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Er folget dem Herrn als ein Kandidat", "tokens": ["Er", "fol\u00b7get", "dem", "Herrn", "als", "ein", "Kan\u00b7di\u00b7dat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Und ist verlobt mit M\u00fcllers Christinchen,", "tokens": ["Und", "ist", "ver\u00b7lobt", "mit", "M\u00fcl\u00b7lers", "Christ\u00b7in\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Mit welcher er wieder wie ein Kaninchen", "tokens": ["Mit", "wel\u00b7cher", "er", "wie\u00b7der", "wie", "ein", "Ka\u00b7nin\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "KOKOM", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.31": {"text": "Getreulich sorget, auf da\u00df die Kl\u00f6pse", "tokens": ["Ge\u00b7treu\u00b7lich", "sor\u00b7get", ",", "auf", "da\u00df", "die", "Kl\u00f6p\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "KOUS", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Sich weiterpflanzen als Kirchensch\u00f6pse.", "tokens": ["Sich", "wei\u00b7ter\u00b7pflan\u00b7zen", "als", "Kir\u00b7chen\u00b7sch\u00f6p\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KOKOM", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Der alte Klops hat auch f\u00fcnfzehn T\u00f6chter,", "tokens": ["Der", "al\u00b7te", "Klops", "hat", "auch", "f\u00fcnf\u00b7zehn", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Durch deren Anblick der Trieb der Geschlechter", "tokens": ["Durch", "de\u00b7ren", "An\u00b7blick", "der", "Trieb", "der", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.35": {"text": "In der ganzen Gemeinde erstorben ist.", "tokens": ["In", "der", "gan\u00b7zen", "Ge\u00b7mein\u00b7de", "ers\u00b7tor\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.36": {"text": "So wirkete er als Pfarrer und Christ.", "tokens": ["So", "wir\u00b7ke\u00b7te", "er", "als", "Pfar\u00b7rer", "und", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOUS", "NN", "KON", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}}}}}