{"textgrid.poem.34955": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verlorner Posten in dem Freiheitskriege,", "tokens": ["Ver\u00b7lor\u00b7ner", "Pos\u00b7ten", "in", "dem", "Frei\u00b7heits\u00b7krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hielt ich seit drei\u00dfig Jahren treulich aus.", "tokens": ["Hielt", "ich", "seit", "drei\u00b7\u00dfig", "Jah\u00b7ren", "treu\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich k\u00e4mpfe ohne Hoffnung, da\u00df ich siege,", "tokens": ["Ich", "k\u00e4mp\u00b7fe", "oh\u00b7ne", "Hoff\u00b7nung", ",", "da\u00df", "ich", "sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich wu\u00dfte, nie komm ich gesund nach Haus.", "tokens": ["Ich", "wu\u00df\u00b7te", ",", "nie", "komm", "ich", "ge\u00b7sund", "nach", "Haus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wachte Tag und Nacht \u2013 Ich konnt nicht schlafen,", "tokens": ["Ich", "wach\u00b7te", "Tag", "und", "Nacht", "\u2013", "Ich", "konnt", "nicht", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie in dem Lagerzelt der Freunde Schar \u2013", "tokens": ["Wie", "in", "dem", "La\u00b7ger\u00b7zelt", "der", "Freun\u00b7de", "Schar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(auch hielt das laute Schnarchen dieser Braven", "tokens": ["(", "auch", "hielt", "das", "lau\u00b7te", "Schnar\u00b7chen", "die\u00b7ser", "Bra\u00b7ven"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich wach, wenn ich ein bi\u00dfchen schlummrig war).", "tokens": ["Mich", "wach", ",", "wenn", "ich", "ein", "bi\u00df\u00b7chen", "schlumm\u00b7rig", "war", ")", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "PIS", "ADJD", "VAFIN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In jenen N\u00e4chten hat Langweil' ergriffen", "tokens": ["In", "je\u00b7nen", "N\u00e4ch\u00b7ten", "hat", "Lang\u00b7weil'", "er\u00b7grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "NN", "VVPP"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mich oft, auch Furcht \u2013 (nur Narren f\u00fcrchten nichts) \u2013", "tokens": ["Mich", "oft", ",", "auch", "Furcht", "\u2013", "(", "nur", "Nar\u00b7ren", "f\u00fcrch\u00b7ten", "nichts", ")", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "$,", "ADV", "NN", "$(", "$(", "ADV", "NN", "VVFIN", "PIS", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie zu verscheuchen, hab ich dann gepfiffen", "tokens": ["Sie", "zu", "ver\u00b7scheu\u00b7chen", ",", "hab", "ich", "dann", "ge\u00b7pfif\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die frechen Reime eines Spottgedichts.", "tokens": ["Die", "fre\u00b7chen", "Rei\u00b7me", "ei\u00b7nes", "Spott\u00b7ge\u00b7dichts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ja, wachsam stand ich, das Gewehr im Arme,", "tokens": ["Ja", ",", "wach\u00b7sam", "stand", "ich", ",", "das", "Ge\u00b7wehr", "im", "Ar\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nahte irgendein verd\u00e4cht'ger Gauch,", "tokens": ["Und", "nah\u00b7te", "ir\u00b7gend\u00b7ein", "ver\u00b7d\u00e4cht'\u00b7ger", "Gauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So scho\u00df ich gut und jagt ihm eine warme,", "tokens": ["So", "scho\u00df", "ich", "gut", "und", "jagt", "ihm", "ei\u00b7ne", "war\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Br\u00fchwarme Kugel in den schn\u00f6den Bauch.", "tokens": ["Br\u00fch\u00b7war\u00b7me", "Ku\u00b7gel", "in", "den", "schn\u00f6\u00b7den", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mitunter freilich mocht es sich ereignen.", "tokens": ["Mi\u00b7tun\u00b7ter", "frei\u00b7lich", "mocht", "es", "sich", "er\u00b7eig\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df solch ein schlechter Gauch gleichfalls sehr gut", "tokens": ["Da\u00df", "solch", "ein", "schlech\u00b7ter", "Gauch", "gleich\u00b7falls", "sehr", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ART", "ADJA", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu schie\u00dfen wu\u00dfte \u2013 ach, ich kann's nicht leugnen \u2013", "tokens": ["Zu", "schie\u00b7\u00dfen", "wu\u00df\u00b7te", "\u2013", "ach", ",", "ich", "kann's", "nicht", "leug\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$(", "XY", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wunden klaffen \u2013 es verstr\u00f6mt mein Blut.", "tokens": ["Die", "Wun\u00b7den", "klaf\u00b7fen", "\u2013", "es", "ver\u00b7str\u00f6mt", "mein", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ein Posten ist vakant! \u2013 Die Wunden klaffen \u2013", "tokens": ["Ein", "Pos\u00b7ten", "ist", "va\u00b7kant", "!", "\u2013", "Die", "Wun\u00b7den", "klaf\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der eine f\u00e4llt, die andern r\u00fccken nach \u2013", "tokens": ["Der", "ei\u00b7ne", "f\u00e4llt", ",", "die", "an\u00b7dern", "r\u00fc\u00b7cken", "nach", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch fall ich unbesiegt, und meine Waffen", "tokens": ["Doch", "fall", "ich", "un\u00b7be\u00b7siegt", ",", "und", "mei\u00b7ne", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "ADJD", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind nicht gebrochen \u2013 nur mein Herze brach.", "tokens": ["Sind", "nicht", "ge\u00b7bro\u00b7chen", "\u2013", "nur", "mein", "Her\u00b7ze", "brach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$(", "ADV", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}