{"dta.poem.7430": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Anastasius Gr\u00fcn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein hei\u00df Gebet, befremdend wohl und neu,", "tokens": ["Ein", "hei\u00df", "Ge\u00b7bet", ",", "be\u00b7frem\u00b7dend", "wohl", "und", "neu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "VVPP", "ADV", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sei, Todesengel, heut' an Dich gerichtet:", "tokens": ["Sei", ",", "To\u00b7de\u00b7sen\u00b7gel", ",", "heut'", "an", "Dich", "ge\u00b7rich\u00b7tet", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tritt in die H\u00fctte, an die harte Streu,", "tokens": ["Tritt", "in", "die", "H\u00fct\u00b7te", ",", "an", "die", "har\u00b7te", "Streu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In den Palast, und horch, wo Einer dichtet!", "tokens": ["In", "den", "Pa\u00b7last", ",", "und", "horch", ",", "wo", "Ei\u00b7ner", "dich\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVIMP", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So lang er sich und seinem Schmerze treu,", "tokens": ["So", "lang", "er", "sich", "und", "sei\u00b7nem", "Schmer\u00b7ze", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bei seinem sch\u00f6nsten Lied werd' er vernichtet!", "tokens": ["Bei", "sei\u00b7nem", "sch\u00f6ns\u00b7ten", "Lied", "werd'", "er", "ver\u00b7nich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr tausend Tote will ich Thr\u00e4nen haben;", "tokens": ["F\u00fcr", "tau\u00b7send", "To\u00b7te", "will", "ich", "Thr\u00e4\u00b7nen", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VMFIN", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch Lebende lernt' ich noch nicht begraben!", "tokens": ["Doch", "Le\u00b7ben\u00b7de", "lernt'", "ich", "noch", "nicht", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ein F\u00e4hndrich warf das Banner hin und floh,", "tokens": ["Ein", "F\u00e4hn\u00b7drich", "warf", "das", "Ban\u00b7ner", "hin", "und", "floh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hat sein Heer, halb siegreich schon, verlassen.", "tokens": ["Und", "hat", "sein", "Heer", ",", "halb", "sieg\u00b7reich", "schon", ",", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "ADJD", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber frage angsterf\u00fcllet: Wo,", "tokens": ["Ich", "a\u00b7ber", "fra\u00b7ge", "angs\u00b7ter\u00b7f\u00fcl\u00b7let", ":", "Wo", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "VVPP", "$.", "PWAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo darf ich ferner lieben oder hassen?", "tokens": ["Wo", "darf", "ich", "fer\u00b7ner", "lie\u00b7ben", "o\u00b7der", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Lied, begeistert, traurig oder froh \u2014", "tokens": ["Ein", "Lied", ",", "be\u00b7geis\u00b7tert", ",", "trau\u00b7rig", "o\u00b7der", "froh"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Am Ende wird's ein Spottlied auf den Gassen!", "tokens": ["Am", "En\u00b7de", "wird's", "ein", "Spott\u00b7lied", "auf", "den", "Gas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das wie ein Held gepanzert vorw\u00e4rts drang,", "tokens": ["Das", "wie", "ein", "Held", "ge\u00b7pan\u00b7zert", "vor\u00b7w\u00e4rts", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "VVPP", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dein Lied, auch Deines, w\u00e4r' der ", "tokens": ["Dein", "Lied", ",", "auch", "Dei\u00b7nes", ",", "w\u00e4r'", "der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PPOSAT", "$,", "VAFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O, sage: Nein! O, sage jenen Flachen,", "tokens": ["O", ",", "sa\u00b7ge", ":", "Nein", "!", "O", ",", "sa\u00b7ge", "je\u00b7nen", "Fla\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$.", "PTKANT", "$.", "NE", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df ewig Deiner Seele sie nicht wert!", "tokens": ["Da\u00df", "e\u00b7wig", "Dei\u00b7ner", "See\u00b7le", "sie", "nicht", "wert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Freiheit tr\u00e4umte j\u00fcngst noch vom Erwachen,", "tokens": ["Die", "Frei\u00b7heit", "tr\u00e4um\u00b7te", "j\u00fcngst", "noch", "vom", "Er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als Du ein \u201eneues Ostern\u201c uns bescheert \u2014", "tokens": ["Als", "Du", "ein", "\u201e", "neu\u00b7es", "Os\u00b7tern", "\u201c", "uns", "be\u00b7scheert"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "$(", "ADJA", "NN", "$(", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Behalt' das Ruder! steure fort den Nachen,", "tokens": ["Be\u00b7halt'", "das", "Ru\u00b7der", "!", "steu\u00b7re", "fort", "den", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Blitz' durch die Finsterni\u00df mit Deinem Schwert!", "tokens": ["Blitz'", "durch", "die", "Fins\u00b7ter\u00b7ni\u00df", "mit", "Dei\u00b7nem", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du wolltest in dem Rath der Sp\u00f6tter stehen?", "tokens": ["Du", "woll\u00b7test", "in", "dem", "Rath", "der", "Sp\u00f6t\u00b7ter", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich will Dich lieber auf dem Munkatsch sehen!", "tokens": ["Ich", "will", "Dich", "lie\u00b7ber", "auf", "dem", "Mun\u00b7katsch", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}