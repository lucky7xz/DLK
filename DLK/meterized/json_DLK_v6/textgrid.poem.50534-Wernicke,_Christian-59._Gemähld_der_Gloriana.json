{"textgrid.poem.50534": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "59. Gem\u00e4hld der Gloriana", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kam ", "tokens": ["Kam"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und findet man gleich in den Strahlen,", "tokens": ["Und", "fin\u00b7det", "man", "gleich", "in", "den", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die ihr ", "tokens": ["Die", "ihr"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Was alle ", "tokens": ["Was", "al\u00b7le"], "token_info": ["word", "word"], "pos": ["PWS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So weicht doch ihrem ", "tokens": ["So", "weicht", "doch", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und vor der ", "tokens": ["Und", "vor", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Gereitzt durch ", "tokens": ["Ge\u00b7reitzt", "durch"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Weil ihr ein ", "tokens": ["Weil", "ihr", "ein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "So ", "tokens": ["So"], "token_info": ["word"], "pos": ["ADV"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Was vormals ", "tokens": ["Was", "vor\u00b7mals"], "token_info": ["word", "word"], "pos": ["PWS", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Hergegen ", "tokens": ["Her\u00b7ge\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Zwar eine ", "tokens": ["Zwar", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Die selber, welche sie im ", "tokens": ["Die", "sel\u00b7ber", ",", "wel\u00b7che", "sie", "im"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "PRELS", "PPER", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die m\u00fcssen doch in ihr der ", "tokens": ["Die", "m\u00fcs\u00b7sen", "doch", "in", "ihr", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "PPOSAT", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Weil alle Welt mit mir gesteht,", "tokens": ["Weil", "al\u00b7le", "Welt", "mit", "mir", "ge\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dass niemand \u00fcber diesen geht,", "tokens": ["Dass", "nie\u00b7mand", "\u00fc\u00b7ber", "die\u00b7sen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den man ", "tokens": ["Den", "man"], "token_info": ["word", "word"], "pos": ["ART", "PIS"], "meter": "+-", "measure": "trochaic.single"}}}}}