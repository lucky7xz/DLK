{"textgrid.poem.36907": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Der Schutz der Kirche", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Christus seine Kirche sch\u00fctzt:", "tokens": ["Wenn", "Chris\u00b7tus", "sei\u00b7ne", "Kir\u00b7che", "sch\u00fctzt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So mag die H\u00f6lle w\u00fcten.", "tokens": ["So", "mag", "die", "H\u00f6l\u00b7le", "w\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er, der zur Rechten Gottes sitzt,", "tokens": ["Er", ",", "der", "zur", "Rech\u00b7ten", "Got\u00b7tes", "sitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat Macht, ihr zu gebieten.", "tokens": ["Hat", "Macht", ",", "ihr", "zu", "ge\u00b7bie\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er ist mit H\u00fclfe nah;", "tokens": ["Er", "ist", "mit", "H\u00fcl\u00b7fe", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn er gebeut, steht's da.", "tokens": ["Wenn", "er", "ge\u00b7beut", ",", "steht's", "da", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sch\u00fctzet seinen Ruhm,", "tokens": ["Er", "sch\u00fct\u00b7zet", "sei\u00b7nen", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und h\u00e4lt das Christentum:", "tokens": ["Und", "h\u00e4lt", "das", "Chris\u00b7ten\u00b7tum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Mag doch die H\u00f6lle w\u00fcten!", "tokens": ["Mag", "doch", "die", "H\u00f6l\u00b7le", "w\u00fc\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gott sieht die F\u00fcrsten auf dem Thron", "tokens": ["Gott", "sieht", "die", "F\u00fcrs\u00b7ten", "auf", "dem", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wider ihn emp\u00f6ren;", "tokens": ["Sich", "wi\u00b7der", "ihn", "em\u00b7p\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn den Gesalbten, seinen Sohn,", "tokens": ["Denn", "den", "Ge\u00b7salb\u00b7ten", ",", "sei\u00b7nen", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den wollen sie nicht ehren.", "tokens": ["Den", "wol\u00b7len", "sie", "nicht", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sch\u00e4men sich des Worts,", "tokens": ["Sie", "sch\u00e4\u00b7men", "sich", "des", "Worts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des Heilands, unsers Horts;", "tokens": ["Des", "Hei\u00b7lands", ",", "un\u00b7sers", "Horts", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Kreuz ist selbst ihr Spott;", "tokens": ["Sein", "Kreuz", "ist", "selbst", "ihr", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Doch ihrer lachet Gott.", "tokens": ["Doch", "ih\u00b7rer", "la\u00b7chet", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Sie m\u00f6gen sich emp\u00f6ren!", "tokens": ["Sie", "m\u00f6\u00b7gen", "sich", "em\u00b7p\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Frevler mag die Wahrheit schm\u00e4hn;", "tokens": ["Der", "Frev\u00b7ler", "mag", "die", "Wahr\u00b7heit", "schm\u00e4hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns kann er sie nicht rauben.", "tokens": ["Uns", "kann", "er", "sie", "nicht", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Unchrist mag ihr widerstehn;", "tokens": ["Der", "Un\u00b7christ", "mag", "ihr", "wi\u00b7der\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir halten fest am Glauben.", "tokens": ["Wir", "hal\u00b7ten", "fest", "am", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gelobt sei Jesus Christ!", "tokens": ["Ge\u00b7lobt", "sei", "Je\u00b7sus", "Christ", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wer hier sein J\u00fcnger ist,", "tokens": ["Wer", "hier", "sein", "J\u00fcn\u00b7ger", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Wort von Herzen h\u00e4lt,", "tokens": ["Sein", "Wort", "von", "Her\u00b7zen", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dem kann die ganze Welt", "tokens": ["Dem", "kann", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Seligkeit nicht rauben.", "tokens": ["Die", "Se\u00b7lig\u00b7keit", "nicht", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf, Christen! die ihr ihm vertraut,", "tokens": ["Auf", ",", "Chris\u00b7ten", "!", "die", "ihr", "ihm", "ver\u00b7traut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$.", "PRELS", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00dft euch kein Drohn erschrecken!", "tokens": ["La\u00dft", "euch", "kein", "Drohn", "er\u00b7schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Gott, der von dem Himmel schaut,", "tokens": ["Der", "Gott", ",", "der", "von", "dem", "Him\u00b7mel", "schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird uns gewi\u00df bedecken.", "tokens": ["Wird", "uns", "ge\u00b7wi\u00df", "be\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Herr Herr Zebaoth", "tokens": ["Der", "Herr", "Herr", "Ze\u00b7bao\u00b7th"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "H\u00e4lt \u00fcber sein Gebot,", "tokens": ["H\u00e4lt", "\u00fc\u00b7ber", "sein", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gibt uns Geduld in Not,", "tokens": ["Gibt", "uns", "Ge\u00b7duld", "in", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und Kraft und Mut im Tod;", "tokens": ["Und", "Kraft", "und", "Mut", "im", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Was will uns denn erschrecken?", "tokens": ["Was", "will", "uns", "denn", "er\u00b7schre\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}