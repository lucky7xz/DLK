{"dta.poem.18548": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "L iebes-Brieff an eine  \n S\u00e4ngerin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Verwundere dich nicht/ du Sch\u00f6ne/ _ _ _", "tokens": ["Ver\u00b7wun\u00b7de\u00b7re", "dich", "nicht", "/", "du", "Sch\u00f6\u00b7ne", "/", "_", "_", "_"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$(", "PPER", "NN", "$(", "XY", "XY", "XY"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df eine frembde Hand dir was bekantes schreibet/", "tokens": ["Da\u00df", "ei\u00b7ne", "fremb\u00b7de", "Hand", "dir", "was", "be\u00b7kan\u00b7tes", "schrei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PIS", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ich auch ein Knecht der s\u00fcssen Herrschafft bin/", "tokens": ["Und", "da\u00df", "ich", "auch", "ein", "Knecht", "der", "s\u00fcs\u00b7sen", "Herr\u00b7schafft", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die dein beliebter Mund mit lauter Hertzen treibet.", "tokens": ["Die", "dein", "be\u00b7lieb\u00b7ter", "Mund", "mit", "lau\u00b7ter", "Hert\u00b7zen", "trei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Stimme Lieblichkeit bezaubert meinen Geist/", "tokens": ["Der", "Stim\u00b7me", "Lieb\u00b7lich\u00b7keit", "be\u00b7zau\u00b7bert", "mei\u00b7nen", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und kan ihn k\u00fcnstlicher/ als schwartze Kunst beschweren:", "tokens": ["Und", "kan", "ihn", "k\u00fcnst\u00b7li\u00b7cher", "/", "als", "schwart\u00b7ze", "Kunst", "be\u00b7schwe\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "$(", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer deiner Anmuth nur ein zartes Ohr erwei\u00dft/", "tokens": ["Wer", "dei\u00b7ner", "An\u00b7muth", "nur", "ein", "zar\u00b7tes", "Ohr", "er\u00b7wei\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mu\u00df vor die Freyheit auch die Sterbe-Lieder h\u00f6ren.", "tokens": ["Mu\u00df", "vor", "die", "Frey\u00b7heit", "auch", "die", "Ster\u00b7be\u00b7Lie\u00b7der", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die art'ge Stellung mehrt noch die Vollkommenheit:", "tokens": ["Die", "art'\u00b7ge", "Stel\u00b7lung", "mehrt", "noch", "die", "Voll\u00b7kom\u00b7men\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Purpur steht dir wohl/ die Majest\u00e4tschen Minen", "tokens": ["Der", "Pur\u00b7pur", "steht", "dir", "wohl", "/", "die", "Ma\u00b7jes\u00b7t\u00e4t\u00b7schen", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sind Z\u00fcge deiner Pracht und auch der W\u00fcrdigkeit/", "tokens": ["Sind", "Z\u00fc\u00b7ge", "dei\u00b7ner", "Pracht", "und", "auch", "der", "W\u00fcr\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "KON", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn dir/ wie die Natur/ das Gl\u00fccke wolte dienen.", "tokens": ["Wenn", "dir", "/", "wie", "die", "Na\u00b7tur", "/", "das", "Gl\u00fc\u00b7cke", "wol\u00b7te", "die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "KOKOM", "ART", "NN", "$(", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zwar deine Sch\u00f6nheit r\u00fchmt kein eintz'ger \u00f6ffentlich/", "tokens": ["Zwar", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "r\u00fchmt", "kein", "eint\u00b7z'\u00b7ger", "\u00f6f\u00b7fent\u00b7lich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PIAT", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Und ob du/ oder nicht/ ein Engel seyst auff Erden/", "tokens": ["Und", "ob", "du", "/", "o\u00b7der", "nicht", "/", "ein", "En\u00b7gel", "seyst", "auff", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$(", "KON", "PTKNEG", "$(", "ART", "NN", "VAFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Doch wisse Liebes-Gluth steigt mehrmahls unter sich/", "tokens": ["Doch", "wis\u00b7se", "Lie\u00b7bes\u00b7Gluth", "steigt", "mehr\u00b7mahls", "un\u00b7ter", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VVFIN", "ADV", "APPR", "PRF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und selten in den Mund/ wenn Hertzen Redner werden.", "tokens": ["Und", "sel\u00b7ten", "in", "den", "Mund", "/", "wenn", "Hert\u00b7zen", "Red\u00b7ner", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$(", "KOUS", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn das Verschwiegenheit die sch\u00f6nste Tugend sey/", "tokens": ["Denn", "das", "Ver\u00b7schwie\u00b7gen\u00b7heit", "die", "sch\u00f6ns\u00b7te", "Tu\u00b7gend", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kan niemand leicht so wohl/ als Nebenbuhler wissen:", "tokens": ["Kan", "nie\u00b7mand", "leicht", "so", "wohl", "/", "als", "Ne\u00b7ben\u00b7buh\u00b7ler", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "ADV", "ADV", "$(", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hier sind sie keinem nicht mit einem Worte treu/", "tokens": ["Hier", "sind", "sie", "kei\u00b7nem", "nicht", "mit", "ei\u00b7nem", "Wor\u00b7te", "treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "PTKNEG", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und tadeln \u00f6ffters das/ was sie im Geiste k\u00fcssen.", "tokens": ["Und", "ta\u00b7deln", "\u00f6ff\u00b7ters", "das", "/", "was", "sie", "im", "Geis\u00b7te", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "$(", "PWS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dr\u00fcm wehlet sich mein Hertz auch diese Redekunst/", "tokens": ["Dr\u00fcm", "weh\u00b7let", "sich", "mein", "Hertz", "auch", "die\u00b7se", "Re\u00b7de\u00b7kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADV", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und dieses stumme Blat soll meine Gluth bekennen/", "tokens": ["Und", "die\u00b7ses", "stum\u00b7me", "Blat", "soll", "mei\u00b7ne", "Gluth", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Flammen strecken sich nach deiner Gegengunst/", "tokens": ["Die", "Flam\u00b7men", "stre\u00b7cken", "sich", "nach", "dei\u00b7ner", "Ge\u00b7gen\u00b7gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ach Sch\u00f6nste! la\u00df sie doch nicht ohne K\u00fchlung brennen", "tokens": ["Ach", "Sch\u00f6ns\u00b7te", "!", "la\u00df", "sie", "doch", "nicht", "oh\u00b7ne", "K\u00fch\u00b7lung", "bren\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "VVIMP", "PPER", "ADV", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich bin von Fleisch und Blut/ und du bist wundersch\u00f6n/", "tokens": ["Ich", "bin", "von", "Fleisch", "und", "Blut", "/", "und", "du", "bist", "wun\u00b7der\u00b7sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$(", "KON", "PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein Wesen und dein Thun besteht in Seltenheiten/", "tokens": ["Dein", "We\u00b7sen", "und", "dein", "Thun", "be\u00b7steht", "in", "Sel\u00b7ten\u00b7hei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und will mein Auge recht wa\u00df ungemeines sehn/", "tokens": ["Und", "will", "mein", "Au\u00b7ge", "recht", "wa\u00df", "un\u00b7ge\u00b7mei\u00b7nes", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ADJD", "VVFIN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So ist dein Mund ein Ort von tausend Lieblichkeiten:", "tokens": ["So", "ist", "dein", "Mund", "ein", "Ort", "von", "tau\u00b7send", "Lieb\u00b7lich\u00b7kei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich wei\u00df das Orpheus hier die Lehre niederlegt/", "tokens": ["Ich", "wei\u00df", "das", "Or\u00b7pheus", "hier", "die", "Leh\u00b7re", "nie\u00b7der\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ob er die B\u00e4ume gleich und Steine tantzend machet:", "tokens": ["Ob", "er", "die", "B\u00e4u\u00b7me", "gleich", "und", "Stei\u00b7ne", "tant\u00b7zend", "ma\u00b7chet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Manch Hertz ist Felsen-Art/ doch wird es leicht bewegt/", "tokens": ["Manch", "Hertz", "ist", "Fel\u00b7sen\u00b7Art", "/", "doch", "wird", "es", "leicht", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$(", "ADV", "VAFIN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn nur die Anmuht singt/ und deine Sch\u00f6nheit lachet.", "tokens": ["Wenn", "nur", "die", "An\u00b7muht", "singt", "/", "und", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was halb erstorben ist/ steigt lebend wieder auff/", "tokens": ["Was", "halb", "ers\u00b7tor\u00b7ben", "ist", "/", "steigt", "le\u00b7bend", "wie\u00b7der", "auff", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "VAFIN", "$(", "VVFIN", "ADJD", "ADV", "APPR", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Und was sich sonsten regt/ erstarrt durch deine H\u00e4nde:", "tokens": ["Und", "was", "sich", "sons\u00b7ten", "regt", "/", "er\u00b7starrt", "durch", "dei\u00b7ne", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "VVFIN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Adern kaltes Blut kriegt den erhitzten Lauff/", "tokens": ["Der", "A\u00b7dern", "kal\u00b7tes", "Blut", "kriegt", "den", "er\u00b7hitz\u00b7ten", "Lauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn du mir Feuer giebst/ und ich dir Blicke sende.", "tokens": ["Wenn", "du", "mir", "Feu\u00b7er", "giebst", "/", "und", "ich", "dir", "Bli\u00b7cke", "sen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVFIN", "$(", "KON", "PPER", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ja wie verm\u00f6gend ist nicht sanffter Saiten-Thon?", "tokens": ["Ja", "wie", "ver\u00b7m\u00f6\u00b7gend", "ist", "nicht", "sanff\u00b7ter", "Sai\u00b7ten\u00b7Thon", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "VVPP", "VAFIN", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er fesselt Thetis Reich/ das Schuppen Heer der Wellen/", "tokens": ["Er", "fes\u00b7selt", "The\u00b7tis", "Reich", "/", "das", "Schup\u00b7pen", "Heer", "der", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "$(", "ART", "NN", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die Harffen klinget kaum/ so schertzt ein Delphin schon/", "tokens": ["Die", "Harf\u00b7fen", "klin\u00b7get", "kaum", "/", "so", "schertzt", "ein", "Del\u00b7phin", "schon", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und mu\u00df sich gantz verliebt in seine Netze stellen.", "tokens": ["Und", "mu\u00df", "sich", "gantz", "ver\u00b7liebt", "in", "sei\u00b7ne", "Net\u00b7ze", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es f\u00e4llt die Grausamkeit der Crocodillen hin/", "tokens": ["Es", "f\u00e4llt", "die", "Grau\u00b7sam\u00b7keit", "der", "Cro\u00b7co\u00b7dil\u00b7len", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Music kan mit der Wuth der Elephanten streiten/", "tokens": ["Mu\u00b7sic", "kan", "mit", "der", "Wuth", "der", "E\u00b7le\u00b7phan\u00b7ten", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und ist Gewalt und List nicht die Bezwingerin/", "tokens": ["Und", "ist", "Ge\u00b7walt", "und", "List", "nicht", "die", "Be\u00b7zwin\u00b7ge\u00b7rin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "KON", "NN", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Bestehn die Stricke doch in angenehmen Saiten.", "tokens": ["Be\u00b7stehn", "die", "Stri\u00b7cke", "doch", "in", "an\u00b7ge\u00b7neh\u00b7men", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Den Thieren bleibt der Grim/ wie Tauben Gall bewust/", "tokens": ["Den", "Thie\u00b7ren", "bleibt", "der", "Grim", "/", "wie", "Tau\u00b7ben", "Gall", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "po