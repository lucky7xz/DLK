{"dta.poem.2868": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6pf' aus deinem Brunn und la\u00df auch andre sch\u00f6pfen!", "tokens": ["Du", "sch\u00f6pf'", "aus", "dei\u00b7nem", "Brunn", "und", "la\u00df", "auch", "and\u00b7re", "sch\u00f6p\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVIMP", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr sch\u00f6pfet ihn nicht aus mit Eimern, Kannen, T\u00f6pfen.", "tokens": ["Ihr", "sch\u00f6p\u00b7fet", "ihn", "nicht", "aus", "mit", "Ei\u00b7mern", ",", "Kan\u00b7nen", ",", "T\u00f6p\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch mi\u00df nicht seine Tief', und la\u00df auch andre nicht", "tokens": ["Doch", "mi\u00df", "nicht", "sei\u00b7ne", "Tie\u00b7f'", ",", "und", "la\u00df", "auch", "and\u00b7re", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,", "KON", "VVIMP", "ADV", "PIS", "PTKNEG"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihn messen, weil dadurch ihm die Quellader bricht.", "tokens": ["Ihn", "mes\u00b7sen", ",", "weil", "da\u00b7durch", "ihm", "die", "Quel\u00b7la\u00b7der", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "KOUS", "PAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Dein gottgegebnes Gut sei, dein mit Lust bese\u00dfnes,", "tokens": ["Dein", "gott\u00b7ge\u00b7geb\u00b7nes", "Gut", "sei", ",", "dein", "mit", "Lust", "be\u00b7se\u00df\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "PPOSAT", "APPR", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein dem Bed\u00fcrfnis angeme\u00dfnes, ungeme\u00dfnes.", "tokens": ["Ein", "dem", "Be\u00b7d\u00fcrf\u00b7nis", "an\u00b7ge\u00b7me\u00df\u00b7nes", ",", "un\u00b7ge\u00b7me\u00df\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}