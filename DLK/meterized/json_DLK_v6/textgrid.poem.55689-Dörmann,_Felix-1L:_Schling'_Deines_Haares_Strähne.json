{"textgrid.poem.55689": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schling' Deines Haares Str\u00e4hne", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schling' Deines Haares Str\u00e4hne", "tokens": ["Schling'", "Dei\u00b7nes", "Haa\u00b7res", "Str\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ums Haupt mir, kleine Braut,", "tokens": ["Ums", "Haupt", "mir", ",", "klei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "NN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zerk\u00fcsse die funkelnde Tr\u00e4ne,", "tokens": ["Zer\u00b7k\u00fcs\u00b7se", "die", "fun\u00b7keln\u00b7de", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die mir von den Wimpern getaut.", "tokens": ["Die", "mir", "von", "den", "Wim\u00b7pern", "ge\u00b7taut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Vom Elend hier auf Erden", "tokens": ["Vom", "E\u00b7lend", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich genug erschaut,", "tokens": ["Hab'", "ich", "ge\u00b7nug", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gen\u00fcgsam will ich werden,", "tokens": ["Ge\u00b7n\u00fcg\u00b7sam", "will", "ich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geliebte, kleine Braut!", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "klei\u00b7ne", "Braut", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich will mein Sehnen begraben,", "tokens": ["Ich", "will", "mein", "Seh\u00b7nen", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die W\u00fcnsche, so brennend und laut,", "tokens": ["Die", "W\u00fcn\u00b7sche", ",", "so", "bren\u00b7nend", "und", "laut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du sollst allein mich haben,", "tokens": ["Du", "sollst", "al\u00b7lein", "mich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geliebte, kleine Braut!", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "klei\u00b7ne", "Braut", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}