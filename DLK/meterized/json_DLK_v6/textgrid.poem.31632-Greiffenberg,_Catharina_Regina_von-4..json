{"textgrid.poem.31632": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesus / Tugend / Ehr und Gl\u00fcck / helffen mir das Jahr anfangen", "tokens": ["Je\u00b7sus", "/", "Tu\u00b7gend", "/", "Ehr", "und", "Gl\u00fcck", "/", "helf\u00b7fen", "mir", "das", "Jahr", "an\u00b7fan\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "seelig / l\u00f6blich / r\u00fchmlich / froh / da\u00df dadurch ich m\u00f6g' erlangen", "tokens": ["see\u00b7lig", "/", "l\u00f6b\u00b7lich", "/", "r\u00fchm\u00b7lich", "/", "froh", "/", "da\u00df", "da\u00b7durch", "ich", "m\u00f6g'", "er\u00b7lan\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$(", "ADJD", "$(", "ADJD", "$(", "ADJD", "$(", "KOUS", "PAV", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ruh' / Ergetzung / Trost und Freud'. Ach ich will mit diesen prangen /", "tokens": ["Ruh'", "/", "Er\u00b7get\u00b7zung", "/", "Trost", "und", "Freud'", ".", "Ach", "ich", "will", "mit", "die\u00b7sen", "pran\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$.", "ITJ", "PPER", "VMFIN", "APPR", "PDAT", "ADJA", "$("], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "wann die nunmeh