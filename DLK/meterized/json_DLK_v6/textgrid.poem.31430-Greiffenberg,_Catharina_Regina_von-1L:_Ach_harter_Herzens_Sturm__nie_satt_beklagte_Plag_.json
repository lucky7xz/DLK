{"textgrid.poem.31430": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach harter Herzens Sturm / nie satt beklagte Plag /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach harter Herzens Sturm / nie satt beklagte Plag /", "tokens": ["Ach", "har\u00b7ter", "Her\u00b7zens", "Sturm", "/", "nie", "satt", "be\u00b7klag\u00b7te", "Plag", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "NN", "$(", "ADV", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die selbste g\u00fctigkeit / als Felsen hart / anflehen /", "tokens": ["die", "selbs\u00b7te", "g\u00fc\u00b7tig\u00b7keit", "/", "als", "Fel\u00b7sen", "hart", "/", "an\u00b7fle\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KOUS", "NN", "ADJD", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "nur unbeweglich fort / zur h\u00fclff' und Rettung / sehen!", "tokens": ["nur", "un\u00b7be\u00b7weg\u00b7lich", "fort", "/", "zur", "h\u00fclff'", "und", "Ret\u00b7tung", "/", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$(", "APPRART", "NN", "KON", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der das geh\u00f6r erschuff / erh\u00f6rt nicht was ich sag /", "tokens": ["der", "das", "ge\u00b7h\u00f6r", "er\u00b7schuff", "/", "er\u00b7h\u00f6rt", "nicht", "was", "ich", "sag", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKVZ", "$(", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "wann Herz-au\u00dfsch\u00fcttend' ich mein Noht und Elend klag /", "tokens": ["wann", "Her\u00b7zau\u00df\u00b7sch\u00fct\u00b7tend'", "ich", "mein", "Noht", "und", "E\u00b7lend", "klag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wann meiner seuffzer wind sie durch die Wolken wehen /", "tokens": ["wann", "mei\u00b7ner", "seuff\u00b7zer", "wind", "sie", "durch", "die", "Wol\u00b7ken", "we\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und hoff / es werd zu ruck / Rath / gnad / und that mitgehen:", "tokens": ["und", "hoff", "/", "es", "werd", "zu", "ruck", "/", "Rath", "/", "gnad", "/", "und", "that", "mit\u00b7ge\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VAFIN", "APPR", "NN", "$(", "NN", "$(", "NN", "$(", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so sp\u00fcr' ich leider nichts / die Plag w\u00e4chst tag vor tag.", "tokens": ["so", "sp\u00fcr'", "ich", "lei\u00b7der", "nichts", "/", "die", "Plag", "w\u00e4chst", "tag", "vor", "tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "$(", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach Erzerbarmer / kanst dich l\u00e4nger wol enthalten?", "tokens": ["Ach", "Er\u00b7zer\u00b7bar\u00b7mer", "/", "kanst", "dich", "l\u00e4n\u00b7ger", "wol", "ent\u00b7hal\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "VMFIN", "PRF", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich wei\u00df du f\u00fchlst selb selbst / die mich l\u00e4ngst \u00e4ngstend Noht.", "tokens": ["Ich", "wei\u00df", "du", "f\u00fchlst", "selb", "selbst", "/", "die", "mich", "l\u00e4ngst", "\u00e4ngs\u00b7tend", "Noht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADJD", "ADV", "$(", "PRELS", "PPER", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ists dir ein Herzens schmerz / so soll er dirs zerspalten /", "tokens": ["Ists", "dir", "ein", "Her\u00b7zens", "schmerz", "/", "so", "soll", "er", "dirs", "zer\u00b7spal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$(", "ADV", "VMFIN", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und du solst helffen / weil dir m\u00fcglich all's / als Gott.", "tokens": ["und", "du", "solst", "helf\u00b7fen", "/", "weil", "dir", "m\u00fcg\u00b7lich", "all's", "/", "als", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$(", "KOUS", "PPER", "ADJD", "PIS", "$(", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bin dein Aug-Apfel ich / wie l\u00e4stu mich antasten?", "tokens": ["Bin", "dein", "Aug\u00b7Ap\u00b7fel", "ich", "/", "wie", "l\u00e4s\u00b7tu", "mich", "an\u00b7tas\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "$(", "PWAV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.14": {"text": "f\u00fchlstu den schmerzen selbst / wie kanst zu heilen rasten?", "tokens": ["f\u00fchl\u00b7stu", "den", "schmer\u00b7zen", "selbst", "/", "wie", "kanst", "zu", "hei\u00b7len", "ras\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADV", "$(", "PWAV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}