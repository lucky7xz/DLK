{"dta.poem.12931": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die gl\u00fcckliche nachtigall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du kern der nachigallen!", "tokens": ["Du", "kern", "der", "na\u00b7chi\u00b7gal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein zustand trifft fast mit dem deinen zu:", "tokens": ["Mein", "zu\u00b7stand", "trifft", "fast", "mit", "dem", "dei\u00b7nen", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin gefangen, gleichwie du:", "tokens": ["Ich", "bin", "ge\u00b7fan\u00b7gen", ",", "gleich\u00b7wie", "du", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du singst, und ich sing auch, um dieser zu gefallen,", "tokens": ["Du", "singst", ",", "und", "ich", "sing", "auch", ",", "um", "die\u00b7ser", "zu", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADV", "$,", "KOUI", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die mir und dir die fessel angelegt.", "tokens": ["Die", "mir", "und", "dir", "die", "fes\u00b7sel", "an\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch mein verh\u00e4ngni\u00df will dir nicht in allem gleichen,", "tokens": ["Doch", "mein", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "will", "dir", "nicht", "in", "al\u00b7lem", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "APPR", "PIS", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dieweil dein lied die Sylvia bewegt;", "tokens": ["Die\u00b7weil", "dein", "lied", "die", "Syl\u00b7via", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NE", "VVFIN", "$."], "meter": "+--+----+", "measure": "dactylic.di.plus"}, "line.8": {"text": "So singest du und lebst, ich sing\u2019 und mu\u00df erbleichen.", "tokens": ["So", "sin\u00b7gest", "du", "und", "lebst", ",", "ich", "sing'", "und", "mu\u00df", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}