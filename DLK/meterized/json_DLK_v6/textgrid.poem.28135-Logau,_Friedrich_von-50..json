{"textgrid.poem.28135": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kl\u00e4ger kam und sprach: Herr Richter, ich bekenne,", "tokens": ["Ein", "Kl\u00e4\u00b7ger", "kam", "und", "sprach", ":", "Herr", "Rich\u00b7ter", ",", "ich", "be\u00b7ken\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "NN", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beklagter soll mir thun, so viel als ich benenne.", "tokens": ["Be\u00b7klag\u00b7ter", "soll", "mir", "thun", ",", "so", "viel", "als", "ich", "be\u00b7nen\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,", "ADV", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Richter sprach: So schau und gibs, Beklagter, hin,", "tokens": ["Der", "Rich\u00b7ter", "sprach", ":", "So", "schau", "und", "gibs", ",", "Be\u00b7klag\u00b7ter", ",", "hin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "PTKVZ", "KON", "VVFIN", "$,", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df du von Schulden los, und ich vom richten bin.", "tokens": ["Da\u00df", "du", "von", "Schul\u00b7den", "los", ",", "und", "ich", "vom", "rich\u00b7ten", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKVZ", "$,", "KON", "PPER", "APPRART", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Beklagter sprach: Ich kan zwar keine Schuld gestehen,", "tokens": ["Be\u00b7klag\u00b7ter", "sprach", ":", "Ich", "kan", "zwar", "kei\u00b7ne", "Schuld", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch geb ich halbes hin, dem zancken zu entgehen.", "tokens": ["Doch", "geb", "ich", "hal\u00b7bes", "hin", ",", "dem", "zan\u00b7cken", "zu", "ent\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,", "PRELS", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer besser richten kan, der richte dr\u00fcber frey,", "tokens": ["Wer", "bes\u00b7ser", "rich\u00b7ten", "kan", ",", "der", "rich\u00b7te", "dr\u00fc\u00b7ber", "frey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer unter dreyen hier der Allerkl\u00fcgste sey.", "tokens": ["Wer", "un\u00b7ter", "drey\u00b7en", "hier", "der", "Al\u00b7ler\u00b7kl\u00fcgs\u00b7te", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "CARD", "ADV", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}