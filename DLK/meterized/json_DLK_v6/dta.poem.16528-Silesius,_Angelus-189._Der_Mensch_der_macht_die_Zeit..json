{"dta.poem.16528": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "189.  Der Mensch der macht die Zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du selber machst die Zeit: das Uhrwerk sind die sinnen:", "tokens": ["Du", "sel\u00b7ber", "machst", "die", "Zeit", ":", "das", "Uhr\u00b7werk", "sind", "die", "sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hemstu die Unruh nur/ so ist die Zeit von hinnen.", "tokens": ["Hems\u00b7tu", "die", "Un\u00b7ruh", "nur", "/", "so", "ist", "die", "Zeit", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$(", "ADV", "VAFIN", "ART", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}