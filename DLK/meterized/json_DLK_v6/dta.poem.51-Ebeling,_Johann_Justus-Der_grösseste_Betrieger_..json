{"dta.poem.51": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  gr\u00f6sseste Betrieger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist voller List, bespannt mit\nNez und Strikken,", "tokens": ["Die", "Welt", "ist", "vol\u00b7ler", "List", ",", "be\u00b7spannt", "mit", "Nez", "und", "Strik\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Mensche suchet stets den andern", "tokens": ["Ein", "Men\u00b7sche", "su\u00b7chet", "stets", "den", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man irrt nicht, wenn man sagt, da\u00df die Betrie-", "tokens": ["Man", "irrt", "nicht", ",", "wenn", "man", "sagt", ",", "da\u00df", "die", "Be\u00b7trie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Menschen gr\u00f6ste Kunst und Haupt-Bem\u00fchung", "tokens": ["Der", "Men\u00b7schen", "gr\u00f6s\u00b7te", "Kunst", "und", "Haup\u00b7tBe\u00b7m\u00fc\u00b7hung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch in der grossen Zahl, die sich auf Trug befleis-", "tokens": ["Doch", "in", "der", "gros\u00b7sen", "Zahl", ",", "die", "sich", "auf", "Trug", "be\u00b7fleis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan ein Scheinheiliger, ein Erzbetrieger heissen.", "tokens": ["Kan", "ein", "Schein\u00b7hei\u00b7li\u00b7ger", ",", "ein", "Erz\u00b7be\u00b7trie\u00b7ger", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.7": {"text": "In dem er sich verstellt in falscher Fr\u00f6mmigkeit,", "tokens": ["In", "dem", "er", "sich", "ver\u00b7stellt", "in", "fal\u00b7scher", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "H\u00fcllt er ein Wolfes Herz ins sanfte Schafeskleid,", "tokens": ["H\u00fcllt", "er", "ein", "Wol\u00b7fes", "Herz", "ins", "sanf\u00b7te", "Scha\u00b7fes\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In dem er heilig prahlt, in Andacht feurig schei-", "tokens": ["In", "dem", "er", "hei\u00b7lig", "prahlt", ",", "in", "An\u00b7dacht", "feu\u00b7rig", "schei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "APPR", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jm Herzen t\u00fckkisch lacht, von aussen kl\u00e4glich wei-", "tokens": ["Jm", "Her\u00b7zen", "t\u00fck\u00b7kisch", "lacht", ",", "von", "aus\u00b7sen", "kl\u00e4g\u00b7lich", "wei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$,", "APPR", "NE", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So stellt er sich vor ", "tokens": ["So", "stellt", "er", "sich", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Der doch mit seinem Aug ins Herze sehen kan.", "tokens": ["Der", "doch", "mit", "sei\u00b7nem", "Aug", "ins", "Her\u00b7ze", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er meinet da\u00df er ihm so gn\u00e4diglich ansehen,", "tokens": ["Er", "mei\u00b7net", "da\u00df", "er", "ihm", "so", "gn\u00e4\u00b7di\u00b7glich", "an\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und so erk\u00fchnt er sich, ", "tokens": ["Und", "so", "er\u00b7k\u00fchnt", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Der seine T\u00fckke sieht, und seine Falschheit kennt", "tokens": ["Der", "sei\u00b7ne", "T\u00fck\u00b7ke", "sieht", ",", "und", "sei\u00b7ne", "Falschheit", "kennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Der wei\u00df wie kalt sein Herz, obgleich die Zunge", "tokens": ["Der", "wei\u00df", "wie", "kalt", "sein", "Herz", ",", "ob\u00b7gleich", "die", "Zun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJD", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Welch sch\u00e4ndlicher Betrug! das Aug ber\u00fckken", "tokens": ["Welch", "sch\u00e4nd\u00b7li\u00b7cher", "Be\u00b7trug", "!", "das", "Aug", "be\u00b7r\u00fck\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Daf\u00fcr wir Ehrfurchts-voll uns allzeit scheuen sol-", "tokens": ["Da\u00b7f\u00fcr", "wir", "Ehr\u00b7furchts\u00b7voll", "uns", "all\u00b7zeit", "scheu\u00b7en", "sol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "PPER", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Heuchler, bildet uns, durch seinen \u00e4usren", "tokens": ["Ein", "Heuch\u00b7ler", ",", "bil\u00b7det", "uns", ",", "durch", "sei\u00b7nen", "\u00e4us\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Er sei ein frommer Mensch der ", "tokens": ["Er", "sei", "ein", "from\u00b7mer", "Mensch", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und er betriegt die Welt mit seinem eitlen Prahlen,", "tokens": ["Und", "er", "be\u00b7triegt", "die", "Welt", "mit", "sei\u00b7nem", "eit\u00b7len", "Prah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er ist ein Heiliger nur mit gemahlten Strahlen.", "tokens": ["Er", "ist", "ein", "Hei\u00b7li\u00b7ger", "nur", "mit", "ge\u00b7mahl\u00b7ten", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Einbildung n\u00e4hrt ihn, es s\u00e4he jederman,", "tokens": ["Die", "Ein\u00b7bil\u00b7dung", "n\u00e4hrt", "ihn", ",", "es", "s\u00e4\u00b7he", "je\u00b7der\u00b7man", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Jhn als den Heiligsten vom ersten Range an,", "tokens": ["Jhn", "als", "den", "Hei\u00b7ligs\u00b7ten", "vom", "ers\u00b7ten", "Ran\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "++-+---+-+-+", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Und jeder merket doch gar bald die Heuchler-Rolle,", "tokens": ["Und", "je\u00b7der", "mer\u00b7ket", "doch", "gar", "bald", "die", "Heuch\u00b7ler\u00b7Rol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Wolfsklau zeiget sich auch durch die Schafes", "tokens": ["Die", "Wolfs\u00b7klau", "zei\u00b7get", "sich", "auch", "durch", "die", "Scha\u00b7fes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und er betriegt sich selbst indem er gar vermeint,", "tokens": ["Und", "er", "be\u00b7triegt", "sich", "selbst", "in\u00b7dem", "er", "gar", "ver\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Er sei ein Heiliger, wie er von aussen scheint,", "tokens": ["Er", "sei", "ein", "Hei\u00b7li\u00b7ger", ",", "wie", "er", "von", "aus\u00b7sen", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der H\u00f6chste werde ihn um seiner Andacht willen,", "tokens": ["Der", "H\u00f6chs\u00b7te", "wer\u00b7de", "ihn", "um", "sei\u00b7ner", "An\u00b7dacht", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mit einen Seegensstrom das gierge Herze stillen.", "tokens": ["Mit", "ei\u00b7nen", "See\u00b7gens\u00b7strom", "das", "gier\u00b7ge", "Her\u00b7ze", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Er hoft und kriegt doch nichts; dieweil; der Zeba-", "tokens": ["Er", "hoft", "und", "kriegt", "doch", "nichts", ";", "die\u00b7weil", ";", "der", "Ze\u00b7ba"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PIS", "$.", "KOUS", "$.", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Den Heuchlern Ach und Weh! zum ewgen Flu-", "tokens": ["Den", "Heuch\u00b7lern", "Ach", "und", "Weh", "!", "zum", "ew\u00b7gen", "Flu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und weil sie nicht recht warm, nur laulicht sind zu", "tokens": ["Und", "weil", "sie", "nicht", "recht", "warm", ",", "nur", "lau\u00b7licht", "sind", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.34": {"text": "So speiet er sie aus, wird sie nicht lieben k\u00f6nnen.", "tokens": ["So", "spei\u00b7et", "er", "sie", "aus", ",", "wird", "sie", "nicht", "lie\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "PTKNEG", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So ist ein falscher Wahn, der \u00e4rgeste Betrug,", "tokens": ["So", "ist", "ein", "fal\u00b7scher", "Wahn", ",", "der", "\u00e4r\u00b7ges\u00b7te", "Be\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Er will gesegnet seyn und wird zulezt zum Fluch:", "tokens": ["Er", "will", "ge\u00b7seg\u00b7net", "seyn", "und", "wird", "zu\u00b7lezt", "zum", "Fluch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "KON", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mit einen Wort er sucht darin nur sein Vergn\u00fcgen", "tokens": ["Mit", "ei\u00b7nen", "Wort", "er", "sucht", "da\u00b7rin", "nur", "sein", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "PAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wie er m\u00f6g GOtt und Welt, und auch sich selbst", "tokens": ["Wie", "er", "m\u00f6g", "Gott", "und", "Welt", ",", "und", "auch", "sich", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VMFIN", "NN", "KON", "NN", "$,", "KON", "ADV", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Wer dieses recht bedenkt, der mu\u00df so gleich gestehn:", "tokens": ["Wer", "die\u00b7ses", "recht", "be\u00b7denkt", ",", "der", "mu\u00df", "so", "gleich", "ge\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "VVFIN", "$,", "ART", "VMFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wer Heuchler sehen wird, mu\u00df Erzbetrieger sehn.", "tokens": ["Wer", "Heuch\u00b7ler", "se\u00b7hen", "wird", ",", "mu\u00df", "Erz\u00b7be\u00b7trie\u00b7ger", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VAFIN", "$,", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}