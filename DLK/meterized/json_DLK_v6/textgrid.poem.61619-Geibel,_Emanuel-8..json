{"textgrid.poem.61619": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie webt so still der Sonnenschein", "tokens": ["Wie", "webt", "so", "still", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im S\u00e4ulenhof! Die F\u00e4cher", "tokens": ["Im", "S\u00e4u\u00b7len\u00b7hof", "!", "Die", "F\u00e4\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der hohen Palmen schaun herein", "tokens": ["Der", "ho\u00b7hen", "Pal\u00b7men", "schaun", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber die flachen D\u00e4cher.", "tokens": ["\u00dc\u00b7ber", "die", "fla\u00b7chen", "D\u00e4\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ein wilder Rosenbusch umzweigt,", "tokens": ["Ein", "wil\u00b7der", "Ro\u00b7sen\u00b7busch", "um\u00b7zweigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Bogentor der Halle;", "tokens": ["Das", "Bo\u00b7gen\u00b7tor", "der", "Hal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Porphyrbecken wallt und steigt", "tokens": ["Im", "Por\u00b7phyr\u00b7be\u00b7cken", "wallt", "und", "steigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Born mit leisem Schalle.", "tokens": ["Der", "Born", "mit", "lei\u00b7sem", "Schal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dort schl\u00fcrft, im Haar das rote Fes,", "tokens": ["Dort", "schl\u00fcrft", ",", "im", "Haar", "das", "ro\u00b7te", "Fes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Arm im goldnen Reife,", "tokens": ["Den", "Arm", "im", "gold\u00b7nen", "Rei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das sch\u00f6nste Kind von Melanes", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Kind", "von", "Me\u00b7la\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Rauch der Wasserpfeife.", "tokens": ["Den", "Rauch", "der", "Was\u00b7ser\u00b7pfei\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie schaut behaglich himmelan,", "tokens": ["Sie", "schaut", "be\u00b7hag\u00b7lich", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kr\u00e4uselt leichte Ringe", "tokens": ["Sie", "kr\u00e4u\u00b7selt", "leich\u00b7te", "Rin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und denkt dabei \u2013 man sieht's ihr an \u2013", "tokens": ["Und", "denkt", "da\u00b7bei", "\u2013", "man", "sieht's", "ihr", "an", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$(", "PIS", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An lauter s\u00fc\u00dfe Dinge:", "tokens": ["An", "lau\u00b7ter", "s\u00fc\u00b7\u00dfe", "Din\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "An ihren Schatz, der nach Korfu", "tokens": ["An", "ih\u00b7ren", "Schatz", ",", "der", "nach", "Kor\u00b7fu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschifft zum Weinverhandeln,", "tokens": ["Ge\u00b7schifft", "zum", "Wein\u00b7ver\u00b7han\u00b7deln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An ihren bunten Kakadu,", "tokens": ["An", "ih\u00b7ren", "bun\u00b7ten", "Ka\u00b7ka\u00b7du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Fruchtkonfekt und Mandeln", "tokens": ["An", "Frucht\u00b7kon\u00b7fekt", "und", "Man\u00b7deln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und an den Halsschmuck von Opal,", "tokens": ["Und", "an", "den", "Hals\u00b7schmuck", "von", "O\u00b7pal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den morgen in Naxia", "tokens": ["Den", "mor\u00b7gen", "in", "Na\u00b7xia"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie tragen soll zum erstenmal", "tokens": ["Sie", "tra\u00b7gen", "soll", "zum", "ers\u00b7ten\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "VMFIN", "APPRART", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Fest der Panagia.", "tokens": ["Am", "Fest", "der", "Pa\u00b7na\u00b7gia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}