{"textgrid.poem.57384": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sterbliche nur gen\u00f6ssen der Freuden froheste, reinste,", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sterbliche nur gen\u00f6ssen der Freuden froheste, reinste,", "tokens": ["Sterb\u00b7li\u00b7che", "nur", "ge\u00b7n\u00f6s\u00b7sen", "der", "Freu\u00b7den", "fro\u00b7hes\u00b7te", ",", "reins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "ART", "NN", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Sie allein die Musik?", "tokens": ["Sie", "al\u00b7lein", "die", "Mu\u00b7sik", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nicht auch die Bewohner der Leyer, oder Apollo's?", "tokens": ["Und", "nicht", "auch", "die", "Be\u00b7woh\u00b7ner", "der", "Le\u00b7yer", ",", "o\u00b7der", "A\u00b7pol\u00b7lo's", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "$,", "KON", "NE", "$."], "meter": "-+-+-+-+-+--++-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Anderer Welten umher?", "tokens": ["An\u00b7de\u00b7rer", "Wel\u00b7ten", "um\u00b7her", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Wir entlocketen nur durch mannichfaltge Ber\u00fchrung,", "tokens": ["Wir", "ent\u00b7lo\u00b7cke\u00b7ten", "nur", "durch", "man\u00b7nich\u00b7falt\u00b7ge", "Be\u00b7r\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "--+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Durch gelinderen st\u00e4rkeren Hauch,", "tokens": ["Durch", "ge\u00b7lin\u00b7de\u00b7ren", "st\u00e4r\u00b7ke\u00b7ren", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Lebende T\u00f6ne den Formen, die jenen wir bildeten? h\u00e4tten", "tokens": ["Le\u00b7ben\u00b7de", "T\u00f6\u00b7ne", "den", "For\u00b7men", ",", "die", "je\u00b7nen", "wir", "bil\u00b7de\u00b7ten", "?", "h\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PRELS", "PDAT", "PPER", "VVFIN", "$.", "VAFIN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Stimmen allein zu Gesang?", "tokens": ["Stim\u00b7men", "al\u00b7lein", "zu", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Andere sch\u00fcfen nicht auch, die Zauberhalle zu ordnen,", "tokens": ["An\u00b7de\u00b7re", "sch\u00fc\u00b7fen", "nicht", "auch", ",", "die", "Zau\u00b7ber\u00b7hal\u00b7le", "zu", "ord\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "---+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Gang und Verhalt?", "tokens": ["Gang", "und", "Ver\u00b7halt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Irrt doch nicht so! Wie wisset ihr denn, ob dort, wo es schimmert,", "tokens": ["Irrt", "doch", "nicht", "so", "!", "Wie", "wis\u00b7set", "ihr", "denn", ",", "ob", "dort", ",", "wo", "es", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "$.", "PWAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Nicht auch freue Musik?", "tokens": ["Nicht", "auch", "freu\u00b7e", "Mu\u00b7sik", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Droben nicht t\u00f6ne lautere Form? nicht hellere Lippe", "tokens": ["Dro\u00b7ben", "nicht", "t\u00f6\u00b7ne", "lau\u00b7te\u00b7re", "Form", "?", "nicht", "hel\u00b7le\u00b7re", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADJA", "ADJA", "NN", "$.", "PTKNEG", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Singend ersch\u00fcttre das Herz?", "tokens": ["Sin\u00b7gend", "er\u00b7sch\u00fctt\u00b7re", "das", "Herz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Ob man vielleicht nicht selbst zu des Haines Ger\u00e4usch, und der Weste", "tokens": ["Ob", "man", "viel\u00b7leicht", "nicht", "selbst", "zu", "des", "Hai\u00b7nes", "Ge\u00b7r\u00e4usch", ",", "und", "der", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PTKNEG", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "S\u00e4useln, stimme den rieselnden Bach?", "tokens": ["S\u00e4u\u00b7seln", ",", "stim\u00b7me", "den", "rie\u00b7seln\u00b7den", "Bach", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Zum Einkl\u00e4nge nicht bringe den Donnersturm mit dem Weltmeer?", "tokens": ["Zum", "Ein\u00b7kl\u00e4n\u00b7ge", "nicht", "brin\u00b7ge", "den", "Don\u00b7ner\u00b7sturm", "mit", "dem", "Welt\u00b7meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Die mit dem tausendstimmigen Chor?", "tokens": ["Die", "mit", "dem", "tau\u00b7sends\u00b7tim\u00b7mi\u00b7gen", "Chor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Irrt doch nicht so! Es freut nicht allein in den Sternen; es freuet", "tokens": ["Irrt", "doch", "nicht", "so", "!", "Es", "freut", "nicht", "al\u00b7lein", "in", "den", "Ster\u00b7nen", ";", "es", "freu\u00b7et"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+-+--++-+--+-", "measure": "iambic.septa.invert"}, "line.20": {"text": "Auch in dem Himmel Musik.", "tokens": ["Auch", "in", "dem", "Him\u00b7mel", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "---+--+", "measure": "iambic.di.chol"}}}}}