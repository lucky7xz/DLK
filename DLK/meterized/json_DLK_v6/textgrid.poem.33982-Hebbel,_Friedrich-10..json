{"textgrid.poem.33982": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unergr\u00fcndlicher Schmerz!", "tokens": ["Un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7cher", "Schmerz", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Knirscht' ich in vorigen Stunden:", "tokens": ["Knirscht'", "ich", "in", "vo\u00b7ri\u00b7gen", "Stun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Jetzt, mit noch blutenden Wunden,", "tokens": ["Jetzt", ",", "mit", "noch", "blu\u00b7ten\u00b7den", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Segnet und preis't dich mein Herz.", "tokens": ["Seg\u00b7net", "und", "preis't", "dich", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Alles Leben ist Raub;", "tokens": ["Al\u00b7les", "Le\u00b7ben", "ist", "Raub", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Funken, die Sonnen entstammen,", "tokens": ["Fun\u00b7ken", ",", "die", "Son\u00b7nen", "ent\u00b7stam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Lodern, das All zu durchflammen,", "tokens": ["Lo\u00b7dern", ",", "das", "All", "zu", "durch\u00b7flam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Da verschluckt sie der Staub.", "tokens": ["Da", "ver\u00b7schluckt", "sie", "der", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Nun ein heiliger Krieg!", "tokens": ["Nun", "ein", "hei\u00b7li\u00b7ger", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "H\u00f6chste und tiefste Gewalten", "tokens": ["H\u00f6chs\u00b7te", "und", "tiefs\u00b7te", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "KON", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Dr\u00e4ngen in allen Gestalten!", "tokens": ["Dr\u00e4n\u00b7gen", "in", "al\u00b7len", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Trotze, so bleibt dir der Sieg.", "tokens": ["Trot\u00b7ze", ",", "so", "bleibt", "dir", "der", "Sieg", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Thatst du in Qual und in Angst", "tokens": ["Thatst", "du", "in", "Qual", "und", "in", "Angst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Erst genug f\u00fcr dein Leben,", "tokens": ["Erst", "ge\u00b7nug", "f\u00fcr", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Werden sie selbst dich erheben,", "tokens": ["Wer\u00b7den", "sie", "selbst", "dich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie du es hoffst und verlangst.", "tokens": ["Wie", "du", "es", "hoffst", "und", "ver\u00b7langst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Greife in's All nun hinein!", "tokens": ["Grei\u00b7fe", "in's", "All", "nun", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Wie du gek\u00e4mpft und geduldet,", "tokens": ["Wie", "du", "ge\u00b7k\u00e4mpft", "und", "ge\u00b7dul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "KON", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sind dir die G\u00f6tter verschuldet,", "tokens": ["Sind", "dir", "die", "G\u00f6t\u00b7ter", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nimm dir, denn Alles ist dein!", "tokens": ["Nimm", "dir", ",", "denn", "Al\u00b7les", "ist", "dein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KON", "PIS", "VAFIN", "PPOSAT", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Nun versagen sie Nichts,", "tokens": ["Nun", "ver\u00b7sa\u00b7gen", "sie", "Nichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Als den letzten der Sterne,", "tokens": ["Als", "den", "letz\u00b7ten", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der dich in d\u00e4mmernder Ferne", "tokens": ["Der", "dich", "in", "d\u00e4m\u00b7mern\u00b7der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Kn\u00fcpft an den Urquell des Lichts.", "tokens": ["Kn\u00fcpft", "an", "den", "Ur\u00b7quell", "des", "Lichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Ihm entlocke den Blitz,", "tokens": ["Ihm", "ent\u00b7lo\u00b7cke", "den", "Blitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Der dich, dein Ird'sches verzehrend,", "tokens": ["Der", "dich", ",", "dein", "Ird'\u00b7sches", "ver\u00b7zeh\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und dich mit Feuer verkl\u00e4rend,", "tokens": ["Und", "dich", "mit", "Feu\u00b7er", "ver\u00b7kl\u00e4\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "L\u00f6s't f\u00fcr den ewigen Sitz!", "tokens": ["L\u00f6s't", "f\u00fcr", "den", "e\u00b7wi\u00b7gen", "Sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}