{"dta.poem.23646": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hannoversches Fr\u00fchlingslied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr lieben guten Herzen,", "tokens": ["Ihr", "lie\u00b7ben", "gu\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr scherztet allergernst;", "tokens": ["Ihr", "scherz\u00b7tet", "al\u00b7ler\u00b7gernst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trotz allem Leid und Schmerzen", "tokens": ["Trotz", "al\u00b7lem", "Leid", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist euch verhasst der Ernst.", "tokens": ["Ist", "euch", "ver\u00b7hasst", "der", "Ernst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Nachtigallen jagen", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "ja\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Ernst jetzt \u00fcber's Meer \u2014", "tokens": ["Den", "Ernst", "jetzt", "\u00fc\u00b7ber's", "Meer"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was solche V\u00f6gel wagen!", "tokens": ["Was", "sol\u00b7che", "V\u00f6\u00b7gel", "wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das wundert mich doch sehr.", "tokens": ["Das", "wun\u00b7dert", "mich", "doch", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}