{"dta.poem.19086": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxVIII.  Antwort an die  Stud. Theol.  auf ihr  \n an ihn gerichtetes Danck-Schreiben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.85", "no:0.14"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer kommt ein Blatt voll treuer Brude-Liebe;", "tokens": ["Hjer", "kommt", "ein", "Blatt", "voll", "treu\u00b7er", "Bru\u00b7de\u00b7Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So arm und schwach mein Trieb noch immer ist:", "tokens": ["So", "arm", "und", "schwach", "mein", "Trieb", "noch", "im\u00b7mer", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O! da\u00df der HErr ihn \u00fcber alles h\u00fcbe,", "tokens": ["O", "!", "da\u00df", "der", "Herr", "ihn", "\u00fc\u00b7ber", "al\u00b7les", "h\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KOUS", "ART", "NN", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sich noch sucht, und seiner Treu vergist!", "tokens": ["Was", "sich", "noch", "sucht", ",", "und", "sei\u00b7ner", "Treu", "ver\u00b7gist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich dancke euch von Grunde meiner Seelen,", "tokens": ["Ich", "dan\u00b7cke", "euch", "von", "Grun\u00b7de", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ihr mich oft und br\u00fcderlich geh\u00f6rt,", "tokens": ["Da\u00df", "ihr", "mich", "oft", "und", "br\u00fc\u00b7der\u00b7lich", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich suchte euch zwar keines zu verheelen,", "tokens": ["Ich", "such\u00b7te", "euch", "zwar", "kei\u00b7nes", "zu", "ver\u00b7hee\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was unsre Ruh in Christo JEsu st\u00f6hrt.", "tokens": ["Was", "uns\u00b7re", "Ruh", "in", "Chris\u00b7to", "Je\u00b7su", "st\u00f6hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich suchte euch, so viel ich selber w\u00fcste,", "tokens": ["Ich", "such\u00b7te", "euch", ",", "so", "viel", "ich", "sel\u00b7ber", "w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit redlichem Gem\u00fcthe kund zu thun:", "tokens": ["Mit", "red\u00b7li\u00b7chem", "Ge\u00b7m\u00fc\u00b7the", "kund", "zu", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Doch ist ", "tokens": ["Doch", "ist"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Den h\u00f6ret auch; denselben folget nun.", "tokens": ["Den", "h\u00f6\u00b7ret", "auch", ";", "den\u00b7sel\u00b7ben", "fol\u00b7get", "nun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "PDS", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Liebe hat sich ja so treu bewiesen", "tokens": ["Die", "Lie\u00b7be", "hat", "sich", "ja", "so", "treu", "be\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "In Erfurt schon, in Leipzig, und auch hier;", "tokens": ["In", "Er\u00b7furt", "schon", ",", "in", "Leip\u00b7zig", ",", "und", "auch", "hier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$,", "APPR", "NE", "$,", "KON", "ADV", "ADV", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Ach! w\u00fcrde sie vor alles hoch gepriesen!", "tokens": ["Ach", "!", "w\u00fcr\u00b7de", "sie", "vor", "al\u00b7les", "hoch", "ge\u00b7prie\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "PPER", "APPR", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Jtzt kommt auch noch in Jena vieles f\u00fcr.", "tokens": ["Jtzt", "kommt", "auch", "noch", "in", "Je\u00b7na", "vie\u00b7les", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "NE", "PIS", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "O! grabet doch nach einer lautern Quelle!", "tokens": ["O", "!", "gra\u00b7bet", "doch", "nach", "ei\u00b7ner", "lau\u00b7tern", "Quel\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "O! folget doch den Starcken hurtig nach!", "tokens": ["O", "!", "fol\u00b7get", "doch", "den", "Star\u00b7cken", "hur\u00b7tig", "nach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch gebet auch den Schwachen ihre Stelle,", "tokens": ["Doch", "ge\u00b7bet", "auch", "den", "Schwa\u00b7chen", "ih\u00b7re", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und geht den Weg, den JEsus selber brach.", "tokens": ["Und", "geht", "den", "Weg", ",", "den", "Je\u00b7sus", "sel\u00b7ber", "brach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Es l\u00e4st sich zwar hier keine Ordnung machen;", "tokens": ["Es", "l\u00e4st", "sich", "zwar", "hier", "kei\u00b7ne", "Ord\u00b7nung", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Es gehe denn ein solches Feuer an,", "tokens": ["Es", "ge\u00b7he", "denn", "ein", "sol\u00b7ches", "Feu\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Das, ohnbetracht der L\u00e4strer und der Schwachen,", "tokens": ["Das", ",", "ohn\u00b7be\u00b7tracht", "der", "L\u00e4st\u00b7rer", "und", "der", "Schwa\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUI", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Sich in geheim nicht l\u00e4nger halten kan.", "tokens": ["Sich", "in", "ge\u00b7heim", "nicht", "l\u00e4n\u00b7ger", "hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJD", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Doch wird der HErr euch allerseits verbinden;", "tokens": ["Doch", "wird", "der", "Herr", "euch", "al\u00b7ler\u00b7seits", "ver\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Jm Methodo, den weiland Francke schrieb,", "tokens": ["Jm", "Me\u00b7tho\u00b7do", ",", "den", "wei\u00b7land", "Fran\u00b7cke", "schrieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Wird sich gewi\u00df ein weitrer Compa\u00df finden;", "tokens": ["Wird", "sich", "ge\u00b7wi\u00df", "ein", "weit\u00b7rer", "Com\u00b7pa\u00df", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Les\u2019t selbigen mit brennendem Betrieb.", "tokens": ["Les't", "sel\u00b7bi\u00b7gen", "mit", "bren\u00b7nen\u00b7dem", "Be\u00b7trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Ich selber will ihn in der Einfalt lesen,", "tokens": ["Ich", "sel\u00b7ber", "will", "ihn", "in", "der", "Ein\u00b7falt", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und was ich seh, dasselbe meld\u2019 ich euch.", "tokens": ["Und", "was", "ich", "seh", ",", "das\u00b7sel\u00b7be", "meld'", "ich", "euch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PDAT", "NN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Der HErr verleyh uns nur sein wahres Wesen,", "tokens": ["Der", "Herr", "ver\u00b7leyh", "uns", "nur", "sein", "wah\u00b7res", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und mach uns treu in der Gedult am Reich!", "tokens": ["Und", "mach", "uns", "treu", "in", "der", "Ge\u00b7dult", "am", "Reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}