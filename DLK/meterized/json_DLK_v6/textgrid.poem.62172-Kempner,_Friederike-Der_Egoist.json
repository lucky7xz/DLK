{"textgrid.poem.62172": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Der Egoist", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und schliefest Du, Schl\u00e4fer, noch einmal so lang,", "tokens": ["Und", "schlie\u00b7fest", "Du", ",", "Schl\u00e4\u00b7fer", ",", "noch", "ein\u00b7mal", "so", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erwachen wirst Du schwer und bang", "tokens": ["Er\u00b7wa\u00b7chen", "wirst", "Du", "schwer", "und", "bang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im liebeleeren Raume \u2013", "tokens": ["Im", "lie\u00b7be\u00b7lee\u00b7ren", "Rau\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Deiner Selbstsucht Traume \u2013", "tokens": ["Aus", "Dei\u00b7ner", "Selbst\u00b7sucht", "Trau\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein \u00bbich\u00ab so traurig und so bang \u2013", "tokens": ["Dein", "\u00bb", "ich", "\u00ab", "so", "trau\u00b7rig", "und", "so", "bang", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$(", "PPER", "$(", "ADV", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und das Erwachen w\u00e4hret lang.", "tokens": ["Und", "das", "Er\u00b7wa\u00b7chen", "w\u00e4h\u00b7ret", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}