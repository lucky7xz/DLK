{"dta.poem.17798": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "41.  Wer die Sonne nicht merckt/ der ist  \n nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn erw\u00e4rmet alls/ ja auch den k\u00e4ltsten stein:", "tokens": ["Die", "Sonn", "er\u00b7w\u00e4r\u00b7met", "alls", "/", "ja", "auch", "den", "k\u00e4lts\u00b7ten", "stein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlstu die wirkung nicht/ so mustu nicht mehr seyn.", "tokens": ["F\u00fchl\u00b7stu", "die", "wir\u00b7kung", "nicht", "/", "so", "mus\u00b7tu", "nicht", "mehr", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKNEG", "$(", "ADV", "VMFIN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}