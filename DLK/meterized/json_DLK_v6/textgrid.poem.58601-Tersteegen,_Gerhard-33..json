{"textgrid.poem.58601": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Arbeit machten dir, o Jesu, meine S\u00fcnden,", "tokens": ["Ja", ",", "Ar\u00b7beit", "mach\u00b7ten", "dir", ",", "o", "Je\u00b7su", ",", "mei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "PPER", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du mu\u00dftest ihre Last an Leib und Seel' empfinden,", "tokens": ["Du", "mu\u00df\u00b7test", "ih\u00b7re", "Last", "an", "Leib", "und", "Seel'", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist, du bist es nur, der mich erl\u00f6set hat;", "tokens": ["Du", "bist", ",", "du", "bist", "es", "nur", ",", "der", "mich", "er\u00b7l\u00f6\u00b7set", "hat", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch mach' ich dir ", "tokens": ["Doch", "mach'", "ich", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ach tilge meine S\u00fcnd' vollkommen aus dem Herzen \u2013", "tokens": ["Ach", "til\u00b7ge", "mei\u00b7ne", "S\u00fcnd'", "voll\u00b7kom\u00b7men", "aus", "dem", "Her\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir machen sie nur M\u00fch und mir so bittre Schmerzen \u2013", "tokens": ["Dir", "ma\u00b7chen", "sie", "nur", "M\u00fch", "und", "mir", "so", "bitt\u00b7re", "Schmer\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "KON", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Um deinetwillen nur, weil du ein Jesus bist,", "tokens": ["Um", "dei\u00b7net\u00b7wil\u00b7len", "nur", ",", "weil", "du", "ein", "Je\u00b7sus", "bist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ich dich lieb' und ehr', wie dein Verlangen ist!", "tokens": ["Da\u00df", "ich", "dich", "lieb'", "und", "ehr'", ",", "wie", "dein", "Ver\u00b7lan\u00b7gen", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}