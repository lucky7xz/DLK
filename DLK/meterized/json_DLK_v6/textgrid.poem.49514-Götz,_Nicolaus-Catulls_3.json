{"textgrid.poem.49514": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Catulls 3", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weinet, Charitinnen, weinet Amors,", "tokens": ["Wei\u00b7net", ",", "Cha\u00b7ri\u00b7tin\u00b7nen", ",", "wei\u00b7net", "A\u00b7mors", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alles, was man artig nennet, weine.", "tokens": ["Al\u00b7les", ",", "was", "man", "ar\u00b7tig", "nen\u00b7net", ",", "wei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meines M\u00e4dchens einziges Vergn\u00fcgen,", "tokens": ["Mei\u00b7nes", "M\u00e4d\u00b7chens", "ein\u00b7zi\u00b7ges", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Meines M\u00e4dchens Sperling ist gestorben.", "tokens": ["Mei\u00b7nes", "M\u00e4d\u00b7chens", "Sper\u00b7ling", "ist", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Den es mehr, als seine Augen, liebte;", "tokens": ["Den", "es", "mehr", ",", "als", "sei\u00b7ne", "Au\u00b7gen", ",", "lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denn er war so allerliebst und artig,", "tokens": ["Denn", "er", "war", "so", "al\u00b7ler\u00b7liebst", "und", "ar\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "So verst\u00e4ndig, und so voll Empfindung,", "tokens": ["So", "ver\u00b7st\u00e4n\u00b7dig", ",", "und", "so", "voll", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Da\u00df er minder nicht sein liebes M\u00e4dchen", "tokens": ["Da\u00df", "er", "min\u00b7der", "nicht", "sein", "lie\u00b7bes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Als das M\u00e4dchen seine Mutter kannte.", "tokens": ["Als", "das", "M\u00e4d\u00b7chen", "sei\u00b7ne", "Mut\u00b7ter", "kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Nie bewegt er sich von ihrem Schoose:", "tokens": ["Nie", "be\u00b7wegt", "er", "sich", "von", "ih\u00b7rem", "Schoo\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Sondern h\u00fcpfte hie, und da, und dorten", "tokens": ["Son\u00b7dern", "h\u00fcpf\u00b7te", "hie", ",", "und", "da", ",", "und", "dor\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "KON", "ADV", "$,", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Auf dem Schoose munter auf und nieder,", "tokens": ["Auf", "dem", "Schoo\u00b7se", "mun\u00b7ter", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Ihr nur piepend, ihr alleine schmeichelnd.", "tokens": ["Ihr", "nur", "pie\u00b7pend", ",", "ihr", "al\u00b7lei\u00b7ne", "schmei\u00b7chelnd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Ach! izt wandert er die dunkle Strase,", "tokens": ["Ach", "!", "izt", "wan\u00b7dert", "er", "die", "dunk\u00b7le", "Stra\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Die man ewig nicht zur\u00fccke wandert.", "tokens": ["Die", "man", "e\u00b7wig", "nicht", "zu\u00b7r\u00fc\u00b7cke", "wan\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Drum verfluch ich, Schatten des Cocytus,", "tokens": ["Drum", "ver\u00b7fluch", "ich", ",", "Schat\u00b7ten", "des", "Co\u00b7cy\u00b7tus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "ART", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Die ihr, was nur artig ist, verschlinget,", "tokens": ["Die", "ihr", ",", "was", "nur", "ar\u00b7tig", "ist", ",", "ver\u00b7schlin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Drum verfluch ich euch, dann ihr entf\u00fchrtet,", "tokens": ["Drum", "ver\u00b7fluch", "ich", "euch", ",", "dann", "ihr", "ent\u00b7f\u00fchr\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Dann ihr stahlt mir ihn, den sch\u00f6nsten Sperling.", "tokens": ["Dann", "ihr", "stahlt", "mir", "ihn", ",", "den", "sch\u00f6ns\u00b7ten", "Sper\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "O verruchte That, o armer Sperling,", "tokens": ["O", "ver\u00b7ruch\u00b7te", "That", ",", "o", "ar\u00b7mer", "Sper\u00b7ling", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Durch dich schwellen, ach! von st\u00e4tem Weinen,", "tokens": ["Durch", "dich", "schwel\u00b7len", ",", "ach", "!", "von", "st\u00e4\u00b7tem", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ITJ", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Durch dich schwellen itzund, und verderben", "tokens": ["Durch", "dich", "schwel\u00b7len", "it\u00b7zund", ",", "und", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Meines holden M\u00e4dchens holde Augen.", "tokens": ["Mei\u00b7nes", "hol\u00b7den", "M\u00e4d\u00b7chens", "hol\u00b7de", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}