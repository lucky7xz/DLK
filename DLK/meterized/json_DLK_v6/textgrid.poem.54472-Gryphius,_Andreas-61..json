{"textgrid.poem.54472": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schau/ Jesu schau! wie ich mit Blutt vnd Stanck beflecket/", "tokens": ["Schau", "/", "Je\u00b7su", "schau", "!", "wie", "ich", "mit", "Blutt", "vnd", "Stanck", "be\u00b7fle\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "PTKVZ", "$.", "PWAV", "PPER", "APPR", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Veracht von aller Welt/ mit Traurigkeit beschwert/", "tokens": ["Ver\u00b7acht", "von", "al\u00b7ler", "Welt", "/", "mit", "Trau\u00b7rig\u00b7keit", "be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$(", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gantz Hilfflo\u00df/ Matt/ vnd Siech/ von Schmertzen bin verzehrt:", "tokens": ["Gantz", "Hilf\u00b7flo\u00df", "/", "Matt", "/", "vnd", "Siech", "/", "von", "Schmert\u00b7zen", "bin", "ver\u00b7zehrt", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "NN", "$(", "KON", "NE", "$(", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schau/ wie der Tod mich schon mit schwartzer Nacht vmbdecket.", "tokens": ["Schau", "/", "wie", "der", "Tod", "mich", "schon", "mit", "schwart\u00b7zer", "Nacht", "vmb\u00b7de\u00b7cket", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOKOM", "ART", "NN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wie offt/ wie offt hab ich den schwachen Arm gestrecket", "tokens": ["Wie", "offt", "/", "wie", "offt", "hab", "ich", "den", "schwa\u00b7chen", "Arm", "ge\u00b7stre\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$(", "KOKOM", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach deinem Gnaden-Kleid! ich bin der Gunst nicht wehrt;", "tokens": ["Nach", "dei\u00b7nem", "Gna\u00b7den\u00b7Kleid", "!", "ich", "bin", "der", "Gunst", "nicht", "wehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch bin ich gleichwol auch ein Sch\u00e4fflein deiner Heerd.", "tokens": ["Doch", "bin", "ich", "gleich\u00b7wol", "auch", "ein", "Sch\u00e4f\u00b7flein", "dei\u00b7ner", "Heerd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drumb bitt ich/ steure doch dem Teufel der mich schrecket", "tokens": ["Drumb", "bitt", "ich", "/", "steu\u00b7re", "doch", "dem", "Teu\u00b7fel", "der", "mich", "schre\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$(", "VVFIN", "ADV", "ART", "NN", "ART", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! r\u00fchre doch mich selbst/ weil ich dich selbst nicht kan", "tokens": ["Ach", "!", "r\u00fch\u00b7re", "doch", "mich", "selbst", "/", "weil", "ich", "dich", "selbst", "nicht", "kan"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "PPER", "ADV", "$(", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In meiner letzten Angst/ mit Gnaden-H\u00e4nden an:", "tokens": ["In", "mei\u00b7ner", "letz\u00b7ten", "Angst", "/", "mit", "Gna\u00b7den\u00b7H\u00e4n\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd rei\u00df mich au\u00df der Noth/ in der ich itzt verschwinde.", "tokens": ["Vnd", "rei\u00df", "mich", "au\u00df", "der", "Noth", "/", "in", "der", "ich", "itzt", "ver\u00b7schwin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo nicht: so la\u00df mich sanfft vnd selig schlaffen ein/", "tokens": ["Wo", "nicht", ":", "so", "la\u00df", "mich", "sanfft", "vnd", "se\u00b7lig", "schlaf\u00b7fen", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "ADV", "VVIMP", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd gieb da\u00df nachmals ich/ di\u00df mein Fleisch/ Haut vnd Bein", "tokens": ["Vnd", "gieb", "da\u00df", "nach\u00b7mals", "ich", "/", "di\u00df", "mein", "Fleisch", "/", "Haut", "vnd", "Bein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "KOUS", "KOUS", "PPER", "$(", "PDS", "PPOSAT", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn du mich wecken wirst/ verkl\u00e4ret wieder finde.", "tokens": ["Wenn", "du", "mich", "we\u00b7cken", "wirst", "/", "ver\u00b7kl\u00e4\u00b7ret", "wie\u00b7der", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VAFIN", "$(", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}