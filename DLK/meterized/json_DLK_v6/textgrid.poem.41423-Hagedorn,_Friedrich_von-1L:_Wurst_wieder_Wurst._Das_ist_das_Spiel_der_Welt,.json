{"textgrid.poem.41423": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wurst wieder Wurst. Das ist das Spiel der Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wurst wieder Wurst. Das ist das Spiel der Welt,", "tokens": ["Wurst", "wie\u00b7der", "Wurst", ".", "Das", "ist", "das", "Spiel", "der", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$.", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auch der Inhalt dieser Fabel.", "tokens": ["Und", "auch", "der", "In\u00b7halt", "die\u00b7ser", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Rabe, welcher sich auf einen Baum gestellt,", "tokens": ["Ein", "Ra\u00b7be", ",", "wel\u00b7cher", "sich", "auf", "ei\u00b7nen", "Baum", "ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hielt einen K\u00e4s' in seinem Schnabel.", "tokens": ["Hielt", "ei\u00b7nen", "K\u00e4s'", "in", "sei\u00b7nem", "Schna\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den K\u00e4se roch der Fuchs. Der Hunger rieth ihm bald,", "tokens": ["Den", "K\u00e4\u00b7se", "roch", "der", "Fuchs", ".", "Der", "Hun\u00b7ger", "rieth", "ihm", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NE", "$.", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem schwarzen R\u00e4uber sich zu nahen.", "tokens": ["Dem", "schwar\u00b7zen", "R\u00e4u\u00b7ber", "sich", "zu", "na\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ha! spricht er, sei gegr\u00fc\u00dft! Ist hier dein Aufenthalt?", "tokens": ["Ha", "!", "spricht", "er", ",", "sei", "ge\u00b7gr\u00fc\u00dft", "!", "Ist", "hier", "dein", "Auf\u00b7ent\u00b7halt", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$.", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Erblickt man hier die reizende Gestalt?", "tokens": ["Er\u00b7blickt", "man", "hier", "die", "rei\u00b7zen\u00b7de", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.9": {"text": "Da\u00df du gef\u00e4llst, mu\u00df, wer dich kennt, bejahen.", "tokens": ["Da\u00df", "du", "ge\u00b7f\u00e4llst", ",", "mu\u00df", ",", "wer", "dich", "kennt", ",", "be\u00b7ja\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Erlaube mir die Lust, dich jetzo recht zu sehn ...", "tokens": ["Er\u00b7lau\u00b7be", "mir", "die", "Lust", ",", "dich", "jet\u00b7zo", "recht", "zu", "sehn", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ja! der Fasan mu\u00df dir an Farbe weichen.", "tokens": ["Ja", "!", "der", "Fa\u00b7san", "mu\u00df", "dir", "an", "Far\u00b7be", "wei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Ist dein Gesang nur halb so sch\u00f6n,", "tokens": ["Ist", "dein", "Ge\u00b7sang", "nur", "halb", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So wird, an Seltenheit, dir auch kein Ph\u00f6nix gleichen.", "tokens": ["So", "wird", ",", "an", "Sel\u00b7ten\u00b7heit", ",", "dir", "auch", "kein", "Ph\u00f6\u00b7nix", "glei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "NN", "$,", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Raben t\u00e4uscht das Lob, das ihm der Falsche gab.", "tokens": ["Den", "Ra\u00b7ben", "t\u00e4uscht", "das", "Lob", ",", "das", "ihm", "der", "Fal\u00b7sche", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er kann sich nicht vor stolzer Freude fassen.", "tokens": ["Er", "kann", "sich", "nicht", "vor", "stol\u00b7zer", "Freu\u00b7de", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ich, denkt er, mu\u00df mich h\u00f6ren lassen,", "tokens": ["Ich", ",", "denkt", "er", ",", "mu\u00df", "mich", "h\u00f6\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und sperrt den Schnabel auf. Sein K\u00e4se f\u00e4llt herab,", "tokens": ["Und", "sperrt", "den", "Schna\u00b7bel", "auf", ".", "Sein", "K\u00e4\u00b7se", "f\u00e4llt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den gleich der Fuchs verschlingt. Er sagt: Mein sch\u00f6nster Rabe,", "tokens": ["Den", "gleich", "der", "Fuchs", "ver\u00b7schlingt", ".", "Er", "sagt", ":", "Mein", "sch\u00f6ns\u00b7ter", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NE", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Schmeichler lebt von dem, der ihn zu gerne h\u00f6rt,", "tokens": ["Ein", "Schmeich\u00b7ler", "lebt", "von", "dem", ",", "der", "ihn", "zu", "ger\u00b7ne", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "$,", "PRELS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie ich dir jetzt bewiesen habe.", "tokens": ["Wie", "ich", "dir", "jetzt", "be\u00b7wie\u00b7sen", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ist diese Lehre nicht zehn solcher K\u00e4se werth?", "tokens": ["Ist", "die\u00b7se", "Leh\u00b7re", "nicht", "zehn", "sol\u00b7cher", "K\u00e4\u00b7se", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "CARD", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Des Fuchses Sch\u00fcler schweigt, mit heimlichem Verlangen,", "tokens": ["Des", "Fuch\u00b7ses", "Sch\u00fc\u00b7ler", "schweigt", ",", "mit", "heim\u00b7li\u00b7chem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Den schlauen F\u00e4nger auch zu fangen.", "tokens": ["Den", "schlau\u00b7en", "F\u00e4n\u00b7ger", "auch", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der trug einst Speck nach seinem Bau,", "tokens": ["Der", "trug", "einst", "Speck", "nach", "sei\u00b7nem", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und er begegnet ihm. Wie, spricht er, H\u00fchnerfresser,", "tokens": ["Und", "er", "be\u00b7geg\u00b7net", "ihm", ".", "Wie", ",", "spricht", "er", ",", "H\u00fch\u00b7ner\u00b7fres\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "PWAV", "$,", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ist jetzo Speck dein Mahl? Du lebest zu genau,", "tokens": ["Ist", "jet\u00b7zo", "Speck", "dein", "Mahl", "?", "Du", "le\u00b7best", "zu", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Fast wie ein M\u00e4uschen lebt. Schalk, dein Geschmack war besser.", "tokens": ["Fast", "wie", "ein", "M\u00e4u\u00b7schen", "lebt", ".", "Schalk", ",", "dein", "Ge\u00b7schmack", "war", "bes\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$.", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Sieh um, in jenen Hof. Die Hennen, die dort gehn,", "tokens": ["Sieh", "um", ",", "in", "je\u00b7nen", "Hof", ".", "Die", "Hen\u00b7nen", ",", "die", "dort", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "APPR", "PDAT", "NN", "$.", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sind kl\u00fcgrer F\u00fcchse Kost: nichts sch\u00f6ners wird man sehn.", "tokens": ["Sind", "kl\u00fcg\u00b7rer", "F\u00fcch\u00b7se", "Kost", ":", "nichts", "sch\u00f6\u00b7ners", "wird", "man", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$.", "PIS", "ADV", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dich sollte wol ein solcher Anblick r\u00fchren.", "tokens": ["Dich", "soll\u00b7te", "wol", "ein", "sol\u00b7cher", "An\u00b7blick", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Allein, du bist nicht dir, noch deinem Vater, gleich.", "tokens": ["Al\u00b7lein", ",", "du", "bist", "nicht", "dir", ",", "noch", "dei\u00b7nem", "Va\u00b7ter", ",", "gleich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PTKNEG", "PPER", "$,", "ADV", "PPOSAT", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sonst warst du doch an Muth und an Erfindung reich.", "tokens": ["Sonst", "warst", "du", "doch", "an", "Muth", "und", "an", "Er\u00b7fin\u00b7dung", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "KON", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da suchte dich das Gl\u00fcck. Der Fuchs l\u00e4\u00dft sich verf\u00fchren,", "tokens": ["Da", "such\u00b7te", "dich", "das", "Gl\u00fcck", ".", "Der", "Fuchs", "l\u00e4\u00dft", "sich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "NE", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wirft seinen Fra\u00df dahin, setzt dem Gefl\u00fcgel nach.", "tokens": ["Wirft", "sei\u00b7nen", "Fra\u00df", "da\u00b7hin", ",", "setzt", "dem", "Ge\u00b7fl\u00fc\u00b7gel", "nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PAV", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Doch jenes macht sich unter Dach,", "tokens": ["Doch", "je\u00b7nes", "macht", "sich", "un\u00b7ter", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und kr\u00e4het, ihm zum Hohn, im sichern H\u00fchnerhause.", "tokens": ["Und", "kr\u00e4\u00b7het", ",", "ihm", "zum", "Hohn", ",", "im", "si\u00b7chern", "H\u00fch\u00b7ner\u00b7hau\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Kr\u00e4ht, ruft er, kr\u00e4ht! mir bleibt ein fetter Fra\u00df zum Schmause.", "tokens": ["Kr\u00e4ht", ",", "ruft", "er", ",", "kr\u00e4ht", "!", "mir", "bleibt", "ein", "fet\u00b7ter", "Fra\u00df", "zum", "Schmau\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er trabt zur\u00fcck, und sucht. Der frohe Rabe sitzt", "tokens": ["Er", "trabt", "zu\u00b7r\u00fcck", ",", "und", "sucht", ".", "Der", "fro\u00b7he", "Ra\u00b7be", "sitzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Auf einem Baum, wo ihn die H\u00f6he sch\u00fctzt.", "tokens": ["Auf", "ei\u00b7nem", "Baum", ",", "wo", "ihn", "die", "H\u00f6\u00b7he", "sch\u00fctzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Den Speck hat er verzehrt. Freund, schreit er, mit Vergn\u00fcgen", "tokens": ["Den", "Speck", "hat", "er", "ver\u00b7zehrt", ".", "Freund", ",", "schreit", "er", ",", "mit", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "NN", "$,", "KOUS", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Erlern' ich F\u00fcchse zu betr\u00fcgen.", "tokens": ["Er\u00b7lern'", "ich", "F\u00fcch\u00b7se", "zu", "be\u00b7tr\u00fc\u00b7gen", "."], "t