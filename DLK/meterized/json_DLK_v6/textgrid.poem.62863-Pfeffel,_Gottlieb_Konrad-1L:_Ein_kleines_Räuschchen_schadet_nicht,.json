{"textgrid.poem.62863": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein kleines R\u00e4uschchen schadet nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines R\u00e4uschchen schadet nicht,", "tokens": ["Ein", "klei\u00b7nes", "R\u00e4uschchen", "scha\u00b7det", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Lernt ich von meiner Amme Suse,", "tokens": ["Lernt", "ich", "von", "mei\u00b7ner", "Am\u00b7me", "Su\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaubt es ihr. Vom losen Wicht,", "tokens": ["Und", "glaubt", "es", "ihr", ".", "Vom", "lo\u00b7sen", "Wicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$.", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Bacchus, lernt es meine Muse,", "tokens": ["Dem", "Bac\u00b7chus", ",", "lernt", "es", "mei\u00b7ne", "Mu\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und trank in Syrakuser-Wein", "tokens": ["Und", "trank", "in", "Sy\u00b7ra\u00b7ku\u00b7ser\u00b7Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich einen Rausch, fieng an zu toben,", "tokens": ["Sich", "ei\u00b7nen", "Rausch", ",", "fi\u00b7eng", "an", "zu", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ein Tapetenk\u00f6niglein", "tokens": ["Und", "ein", "Ta\u00b7pe\u00b7ten\u00b7k\u00f6\u00b7nig\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Pindarisch zum Trajan zu loben.", "tokens": ["Pin\u00b7da\u00b7risch", "zum", "Tra\u00b7jan", "zu", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Dann stieg sie auf den Rabenstein", "tokens": ["Dann", "stieg", "sie", "auf", "den", "Ra\u00b7ben\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und rief den Teufeln, Volen, Elfen,", "tokens": ["Und", "rief", "den", "Teu\u00b7feln", ",", "Vo\u00b7len", ",", "El\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sie m\u00f6chten beym Cometenschein", "tokens": ["Sie", "m\u00f6ch\u00b7ten", "beym", "Co\u00b7me\u00b7ten\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Schauspiel ihr tragieren helfen,", "tokens": ["Ein", "Schau\u00b7spiel", "ihr", "tra\u00b7gie\u00b7ren", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Allein sie blieben gar zu lang.", "tokens": ["Al\u00b7lein", "sie", "blie\u00b7ben", "gar", "zu", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Da grif sie nach Hans Sachsens Leyer,", "tokens": ["Da", "grif", "sie", "nach", "Hans", "Sach\u00b7sens", "Le\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Und heulte weichen Minnesang", "tokens": ["Und", "heul\u00b7te", "wei\u00b7chen", "Min\u00b7ne\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Den Bogen voll um einen Dreyer.", "tokens": ["Den", "Bo\u00b7gen", "voll", "um", "ei\u00b7nen", "Drey\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Nun trieb sie des Genies Drang", "tokens": ["Nun", "trieb", "sie", "des", "Ge\u00b7nies", "Drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "In unsrer Barden Wodansfeyer;", "tokens": ["In", "uns\u00b7rer", "Bar\u00b7den", "Wo\u00b7dans\u00b7fe\u00b7yer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Hier kr\u00f6nt sie sich mit Eichenlaub,", "tokens": ["Hier", "kr\u00f6nt", "sie", "sich", "mit", "Ei\u00b7chen\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Bestreut ihr Wamms mit Heldenstaub,", "tokens": ["Be\u00b7streut", "ihr", "Wamms", "mit", "Hel\u00b7den\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Macht Blitze sich aus Fliegenwedeln,", "tokens": ["Macht", "Blit\u00b7ze", "sich", "aus", "Flie\u00b7gen\u00b7we\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "S\u00e4uft Bonzenblut aus F\u00fcrstensch\u00e4deln,", "tokens": ["S\u00e4uft", "Bon\u00b7zen\u00b7blut", "aus", "F\u00fcrs\u00b7ten\u00b7sch\u00e4\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und klettert mit zerstreutem Haar", "tokens": ["Und", "klet\u00b7tert", "mit", "zer\u00b7streu\u00b7tem", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nun gar auf des Parnasses Spitzen.", "tokens": ["Nun", "gar", "auf", "des", "Par\u00b7nas\u00b7ses", "Spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Hier sah sie die geweihte Schaar", "tokens": ["Hier", "sah", "sie", "die", "ge\u00b7weih\u00b7te", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Des Helios im Kreise sitzen,", "tokens": ["Des", "He\u00b7li\u00b7os", "im", "Krei\u00b7se", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und kr\u00e4hte wie ein Puterhahn", "tokens": ["Und", "kr\u00e4h\u00b7te", "wie", "ein", "Pu\u00b7ter\u00b7hahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Den hehren Schatten Gellerts an.", "tokens": ["Den", "heh\u00b7ren", "Schat\u00b7ten", "Gel\u00b7lerts", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ein Blick, aus welchem stille Gr\u00f6\u00dfe", "tokens": ["Ein", "Blick", ",", "aus", "wel\u00b7chem", "stil\u00b7le", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und Menschenhuld und Mitleid sahn,", "tokens": ["Und", "Men\u00b7schen\u00b7huld", "und", "Mit\u00b7leid", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Entfuhr dem Edeln. Angst und Bl\u00e4sse", "tokens": ["Ent\u00b7fuhr", "dem", "E\u00b7deln", ".", "Angst", "und", "Bl\u00e4s\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Des Tods ergrif die Schwelgerin;", "tokens": ["Des", "Tods", "er\u00b7grif", "die", "Schwel\u00b7ge\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Sie schlug die Brust, ward pl\u00f6tzlich n\u00fcchtern,", "tokens": ["Sie", "schlug", "die", "Brust", ",", "ward", "pl\u00f6tz\u00b7lich", "n\u00fcch\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Fiel ihm zu Fu\u00df und lallte sch\u00fcchtern:", "tokens": ["Fiel", "ihm", "zu", "Fu\u00df", "und", "lall\u00b7te", "sch\u00fcch\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Vergieb mir armen S\u00fcnderin.", "tokens": ["Ver\u00b7gieb", "mir", "ar\u00b7men", "S\u00fcn\u00b7de\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}