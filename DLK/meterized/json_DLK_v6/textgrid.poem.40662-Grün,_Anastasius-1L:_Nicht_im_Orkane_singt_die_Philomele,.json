{"textgrid.poem.40662": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht im Orkane singt die Philomele,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht im Orkane singt die Philomele,", "tokens": ["Nicht", "im", "Or\u00b7ka\u00b7ne", "singt", "die", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sie lauscht im Buschverstecke, wir's gewittert,", "tokens": ["Sie", "lauscht", "im", "Buschver\u00b7ste\u00b7cke", ",", "wir's", "ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie Sturm die Orgel schl\u00e4gt und Eichen splittert;", "tokens": ["Wie", "Sturm", "die", "Or\u00b7gel", "schl\u00e4gt", "und", "Ei\u00b7chen", "split\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Grauen schn\u00fcrt ihr zu die zarte Kehle.", "tokens": ["Das", "Grau\u00b7en", "schn\u00fcrt", "ihr", "zu", "die", "zar\u00b7te", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Sturm doch bleibt gewonnen ihrer Seele. \u2013", "tokens": ["Der", "Sturm", "doch", "bleibt", "ge\u00b7won\u00b7nen", "ih\u00b7rer", "See\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VVPP", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Thau und Duft um deine Rosen zittert,", "tokens": ["Wenn", "Thau", "und", "Duft", "um", "dei\u00b7ne", "Ro\u00b7sen", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Mainacht, mondgekr\u00f6nt und sternbeflittert,", "tokens": ["O", "Mai\u00b7nacht", ",", "mond\u00b7ge\u00b7kr\u00f6nt", "und", "stern\u00b7be\u00b7flit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann jauchzt ihr Sang durch deine Bl\u00fcthens\u00e4le.", "tokens": ["Dann", "jauchzt", "ihr", "Sang", "durch", "dei\u00b7ne", "Bl\u00fc\u00b7then\u00b7s\u00e4\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wei\u00dft du gut mit feinerm Ohr zu lauschen,", "tokens": ["Und", "wei\u00dft", "du", "gut", "mit", "fei\u00b7nerm", "Ohr", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So h\u00f6rst du nur den Sturm von damals rauschen,", "tokens": ["So", "h\u00f6rst", "du", "nur", "den", "Sturm", "von", "da\u00b7mals", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch ihre Kehle jene Donner schmettern;", "tokens": ["Durch", "ih\u00b7re", "Keh\u00b7le", "je\u00b7ne", "Don\u00b7ner", "schmet\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du h\u00f6rst den Angstschrei, banges Wipfelsausen,", "tokens": ["Du", "h\u00f6rst", "den", "Angst\u00b7schrei", ",", "ban\u00b7ges", "Wip\u00b7fel\u00b7sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den nahen raschen Schlag, ein fern Verbrausen, \u2013", "tokens": ["Den", "na\u00b7hen", "ra\u00b7schen", "Schlag", ",", "ein", "fern", "Ver\u00b7brau\u00b7sen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch s\u00fc\u00dfer Wohllaut nur rollt in den Wettern.", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", "Wohl\u00b7laut", "nur", "rollt", "in", "den", "Wet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}