{"textgrid.poem.35881": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: In allen N\u00f6then eilet", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In allen N\u00f6then eilet", "tokens": ["In", "al\u00b7len", "N\u00f6\u00b7then", "ei\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Kind zum Mutterschoo\u00df,", "tokens": ["Das", "Kind", "zum", "Mut\u00b7ter\u00b7schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo jede Wunde heilet,", "tokens": ["Wo", "je\u00b7de", "Wun\u00b7de", "hei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4r' sie noch so gro\u00df.", "tokens": ["Und", "w\u00e4r'", "sie", "noch", "so", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So kann auch ich nur wenden", "tokens": ["So", "kann", "auch", "ich", "nur", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den tr\u00fcben Blick zu Dir;", "tokens": ["Den", "tr\u00fc\u00b7ben", "Blick", "zu", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Deinen milden H\u00e4nden", "tokens": ["Von", "Dei\u00b7nen", "mil\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Thaut jede Tr\u00f6stung mir.", "tokens": ["Thaut", "je\u00b7de", "Tr\u00f6s\u00b7tung", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wer rief Dich je vergebens,", "tokens": ["Wer", "rief", "Dich", "je", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Maria! hoffend an?", "tokens": ["Ma\u00b7ria", "!", "hof\u00b7fend", "an", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Du Mutter alles Lebens,", "tokens": ["Du", "Mut\u00b7ter", "al\u00b7les", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du brichst des Todes Bann.", "tokens": ["Du", "brichst", "des", "To\u00b7des", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl kannst Du Schmerz erkunden,", "tokens": ["Wohl", "kannst", "Du", "Schmerz", "er\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl ist Dir Leid bewu\u00dft:", "tokens": ["Wohl", "ist", "Dir", "Leid", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es brennen sieben Wunden", "tokens": ["Es", "bren\u00b7nen", "sie\u00b7ben", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVINF", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Deiner reinen Brust.", "tokens": ["In", "Dei\u00b7ner", "rei\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In unsagbaren Schmerzen", "tokens": ["In", "un\u00b7sag\u00b7ba\u00b7ren", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blick' ich nur auf zu Dir,", "tokens": ["Blick'", "ich", "nur", "auf", "zu", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da str\u00f6mt aus Deinem Herzen", "tokens": ["Da", "str\u00f6mt", "aus", "Dei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon Trost und Friede mir.", "tokens": ["Schon", "Trost", "und", "Frie\u00b7de", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und was so schwer mich kr\u00e4nkte,", "tokens": ["Und", "was", "so", "schwer", "mich", "kr\u00e4nk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich den Muth verlor", "tokens": ["Da\u00df", "ich", "den", "Muth", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tr\u00fcb' die Blicke senkte,", "tokens": ["Und", "tr\u00fcb'", "die", "Bli\u00b7cke", "senk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das tr\u00e4gt mich nun empor.", "tokens": ["Das", "tr\u00e4gt", "mich", "nun", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das senkt gleich Friedenspalmen", "tokens": ["Das", "senkt", "gleich", "Frie\u00b7dens\u00b7pal\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich mild in's Herz hinein;", "tokens": ["Sich", "mild", "in's", "Herz", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das weckt zu Dankespsalmen", "tokens": ["Das", "weckt", "zu", "Dan\u00b7ke\u00b7spsal\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Seele mein.", "tokens": ["Die", "gan\u00b7ze", "See\u00b7le", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}