{"textgrid.poem.57769": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der eifers\u00fcchtige J\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00e4ger und das bin ich,", "tokens": ["Ein", "J\u00e4\u00b7ger", "und", "das", "bin", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PDS", "VAFIN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Kleid und das ist gr\u00fcn;", "tokens": ["Mein", "Kleid", "und", "das", "ist", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Eh da\u00df die Sonne scheinet,", "tokens": ["Eh", "da\u00df", "die", "Son\u00b7ne", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mu\u00df ich zu Holze ziehn.", "tokens": ["Mu\u00df", "ich", "zu", "Hol\u00b7ze", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Eh da\u00df die Sonne scheinet,", "tokens": ["Eh", "da\u00df", "die", "Son\u00b7ne", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Vor Tau und auch vor Tag;", "tokens": ["Vor", "Tau", "und", "auch", "vor", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schlafen alle Leute,", "tokens": ["Es", "schla\u00b7fen", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Schatz und der ist wach.", "tokens": ["Mein", "Schatz", "und", "der", "ist", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "ART", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Schatz der steht am Fenster", "tokens": ["Mein", "Schatz", "der", "steht", "am", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seinem Hemdelein;", "tokens": ["In", "sei\u00b7nem", "Hem\u00b7del\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit seinen wei\u00dfen H\u00e4nden", "tokens": ["Mit", "sei\u00b7nen", "wei\u00b7\u00dfen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da winkt er mir herein.", "tokens": ["Da", "winkt", "er", "mir", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hinein kann ich nicht kommen,", "tokens": ["Hin\u00b7ein", "kann", "ich", "nicht", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich gehe auf die Pirsch;", "tokens": ["Ich", "ge\u00b7he", "auf", "die", "Pirsch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Holze mu\u00df ich ziehen,", "tokens": ["Zu", "Hol\u00b7ze", "mu\u00df", "ich", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da steht ein guter Hirsch.", "tokens": ["Da", "steht", "ein", "gu\u00b7ter", "Hirsch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und mu\u00dft du ziehn zu Holze", "tokens": ["Und", "mu\u00dft", "du", "ziehn", "zu", "Hol\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lassen mich allein,", "tokens": ["Und", "las\u00b7sen", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So soll ein andrer schlafen", "tokens": ["So", "soll", "ein", "an\u00b7drer", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei mir im K\u00e4mmerlein.", "tokens": ["Bei", "mir", "im", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und schl\u00e4ft bei dir ein andrer,", "tokens": ["Und", "schl\u00e4ft", "bei", "dir", "ein", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ich habe Kraut und Lot;", "tokens": ["Ich", "ha\u00b7be", "Kraut", "und", "Lot", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00fc\u00dft er dich des Abends,", "tokens": ["Und", "k\u00fc\u00dft", "er", "dich", "des", "A\u00b7bends", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist er morgens tot.", "tokens": ["So", "ist", "er", "mor\u00b7gens", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}