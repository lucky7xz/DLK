{"textgrid.poem.67000": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Zu einer Kriegsradierung", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grausiger Geier der Menschheitsnot,", "tokens": ["Grau\u00b7si\u00b7ger", "Gei\u00b7er", "der", "Menschheits\u00b7not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Stern des Krieges, der stechend loht,", "tokens": ["Stern", "des", "Krie\u00b7ges", ",", "der", "ste\u00b7chend", "loht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Eiserner Helm der bannenden Sphinx,", "tokens": ["Ei\u00b7ser\u00b7ner", "Helm", "der", "ban\u00b7nen\u00b7den", "Sphinx", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Unheilbr\u00fctendes Dunkel rings \u2013", "tokens": ["Un\u00b7heil\u00b7br\u00fc\u00b7ten\u00b7des", "Dun\u00b7kel", "rings", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wahre dich, lautere Flamme,", "tokens": ["Wah\u00b7re", "dich", ",", "lau\u00b7te\u00b7re", "Flam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Heiligem Leben geweiht,", "tokens": ["Hei\u00b7li\u00b7gem", "Le\u00b7ben", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Im bluttriefenden Schlamme", "tokens": ["Im", "blut\u00b7trie\u00b7fen\u00b7den", "Schlam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kunstverheerender Zeit!", "tokens": ["Kunst\u00b7ver\u00b7hee\u00b7ren\u00b7der", "Zeit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}