{"textgrid.poem.62955": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein grauer Fackir, welcher zwar", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein grauer Fackir, welcher zwar", "tokens": ["Ein", "grau\u00b7er", "Fa\u00b7ckir", ",", "wel\u00b7cher", "zwar"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch frisch und stark von Gliedern war,", "tokens": ["Noch", "frisch", "und", "stark", "von", "Glie\u00b7dern", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein von Jugend auf an Kr\u00fccken", "tokens": ["Al\u00b7lein", "von", "Ju\u00b7gend", "auf", "an", "Kr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu gehn gewohnt, itzt ohne sie", "tokens": ["Zu", "gehn", "ge\u00b7wohnt", ",", "itzt", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVPP", "$,", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht f\u00e4hig war vom Fleck zu r\u00fccken,", "tokens": ["Nicht", "f\u00e4\u00b7hig", "war", "vom", "Fleck", "zu", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Warf sich am Ganges auf sein Knie,", "tokens": ["Warf", "sich", "am", "Gan\u00b7ges", "auf", "sein", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um einsam sein Gebet zu halten.", "tokens": ["Um", "ein\u00b7sam", "sein", "Ge\u00b7bet", "zu", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Europ\u00e4er sah ihn da", "tokens": ["Ein", "Eu\u00b7ro\u00b7p\u00e4\u00b7er", "sah", "ihn", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Voll Andacht seine H\u00e4nde falten:", "tokens": ["Voll", "An\u00b7dacht", "sei\u00b7ne", "H\u00e4n\u00b7de", "fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es war ein kleiner Attila", "tokens": ["Es", "war", "ein", "klei\u00b7ner", "At\u00b7ti\u00b7la"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vom neuen Philosophenorden:", "tokens": ["Vom", "neu\u00b7en", "Phi\u00b7lo\u00b7so\u00b7phen\u00b7or\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ihm ward das Rauben und das Morden,", "tokens": ["Ihm", "ward", "das", "Rau\u00b7ben", "und", "das", "Mor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nur nicht der Aberglaube recht.", "tokens": ["Nur", "nicht", "der", "A\u00b7berg\u00b7lau\u00b7be", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er schlich versteckt sich durch die Hecken,", "tokens": ["Er", "schlich", "ver\u00b7steckt", "sich", "durch", "die", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und lachend nahm der Henkersknecht", "tokens": ["Und", "la\u00b7chend", "nahm", "der", "Hen\u00b7kers\u00b7knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dem Beter seine beyden Stecken.", "tokens": ["Dem", "Be\u00b7ter", "sei\u00b7ne", "bey\u00b7den", "Ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der Fackir rafft sich auf und f\u00e4llt:", "tokens": ["Der", "Fa\u00b7ckir", "rafft", "sich", "auf", "und", "f\u00e4llt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er fleht, er weint; allein vergebens.", "tokens": ["Er", "fleht", ",", "er", "weint", ";", "al\u00b7lein", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der R\u00e4uber h\u00fcpfte durch das Feld;", "tokens": ["Der", "R\u00e4u\u00b7ber", "h\u00fcpf\u00b7te", "durch", "das", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und trug die St\u00fctzen seines Lebens", "tokens": ["Und", "trug", "die", "St\u00fct\u00b7zen", "sei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Als Siegeszeichen vor sich her.", "tokens": ["Als", "Sie\u00b7ges\u00b7zei\u00b7chen", "vor", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ein weiser Brame, welcher mehr,", "tokens": ["Ein", "wei\u00b7ser", "Bra\u00b7me", ",", "wel\u00b7cher", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Als was der Vedam lehrt, gelernet,", "tokens": ["Als", "was", "der", "Ve\u00b7dam", "lehrt", ",", "ge\u00b7ler\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PWS", "ART", "NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und das Ger\u00e4usch der Welt zu fliehn,", "tokens": ["Und", "das", "Ge\u00b7r\u00e4usch", "der", "Welt", "zu", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Sich in die Einsamkeit entfernet,", "tokens": ["Sich", "in", "die", "Ein\u00b7sam\u00b7keit", "ent\u00b7fer\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Eilt zum verla\u00dfnen Pilger hin,", "tokens": ["Eilt", "zum", "ver\u00b7la\u00df\u00b7nen", "Pil\u00b7ger", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Tr\u00e4gt liebreich ihn auf seinem R\u00fccken", "tokens": ["Tr\u00e4gt", "lieb\u00b7reich", "ihn", "auf", "sei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "In seine H\u00fctte, tr\u00f6stet ihn,", "tokens": ["In", "sei\u00b7ne", "H\u00fct\u00b7te", ",", "tr\u00f6s\u00b7tet", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und schenkt ihm ein Paar neue Kr\u00fccken;", "tokens": ["Und", "schenkt", "ihm", "ein", "Paar", "neu\u00b7e", "Kr\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.30": {"text": "Hier, Freund, sprach er; indessen sie", "tokens": ["Hier", ",", "Freund", ",", "sprach", "er", ";", "in\u00b7des\u00b7sen", "sie"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "$.", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Des Armen H\u00e4nde gierig fassen:", "tokens": ["Des", "Ar\u00b7men", "H\u00e4n\u00b7de", "gie\u00b7rig", "fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Auch dieses ist Philosophie,", "tokens": ["Auch", "die\u00b7ses", "ist", "Phi\u00b7lo\u00b7so\u00b7phie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Dem, der sie brauchet, sie zu lassen.", "tokens": ["Dem", ",", "der", "sie", "brau\u00b7chet", ",", "sie", "zu", "las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Das thust du, Freund, und wirst mi\u00dfkannt", "tokens": ["Das", "thust", "du", ",", "Freund", ",", "und", "wirst", "mi\u00df\u00b7kannt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und wirst der Toleranz zur Ehre,", "tokens": ["Und", "wirst", "der", "To\u00b7le\u00b7ranz", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "F\u00fcr diese ketzerische Lehre,", "tokens": ["F\u00fcr", "die\u00b7se", "ket\u00b7ze\u00b7ri\u00b7sche", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.37": {"text": "Wohl gar ein Jesuit genannt.", "tokens": ["Wohl", "gar", "ein", "Je\u00b7su\u00b7it", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Sey mir gegr\u00fc\u00dft, ein lieber Pater!", "tokens": ["Sey", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "ein", "lie\u00b7ber", "Pa\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.39": {"text": "Du heiligst Loyolas Gewand.", "tokens": ["Du", "hei\u00b7ligst", "Lo\u00b7yo\u00b7las", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Ein Gl\u00fcck ists, da\u00df der heilge Vater,", "tokens": ["Ein", "Gl\u00fcck", "ists", ",", "da\u00df", "der", "heil\u00b7ge", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Als er die schwarze Zunft verbannt,", "tokens": ["Als", "er", "die", "schwar\u00b7ze", "Zunft", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Den Bruder Caspar nicht gekannt;", "tokens": ["Den", "Bru\u00b7der", "Cas\u00b7par", "nicht", "ge\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Es w\u00e4re sonst dein ganzer Orden", "tokens": ["Es", "w\u00e4\u00b7re", "sonst", "dein", "gan\u00b7zer", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Blos dir zu lieb verschonet worden.", "tokens": ["Blos", "dir", "zu", "lieb", "ver\u00b7scho\u00b7net", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKA", "ADJD", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}