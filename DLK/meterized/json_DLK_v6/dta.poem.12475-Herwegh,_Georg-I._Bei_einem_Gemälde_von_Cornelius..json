{"dta.poem.12475": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Bei einem Gem\u00e4lde von Cornelius.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist die Madonna der Poeten,", "tokens": ["Die", "Zeit", "ist", "die", "Ma\u00b7don\u00b7na", "der", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Den Heiland soll. Drum halt' die Zeit in Ehren:", "tokens": ["Den", "Hei\u00b7land", "soll", ".", "Drum", "halt'", "die", "Zeit", "in", "Eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "$.", "PAV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du kannst nichts H\u00f6heres, denn sie, vertreten.", "tokens": ["Du", "kannst", "nichts", "H\u00f6\u00b7he\u00b7res", ",", "denn", "sie", ",", "ver\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "NN", "$,", "KON", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hat deine Zeit einmal nicht Lust zum Beten,", "tokens": ["Hat", "dei\u00b7ne", "Zeit", "ein\u00b7mal", "nicht", "Lust", "zum", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du wirst sie keines Besseren belehren!", "tokens": ["Du", "wirst", "sie", "kei\u00b7nes", "Bes\u00b7se\u00b7ren", "be\u00b7leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warum die Augen ewig r\u00fcckw\u00e4rts kehren?", "tokens": ["Wa\u00b7rum", "die", "Au\u00b7gen", "e\u00b7wig", "r\u00fcck\u00b7w\u00e4rts", "keh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im eigenen Jahrhundert dich versp\u00e4ten?", "tokens": ["Im", "ei\u00b7ge\u00b7nen", "Jahr\u00b7hun\u00b7dert", "dich", "ver\u00b7sp\u00e4\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich achte all dies strahlende Gelichter", "tokens": ["Ich", "ach\u00b7te", "all", "dies", "strah\u00b7len\u00b7de", "Ge\u00b7lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deinen ganzen Himmel nicht sehr theuer,", "tokens": ["Und", "dei\u00b7nen", "gan\u00b7zen", "Him\u00b7mel", "nicht", "sehr", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Obschon du h\u00f6flichst drein gesetzt den Dichter.", "tokens": ["Ob\u00b7schon", "du", "h\u00f6f\u00b7lichst", "drein", "ge\u00b7setzt", "den", "Dich\u00b7ter."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nimm einen Lorbeer f\u00fcr die ", "tokens": ["Nimm", "ei\u00b7nen", "Lor\u00b7beer", "f\u00fcr", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und f\u00fcr die ", "tokens": ["Und", "f\u00fcr", "die"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Doch deine Heiligen \u2014 die wirf in's Feuer!", "tokens": ["Doch", "dei\u00b7ne", "Hei\u00b7li\u00b7gen", "die", "wirf", "in's", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}