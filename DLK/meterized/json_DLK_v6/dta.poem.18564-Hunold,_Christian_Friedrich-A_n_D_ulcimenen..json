{"dta.poem.18564": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A n  D ulcimenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.57", "id:0.42"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So kan ich mi ch/ galante Dulcimene/", "tokens": ["So", "kan", "ich", "mi", "ch", "/", "ga\u00b7lan\u00b7te", "Dul\u00b7ci\u00b7me\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NE", "NE", "$(", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bey dir vergn\u00fcgt und gl\u00fccklich sehn.", "tokens": ["Bey", "dir", "ver\u00b7gn\u00fcgt", "und", "gl\u00fcck\u00b7lich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freundlichkeit macht dich gedoppel", "tokens": ["Die", "Freund\u00b7lich\u00b7keit", "macht", "dich", "ge\u00b7dop\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die\u00df sol mir zur Lust geschehn.", "tokens": ["Und", "die\u00df", "sol", "mir", "zur", "Lust", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Gl\u00fccks-Gestirn will nach den Finsternissen/", "tokens": ["Mein", "Gl\u00fccks\u00b7Ge\u00b7stirn", "will", "nach", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich durch den Strahl der Gnaden wieder k\u00fcssen.", "tokens": ["Mich", "durch", "den", "Strahl", "der", "Gna\u00b7den", "wie\u00b7der", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich dencke noch an das gehabte Leyden/", "tokens": ["Ich", "den\u00b7cke", "noch", "an", "das", "ge\u00b7hab\u00b7te", "Ley\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und wie ich an zu trauren fing/", "tokens": ["Und", "wie", "ich", "an", "zu", "trau\u00b7ren", "fing", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als durch den Zorn die Sonne meiner Freuden/", "tokens": ["Als", "durch", "den", "Zorn", "die", "Son\u00b7ne", "mei\u00b7ner", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auff mein Verschulden unterging.", "tokens": ["Auff", "mein", "Ver\u00b7schul\u00b7den", "un\u00b7ter\u00b7ging", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein sie