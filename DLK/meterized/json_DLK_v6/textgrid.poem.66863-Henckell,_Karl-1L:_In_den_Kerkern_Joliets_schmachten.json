{"textgrid.poem.66863": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: In den Kerkern Joliets schmachten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Kerkern Joliets schmachten", "tokens": ["In", "den", "Ker\u00b7kern", "Jo\u00b7liets", "schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fchner Freiheitsk\u00e4mpfer drei,", "tokens": ["K\u00fch\u00b7ner", "Frei\u00b7heits\u00b7k\u00e4mp\u00b7fer", "drei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die der Fackel Glut entfachten", "tokens": ["Die", "der", "Fa\u00b7ckel", "Glut", "ent\u00b7fach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wider goldne Tyrannei.", "tokens": ["Wi\u00b7der", "gold\u00b7ne", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fcnf geopfert f\u00fcr den Henker,", "tokens": ["F\u00fcnf", "ge\u00b7o\u00b7pfert", "f\u00fcr", "den", "Hen\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Drei f\u00fcr das lebendige Grab,", "tokens": ["Drei", "f\u00fcr", "das", "le\u00b7ben\u00b7di\u00b7ge", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Weil ihr Mut dem Wort der Denker", "tokens": ["Weil", "ihr", "Mut", "dem", "Wort", "der", "Den\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcgel in die Tiefe gab.", "tokens": ["Fl\u00fc\u00b7gel", "in", "die", "Tie\u00b7fe", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinter Schlo\u00df und Riegel trauern", "tokens": ["Hin\u00b7ter", "Schlo\u00df", "und", "Rie\u00b7gel", "trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahrheitszeugen Jahr f\u00fcr Jahr,", "tokens": ["Wahr\u00b7heits\u00b7zeu\u00b7gen", "Jahr", "f\u00fcr", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch um Joliets Kerkermauern", "tokens": ["Doch", "um", "Jo\u00b7liets", "Ker\u00b7ker\u00b7mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ruhlos kreist der Freiheit Aar.", "tokens": ["Ruh\u00b7los", "kreist", "der", "Frei\u00b7heit", "Aar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unter seinen Schwingen tropfen", "tokens": ["Un\u00b7ter", "sei\u00b7nen", "Schwin\u00b7gen", "trop\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rote Tropfen schwer hervor,", "tokens": ["Ro\u00b7te", "Trop\u00b7fen", "schwer", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "N\u00e4chtlich mu\u00df sein Schnabel klopfen", "tokens": ["N\u00e4cht\u00b7lich", "mu\u00df", "sein", "Schna\u00b7bel", "klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dreimal an das dunkle Tor.", "tokens": ["Drei\u00b7mal", "an", "das", "dunk\u00b7le", "Tor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wird einmal Erl\u00f6sung tagen", "tokens": ["Wird", "ein\u00b7mal", "Er\u00b7l\u00f6\u00b7sung", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aus der Knechtschaft Schmach und Not,", "tokens": ["Aus", "der", "Knecht\u00b7schaft", "Schmach", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird der Aar die Fl\u00fcgel schlagen", "tokens": ["Wird", "der", "Aar", "die", "Fl\u00fc\u00b7gel", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freudeweit ins Morgenrot?", "tokens": ["Freu\u00b7de\u00b7weit", "ins", "Mor\u00b7gen\u00b7rot", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}