{"textgrid.poem.39804": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum edleren Gesang sind wir verbunden,", "tokens": ["Zum", "ed\u00b7le\u00b7ren", "Ge\u00b7sang", "sind", "wir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu jenem, den kein b\u00f6ser Mi\u00dflaut st\u00f6rt;", "tokens": ["Zu", "je\u00b7nem", ",", "den", "kein", "b\u00f6\u00b7ser", "Mi\u00df\u00b7laut", "st\u00f6rt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PRELS", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gemeines soll auf immer sein verschwunden,", "tokens": ["Ge\u00b7mei\u00b7nes", "soll", "auf", "im\u00b7mer", "sein", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ADV", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Menschliche, das Reine nur geh\u00f6rt:", "tokens": ["Das", "Menschli\u00b7che", ",", "das", "Rei\u00b7ne", "nur", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Haupt ein F\u00fchrer ward von uns gefunden,", "tokens": ["Ein", "Haupt", "ein", "F\u00fch\u00b7rer", "ward", "von", "uns", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Jedes als den keuschen S\u00e4nger ehrt,", "tokens": ["Den", "Je\u00b7des", "als", "den", "keu\u00b7schen", "S\u00e4n\u00b7ger", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den fr\u00fcher Tod f\u00fcr Alles, was nach oben", "tokens": ["Den", "fr\u00fc\u00b7her", "Tod", "f\u00fcr", "Al\u00b7les", ",", "was", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIS", "$,", "PRELS", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sich sehnlich kehrt, zum Schutzgeist hat erhoben.", "tokens": ["Sich", "sehn\u00b7lich", "kehrt", ",", "zum", "Schutz\u00b7geist", "hat", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den l\u00e4ngst Geschiednen kommen wir zu feiern,", "tokens": ["Den", "l\u00e4ngst", "Ge\u00b7schied\u00b7nen", "kom\u00b7men", "wir", "zu", "fei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Todestag liegt hinter uns schon weit,", "tokens": ["Sein", "To\u00b7des\u00b7tag", "liegt", "hin\u00b7ter", "uns", "schon", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja, schon als Nachwelt preisen wir den Theuern,", "tokens": ["Ja", ",", "schon", "als", "Nach\u00b7welt", "prei\u00b7sen", "wir", "den", "Theu\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KOUS", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der mit uns k\u00f6nnte wandeln in der Zeit;", "tokens": ["Der", "mit", "uns", "k\u00f6nn\u00b7te", "wan\u00b7deln", "in", "der", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VMFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Inde\u00df wir stimmen unsre matten Leiern,", "tokens": ["In\u00b7de\u00df", "wir", "stim\u00b7men", "uns\u00b7re", "mat\u00b7ten", "Lei\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "S\u00e4ng' er vielleicht das sch\u00f6nste Lied noch heut;", "tokens": ["S\u00e4ng'", "er", "viel\u00b7leicht", "das", "sch\u00f6ns\u00b7te", "Lied", "noch", "heut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir Vielen, die wir fr\u00f6hlich streben, sprossen,", "tokens": ["Wir", "Vie\u00b7len", ",", "die", "wir", "fr\u00f6h\u00b7lich", "stre\u00b7ben", ",", "spros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir k\u00f6nnten bl\u00fchen, seine Zeitgenossen.", "tokens": ["Wir", "k\u00f6nn\u00b7ten", "bl\u00fc\u00b7hen", ",", "sei\u00b7ne", "Zeit\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was w\u00e4re nicht aus deinem Geist entsprungen,", "tokens": ["Was", "w\u00e4\u00b7re", "nicht", "aus", "dei\u00b7nem", "Geist", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was h\u00e4tte sich, der deutschen Kunst zum Preis,", "tokens": ["Was", "h\u00e4t\u00b7te", "sich", ",", "der", "deut\u00b7schen", "Kunst", "zum", "Preis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht deiner k\u00f6niglichen Stirn entschwungen,", "tokens": ["Nicht", "dei\u00b7ner", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Stirn", "ent\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du Herrlicher \u2013 noch jetzt ein r\u00fcst'ger Greis \u2013", "tokens": ["Du", "Herr\u00b7li\u00b7cher", "\u2013", "noch", "jetzt", "ein", "r\u00fcst'\u00b7ger", "Greis", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Wenn du in reifer F\u00fclle fortgesungen,", "tokens": ["Wenn", "du", "in", "rei\u00b7fer", "F\u00fcl\u00b7le", "fort\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Entr\u00e4tselt, dargestellt mit heil'gem Flei\u00df,", "tokens": ["Ent\u00b7r\u00e4t\u00b7selt", ",", "dar\u00b7ge\u00b7stellt", "mit", "heil'\u00b7gem", "Flei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was in dem Schacht der Zeiten und der Herzen", "tokens": ["Was", "in", "dem", "Schacht", "der", "Zei\u00b7ten", "und", "der", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verborgen quillt von Leben, Lust und Schmerzen.", "tokens": ["Ver\u00b7bor\u00b7gen", "quillt", "von", "Le\u00b7ben", ",", "Lust", "und", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was Gutes ward geglaubt, gew\u00fcnscht, betrauert,", "tokens": ["Was", "Gu\u00b7tes", "ward", "ge\u00b7glaubt", ",", "ge\u00b7w\u00fcnscht", ",", "be\u00b7trau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das du nicht mitempfunden h\u00e4ttest warm;", "tokens": ["Das", "du", "nicht", "mi\u00b7temp\u00b7fun\u00b7den", "h\u00e4t\u00b7test", "warm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was h\u00e4tte Treffliches gebl\u00fcht, gedauert,", "tokens": ["Was", "h\u00e4t\u00b7te", "Treff\u00b7li\u00b7ches", "ge\u00b7bl\u00fcht", ",", "ge\u00b7dau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An dem nicht mitgeschaffen stark dein Arm?", "tokens": ["An", "dem", "nicht", "mit\u00b7ge\u00b7schaf\u00b7fen", "stark", "dein", "Arm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "VVINF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie h\u00e4tte Mitgef\u00fchl dich tief durchschauert,", "tokens": ["Wie", "h\u00e4t\u00b7te", "Mit\u00b7ge\u00b7f\u00fchl", "dich", "tief", "durch\u00b7schau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo Hoffnung je verwandelt ward in Harm:", "tokens": ["Wo", "Hoff\u00b7nung", "je", "ver\u00b7wan\u00b7delt", "ward", "in", "Harm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVPP", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie h\u00e4tte Leid und Freude dich durchdrungen,", "tokens": ["Wie", "h\u00e4t\u00b7te", "Leid", "und", "Freu\u00b7de", "dich", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Lied um Lied sich draus emporgerungen.", "tokens": ["Und", "Lied", "um", "Lied", "sich", "draus", "em\u00b7por\u00b7ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und der du stets das fremde Streben ehrtest,", "tokens": ["Und", "der", "du", "stets", "das", "frem\u00b7de", "Stre\u00b7ben", "ehr\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht stolz und kalt auf deinen Ruhm beschr\u00e4nkt,", "tokens": ["Nicht", "stolz", "und", "kalt", "auf", "dei\u00b7nen", "Ruhm", "be\u00b7schr\u00e4nkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Freude, wenn du J\u00fcngre nun belehrtest,", "tokens": ["O", "Freu\u00b7de", ",", "wenn", "du", "J\u00fcng\u00b7re", "nun", "be\u00b7lehr\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Aug' auf jeden Bl\u00fctenkeim gesenkt;", "tokens": ["Dein", "Aug'", "auf", "je\u00b7den", "Bl\u00fc\u00b7ten\u00b7keim", "ge\u00b7senkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem Tro\u00df Nachahmender mit W\u00fcrde wehrtest,", "tokens": ["Dem", "Tro\u00df", "Nac\u00b7hah\u00b7men\u00b7der", "mit", "W\u00fcr\u00b7de", "wehr\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Inde\u00df dein Geist verwandte Geister tr\u00e4nkt:", "tokens": ["In\u00b7de\u00df", "dein", "Geist", "ver\u00b7wand\u00b7te", "Geis\u00b7ter", "tr\u00e4nkt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O wohl dem, der, um seine Stirn geschlagen,", "tokens": ["O", "wohl", "dem", ",", "der", ",", "um", "sei\u00b7ne", "Stirn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "$,", "PRELS", "$,", "KOUI", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Reis aus deinem Kranze d\u00fcrfte tragen!", "tokens": ["Ein", "Reis", "aus", "dei\u00b7nem", "Kran\u00b7ze", "d\u00fcrf\u00b7te", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Im Reich des Sch\u00f6nen ist ein Krieg entglommen,", "tokens": ["Im", "Reich", "des", "Sch\u00f6\u00b7nen", "ist", "ein", "Krieg", "ent\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man w\u00e4gt, verwirft, lobpreiset und verdammt,", "tokens": ["Man", "w\u00e4gt", ",", "ver\u00b7wirft", ",", "lob\u00b7prei\u00b7set", "und", "ver\u00b7dammt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Besonnen h\u00e4ttest du das Wort genommen,", "tokens": ["Be\u00b7son\u00b7nen", "h\u00e4t\u00b7test", "du", "das", "Wort", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Eintracht Herold, die vom Himmel stammt;", "tokens": ["Der", "Ein\u00b7tracht", "He\u00b7rold", ",", "die", "vom", "Him\u00b7mel", "stammt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und Ruh' und Milde w\u00e4r' auf uns gekommen,", "tokens": ["Und", "Ruh'", "und", "Mil\u00b7de", "w\u00e4r'", "auf", "uns", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ge\u00fcbet h\u00e4ttest du das Friedensamt:", "tokens": ["Ge\u00b7\u00fc\u00b7bet", "h\u00e4t\u00b7test", "du", "das", "Frie\u00b7den\u00b7samt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.