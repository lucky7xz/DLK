", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst die H\u00f6chsten schauten's ohne Neid;", "tokens": ["Und", "selbst", "die", "H\u00f6chs\u00b7ten", "schau\u00b7ten's", "oh\u00b7ne", "Neid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein frommer Greis, gel\u00e4utert, hocherfahren,", "tokens": ["Ein", "from\u00b7mer", "Greis", ",", "ge\u00b7l\u00e4u\u00b7tert", ",", "ho\u00b7cher\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "W\u00fcrd'st du befragt um Zeit und Ewigkeit,", "tokens": ["W\u00fcrd'st", "du", "be\u00b7fragt", "um", "Zeit", "und", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und ernste Denker, edle K\u00fcnstler, Dichter,", "tokens": ["Und", "erns\u00b7te", "Den\u00b7ker", ",", "ed\u00b7le", "K\u00fcnst\u00b7ler", ",", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie riefen dich als ihres Strebens Richter.", "tokens": ["Sie", "rie\u00b7fen", "dich", "als", "ih\u00b7res", "Stre\u00b7bens", "Rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-",