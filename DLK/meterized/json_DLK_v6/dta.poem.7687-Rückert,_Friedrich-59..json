{"dta.poem.7687": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du siehst, da\u00df leicht wie Nichts dem einen von der Hand", "tokens": ["Du", "siehst", ",", "da\u00df", "leicht", "wie", "Nichts", "dem", "ei\u00b7nen", "von", "der", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJD", "KOKOM", "PIS", "ART", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geht etwas, das gar schwer dir geht in den Verstand.", "tokens": ["Geht", "et\u00b7was", ",", "das", "gar", "schwer", "dir", "geht", "in", "den", "Ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "ADV", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dagegen wei\u00dft du flink mit etwas umzuspringen,", "tokens": ["Da\u00b7ge\u00b7gen", "wei\u00dft", "du", "flink", "mit", "et\u00b7was", "um\u00b7zu\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "APPR", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wovon dem andern fast will kein Begriff gelingen.", "tokens": ["Wo\u00b7von", "dem", "an\u00b7dern", "fast", "will", "kein", "Be\u00b7griff", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADV", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Entweder wenn du nun das Deine sch\u00e4tzest hoch,", "tokens": ["Ent\u00b7we\u00b7der", "wenn", "du", "nun", "das", "Dei\u00b7ne", "sch\u00e4t\u00b7zest", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "PPOSAT", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sch\u00e4tze nicht gering auch das des andern doch.", "tokens": ["So", "sch\u00e4t\u00b7ze", "nicht", "ge\u00b7ring", "auch", "das", "des", "an\u00b7dern", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADJD", "ADV", "ART", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wenn du dieses willst anschlagen so gering,", "tokens": ["Und", "wenn", "du", "die\u00b7ses", "willst", "an\u00b7schla\u00b7gen", "so", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "VMFIN", "VVINF", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So halte deines auch f\u00fcr kein so gro\u00dfes Ding.", "tokens": ["So", "hal\u00b7te", "dei\u00b7nes", "auch", "f\u00fcr", "kein", "so", "gro\u00b7\u00dfes", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADV", "APPR", "PIAT", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}