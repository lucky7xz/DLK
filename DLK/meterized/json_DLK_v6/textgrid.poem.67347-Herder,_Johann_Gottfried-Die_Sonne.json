{"textgrid.poem.67347": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbund sollt' der Eulen ganzes Heer", "tokens": ["\u00bb", "und", "sollt'", "der", "Eu\u00b7len", "gan\u00b7zes", "Heer"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Sonnenlicht erblinden,", "tokens": ["Am", "Son\u00b7nen\u00b7licht", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch sendet sie ihr Strahlenmeer,", "tokens": ["Noch", "sen\u00b7det", "sie", "ihr", "Strah\u00b7len\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Das weite Weltall um sich her", "tokens": ["Das", "wei\u00b7te", "Wel\u00b7tall", "um", "sich", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Leben zu entz\u00fcnden.\u00ab", "tokens": ["Mit", "Le\u00b7ben", "zu", "ent\u00b7z\u00fcn\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch sieh, wie sanft sich in der Luft", "tokens": ["Doch", "sieh", ",", "wie", "sanft", "sich", "in", "der", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nebel rings zerstreuen!", "tokens": ["Die", "Ne\u00b7bel", "rings", "zer\u00b7streu\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie l\u00e4\u00dft den Eulen ihre Kluft,", "tokens": ["Sie", "l\u00e4\u00dft", "den", "Eu\u00b7len", "ih\u00b7re", "Kluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Maulwurf seine d\u00fcstre Gruft", "tokens": ["Dem", "Maul\u00b7wurf", "sei\u00b7ne", "d\u00fcst\u00b7re", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und will mit Licht erfreuen.", "tokens": ["Und", "will", "mit", "Licht", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So la\u00df, wenn Deine Flammen gl\u00fchn,", "tokens": ["So", "la\u00df", ",", "wenn", "Dei\u00b7ne", "Flam\u00b7men", "gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht schrecken sie, nicht toben!", "tokens": ["Nicht", "schre\u00b7cken", "sie", ",", "nicht", "to\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df, wer da fliehen will, entfliehn!", "tokens": ["La\u00df", ",", "wer", "da", "flie\u00b7hen", "will", ",", "ent\u00b7fliehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$,", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Was bl\u00fchen kann, wird durch sie bl\u00fchn", "tokens": ["Was", "bl\u00fc\u00b7hen", "kann", ",", "wird", "durch", "sie", "bl\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVINF", "VMFIN", "$,", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Dich als Sonne loben.", "tokens": ["Und", "Dich", "als", "Son\u00b7ne", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}