{"textgrid.poem.35216": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[ihr Perlen dieser Heid/ ihr zarten Wasserballen]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Perlen dieser Heid/ ihr zarten Wasserballen/", "tokens": ["Ihr", "Per\u00b7len", "die\u00b7ser", "Heid", "/", "ihr", "zar\u00b7ten", "Was\u00b7ser\u00b7bal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Kinder k\u00fcler Nacht/ ihr hellen Feld-Krystallen/", "tokens": ["Ihr", "Kin\u00b7der", "k\u00fc\u00b7ler", "Nacht", "/", "ihr", "hel\u00b7len", "Feld\u00b7Kry\u00b7stal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Silbertropfen ihr/ die unser Ph\u00f6bus liebt/", "tokens": ["Ihr", "Sil\u00b7ber\u00b7trop\u00b7fen", "ihr", "/", "die", "un\u00b7ser", "Ph\u00f6\u00b7bus", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die unsre Felderlust mit Lust und Leben laben/", "tokens": ["Die", "uns\u00b7re", "Fel\u00b7der\u00b7lust", "mit", "Lust", "und", "Le\u00b7ben", "la\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Taut fenner Wolkenab/ giest/ wann der Schatt\u0113 giebt/", "tokens": ["Taut", "fen\u00b7ner", "Wol\u00b7ken\u00b7ab", "/", "giest", "/", "wann", "der", "Schat\u00b7t\u0113", "giebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "VVFIN", "$(", "PWAV", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "So kriegen wir durch euch auch fette Feldergaben/", "tokens": ["So", "krie\u00b7gen", "wir", "durch", "euch", "auch", "fet\u00b7te", "Fel\u00b7der\u00b7ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird von eurem Na\u00df des Ph\u00f6bus Hitz ge\u00fcbt/", "tokens": ["So", "wird", "von", "eu\u00b7rem", "Na\u00df", "des", "Ph\u00f6\u00b7bus", "Hitz", "ge\u00b7\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df unsre Brachen fr\u00fc die warmen Strahlen haben.", "tokens": ["Da\u00df", "uns\u00b7re", "Bra\u00b7chen", "fr\u00fc", "die", "war\u00b7men", "Strah\u00b7len", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}