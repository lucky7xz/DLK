{"textgrid.poem.48410": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Auf eines von Gr\u00fcnental Leichbestattung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit, in der der Mensch sein Leben pflegt zu f\u00fchren,", "tokens": ["Die", "Zeit", ",", "in", "der", "der", "Mensch", "sein", "Le\u00b7ben", "pflegt", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist wie ein gr\u00fcner Tal, den frische Bl\u00e4tter zieren,", "tokens": ["ist", "wie", "ein", "gr\u00fc\u00b7ner", "Tal", ",", "den", "fri\u00b7sche", "Bl\u00e4t\u00b7ter", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da Blumen aller Art im k\u00fchlen Grunde stehn", "tokens": ["da", "Blu\u00b7men", "al\u00b7ler", "Art", "im", "k\u00fch\u00b7len", "Grun\u00b7de", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PIAT", "NN", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und um den lautren Quell und stillen Bach aufgehn", "tokens": ["und", "um", "den", "laut\u00b7ren", "Quell", "und", "stil\u00b7len", "Bach", "auf\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "in ungez\u00e4hlter Zahl. Itzt, wenn die Scho\u00df der Erden", "tokens": ["in", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Zahl", ".", "Itzt", ",", "wenn", "die", "Scho\u00df", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "von einer manbarn Luft geschw\u00e4ngert pflegt zu werden,", "tokens": ["von", "ei\u00b7ner", "man\u00b7barn", "Luft", "ge\u00b7schw\u00e4n\u00b7gert", "pflegt", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "VVFIN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "gebiert manch sch\u00f6nes Kind; wenn das verlebte Jahr", "tokens": ["ge\u00b7biert", "manch", "sch\u00f6\u00b7nes", "Kind", ";", "wenn", "das", "ver\u00b7leb\u00b7te", "Jahr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ein J\u00fcngling wieder wird, da schl\u00e4gt das junge Haar", "tokens": ["ein", "J\u00fcng\u00b7ling", "wie\u00b7der", "wird", ",", "da", "schl\u00e4gt", "das", "jun\u00b7ge", "Haar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "den Lindenb\u00e4umen aus. Der angen\u00e4me Reif", "tokens": ["den", "Lin\u00b7den\u00b7b\u00e4u\u00b7men", "aus", ".", "Der", "an\u00b7ge\u00b7n\u00e4\u00b7me", "Reif"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "macht bei gesunder Nacht die schwachen Gr\u00e4ser steif,", "tokens": ["macht", "bei", "ge\u00b7sun\u00b7der", "Nacht", "die", "schwa\u00b7chen", "Gr\u00e4\u00b7ser", "steif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "die Sonne wirkt die Frucht: stets wird was Neues funden,", "tokens": ["die", "Son\u00b7ne", "wirkt", "die", "Frucht", ":", "stets", "wird", "was", "Neu\u00b7es", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "ADV", "VAFIN", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "das Jahr ist niemals leer, es tauschet alle Stunden.", "tokens": ["das", "Jahr", "ist", "nie\u00b7mals", "leer", ",", "es", "tau\u00b7schet", "al\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Eins kan nicht allzeit sein, wie denn auch Alles nicht.", "tokens": ["Eins", "kan", "nicht", "all\u00b7zeit", "sein", ",", "wie", "denn", "auch", "Al\u00b7les", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "ADV", "VAINF", "$,", "PWAV", "ADV", "ADV", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn sich der Hyacinth mit seiner Zier entbricht,", "tokens": ["Wenn", "sich", "der", "Hya\u00b7cin\u00b7th", "mit", "sei\u00b7ner", "Zier", "ent\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "da sind die Tulpen dar. Wenn diese sind vergangen,", "tokens": ["da", "sind", "die", "Tul\u00b7pen", "dar", ".", "Wenn", "die\u00b7se", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$.", "KOUS", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da stehn Paeonien und Rosen in dem Prangen.", "tokens": ["da", "stehn", "Pae\u00b7o\u00b7ni\u00b7en", "und", "Ro\u00b7sen", "in", "dem", "Pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.17": {"text": "Itzt schosset di\u00df herf\u00fcr, itzt f\u00e4llet jenes ab;", "tokens": ["Itzt", "schos\u00b7set", "di\u00df", "her\u00b7f\u00fcr", ",", "itzt", "f\u00e4l\u00b7let", "je\u00b7nes", "ab", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "PTKVZ", "$,", "ADV", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "was Eines wieder war, das ist des Andern Grab.", "tokens": ["was", "Ei\u00b7nes", "wie\u00b7der", "war", ",", "das", "ist", "des", "An\u00b7dern", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VAFIN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bald k\u00f6mpt der fr\u00f6de Herbst mit seinen kranken L\u00fcften,", "tokens": ["Bald", "k\u00f6mpt", "der", "fr\u00f6\u00b7de", "Herbst", "mit", "sei\u00b7nen", "kran\u00b7ken", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "mit den er alle Zier wei\u00df t\u00f6tlich zu vergiften.", "tokens": ["mit", "den", "er", "al\u00b7le", "Zier", "wei\u00df", "t\u00f6t\u00b7lich", "zu", "ver\u00b7gif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Schwind- und Gelbesucht greift B\u00e4um' und Bl\u00e4tter an,", "tokens": ["Die", "Schwin\u00b7d", "und", "Gel\u00b7be\u00b7sucht", "greift", "B\u00e4um'", "und", "Bl\u00e4t\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "der Saft vertrucknet aus, der matten Erden Man,", "tokens": ["der", "Saft", "ver\u00b7truck\u00b7net", "aus", ",", "der", "mat\u00b7ten", "Er\u00b7den", "Man", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "der m\u00fcde Himmel greist. Die Mutter, die veraltet,", "tokens": ["der", "m\u00fc\u00b7de", "Him\u00b7mel", "greist", ".", "Die", "Mut\u00b7ter", ",", "die", "ver\u00b7al\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "wird runzlicht an der Haut, die Fruchtbarkeit erkaltet.", "tokens": ["wird", "runz\u00b7licht", "an", "der", "Haut", ",", "die", "Frucht\u00b7bar\u00b7keit", "er\u00b7kal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der halb erfrorne Nord weht durch das schwache Tal,", "tokens": ["Der", "halb", "er\u00b7fror\u00b7ne", "Nord", "weht", "durch", "das", "schwa\u00b7che", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "macht das Gefelde blo\u00df, die kranken B\u00e4ume kahl,", "tokens": ["macht", "das", "Ge\u00b7fel\u00b7de", "blo\u00df", ",", "die", "kran\u00b7ken", "B\u00e4u\u00b7me", "kahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.27": {"text": "rei\u00dft alles mit sich hin, verbl\u00e4st dem stillen Quelle", "tokens": ["rei\u00dft", "al\u00b7les", "mit", "sich", "hin", ",", "ver\u00b7bl\u00e4st", "dem", "stil\u00b7len", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "PRF", "PTKVZ", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "den sonst gewohnten Pa\u00df, da\u00df er nicht von der Stelle,", "tokens": ["den", "sonst", "ge\u00b7wohn\u00b7ten", "Pa\u00df", ",", "da\u00df", "er", "nicht", "von", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "nicht vor sich rinnen kan. Wo ist alsdenn die Zeit,", "tokens": ["nicht", "vor", "sich", "rin\u00b7nen", "kan", ".", "Wo", "ist", "als\u00b7denn", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PRF", "VVINF", "VMFIN", "$.", "PWAV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "die Zier, die sch\u00f6ne Lust mit aller Fr\u00f6ligkeit?", "tokens": ["die", "Zier", ",", "die", "sch\u00f6\u00b7ne", "Lust", "mit", "al\u00b7ler", "Fr\u00f6\u00b7lig\u00b7keit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So ist es auch bewandt um aller Menschen Sachen;", "tokens": ["So", "ist", "es", "auch", "be\u00b7wandt", "um", "al\u00b7ler", "Men\u00b7schen", "Sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "ihr Leben ist der Tal, der uns itzt Freude machen,", "tokens": ["ihr", "Le\u00b7ben", "ist", "der", "Tal", ",", "der", "uns", "itzt", "Freu\u00b7de", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "itzt Unlust geben kan. Die Blumen sind selbst sie", "tokens": ["itzt", "Un\u00b7lust", "ge\u00b7ben", "kan", ".", "Die", "Blu\u00b7men", "sind", "selbst", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVINF", "VMFIN", "$.", "ART", "NN", "VAFIN", "ADV", "PPER"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.34": {"text": "mit aller Zier und Pracht, da diese balde fr\u00fch'", "tokens": ["mit", "al\u00b7ler", "Zier", "und", "Pracht", ",", "da", "die\u00b7se", "bal\u00b7de", "fr\u00fch'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "und jene spat verf\u00e4llt. Hier gilt es nicht zu bauen", "tokens": ["und", "je\u00b7ne", "spat", "ver\u00b7f\u00e4llt", ".", "Hier", "gilt", "es", "nicht", "zu", "bau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADV", "VVPP", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "auf seiner Jugend Zeit. Die Jungen, wie die Grauen", "tokens": ["auf", "sei\u00b7ner", "Ju\u00b7gend", "Zeit", ".", "Die", "Jun\u00b7gen", ",", "wie", "die", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "sind stets dem Tode reif. Die Veilge, die schlug aus", "tokens": ["sind", "stets", "dem", "To\u00b7de", "reif", ".", "Die", "Veil\u00b7ge", ",", "die", "schlug", "aus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$.", "ART", "NN", "$,", "PRELS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "vor sieben Tagen schon, und die kaum halb ist raus,", "tokens": ["vor", "sie\u00b7ben", "Ta\u00b7gen", "schon", ",", "und", "die", "kaum", "halb", "ist", "raus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "$,", "KON", "ART", "ADV", "ADJD", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "meit eine Sichel ab. Die fl\u00fcchtigen Narcissen", "tokens": ["meit", "ei\u00b7ne", "Si\u00b7chel", "ab", ".", "Die", "fl\u00fcch\u00b7ti\u00b7gen", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "sind drum geringer nicht, ob sie schon bald hin m\u00fcssen,", "tokens": ["sind", "drum", "ge\u00b7rin\u00b7ger", "nicht", ",", "ob", "sie", "schon", "bald", "hin", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADJD", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "als etwan Ro\u00dfmarin, die zwar sehr lange steht,", "tokens": ["als", "et\u00b7wan", "Ro\u00df\u00b7ma\u00b7rin", ",", "die", "zwar", "sehr", "lan\u00b7ge", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "doch, wenn der Frost bei\u00dft an, zugleich auch untergeht.", "tokens": ["doch", ",", "wenn", "der", "Frost", "bei\u00dft", "an", ",", "zu\u00b7gleich", "auch", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wir haben nur ein Ziel, wie auch die Blumen haben:", "tokens": ["Wir", "ha\u00b7ben", "nur", "ein", "Ziel", ",", "wie", "auch", "die", "Blu\u00b7men", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "es sei fr\u00fch oder spat, wir werden doch vergraben", "tokens": ["es", "sei", "fr\u00fch", "o\u00b7der", "spat", ",", "wir", "wer\u00b7den", "doch", "ver\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "in unser Mutter Scho\u00df. Di\u00df fehlet uns allein,", "tokens": ["in", "un\u00b7ser", "Mut\u00b7ter", "Scho\u00df", ".", "Di\u00df", "feh\u00b7let", "uns", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "da\u00df wir geringer noch als alle Blumen sein.", "tokens": ["da\u00df", "wir", "ge\u00b7rin\u00b7ger", "noch", "als", "al\u00b7le", "Blu\u00b7men", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "KOKOM", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Zeit, die itzt verschlei\u00dft, kan sich an sich erholen,", "tokens": ["Die", "Zeit", ",", "die", "itzt", "ver\u00b7schlei\u00dft", ",", "kan", "sich", "an", "sich", "er\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VMFIN", "PRF", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.48": {"text": "das Laub schl\u00e4gt wieder aus, die sterbenden Violen", "tokens": ["das", "Laub", "schl\u00e4gt", "wie\u00b7der", "aus", ",", "die", "ster\u00b7ben\u00b7den", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "bekommen ihren Geist, die Wasser tauen auf.", "tokens": ["be\u00b7kom\u00b7men", "ih\u00b7ren", "Geist", ",", "die", "Was\u00b7ser", "tau\u00b7en", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Sind wir nur einmal hin, da gilt kein Wiederlauf,", "tokens": ["Sind", "wir", "nur", "ein\u00b7mal", "hin", ",", "da", "gilt", "kein", "Wie\u00b7der\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "wir bleiben, wo wir sein. Di\u00df haben wir zu hoffen,", "tokens": ["wir", "blei\u00b7ben", ",", "wo", "wir", "sein", ".", "Di\u00df", "ha\u00b7ben", "wir", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "PPER", "VAINF", "$.", "PDS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "da\u00df noch ein gr\u00fcner Tal uns allen stehet offen,", "tokens": ["da\u00df", "noch", "ein", "gr\u00fc\u00b7ner", "Tal", "uns", "al\u00b7len", "ste\u00b7het", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "da zwar auch Blumen sein, nicht aber die vergehn;", "tokens": ["da", "zwar", "auch", "Blu\u00b7men", "sein", ",", "nicht", "a\u00b7ber", "die", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "VAINF", "$,", "PTKNEG", "ADV", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "daselbsten sollen wir auch unverg\u00e4nglich stehn,", "tokens": ["da\u00b7selbs\u00b7ten", "sol\u00b7len", "wir", "auch", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "den Amaranthen gleich. In diesen ist versetzet", "tokens": ["den", "A\u00b7ma\u00b7ran\u00b7then", "gleich", ".", "In", "die\u00b7sen", "ist", "ver\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "APPR", "PDS", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "auch unser Gr\u00fcnental; er ists, der sich ergetzet,", "tokens": ["auch", "un\u00b7ser", "Gr\u00fc\u00b7nen\u00b7tal", ";", "er", "ists", ",", "der", "sich", "er\u00b7get\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "der fromme Gottes-Freund, in einer solchen Lust,", "tokens": ["der", "from\u00b7me", "Got\u00b7tes\u00b7Freund", ",", "in", "ei\u00b7ner", "sol\u00b7chen", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "die er zwar oft genant, doch aber nie gewust.", "tokens": ["die", "er", "zwar", "oft", "ge\u00b7nant", ",", "doch", "a\u00b7ber", "nie", "ge\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Da gr\u00fcnt der Gr\u00fcnental, da wird er nicht verwelken,", "tokens": ["Da", "gr\u00fcnt", "der", "Gr\u00fc\u00b7nen\u00b7tal", ",", "da", "wird", "er", "nicht", "ver\u00b7wel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "gibt einen Ruch von sich, wie die gesunden Nelken,", "tokens": ["gibt", "ei\u00b7nen", "Ruch", "von", "sich", ",", "wie", "die", "ge\u00b7sun\u00b7den", "Nel\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PRF", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "an die Gott t\u00e4glich reucht, nach welcher sch\u00f6nen Blum'", "tokens": ["an", "die", "Gott", "t\u00e4g\u00b7lich", "reucht", ",", "nach", "wel\u00b7cher", "sch\u00f6\u00b7nen", "Blum'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$,", "APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "auch reucht des Edelen gelobter Nam' und Ruhm.", "tokens": ["auch", "reucht", "des", "E\u00b7de\u00b7len", "ge\u00b7lob\u00b7ter", "Nam'", "und", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}