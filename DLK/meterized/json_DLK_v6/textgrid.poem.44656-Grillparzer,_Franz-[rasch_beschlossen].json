{"textgrid.poem.44656": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[rasch beschlossen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rasch beschlossen", "tokens": ["Rasch", "be\u00b7schlos\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Unanimiter versto\u00dfen!", "tokens": ["Un\u00b7a\u00b7ni\u00b7mi\u00b7ter", "ver\u00b7sto\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Miraculum!", "tokens": ["Mi\u00b7ra\u00b7cu\u00b7lum", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Welch Gremium!", "tokens": ["Welch", "Gre\u00b7mi\u00b7um", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Solch Einigkeit!", "tokens": ["Solch", "Ei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Keine Kleinigkeit!", "tokens": ["Kei\u00b7ne", "Klei\u00b7nig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da wird man inne,", "tokens": ["Da", "wird", "man", "in\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Was \u00dcbles der ", "tokens": ["Was", "\u00dcb\u00b7les", "der"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PIS", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mehr K\u00f6pfe, mehr Sinne!", "tokens": ["Mehr", "K\u00f6p\u00b7fe", ",", "mehr", "Sin\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und so viel ", "tokens": ["Und", "so", "viel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Einig wie Br\u00fcder!", "tokens": ["Ei\u00b7nig", "wie", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}