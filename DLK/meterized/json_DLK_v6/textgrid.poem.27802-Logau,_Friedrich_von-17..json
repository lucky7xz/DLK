{"textgrid.poem.27802": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr gro\u00dfer Herren Mund geh\u00f6rt das Allerbeste,", "tokens": ["F\u00fcr", "gro\u00b7\u00dfer", "Her\u00b7ren", "Mund", "ge\u00b7h\u00f6rt", "das", "Al\u00b7ler\u00b7bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mag leichte, wo sich von ein grober Bauer m\u00e4ste.", "tokens": ["Mag", "leich\u00b7te", ",", "wo", "sich", "von", "ein", "gro\u00b7ber", "Bau\u00b7er", "m\u00e4s\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "$,", "PWAV", "PRF", "APPR", "ART", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Fried ist eine Kost, die k\u00f6stlich n\u00e4hrt und speist;", "tokens": ["Der", "Fried", "ist", "ei\u00b7ne", "Kost", ",", "die", "k\u00f6st\u00b7lich", "n\u00e4hrt", "und", "speist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum wird gemeiner Mann davon ietzt abgeweist.", "tokens": ["Drum", "wird", "ge\u00b7mei\u00b7ner", "Mann", "da\u00b7von", "ietzt", "ab\u00b7ge\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ADJA", "NN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}