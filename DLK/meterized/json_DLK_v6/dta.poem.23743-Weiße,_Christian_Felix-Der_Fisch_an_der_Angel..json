{"dta.poem.23743": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Fisch an der Angel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Das kleine Fischgen spielet hier", "tokens": ["Das", "klei\u00b7ne", "Fischgen", "spie\u00b7let", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In silbernem Bach,", "tokens": ["In", "sil\u00b7ber\u00b7nem", "Bach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und h\u00e4ngt, voll l\u00fcsterner Begier,", "tokens": ["Und", "h\u00e4ngt", ",", "voll", "l\u00fcs\u00b7ter\u00b7ner", "Be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blo\u00df seinen Freuden nach.", "tokens": ["Blo\u00df", "sei\u00b7nen", "Freu\u00b7den", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es merket nicht die blutge List,", "tokens": ["Es", "mer\u00b7ket", "nicht", "die", "blut\u00b7ge", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den freundlichen Feind,", "tokens": ["Den", "freund\u00b7li\u00b7chen", "Feind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der desto mehr zu f\u00fcrchten ist,", "tokens": ["Der", "des\u00b7to", "mehr", "zu", "f\u00fcrch\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Je g\u00fctiger er scheint.", "tokens": ["Je", "g\u00fc\u00b7ti\u00b7ger", "er", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Ruthe mit der Angel spielt", "tokens": ["Die", "Ru\u00b7the", "mit", "der", "An\u00b7gel", "spielt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schon \u00fcber ihn hin,", "tokens": ["Schon", "\u00fc\u00b7ber", "ihn", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und voller Neubegierde schielt", "tokens": ["Und", "vol\u00b7ler", "Neu\u00b7be\u00b7gier\u00b7de", "schielt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es blo\u00df nach dem Gewinn.", "tokens": ["Es", "blo\u00df", "nach", "dem", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es naht sich schon &#8212; itzt schnappt es zu!", "tokens": ["Es", "naht", "sich", "schon", "&#8212;", "itzt", "schnappt", "es", "zu", "!"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was hast du gethan?", "tokens": ["Was", "hast", "du", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du blutest, armes Thierchen du,", "tokens": ["Du", "blu\u00b7test", ",", "ar\u00b7mes", "Thier\u00b7chen", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O bissest du nicht an! \u2014", "tokens": ["O", "bis\u00b7sest", "du", "nicht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mich rei\u00dfe nie, was mir gef\u00e4llt,", "tokens": ["Mich", "rei\u00b7\u00dfe", "nie", ",", "was", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Unpr\u00fcfend dahin!", "tokens": ["Un\u00b7pr\u00fc\u00b7fend", "da\u00b7hin", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dein Beyspiel lehre mich die Welt", "tokens": ["Dein", "Bey\u00b7spiel", "leh\u00b7re", "mich", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Reizung fliehn!", "tokens": ["Und", "ih\u00b7re", "Rei\u00b7zung", "fliehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}