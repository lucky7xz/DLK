{"textgrid.poem.58600": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, ich bin der Herr, und ist au\u00dfer mir kein Heiland. Jes. 43, 11", "tokens": ["Ich", ",", "ich", "bin", "der", "Herr", ",", "und", "ist", "au\u00b7\u00dfer", "mir", "kein", "Hei\u00b7land", ".", "Jes", ".", "43", ",", "11"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "KON", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich bin's, ich bin's, was du je kannst begehren,", "tokens": ["Ich", "bin's", ",", "ich", "bin's", ",", "was", "du", "je", "kannst", "be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich bin's, ich bin's, und nicht die Kreatur;", "tokens": ["Ich", "bin's", ",", "ich", "bin's", ",", "und", "nicht", "die", "Kre\u00b7a\u00b7tur", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "$,", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie bleibt nicht lang, sie kann den Geist nicht n\u00e4hren,", "tokens": ["Sie", "bleibt", "nicht", "lang", ",", "sie", "kann", "den", "Geist", "nicht", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "$,", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Gott, dein Gut bin ich, Jehova, nur.", "tokens": ["Dein", "Gott", ",", "dein", "Gut", "bin", "ich", ",", "Je\u00b7ho\u00b7va", ",", "nur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "$,", "NE", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich kann allein dein S\u00fcndenelend heilen,", "tokens": ["Ich", "kann", "al\u00b7lein", "dein", "S\u00fcn\u00b7de\u00b7ne\u00b7lend", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dein eignes Tun, o Seele, reicht nicht zu,", "tokens": ["Dein", "eig\u00b7nes", "Tun", ",", "o", "See\u00b7le", ",", "reicht", "nicht", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kehr ein in mich, wirst du dein Herz nicht teilen,", "tokens": ["Kehr", "ein", "in", "mich", ",", "wirst", "du", "dein", "Herz", "nicht", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "PPER", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So schenk' ich hier schon Seligkeit und Ruh!", "tokens": ["So", "schenk'", "ich", "hier", "schon", "Se\u00b7lig\u00b7keit", "und", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}