{"textgrid.poem.65796": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "70", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df du geschm\u00e4ht wirst, nicht ver\u00fcbl' ich's dir;", "tokens": ["Da\u00df", "du", "ge\u00b7schm\u00e4ht", "wirst", ",", "nicht", "ver\u00b7\u00fcbl'", "ich's", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "PTKNEG", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn stets war Anmut der Verleumdung Ziel.", "tokens": ["Denn", "stets", "war", "An\u00b7mut", "der", "Ver\u00b7leum\u00b7dung", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verdacht und Argwohn sind des Sch\u00f6nen Zier,", "tokens": ["Ver\u00b7dacht", "und", "Arg\u00b7wohn", "sind", "des", "Sch\u00f6\u00b7nen", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Himmelblau ein schwarzes Kr\u00e4henspiel.", "tokens": ["Im", "Him\u00b7mel\u00b7blau", "ein", "schwar\u00b7zes", "Kr\u00e4\u00b7hen\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn gut du bist, bew\u00e4hrt Verleumdung deine G\u00fcte", "tokens": ["Wenn", "gut", "du", "bist", ",", "be\u00b7w\u00e4hrt", "Ver\u00b7leum\u00b7dung", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "VAFIN", "$,", "VVFIN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur desto reiner, weil dich Welt umkost:", "tokens": ["Nur", "des\u00b7to", "rei\u00b7ner", ",", "weil", "dich", "Welt", "um\u00b7kost", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Denn Lasters Wurm liebt sich die sch\u00f6nste Bl\u00fcte,", "tokens": ["Denn", "Las\u00b7ters", "Wurm", "liebt", "sich", "die", "sch\u00f6ns\u00b7te", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dein Lenz zeigt sich rein und fleckenlos.", "tokens": ["Und", "dein", "Lenz", "zeigt", "sich", "rein", "und", "fle\u00b7cken\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Durch Jugendhinterhalte bist du dicht,", "tokens": ["Durch", "Ju\u00b7gend\u00b7hin\u00b7ter\u00b7hal\u00b7te", "bist", "du", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Bald unber\u00fchrt, bald siegreich durchgedrungen;", "tokens": ["Bald", "un\u00b7be\u00b7r\u00fchrt", ",", "bald", "sieg\u00b7reich", "durch\u00b7ge\u00b7drun\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und dennoch fesselt dieser Ruhm dir nicht", "tokens": ["Und", "den\u00b7noch", "fes\u00b7selt", "die\u00b7ser", "Ruhm", "dir", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PDAT", "NN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Die ewig losgelassnen b\u00f6sen Zungen.", "tokens": ["Die", "e\u00b7wig", "los\u00b7ge\u00b7lass\u00b7nen", "b\u00f6\u00b7sen", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Du einzig m\u00fc\u00dftest, ohne schlimmen Schein,", "tokens": ["Du", "ein\u00b7zig", "m\u00fc\u00df\u00b7test", ",", "oh\u00b7ne", "schlim\u00b7men", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VMFIN", "$,", "KOUI", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Von Herzensk\u00f6nigreichen Meister sein.", "tokens": ["Von", "Her\u00b7zens\u00b7k\u00f6\u00b7nig\u00b7rei\u00b7chen", "Meis\u00b7ter", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}