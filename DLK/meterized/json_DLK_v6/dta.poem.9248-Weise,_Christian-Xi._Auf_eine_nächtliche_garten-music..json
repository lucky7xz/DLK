{"dta.poem.9248": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n Auf eine n\u00e4chtliche garten-music.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6ne lust! was soll man dir vergleichen?", "tokens": ["Du", "sch\u00f6\u00b7ne", "lust", "!", "was", "soll", "man", "dir", "ver\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PWS", "VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sonne w\u00fcnscht der erden gute nacht", "tokens": ["Die", "son\u00b7ne", "w\u00fcnscht", "der", "er\u00b7den", "gu\u00b7te", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00e4st die glut der strengen stralen weichen/", "tokens": ["Und", "l\u00e4st", "die", "glut", "der", "stren\u00b7gen", "stra\u00b7len", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indem der West di\u00df ufer lieblich macht/", "tokens": ["In\u00b7dem", "der", "West", "di\u00df", "u\u00b7fer", "lieb\u00b7lich", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie spielen die l\u00fcffte/ wie saussen die b\u00e4ume/", "tokens": ["Wie", "spie\u00b7len", "die", "l\u00fcff\u00b7te", "/", "wie", "saus\u00b7sen", "die", "b\u00e4u\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "$(", "PWAV", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Wie rauschen die wellen/ und k\u00fctzeln die tr\u00e4ume.", "tokens": ["Wie", "rau\u00b7schen", "die", "wel\u00b7len", "/", "und", "k\u00fct\u00b7zeln", "die", "tr\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "VVINF", "$(", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "2. Der heisse tag ist allbereit betrogen/", "tokens": ["Der", "heis\u00b7se", "tag", "ist", "all\u00b7be\u00b7reit", "be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die k\u00fchle nacht erf\u00fcllt den edlen rest/", "tokens": ["Die", "k\u00fch\u00b7le", "nacht", "er\u00b7f\u00fcllt", "den", "ed\u00b7len", "rest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil sie der glut ihr antheil hat entzogen/", "tokens": ["Weil", "sie", "der", "glut", "ihr", "an\u00b7theil", "hat", "ent\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und uns die zeit also geniessen l\u00e4st.", "tokens": ["Und", "uns", "die", "zeit", "al\u00b7so", "ge\u00b7nies\u00b7sen", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So m\u00fcssen die l\u00fcste der irrdischen schatten", "tokens": ["So", "m\u00fcs\u00b7sen", "die", "l\u00fcs\u00b7te", "der", "irr\u00b7di\u00b7schen", "schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "ART", "ADJA", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Den mangel des himmlischen liechtes erstatten.", "tokens": ["Den", "man\u00b7gel", "des", "himm\u00b7li\u00b7schen", "liech\u00b7tes", "er\u00b7stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "ADJA", "VVFIN", "$."], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "3. Die erde ruht und wartet schon auff morgen/", "tokens": ["Die", "er\u00b7de", "ruht", "und", "war\u00b7tet", "schon", "auff", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "APPR", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sehn die zier der stillen nacht-zeit an/", "tokens": ["Wir", "sehn", "die", "zier", "der", "stil\u00b7len", "nacht\u00b7zeit", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da wollen wir ein halbes st\u00fcndgen borgen/", "tokens": ["Da", "wol\u00b7len", "wir", "ein", "hal\u00b7bes", "st\u00fcnd\u00b7gen", "bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo nur der tag die nacht bezahlen kan/", "tokens": ["Wo", "nur", "der", "tag", "die", "nacht", "be\u00b7zah\u00b7len", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So schlaffet ihr wiesen und lasset uns wachen/", "tokens": ["So", "schlaf\u00b7fet", "ihr", "wie\u00b7sen", "und", "las\u00b7set", "uns", "wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der himmel bef\u00f6rdert die lustigen sachen.", "tokens": ["Der", "him\u00b7mel", "be\u00b7f\u00f6r\u00b7dert", "die", "lus\u00b7ti\u00b7gen", "sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "4. Doch d\u00fcrffen wir uns dieses gl\u00fccke nehmen/", "tokens": ["Doch", "d\u00fcrf\u00b7fen", "wir", "uns", "die\u00b7ses", "gl\u00fc\u00b7cke", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df wir den klang der s\u00fcssen seiten ziehn/", "tokens": ["Da\u00df", "wir", "den", "klang", "der", "s\u00fcs\u00b7sen", "sei\u00b7ten", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die jugend will sich in den schlaff beqvemen/", "tokens": ["Die", "ju\u00b7gend", "will", "sich", "in", "den", "schlaff", "be\u00b7qve\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ART", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und unser thon will ihren sinn bem\u00fchn:", "tokens": ["Und", "un\u00b7ser", "thon", "will", "ih\u00b7ren", "sinn", "be\u00b7m\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die niedlichen winde befehlen im k\u00fchlen", "tokens": ["Die", "nied\u00b7li\u00b7chen", "win\u00b7de", "be\u00b7feh\u00b7len", "im", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Den hurtigen seiten was sachter zu spielen.", "tokens": ["Den", "hur\u00b7ti\u00b7gen", "sei\u00b7ten", "was", "sach\u00b7ter", "zu", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PWS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "5. Wir wollens thun/ verstummt ihr eitlen lieder/", "tokens": ["Wir", "wol\u00b7lens", "thun", "/", "ver\u00b7stummt", "ihr", "eit\u00b7len", "lie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verst\u00f6rt die ruh der edlen tugend nicht/", "tokens": ["Ver\u00b7st\u00f6rt", "die", "ruh", "der", "ed\u00b7len", "tu\u00b7gend", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Legt eure lust bey andern wiesen nieder/", "tokens": ["Legt", "eu\u00b7re", "lust", "bey", "an\u00b7dern", "wie\u00b7sen", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PIS", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil Venus hier schon durch die wolcken bricht.", "tokens": ["Weil", "Ve\u00b7nus", "hier", "schon", "durch", "die", "wol\u00b7cken", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und fordert bey allen vermeidlichen straffen/", "tokens": ["Und", "for\u00b7dert", "bey", "al\u00b7len", "ver\u00b7meid\u00b7li\u00b7chen", "straf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Man lasse die nachbarn in sicherheit schlaffen.", "tokens": ["Man", "las\u00b7se", "die", "nach\u00b7barn", "in", "si\u00b7cher\u00b7heit", "schlaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}