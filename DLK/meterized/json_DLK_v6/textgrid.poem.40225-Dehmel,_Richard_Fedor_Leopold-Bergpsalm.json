{"textgrid.poem.40225": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Bergpsalm", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sturm hat seine Schlangen losgelassen \u2013", "tokens": ["Der", "Sturm", "hat", "sei\u00b7ne", "Schlan\u00b7gen", "los\u00b7ge\u00b7las\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in langen Wogen rauschen Gras und Rohr \u2013", "tokens": ["in", "lan\u00b7gen", "Wo\u00b7gen", "rau\u00b7schen", "Gras", "und", "Rohr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "es zischt der See \u2013 die Weiden, silberblassen", "tokens": ["es", "zischt", "der", "See", "\u2013", "die", "Wei\u00b7den", ",", "sil\u00b7ber\u00b7blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zerw\u00fchlten Hauptes, seufzen laut empor.", "tokens": ["zer\u00b7w\u00fchl\u00b7ten", "Haup\u00b7tes", ",", "seuf\u00b7zen", "laut", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "auf kahler H\u00f6he will ich einsam stehn \u2013", "tokens": ["auf", "kah\u00b7ler", "H\u00f6\u00b7he", "will", "ich", "ein\u00b7sam", "stehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und meine ferne Heimat d\u00e4mmern sehn \u2013", "tokens": ["und", "mei\u00b7ne", "fer\u00b7ne", "Hei\u00b7mat", "d\u00e4m\u00b7mern", "sehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und h\u00f6ren, wie die dunkeln Wolken brausen.", "tokens": ["und", "h\u00f6\u00b7ren", ",", "wie", "die", "dun\u00b7keln", "Wol\u00b7ken", "brau\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr grauen Pilger \u00fcber mir \u2013 wohin?", "tokens": ["Ihr", "grau\u00b7en", "Pil\u00b7ger", "\u00fc\u00b7ber", "mir", "\u2013", "wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "$(", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O k\u00f6nnt' ich ", "tokens": ["O", "k\u00f6nnt'", "ich"], "token_info": ["word", "word", "word"], "pos": ["NE", "VMFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "wie ihr ", "tokens": ["wie", "ihr"], "token_info": ["word", "word"], "pos": ["PWAV", "PPER"], "meter": "++", "measure": "spondeus"}, "line.4": {"text": "und Seele \u2013 meiner ", "tokens": ["und", "See\u00b7le", "\u2013", "mei\u00b7ner"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "NN", "$(", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "O meine Heimat \u2013! schimmernd winkt der Flu\u00df", "tokens": ["O", "mei\u00b7ne", "Hei\u00b7mat", "\u2013", "!", "schim\u00b7mernd", "winkt", "der", "Flu\u00df"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$(", "$.", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und gr\u00fc\u00dft zum Himmel aus dem Blau der B\u00e4ume,", "tokens": ["und", "gr\u00fc\u00dft", "zum", "Him\u00b7mel", "aus", "dem", "Blau", "der", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "wie aus dem Zauberwald der Kindertr\u00e4ume", "tokens": ["wie", "aus", "dem", "Zau\u00b7ber\u00b7wald", "der", "Kin\u00b7der\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "der Mutter Blick, der Mutter reiner Ku\u00df ...", "tokens": ["der", "Mut\u00b7ter", "Blick", ",", "der", "Mut\u00b7ter", "rei\u00b7ner", "Ku\u00df", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "dort pulst im Dunst der Weltstadt zitternd Herz!", "tokens": ["dort", "pulst", "im", "Dunst", "der", "Welt\u00b7stadt", "zit\u00b7ternd", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "es zuckt ein Schrei auf ", "tokens": ["es", "zuckt", "ein", "Schrei", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "nach Gl\u00fcck und Frieden: Wurm, was ", "tokens": ["nach", "Gl\u00fcck", "und", "Frie\u00b7den", ":", "Wurm", ",", "was"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "NN", "$,", "PWS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "nicht sickert Einsam mehr von Brust zu Br\u00fcsten", "tokens": ["nicht", "si\u00b7ckert", "Ein\u00b7sam", "mehr", "von", "Brust", "zu", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "ADJD", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "aus ", "tokens": ["aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "heut st\u00f6hnt ein ", "tokens": ["heut", "st\u00f6hnt", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "und ", "tokens": ["und"], "token_info": ["word"], "pos": ["KON"], "meter": "-", "measure": "single.down"}}, "stanza.4": {"line.1": {"text": "siehst du den Qualm mit dicken F\u00e4usten drohn", "tokens": ["siehst", "du", "den", "Qualm", "mit", "di\u00b7cken", "F\u00e4us\u00b7ten", "drohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dort \u00fcberm Wald der ", "tokens": ["dort", "\u00fc\u00b7berm", "Wald", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "auf deine Reinheitsinbrunst schwarz ein Hohn", "tokens": ["auf", "dei\u00b7ne", "Rein\u00b7heits\u00b7in\u00b7brunst", "schwarz", "ein", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du hast ", "tokens": ["Du", "hast"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "in dumpfer Glut dich selber nur genossen:", "tokens": ["in", "dum\u00b7pfer", "Glut", "dich", "sel\u00b7ber", "nur", "ge\u00b7nos\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "sch\u00fctt' aus den ", "tokens": ["sch\u00fctt'", "aus", "den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "und deine S\u00fcnde stirbt im Eignen Schoo\u00df! \u2013", "tokens": ["und", "dei\u00b7ne", "S\u00fcn\u00b7de", "stirbt", "im", "Eig\u00b7nen", "Schoo\u00df", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und blutig gl\u00fcht es um die zackigen T\u00fcrme,", "tokens": ["Und", "blu\u00b7tig", "gl\u00fcht", "es", "um", "die", "za\u00b7cki\u00b7gen", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ein Dornenkranz umflammt die Stirn der Stadt;", "tokens": ["ein", "Dor\u00b7nen\u00b7kranz", "um\u00b7flammt", "die", "Stirn", "der", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "ein goldner F\u00e4cher scheucht die Wolkenst\u00fcrme,", "tokens": ["ein", "gold\u00b7ner", "F\u00e4\u00b7cher", "scheucht", "die", "Wol\u00b7ken\u00b7st\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "herniedersprie\u00dft ein Sonnenpalmenblatt.", "tokens": ["her\u00b7nie\u00b7der\u00b7sprie\u00dft", "ein", "Son\u00b7nen\u00b7pal\u00b7men\u00b7blatt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Herz der Weltstadt, Millionenstimme,", "tokens": ["O", "Herz", "der", "Welt\u00b7stadt", ",", "Mil\u00b7li\u00b7o\u00b7nen\u00b7stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "die gell nach Brot vor ", "tokens": ["die", "gell", "nach", "Brot", "vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "hinquillt wie Heilandsblut in diese Zeit,", "tokens": ["hin\u00b7quillt", "wie", "Hei\u00b7lands\u00b7blut", "in", "die\u00b7se", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "der Strom der ", "tokens": ["der", "Strom", "der"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Den Kelch des ", "tokens": ["Den", "Kelch", "des"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "das Kreuz der M\u00fchsal ", "tokens": ["das", "Kreuz", "der", "M\u00fch\u00b7sal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "knarrend die Kiefer \u00e4chzt, mein Mantel knattert:", "tokens": ["knar\u00b7rend", "die", "Kie\u00b7fer", "\u00e4chzt", ",", "mein", "Man\u00b7tel", "knat\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "nicht la\u00df in Tr\u00e4umen deine Glut vermodern,", "tokens": ["nicht", "la\u00df", "in", "Tr\u00e4u\u00b7men", "dei\u00b7ne", "Glut", "ver\u00b7mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVIMP", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "la\u00df du aus ", "tokens": ["la\u00df", "du", "aus"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Empor, mein Geist! Hinab, mein Herz \u2013 \u2013 hinab!", "tokens": ["Em\u00b7por", ",", "mein", "Geist", "!", "Hin\u00b7ab", ",", "mein", "Herz", "\u2013", "\u2013", "hin\u00b7ab", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "ADV", "$,", "PPOSAT", "NN", "$(", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}