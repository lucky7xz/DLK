{"textgrid.poem.30984": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hannen sind die Frauen g\u00fcnstig, weil sie ihre M\u00e4nner lehren,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hannen sind die Frauen g\u00fcnstig, weil sie ihre M\u00e4nner lehren,", "tokens": ["Han\u00b7nen", "sind", "die", "Frau\u00b7en", "g\u00fcns\u00b7tig", ",", "weil", "sie", "ih\u00b7re", "M\u00e4n\u00b7ner", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie sie ihnen sollen locken, sie mit Lust und Kost verehren", "tokens": ["Wie", "sie", "ih\u00b7nen", "sol\u00b7len", "lo\u00b7cken", ",", "sie", "mit", "Lust", "und", "Kost", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "VVINF", "$,", "PPER", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und fein r\u00fcstig fr\u00fc anfangen; sonsten ist nicht zu vergessen,", "tokens": ["Und", "fein", "r\u00fcs\u00b7tig", "fr\u00fc", "an\u00b7fan\u00b7gen", ";", "sons\u00b7ten", "ist", "nicht", "zu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ADJD", "VVINF", "$.", "ADV", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Da\u00df nicht minder junge Frauen gerne junge H\u00fcner essen.", "tokens": ["Da\u00df", "nicht", "min\u00b7der", "jun\u00b7ge", "Frau\u00b7en", "ger\u00b7ne", "jun\u00b7ge", "H\u00fc\u00b7ner", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}