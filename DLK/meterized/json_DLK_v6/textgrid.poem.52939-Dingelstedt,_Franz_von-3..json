{"textgrid.poem.52939": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So str\u00f6me denn in vollem Ma\u00df", "tokens": ["So", "str\u00f6\u00b7me", "denn", "in", "vol\u00b7lem", "Ma\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hernieder, du Maienregen,", "tokens": ["Her\u00b7nie\u00b7der", ",", "du", "Mai\u00b7en\u00b7re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ersehntes, k\u00f6stliches, tr\u00f6stliches Na\u00df,", "tokens": ["Er\u00b7sehn\u00b7tes", ",", "k\u00f6st\u00b7li\u00b7ches", ",", "tr\u00f6st\u00b7li\u00b7ches", "Na\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Befruchtender Erden-Segen!", "tokens": ["Be\u00b7fruch\u00b7ten\u00b7der", "Er\u00b7den\u00b7Se\u00b7gen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Lang konnte der Himmel bang und schwer", "tokens": ["Lang", "konn\u00b7te", "der", "Him\u00b7mel", "bang", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die lindernde Tr\u00e4ne nicht finden,", "tokens": ["Die", "lin\u00b7dern\u00b7de", "Tr\u00e4\u00b7ne", "nicht", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Seine Wimper, die Wolken, dr\u00fcckten sehr", "tokens": ["Sei\u00b7ne", "Wim\u00b7per", ",", "die", "Wol\u00b7ken", ",", "dr\u00fcck\u00b7ten", "sehr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVFIN", "ADV"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und zuckten, gejagt von den Winden", "tokens": ["Und", "zuck\u00b7ten", ",", "ge\u00b7jagt", "von", "den", "Win\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Ihm lag es wie vergangene Lust,", "tokens": ["Ihm", "lag", "es", "wie", "ver\u00b7gan\u00b7ge\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie Ahnung k\u00fcnftiger Schmerzen,", "tokens": ["Wie", "Ah\u00b7nung", "k\u00fcnf\u00b7ti\u00b7ger", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bergeschwer auf der schw\u00fclen Brust,", "tokens": ["Ber\u00b7ge\u00b7schwer", "auf", "der", "schw\u00fc\u00b7len", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Auf dem bebenden G\u00f6tter-Herzen.", "tokens": ["Auf", "dem", "be\u00b7ben\u00b7den", "G\u00f6t\u00b7ter\u00b7Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun brechen und gie\u00dfen allzumal", "tokens": ["Nun", "bre\u00b7chen", "und", "gie\u00b7\u00dfen", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Schleusen, die Str\u00f6me, die Quellen;", "tokens": ["Die", "Schleu\u00b7sen", ",", "die", "Str\u00f6\u00b7me", ",", "die", "Quel\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durch zerrissene Schleier blinkt ein Strahl,", "tokens": ["Durch", "zer\u00b7ris\u00b7se\u00b7ne", "Schlei\u00b7er", "blinkt", "ein", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Das neue Blau zu erhellen.", "tokens": ["Das", "neu\u00b7e", "Blau", "zu", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Wann hast Du zuletzt Dich ausgeweint,", "tokens": ["Wann", "hast", "Du", "zu\u00b7letzt", "Dich", "aus\u00b7ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Mein Herz, in Klang und Klage,", "tokens": ["Mein", "Herz", ",", "in", "Klang", "und", "Kla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wann, mein armes Herz, erscheint", "tokens": ["Und", "wann", ",", "mein", "ar\u00b7mes", "Herz", ",", "er\u00b7scheint"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "$,", "PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Neulicht fr\u00f6hlicher Tage?!", "tokens": ["Dein", "Neu\u00b7licht", "fr\u00f6h\u00b7li\u00b7cher", "Ta\u00b7ge", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}