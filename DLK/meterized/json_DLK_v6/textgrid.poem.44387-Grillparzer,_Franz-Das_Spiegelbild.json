{"textgrid.poem.44387": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Das Spiegelbild", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lag im gr\u00fcnen Laubgezelt,", "tokens": ["Ich", "lag", "im", "gr\u00fc\u00b7nen", "Laub\u00b7ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stirn in hei\u00dfer Hand,", "tokens": ["Die", "Stirn", "in", "hei\u00b7\u00dfer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verbaut von Zweigen Flur und Feld,", "tokens": ["Ver\u00b7baut", "von", "Zwei\u00b7gen", "Flur", "und", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An eines Brunnens Rand.", "tokens": ["An", "ei\u00b7nes", "Brun\u00b7nens", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und als ich so am Rand gelegt", "tokens": ["Und", "als", "ich", "so", "am", "Rand", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bild im Quell gewahrt,", "tokens": ["Mein", "Bild", "im", "Quell", "ge\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchlt ich mich wunderbar bewegt,", "tokens": ["F\u00fchlt", "ich", "mich", "wun\u00b7der\u00b7bar", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verga\u00df des Wassers Art", "tokens": ["Ver\u00b7ga\u00df", "des", "Was\u00b7sers", "Art"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und rief: So hegest du mein Bild,", "tokens": ["Und", "rief", ":", "So", "he\u00b7gest", "du", "mein", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Wesen, still und rein,", "tokens": ["Du", "We\u00b7sen", ",", "still", "und", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Herzens Sehnen, ungestillt,", "tokens": ["Des", "Her\u00b7zens", "Seh\u00b7nen", ",", "un\u00b7ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll drum dein eigen sein,", "tokens": ["Soll", "drum", "dein", "ei\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "PPOSAT", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "An deinem Ufer will ich ruhn,", "tokens": ["An", "dei\u00b7nem", "U\u00b7fer", "will", "ich", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will mir ein Laubdach baun,", "tokens": ["Will", "mir", "ein", "Laub\u00b7dach", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Matt von des Lebens M\u00fchn und Tun", "tokens": ["Matt", "von", "des", "Le\u00b7bens", "M\u00fchn", "und", "Tun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "In deine Wellen schaun.", "tokens": ["In", "dei\u00b7ne", "Wel\u00b7len", "schaun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da, neben meinem, in dem Quell,", "tokens": ["Da", ",", "ne\u00b7ben", "mei\u00b7nem", ",", "in", "dem", "Quell", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PPOSAT", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewahr ich noch ein Haupt;", "tokens": ["Ge\u00b7wahr", "ich", "noch", "ein", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist mein Freund, erkenn ich schnell,", "tokens": ["Es", "ist", "mein", "Freund", ",", "er\u00b7kenn", "ich", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den ich entfernt geglaubt;", "tokens": ["Den", "ich", "ent\u00b7fernt", "ge\u00b7glaubt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wie er schalkhaft l\u00e4chelnd, froh,", "tokens": ["Und", "wie", "er", "schalk\u00b7haft", "l\u00e4\u00b7chelnd", ",", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich \u00fcber mich gebeugt,", "tokens": ["Sich", "\u00fc\u00b7ber", "mich", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit emsger Treue, ebenso", "tokens": ["Mit", "ems\u00b7ger", "Treu\u00b7e", ",", "e\u00b7ben\u00b7so"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Spiegelquell ihn zeigt.", "tokens": ["Der", "Spie\u00b7gel\u00b7quell", "ihn", "zeigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da war ich schnell vom Traum erwacht,", "tokens": ["Da", "war", "ich", "schnell", "vom", "Traum", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch z\u00fcrnt ich nicht dem Quell,", "tokens": ["Doch", "z\u00fcrnt", "ich", "nicht", "dem", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich z\u00fcrnte, da\u00df ich nicht bedacht,", "tokens": ["Ich", "z\u00fcrn\u00b7te", ",", "da\u00df", "ich", "nicht", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was doch vom Anfang hell:", "tokens": ["Was", "doch", "vom", "An\u00b7fang", "hell", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Des Wassers Art ist eben so,", "tokens": ["Des", "Was\u00b7sers", "Art", "ist", "e\u00b7ben", "so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt nicht nur ", "tokens": ["Zeigt", "nicht", "nur"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die ganze Welt ist dessen froh,", "tokens": ["Die", "gan\u00b7ze", "Welt", "ist", "des\u00b7sen", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich auch grolle nicht;", "tokens": ["Und", "ich", "auch", "grol\u00b7le", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auch in der Folge will ich gern", "tokens": ["Auch", "in", "der", "Fol\u00b7ge", "will", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deinem Ufer gehn,", "tokens": ["An", "dei\u00b7nem", "U\u00b7fer", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Recht innig froh, auch mich von fern", "tokens": ["Recht", "in\u00b7nig", "froh", ",", "auch", "mich", "von", "fern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJD", "$,", "ADV", "PPER", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deinem Selbst zu sehn;", "tokens": ["In", "dei\u00b7nem", "Selbst", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch wohnen hier, mich dir vertraun \u2013", "tokens": ["Doch", "woh\u00b7nen", "hier", ",", "mich", "dir", "ver\u00b7traun", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df fahren das, mein Sinn!", "tokens": ["La\u00df", "fah\u00b7ren", "das", ",", "mein", "Sinn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PDS", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer wird sein Gl\u00fcck auf Wasser baun?", "tokens": ["Wer", "wird", "sein", "Gl\u00fcck", "auf", "Was\u00b7ser", "baun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und also ging ich hin!", "tokens": ["Und", "al\u00b7so", "ging", "ich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}