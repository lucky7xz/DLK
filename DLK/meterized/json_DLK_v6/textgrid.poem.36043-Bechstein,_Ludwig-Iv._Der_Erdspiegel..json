{"textgrid.poem.36043": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Iv. Der Erdspiegel.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Kreuzweg stehen zwei M\u00e4nner in der Nacht;", "tokens": ["Auf", "ei\u00b7nem", "Kreuz\u00b7weg", "ste\u00b7hen", "zwei", "M\u00e4n\u00b7ner", "in", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wird einer finstern Gottheit ein Opfer dargebracht?", "tokens": ["Wird", "ei\u00b7ner", "fins\u00b7tern", "Got\u00b7theit", "ein", "Op\u00b7fer", "dar\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Rund um ein Feuer legen sie Sch\u00e4del und Gebein,", "tokens": ["Rund", "um", "ein", "Feu\u00b7er", "le\u00b7gen", "sie", "Sch\u00e4\u00b7del", "und", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "NE", "KON", "NN", "$,"], "meter": "+--+-++-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Und streuen Zauberkr\u00e4uter in seine Gluth hinein.", "tokens": ["Und", "streu\u00b7en", "Zau\u00b7ber\u00b7kr\u00e4u\u00b7ter", "in", "sei\u00b7ne", "Gluth", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit Henkerschwertern graben sie still den Boden auf;", "tokens": ["Mit", "Hen\u00b7ker\u00b7schwer\u00b7tern", "gra\u00b7ben", "sie", "still", "den", "Bo\u00b7den", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was suchen sie da drunten, was f\u00f6rdern sie herauf?", "tokens": ["Was", "su\u00b7chen", "sie", "da", "drun\u00b7ten", ",", "was", "f\u00f6r\u00b7dern", "sie", "her\u00b7auf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sind's Truhen, voll von Golde, von Prachtkleinoden schwer?", "tokens": ["Sin\u00b7d's", "Tru\u00b7hen", ",", "voll", "von", "Gol\u00b7de", ",", "von", "Pracht\u00b7klein\u00b7o\u00b7den", "schwer", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "APPR", "NE", "$,", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ist's einen H\u00fcnen Leichnam und seine Zauberwehr?", "tokens": ["Ist's", "ei\u00b7nen", "H\u00fc\u00b7nen", "Leich\u00b7nam", "und", "sei\u00b7ne", "Zau\u00b7ber\u00b7wehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Windsbraut tanzt wie rasend den tollen Ringelreihn,", "tokens": ["Die", "Winds\u00b7braut", "tanzt", "wie", "ra\u00b7send", "den", "tol\u00b7len", "Rin\u00b7gel\u00b7reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wild in die Flammen blasend, umsaust den Rabenstein;", "tokens": ["Wild", "in", "die", "Flam\u00b7men", "bla\u00b7send", ",", "um\u00b7saust", "den", "Ra\u00b7ben\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dort wogt es nebelduftig, dort st\u00f6hnt's und wimmert's leis,", "tokens": ["Dort", "wogt", "es", "ne\u00b7bel\u00b7duf\u00b7tig", ",", "dort", "st\u00f6hnt's", "und", "wim\u00b7mert's", "leis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+---+-++-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und Schatten, bleich und luftig, drehn wehend um im Kreis.", "tokens": ["Und", "Schat\u00b7ten", ",", "bleich", "und", "luf\u00b7tig", ",", "drehn", "we\u00b7hend", "um", "im", "Kreis", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "CARD", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer sind die Nekromanten, die dort so still bem\u00fcht,", "tokens": ["Wer", "sind", "die", "Ne\u00b7kro\u00b7man\u00b7ten", ",", "die", "dort", "so", "still", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo bleiche Sch\u00e4del grinsen, wo Gluth in Funken spr\u00fcht?", "tokens": ["Wo", "blei\u00b7che", "Sch\u00e4\u00b7del", "grin\u00b7sen", ",", "wo", "Gluth", "in", "Fun\u00b7ken", "spr\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Was ist's das sie vollbringen im Graun der Mitternacht,", "tokens": ["Was", "ist's", "das", "sie", "voll\u00b7brin\u00b7gen", "im", "Graun", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "PPER", "VVINF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und haben nur des Werkes, nicht der Gestalten Acht?", "tokens": ["Und", "ha\u00b7ben", "nur", "des", "Wer\u00b7kes", ",", "nicht", "der", "Ge\u00b7stal\u00b7ten", "Acht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "CARD", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Der alte finstre Zaubrer ist Heilinger genannt,", "tokens": ["Der", "al\u00b7te", "finst\u00b7re", "Zaub\u00b7rer", "ist", "Hei\u00b7lin\u00b7ger", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Dess unheilvolle Seele sich l\u00e4ngst dem Heil entwandt.", "tokens": ["Dess", "un\u00b7heil\u00b7vol\u00b7le", "See\u00b7le", "sich", "l\u00e4ngst", "dem", "Heil", "ent\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Zweite, der so k\u00fchn blickt, den keine Furcht durchgraust,", "tokens": ["Der", "Zwei\u00b7te", ",", "der", "so", "k\u00fchn", "blickt", ",", "den", "kei\u00b7ne", "Furcht", "durch\u00b7graust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Der J\u00fcngling ists, der Wandrer, der Astrolog, der ", "tokens": ["Der", "J\u00fcng\u00b7ling", "ists", ",", "der", "Wand\u00b7rer", ",", "der", "Ast\u00b7ro\u00b7log", ",", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihn lehrt der alte Meister die Werke dunkler Kunst,", "tokens": ["Ihn", "lehrt", "der", "al\u00b7te", "Meis\u00b7ter", "die", "Wer\u00b7ke", "dunk\u00b7ler", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und feurig buhlt der J\u00fcngling um solchen Lehrers Gunst.", "tokens": ["Und", "feu\u00b7rig", "buhlt", "der", "J\u00fcng\u00b7ling", "um", "sol\u00b7chen", "Leh\u00b7rers", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Wandrer fand die Formel, die Geister ruft und bannt,", "tokens": ["Der", "Wand\u00b7rer", "fand", "die", "For\u00b7mel", ",", "die", "Geis\u00b7ter", "ruft", "und", "bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der Astrolog hat freudig nun seinen Stern erkannt.", "tokens": ["Der", "Ast\u00b7ro\u00b7log", "hat", "freu\u00b7dig", "nun", "sei\u00b7nen", "Stern", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Und \u00e4msiger sie graben, und wilder braust der Sturm;", "tokens": ["Und", "\u00e4m\u00b7si\u00b7ger", "sie", "gra\u00b7ben", ",", "und", "wil\u00b7der", "braust", "der", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVINF", "$,", "KON", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Laut kr\u00e4chzen heis're Raben vom Burgruinenthurm.", "tokens": ["Laut", "kr\u00e4ch\u00b7zen", "heis'\u00b7re", "Ra\u00b7ben", "vom", "Bur\u00b7gru\u00b7i\u00b7nen\u00b7thurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und Irrlichtfl\u00e4mmchen bl\u00e4ulich umwehn das Kreuzwegfeuer,", "tokens": ["Und", "Irr\u00b7licht\u00b7fl\u00e4mm\u00b7chen", "bl\u00e4u\u00b7lich", "um\u00b7wehn", "das", "Kreuz\u00b7weg\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und grausig naht und gr\u00e4ulich, formlos manch Ungeheuer.", "tokens": ["Und", "grau\u00b7sig", "naht", "und", "gr\u00e4u\u00b7lich", ",", "form\u00b7los", "manch", "Un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KON", "ADJD", "$,", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Zwei bleiche Schatten schweben gar nah, gar nah heran,", "tokens": ["Zwei", "blei\u00b7che", "Schat\u00b7ten", "schwe\u00b7ben", "gar", "nah", ",", "gar", "nah", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Augen voll von Z\u00e4hren sehn sie den J\u00fcngling an.", "tokens": ["Mit", "Au\u00b7gen", "voll", "von", "Z\u00e4h\u00b7ren", "sehn", "sie", "den", "J\u00fcng\u00b7ling", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sie winken ihm, so flehend, so schwermuthvoll, so mild,", "tokens": ["Sie", "win\u00b7ken", "ihm", ",", "so", "fle\u00b7hend", ",", "so", "schwer\u00b7muth\u00b7voll", ",", "so", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der J\u00fcngling sieht mit Beben der theuren Aeltern Bild.", "tokens": ["Der", "J\u00fcng\u00b7ling", "sieht", "mit", "Be\u00b7ben", "der", "theu\u00b7ren", "A\u00b7el\u00b7tern", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Der Zaubrer aber winkt ihm nicht ferner hinzusehn,", "tokens": ["Der", "Zaub\u00b7rer", "a\u00b7ber", "winkt", "ihm", "nicht", "fer\u00b7ner", "hin\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wo die Gestalten trauernd, gleich Marmorurnen stehn.", "tokens": ["Wo", "die", "Ge\u00b7stal\u00b7ten", "trau\u00b7ernd", ",", "gleich", "Mar\u00b7mo\u00b7rur\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,", "ADV", "NN", "VVINF", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dem Grabe seiner Unschuld die eine scheint geweiht,", "tokens": ["Dem", "Gra\u00b7be", "sei\u00b7ner", "Un\u00b7schuld", "die", "ei\u00b7ne", "scheint", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "PIS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die zweite gilt dem Grabe von seiner Seligkeit.", "tokens": ["Die", "zwei\u00b7te", "gilt", "dem", "Gra\u00b7be", "von", "sei\u00b7ner", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Und beide schwinden seufzend, und wilder wallt's umher,", "tokens": ["Und", "bei\u00b7de", "schwin\u00b7den", "seuf\u00b7zend", ",", "und", "wil\u00b7der", "wallt's", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "VVPP", "$,", "KON", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Von Wolken und Gestalten wogt rings ein Zaubermeer,", "tokens": ["Von", "Wol\u00b7ken", "und", "Ge\u00b7stal\u00b7ten", "wogt", "rings", "ein", "Zau\u00b7ber\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das um die M\u00e4nner fluthet wie Windeswirbellauf,", "tokens": ["Das", "um", "die", "M\u00e4n\u00b7ner", "flut\u00b7het", "wie", "Win\u00b7des\u00b7wir\u00b7bel\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch Keiner steht entmuthet, und Keiner achtet drauf.", "tokens": ["Doch", "Kei\u00b7ner", "steht", "ent\u00b7mu\u00b7thet", ",", "und", "Kei\u00b7ner", "ach\u00b7tet", "drauf", "."], "token_info": ["word", "w