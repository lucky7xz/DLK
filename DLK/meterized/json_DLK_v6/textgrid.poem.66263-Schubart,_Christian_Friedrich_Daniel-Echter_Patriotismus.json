{"textgrid.poem.66263": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Echter Patriotismus", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der weise Mann allein, der seine Gaben braucht,", "tokens": ["Der", "wei\u00b7se", "Mann", "al\u00b7lein", ",", "der", "sei\u00b7ne", "Ga\u00b7ben", "braucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem nie die kecke Faust vom Blut der B\u00fcrger raucht,", "tokens": ["Dem", "nie", "die", "ke\u00b7cke", "Faust", "vom", "Blut", "der", "B\u00fcr\u00b7ger", "raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Freiheit, Vaterland, tief in der Seele lodert,", "tokens": ["Dem", "Frei\u00b7heit", ",", "Va\u00b7ter\u00b7land", ",", "tief", "in", "der", "See\u00b7le", "lo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der \u2013 ", "tokens": ["Der", "\u2013"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "(denn mitten in dem Glanze der G\u00f6tterherrlichkeit", "tokens": ["(", "denn", "mit\u00b7ten", "in", "dem", "Glan\u00b7ze", "der", "G\u00f6t\u00b7ter\u00b7herr\u00b7lich\u00b7keit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Belastet doch die F\u00fcrsten der Druck der Menschlichkeit)", "tokens": ["Be\u00b7las\u00b7tet", "doch", "die", "F\u00fcrs\u00b7ten", "der", "Druck", "der", "Menschlich\u00b7keit", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+--+-++", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wer viel im Stillen wirkt und ohne laute Ehre", "tokens": ["Wer", "viel", "im", "Stil\u00b7len", "wirkt", "und", "oh\u00b7ne", "lau\u00b7te", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPRART", "NN", "VVFIN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Schon mit dem innren Lohn allein zufrieden w\u00e4re;", "tokens": ["Schon", "mit", "dem", "inn\u00b7ren", "Lohn", "al\u00b7lein", "zu\u00b7frie\u00b7den", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer gern durch Geist und Herz dem Vaterlande n\u00fctzt", "tokens": ["Wer", "gern", "durch", "Geist", "und", "Herz", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "n\u00fctzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und, wenn Gefahr ihm droht, sein Blut f\u00fcr es verspritzt;", "tokens": ["Und", ",", "wenn", "Ge\u00b7fahr", "ihm", "droht", ",", "sein", "Blut", "f\u00fcr", "es", "ver\u00b7spritzt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wen V\u00f6lkerqu\u00e4ler nur durch Tyranneien zwingen,", "tokens": ["Wen", "V\u00f6l\u00b7ker\u00b7qu\u00e4\u00b7ler", "nur", "durch", "Ty\u00b7ran\u00b7nei\u00b7en", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den blanken Freiheitsdolch in k\u00fchner Faust zu schwingen;", "tokens": ["Den", "blan\u00b7ken", "Frei\u00b7heits\u00b7dolch", "in", "k\u00fch\u00b7ner", "Faust", "zu", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer Herrn und B\u00fcrger liebt und mehr als Alles \u2013", "tokens": ["Wer", "Herrn", "und", "B\u00fcr\u00b7ger", "liebt", "und", "mehr", "als", "Al\u00b7les", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "KON", "ADV", "KOUS", "PIS", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Nur der ist freier Mann, nur der ist Patriot.", "tokens": ["Nur", "der", "ist", "frei\u00b7er", "Mann", ",", "nur", "der", "ist", "Pat\u00b7ri\u00b7ot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VAFIN", "ADJD", "NN", "$,", "ADV", "ART", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}