{"dta.poem.21853": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Wiltu/ N\u00e4rrin/ schelten/", "tokens": ["Wil\u00b7tu", "/", "N\u00e4r\u00b7rin", "/", "schel\u00b7ten", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "$(", "NN", "$(", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "la\u00df die Pausen gelten", "tokens": ["la\u00df", "die", "Pau\u00b7sen", "gel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "und nim den Takt in acht/", "tokens": ["und", "nim", "den", "Takt", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "CARD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kein Lied ist wol gemacht", "tokens": ["Kein", "Lied", "ist", "wol", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "da\u00df nur Schw\u00e4nze heget", "tokens": ["da\u00df", "nur", "Schw\u00e4n\u00b7ze", "he\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "und den Atem leget.", "tokens": ["und", "den", "A\u00b7tem", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}