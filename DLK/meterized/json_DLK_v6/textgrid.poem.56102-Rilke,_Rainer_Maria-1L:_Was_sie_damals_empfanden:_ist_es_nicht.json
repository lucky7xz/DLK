{"textgrid.poem.56102": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was sie damals empfanden: ist es nicht", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sie damals empfanden: ist es nicht", "tokens": ["Was", "sie", "da\u00b7mals", "emp\u00b7fan\u00b7den", ":", "ist", "es", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$.", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vor allen Geheimnissen s\u00fc\u00df", "tokens": ["vor", "al\u00b7len", "Ge\u00b7heim\u00b7nis\u00b7sen", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "und immer noch irdisch:", "tokens": ["und", "im\u00b7mer", "noch", "ir\u00b7disch", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "da er, ein wenig bla\u00df noch vom Grab,", "tokens": ["da", "er", ",", "ein", "we\u00b7nig", "bla\u00df", "noch", "vom", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "PIS", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "erleichtert zu ihr trat:", "tokens": ["er\u00b7leich\u00b7tert", "zu", "ihr", "trat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "an allen Stellen erstanden.", "tokens": ["an", "al\u00b7len", "Stel\u00b7len", "er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "O zu ihr zuerst. Wie waren sie da", "tokens": ["O", "zu", "ihr", "zu\u00b7erst", ".", "Wie", "wa\u00b7ren", "sie", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPER", "ADV", "$.", "PWAV", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "unaussprechlich in Heilung.", "tokens": ["un\u00b7aus\u00b7sprech\u00b7lich", "in", "Hei\u00b7lung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Ja sie heilten, das war's. Sie hatten nicht n\u00f6tig,", "tokens": ["Ja", "sie", "heil\u00b7ten", ",", "das", "wa\u00b7r'", "s.", "Sie", "hat\u00b7ten", "nicht", "n\u00f6\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "VVIMP", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "sich stark zu ber\u00fchren.", "tokens": ["sich", "stark", "zu", "be\u00b7r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Er legte ihr eine Sekunde", "tokens": ["Er", "leg\u00b7te", "ihr", "ei\u00b7ne", "Se\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "kaum seine n\u00e4chstens", "tokens": ["kaum", "sei\u00b7ne", "n\u00e4chs\u00b7tens"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "ewige Hand an die frauliche Schulter.", "tokens": ["e\u00b7wi\u00b7ge", "Hand", "an", "die", "frau\u00b7li\u00b7che", "Schul\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Und sie begannen", "tokens": ["Und", "sie", "be\u00b7gan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "still wie die B\u00e4ume im Fr\u00fchling,", "tokens": ["still", "wie", "die", "B\u00e4u\u00b7me", "im", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.16": {"text": "unendlich zugleich,", "tokens": ["un\u00b7end\u00b7lich", "zu\u00b7gleich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "diese Jahreszeit", "tokens": ["die\u00b7se", "Jah\u00b7res\u00b7zeit"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "ihres \u00e4u\u00dfersten Umgangs.", "tokens": ["ih\u00b7res", "\u00e4u\u00b7\u00dfers\u00b7ten", "Um\u00b7gangs", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}}}}}