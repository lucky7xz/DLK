{"dta.poem.10744": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Nach jhr steht mein verlangen/", "tokens": ["Nach", "jhr", "steht", "mein", "ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein sehnlich hertzegird:", "tokens": ["Mein", "sehn\u00b7lich", "hert\u00b7ze\u00b7gird", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Creutz last sie mich hangen/", "tokens": ["Am", "Creutz", "last", "sie", "mich", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meins lebens nimmer wird.", "tokens": ["Meins", "le\u00b7bens", "nim\u00b7mer", "wird", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zwar bald ich tod mu\u00df sein.", "tokens": ["Zwar", "bald", "ich", "tod", "mu\u00df", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Je weiter sie mich neidet/", "tokens": ["Je", "wei\u00b7ter", "sie", "mich", "nei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Je lenger mein Hertz leidet.", "tokens": ["Je", "len\u00b7ger", "mein", "Hertz", "lei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Ist das nit schwere pein?", "tokens": ["Ist", "das", "nit", "schwe\u00b7re", "pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}