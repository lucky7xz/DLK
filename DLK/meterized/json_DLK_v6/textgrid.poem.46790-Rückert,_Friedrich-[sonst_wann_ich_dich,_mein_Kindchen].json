{"textgrid.poem.46790": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sonst wann ich dich, mein Kindchen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonst wann ich dich, mein Kindchen,", "tokens": ["Sonst", "wann", "ich", "dich", ",", "mein", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Morgen sah erwacht,", "tokens": ["Am", "Mor\u00b7gen", "sah", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fand ich dich sch\u00f6ner, frischer", "tokens": ["Fand", "ich", "dich", "sch\u00f6\u00b7ner", ",", "fri\u00b7scher"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geworden \u00fcbernacht.", "tokens": ["Ge\u00b7wor\u00b7den", "\u00fc\u00b7bern\u00b7acht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun Morgens, wenn dein Bildchen", "tokens": ["Nun", "Mor\u00b7gens", ",", "wenn", "dein", "Bild\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir hier entgegenlacht,", "tokens": ["Mir", "hier", "ent\u00b7ge\u00b7gen\u00b7lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Find' ich's nicht farbenfrischer", "tokens": ["Find'", "ich's", "nicht", "far\u00b7ben\u00b7fri\u00b7scher"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Geworden in der Nacht;", "tokens": ["Ge\u00b7wor\u00b7den", "in", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mich freut's, da\u00df nur erblichen", "tokens": ["Mich", "freut's", ",", "da\u00df", "nur", "er\u00b7bli\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht ist die Farbenpracht,", "tokens": ["Nicht", "ist", "die", "Far\u00b7ben\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie ach, von dir gewichen", "tokens": ["Wie", "ach", ",", "von", "dir", "ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ITJ", "$,", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Glanz ist \u00fcbernacht.", "tokens": ["Der", "Glanz", "ist", "\u00fc\u00b7bern\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}