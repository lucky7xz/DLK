{"textgrid.poem.35487": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s ist Samstag. Heilige Stille geht", "tokens": ["'s", "ist", "Sams\u00b7tag", ".", "Hei\u00b7li\u00b7ge", "Stil\u00b7le", "geht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$.", "ADJA", "NN", "VVFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ueber die Erde mit leisen Tritten ...", "tokens": ["Ue\u00b7ber", "die", "Er\u00b7de", "mit", "lei\u00b7sen", "Trit\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "In mir ist's klar wie zum Gebet, \u2013", "tokens": ["In", "mir", "ist's", "klar", "wie", "zum", "Ge\u00b7bet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADJD", "KOKOM", "APPRART", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und jeder Schmerz, den ich erlitten,", "tokens": ["Und", "je\u00b7der", "Schmerz", ",", "den", "ich", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verfl\u00fcchtigt sich wie dieser Hauch,", "tokens": ["Ver\u00b7fl\u00fcch\u00b7tigt", "sich", "wie", "die\u00b7ser", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KOKOM", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der mir des Herbstes letztes Laubwerk", "tokens": ["Der", "mir", "des", "Herbs\u00b7tes", "letz\u00b7tes", "Laub\u00b7werk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zu F\u00fc\u00dfen wirft \u2013 wie dieser Rauch,", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "wirft", "\u2013", "wie", "die\u00b7ser", "Rauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$(", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der sich in Schleiern hebt aus Staubwerk ...", "tokens": ["Der", "sich", "in", "Schlei\u00b7ern", "hebt", "aus", "Staub\u00b7werk", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}