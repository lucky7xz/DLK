{"textgrid.poem.53538": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Olle Kamellen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor der Front ein junger Bengel.", "tokens": ["Vor", "der", "Front", "ein", "jun\u00b7ger", "Ben\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er moniert die Fehler, die Schlappheit, die M\u00e4ngel.", "tokens": ["Er", "mo\u00b7niert", "die", "Feh\u00b7ler", ",", "die", "Schlap\u00b7pheit", ",", "die", "M\u00e4n\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Im Gliede lauter alte Leute.", "tokens": ["Im", "Glie\u00b7de", "lau\u00b7ter", "al\u00b7te", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": " . . . Schlechter Laune der Leutnant heute . . .", "tokens": [".", ".", ".", "Schlech\u00b7ter", "Lau\u00b7ne", "der", "Leut\u00b7nant", "heu\u00b7te", ".", ".", "."], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "ADJA", "NN", "ART", "NN", "ADV", "$.", "$.", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "\u00bbdas kann ich der Kompanie erkl\u00e4ren:", "tokens": ["\u00bb", "das", "kann", "ich", "der", "Kom\u00b7pa\u00b7nie", "er\u00b7kl\u00e4\u00b7ren", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Ich werde euch Kerls das Strammstehen schon lehren!", "tokens": ["Ich", "wer\u00b7de", "euch", "Kerls", "das", "Stramm\u00b7ste\u00b7hen", "schon", "leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nehmen Sie die Knochen zusammen, Sie Schwein!\u00ab", "tokens": ["Neh\u00b7men", "Sie", "die", "Kno\u00b7chen", "zu\u00b7sam\u00b7men", ",", "Sie", "Schwein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PPER", "NN", "$.", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Und das soll alles vergessen sein?", "tokens": ["Und", "das", "soll", "al\u00b7les", "ver\u00b7ges\u00b7sen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Drin im Kasino ist gro\u00dfer Trubel.", "tokens": ["Drin", "im", "Ka\u00b7si\u00b7no", "ist", "gro\u00b7\u00dfer", "Tru\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NE", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gl\u00e4serklingen. Hurragejubel.", "tokens": ["Gl\u00e4\u00b7ser\u00b7klin\u00b7gen", ".", "Hur\u00b7ra\u00b7ge\u00b7ju\u00b7bel", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Sieben G\u00e4nge, dreierlei Weine.", "tokens": ["Sie\u00b7ben", "G\u00e4n\u00b7ge", ",", "drei\u00b7er\u00b7lei", "Wei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "CARD", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der Posten drau\u00dfen hat kalte Beine.", "tokens": ["Der", "Pos\u00b7ten", "drau\u00b7\u00dfen", "hat", "kal\u00b7te", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er denkt an Muttern, an zu Haus;", "tokens": ["Er", "denkt", "an", "Mut\u00b7tern", ",", "an", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "die Kinder, schreibt sie, sehn elend aus.", "tokens": ["die", "Kin\u00b7der", ",", "schreibt", "sie", ",", "sehn", "e\u00b7lend", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Drin sind sie lustig und kr\u00e4hen und schrein \u2013", "tokens": ["Drin", "sind", "sie", "lus\u00b7tig", "und", "kr\u00e4\u00b7hen", "und", "schrein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVINF", "KON", "ADJD", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Und das soll alles vergessen sein?", "tokens": ["Und", "das", "soll", "al\u00b7les", "ver\u00b7ges\u00b7sen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und das sei alles vergeben, vergessen?", "tokens": ["Und", "das", "sei", "al\u00b7les", "ver\u00b7ge\u00b7ben", ",", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIS", "VVPP", "$,", "VVPP", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Die Tritte nach unten? der Diebstahl am Essen?", "tokens": ["Die", "Trit\u00b7te", "nach", "un\u00b7ten", "?", "der", "Dieb\u00b7stahl", "am", "Es\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "$.", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bei Gott! das sind keine alten Kamellen!", "tokens": ["Bei", "Gott", "!", "das", "sind", "kei\u00b7ne", "al\u00b7ten", "Ka\u00b7mel\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "PDS", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es wimmelt noch heute von solchen Gesellen!", "tokens": ["Es", "wim\u00b7melt", "noch", "heu\u00b7te", "von", "sol\u00b7chen", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Eingedrillter Kadaverrespekt \u2013", "tokens": ["Ein\u00b7ge\u00b7drill\u00b7ter", "Ka\u00b7da\u00b7ver\u00b7res\u00b7pekt", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "wie tief der noch heut in den K\u00f6pfen steckt!", "tokens": ["wie", "tief", "der", "noch", "heut", "in", "den", "K\u00f6p\u00b7fen", "steckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er ri\u00df uns in jenen Krieg hinein \u2013", "tokens": ["Er", "ri\u00df", "uns", "in", "je\u00b7nen", "Krieg", "hin\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PDAT", "NN", "PTKVZ", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und das soll alles vergessen sein?", "tokens": ["Und", "das", "soll", "al\u00b7les", "ver\u00b7ges\u00b7sen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nicht vergessen. Wir wollen das \u00e4ndern.", "tokens": ["Nicht", "ver\u00b7ges\u00b7sen", ".", "Wir", "wol\u00b7len", "das", "\u00e4n\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$.", "PPER", "VMFIN", "PDS", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ein freies Land unter freien L\u00e4ndern", "tokens": ["Ein", "frei\u00b7es", "Land", "un\u00b7ter", "frei\u00b7en", "L\u00e4n\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "sei Deutschland \u2013 mit freien Bewohnern drin,", "tokens": ["sei", "Deutschland", "\u2013", "mit", "frei\u00b7en", "Be\u00b7woh\u00b7nern", "drin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ohne den knechtischen Dienersinn.", "tokens": ["oh\u00b7ne", "den", "knech\u00b7ti\u00b7schen", "Die\u00b7ner\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Wir wollen nicht Rache an Offizieren.", "tokens": ["Wir", "wol\u00b7len", "nicht", "Ra\u00b7che", "an", "Of\u00b7fi\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Wir wollen den deutschen Sinn reformieren.", "tokens": ["Wir", "wol\u00b7len", "den", "deut\u00b7schen", "Sinn", "re\u00b7for\u00b7mie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sei ein freier Deutscher \u2013 Bruder, schlag ein!", "tokens": ["Sei", "ein", "frei\u00b7er", "Deut\u00b7scher", "\u2013", "Bru\u00b7der", ",", "schlag", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$(", "NN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Und dann soll alles vergessen sein!", "tokens": ["Und", "dann", "soll", "al\u00b7les", "ver\u00b7ges\u00b7sen", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}