{"textgrid.poem.35944": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, wie selig athmest du,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, wie selig athmest du,", "tokens": ["O", ",", "wie", "se\u00b7lig", "ath\u00b7mest", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lachst so hold in leichtem Schlummer,", "tokens": ["Lachst", "so", "hold", "in", "leich\u00b7tem", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Z\u00fcge k\u00fcnden Ruh;", "tokens": ["Dei\u00b7ne", "Z\u00fc\u00b7ge", "k\u00fcn\u00b7den", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herzchen wei\u00df von keinem Kummer.", "tokens": ["Herz\u00b7chen", "wei\u00df", "von", "kei\u00b7nem", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einst, ach! war ich auch so klein,", "tokens": ["Einst", ",", "ach", "!", "war", "ich", "auch", "so", "klein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ITJ", "$.", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch so frei von aller Pein.", "tokens": ["Auch", "so", "frei", "von", "al\u00b7ler", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kind, wie hab' ich dich so lieb,", "tokens": ["Kind", ",", "wie", "hab'", "ich", "dich", "so", "lieb", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich in deinem Engelschimmer!", "tokens": ["Dich", "in", "dei\u00b7nem", "En\u00b7gel\u00b7schim\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist nie m\u00fcrrisch oder tr\u00fcb,", "tokens": ["Bist", "nie", "m\u00fcr\u00b7risch", "o\u00b7der", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blickst so fromm und freundlich immer.", "tokens": ["Blickst", "so", "fromm", "und", "freund\u00b7lich", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, auch ich war einst so froh, \u2013", "tokens": ["Ja", ",", "auch", "ich", "war", "einst", "so", "froh", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "ADV", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, jetzt ist es nicht mehr so!", "tokens": ["Ach", ",", "jetzt", "ist", "es", "nicht", "mehr", "so", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebchen, schlafe nur, mein Arm", "tokens": ["Lieb\u00b7chen", ",", "schla\u00b7fe", "nur", ",", "mein", "Arm"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist dir sanfte, weiche Wiege;", "tokens": ["Ist", "dir", "sanf\u00b7te", ",", "wei\u00b7che", "Wie\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Macht' ich dich nicht satt und warm,", "tokens": ["Macht'", "ich", "dich", "nicht", "satt", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheuch' ich dir nicht jede Fliege?", "tokens": ["Scheuch'", "ich", "dir", "nicht", "je\u00b7de", "Flie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kind, du kennst noch keinen Schmerz,", "tokens": ["Kind", ",", "du", "kennst", "noch", "kei\u00b7nen", "Schmerz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlst ja nicht das eigne Herz.", "tokens": ["F\u00fchlst", "ja", "nicht", "das", "eig\u00b7ne", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist mir's doch so s\u00fc\u00dfe Lust,", "tokens": ["Ist", "mir's", "doch", "so", "s\u00fc\u00b7\u00dfe", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich, du frommes Kind, zu herzen!", "tokens": ["Dich", ",", "du", "from\u00b7mes", "Kind", ",", "zu", "her\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schirmte gern die zarte Brust", "tokens": ["Schirm\u00b7te", "gern", "die", "zar\u00b7te", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor des rauhen Lebens Schmerzen.", "tokens": ["Vor", "des", "rau\u00b7hen", "Le\u00b7bens", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch, auch du f\u00fchlst einst wie ich,", "tokens": ["Doch", ",", "auch", "du", "f\u00fchlst", "einst", "wie", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "PPER", "VVFIN", "ADV", "KOKOM", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Armes Kind! das dauert mich.", "tokens": ["Ar\u00b7mes", "Kind", "!", "das", "dau\u00b7ert", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weh! ich lie\u00df auf dein Gesicht", "tokens": ["Weh", "!", "ich", "lie\u00df", "auf", "dein", "Ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine hei\u00dfe Thr\u00e4ne fallen;", "tokens": ["Ei\u00b7ne", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7ne", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du erwachst? o, weine nicht,", "tokens": ["Du", "er\u00b7wachst", "?", "o", ",", "wei\u00b7ne", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "FM", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen auch in's Gr\u00fcne wallen.", "tokens": ["Wol\u00b7len", "auch", "in's", "Gr\u00fc\u00b7ne", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Brannte denn die Thr\u00e4ne so?", "tokens": ["Brann\u00b7te", "denn", "die", "Thr\u00e4\u00b7ne", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebchen, sei doch wieder froh!", "tokens": ["Lieb\u00b7chen", ",", "sei", "doch", "wie\u00b7der", "froh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh, ich trage dich hinaus,", "tokens": ["Sieh", ",", "ich", "tra\u00b7ge", "dich", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "APZR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die frommen L\u00e4mmer springen,", "tokens": ["Wo", "die", "from\u00b7men", "L\u00e4m\u00b7mer", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In das arme Hirtenhaus;", "tokens": ["In", "das", "ar\u00b7me", "Hir\u00b7ten\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6r' nur, wie die Engel singen:", "tokens": ["H\u00f6r'", "nur", ",", "wie", "die", "En\u00b7gel", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh, ein Kindlein gr\u00fc\u00dfet dich,", "tokens": ["Sieh", ",", "ein", "Kin\u00b7dlein", "gr\u00fc\u00b7\u00dfet", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, es kam f\u00fcr dich und mich!", "tokens": ["Ach", ",", "es", "kam", "f\u00fcr", "dich", "und", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sieh, wie arm auf Heu es liegt,", "tokens": ["Sieh", ",", "wie", "arm", "auf", "Heu", "es", "liegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nein, nun darfst du nicht mehr weinen,", "tokens": ["Nein", ",", "nun", "darfst", "du", "nicht", "mehr", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn man dich in Federn wiegt;", "tokens": ["Wenn", "man", "dich", "in", "Fe\u00b7dern", "wiegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Still, mein Kind! man sollte meinen,", "tokens": ["Still", ",", "mein", "Kind", "!", "man", "soll\u00b7te", "mei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPOSAT", "NN", "$.", "PIS", "VMFIN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jesus litt, was uns geb\u00fchrt,", "tokens": ["Je\u00b7sus", "litt", ",", "was", "uns", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wir w\u00fcrden kaum ger\u00fchrt.", "tokens": ["Und", "wir", "w\u00fcr\u00b7den", "kaum", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}