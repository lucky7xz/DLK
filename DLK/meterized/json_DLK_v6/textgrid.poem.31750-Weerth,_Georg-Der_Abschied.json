{"textgrid.poem.31750": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine alte, gute Mutter,", "tokens": ["Mei\u00b7ne", "al\u00b7te", ",", "gu\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die n\u00e4hte die halbe Nacht;", "tokens": ["Die", "n\u00e4h\u00b7te", "die", "hal\u00b7be", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie hat mir aus feinem Linnen", "tokens": ["Sie", "hat", "mir", "aus", "fei\u00b7nem", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein feines Hemd gemacht.", "tokens": ["Ein", "fei\u00b7nes", "Hemd", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Meine wundersch\u00f6ne Schwester,", "tokens": ["Mei\u00b7ne", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die hat einen freien Sinn;", "tokens": ["Die", "hat", "ei\u00b7nen", "frei\u00b7en", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die stickte mit stolzer Seide", "tokens": ["Die", "stick\u00b7te", "mit", "stol\u00b7zer", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Meinen stolzen Namen darin.", "tokens": ["Mei\u00b7nen", "stol\u00b7zen", "Na\u00b7men", "da\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PAV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und morgens, um halber viere,", "tokens": ["Und", "mor\u00b7gens", ",", "um", "hal\u00b7ber", "vie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "APPO", "PIS", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da hat der Hahn gekr\u00e4ht;", "tokens": ["Da", "hat", "der", "Hahn", "ge\u00b7kr\u00e4ht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun schn\u00fcre seinen Ranzen,", "tokens": ["Nun", "schn\u00fc\u00b7re", "sei\u00b7nen", "Ran\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer auf die Reise geht!", "tokens": ["Wer", "auf", "die", "Rei\u00b7se", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und morgens, um halber f\u00fcnfe,", "tokens": ["Und", "mor\u00b7gens", ",", "um", "hal\u00b7ber", "f\u00fcn\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUI", "APPO", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da hab ich meinen Vater geweckt;", "tokens": ["Da", "hab", "ich", "mei\u00b7nen", "Va\u00b7ter", "ge\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der hat drei rostige Kronen", "tokens": ["Der", "hat", "drei", "ros\u00b7ti\u00b7ge", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In meinen Sack gesteckt.", "tokens": ["In", "mei\u00b7nen", "Sack", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir standen unter der Linde,", "tokens": ["Wir", "stan\u00b7den", "un\u00b7ter", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da ward mein Herz so schwer;", "tokens": ["Da", "ward", "mein", "Herz", "so", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Meine treue Mutter meinte,", "tokens": ["Mei\u00b7ne", "treu\u00b7e", "Mut\u00b7ter", "mein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie s\u00e4he mich nimmermehr.", "tokens": ["Sie", "s\u00e4\u00b7he", "mich", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Mein Vater ward so stille,", "tokens": ["Mein", "Va\u00b7ter", "ward", "so", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Meine Schwester schluchzte darauf \u2013", "tokens": ["Mei\u00b7ne", "Schwes\u00b7ter", "schluchz\u00b7te", "da\u00b7rauf", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da ging in den Weizenfeldern", "tokens": ["Da", "ging", "in", "den", "Wei\u00b7zen\u00b7fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die goldene Sonne auf.", "tokens": ["Die", "gol\u00b7de\u00b7ne", "Son\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und vor den Toren klang es:", "tokens": ["Und", "vor", "den", "To\u00b7ren", "klang", "es", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbade, du dumpfige Stadt!", "tokens": ["\u00bb", "a\u00b7de", ",", "du", "dump\u00b7fi\u00b7ge", "Stadt", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nun freue sich, wer ein freies,", "tokens": ["Nun", "freu\u00b7e", "sich", ",", "wer", "ein", "frei\u00b7es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "PWS", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ein lustiges Leben hat!\u00ab", "tokens": ["Ein", "lus\u00b7ti\u00b7ges", "Le\u00b7ben", "hat", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}