{"textgrid.poem.36216": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kleine Frauen, kleine Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine Frauen, kleine Lieder,", "tokens": ["Klei\u00b7ne", "Frau\u00b7en", ",", "klei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach man liebt, und liebt sie wieder.", "tokens": ["Ach", "man", "liebt", ",", "und", "liebt", "sie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIS", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie die Blume gl\u00e4nzt dem Kinde,", "tokens": ["Wie", "die", "Blu\u00b7me", "gl\u00e4nzt", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4cheln Leichtsinn uns die M\u00e4dchen,", "tokens": ["L\u00e4\u00b7cheln", "Leicht\u00b7sinn", "uns", "die", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leichte rollt des Lebens R\u00e4dchen", "tokens": ["Leich\u00b7te", "rollt", "des", "Le\u00b7bens", "R\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Liebe Lustgewinde.", "tokens": ["In", "der", "Lie\u00b7be", "Lust\u00b7ge\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Darum singt man froh und linde,", "tokens": ["Da\u00b7rum", "singt", "man", "froh", "und", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kleine Frauen, kleine Lieder,", "tokens": ["Klei\u00b7ne", "Frau\u00b7en", ",", "klei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebt sie, und sie lieben wieder.", "tokens": ["Liebt", "sie", ",", "und", "sie", "lie\u00b7ben", "wie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und es gleiten von der Kehle", "tokens": ["Und", "es", "glei\u00b7ten", "von", "der", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Spiele, diese W\u00f6rtchen,", "tokens": ["Die\u00b7se", "Spie\u00b7le", ",", "die\u00b7se", "W\u00f6rt\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein s\u00fc\u00dfes Lieblings\u00f6rtchen", "tokens": ["Wie", "ein", "s\u00fc\u00b7\u00dfes", "Lieb\u00b7lin\u00b7gs\u00f6rt\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieblich schwebet vor der Seele.", "tokens": ["Lieb\u00b7lich", "schwe\u00b7bet", "vor", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, man fragt nicht, ob was fehle:", "tokens": ["Ach", ",", "man", "fragt", "nicht", ",", "ob", "was", "feh\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn man singt die kleinen Lieder,", "tokens": ["Denn", "man", "singt", "die", "klei\u00b7nen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie man liebt, und singt sie wieder.", "tokens": ["Wie", "man", "liebt", ",", "und", "singt", "sie", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}