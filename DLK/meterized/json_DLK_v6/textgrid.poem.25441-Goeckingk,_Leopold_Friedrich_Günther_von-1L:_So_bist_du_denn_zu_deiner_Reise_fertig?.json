{"textgrid.poem.25441": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: So bist du denn zu deiner Reise fertig?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du denn zu deiner Reise fertig?", "tokens": ["So", "bist", "du", "denn", "zu", "dei\u00b7ner", "Rei\u00b7se", "fer\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt bist du noch in meiner Hand,", "tokens": ["Jetzt", "bist", "du", "noch", "in", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald aber wie allgegenw\u00e4rtig", "tokens": ["Bald", "a\u00b7ber", "wie", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "KOKOM", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Rheine bis zum Donaustrand'.", "tokens": ["Vom", "Rhei\u00b7ne", "bis", "zum", "Do\u00b7naustrand'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Bald wirst du, liebes S\u00f6hnchen, nun", "tokens": ["Bald", "wirst", "du", ",", "lie\u00b7bes", "S\u00f6hn\u00b7chen", ",", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei Prinzen und Prinzessen", "tokens": ["Bei", "Prin\u00b7zen", "und", "Prin\u00b7zes\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Auf weichen Ottomannen ruhn,", "tokens": ["Auf", "wei\u00b7chen", "Ot\u00b7to\u00b7man\u00b7nen", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um, wenn vielleicht der Schlaf sie hat vergessen,", "tokens": ["Um", ",", "wenn", "viel\u00b7leicht", "der", "Schlaf", "sie", "hat", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Dienst des Opiums zu thun.", "tokens": ["Den", "Dienst", "des", "O\u00b7piums", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Vielleicht nimmt gar ein Hoffr\u00e4ulein voll Gnade", "tokens": ["Viel\u00b7leicht", "nimmt", "gar", "ein", "Hof\u00b7fr\u00e4u\u00b7lein", "voll", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dich beim Frisiren auf den Schoo\u00df,", "tokens": ["Dich", "beim", "Fri\u00b7si\u00b7ren", "auf", "den", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Und seufzt mitleidig: Ewig Schade!", "tokens": ["Und", "seufzt", "mit\u00b7lei\u00b7dig", ":", "E\u00b7wig", "Scha\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "W\u00e4rst du nur ein Franzos'!", "tokens": ["W\u00e4rst", "du", "nur", "ein", "Fran\u00b7zos'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Doch, guter Junge, la\u00df dich das nicht irren;", "tokens": ["Doch", ",", "gu\u00b7ter", "Jun\u00b7ge", ",", "la\u00df", "dich", "das", "nicht", "ir\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "VVIMP", "PPER", "PDS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Geh, wie dein Vater, deinen Gang", "tokens": ["Geh", ",", "wie", "dein", "Va\u00b7ter", ",", "dei\u00b7nen", "Gang"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Geruhig fort, la\u00df um dein Ohr den Klang", "tokens": ["Ge\u00b7ru\u00b7hig", "fort", ",", "la\u00df", "um", "dein", "Ohr", "den", "Klang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.17": {"text": "Der Stangen und der H\u00f6rner plumper Sbirren,", "tokens": ["Der", "Stan\u00b7gen", "und", "der", "H\u00f6r\u00b7ner", "plum\u00b7per", "Sbir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Des Klatschens selbst der Kenner, schwirren,", "tokens": ["Des", "Klat\u00b7schens", "selbst", "der", "Ken\u00b7ner", ",", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn, sie zu r\u00fchren, dir gelang.", "tokens": ["Wenn", ",", "sie", "zu", "r\u00fch\u00b7ren", ",", "dir", "ge\u00b7lang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Du weist, da\u00df ich dich nicht erzog,", "tokens": ["Du", "weist", ",", "da\u00df", "ich", "dich", "nicht", "er\u00b7zog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Um in der gro\u00dfen Welt zu schimmern;", "tokens": ["Um", "in", "der", "gro\u00b7\u00dfen", "Welt", "zu", "schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Wie sollt' ich mich f\u00fcr dich um sie bek\u00fcmmern,", "tokens": ["Wie", "sollt'", "ich", "mich", "f\u00fcr", "dich", "um", "sie", "be\u00b7k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Da ich ihr selbst so fr\u00fch entflog?", "tokens": ["Da", "ich", "ihr", "selbst", "so", "fr\u00fch", "ent\u00b7flog", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Genug, wenn mein Gef\u00fchl mich nicht betrog,", "tokens": ["Ge\u00b7nug", ",", "wenn", "mein", "Ge\u00b7f\u00fchl", "mich", "nicht", "be\u00b7trog", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "So wird sich nie durch dich ein Herz verschlimmern;", "tokens": ["So", "wird", "sich", "nie", "durch", "dich", "ein", "Herz", "ver\u00b7schlim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So wird der Mann, der Freude liebt,", "tokens": ["So", "wird", "der", "Mann", ",", "der", "Freu\u00b7de", "liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Vielleicht dich gern erz\u00e4hlen h\u00f6ren,", "tokens": ["Viel\u00b7leicht", "dich", "gern", "er\u00b7z\u00e4h\u00b7len", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wie in der Kunst, die Freuden zu vermehren,", "tokens": ["Wie", "in", "der", "Kunst", ",", "die", "Freu\u00b7den", "zu", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Dein Vater sich sein Leben lang ge\u00fcbt,", "tokens": ["Dein", "Va\u00b7ter", "sich", "sein", "Le\u00b7ben", "lang", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Durch nichts so leicht lie\u00df seine Ruhe st\u00f6ren,", "tokens": ["Durch", "nichts", "so", "leicht", "lie\u00df", "sei\u00b7ne", "Ru\u00b7he", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und, wenn wir durch den Tod nur keinen Freund verl\u00f6ren,", "tokens": ["Und", ",", "wenn", "wir", "durch", "den", "Tod", "nur", "kei\u00b7nen", "Freund", "ver\u00b7l\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sich selbst als Greis noch \u00fcber nichts betr\u00fcbt.", "tokens": ["Sich", "selbst", "als", "Greis", "noch", "\u00fc\u00b7ber", "nichts", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KOUS", "NN", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Du brauchst, verlangt man etwa mehr", "tokens": ["Du", "brauchst", ",", "ver\u00b7langt", "man", "et\u00b7wa", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PIS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Zu wissen, nicht halsstarrig auszuweichen;", "tokens": ["Zu", "wis\u00b7sen", ",", "nicht", "hals\u00b7star\u00b7rig", "aus\u00b7zu\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKNEG", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Doch sag' nur die\u00df: da\u00df wir einander sehr", "tokens": ["Doch", "sag'", "nur", "die\u00df", ":", "da\u00df", "wir", "ein\u00b7an\u00b7der", "sehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PDS", "$.", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Im Guten, und im B\u00f6sen gleichen.", "tokens": ["Im", "Gu\u00b7ten", ",", "und", "im", "B\u00f6\u00b7sen", "glei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Besorge nicht, da\u00df dich, wie ", "tokens": ["Be\u00b7sor\u00b7ge", "nicht", ",", "da\u00df", "dich", ",", "wie"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "PPER", "$,", "PWAV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.38": {"text": "Die Welt einst werde L\u00fcgen strafen.", "tokens": ["Die", "Welt", "einst", "wer\u00b7de", "L\u00fc\u00b7gen", "stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Zwar bist auch du vielleicht der Motten Speise schon,", "tokens": ["Zwar", "bist", "auch", "du", "viel\u00b7leicht", "der", "Mot\u00b7ten", "Spei\u00b7se", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "ART", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn ich bei W\u00fcrmern werde schlafen;", "tokens": ["Wenn", "ich", "bei", "W\u00fcr\u00b7mern", "wer\u00b7de", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Doch, wenn du, (m\u00f6cht' ich wahr doch prophezeihn!)", "tokens": ["Doch", ",", "wenn", "du", ",", "(", "m\u00f6cht'", "ich", "wahr", "doch", "pro\u00b7phe\u00b7zeihn", "!", ")"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "$,", "$(", "VMFIN", "PPER", "ADJD", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Ein weitres Ziel, als ich, dir kannst erstreben,", "tokens": ["Ein", "weit\u00b7res", "Ziel", ",", "als", "ich", ",", "dir", "kannst", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "So soll gewi\u00df mein ganzes Leben", "tokens": ["So", "soll", "ge\u00b7wi\u00df", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Kein Vorwurf dir bei deinen Freunden seyn;", "tokens": ["Kein", "Vor\u00b7wurf", "dir", "bei", "dei\u00b7nen", "Freun\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Denn, Lehren hat mein Mund gewagt der Welt zu geben,", "tokens": ["Denn", ",", "Leh\u00b7ren", "hat", "mein", "Mund", "ge\u00b7wagt", "der", "Welt", "zu", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Doch pr\u00e4gt' ich sie zuvor erst meinem Herzen ein.", "tokens": ["Doch", "pr\u00e4gt'", "ich", "sie", "zu\u00b7vor", "erst", "mei\u00b7nem", "Her\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Nicht halb so sch\u00f6n wardst du und deine Br\u00fcder", "tokens": ["Nicht", "halb", "so", "sch\u00f6n", "wardst", "du", "und", "dei\u00b7ne", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ADV", "ADJD", "VAFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Von mir gezeugt, als ", "tokens": ["Von", "mir", "ge\u00b7zeugt", ",", "als"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "KOUS"], "meter": "+--+-", "measure": "amphi