{"textgrid.poem.49538": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Sinngedicht", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Reisender kam einst nach Zabern hin.", "tokens": ["Ein", "Rei\u00b7sen\u00b7der", "kam", "einst", "nach", "Za\u00b7bern", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sieh! da ein Schlo\u00df, sprach er in seinem Sinn! \u2013 \u2013", "tokens": ["Sieh", "!", "da", "ein", "Schlo\u00df", ",", "sprach", "er", "in", "sei\u00b7nem", "Sinn", "!", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$.", "KOUS", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indem erschien die Herzogin:", "tokens": ["In\u00b7dem", "er\u00b7schien", "die", "Her\u00b7zo\u00b7gin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ha! dacht er, nein! \u2013 es ist ein Tempel.", "tokens": ["Ha", "!", "dacht", "er", ",", "nein", "!", "\u2013", "es", "ist", "ein", "Tem\u00b7pel", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "PTKANT", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Tugend, seh ich, wohnt darinn!", "tokens": ["Die", "Tu\u00b7gend", ",", "seh", "ich", ",", "wohnt", "da\u00b7rinn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}