{"textgrid.poem.63606": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit jedem neuen Kind wirst du zum Kinde.", "tokens": ["Mit", "je\u00b7dem", "neu\u00b7en", "Kind", "wirst", "du", "zum", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Werdeschrei des jungen Lebens sprengt", "tokens": ["Der", "Wer\u00b7de\u00b7schrei", "des", "jun\u00b7gen", "Le\u00b7bens", "sprengt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des grauen Herzens narbenvolle Rinde,", "tokens": ["Des", "grau\u00b7en", "Her\u00b7zens", "nar\u00b7ben\u00b7vol\u00b7le", "Rin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df es wie Fr\u00fchlingsschauer dich umf\u00e4ngt", "tokens": ["Da\u00df", "es", "wie", "Fr\u00fch\u00b7lings\u00b7schau\u00b7er", "dich", "um\u00b7f\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und dir erbl\u00fcht ein selig Mitempfinden", "tokens": ["Und", "dir", "er\u00b7bl\u00fcht", "ein", "se\u00b7lig", "Mi\u00b7temp\u00b7fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Lebenslust, die hier zum Lichte dr\u00e4ngt.", "tokens": ["Der", "Le\u00b7bens\u00b7lust", ",", "die", "hier", "zum", "Lich\u00b7te", "dr\u00e4ngt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In Tr\u00e4nen will das Auge dir erblinden;", "tokens": ["In", "Tr\u00e4\u00b7nen", "will", "das", "Au\u00b7ge", "dir", "er\u00b7blin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Schatten, die den Erdentag umgraun,", "tokens": ["Die", "Schat\u00b7ten", ",", "die", "den", "Er\u00b7den\u00b7tag", "um\u00b7graun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Im Strahl der Hoffnung m\u00fcssen sie verschwinden.", "tokens": ["Im", "Strahl", "der", "Hoff\u00b7nung", "m\u00fcs\u00b7sen", "sie", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "O holder Mut, o l\u00e4chelndes Vertraun!", "tokens": ["O", "hol\u00b7der", "Mut", ",", "o", "l\u00e4\u00b7cheln\u00b7des", "Ver\u00b7traun", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Es lebt! \u2013 und alle Sorgen sind vorbei.", "tokens": ["Es", "lebt", "!", "\u2013", "und", "al\u00b7le", "Sor\u00b7gen", "sind", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "KON", "PIAT", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wer m\u00f6chte noch zur\u00fcck nach Gr\u00e4bern schaun?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "noch", "zu\u00b7r\u00fcck", "nach", "Gr\u00e4\u00b7bern", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKVZ", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Nur mir hat dieser benedeite Schrei", "tokens": ["Nur", "mir", "hat", "die\u00b7ser", "be\u00b7ne\u00b7dei\u00b7te", "Schrei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das Auge nicht getr\u00e4nkt mit Freudeng\u00fcssen;", "tokens": ["Das", "Au\u00b7ge", "nicht", "ge\u00b7tr\u00e4nkt", "mit", "Freu\u00b7den\u00b7g\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Erstarrt im dumpfen Graun stand ich dabei.", "tokens": ["Er\u00b7starrt", "im", "dum\u00b7pfen", "Graun", "stand", "ich", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "O Kind, wie harmvoll mu\u00dft' ich dich begr\u00fc\u00dfen", "tokens": ["O", "Kind", ",", "wie", "harm\u00b7voll", "mu\u00dft'", "ich", "dich", "be\u00b7gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und habe meines Schluchzens Kampf und Beben", "tokens": ["Und", "ha\u00b7be", "mei\u00b7nes", "Schluch\u00b7zens", "Kampf", "und", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Erstickt an deines M\u00fcndleins ersten K\u00fcssen.", "tokens": ["Er\u00b7stickt", "an", "dei\u00b7nes", "M\u00fcnd\u00b7leins", "ers\u00b7ten", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Die Stunde war zum Eintritt in das Leben", "tokens": ["Die", "Stun\u00b7de", "war", "zum", "Ein\u00b7tritt", "in", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Nicht klug gew\u00e4hlt, zu kurze Frist vergangen,", "tokens": ["Nicht", "klug", "ge\u00b7w\u00e4hlt", ",", "zu", "kur\u00b7ze", "Frist", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Seitdem dein Bruder uns Valet gegeben.", "tokens": ["Seit\u00b7dem", "dein", "Bru\u00b7der", "uns", "Va\u00b7let", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Die Hand, die eben seine blassen Wangen", "tokens": ["Die", "Hand", ",", "die", "e\u00b7ben", "sei\u00b7ne", "blas\u00b7sen", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Geliebkost, k\u00fchl von Todesschwei\u00df beronnen,", "tokens": ["Ge\u00b7lieb\u00b7kost", ",", "k\u00fchl", "von", "To\u00b7des\u00b7schwei\u00df", "be\u00b7ron\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wie sollte sie dein H\u00e4ndlein nun umfangen?", "tokens": ["Wie", "soll\u00b7te", "sie", "dein", "H\u00e4nd\u00b7lein", "nun", "um\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wie t\u00f6richt schienst du mir, wie unbesonnen,", "tokens": ["Wie", "t\u00f6\u00b7richt", "schienst", "du", "mir", ",", "wie", "un\u00b7be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "In diesem W\u00fcrfelspiel dein Gl\u00fcck zu wagen,", "tokens": ["In", "die\u00b7sem", "W\u00fcr\u00b7fel\u00b7spiel", "dein", "Gl\u00fcck", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Drin jener seinen Einsatz kaum gewonnen!", "tokens": ["Drin", "je\u00b7ner", "sei\u00b7nen", "Ein\u00b7satz", "kaum", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Da h\u00f6rt' ich deine Mutter nach dir fragen:", "tokens": ["Da", "h\u00f6rt'", "ich", "dei\u00b7ne", "Mut\u00b7ter", "nach", "dir", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Wo ist das Kind? O gebt es mir, o gebt,", "tokens": ["Wo", "ist", "das", "Kind", "?", "O", "gebt", "es", "mir", ",", "o", "gebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "NE", "VVFIN", "PPER", "PPER", "$,", "FM", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Und la\u00dft mich f\u00fchlen seines Herzens Schlagen!", "tokens": ["Und", "la\u00dft", "mich", "f\u00fch\u00b7len", "sei\u00b7nes", "Her\u00b7zens", "Schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und wie dein kleiner Mund an ihrem bebt'", "tokens": ["Und", "wie", "dein", "klei\u00b7ner", "Mund", "an", "ih\u00b7rem", "bebt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Und sie mit mattem L\u00e4cheln, von Entz\u00fccken", "tokens": ["Und", "sie", "mit", "mat\u00b7tem", "L\u00e4\u00b7cheln", ",", "von", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Wie trunken, hauchte: Unser Kind! Es lebt! \u2013", "tokens": ["Wie", "trun\u00b7ken", ",", "hauch\u00b7te", ":", "Un\u00b7ser", "Kind", "!", "Es", "lebt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "$,", "VVFIN", "$.", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Da sprang der Reif um meine Brust in St\u00fccken,", "tokens": ["Da", "sprang", "der", "Reif", "um", "mei\u00b7ne", "Brust", "in", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und ich erkannte, da\u00df du wohlgetan,", "tokens": ["Und", "ich", "er\u00b7kann\u00b7te", ",", "da\u00df", "du", "wohl\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Da du's gewagt mit dieses Lebens T\u00fccken.", "tokens": ["Da", "du's", "ge\u00b7wagt", "mit", "die\u00b7ses", "Le\u00b7bens", "T\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Es leuchtet doch ein Stern auf deiner Bahn,", "tokens": ["Es", "leuch\u00b7tet", "doch", "ein", "Stern", "auf", "dei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Der wohl des Weges M\u00fche kann verg\u00fcten", "tokens": ["Der", "wohl", "des", "We\u00b7ges", "M\u00fc\u00b7he", "kann", "ver\u00b7g\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und dir zu H\u00e4upten stand, dich zu empfahn:", "tokens": ["Und", "dir", "zu", "H\u00e4up\u00b7ten", "stand", ",", "dich", "zu", "em\u00b7pfahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Ein Mutterauge wird dein Leben h\u00fcten,", "tokens": ["Ein", "Mut\u00b7ter\u00b7au\u00b7ge", "wird", "dein", "Le\u00b7ben", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Ein Mutterherz dir deine Schmerzen lindern,", "tokens": ["Ein", "Mut\u00b7ter\u00b7herz", "dir", "dei\u00b7ne", "Schmer\u00b7zen", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Ein Muttersegen reifen deine Bl\u00fcten \u2013", "tokens": ["Ein", "Mut\u00b7ter\u00b7se\u00b7gen", "rei\u00b7fen", "dei\u00b7ne", "Bl\u00fc\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "So wag es denn, mit andern Mutterkindern!", "tokens": ["So", "wag", "es", "denn", ",", "mit", "an\u00b7dern", "Mut\u00b7ter\u00b7kin\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}