{"dta.poem.10014": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herrliche Scenen treffen nur blo\u00df mit schwachem", "tokens": ["Herr\u00b7li\u00b7che", "Sce\u00b7nen", "tref\u00b7fen", "nur", "blo\u00df", "mit", "schwa\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ADV", "APPR", "ADJA"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Gem\u00fcthe des Schauers; sie locken allein das Ge-", "tokens": ["Das", "Ge\u00b7m\u00fc\u00b7the", "des", "Schau\u00b7ers", ";", "sie", "lo\u00b7cken", "al\u00b7lein", "das", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und erheben mit m\u00e4chtigem Trieb das f\u00fchllose Herz", "tokens": ["Und", "er\u00b7he\u00b7ben", "mit", "m\u00e4ch\u00b7ti\u00b7gem", "Trieb", "das", "f\u00fchl\u00b7lo\u00b7se", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Also reizt die d\u00e4dalische Landschaft das Auge des Sch\u00e4fers,", "tokens": ["Al\u00b7so", "reizt", "die", "d\u00e4\u00b7da\u00b7li\u00b7sche", "Land\u00b7schaft", "das", "Au\u00b7ge", "des", "Sch\u00e4\u00b7fers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Der von der heiteren Stirn des hohen Hymettus herab-", "tokens": ["Der", "von", "der", "hei\u00b7te\u00b7ren", "Stirn", "des", "ho\u00b7hen", "Hy\u00b7met\u00b7tus", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Hier stehn W\u00e4lder von Palmen, wo sonst die Stimme", "tokens": ["Hier", "stehn", "W\u00e4l\u00b7der", "von", "Pal\u00b7men", ",", "wo", "sonst", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Lehrreich erschallt; dort hebt aus dunkeln geheiligten", "tokens": ["Lehr\u00b7reich", "er\u00b7schallt", ";", "dort", "hebt", "aus", "dun\u00b7keln", "ge\u00b7hei\u00b7lig\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.8": {"text": "Sich der Oelbaum, der nimmer hier welkt, mit silber-", "tokens": ["Sich", "der", "O\u00b7el\u00b7baum", ",", "der", "nim\u00b7mer", "hier", "welkt", ",", "mit", "sil\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,", "APPR", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Dort verbreiten H\u00fcgel voll Reben die purpurnen Sch\u00e4tze,", "tokens": ["Dort", "ver\u00b7brei\u00b7ten", "H\u00fc\u00b7gel", "voll", "Re\u00b7ben", "die", "pur\u00b7pur\u00b7nen", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und manch sonnichtes Thal erstreckt in langen Prospekten", "tokens": ["Und", "manch", "son\u00b7nich\u00b7tes", "Thal", "er\u00b7streckt", "in", "lan\u00b7gen", "Pros\u00b7pek\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Fruchtbar sich weit in das Land; dort th\u00fcrmt, in Fluren", "tokens": ["Frucht\u00b7bar", "sich", "weit", "in", "das", "Land", ";", "dort", "th\u00fcrmt", ",", "in", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PRF", "ADJD", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "$,", "APPR", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Schimmernd, Athen sich auf; allein obgleich durch die", "tokens": ["Schim\u00b7mernd", ",", "A\u00b7then", "sich", "auf", ";", "al\u00b7lein", "ob\u00b7gleich", "durch", "die"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "NE", "PRF", "PTKVZ", "$.", "ADV", "KOUS", "APPR", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Seine zur Weisheit begeisternde Fluth Jlissus dahin", "tokens": ["Sei\u00b7ne", "zur", "Weis\u00b7heit", "be\u00b7geis\u00b7tern\u00b7de", "Fluth", "Jlis\u00b7sus", "da\u00b7hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "APPRART", "NN", "ADJA", "NN", "NE", "PAV"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Dessen krummes Gestade dickwallender Lorbeer be-", "tokens": ["Des\u00b7sen", "krum\u00b7mes", "Ge\u00b7sta\u00b7de", "dick\u00b7wal\u00b7len\u00b7der", "Lor\u00b7beer", "be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Obgleich seinen herrlichsten Glanz der rosichte Morgen", "tokens": ["Ob\u00b7gleich", "sei\u00b7nen", "herr\u00b7lichs\u00b7ten", "Glanz", "der", "ro\u00b7sich\u00b7te", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Ueber die heitre Scene verstreut: so f\u00fchlet der M\u00f6nch", "tokens": ["Ue\u00b7ber", "die", "heit\u00b7re", "Sce\u00b7ne", "ver\u00b7streut", ":", "so", "f\u00fch\u00b7let", "der", "M\u00f6nch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "In der ruhigen Brust mehr, und wahrhaftere Freuden,", "tokens": ["In", "der", "ru\u00b7hi\u00b7gen", "Brust", "mehr", ",", "und", "wahr\u00b7haf\u00b7te\u00b7re", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,", "KON", "ADJA", "NN", "$,"], "meter": "--++-+--++--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Wenn er vom hangenden Fels, der seine H\u00f6hle bedecket,", "tokens": ["Wenn", "er", "vom", "han\u00b7gen\u00b7den", "Fels", ",", "der", "sei\u00b7ne", "H\u00f6h\u00b7le", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+--", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Das verfallne Persepolis sieht. Die sinkenden Pfeiler", "tokens": ["Das", "ver\u00b7fall\u00b7ne", "Per\u00b7se\u00b7po\u00b7lis", "sieht", ".", "Die", "sin\u00b7ken\u00b7den", "Pfei\u00b7ler"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Sind auf die Ebnen umher in wilder Ordnung zer-", "tokens": ["Sind", "auf", "die", "Eb\u00b7nen", "um\u00b7her", "in", "wil\u00b7der", "Ord\u00b7nung", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Eine weite Verw\u00fcstung! Gleich einem verdorreten", "tokens": ["Ei\u00b7ne", "wei\u00b7te", "Ver\u00b7w\u00fcs\u00b7tung", "!", "Gleich", "ei\u00b7nem", "ver\u00b7dor\u00b7re\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "ART", "ADJA"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Welchen der Donner zerschellt, steigt hier die modern-", "tokens": ["Wel\u00b7chen", "der", "Don\u00b7ner", "zer\u00b7schellt", ",", "steigt", "hier", "die", "mo\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVPP", "$,", "VVFIN", "ADV", "ART", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.23": {"text": "Gegen die Wolken empor; hier zeigen parische Schl\u00f6sser", "tokens": ["Ge\u00b7gen", "die", "Wol\u00b7ken", "em\u00b7por", ";", "hier", "zei\u00b7gen", "pa\u00b7ri\u00b7sche", "Schl\u00f6s\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "ADV", "VVINF", "ADJA", "NN"], "meter": "+--+--+-+--+-+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Halb sich w\u00f6lbende Hallen, mit dicken Dornen bewachsen,", "tokens": ["Halb", "sich", "w\u00f6l\u00b7ben\u00b7de", "Hal\u00b7len", ",", "mit", "di\u00b7cken", "Dor\u00b7nen", "be\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.25": {"text": "Wo der R\u00e4uber ietzt laurt; der Fledermaus \u00f6de Be-", "tokens": ["Wo", "der", "R\u00e4u\u00b7ber", "ietzt", "laurt", ";", "der", "Fle\u00b7der\u00b7maus", "\u00f6\u00b7de", "Be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$.", "ART", "NN", "ADJA", "TRUNC"], "meter": "--+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Welche des Abends von da in d\u00e4mmernde Schatten hin-", "tokens": ["Wel\u00b7che", "des", "A\u00b7bends", "von", "da", "in", "d\u00e4m\u00b7mern\u00b7de", "Schat\u00b7ten", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "ADV", "APPR", "ADJA", "NN", "TRUNC"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Und wo ihren fleckigten Schweif die Otter sich nachschleppt,", "tokens": ["Und", "wo", "ih\u00b7ren", "flec\u00b7kig\u00b7ten", "Schweif", "die", "Ot\u00b7ter", "sich", "nach\u00b7schleppt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJA", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Ehmals die Wohnung des feinsten Geschmacks, und der", "tokens": ["Eh\u00b7mals", "die", "Woh\u00b7nung", "des", "feins\u00b7ten", "Ge\u00b7schmacks", ",", "und", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "$,", "KON", "ART"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.29": {"text": "Tempel erheben sich dort; in ihren geheiligten Grenzen", "tokens": ["Tem\u00b7pel", "er\u00b7he\u00b7ben", "sich", "dort", ";", "in", "ih\u00b7ren", "ge\u00b7hei\u00b7lig\u00b7ten", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "W\u00e4chst der Fichtenbaum auf, da die nun nackenden", "tokens": ["W\u00e4chst", "der", "Fich\u00b7ten\u00b7baum", "auf", ",", "da", "die", "nun", "na\u00b7cken\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "ART", "ADV", "VVINF"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.31": {"text": "Sonst vom flei\u00dfigen Kaufmann besucht, mit Grase be-", "tokens": ["Sonst", "vom", "flei\u00b7\u00dfi\u00b7gen", "Kauf\u00b7mann", "be\u00b7sucht", ",", "mit", "Gra\u00b7se", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVPP", "$,", "APPR", "NN", "TRUNC"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.32": {"text": "S\u00e4ulen liegen auf S\u00e4ulen gest\u00fcrzt, heruntergerissen", "tokens": ["S\u00e4u\u00b7len", "lie\u00b7gen", "auf", "S\u00e4u\u00b7len", "ge\u00b7st\u00fcrzt", ",", "her\u00b7un\u00b7ter\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "VVPP", "$,", "VVPP"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.33": {"text": "Von dem festen Gestell, und vermehren die modernde", "tokens": ["Von", "dem", "fes\u00b7ten", "Ge\u00b7stell", ",", "und", "ver\u00b7meh\u00b7ren", "die", "mo\u00b7dern\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "ADJA"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.34": {"text": "Weit umher erscheinen dem Blick die hangenden", "tokens": ["Weit", "um\u00b7her", "er\u00b7schei\u00b7nen", "dem", "Blick", "die", "han\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Von der verw\u00fcsteten Pracht, in einer verworrenen Scene", "tokens": ["Von", "der", "ver\u00b7w\u00fcs\u00b7te\u00b7ten", "Pracht", ",", "in", "ei\u00b7ner", "ver\u00b7wor\u00b7re\u00b7nen", "Sce\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "---+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Von Pall\u00e4sten, und H\u00e4usern, und B\u00f6gen, und D\u00e4m-", "tokens": ["Von", "Pal\u00b7l\u00e4s\u00b7ten", ",", "und", "H\u00e4u\u00b7sern", ",", "und", "B\u00f6\u00b7gen", ",", "und", "D\u00e4m"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "KON", "TRUNC"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.37": {"text": "Wo der Ruin, und Schrecken, und Graus, im schwarzen", "tokens": ["Wo", "der", "Ru\u00b7in", ",", "und", "Schre\u00b7cken", ",", "und", "Graus", ",", "im", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "KON", "NN", "$,", "KON", "NN", "$,", "APPRART", "VVINF"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}}}}