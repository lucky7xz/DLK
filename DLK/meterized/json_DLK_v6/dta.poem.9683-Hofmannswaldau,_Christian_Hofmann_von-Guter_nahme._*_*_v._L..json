{"dta.poem.9683": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Guter nahme.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ehr verschwindet/ gl\u00fcck vergeht/ geld nimmt ab und alle", "tokens": ["Ehr", "ver\u00b7schwin\u00b7det", "/", "gl\u00fcck", "ver\u00b7geht", "/", "geld", "nimmt", "ab", "und", "al\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "ADJD", "VVFIN", "$(", "NN", "VVFIN", "PTKVZ", "KON", "PIAT"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "sachen:", "tokens": ["sa\u00b7chen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Nichts kan frey vom untergang als ein guter nahme machen.", "tokens": ["Nichts", "kan", "frey", "vom", "un\u00b7ter\u00b7gang", "als", "ein", "gu\u00b7ter", "nah\u00b7me", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "APPRART", "ADJD", "KOKOM", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}