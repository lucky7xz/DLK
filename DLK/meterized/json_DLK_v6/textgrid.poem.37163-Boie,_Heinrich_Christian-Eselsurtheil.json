{"textgrid.poem.37163": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Eselsurtheil", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin, sprach Herr von Pilz, vom \u00e4ltesten Geschlechte!", "tokens": ["Ich", "bin", ",", "sprach", "Herr", "von", "Pilz", ",", "vom", "\u00e4l\u00b7tes\u00b7ten", "Ge\u00b7schlech\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "NN", "APPR", "NN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stemmt an seinen Wanst die Linke und die Rechte.", "tokens": ["Und", "stemmt", "an", "sei\u00b7nen", "Wanst", "die", "Lin\u00b7ke", "und", "die", "Rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Esel warf die Schnauz und schrie Iha!", "tokens": ["Sein", "E\u00b7sel", "warf", "die", "Schnauz", "und", "schrie", "I\u00b7ha", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin von \u00e4lterem Geschlechte,", "tokens": ["Ich", "bin", "von", "\u00e4l\u00b7te\u00b7rem", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn schon vor Adam war der Esel Ahnherr da.", "tokens": ["Denn", "schon", "vor", "A\u00b7dam", "war", "der", "E\u00b7sel", "Ahn\u00b7herr", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VAFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}