{"textgrid.poem.42697": {"metadata": {"author": {"name": "Ratschky, Joseph Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Horch! welch ein dumpfer Laut, wildmurrend, wie der tr\u00e4ge", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch! welch ein dumpfer Laut, wildmurrend, wie der tr\u00e4ge", "tokens": ["Horch", "!", "welch", "ein", "dum\u00b7pfer", "Laut", ",", "wild\u00b7mur\u00b7rend", ",", "wie", "der", "tr\u00e4\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PWAT", "ART", "ADJA", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschw\u00e4chte Ton entfernter Donnerschl\u00e4ge,", "tokens": ["Ge\u00b7schw\u00e4ch\u00b7te", "Ton", "ent\u00b7fern\u00b7ter", "Don\u00b7ner\u00b7schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dringt feyerlich an mein erstauntes Ohr,", "tokens": ["Dringt", "fey\u00b7er\u00b7lich", "an", "mein", "er\u00b7staun\u00b7tes", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und welch ein fremdes banges Zischen", "tokens": ["Und", "welch", "ein", "frem\u00b7des", "ban\u00b7ges", "Zi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Steigt schauderhaft aus den Geb\u00fcschen", "tokens": ["Steigt", "schau\u00b7der\u00b7haft", "aus", "den", "Ge\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des steilen Abhangs dort empor!", "tokens": ["Des", "stei\u00b7len", "Ab\u00b7hangs", "dort", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fchrt dieser enge Pfad mich zu der Feueresse", "tokens": ["F\u00fchrt", "die\u00b7ser", "en\u00b7ge", "Pfad", "mich", "zu", "der", "Feu\u00b7e\u00b7res\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des lahmen Donnerschmieds Vulkan?", "tokens": ["Des", "lah\u00b7men", "Don\u00b7ner\u00b7schmieds", "Vul\u00b7kan", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wie? oder w\u00e4lzt nicht fern sich \u00fcber Stein' und Kl\u00f6sse", "tokens": ["Wie", "?", "o\u00b7der", "w\u00e4lzt", "nicht", "fern", "sich", "\u00fc\u00b7ber", "Stein'", "und", "Kl\u00f6s\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KON", "VVFIN", "PTKNEG", "ADJD", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Flammenschwall des Phlegetons heran?", "tokens": ["Der", "Flam\u00b7men\u00b7schwall", "des", "Phle\u00b7ge\u00b7tons", "he\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Befl\u00fcgle deinen Schritt, o F\u00fchrer! solch ein Feuer", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "dei\u00b7nen", "Schritt", ",", "o", "F\u00fch\u00b7rer", "!", "solch", "ein", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Blies Neubegier noch nie in meinen Adern an.", "tokens": ["Blies", "Neu\u00b7be\u00b7gier", "noch", "nie", "in", "mei\u00b7nen", "A\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Befl\u00fcgle deinen Schritt! ein grosses Abentheuer", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "dei\u00b7nen", "Schritt", "!", "ein", "gros\u00b7ses", "A\u00b7bent\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Harrt unser. Lass uns k\u00fchn der Tiefe Rand uns nahn!", "tokens": ["Harrt", "un\u00b7ser", ".", "Lass", "uns", "k\u00fchn", "der", "Tie\u00b7fe", "Rand", "uns", "nahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "$.", "NE", "PPER", "ADJD", "ART", "ADJA", "NN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ha! wo bin ich? welche niegeseh'ne", "tokens": ["Ha", "!", "wo", "bin", "ich", "?", "wel\u00b7che", "nie\u00b7ge\u00b7seh'\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PPER", "$.", "PWAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Majest\u00e4tischf\u00fcrchterliche Scene", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7tischf\u00fcrch\u00b7ter\u00b7li\u00b7che", "Sce\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Natur enth\u00fcllt sich meinem Sinn!", "tokens": ["Der", "Na\u00b7tur", "ent\u00b7h\u00fcllt", "sich", "mei\u00b7nem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "T\u00e4uschen mich die Augen? oder raffte", "tokens": ["T\u00e4u\u00b7schen", "mich", "die", "Au\u00b7gen", "?", "o\u00b7der", "raff\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Zaubertrug mich in die fabelhafte", "tokens": ["Zau\u00b7ber\u00b7trug", "mich", "in", "die", "fa\u00b7bel\u00b7haf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Heimath wunderbarer Feen hin?", "tokens": ["Hei\u00b7math", "wun\u00b7der\u00b7ba\u00b7rer", "Feen", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh! ein Schneestrom, aufgepfl\u00fcgt von Klippen,", "tokens": ["Sieh", "!", "ein", "Schnee\u00b7strom", ",", "auf\u00b7ge\u00b7pfl\u00fcgt", "von", "Klip\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dr\u00e4ngt durch kahlgenagter Berge Rippen", "tokens": ["Dr\u00e4ngt", "durch", "kahl\u00b7ge\u00b7nag\u00b7ter", "Ber\u00b7ge", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sich heran an eine Felsenwand,", "tokens": ["Sich", "he\u00b7ran", "an", "ei\u00b7ne", "Fel\u00b7sen\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und entst\u00fcrzt wildschnaubend, gleich dem Winde,", "tokens": ["Und", "ent\u00b7st\u00fcrzt", "wild\u00b7schnau\u00b7bend", ",", "gleich", "dem", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der dem Rachen engverschlossner Schl\u00fcnde", "tokens": ["Der", "dem", "Ra\u00b7chen", "eng\u00b7ver\u00b7schloss\u00b7ner", "Schl\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sich entreisst, dem schaudervollen Rand.", "tokens": ["Sich", "en\u00b7treisst", ",", "dem", "schau\u00b7der\u00b7vol\u00b7len", "Rand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wie erbebt die schwache bange M\u00fchle,", "tokens": ["Wie", "er\u00b7bebt", "die", "schwa\u00b7che", "ban\u00b7ge", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deren Wand im heftigsten Gew\u00fchle", "tokens": ["De\u00b7ren", "Wand", "im", "hef\u00b7tigs\u00b7ten", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Diessseits die erbosste Flut besch\u00e4umt!", "tokens": ["Diess\u00b7seits", "die", "er\u00b7boss\u00b7te", "Flut", "be\u00b7sch\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Selbst die Veste Laufen scheint zu wanken,", "tokens": ["Selbst", "die", "Ves\u00b7te", "Lau\u00b7fen", "scheint", "zu", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Deren Giebel zwischen gr\u00fcnen Ranken", "tokens": ["De\u00b7ren", "Gie\u00b7bel", "zwi\u00b7schen", "gr\u00fc\u00b7nen", "Ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jenseits einem schroffen Berg entkeimt.", "tokens": ["Jen\u00b7seits", "ei\u00b7nem", "schrof\u00b7fen", "Berg", "ent\u00b7keimt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Nur die Zwillingsfelsen, deren nackte", "tokens": ["Nur", "die", "Zwil\u00b7lings\u00b7fel\u00b7sen", ",", "de\u00b7ren", "nack\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Scheiteln mitten in dem Katarakte", "tokens": ["Schei\u00b7teln", "mit\u00b7ten", "in", "dem", "Ka\u00b7ta\u00b7rak\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dort des Stroms vereinte Wuth best\u00fcrmt,", "tokens": ["Dort", "des", "Stroms", "ver\u00b7ein\u00b7te", "Wuth", "be\u00b7st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und von deren H\u00e4uptern Schaum in Flocken", "tokens": ["Und", "von", "de\u00b7ren", "H\u00e4up\u00b7tern", "Schaum", "in", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "NN", "NN", "APPR", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Dick emporstaubt, ragen unerschrocken", "tokens": ["Dick", "em\u00b7por\u00b7staubt", ",", "ra\u00b7gen", "un\u00b7er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aus der Flut, die wolkenw\u00e4rts sich th\u00fcrmt.", "tokens": ["Aus", "der", "Flut", ",", "die", "wol\u00b7ken\u00b7w\u00e4rts", "sich", "th\u00fcrmt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Beym Himmel! nicht umsonst verhiess des Rufes Stimme", "tokens": ["Beym", "Him\u00b7mel", "!", "nicht", "um\u00b7sonst", "ver\u00b7hiess", "des", "Ru\u00b7fes", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PTKNEG", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir grosse Wunder hier. Lass uns bergunter gehn,", "tokens": ["Mir", "gros\u00b7se", "Wun\u00b7der", "hier", ".", "Lass", "uns", "ber\u00b7gun\u00b7ter", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$.", "NE", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O F\u00fchrer, und beherzt in seinem vollen Grimme", "tokens": ["O", "F\u00fch\u00b7rer", ",", "und", "be\u00b7herzt", "in", "sei\u00b7nem", "vol\u00b7len", "Grim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Sturz der wilden Wogen sehn!", "tokens": ["Den", "Sturz", "der", "wil\u00b7den", "Wo\u00b7gen", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm! lass uns hin zu jenem Nachen eilen,", "tokens": ["Komm", "!", "lass", "uns", "hin", "zu", "je\u00b7nem", "Na\u00b7chen", "ei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der am Gestade dort uns freundlich zu sich winkt,", "tokens": ["Der", "am", "Ge\u00b7sta\u00b7de", "dort", "uns", "freund\u00b7lich", "zu", "sich", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "PPER", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und die besch\u00e4umten Fluten theilen,", "tokens": ["Und", "die", "be\u00b7sch\u00e4um\u00b7ten", "Flu\u00b7ten", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus denen spiegelhell die Sonne wiederblinkt!...", "tokens": ["Aus", "de\u00b7nen", "spie\u00b7gel\u00b7hell", "die", "Son\u00b7ne", "wie\u00b7der\u00b7blinkt", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Schon tanzt, vom Ufer fern, der k\u00fchne", "tokens": ["Schon", "tanzt", ",", "vom", "U\u00b7fer", "fern", ",", "der", "k\u00fch\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPRART", "NN", "PTKVZ", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Harmlose Kahn mit uns durch den emp\u00f6rten Schwall:", "tokens": ["Harm\u00b7lo\u00b7se", "Kahn", "mit", "uns", "durch", "den", "em\u00b7p\u00f6r\u00b7ten", "Schwall", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Sieh! h\u00f6her hebt sich itzt des Falles breite B\u00fchne,", "tokens": ["Sieh", "!", "h\u00f6\u00b7her", "hebt", "sich", "itzt", "des", "Fal\u00b7les", "brei\u00b7te"