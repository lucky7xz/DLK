{"textgrid.poem.46662": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ach, von dort]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, von dort,", "tokens": ["Ach", ",", "von", "dort", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wo sie sind vor mir verholen,", "tokens": ["Wo", "sie", "sind", "vor", "mir", "ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich sie mir wiederholen!", "tokens": ["K\u00f6nnt'", "ich", "sie", "mir", "wie\u00b7der\u00b7ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df sie nur sich finden lie\u00dfen!", "tokens": ["Da\u00df", "sie", "nur", "sich", "fin\u00b7den", "lie\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PRF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Suchen soll mich nicht verdrie\u00dfen,", "tokens": ["Su\u00b7chen", "soll", "mich", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Pfad, wo Dornen sprie\u00dfen,", "tokens": ["Auf", "dem", "Pfad", ",", "wo", "Dor\u00b7nen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fort und fort", "tokens": ["Fort", "und", "fort"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Lief' ich gern mit blut'gen Sohlen,", "tokens": ["Lie\u00b7f'", "ich", "gern", "mit", "blut'\u00b7gen", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "K\u00f6nnt' ich sie mir wieder holen.", "tokens": ["K\u00f6nnt'", "ich", "sie", "mir", "wie\u00b7der", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sollt' ich eine M\u00fche scheuen?", "tokens": ["Sollt'", "ich", "ei\u00b7ne", "M\u00fc\u00b7he", "scheu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollte mich ein Weg gereuen?", "tokens": ["Soll\u00b7te", "mich", "ein", "Weg", "ge\u00b7reu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Thr\u00e4nen wollt' ich streuen", "tokens": ["Mei\u00b7ne", "Thr\u00e4\u00b7nen", "wollt'", "ich", "streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Wort,", "tokens": ["Oh\u00b7ne", "Wort", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Gehen \u00fcber gl\u00fchn'de Kohlen,", "tokens": ["Ge\u00b7hen", "\u00fc\u00b7ber", "gl\u00fchn'\u00b7de", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnt' ich sie mir wieder holen.", "tokens": ["K\u00f6nnt'", "ich", "sie", "mir", "wie\u00b7der", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In ein Schiff von Glas geschliffen", "tokens": ["In", "ein", "Schiff", "von", "Glas", "ge\u00b7schlif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wollt' ich steigen und durchschiffen", "tokens": ["Wollt'", "ich", "stei\u00b7gen", "und", "durch\u00b7schif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meere voll von Felsenriffen,", "tokens": ["Mee\u00b7re", "voll", "von", "Fel\u00b7sen\u00b7rif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Port,", "tokens": ["Oh\u00b7ne", "Port", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Suchen an den beiden Polen,", "tokens": ["Su\u00b7chen", "an", "den", "bei\u00b7den", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00f6nnt' ich sie mir wieder holen.", "tokens": ["K\u00f6nnt'", "ich", "sie", "mir", "wie\u00b7der", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Von dem Ort, von jenem Ort,", "tokens": ["Von", "dem", "Ort", ",", "von", "je\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo man sie mir hingestohlen,", "tokens": ["Wo", "man", "sie", "mir", "hin\u00b7ge\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach von dort,", "tokens": ["Ach", "von", "dort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wo sie sind vor mir verholen,", "tokens": ["Wo", "sie", "sind", "vor", "mir", "ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt' ich sie mir wieder holen!", "tokens": ["K\u00f6nnt'", "ich", "sie", "mir", "wie\u00b7der", "ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}