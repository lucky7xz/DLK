{"dta.poem.15767": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geehret sei das Wort! es ist des Geistes Spiegel,", "tokens": ["Ge\u00b7eh\u00b7ret", "sei", "das", "Wort", "!", "es", "ist", "des", "Geis\u00b7tes", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist des Gedankens, der gereift, Vollendungssiegel.", "tokens": ["Ist", "des", "Ge\u00b7dan\u00b7kens", ",", "der", "ge\u00b7reift", ",", "Vol\u00b7len\u00b7dungs\u00b7sie\u00b7gel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "VVPP", "$,", "NN", "$."], "meter": "+--+-+-++-+-+", "measure": "iambic.septa.invert"}}, "stanza.2": {"line.1": {"text": "Wo ihm das Siegel fehlt, gilt er sich selber nicht;", "tokens": ["Wo", "ihm", "das", "Sie\u00b7gel", "fehlt", ",", "gilt", "er", "sich", "sel\u00b7ber", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und wo der Spiegel fehlt, gewahrt sich nicht das Licht.", "tokens": ["Und", "wo", "der", "Spie\u00b7gel", "fehlt", ",", "ge\u00b7wahrt", "sich", "nicht", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "PRF", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn es Spiegel ist, so ist es nur zum Gleichen,", "tokens": ["Doch", "wenn", "es", "Spie\u00b7gel", "ist", ",", "so", "ist", "es", "nur", "zum", "Glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn es Siegel ist, so ist es nur zum Zeichen.", "tokens": ["Und", "wenn", "es", "Sie\u00b7gel", "ist", ",", "so", "ist", "es", "nur", "zum", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nie dem Gespiegelten entspricht der Spiegelglanz,", "tokens": ["Nie", "dem", "Ge\u00b7spie\u00b7gel\u00b7ten", "ent\u00b7spricht", "der", "Spie\u00b7gel\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nie dem Versiegelten das \u00e4u\u00dfre Siegel ganz.", "tokens": ["Nie", "dem", "Ver\u00b7sie\u00b7gel\u00b7ten", "das", "\u00e4u\u00df\u00b7re", "Sie\u00b7gel", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Wer in die Formeln will des Worts die Geister bannen,", "tokens": ["Wer", "in", "die", "For\u00b7meln", "will", "des", "Worts", "die", "Geis\u00b7ter", "ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Formeln bleiben ihm, die Geister gehn vondannen.", "tokens": ["Die", "For\u00b7meln", "blei\u00b7ben", "ihm", ",", "die", "Geis\u00b7ter", "gehn", "von\u00b7dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Du aber suche fein die Geister zu belauschen,", "tokens": ["Du", "a\u00b7ber", "su\u00b7che", "fein", "die", "Geis\u00b7ter", "zu", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie, wandelnd unsichtbar, sie Wortgewande tauschen.", "tokens": ["Wie", ",", "wan\u00b7delnd", "un\u00b7sicht\u00b7bar", ",", "sie", "Wort\u00b7ge\u00b7wan\u00b7de", "tau\u00b7schen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ADJD", "$,", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}