{"dta.poem.5368": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Recht ihm danckbar werden m\u00f6gtest? sind des heutgen Ta-", "tokens": ["Recht", "ihm", "dan\u00b7ck\u00b7bar", "wer\u00b7den", "m\u00f6g\u00b7test", "?", "sind", "des", "heut\u00b7gen", "Ta"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "VAINF", "VMFIN", "$.", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "ges Stunden", "tokens": ["ges", "Stun\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "N\u00fctzlich angewendet worden? hast du, um, nach dieser Zeit,", "tokens": ["N\u00fctz\u00b7lich", "an\u00b7ge\u00b7wen\u00b7det", "wor\u00b7den", "?", "hast", "du", ",", "um", ",", "nach", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAPP", "$.", "VAFIN", "PPER", "$,", "KOUI", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dich an den noch h\u00f6hern Freuden einer seel\u2019gen Ewigkeit", "tokens": ["Dich", "an", "den", "noch", "h\u00f6\u00b7hern", "Freu\u00b7den", "ei\u00b7ner", "seel'\u00b7gen", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Zu vergn\u00fcgen, dich vergn\u00fcget? ists geschehn? so freue dich;", "tokens": ["Zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "dich", "ver\u00b7gn\u00fc\u00b7get", "?", "ists", "ge\u00b7schehn", "?", "so", "freu\u00b7e", "dich", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "VVFIN", "$.", "VAFIN", "VVPP", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Danck auch daf\u00fcr, r\u00fchm\u2019 und preise deinen Gott absonderlich.", "tokens": ["Danck", "auch", "da\u00b7f\u00fcr", ",", "r\u00fchm'", "und", "prei\u00b7se", "dei\u00b7nen", "Gott", "ab\u00b7son\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PAV", "$,", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Weil es aber leider \u00f6fters unterblieben, will ich mich,", "tokens": ["Weil", "es", "a\u00b7ber", "lei\u00b7der", "\u00f6f\u00b7ters", "un\u00b7ter\u00b7blie\u00b7ben", ",", "will", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVINF", "$,", "VMFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Vater der Barmhertzigkeit, jetzt in Demuth zu dir kehren,", "tokens": ["Va\u00b7ter", "der", "Barm\u00b7hert\u00b7zig\u00b7keit", ",", "jetzt", "in", "De\u00b7muth", "zu", "dir", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "APPR", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "+---+-+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und in einer ernsten Reue, voll Vertrauen, dich verehren.", "tokens": ["Und", "in", "ei\u00b7ner", "erns\u00b7ten", "Reu\u00b7e", ",", "voll", "Ver\u00b7trau\u00b7en", ",", "dich", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "HeRR, ich flehe dich von Hertzen, voll Betr\u00fcbni\u00df, voller", "tokens": ["HeRR", ",", "ich", "fle\u00b7he", "dich", "von", "Hert\u00b7zen", ",", "voll", "Be\u00b7tr\u00fcb\u00b7ni\u00df", ",", "vol\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,", "ADJD", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Schaam", "tokens": ["Schaam"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Ueber mein unachtsam Wesen; und, voll Reue, voller Gram", "tokens": ["Ue\u00b7ber", "mein", "u\u00b7nacht\u00b7sam", "We\u00b7sen", ";", "und", ",", "voll", "Reu\u00b7e", ",", "vol\u00b7ler", "Gram"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$.", "KON", "$,", "ADJD", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Ueber mein so blind Betragen, ernstlich um Vergebung an,", "tokens": ["Ue\u00b7ber", "mein", "so", "blind", "Be\u00b7tra\u00b7gen", ",", "ernst\u00b7lich", "um", "Ver\u00b7ge\u00b7bung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJD", "NN", "$,", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Ach, verzeihe, wenn ich heute nicht, was ich gesollt-", "tokens": ["Ach", ",", "ver\u00b7zei\u00b7he", ",", "wenn", "ich", "heu\u00b7te", "nicht", ",", "was", "ich", "ge\u00b7soll\u00b7t"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "$,", "PWS", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.15": {"text": "gethan!", "tokens": ["ge\u00b7than", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Und da ich nunmehr zur Ruh meine matten Glieder strecke;", "tokens": ["Und", "da", "ich", "nun\u00b7mehr", "zur", "Ruh", "mei\u00b7ne", "mat\u00b7ten", "Glie\u00b7der", "stre\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "----+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ach so la\u00df mich sanfte ruhn! da\u00df kein b\u00f6ser Traum mich", "tokens": ["Ach", "so", "la\u00df", "mich", "sanf\u00b7te", "ruhn", "!", "da\u00df", "kein", "b\u00f6\u00b7ser", "Traum", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVIMP", "PPER", "VVFIN", "VVINF", "$.", "KOUS", "PIAT", "ADJA", "NN", "PPER"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "schrecke!", "tokens": ["schre\u00b7cke", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "La\u00df den Leib, doch auch zugleich meinen Geist, gest\u00e4rcket seyn,", "tokens": ["La\u00df", "den", "Leib", ",", "doch", "auch", "zu\u00b7gleich", "mei\u00b7nen", "Geist", ",", "ge\u00b7st\u00e4r\u00b7cket", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$,", "VVPP", "VAINF", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Da\u00df sie beide ihre Pflicht, bey des k\u00fcnftgen Tages Schein,", "tokens": ["Da\u00df", "sie", "bei\u00b7de", "ih\u00b7re", "Pflicht", ",", "bey", "des", "k\u00fcnft\u00b7gen", "Ta\u00b7ges", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.21": {"text": "Zu beachten f\u00e4hig bleiben, und, zu deiner Ehr\u2019 zu leben,", "tokens": ["Zu", "be\u00b7ach\u00b7ten", "f\u00e4\u00b7hig", "blei\u00b7ben", ",", "und", ",", "zu", "dei\u00b7ner", "Ehr'", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "VVINF", "$,", "KON", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "bestreben,", "tokens": ["be\u00b7stre\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Deine Weisheit, Lieb\u2019 und Allmacht, in den wunderbaren", "tokens": ["Dei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'", "und", "All\u00b7macht", ",", "in", "den", "wun\u00b7der\u00b7ba\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.25": {"text": "Wercken,", "tokens": ["Wer\u00b7cken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Zu betrachten, zu bewundern, zu erheben, zu bemercken,", "tokens": ["Zu", "be\u00b7trach\u00b7ten", ",", "zu", "be\u00b7wun\u00b7dern", ",", "zu", "er\u00b7he\u00b7ben", ",", "zu", "be\u00b7mer\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.27": {"text": "Sich mit Lust besch\u00e4ftigen! Wann wir auch den N\u00e4chsten", "tokens": ["Sich", "mit", "Lust", "be\u00b7sch\u00e4f\u00b7ti\u00b7gen", "!", "Wann", "wir", "auch", "den", "N\u00e4chs\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "NN", "VVINF", "$.", "PWAV", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "lieben", "tokens": ["lie\u00b7ben"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Und ihm gutes g\u00f6nnen sollen, wie Natur und Bibel lehrt,", "tokens": ["Und", "ihm", "gu\u00b7tes", "g\u00f6n\u00b7nen", "sol\u00b7len", ",", "wie", "Na\u00b7tur", "und", "Bi\u00b7bel", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "VVINF", "VMFIN", "$,", "PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Ach, so bin ich auch an die jetzt zu dencken angetrieben,", "tokens": ["Ach", ",", "so", "bin", "ich", "auch", "an", "die", "jetzt", "zu", "den\u00b7cken", "an\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PRELS", "ADV", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.31": {"text": "Welche Mangel, Kummer, Armuth, Frost, Gefahr und", "tokens": ["Wel\u00b7che", "Man\u00b7gel", ",", "Kum\u00b7mer", ",", "Ar\u00b7muth", ",", "Frost", ",", "Ge\u00b7fahr", "und"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.32": {"text": "Noch beschwehrt,", "tokens": ["Noch", "be\u00b7schwehrt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}}}}}