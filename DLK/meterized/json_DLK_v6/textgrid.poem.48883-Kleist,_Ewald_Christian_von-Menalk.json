{"textgrid.poem.48883": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Menalk", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menalk floh kummervoll den Reiz der sch\u00f6nsten Flur,", "tokens": ["Me\u00b7nalk", "floh", "kum\u00b7mer\u00b7voll", "den", "Reiz", "der", "sch\u00f6ns\u00b7ten", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Schatten und kein Bach, sein Harm gefiel ihm nur.", "tokens": ["Kein", "Schat\u00b7ten", "und", "kein", "Bach", ",", "sein", "Harm", "ge\u00b7fiel", "ihm", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Heerde gieng zerstreut; er n\u00e4hrt' in einer H\u00f6hle", "tokens": ["Die", "Heer\u00b7de", "gieng", "zer\u00b7streut", ";", "er", "n\u00e4hrt'", "in", "ei\u00b7ner", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom fr\u00fchen Morgen an, die Schmerzen seiner Seele.", "tokens": ["Vom", "fr\u00fc\u00b7hen", "Mor\u00b7gen", "an", ",", "die", "Schmer\u00b7zen", "sei\u00b7ner", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ungl\u00fccklicher Menalk! gedacht er da bey sich,", "tokens": ["Un\u00b7gl\u00fcck\u00b7li\u00b7cher", "Me\u00b7nalk", "!", "ge\u00b7dacht", "er", "da", "bey", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVPP", "PPER", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Warum bist du gezeugt? Die Schickung hasset dich,", "tokens": ["Wa\u00b7rum", "bist", "du", "ge\u00b7zeugt", "?", "Die", "Schi\u00b7ckung", "has\u00b7set", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "$.", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch sie ward Doris j\u00fcngst von dieser Flur gezogen.", "tokens": ["Durch", "sie", "ward", "Do\u00b7ris", "j\u00fcngst", "von", "die\u00b7ser", "Flur", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NE", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "O w\u00e4r den Augenblick dein Geist ihr nachgeflogen,", "tokens": ["O", "w\u00e4r", "den", "Au\u00b7gen\u00b7blick", "dein", "Geist", "ihr", "nach\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und dieser Leib verwest! Zwar bey Amyntens Tod", "tokens": ["Und", "die\u00b7ser", "Leib", "ver\u00b7west", "!", "Zwar", "bey", "A\u00b7myn\u00b7tens", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "$.", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fchlt ich die Freude nicht, die mir der Fr\u00fchling both;", "tokens": ["F\u00fchlt", "ich", "die", "Freu\u00b7de", "nicht", ",", "die", "mir", "der", "Fr\u00fch\u00b7ling", "both", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch endlich hat die Zeit den Kummer \u00fcberwunden,", "tokens": ["Doch", "end\u00b7lich", "hat", "die", "Zeit", "den", "Kum\u00b7mer", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er ist, dacht ich, zuerst der Nichtigkeit entbunden,", "tokens": ["Er", "ist", ",", "dacht", "ich", ",", "zu\u00b7erst", "der", "Nich\u00b7tig\u00b7keit", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und schaut dir jezt vielleicht von oben gl\u00e4nzend zu,", "tokens": ["Und", "schaut", "dir", "jezt", "viel\u00b7leicht", "von", "o\u00b7ben", "gl\u00e4n\u00b7zend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schaut Sternen unter sich, ist gl\u00fccklicher als du,", "tokens": ["Schaut", "Ster\u00b7nen", "un\u00b7ter", "sich", ",", "ist", "gl\u00fcck\u00b7li\u00b7cher", "als", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PRF", "$,", "VAFIN", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nur jezt wird keine Zeit dein ewig Leid vermindern,", "tokens": ["Nur", "jezt", "wird", "kei\u00b7ne", "Zeit", "dein", "e\u00b7wig", "Leid", "ver\u00b7min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "NN", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sie lebt und lebt entfernt! Komm Tod! du kannst es lindern,", "tokens": ["Sie", "lebt", "und", "lebt", "ent\u00b7fernt", "!", "Komm", "Tod", "!", "du", "kannst", "es", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$.", "VVFIN", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Komm, jezt ist Welt und Gl\u00fcck und Leben mir verha\u00dft.", "tokens": ["Komm", ",", "jezt", "ist", "Welt", "und", "Gl\u00fcck", "und", "Le\u00b7ben", "mir", "ver\u00b7ha\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VAFIN", "NN", "KON", "NN", "KON", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ihr Felsen st\u00fcrzt herab, begrabt mich in der Last,", "tokens": ["Ihr", "Fel\u00b7sen", "st\u00fcrzt", "her\u00b7ab", ",", "be\u00b7grabt", "mich", "in", "der", "Last", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die meiner Scheitel droht \u2013 \u2013 \u2013 O mu\u00df ich euch ihr Auen,", "tokens": ["Die", "mei\u00b7ner", "Schei\u00b7tel", "droht", "\u2013", "\u2013", "\u2013", "O", "mu\u00df", "ich", "euch", "ihr", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "$(", "$(", "NE", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ihr uns oft verbargt, noch ferner gr\u00fcnen schauen!", "tokens": ["Die", "ihr", "uns", "oft", "ver\u00b7bargt", ",", "noch", "fer\u00b7ner", "gr\u00fc\u00b7nen", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP", "$,", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ihr martert meinen Geist, reizt ihr gleich das Gesicht,", "tokens": ["Ihr", "mar\u00b7tert", "mei\u00b7nen", "Geist", ",", "reizt", "ihr", "gleich", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Ihr zeigt mir Doris Bild, und zeigt mir Doris nicht.", "tokens": ["Ihr", "zeigt", "mir", "Do\u00b7ris", "Bild", ",", "und", "zeigt", "mir", "Do\u00b7ris", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "NN", "$,", "KON", "VVFIN", "PPER", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nur zum entfernten Belt \u2013 \u2013 Doch wer kann dir entrinnen,", "tokens": ["Nur", "zum", "ent\u00b7fern\u00b7ten", "Belt", "\u2013", "\u2013", "Doch", "wer", "kann", "dir", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$(", "$(", "KON", "PWS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "O Liebe, welch ein Wahn bet\u00e4ubt die m\u00fcden Sinnen!", "tokens": ["O", "Lie\u00b7be", ",", "welch", "ein", "Wahn", "be\u00b7t\u00e4ubt", "die", "m\u00fc\u00b7den", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAT", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und trieb auch Angst und Quaal zum Nordpol meinen Schritt!", "tokens": ["Und", "trieb", "auch", "Angst", "und", "Qua\u00b7al", "zum", "Nord\u00b7pol", "mei\u00b7nen", "Schritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "KON", "NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "So fl\u00f6h doch Doris Bild, gleich meinem Schatten mit.", "tokens": ["So", "fl\u00f6h", "doch", "Do\u00b7ris", "Bild", ",", "gleich", "mei\u00b7nem", "Schat\u00b7ten", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ja dort \u2013 \u2013 dort seh ich sie, dort hat sie oft gesprungen,", "tokens": ["Ja", "dort", "\u2013", "\u2013", "dort", "seh", "ich", "sie", ",", "dort", "hat", "sie", "oft", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$(", "$(", "ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und oft in buntem Klee den Arm um mich geschlungen.", "tokens": ["Und", "oft", "in", "bun\u00b7tem", "Klee", "den", "Arm", "um", "mich", "ge\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dort, d\u00e4ucht mich, h\u00f6r ich noch am Teich den Zauberklang,", "tokens": ["Dort", ",", "d\u00e4ucht", "mich", ",", "h\u00f6r", "ich", "noch", "am", "Teich", "den", "Zau\u00b7ber\u00b7klang", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Als sie und Galathee Dianens Glut besang.", "tokens": ["Als", "sie", "und", "Ga\u00b7la\u00b7thee", "Di\u00b7a\u00b7nens", "Glut", "be\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "NE", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich war Endymion, nach dem sie heimlich blickte,", "tokens": ["Ich", "war", "En\u00b7dy\u00b7mi\u00b7on", ",", "nach", "dem", "sie", "heim\u00b7lich", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dem sie bey manchem Ort die Hand verstohlen dr\u00fcckte.", "tokens": ["Dem", "sie", "bey", "man\u00b7chem", "Ort", "die", "Hand", "ver\u00b7stoh\u00b7len", "dr\u00fcck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Dort ruht ich einst allein im Rosenthal am Bach,", "tokens": ["Dort", "ruht", "ich", "einst", "al\u00b7lein", "im", "Ro\u00b7sen\u00b7thal", "am", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich schlo\u00df die Augen zu, dacht ihrem Liebreitz nach,", "tokens": ["Ich", "schlo\u00df", "die", "Au\u00b7gen", "zu", ",", "dacht", "ih\u00b7rem", "Lieb\u00b7reitz", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Die Lose wu\u00dfte sich am Ufer hinter Str\u00e4uchen,", "tokens": ["Die", "Lo\u00b7se", "wu\u00df\u00b7te", "sich", "am", "U\u00b7fer", "hin\u00b7ter", "Str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ohn da\u00df ich sie vernahm, behutsam anzuschleichen,", "tokens": ["Ohn", "da\u00df", "ich", "sie", "ver\u00b7nahm", ",", "be\u00b7hut\u00b7sam", "an\u00b7zu\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und stund ihr D\u00e4mon gleich, der um sie buhlte, nah,", "tokens": ["Und", "stund", "ihr", "D\u00e4\u00b7mon", "gleich", ",", "der", "um", "sie", "buhl\u00b7te", ",", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So k\u00fc\u00dfte sie mich doch, als er nur seitw\u00e4rts sah,", "tokens": ["So", "k\u00fc\u00df\u00b7te", "sie", "mich", "doch", ",", "als", "er", "nur", "seit\u00b7w\u00e4rts", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Schnell sprang sie um den Strauch, die Bl\u00e4tter h\u00f6rt ich rauschen;", "tokens": ["Schnell", "sprang", "sie", "um", "