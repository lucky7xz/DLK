{"textgrid.poem.36353": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Retter", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eile herbei zu retten,", "tokens": ["Ei\u00b7le", "her\u00b7bei", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "O Menschensohn, und brich des Fremdlings Ketten!", "tokens": ["O", "Men\u00b7schen\u00b7sohn", ",", "und", "brich", "des", "Fremd\u00b7lings", "Ket\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df dich den Tod gel\u00fcsten,", "tokens": ["La\u00df", "dich", "den", "Tod", "ge\u00b7l\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum zweitenmale dich mit Sieg zu r\u00fcsten!", "tokens": ["Zum", "zwei\u00b7ten\u00b7ma\u00b7le", "dich", "mit", "Sieg", "zu", "r\u00fcs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Tigers grimme Klauen", "tokens": ["Des", "Ti\u00b7gers", "grim\u00b7me", "Klau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerrei\u00dfen deines Fr\u00fchlings Blumenauen;", "tokens": ["Zer\u00b7rei\u00b7\u00dfen", "dei\u00b7nes", "Fr\u00fch\u00b7lings", "Blu\u00b7me\u00b7nau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Banden all' entlassen,", "tokens": ["Der", "Ban\u00b7den", "all'", "ent\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wankt alles, wogt und kriegt in wildem Hassen;", "tokens": ["Wankt", "al\u00b7les", ",", "wogt", "und", "kriegt", "in", "wil\u00b7dem", "Has\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wer treu noch, ganz verraten,", "tokens": ["Wer", "treu", "noch", ",", "ganz", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wird schlimm gelohnt der alten Tugend Taten.", "tokens": ["Wird", "schlimm", "ge\u00b7lohnt", "der", "al\u00b7ten", "Tu\u00b7gend", "Ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "La\u00df, wieder zu erscheinen,", "tokens": ["La\u00df", ",", "wie\u00b7der", "zu", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Dich endlich doch erflehen von den Deinen!", "tokens": ["Dich", "end\u00b7lich", "doch", "er\u00b7fle\u00b7hen", "von", "den", "Dei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zu dir den frohen Glauben,", "tokens": ["Zu", "dir", "den", "fro\u00b7hen", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Soll keiner Arglist Finsternis uns rauben.", "tokens": ["Soll", "kei\u00b7ner", "Arg\u00b7list", "Fins\u00b7ter\u00b7nis", "uns", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "NE", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Als K\u00f6nig wirst du kommen,", "tokens": ["Als", "K\u00f6\u00b7nig", "wirst", "du", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Des Herzens Opfer ist schon angeglommen;", "tokens": ["Des", "Her\u00b7zens", "Op\u00b7fer", "ist", "schon", "an\u00b7ge\u00b7glom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Nicht ferne sind die Zeiten,", "tokens": ["Nicht", "fer\u00b7ne", "sind", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Da man dir wird die Lagerstatt bereiten.", "tokens": ["Da", "man", "dir", "wird", "die", "La\u00b7ger\u00b7statt", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Als L\u00f6we wirst du siegen,", "tokens": ["Als", "L\u00f6\u00b7we", "wirst", "du", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Als Adler in der Sonne Lichtglanz fliegen;", "tokens": ["Als", "Ad\u00b7ler", "in", "der", "Son\u00b7ne", "Licht\u00b7glanz", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Strafend den F\u00fcrst der B\u00f6sen,", "tokens": ["Stra\u00b7fend", "den", "F\u00fcrst", "der", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.22": {"text": "Wirst du vom schlimmen Joch dein Volk erl\u00f6sen.", "tokens": ["Wirst", "du", "vom", "schlim\u00b7men", "Joch", "dein", "Volk", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Der Ehre treu, dem Rechte,", "tokens": ["Der", "Eh\u00b7re", "treu", ",", "dem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Sind dennoch jetzt auch deines Volks Geschlechte;", "tokens": ["Sind", "den\u00b7noch", "jetzt", "auch", "dei\u00b7nes", "Volks", "Ge\u00b7schlech\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Vereine du nur wieder", "tokens": ["Ver\u00b7ei\u00b7ne", "du", "nur", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Zu einer Treue Bund die Heldenbr\u00fcder.", "tokens": ["Zu", "ei\u00b7ner", "Treu\u00b7e", "Bund", "die", "Hel\u00b7den\u00b7br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Den wir als Retter gr\u00fc\u00dfen,", "tokens": ["Den", "wir", "als", "Ret\u00b7ter", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "In Liebe sinkend zu des K\u00f6nigs F\u00fc\u00dfen,", "tokens": ["In", "Lie\u00b7be", "sin\u00b7kend", "zu", "des", "K\u00f6\u00b7nigs", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "La\u00df deine Lieb' uns erben,", "tokens": ["La\u00df", "dei\u00b7ne", "Lieb'", "uns", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "K\u00e4mpfend gleich dir den sch\u00f6nen Tod doch sterben.", "tokens": ["K\u00e4mp\u00b7fend", "gleich", "dir", "den", "sch\u00f6\u00b7nen", "Tod", "doch", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}