{"dta.poem.23437": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Armenrecht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wem klag' ich jetzo meine Noth?", "tokens": ["Wem", "klag'", "ich", "jet\u00b7zo", "mei\u00b7ne", "Noth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch du, des Kaisers Vogt, bist todt,", "tokens": ["Auch", "du", ",", "des", "Kai\u00b7sers", "Vogt", ",", "bist", "todt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "NN", "NE", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du der Kirche Vogt desgleichen,", "tokens": ["Und", "du", "der", "Kir\u00b7che", "Vogt", "des\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du Vogt der Armen und der Reichen!", "tokens": ["Du", "Vogt", "der", "Ar\u00b7men", "und", "der", "Rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Todt ist der Vogt! ein schwacher Schein,", "tokens": ["Todt", "ist", "der", "Vogt", "!", "ein", "schwa\u00b7cher", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Name blieb uns nur allein.", "tokens": ["Der", "Na\u00b7me", "blieb", "uns", "nur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was haben wir, wir Bettelleute,", "tokens": ["Was", "ha\u00b7ben", "wir", ",", "wir", "Bet\u00b7tel\u00b7leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was haben wir f\u00fcr V\u00f6gte heute?", "tokens": ["Was", "ha\u00b7ben", "wir", "f\u00fcr", "V\u00f6g\u00b7te", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}