{"textgrid.poem.63317": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Seilt\u00e4nzer", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er geht. Die schr\u00e4ge Stange tr\u00e4gt ihn linde.", "tokens": ["Er", "geht", ".", "Die", "schr\u00e4\u00b7ge", "Stan\u00b7ge", "tr\u00e4gt", "ihn", "lin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Himmel schl\u00e4gt um ihn ein Feuerrad.", "tokens": ["Der", "Him\u00b7mel", "schl\u00e4gt", "um", "ihn", "ein", "Feu\u00b7er\u00b7rad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein L\u00e4cheln f\u00e4llt von einem mageren Kinde,", "tokens": ["Ein", "L\u00e4\u00b7cheln", "f\u00e4llt", "von", "ei\u00b7nem", "ma\u00b7ge\u00b7ren", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und an dem L\u00e4cheln wird die Mutter satt.", "tokens": ["Und", "an", "dem", "L\u00e4\u00b7cheln", "wird", "die", "Mut\u00b7ter", "satt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein jeder f\u00fchlt sich \u00fcber sich erhaben", "tokens": ["Ein", "je\u00b7der", "f\u00fchlt", "sich", "\u00fc\u00b7ber", "sich", "er\u00b7ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und t\u00e4nzelt gl\u00fccklich auf gespanntem Seil.", "tokens": ["Und", "t\u00e4n\u00b7zelt", "gl\u00fcck\u00b7lich", "auf", "ge\u00b7spann\u00b7tem", "Seil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Menschen wimmeln braun wie K\u00fcchenschaben,", "tokens": ["Die", "Men\u00b7schen", "wim\u00b7meln", "braun", "wie", "K\u00fc\u00b7chen\u00b7scha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sind dem Blick der H\u00f6he wehrlos feil.", "tokens": ["Und", "sind", "dem", "Blick", "der", "H\u00f6\u00b7he", "wehr\u00b7los", "feil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort unten hockt in schmutzigen Galoschen", "tokens": ["Dort", "un\u00b7ten", "hockt", "in", "schmut\u00b7zi\u00b7gen", "Ga\u00b7lo\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Niedere und Gemeine, und es hebt", "tokens": ["Das", "Nie\u00b7de\u00b7re", "und", "Ge\u00b7mei\u00b7ne", ",", "und", "es", "hebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Stirn zur H\u00f6he f\u00fcr zwei povre Groschen,", "tokens": ["Die", "Stirn", "zur", "H\u00f6\u00b7he", "f\u00fcr", "zwei", "pov\u00b7re", "Gro\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An denen feucht der Schwei\u00df des Werktags klebt.", "tokens": ["An", "de\u00b7nen", "feucht", "der", "Schwei\u00df", "des", "Werk\u00b7tags", "klebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}