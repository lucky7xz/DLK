{"textgrid.poem.25184": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachts in stummer Kammer lag ich", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachts in stummer Kammer lag ich", "tokens": ["Nachts", "in", "stum\u00b7mer", "Kam\u00b7mer", "lag", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strebensm\u00fcd' und lebensbang;", "tokens": ["Stre\u00b7bens\u00b7m\u00fcd'", "und", "le\u00b7bens\u00b7bang", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sorgen, irr wie Flederm\u00e4use,", "tokens": ["Sor\u00b7gen", ",", "irr", "wie", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Huschten das Geb\u00e4lk entlang.", "tokens": ["Huschten", "das", "Ge\u00b7b\u00e4lk", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Geisterhaft ein ernstes Weib", "tokens": ["Geis\u00b7ter\u00b7haft", "ein", "erns\u00b7tes", "Weib"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir zum Trost am Lager wachte,", "tokens": ["Mir", "zum", "Trost", "am", "La\u00b7ger", "wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Starrte in die Kerzenflamme;", "tokens": ["Starr\u00b7te", "in", "die", "Ker\u00b7zen\u00b7flam\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da las ich, was sie dachte:", "tokens": ["Und", "da", "las", "ich", ",", "was", "sie", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbweine, bis du mit verweinten", "tokens": ["\u00bb", "wei\u00b7ne", ",", "bis", "du", "mit", "ver\u00b7wein\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Augen endlich klar erschaust,", "tokens": ["Au\u00b7gen", "end\u00b7lich", "klar", "er\u00b7schaust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die holde Welt ein Trugbild,", "tokens": ["Da\u00df", "die", "hol\u00b7de", "Welt", "ein", "Trug\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem du niemals straflos traust. \u2013", "tokens": ["Dem", "du", "nie\u00b7mals", "straf\u00b7los", "traust", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "F\u00fcllst du stattlich Schrein und Truhe,", "tokens": ["F\u00fcllst", "du", "statt\u00b7lich", "Schrein", "und", "Tru\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bleibt die Seele dennoch leer;", "tokens": ["Bleibt", "die", "See\u00b7le", "den\u00b7noch", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trinkst du von dem Trank der Ehre,", "tokens": ["Trinkst", "du", "von", "dem", "Trank", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird dich d\u00fcrsten mehr und mehr.", "tokens": ["Wird", "dich", "d\u00fcrs\u00b7ten", "mehr", "und", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VMFIN", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und nun Becher, Spiele, T\u00e4nze,", "tokens": ["Und", "nun", "Be\u00b7cher", ",", "Spie\u00b7le", ",", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Festgepr\u00e4nge durch den Saal ...", "tokens": ["Fest\u00b7ge\u00b7pr\u00e4n\u00b7ge", "durch", "den", "Saal", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mitten in dem Rausch der Freude", "tokens": ["Mit\u00b7ten", "in", "dem", "Rausch", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchltest du geheime Qual.", "tokens": ["F\u00fchl\u00b7test", "du", "ge\u00b7hei\u00b7me", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Voller Liebe und Vertrauen", "tokens": ["Vol\u00b7ler", "Lie\u00b7be", "und", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fccktest du so manche Hand;", "tokens": ["Dr\u00fcck\u00b7test", "du", "so", "man\u00b7che", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als du in das Herz gesehen,", "tokens": ["Als", "du", "in", "das", "Herz", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast du tr\u00fcbe dich gewandt.", "tokens": ["Hast", "du", "tr\u00fc\u00b7be", "dich", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Einem argen Rosenstrauche", "tokens": ["Ei\u00b7nem", "ar\u00b7gen", "Ro\u00b7sen\u00b7strau\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleicht das Leben, dornbewehrt;", "tokens": ["Gleicht", "das", "Le\u00b7ben", ",", "dorn\u00b7be\u00b7wehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt die wunde Hand die Rose,", "tokens": ["H\u00e4lt", "die", "wun\u00b7de", "Hand", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat ein Wurm den Kelch versehrt.", "tokens": ["Hat", "ein", "Wurm", "den", "Kelch", "ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Darum la\u00df die Rosen glei\u00dfen,", "tokens": ["Da\u00b7rum", "la\u00df", "die", "Ro\u00b7sen", "glei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Geh vorbei und blicke kalt!", "tokens": ["Geh", "vor\u00b7bei", "und", "bli\u00b7cke", "kalt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber solche, die entsagen,", "tokens": ["Ue\u00b7ber", "sol\u00b7che", ",", "die", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat das Leiden nicht Gewalt.\u00ab", "tokens": ["Hat", "das", "Lei\u00b7den", "nicht", "Ge\u00b7walt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}