{"textgrid.poem.65049": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O sag' mir! hast du je empfunden", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sag' mir! hast du je empfunden", "tokens": ["O", "sag'", "mir", "!", "hast", "du", "je", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie seltsam es das Herz beschleicht,", "tokens": ["Wie", "selt\u00b7sam", "es", "das", "Herz", "be\u00b7schleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn uns in seinen Sterbestunden", "tokens": ["Wenn", "uns", "in", "sei\u00b7nen", "Ster\u00b7be\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Lenz noch eine Gabe reicht? \u2013", "tokens": ["Der", "Lenz", "noch", "ei\u00b7ne", "Ga\u00b7be", "reicht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn aus der Schaar der Waldess\u00e4nger,", "tokens": ["Wenn", "aus", "der", "Schaar", "der", "Wal\u00b7des\u00b7s\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die rasch sich nach dem S\u00fcden schwingt,", "tokens": ["Die", "rasch", "sich", "nach", "dem", "S\u00fc\u00b7den", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor'm Herbste fliehend, ihrem Dr\u00e4nger,", "tokens": ["Vor'm", "Herbs\u00b7te", "flie\u00b7hend", ",", "ih\u00b7rem", "Dr\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein letztes Lied noch zu uns klingt? \u2013", "tokens": ["Ein", "letz\u00b7tes", "Lied", "noch", "zu", "uns", "klingt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn, bald dem rauhen Nord erlegen,", "tokens": ["Wenn", ",", "bald", "dem", "rau\u00b7hen", "Nord", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sie entbl\u00e4ttert und verstreut,", "tokens": ["Der", "sie", "ent\u00b7bl\u00e4t\u00b7tert", "und", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blume, wie zum Liebessegen", "tokens": ["Die", "Blu\u00b7me", ",", "wie", "zum", "Lie\u00b7bes\u00b7se\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ihren letzten Duft uns beut? \u2013", "tokens": ["Noch", "ih\u00b7ren", "letz\u00b7ten", "Duft", "uns", "beut", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So wisse: dieses Deingedenken,", "tokens": ["So", "wis\u00b7se", ":", "die\u00b7ses", "Dein\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Heil auf dich herniederruft,", "tokens": ["Das", "Heil", "auf", "dich", "her\u00b7nie\u00b7der\u00b7ruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df sanfte mich in dich Versenken,", "tokens": ["Die\u00df", "sanf\u00b7te", "mich", "in", "dich", "Ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist meines Herzens letzter Duft.", "tokens": ["Ist", "mei\u00b7nes", "Her\u00b7zens", "letz\u00b7ter", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dieses Lied, als Seelenkunde", "tokens": ["Und", "die\u00b7ses", "Lied", ",", "als", "See\u00b7len\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dir hint\u00f6nend liebevoll,", "tokens": ["Zu", "dir", "hin\u00b7t\u00f6\u00b7nend", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist ein Gru\u00df von einem Munde,", "tokens": ["Es", "ist", "ein", "Gru\u00df", "von", "ei\u00b7nem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der bald im Tod verstummen soll! \u2013", "tokens": ["Der", "bald", "im", "Tod", "ver\u00b7stum\u00b7men", "soll", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}