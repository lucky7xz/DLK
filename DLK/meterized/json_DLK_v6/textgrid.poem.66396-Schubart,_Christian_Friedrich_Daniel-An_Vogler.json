{"textgrid.poem.66396": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Vogler", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt inn' in deinem Cherubsfluge,", "tokens": ["Halt", "inn'", "in", "dei\u00b7nem", "Che\u00b7rubs\u00b7flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halt inne, du gekostester Sohn der Harmonie,", "tokens": ["Halt", "in\u00b7ne", ",", "du", "ge\u00b7kos\u00b7tes\u00b7ter", "Sohn", "der", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Orgelgeist, des ersten Tongeb\u00e4udes Beseeler,", "tokens": ["Or\u00b7gel\u00b7geist", ",", "des", "ers\u00b7ten", "Ton\u00b7ge\u00b7b\u00e4u\u00b7des", "Be\u00b7see\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Halt inn' in deinem Cherubsfluge,", "tokens": ["Halt", "inn'", "in", "dei\u00b7nem", "Che\u00b7rubs\u00b7flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich am Halse dir hang' und weine,", "tokens": ["Da\u00df", "ich", "am", "Hal\u00b7se", "dir", "hang'", "und", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ach, des Abschieds blutige Z\u00e4hre.", "tokens": ["Ach", ",", "des", "Ab\u00b7schieds", "blu\u00b7ti\u00b7ge", "Z\u00e4h\u00b7re", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Dir winkt aus seiner K\u00f6nigsburg ", "tokens": ["Dir", "winkt", "aus", "sei\u00b7ner", "K\u00f6\u00b7nigs\u00b7burg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Donnerer in Finnlands Gekl\u00fcften,", "tokens": ["Der", "Don\u00b7ne\u00b7rer", "in", "Finn\u00b7lands", "Ge\u00b7kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Fernhin winkt er dir in die Engen der Nordsee.", "tokens": ["Fern\u00b7hin", "winkt", "er", "dir", "in", "die", "En\u00b7gen", "der", "Nord\u00b7see", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Du folgst \u2013 ich aber bleibe;", "tokens": ["Du", "folgst", "\u2013", "ich", "a\u00b7ber", "blei\u00b7be", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zwar unter milderem Himmel \u2013", "tokens": ["Zwar", "un\u00b7ter", "mil\u00b7de\u00b7rem", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Doch ohne dich!", "tokens": ["Doch", "oh\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Mich rei\u00dft nicht mehr dein Orgelspiel", "tokens": ["Mich", "rei\u00dft", "nicht", "mehr", "dein", "Or\u00b7gel\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit magischer Kraft himmelan!", "tokens": ["Mit", "ma\u00b7gi\u00b7scher", "Kraft", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Da ich w\u00e4hne, zu h\u00f6ren", "tokens": ["Da", "ich", "w\u00e4h\u00b7ne", ",", "zu", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Das Get\u00f6se krystallner Meere,", "tokens": ["Das", "Ge\u00b7t\u00f6\u00b7se", "krys\u00b7tall\u00b7ner", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Und das gro\u00dfe Hallelujah,", "tokens": ["Und", "das", "gro\u00b7\u00dfe", "Hal\u00b7le\u00b7lu\u00b7jah", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Und die Donner vom Berge der Gegenwart Gottes,", "tokens": ["Und", "die", "Don\u00b7ner", "vom", "Ber\u00b7ge", "der", "Ge\u00b7gen\u00b7wart", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.19": {"text": "Und der himmlischen Harfe Lispel.", "tokens": ["Und", "der", "himm\u00b7li\u00b7schen", "Har\u00b7fe", "Lis\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ein Orgelbeherrscher bist du;", "tokens": ["Ein", "Or\u00b7gel\u00b7be\u00b7herr\u00b7scher", "bist", "du", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Doch vermagst du nicht, zu finden den Ton,", "tokens": ["Doch", "ver\u00b7magst", "du", "nicht", ",", "zu", "fin\u00b7den", "den", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Durch der Z\u00fcge Vermischung,", "tokens": ["Durch", "der", "Z\u00fc\u00b7ge", "Ver\u00b7mi\u00b7schung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der meines Schmerzens Tiefe tr\u00e4fe;", "tokens": ["Der", "mei\u00b7nes", "Schmer\u00b7zens", "Tie\u00b7fe", "tr\u00e4\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn du gehst, Bruder meines Geistes, du gehst!! \u2013", "tokens": ["Denn", "du", "gehst", ",", "Bru\u00b7der", "mei\u00b7nes", "Geis\u00b7tes", ",", "du", "gehst", "!!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "NN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Halt inn' in deinem Cherubsfluge,", "tokens": ["Halt", "inn'", "in", "dei\u00b7nem", "Che\u00b7rubs\u00b7flu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Orgelgeist, des ersten Tongeb\u00e4udes Beseeler,", "tokens": ["Or\u00b7gel\u00b7geist", ",", "des", "ers\u00b7ten", "Ton\u00b7ge\u00b7b\u00e4u\u00b7des", "Be\u00b7see\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Halt inne, da\u00df ich am Halse dir weine", "tokens": ["Halt", "in\u00b7ne", ",", "da\u00df", "ich", "am", "Hal\u00b7se", "dir", "wei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Des Abschieds blutige Z\u00e4hre.", "tokens": ["Des", "Ab\u00b7schieds", "blu\u00b7ti\u00b7ge", "Z\u00e4h\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}