{"textgrid.poem.37010": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "\u2020 Pluto-Plutus \u2020", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebst Du das Leben, Wand'rer, mu\u00dft Du danken", "tokens": ["Liebst", "Du", "das", "Le\u00b7ben", ",", "Wan\u00b7d'\u00b7rer", ",", "mu\u00dft", "Du", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Himmel f\u00fcr dies Gl\u00fcck!", "tokens": ["Dem", "Him\u00b7mel", "f\u00fcr", "dies", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ", "tokens": ["Von"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Kehrt Pluto nicht zur\u00fcck!", "tokens": ["Kehrt", "Plu\u00b7to", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}