{"textgrid.poem.63952": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu dir meine Flucht,", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu dir meine Flucht,", "tokens": ["Zu", "dir", "mei\u00b7ne", "Flucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "An deinen lindernden Busen,", "tokens": ["An", "dei\u00b7nen", "lin\u00b7dern\u00b7den", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In deine weich", "tokens": ["In", "dei\u00b7ne", "weich"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Umschlingenden Arme", "tokens": ["Um\u00b7schlin\u00b7gen\u00b7den", "Ar\u00b7me"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Rett ich mein Herz,", "tokens": ["Rett", "ich", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das prosawunde", "tokens": ["Das", "pro\u00b7sa\u00b7wun\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Qualenzuckende Herz,", "tokens": ["Qua\u00b7len\u00b7zu\u00b7cken\u00b7de", "Herz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "O du meine tr\u00f6stende Mutter,", "tokens": ["O", "du", "mei\u00b7ne", "tr\u00f6s\u00b7ten\u00b7de", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Sorgen verkosendes Lieb,", "tokens": ["Sor\u00b7gen", "ver\u00b7ko\u00b7sen\u00b7des", "Lieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.10": {"text": "O du meine Muse!", "tokens": ["O", "du", "mei\u00b7ne", "Mu\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Ruhe l\u00e4chelt dein Auge,", "tokens": ["Ru\u00b7he", "l\u00e4\u00b7chelt", "dein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Dein mildes, hehres Auge,", "tokens": ["Dein", "mil\u00b7des", ",", "heh\u00b7res", "Au\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "In meine dunklen Qualen und Sorgen,", "tokens": ["In", "mei\u00b7ne", "dunk\u00b7len", "Qua\u00b7len", "und", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Das gl\u00e4nzende tiefe,", "tokens": ["Das", "gl\u00e4n\u00b7zen\u00b7de", "tie\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Das mit olympischer Klarheit", "tokens": ["Das", "mit", "o\u00b7lym\u00b7pi\u00b7scher", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Tr\u00e4nkte den greisen Homer,", "tokens": ["Tr\u00e4nk\u00b7te", "den", "grei\u00b7sen", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.17": {"text": "Mit tragischer Milde umgo\u00df", "tokens": ["Mit", "tra\u00b7gi\u00b7scher", "Mil\u00b7de", "um\u00b7go\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "Die bruderbestattende,", "tokens": ["Die", "bru\u00b7der\u00b7be\u00b7stat\u00b7ten\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.19": {"text": "Still ins Todesbrautgemach", "tokens": ["Still", "ins", "To\u00b7des\u00b7braut\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Steigende Jungfrau ...", "tokens": ["Stei\u00b7gen\u00b7de", "Jung\u00b7frau", "..."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Muse, du w\u00f6lbtest", "tokens": ["Mu\u00b7se", ",", "du", "w\u00f6lb\u00b7test"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Den blauen Himmel von Hellas", "tokens": ["Den", "blau\u00b7en", "Him\u00b7mel", "von", "Hel\u00b7las"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Auf Marmor Plastik,", "tokens": ["Auf", "Mar\u00b7mor", "Plas\u00b7tik", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Mit blitzzerrissener,", "tokens": ["Mit", "blitz\u00b7zer\u00b7ris\u00b7se\u00b7ner", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "ADJA", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.25": {"text": "D\u00fcsterer Wolken hehrer Phantastik", "tokens": ["D\u00fcs\u00b7te\u00b7rer", "Wol\u00b7ken", "heh\u00b7rer", "Phan\u00b7tas\u00b7tik"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.26": {"text": "Bangtest du", "tokens": ["Bang\u00b7test", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.27": {"text": "Dem Jehovah heiligen Lande.", "tokens": ["Dem", "Je\u00b7ho\u00b7vah", "hei\u00b7li\u00b7gen", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Wilde Schw\u00fcle lie\u00dfest du zittern", "tokens": ["Wil\u00b7de", "Schw\u00fc\u00b7le", "lie\u00b7\u00dfest", "du", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.29": {"text": "\u00dcber dem \u00fcppigen Traubengeh\u00e4nge,", "tokens": ["\u00dc\u00b7ber", "dem", "\u00fcp\u00b7pi\u00b7gen", "Trau\u00b7ben\u00b7ge\u00b7h\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.30": {"text": "Drin der schwellende Busen der Braut", "tokens": ["Drin", "der", "schwel\u00b7len\u00b7de", "Bu\u00b7sen", "der", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.31": {"text": "Wogte unter den K\u00fcssen", "tokens": ["Wog\u00b7te", "un\u00b7ter", "den", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.32": {"text": "Des ebenholz-lockigen Freundes.", "tokens": ["Des", "e\u00b7ben\u00b7holz\u00b7lo\u00b7cki\u00b7gen", "Freun\u00b7des", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Ein Leichenfeld", "tokens": ["Ein", "Lei\u00b7chen\u00b7feld"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.34": {"text": "Sieht der begeisterte Seher,", "tokens": ["Sieht", "der", "be\u00b7geis\u00b7ter\u00b7te", "Se\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.35": {"text": "Ein Wirbelwind", "tokens": ["Ein", "Wir\u00b7bel\u00b7wind"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Dreht die Gebeine zusammen,", "tokens": ["Dreht", "die", "Ge\u00b7bei\u00b7ne", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.37": {"text": "Sie f\u00fcgen sich ein,", "tokens": ["Sie", "f\u00fc\u00b7gen", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.38": {"text": "Dasteht", "tokens": ["Das\u00b7teht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.39": {"text": "Ein totenk\u00f6pfiges Heer.", "tokens": ["Ein", "to\u00b7ten\u00b7k\u00f6p\u00b7fi\u00b7ges", "Heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.40": {"text": "Sehnen schie\u00dfen und Fleisch spro\u00dft,", "tokens": ["Seh\u00b7nen", "schie\u00b7\u00dfen", "und", "Fleisch", "spro\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.41": {"text": "Haut spannt sich herum.", "tokens": ["Haut", "spannt", "sich", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.42": {"text": "Ein Gotteshauch:", "tokens": ["Ein", "Got\u00b7tes\u00b7hauch", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "Mit den Waffen", "tokens": ["Mit", "den", "Waf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.44": {"text": "Rasselt das Heer. \u2013", "tokens": ["Ras\u00b7selt", "das", "Heer", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.45": {"text": "Dem glutenhagern", "tokens": ["Dem", "glu\u00b7ten\u00b7ha\u00b7gern"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.46": {"text": "Erschienst du", "tokens": ["Er\u00b7schienst", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.47": {"text": "Hoher d\u00fcsterer Gestalt,", "tokens": ["Ho\u00b7her", "d\u00fcs\u00b7te\u00b7rer", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Dr\u00fccktest kohlenbrennenden Ku\u00df", "tokens": ["Dr\u00fcck\u00b7test", "koh\u00b7len\u00b7bren\u00b7nen\u00b7den", "Ku\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.49": {"text": "Auf seine schroffe steinerne Stirn,", "tokens": ["Auf", "sei\u00b7ne", "schrof\u00b7fe", "stei\u00b7ner\u00b7ne", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.50": {"text": "Draus die d\u00fcstern Flammenrhythmen", "tokens": ["Draus", "die", "d\u00fcs\u00b7tern", "Flam\u00b7menr\u00b7hyth\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.52": {"text": "Das hei\u00dfe Angstgebet,", "tokens": ["Das", "hei\u00b7\u00dfe", "Angst\u00b7ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Das Flammenflehen:", "tokens": ["Das", "Flam\u00b7men\u00b7fle\u00b7hen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "O, entsende auch mich!", "tokens": ["O", ",", "ent\u00b7sen\u00b7de", "auch", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "La\u00df mich nicht stehn", "tokens": ["La\u00df", "mich", "nicht", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Im Alltagsgrau,", "tokens": ["Im", "All\u00b7tags\u00b7grau", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und Neidesblicke", "tokens": ["Und", "Nei\u00b7des\u00b7bli\u00b7cke"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Werfen auf die Erkorenen,", "tokens": ["Wer\u00b7fen", "auf", "die", "Er\u00b7ko\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Gedr\u00fcckt durch niedere Prosa,", "tokens": ["Ge\u00b7dr\u00fcckt", "durch", "nie\u00b7de\u00b7re", "Pro\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gequ\u00e4lt von den Stichen", "tokens": ["Ge\u00b7qu\u00e4lt", "von", "den", "Sti\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Des kleinlichen Lebens,", "tokens": ["Des", "klein\u00b7li\u00b7chen", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der Philister Umgebung,", "tokens": ["Der", "Phi\u00b7lis\u00b7ter", "Um\u00b7ge\u00b7bung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Philisterhaft", "tokens": ["Phi\u00b7lis\u00b7ter\u00b7haft"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Die Pfennige zu rechnen gezwungen.", "tokens": ["Die", "Pfen\u00b7ni\u00b7ge", "zu", "rech\u00b7nen", "ge\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Nein Muse, so grausam", "tokens": ["Nein", "Mu\u00b7se", ",", "so", "grau\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKANT", "NN", "$,", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Kannst du nicht sein,", "tokens": ["Kannst", "du", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Mich hocken nicht lassen", "tokens": ["Mich", "ho\u00b7cken", "nicht", "las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Auf dumpfem Bureau,", "tokens": ["Auf", "dum\u00b7pfem", "Bu\u00b7re\u00b7au", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Angewidert von Allem,", "tokens": ["An\u00b7ge\u00b7wi\u00b7dert", "von", "Al\u00b7lem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIS", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.17": {"text": "Verh\u00f6hnt von Allen!", "tokens": ["Ver\u00b7h\u00f6hnt", "von", "Al\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Mit selbstzerfressendem Grimm,", "tokens": ["Mit", 