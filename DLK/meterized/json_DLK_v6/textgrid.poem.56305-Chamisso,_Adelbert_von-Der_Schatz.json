{"textgrid.poem.56305": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatz", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fernher aus geheimem Schreine", "tokens": ["Fern\u00b7her", "aus", "ge\u00b7hei\u00b7mem", "Schrei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winkt ein Schatz so wunderbar;", "tokens": ["Winkt", "ein", "Schatz", "so", "wun\u00b7der\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df allein nur, wen er meine,", "tokens": ["Wei\u00df", "al\u00b7lein", "nur", ",", "wen", "er", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Ort, wo er bewahrt.", "tokens": ["Und", "den", "Ort", ",", "wo", "er", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wir streben, und wir meinen,", "tokens": ["Und", "wir", "stre\u00b7ben", ",", "und", "wir", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Streben, meinen immerdar,", "tokens": ["Stre\u00b7ben", ",", "mei\u00b7nen", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schweifen durch des Lebens Weite,", "tokens": ["Schwei\u00b7fen", "durch", "des", "Le\u00b7bens", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und verachten die Gefahr;", "tokens": ["Und", "ver\u00b7ach\u00b7ten", "die", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wir begehren nur das Eine,", "tokens": ["Wir", "be\u00b7geh\u00b7ren", "nur", "das", "Ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wir begehren immerdar;", "tokens": ["Wir", "be\u00b7geh\u00b7ren", "im\u00b7mer\u00b7dar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Immerdar auch will's erscheinen,", "tokens": ["Im\u00b7mer\u00b7dar", "auch", "will's", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ach verschwinden immerdar.", "tokens": ["Ach", "ver\u00b7schwin\u00b7den", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}