{"textgrid.poem.60781": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nie sehnt ein willk\u00fcr\u00fcbender Herrscher sich", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie sehnt ein willk\u00fcr\u00fcbender Herrscher sich", "tokens": ["Nie", "sehnt", "ein", "will\u00b7k\u00fc\u00b7r\u00fc\u00b7ben\u00b7der", "Herr\u00b7scher", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach Dichterweihrauch, dessen er nicht bedarf:", "tokens": ["Nach", "Dich\u00b7ter\u00b7weih\u00b7rauch", ",", "des\u00b7sen", "er", "nicht", "be\u00b7darf", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELAT", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Er legt ans Schwert kraftvoll die Faust und", "tokens": ["Er", "legt", "ans", "Schwert", "kraft\u00b7voll", "die", "Faust", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJD", "ART", "NN", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wen er zum Opfer sich w\u00e4hlt und wer ihm", "tokens": ["Wen", "er", "zum", "Op\u00b7fer", "sich", "w\u00e4hlt", "und", "wer", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "KON", "PWS", "PPER"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mi\u00dff\u00e4llt und wer Freiheit zu verk\u00fcnden wagt,", "tokens": ["Mi\u00df\u00b7f\u00e4llt", "und", "wer", "Frei\u00b7heit", "zu", "ver\u00b7k\u00fcn\u00b7den", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PWS", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den trifft der Tod, den decken Sibiriens", "tokens": ["Den", "trifft", "der", "Tod", ",", "den", "de\u00b7cken", "Si\u00b7bi\u00b7ri\u00b7ens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schneefelder zu, der wird geschmiedet,", "tokens": ["Schnee\u00b7fel\u00b7der", "zu", ",", "der", "wird", "ge\u00b7schmie\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Tief in der Grotte des Felseneilands,", "tokens": ["Tief", "in", "der", "Grot\u00b7te", "des", "Fel\u00b7sen\u00b7ei\u00b7lands", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Titanenhaft auf eisernen Rost, zu dem", "tokens": ["Ti\u00b7ta\u00b7nen\u00b7haft", "auf", "ei\u00b7ser\u00b7nen", "Rost", ",", "zu", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "APPR", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Meer emporschl\u00e4gt. Aber das Volk bedarf,", "tokens": ["Das", "Meer", "em\u00b7por\u00b7schl\u00e4gt", ".", "A\u00b7ber", "das", "Volk", "be\u00b7darf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ohnm\u00e4chtig schmerzvoll, eines Mannes,", "tokens": ["Ohn\u00b7m\u00e4ch\u00b7tig", "schmerz\u00b7voll", ",", "ei\u00b7nes", "Man\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welcher im Lied es empfiehlt der Nachwelt", "tokens": ["Wel\u00b7cher", "im", "Lied", "es", "emp\u00b7fiehlt", "der", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "APPRART", "NN", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Als Stoff des Mitleids, welcher erz\u00e4hlt, wie schnell", "tokens": ["Als", "Stoff", "des", "Mit\u00b7leids", ",", "wel\u00b7cher", "er\u00b7z\u00e4hlt", ",", "wie", "schnell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PWAV", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zusagen wehn aus f\u00fcrstlichem Mund, und ach!", "tokens": ["Zu\u00b7sa\u00b7gen", "wehn", "aus", "f\u00fcrst\u00b7li\u00b7chem", "Mund", ",", "und", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "XY", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gleichschnell verweht sind, wie man Schw\u00fcre", "tokens": ["Gleich\u00b7schnell", "ver\u00b7weht", "sind", ",", "wie", "man", "Schw\u00fc\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "VAFIN", "$,", "PWAV", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bricht in der N\u00e4he des Pols und s\u00fcdw\u00e4rts!", "tokens": ["Bricht", "in", "der", "N\u00e4\u00b7he", "des", "Pols", "und", "s\u00fcd\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Sind Schw\u00fcre nicht (leicht l\u00f6st sie der Papst) ein Spiel", "tokens": ["Sind", "Schw\u00fc\u00b7re", "nicht", "(", "leicht", "l\u00f6st", "sie", "der", "Papst", ")", "ein", "Spiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "PTKNEG", "$(", "ADJD", "VVFIN", "PPER", "ART", "NN", "$(", "ART", "NN"], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Herzloser Bourbons? Nichtigem, falschem Eid,", "tokens": ["Herz\u00b7lo\u00b7ser", "Bour\u00b7bons", "?", "Nich\u00b7ti\u00b7gem", ",", "fal\u00b7schem", "Eid", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ach, lauschte Frankreich; lauschte Spanien,", "tokens": ["Ach", ",", "lauschte", "Fran\u00b7kreich", ";", "lauschte", "Spa\u00b7ni\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "NE", "$.", "VVFIN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Lauschte das Land um Messinas Pharus,", "tokens": ["Lauschte", "das", "Land", "um", "Mes\u00b7si\u00b7nas", "Pha\u00b7rus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Diesseits und jenseits! Einen erblickten wir,", "tokens": ["Dies\u00b7seits", "und", "jen\u00b7seits", "!", "Ei\u00b7nen", "er\u00b7blick\u00b7ten", "wir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$.", "ART", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der seines Zwingherrn blutige Hand gek\u00fc\u00dft,", "tokens": ["Der", "sei\u00b7nes", "Zwing\u00b7herrn", "blu\u00b7ti\u00b7ge", "Hand", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nachdem umsonst sein Volk des Wagens", "tokens": ["Nach\u00b7dem", "um\u00b7sonst", "sein", "Volk", "des", "Wa\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stricke zerhaun, den geliebten K\u00f6nig", "tokens": ["Stri\u00b7cke", "zer\u00b7haun", ",", "den", "ge\u00b7lieb\u00b7ten", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Nicht lassen wollend. Jener entwich, da focht's", "tokens": ["Nicht", "las\u00b7sen", "wol\u00b7lend", ".", "Je\u00b7ner", "ent\u00b7wich", ",", "da", "focht's"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVINF", "VVPP", "$.", "PDAT", "ADJD", "$,", "KOUS", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sechs Jahr um ihn, sechs Jahre, befreit zuletzt", "tokens": ["Sechs", "Jahr", "um", "ihn", ",", "sechs", "Jah\u00b7re", ",", "be\u00b7freit", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["CARD", "NN", "APPR", "PPER", "$,", "CARD", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihn aus der Haft. Er kommt und liefert", "tokens": ["Ihn", "aus", "der", "Haft", ".", "Er", "kommt", "und", "lie\u00b7fert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seine Befreier dem Blutger\u00fcst aus.", "tokens": ["Sei\u00b7ne", "Be\u00b7frei\u00b7er", "dem", "Blut\u00b7ge\u00b7r\u00fcst", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "War solches Undanks f\u00e4hig ein Nero selbst?", "tokens": ["War", "sol\u00b7ches", "Un\u00b7danks", "f\u00e4\u00b7hig", "ein", "Ne\u00b7ro", "selbst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "ART", "NE", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem, der f\u00fcr ihn sich opferte, mindestens", "tokens": ["Dem", ",", "der", "f\u00fcr", "ihn", "sich", "op\u00b7fer\u00b7te", ",", "min\u00b7des\u00b7tens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPER", "PRF", "VVFIN", "$,", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Dem Strang des Henkers ihn entr\u00fcckend,", "tokens": ["Dem", "Strang", "des", "Hen\u00b7kers", "ihn", "ent\u00b7r\u00fc\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00e4tt er ein r\u00fchmliches Grab geg\u00f6nnt ihm!", "tokens": ["H\u00e4tt", "er", "ein", "r\u00fchm\u00b7li\u00b7ches", "Grab", "ge\u00b7g\u00f6nnt", "ihm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Ihr f\u00fcrchtet nichts, Tyrannen, allein den Tod", "tokens": ["Ihr", "f\u00fcrch\u00b7tet", "nichts", ",", "Ty\u00b7ran\u00b7nen", ",", "al\u00b7lein", "den", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch f\u00fcrchtet ihr, der kein Diadem verschont:", "tokens": ["Doch", "f\u00fcrch\u00b7tet", "ihr", ",", "der", "kein", "Di\u00b7a\u00b7dem", "ver\u00b7schont", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So m\u00f6ge denn ums Sterbelager", "tokens": ["So", "m\u00f6\u00b7ge", "denn", "ums", "Ster\u00b7be\u00b7la\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dr\u00e4ngen sich euch der verha\u00dfte Chorus", "tokens": ["Dr\u00e4n\u00b7gen", "sich", "euch", "der", "ver\u00b7ha\u00df\u00b7te", "Cho\u00b7rus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "All derer, die dumpfbr\u00fctende Kerkerluft", "tokens": ["All", "de\u00b7rer", ",", "die", "dumpf\u00b7br\u00fc\u00b7ten\u00b7de", "Ker\u00b7ker\u00b7luft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PDS", "$,", "ART", "ADJA", "NN"], "meter": "-----+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fr\u00fchzeitig wegrafft, all der Gequ\u00e4lten Geist,", "tokens": ["Fr\u00fch\u00b7zei\u00b7tig", "weg\u00b7rafft", ",", "all", "der", "Ge\u00b7qu\u00e4l\u00b7ten", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Die auf Galeeren euch, mit M\u00f6rdern", "tokens": ["Die", "auf", "Ga\u00b7lee\u00b7ren", "euch", ",", "mit", "M\u00f6r\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "NN", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eng aneinander gekoppelt, fluchen,", "tokens": ["Eng", "an\u00b7ein\u00b7an\u00b7der", "ge\u00b7kop\u00b7pelt", ",", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$,", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "All derer, die, weit \u00fcber die Welt zerstreut,", "tokens": ["All", "de\u00b7rer", ",", "die", ",", "weit", "\u00fc\u00b7ber", "die", "Welt", "zer\u00b7streut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "$,", "PRELS", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "----+---+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Vom Bild der Heimat ihre Gem\u00fcter voll,", "tokens": ["Vom", "Bild", "der", "Hei\u00b7mat", "ih\u00b7re", "Ge\u00b7m\u00fc\u00b7ter", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An fremder T\u00fcr ihr Brot erbetteln,", "tokens": ["An", "frem\u00b7der", "T\u00fcr", "ihr", "Brot", "er\u00b7bet\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja, zu Barbaren verbannt, des Moslems", "tokens": ["Ja", ",", "zu", "Bar\u00b7ba\u00b7ren", "ver\u00b7bannt", ",", "des", "Mos\u00b7lems"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "NN", "VVPP", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Mildt\u00e4tigkeit anflehen! Um euer Bett", "tokens": ["Mild\u00b7t\u00e4\u00b7tig\u00b7keit", "an\u00b7fle\u00b7hen", "!", "Um", "eu\u00b7er", "Bett"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "$.", "KOUI", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wird manch Gespenst mit drohendem Finger stehn,", "tokens": ["Wird", "manch", "Ge\u00b7spenst", "mit", "dro\u00b7hen\u00b7dem", "Fin\u00b7ger", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch Kettenl\u00e4rm euch weckend, oder", "tokens": ["Durch", "Ket\u00b7ten\u00b7l\u00e4rm", "euch", "we\u00b7ckend", ",", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "PPER", "ADJD", "$,", "KON"], "me