{"textgrid.poem.37667": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich ein sinniger Knecht des Herrn die Welt mir erschauet,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich ein sinniger Knecht des Herrn die Welt mir erschauet,", "tokens": ["Als", "ich", "ein", "sin\u00b7ni\u00b7ger", "Knecht", "des", "Herrn", "die", "Welt", "mir", "er\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lag mir in mystischem Duft, tief am Altare das Herz;", "tokens": ["Lag", "mir", "in", "mys\u00b7ti\u00b7schem", "Duft", ",", "tief", "am", "Al\u00b7ta\u00b7re", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch ich sah sich entz\u00fcnden zwei Kerzen, doch erst nur die eine,", "tokens": ["Doch", "ich", "sah", "sich", "ent\u00b7z\u00fcn\u00b7den", "zwei", "Ker\u00b7zen", ",", "doch", "erst", "nur", "die", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "VVFIN", "CARD", "NN", "$,", "ADV", "ADV", "ADV", "ART", "ART", "$,"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Streifig sie leuchtete durch, in Jerusalems Nacht.", "tokens": ["Strei\u00b7fig", "sie", "leuch\u00b7te\u00b7te", "durch", ",", "in", "Je\u00b7ru\u00b7sa\u00b7lems", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "PTKVZ", "$,", "APPR", "NE", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und ich sahe die St\u00e4ubchen durchz\u00fcckt von Freude im Hirne,", "tokens": ["Und", "ich", "sa\u00b7he", "die", "St\u00e4ub\u00b7chen", "durch\u00b7z\u00fcckt", "von", "Freu\u00b7de", "im", "Hir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVPP", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Und ich beschrieb es so gern, wie mir der Staub noch so lieb.", "tokens": ["Und", "ich", "be\u00b7schrieb", "es", "so", "gern", ",", "wie", "mir", "der", "Staub", "noch", "so", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Doch da haben die Ritter der Wallfahrt gar h\u00f6hnend gelachet,", "tokens": ["Doch", "da", "ha\u00b7ben", "die", "Rit\u00b7ter", "der", "Wall\u00b7fahrt", "gar", "h\u00f6h\u00b7nend", "ge\u00b7la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.8": {"text": "Und ihr Lachen verdarb's, wirbelnd den ewigen Staub.", "tokens": ["Und", "ihr", "La\u00b7chen", "verd\u00b7a\u00b7rb's", ",", "wir\u00b7belnd", "den", "e\u00b7wi\u00b7gen", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "$,", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "Traurig dacht' ich zu sinken nun wieder in's mystische Dunkel,", "tokens": ["Trau\u00b7rig", "dacht'", "ich", "zu", "sin\u00b7ken", "nun", "wie\u00b7der", "in's", "mys\u00b7ti\u00b7sche", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKZU", "VVINF", "ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Denn die Flamme, sie war ohne den Staub mir nicht lieb;", "tokens": ["Denn", "die", "Flam\u00b7me", ",", "sie", "war", "oh\u00b7ne", "den", "Staub", "mir", "nicht", "lieb", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Aber da z\u00fcndet sich helle die Flamme der anderen Seite,", "tokens": ["A\u00b7ber", "da", "z\u00fcn\u00b7det", "sich", "hel\u00b7le", "die", "Flam\u00b7me", "der", "an\u00b7de\u00b7ren", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Blumen, die sah ich verwelkt, Knochen vom Opfer des Thiers,", "tokens": ["Blu\u00b7men", ",", "die", "sah", "ich", "ver\u00b7welkt", ",", "Kno\u00b7chen", "vom", "Op\u00b7fer", "des", "Thiers", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "PPER", "VVPP", "$,", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.13": {"text": "Und die Ritter, sie schwanden wie leere Gestalten des Zwielichts", "tokens": ["Und", "die", "Rit\u00b7ter", ",", "sie", "schwan\u00b7den", "wie", "lee\u00b7re", "Ge\u00b7stal\u00b7ten", "des", "Zwie\u00b7lichts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "KOKOM", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+--+-+-+", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Und das heilige Grab war von Pilgern so leer.", "tokens": ["Und", "das", "hei\u00b7li\u00b7ge", "Grab", "war", "von", "Pil\u00b7gern", "so", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.15": {"text": "Doch da rief mir's von oben, das Oben das war nun erleuchtet,", "tokens": ["Doch", "da", "rief", "mir's", "von", "o\u00b7ben", ",", "das", "O\u00b7ben", "das", "war", "nun", "er\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "NE", "APPR", "ADV", "$,", "PRELS", "ADV", "PDS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Jene, beschreib ich sie wem, der sie nicht selber gesehn.", "tokens": ["Je\u00b7ne", ",", "be\u00b7schreib", "ich", "sie", "wem", ",", "der", "sie", "nicht", "sel\u00b7ber", "ge\u00b7sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "PPER", "PIS", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.17": {"text": "\u00bblasse dem Staub, was des Staubes, und nehme was Eins und verbunden,", "tokens": ["\u00bb", "las\u00b7se", "dem", "Staub", ",", "was", "des", "Stau\u00b7bes", ",", "und", "neh\u00b7me", "was", "Eins", "und", "ver\u00b7bun\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "$,", "KON", "VVFIN", "PIS", "NN", "KON", "VVPP", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Dr\u00fccke an's h\u00f6here Herz Blumen und Thiere mit Lust;", "tokens": ["Dr\u00fc\u00b7cke", "an's", "h\u00f6\u00b7he\u00b7re", "Herz", "Blu\u00b7men", "und", "Thie\u00b7re", "mit", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Staub ist opfernder Duft, du darfst nicht jenen entziehen,", "tokens": ["Staub", "ist", "op\u00b7fern\u00b7der", "Duft", ",", "du", "darfst", "nicht", "je\u00b7nen", "ent\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJA", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "PDS", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Was da heilet und hilft, frisch ist dein sinniger Sinn.", "tokens": ["Was", "da", "hei\u00b7let", "und", "hilft", ",", "frisch", "ist", "dein", "sin\u00b7ni\u00b7ger", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Trete mit r\u00fcstigem Fu\u00df, ich zog dich aus W\u00fcsten zur N\u00e4he,", "tokens": ["Tre\u00b7te", "mit", "r\u00fcs\u00b7ti\u00b7gem", "Fu\u00df", ",", "ich", "zog", "dich", "aus", "W\u00fcs\u00b7ten", "zur", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Ziehe zu W\u00fcsten nun hin, denn du wei\u00dft was mir lieb.", "tokens": ["Zie\u00b7he", "zu", "W\u00fcs\u00b7ten", "nun", "hin", ",", "denn", "du", "wei\u00dft", "was", "mir", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PWS", "PPER", "ADJD", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.23": {"text": "Suche mir Blumen die frisch, auf mehre sie glaubend an Dauer,", "tokens": ["Su\u00b7che", "mir", "Blu\u00b7men", "die", "frisch", ",", "auf", "meh\u00b7re", "sie", "glau\u00b7bend", "an", "Dau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "ADJD", "$,", "APPR", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.24": {"text": "F\u00fchre die Heerden dazu, trete im Staube danach.", "tokens": ["F\u00fch\u00b7re", "die", "Heer\u00b7den", "da\u00b7zu", ",", "tre\u00b7te", "im", "Stau\u00b7be", "da\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PAV", "$,", "VVFIN", "APPRART", "NN", "PAV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Abends siehst du im Staub, scharf scharrend, den lieblichen Funken,", "tokens": ["A\u00b7bends", "siehst", "du", "im", "Staub", ",", "scharf", "schar\u00b7rend", ",", "den", "lieb\u00b7li\u00b7chen", "Fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "VVFIN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "An den N\u00e4geln des Schuh's, wie ein versunkener Stern,", "tokens": ["An", "den", "N\u00e4\u00b7geln", "des", "Schuh's", ",", "wie", "ein", "ver\u00b7sun\u00b7ke\u00b7ner", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Wenn du auch diesen gesehn, so schlag ihn in's offene Auge,", "tokens": ["Wenn", "du", "auch", "die\u00b7sen", "ge\u00b7sehn", ",", "so", "schlag", "ihn", "in's", "of\u00b7fe\u00b7ne", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "VVPP", "$,", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "So nur f\u00e4hrt er in dich, da\u00df du ihn bringest mit dir,", "tokens": ["So", "nur", "f\u00e4hrt", "er", "in", "dich", ",", "da\u00df", "du", "ihn", "brin\u00b7gest", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Treibe die Heerden zusammen, und schneide die Blumen, du kennst sie,", "tokens": ["Trei\u00b7be", "die", "Heer\u00b7den", "zu\u00b7sam\u00b7men", ",", "und", "schnei\u00b7de", "die", "Blu\u00b7men", ",", "du", "kennst", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Lasse die Wurzel zur\u00fcck, von den Thieren ein Paar.", "tokens": ["Las\u00b7se", "die", "Wur\u00b7zel", "zu\u00b7r\u00fcck", ",", "von", "den", "Thie\u00b7ren", "ein", "Paar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.31": {"text": "Alle die andern treibe zu mir, mit sicheren Schritten", "tokens": ["Al\u00b7le", "die", "an\u00b7dern", "trei\u00b7be", "zu", "mir", ",", "mit", "si\u00b7che\u00b7ren", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "VVFIN", "APPR", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+---+-", "measure": "iambic.hexa.invert"}, "line.32": {"text": "Putze die Lichter heraus, z\u00fcnde sie wieder am Aug'.", "tokens": ["Put\u00b7ze", "die", "Lich\u00b7ter", "he\u00b7raus", ",", "z\u00fcn\u00b7de", "sie", "wie\u00b7der", "am", "Aug'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.33": {"text": "Wie sie dann heller und heller und heller und heller sich z\u00fcnden,", "tokens": ["Wie", "sie", "dann", "hel\u00b7ler", "und", "hel\u00b7ler", "und", "hel\u00b7ler", "und", "hel\u00b7ler", "sich", "z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Hell dann wirst du mich sehn, freudig der Blumen, des Thiers.", "tokens": ["Hell", "dann", "wirst", "du", "mich", "sehn", ",", "freu\u00b7dig", "der", "Blu\u00b7men", ",", "des", "Thiers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,", "ADJD", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Dann erst singe von mir, da\u00df ersch\u00f6pfet dein Athem versinke,", "tokens": ["Dann", "erst", "sin\u00b7ge", "von", "mir", ",", "da\u00df", "er\u00b7sch\u00f6p\u00b7fet", "dein", "A\u00b7them", "ver\u00b7sin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "$,", "KOUS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Denn die Sch\u00f6pfung beginnt, wo der Sch\u00f6pfer verschwind't.\u00ab", "tokens": ["Denn", "die", "Sch\u00f6p\u00b7fung", "be\u00b7ginnt", ",", "wo", "der", "Sch\u00f6p\u00b7fer", "ver\u00b7schwind'", "t.", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "wor