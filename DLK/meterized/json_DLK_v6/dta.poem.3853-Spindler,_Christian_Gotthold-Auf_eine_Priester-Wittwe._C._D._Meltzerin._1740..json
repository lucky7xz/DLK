{"dta.poem.3853": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Priester-Wittwe.  \n  C. D. Meltzerin.  1740.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Geh nun, gepr\u00fcfftes Priester-Weib,", "tokens": ["Geh", "nun", ",", "ge\u00b7pr\u00fcff\u00b7tes", "Pries\u00b7ter\u00b7Weib", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh zu dem Chor der Cherubinen.", "tokens": ["Geh", "zu", "dem", "Chor", "der", "Che\u00b7ru\u00b7bi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begraben wir den matten Leib,", "tokens": ["Be\u00b7gra\u00b7ben", "wir", "den", "mat\u00b7ten", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So soll dein Nachruhm allzeit gr\u00fcnen.", "tokens": ["So", "soll", "dein", "Nach\u00b7ruhm", "all\u00b7zeit", "gr\u00fc\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du Spiegel frommer Priester-Frauen,", "tokens": ["Du", "Spie\u00b7gel", "from\u00b7mer", "Pries\u00b7ter\u00b7Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir wollen Dir dein Grabmahl bauen,", "tokens": ["Wir", "wol\u00b7len", "Dir", "dein", "Grab\u00b7mahl", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und setzen diese Grab-Schrifft hin:", "tokens": ["Und", "set\u00b7zen", "die\u00b7se", "Grab\u00b7Schrifft", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hier liegt eine Priester-Wittwe, die gepr\u00fcfte", "tokens": ["Hier", "liegt", "ei\u00b7ne", "Pries\u00b7ter\u00b7Witt\u00b7we", ",", "die", "ge\u00b7pr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Meltzerin.", "tokens": ["Melt\u00b7ze\u00b7rin", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}