{"textgrid.poem.28363": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zweifligunda gieng zur Beicht,", "tokens": ["Zweif\u00b7li\u00b7gun\u00b7da", "gieng", "zur", "Beicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im trauren gleich vielleicht,", "tokens": ["Und", "im", "trau\u00b7ren", "gleich", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der Pfarr fragt ohngefehr,", "tokens": ["Als", "der", "Pfarr", "fragt", "ohn\u00b7ge\u00b7fehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob sie eine Jungfer w\u00e4r,", "tokens": ["Ob", "sie", "ei\u00b7ne", "Jung\u00b7fer", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprach sie: Ja, ich armes Kind,", "tokens": ["Sprach", "sie", ":", "Ja", ",", "ich", "ar\u00b7mes", "Kind", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PTKANT", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber wie sie heuer sind.", "tokens": ["A\u00b7ber", "wie", "sie", "heu\u00b7er", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}