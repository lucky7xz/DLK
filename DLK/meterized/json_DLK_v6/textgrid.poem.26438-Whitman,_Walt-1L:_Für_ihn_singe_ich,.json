{"textgrid.poem.26438": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcr ihn singe ich,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr ihn singe ich,", "tokens": ["F\u00fcr", "ihn", "sin\u00b7ge", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Errichte das Gegenw\u00e4rtige auf dem Vergangenen,", "tokens": ["Er\u00b7rich\u00b7te", "das", "Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7ge", "auf", "dem", "Ver\u00b7gan\u00b7ge\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "(gleich einem perennierenden Baum, aus seiner Wurzel das Jetzt aus dem Einst)", "tokens": ["(", "gleich", "ei\u00b7nem", "pe\u00b7ren\u00b7nie\u00b7ren\u00b7den", "Baum", ",", "aus", "sei\u00b7ner", "Wur\u00b7zel", "das", "Jetzt", "aus", "dem", "Einst", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "ART", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Erweitere ihn durch Zeit und Raum und verschmelze die unsterblichen Gesetze,", "tokens": ["Er\u00b7wei\u00b7te\u00b7re", "ihn", "durch", "Zeit", "und", "Raum", "und", "ver\u00b7schmel\u00b7ze", "die", "uns\u00b7terb\u00b7li\u00b7chen", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Damit durch sie er selber werde sein eigenes Gesetz!", "tokens": ["Da\u00b7mit", "durch", "sie", "er", "sel\u00b7ber", "wer\u00b7de", "sein", "ei\u00b7ge\u00b7nes", "Ge\u00b7setz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "PPER", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}