{"textgrid.poem.25721": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Singst du ewig nur von ", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singst du ewig nur von ", "tokens": ["Singst", "du", "e\u00b7wig", "nur", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zwar ein Thor kann den Gesang", "tokens": ["Zwar", "ein", "Thor", "kann", "den", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keuscher Liebe, nur verdammen,", "tokens": ["Keu\u00b7scher", "Lie\u00b7be", ",", "nur", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die des Weisen Herz durchdrang;", "tokens": ["Die", "des", "Wei\u00b7sen", "Herz", "durch\u00b7drang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber Lieb' und Dichterglut", "tokens": ["A\u00b7ber", "Lieb'", "und", "Dich\u00b7ter\u00b7glut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind so selten ", "tokens": ["Sind", "so", "sel\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Dennoch, o du Afterbrut!", "tokens": ["Den\u00b7noch", ",", "o", "du", "Af\u00b7ter\u00b7brut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Singst du ewig nur von Flammen?", "tokens": ["Singst", "du", "e\u00b7wig", "nur", "von", "Flam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Soll der Bach nur immer ", "tokens": ["Soll", "der", "Bach", "nur", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADV"], "meter": "+-+-++", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Nennst du das, du blinder Thor,", "tokens": ["Nennst", "du", "das", ",", "du", "blin\u00b7der", "Thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Leise die Natur belauschen,", "tokens": ["Lei\u00b7se", "die", "Na\u00b7tur", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und durchschauen ihren Flor?", "tokens": ["Und", "durch\u00b7schau\u00b7en", "ih\u00b7ren", "Flor", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Eurer Tausend w\u00fcrd' ich gern", "tokens": ["Eu\u00b7rer", "Tau\u00b7send", "w\u00fcrd'", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Gegen Einen ", "tokens": ["Ge\u00b7gen", "Ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Dennoch, malerische Herrn!", "tokens": ["Den\u00b7noch", ",", "ma\u00b7le\u00b7ri\u00b7sche", "Herrn", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Soll der Bach nur immer rauschen?", "tokens": ["Soll", "der", "Bach", "nur", "im\u00b7mer", "rau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "In des wahren Dichters K\u00f6cher", "tokens": ["In", "des", "wah\u00b7ren", "Dich\u00b7ters", "K\u00f6\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ruhn der goldnen Pfeile viel!", "tokens": ["Ruhn", "der", "gold\u00b7nen", "Pfei\u00b7le", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Einer sey der Unschuld R\u00e4cher,", "tokens": ["Ei\u00b7ner", "sey", "der", "Un\u00b7schuld", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Des Tyrannen Herz sein Ziel!", "tokens": ["Des", "Ty\u00b7ran\u00b7nen", "Herz", "sein", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Schlag den Takt, du leichter Pfeil,", "tokens": ["Schlag", "den", "Takt", ",", "du", "leich\u00b7ter", "Pfeil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Zu dem Klange froher Becher,", "tokens": ["Zu", "dem", "Klan\u00b7ge", "fro\u00b7her", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Auf des Narren Haupt! Und Heil", "tokens": ["Auf", "des", "Nar\u00b7ren", "Haupt", "!", "Und", "Heil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$.", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Sey mit ", "tokens": ["Sey", "mit"], "token_info": ["word", "word"], "pos": ["VAFIN", "APPR"], "meter": "-+", "measure": "iambic.single"}}}}}