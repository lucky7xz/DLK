{"textgrid.poem.34175": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Ungar und seine Anspr\u00fcche an Deutschland", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine B\u00fcrgerkrone! Ich rettete einen der B\u00fcrger!", "tokens": ["Ei\u00b7ne", "B\u00fcr\u00b7ger\u00b7kro\u00b7ne", "!", "Ich", "ret\u00b7te\u00b7te", "ei\u00b7nen", "der", "B\u00fcr\u00b7ger", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "ART", "ART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Rief der r\u00f6mische Narr, als er der Tiber entsprang.", "tokens": ["Rief", "der", "r\u00f6\u00b7mi\u00b7sche", "Narr", ",", "als", "er", "der", "Ti\u00b7ber", "ent\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NE", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Einen Kranz, Europa! Ich habe den T\u00fcrken bestanden!", "tokens": ["Ei\u00b7nen", "Kranz", ",", "Eu\u00b7ro\u00b7pa", "!", "Ich", "ha\u00b7be", "den", "T\u00fcr\u00b7ken", "be\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Ruft der Ungar, und doch lebt er nur, weil er es that!", "tokens": ["Ruft", "der", "Un\u00b7gar", ",", "und", "doch", "lebt", "er", "nur", ",", "weil", "er", "es", "that", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}