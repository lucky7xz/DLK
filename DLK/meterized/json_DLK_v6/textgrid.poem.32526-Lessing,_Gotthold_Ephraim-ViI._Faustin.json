{"textgrid.poem.32526": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "ViI. Faustin", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Faustin, der ganze funfzehn Jahr", "tokens": ["Faus\u00b7tin", ",", "der", "gan\u00b7ze", "funf\u00b7zehn", "Jahr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "ADJA", "CARD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Entfernt von Haus und Hof und Weib und Kindern war,", "tokens": ["Ent\u00b7fernt", "von", "Haus", "und", "Hof", "und", "Weib", "und", "Kin\u00b7dern", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ward, von dem Wucher reich gemacht,", "tokens": ["Ward", ",", "von", "dem", "Wu\u00b7cher", "reich", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seinem Schiffe heimgebracht.", "tokens": ["Auf", "sei\u00b7nem", "Schif\u00b7fe", "heim\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbgott, seufzt der redliche Faustin,", "tokens": ["\u00bb", "gott", ",", "seufzt", "der", "red\u00b7li\u00b7che", "Faus\u00b7tin", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Als ihm die Vaterstadt in dunkler Fern erschien,", "tokens": ["Als", "ihm", "die", "Va\u00b7ter\u00b7stadt", "in", "dunk\u00b7ler", "Fern", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gott, strafe mich nicht meiner S\u00fcnden,", "tokens": ["Gott", ",", "stra\u00b7fe", "mich", "nicht", "mei\u00b7ner", "S\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und gib mir nicht verdienten Lohn!", "tokens": ["Und", "gib", "mir", "nicht", "ver\u00b7dien\u00b7ten", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "La\u00df, weil du gn\u00e4dig bist, mich Tochter, Weib und Sohn", "tokens": ["La\u00df", ",", "weil", "du", "gn\u00e4\u00b7dig", "bist", ",", "mich", "Toch\u00b7ter", ",", "Weib", "und", "Sohn"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gesund und fr\u00f6hlich wieder finden.\u00ab", "tokens": ["Ge\u00b7sund", "und", "fr\u00f6h\u00b7lich", "wie\u00b7der", "fin\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "So seufzt Faustin, und Gott erh\u00f6rt den S\u00fcnder.", "tokens": ["So", "seufzt", "Faus\u00b7tin", ",", "und", "Gott", "er\u00b7h\u00f6rt", "den", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er kam, und fand sein Haus in \u00dcberflu\u00df und Ruh.", "tokens": ["Er", "kam", ",", "und", "fand", "sein", "Haus", "in", "\u00dc\u00b7berf\u00b7lu\u00df", "und", "Ruh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er fand sein Weib und seine beiden Kinder,", "tokens": ["Er", "fand", "sein", "Weib", "und", "sei\u00b7ne", "bei\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und - Segen Gottes! - zwei dazu.", "tokens": ["Und", "Se\u00b7gen", "Got\u00b7tes", "!", "zwei", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "NN", "$.", "$(", "CARD", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}