{"dta.poem.1295": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der alte Teutsche trug die Kinder an den Rhein/", "tokens": ["Der", "al\u00b7te", "Teut\u00b7sche", "trug", "die", "Kin\u00b7der", "an", "den", "Rhein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es solte dieses Bad der Unschuld Probe seyn.", "tokens": ["Es", "sol\u00b7te", "die\u00b7ses", "Bad", "der", "Un\u00b7schuld", "Pro\u00b7be", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "NE", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir haben allesammt ein heilsam Bad von n\u00f6then/", "tokens": ["Wir", "ha\u00b7ben", "al\u00b7le\u00b7sammt", "ein", "heil\u00b7sam", "Bad", "von", "n\u00f6\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NE", "APPR", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn uns die Erb-Schuld nicht err\u00f6then soll und t\u00f6dten.", "tokens": ["Wenn", "uns", "die", "Er\u00b7bSchuld", "nicht", "er\u00b7r\u00f6\u00b7then", "soll", "und", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Allein/ es ist hierzu kein schlechtes Wasser gutt/", "tokens": ["Al\u00b7lein", "/", "es", "ist", "hier\u00b7zu", "kein", "schlech\u00b7tes", "Was\u00b7ser", "gutt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VAFIN", "PAV", "PIAT", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Rhein macht keinen rein/ die st\u00e4rckste Wasser-Flutt", "tokens": ["Der", "Rhein", "macht", "kei\u00b7nen", "rein", "/", "die", "st\u00e4rcks\u00b7te", "Was\u00b7ser\u00b7Flutt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PIAT", "ADJD", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "W\u00e4scht keinen Flecken ab/ der an der Seele klebet/", "tokens": ["W\u00e4scht", "kei\u00b7nen", "Fle\u00b7cken", "ab", "/", "der", "an", "der", "See\u00b7le", "kle\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn aber GOttes Geist ob Jordans Strome schwebet/", "tokens": ["Wenn", "a\u00b7ber", "Got\u00b7tes", "Geist", "ob", "Jor\u00b7dans", "Stro\u00b7me", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "NN", "KOUS", "NE", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn er mit Wunder-Krafft den Teich Bethe\u00dfda r\u00fchrt/", "tokens": ["Wenn", "er", "mit", "Wun\u00b7der\u00b7Krafft", "den", "Teich", "Be\u00b7the\u00df\u00b7da", "r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn der Erl\u00f6ser uns zu einem Brunnen f\u00fchrt/", "tokens": ["Wenn", "der", "Er\u00b7l\u00f6\u00b7ser", "uns", "zu", "ei\u00b7nem", "Brun\u00b7nen", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Geist und Wort geweyht/ so ist ein Bach zu finden/", "tokens": ["Durch", "Geist", "und", "Wort", "ge\u00b7weyht", "/", "so", "ist", "ein", "Bach", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$(", "ADV", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dem man sauber wird von allem Koth der S\u00fcnden.", "tokens": ["In", "dem", "man", "sau\u00b7ber", "wird", "von", "al\u00b7lem", "Koth", "der", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADV", "VAFIN", "APPR", "PIS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Quelle macht dich auch/ geliebte Seele/ rein/", "tokens": ["Die", "Quel\u00b7le", "macht", "dich", "auch", "/", "ge\u00b7lieb\u00b7te", "See\u00b7le", "/", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "ADJA", "NN", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wird dir Lebenslang der Unschuld Zeugni\u00df seyn;", "tokens": ["Und", "wird", "dir", "Le\u00b7bens\u00b7lang", "der", "Un\u00b7schuld", "Zeug\u00b7ni\u00df", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie bring\u2019 an Leib und Seel erw\u00fcnschtes Wohlgedeyn/", "tokens": ["Sie", "bring'", "an", "Leib", "und", "Seel", "er\u00b7w\u00fcnschtes", "Wohl\u00b7ge\u00b7deyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und f\u00fchre dich zum Strom der ewgen Wollust ein.", "tokens": ["Und", "f\u00fch\u00b7re", "dich", "zum", "Strom", "der", "ew\u00b7gen", "Wol\u00b7lust", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}