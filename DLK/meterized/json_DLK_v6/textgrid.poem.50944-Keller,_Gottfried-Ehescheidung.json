{"textgrid.poem.50944": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ehescheidung", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Pf\u00e4ffel kam ein P\u00e4rchen und schrie:", "tokens": ["Zum", "Pf\u00e4f\u00b7fel", "kam", "ein", "P\u00e4r\u00b7chen", "und", "schrie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbgeschwinde la\u00dft uns frein!", "tokens": ["\u00bb", "ge\u00b7schwin\u00b7de", "la\u00dft", "uns", "frein", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir k\u00f6nnen nicht eine einzige Stund", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nicht", "ei\u00b7ne", "ein\u00b7zi\u00b7ge", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mehr ohne einander sein!\u00ab", "tokens": ["Mehr", "oh\u00b7ne", "ein\u00b7an\u00b7der", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "PRF", "VAINF", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und aber ein J\u00e4hrlein kaum verstrich,", "tokens": ["Und", "a\u00b7ber", "ein", "J\u00e4hr\u00b7lein", "kaum", "ver\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie liefen herbei und schrien:", "tokens": ["Sie", "lie\u00b7fen", "her\u00b7bei", "und", "schri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbherr Pfarrer, trennt und scheidet uns,", "tokens": ["\u00bb", "herr", "Pfar\u00b7rer", ",", "trennt", "und", "schei\u00b7det", "uns", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft keine Minute verziehn!\u00ab", "tokens": ["La\u00dft", "kei\u00b7ne", "Mi\u00b7nu\u00b7te", "ver\u00b7ziehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PIAT", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Das Pf\u00e4fflein runzelte sich und sprach:", "tokens": ["Das", "Pf\u00e4f\u00b7flein", "run\u00b7zel\u00b7te", "sich", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbmacht euch die Scham nicht rot?", "tokens": ["\u00bb", "macht", "euch", "die", "Scham", "nicht", "rot", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wir haben es alle drei beschworn:", "tokens": ["Wir", "ha\u00b7ben", "es", "al\u00b7le", "drei", "be\u00b7schworn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "CARD", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Euch trenne nur der Tod!\u00ab \u2013", "tokens": ["Euch", "tren\u00b7ne", "nur", "der", "Tod", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbrot macht die Scham, doch Reue bleich!", "tokens": ["\u00bb", "rot", "macht", "die", "Scham", ",", "doch", "Reu\u00b7e", "bleich", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "NN", "$,", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr Pfarrer, gebt uns frei!\u00ab", "tokens": ["Herr", "Pfar\u00b7rer", ",", "gebt", "uns", "frei", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mann bot einen Beutel dar,", "tokens": ["Der", "Mann", "bot", "ei\u00b7nen", "Beu\u00b7tel", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frau der Beutel zwei.", "tokens": ["Die", "Frau", "der", "Beu\u00b7tel", "zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da tat das Pf\u00e4ffel zwischen sie", "tokens": ["Da", "tat", "das", "Pf\u00e4f\u00b7fel", "zwi\u00b7schen", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein K\u00e4tzelein, heil und ganz;", "tokens": ["Ein", "K\u00e4t\u00b7zel\u00b7ein", ",", "heil", "und", "ganz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Mann, der hielt es bei dem Kopf,", "tokens": ["Der", "Mann", ",", "der", "hielt", "es", "bei", "dem", "Kopf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frau hielt es am Schwanz.", "tokens": ["Die", "Frau", "hielt", "es", "am", "Schwanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Pfaff mit gro\u00dfem Messer hieb", "tokens": ["Der", "Pfaff", "mit", "gro\u00b7\u00dfem", "Mes\u00b7ser", "hieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das K\u00e4tzelein entzwei:", "tokens": ["Das", "K\u00e4t\u00b7zel\u00b7ein", "ent\u00b7zwei", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbes trennt, es trennt, es trennt der Tod!\u00ab", "tokens": ["\u00bb", "es", "trennt", ",", "es", "trennt", ",", "es", "trennt", "der", "Tod", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da waren sie wieder frei.", "tokens": ["Da", "wa\u00b7ren", "sie", "wie\u00b7der", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}