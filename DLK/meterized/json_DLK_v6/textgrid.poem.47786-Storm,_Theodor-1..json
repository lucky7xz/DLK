{"textgrid.poem.47786": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich dich kaum gesehn,", "tokens": ["Als", "ich", "dich", "kaum", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00dft es mein Herz gestehn,", "tokens": ["Mu\u00dft", "es", "mein", "Herz", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich k\u00f6nnt dir nimmermehr", "tokens": ["Ich", "k\u00f6nnt", "dir", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vor\u00fcbergehn.", "tokens": ["Vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "F\u00e4llt nun der Sternenschein", "tokens": ["F\u00e4llt", "nun", "der", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nachts in mein K\u00e4mmerlein,", "tokens": ["Nachts", "in", "mein", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lieg ich und schlafe nicht", "tokens": ["Lieg", "ich", "und", "schla\u00b7fe", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und denke dein.", "tokens": ["Und", "den\u00b7ke", "dein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ist doch die Seele mein", "tokens": ["Ist", "doch", "die", "See\u00b7le", "mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So ganz geworden dein,", "tokens": ["So", "ganz", "ge\u00b7wor\u00b7den", "dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAPP", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zittert in deiner Hand,", "tokens": ["Zit\u00b7tert", "in", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Tu ihr kein Leid!", "tokens": ["Tu", "ihr", "kein", "Leid", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}