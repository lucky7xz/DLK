{"dta.poem.16069": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "123.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft hab' ich umgestimmt die Saiten meines Psalters", "tokens": ["Oft", "hab'", "ich", "um\u00b7ge\u00b7stimmt", "die", "Sai\u00b7ten", "mei\u00b7nes", "Psal\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Wechsel meiner Zeit und meines Lebensalters.", "tokens": ["Im", "Wech\u00b7sel", "mei\u00b7ner", "Zeit", "und", "mei\u00b7nes", "Le\u00b7bens\u00b7al\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun t\u00f6nen sie voll Ernst, und wer da will, entscheid' es,", "tokens": ["Nun", "t\u00f6\u00b7nen", "sie", "voll", "Ernst", ",", "und", "wer", "da", "will", ",", "ent\u00b7scheid'", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,", "KON", "PWS", "ADV", "VMFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob Alter oder Zeit dran schuld sei, oder beides.", "tokens": ["Ob", "Al\u00b7ter", "o\u00b7der", "Zeit", "dran", "schuld", "sei", ",", "o\u00b7der", "bei\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PAV", "ADJD", "VAFIN", "$,", "KON", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Zeit ist ernst sogar der jugendlichen Schaar,", "tokens": ["Die", "Zeit", "ist", "ernst", "so\u00b7gar", "der", "ju\u00b7gend\u00b7li\u00b7chen", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mehr noch einem, dem mit ihr gebleicht das Haar.", "tokens": ["Wie", "mehr", "noch", "ei\u00b7nem", ",", "dem", "mit", "ihr", "ge\u00b7bleicht", "das", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "$,", "PRELS", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}