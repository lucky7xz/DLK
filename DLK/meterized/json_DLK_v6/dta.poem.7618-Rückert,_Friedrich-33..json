{"dta.poem.7618": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Gaukler, wie geschickt er seine Glieder braucht,", "tokens": ["Der", "Gauk\u00b7ler", ",", "wie", "ge\u00b7schickt", "er", "sei\u00b7ne", "Glie\u00b7der", "braucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "VVPP", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Prall wie aus Stahl gespannt, und wie aus Luft gehaucht!", "tokens": ["Prall", "wie", "aus", "Stahl", "ge\u00b7spannt", ",", "und", "wie", "aus", "Luft", "ge\u00b7haucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "NN", "VVPP", "$,", "KON", "PWAV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wozu braucht er sie? Um Schauder, Furcht und Schrecken,", "tokens": ["Und", "wo\u00b7zu", "braucht", "er", "sie", "?", "Um", "Schau\u00b7der", ",", "Furcht", "und", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PPER", "$.", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anstatt Bewunderung, ja Abscheu zu erwecken.", "tokens": ["An\u00b7statt", "Be\u00b7wun\u00b7de\u00b7rung", ",", "ja", "Ab\u00b7scheu", "zu", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der arme Gaukler! so geht seine Kunst nach Brot;", "tokens": ["Der", "ar\u00b7me", "Gauk\u00b7ler", "!", "so", "geht", "sei\u00b7ne", "Kunst", "nach", "Brot", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch andre thun's ihm gleich, und habens minder noth.", "tokens": ["Doch", "and\u00b7re", "thun's", "ihm", "gleich", ",", "und", "ha\u00b7bens", "min\u00b7der", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "$,", "KON", "NE", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn schon ein Schauder ist misbrauchte K\u00f6rperkraft,", "tokens": ["Wenn", "schon", "ein", "Schau\u00b7der", "ist", "mis\u00b7brauch\u00b7te", "K\u00f6r\u00b7per\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Misbrauchter Geist und Witz ist doppelt schauderhaft.", "tokens": ["Mis\u00b7brauch\u00b7ter", "Geist", "und", "Witz", "ist", "dop\u00b7pelt", "schau\u00b7der\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}