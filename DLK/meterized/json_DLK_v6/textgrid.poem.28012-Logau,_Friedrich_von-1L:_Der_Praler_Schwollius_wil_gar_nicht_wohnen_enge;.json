{"textgrid.poem.28012": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Praler Schwollius wil gar nicht wohnen enge;", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Praler Schwollius wil gar nicht wohnen enge;", "tokens": ["Der", "Pra\u00b7ler", "Schwol\u00b7lius", "wil", "gar", "nicht", "woh\u00b7nen", "en\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VMFIN", "ADV", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein Hau\u00df mu\u00df sein geraumt, gewaschen alle G\u00e4nge;", "tokens": ["Sein", "Hau\u00df", "mu\u00df", "sein", "ge\u00b7raumt", ",", "ge\u00b7wa\u00b7schen", "al\u00b7le", "G\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "VVPP", "$,", "ADJA", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht wunder! ihn verdru\u00df, da er erst ward ein Kind,", "tokens": ["Nicht", "wun\u00b7der", "!", "ihn", "ver\u00b7dru\u00df", ",", "da", "er", "erst", "ward", "ein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$.", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Beschlossen seyn dahin, wo lauter N\u00e4chte sind;", "tokens": ["Be\u00b7schlos\u00b7sen", "seyn", "da\u00b7hin", ",", "wo", "lau\u00b7ter", "N\u00e4ch\u00b7te", "sind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "PAV", "$,", "PWAV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum brach er bald herf\u00fcr, wo's eng und unrein ware,", "tokens": ["Drum", "brach", "er", "bald", "her\u00b7f\u00fcr", ",", "wo's", "eng", "und", "un\u00b7rein", "wa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "ADJD", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ob seine Mutter gleich, war Frau vom Viertel-Jahre.", "tokens": ["Ob", "sei\u00b7ne", "Mut\u00b7ter", "gleich", ",", "war", "Frau", "vom", "Vier\u00b7tel\u00b7Jah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "VAFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}