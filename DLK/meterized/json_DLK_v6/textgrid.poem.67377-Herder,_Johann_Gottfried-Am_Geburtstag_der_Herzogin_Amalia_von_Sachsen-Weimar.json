{"textgrid.poem.67377": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Am Geburtstag der Herzogin Amalia von Sachsen-Weimar", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei gegr\u00fc\u00dfet, sch\u00f6ne Sonne,", "tokens": ["Sei", "ge\u00b7gr\u00fc\u00b7\u00dfet", ",", "sch\u00f6\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei willkommen, Tag der Wonne,", "tokens": ["Sei", "will\u00b7kom\u00b7men", ",", "Tag", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Musen Heiligthum!", "tokens": ["In", "der", "Mu\u00b7sen", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Schwester kommt zu ihnen;", "tokens": ["Ih\u00b7re", "Schwes\u00b7ter", "kommt", "zu", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Holde Musen, Ihr zu dienen,", "tokens": ["Hol\u00b7de", "Mu\u00b7sen", ",", "Ihr", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schafft uns ein Elysium!", "tokens": ["Schafft", "uns", "ein", "E\u00b7ly\u00b7si\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fchl' ich nicht von jenen H\u00f6hen", "tokens": ["F\u00fchl'", "ich", "nicht", "von", "je\u00b7nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heitre, sch\u00f6ne L\u00fcfte wehen,", "tokens": ["Heit\u00b7re", ",", "sch\u00f6\u00b7ne", "L\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Voll Gesundheit, voller Ruh?", "tokens": ["Voll", "Ge\u00b7sund\u00b7heit", ",", "vol\u00b7ler", "Ruh", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, die W\u00fcnsche Ihrer Treuen,", "tokens": ["Ach", ",", "die", "W\u00fcn\u00b7sche", "Ih\u00b7rer", "Treu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die entfernt sich mit uns freuen,", "tokens": ["Die", "ent\u00b7fernt", "sich", "mit", "uns", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wehn uns diese L\u00fcfte zu.", "tokens": ["Wehn", "uns", "die\u00b7se", "L\u00fcf\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wir, die heute Sie umschlie\u00dfen,", "tokens": ["Wir", ",", "die", "heu\u00b7te", "Sie", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Sie liebend n\u00e4her gr\u00fc\u00dfen,", "tokens": ["Die", "Sie", "lie\u00b7bend", "n\u00e4\u00b7her", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gehn am Gl\u00fccke Jenen vor.", "tokens": ["Gehn", "am", "Gl\u00fc\u00b7cke", "Je\u00b7nen", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PDS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Helfet Sie mit uns umschlie\u00dfen,", "tokens": ["Hel\u00b7fet", "Sie", "mit", "uns", "um\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Helfet Sie mit uns begr\u00fc\u00dfen,", "tokens": ["Hel\u00b7fet", "Sie", "mit", "uns", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Musen, werdet uns ein Chor!", "tokens": ["Mu\u00b7sen", ",", "wer\u00b7det", "uns", "ein", "Chor", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Apollo, komm, la\u00df Deine Locken fliegen", "tokens": ["A\u00b7pol\u00b7lo", ",", "komm", ",", "la\u00df", "Dei\u00b7ne", "Lo\u00b7cken", "flie\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "VVIMP", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weih Ihr Deinen j\u00fcngsten Kranz!", "tokens": ["Und", "weih", "Ihr", "Dei\u00b7nen", "j\u00fcng\u00b7sten", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Weste sich um ihre G\u00f6ttin wiegen,", "tokens": ["Wie", "Wes\u00b7te", "sich", "um", "ih\u00b7re", "G\u00f6t\u00b7tin", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umschwebe Sie der Charitinnen Tanz!", "tokens": ["Um\u00b7schwe\u00b7be", "Sie", "der", "Cha\u00b7ri\u00b7tin\u00b7nen", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hygea schling' um ihre T\u00e4nze,", "tokens": ["Hy\u00b7gea", "schling'", "um", "ih\u00b7re", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In ihren jugendlichen Reihn,", "tokens": ["In", "ih\u00b7ren", "ju\u00b7gend\u00b7li\u00b7chen", "Reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den sch\u00f6nsten ihrer Kr\u00e4nze,", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "ih\u00b7rer", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Kranz der Freuden ein!", "tokens": ["Den", "Kranz", "der", "Freu\u00b7den", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Genie\u00dfe diese Tage,", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "die\u00b7se", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die langerw\u00fcnschten Tage,", "tokens": ["Die", "lan\u00b7ger\u00b7w\u00fcnschten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im alten Heiligthum!", "tokens": ["Im", "al\u00b7ten", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auch im Andenken werde", "tokens": ["Auch", "im", "An\u00b7den\u00b7ken", "wer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VAFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Dir einst auf fremder Erde", "tokens": ["Dir", "einst", "auf", "frem\u00b7der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Rom ein Elysium!", "tokens": ["Rom", "ein", "E\u00b7ly\u00b7si\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}