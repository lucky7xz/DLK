{"textgrid.poem.47172": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mutter am Abend", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Sonne die Augen zugethan,", "tokens": ["Wie", "Son\u00b7ne", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mond ihr nachblickt mit Harme,", "tokens": ["Der", "Mond", "ihr", "nach\u00b7blickt", "mit", "Har\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00e4ngt das Kindlein zu weinen an", "tokens": ["F\u00e4ngt", "das", "Kin\u00b7dlein", "zu", "wei\u00b7nen", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF", "APPR"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Selbst auf der Mutter Arme.", "tokens": ["Selbst", "auf", "der", "Mut\u00b7ter", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat in die Welt hinaus gelacht,", "tokens": ["Es", "hat", "in", "die", "Welt", "hin\u00b7aus", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APZR", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So lange sie golden gefunkelt;", "tokens": ["So", "lan\u00b7ge", "sie", "gol\u00b7den", "ge\u00b7fun\u00b7kelt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den sch\u00f6nen Schimmer hat die Nacht,", "tokens": ["Den", "sch\u00f6\u00b7nen", "Schim\u00b7mer", "hat", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Augenspielzeug, verdunkelt.", "tokens": ["Das", "Au\u00b7gen\u00b7spiel\u00b7zeug", ",", "ver\u00b7dun\u00b7kelt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Einen Schauer f\u00fchlt die Natur,", "tokens": ["Ei\u00b7nen", "Schau\u00b7er", "f\u00fchlt", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Bl\u00e4tter beben im Winde;", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "be\u00b7ben", "im", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du, Mensch, bist ihm entwachsen nur,", "tokens": ["Du", ",", "Mensch", ",", "bist", "ihm", "ent\u00b7wach\u00b7sen", "nur", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch f\u00fchlst du ihn nach im Kinde.", "tokens": ["Doch", "f\u00fchlst", "du", "ihn", "nach", "im", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Die V\u00f6glein schlie\u00dfen die Augen zu,", "tokens": ["Die", "V\u00f6\u00b7glein", "schlie\u00b7\u00dfen", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Graus der Nacht nicht zu sehen.", "tokens": ["Den", "Graus", "der", "Nacht", "nicht", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mutter, bringe dein Kind zur Ruh'!", "tokens": ["Mut\u00b7ter", ",", "brin\u00b7ge", "dein", "Kind", "zur", "Ruh'", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ihm kann nichts Bessers geschehen.", "tokens": ["Ihm", "kann", "nichts", "Bes\u00b7sers", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}