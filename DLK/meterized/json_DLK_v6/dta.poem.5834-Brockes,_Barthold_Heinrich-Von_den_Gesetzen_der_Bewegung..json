{"dta.poem.5834": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der C\u00f6rper prallt zur\u00fcck, da, wo er hergekommen.", "tokens": ["Der", "C\u00f6r\u00b7per", "prallt", "zu\u00b7r\u00fcck", ",", "da", ",", "wo", "er", "her\u00b7ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "$,", "PWAV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat er zuvor den Weg gerade vorgenommen;", "tokens": ["Hat", "er", "zu\u00b7vor", "den", "Weg", "ge\u00b7ra\u00b7de", "vor\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kommt er auch wiederum gerade r\u00fcckwerts her:", "tokens": ["Kommt", "er", "auch", "wie\u00b7de\u00b7rum", "ge\u00b7ra\u00b7de", "r\u00fcck\u00b7werts", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo aber seine Fahrt bald weniger, bald mehr", "tokens": ["Wo", "a\u00b7ber", "sei\u00b7ne", "Fahrt", "bald", "we\u00b7ni\u00b7ger", ",", "bald", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADV", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gebogen oder schieff den Vorwurff trifft; so fliegt", "tokens": ["Ge\u00b7bo\u00b7gen", "o\u00b7der", "schieff", "den", "Vor\u00b7wurff", "trifft", ";", "so", "fliegt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er alsbald seitenwerts. Da denn vom Gegenschlag", "tokens": ["Er", "als\u00b7bald", "sei\u00b7ten\u00b7werts", ".", "Da", "denn", "vom", "Ge\u00b7gen\u00b7schlag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$.", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Winckel sich mit dem, woran er stossen mag,", "tokens": ["Der", "Win\u00b7ckel", "sich", "mit", "dem", ",", "wo\u00b7ran", "er", "stos\u00b7sen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In einer ganzen Gleichheit f\u00fcgt.", "tokens": ["In", "ei\u00b7ner", "gan\u00b7zen", "Gleich\u00b7heit", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}