{"dta.poem.19639": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Wahrheit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vier Jungfr\u00e4ulein von hohem Stamm,               ", "tokens": ["Vier", "Jung\u00b7fr\u00e4u\u00b7lein", "von", "ho\u00b7hem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die waren bei einander,", "tokens": ["Die", "wa\u00b7ren", "bei", "ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PRF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ignis Feuer die erst mit Nahm,", "tokens": ["Ig\u00b7nis", "Feu\u00b7er", "die", "erst", "mit", "Nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Aqua Wasser die ander:", "tokens": ["A\u00b7qua", "Was\u00b7ser", "die", "an\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJD", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Aer die Luft, so hie\u00df die dritt,", "tokens": ["A\u00b7er", "die", "Luft", ",", "so", "hie\u00df", "die", "dritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dann Veritas die Wahrheit,", "tokens": ["Dann", "Ve\u00b7ri\u00b7tas", "die", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die stand da in des Gartens Mitt,", "tokens": ["Die", "stand", "da", "in", "des", "Gar\u00b7tens", "Mitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leuchtete in Klarheit.", "tokens": ["Und", "leuch\u00b7te\u00b7te", "in", "Klar\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sehne mich gar oft nach euch,", "tokens": ["Ich", "seh\u00b7ne", "mich", "gar", "oft", "nach", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach sie mit klugen Sinnen,", "tokens": ["Sprach", "sie", "mit", "klu\u00b7gen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum saget mir, eh ich entfleug,", "tokens": ["Drum", "sa\u00b7get", "mir", ",", "eh", "ich", "ent\u00b7fleug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo soll ich euch stets finden?", "tokens": ["Wo", "soll", "ich", "euch", "stets", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Feuer sprach: Schlag an ein Stein", "tokens": ["Das", "Feu\u00b7er", "sprach", ":", "Schlag", "an", "ein", "Stein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit guten Schwerdtes Spitzen,", "tokens": ["Mit", "gu\u00b7ten", "Schwerd\u00b7tes", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So werd ich schnelle bei dir sein,", "tokens": ["So", "werd", "ich", "schnel\u00b7le", "bei", "dir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und freudig Funken spritzen.", "tokens": ["Und", "freu\u00b7dig", "Fun\u00b7ken", "sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Wasser sprach: Wo Binsen stehn,", "tokens": ["Das", "Was\u00b7ser", "sprach", ":", "Wo", "Bin\u00b7sen", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sollst du nach mir graben,", "tokens": ["Da", "sollst", "du", "nach", "mir", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst mich bei der Wurzel sehn,", "tokens": ["Du", "wirst", "mich", "bei", "der", "Wur\u00b7zel", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da will ich dich erlaben.", "tokens": ["Da", "will", "ich", "dich", "er\u00b7la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Luft sprach: Wenn an einem Baum", "tokens": ["Die", "Luft", "sprach", ":", "Wenn", "an", "ei\u00b7nem", "Baum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Bl\u00e4ttlein gehn und nicken,", "tokens": ["Die", "Bl\u00e4tt\u00b7lein", "gehn", "und", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da bin ich auch in selbem Raum,", "tokens": ["Da", "bin", "ich", "auch", "in", "sel\u00b7bem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will dich bald erquicken.", "tokens": ["Und", "will", "dich", "bald", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "All drei sie sprachen wonnsamlich:", "tokens": ["All", "drei", "sie", "spra\u00b7chen", "wonn\u00b7sam\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du edele Warheite!", "tokens": ["Du", "e\u00b7de\u00b7le", "War\u00b7hei\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo sollen wir dann finden dich?", "tokens": ["Wo", "sol\u00b7len", "wir", "dann", "fin\u00b7den", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wahrheit sprach: Im Leide.", "tokens": ["Die", "Wahr\u00b7heit", "sprach", ":", "Im", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O ihr Schwestern Mord \u00fcber Mord!", "tokens": ["O", "ihr", "Schwes\u00b7tern", "Mord", "\u00fc\u00b7ber", "Mord", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Kein eigen Haus mir bleibet,", "tokens": ["Kein", "ei\u00b7gen", "Haus", "mir", "blei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man findet mich nicht hier, nicht dort,", "tokens": ["Man", "fin\u00b7det", "mich", "nicht", "hier", ",", "nicht", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder mich vertreibet.", "tokens": ["Ein", "je\u00b7der", "mich", "ver\u00b7trei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich pocht auch bei Gelehrten an,", "tokens": ["Ich", "pocht", "auch", "bei", "Ge\u00b7lehr\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil ehrlich ist ihr Wandel,", "tokens": ["Weil", "ehr\u00b7lich", "ist", "ihr", "Wan\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist ihr Werk ein Lug und Wahn", "tokens": ["Doch", "ist", "ihr", "Werk", "ein", "Lug", "und", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sp\u00e4rlich nach dem Handel.", "tokens": ["Und", "sp\u00e4r\u00b7lich", "nach", "dem", "Han\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie fingen mich und banden mich,", "tokens": ["Sie", "fin\u00b7gen", "mich", "und", "ban\u00b7den", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begossen mich mit Dinten,", "tokens": ["Be\u00b7gos\u00b7sen", "mich", "mit", "Din\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In mein schneewei\u00dfes Angesicht,", "tokens": ["In", "mein", "schnee\u00b7wei\u00b7\u00dfes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich muste schier erblinden.", "tokens": ["Ich", "mus\u00b7te", "schier", "er\u00b7blin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Mit B\u00fcchern schlugen sie mich dumm,", "tokens": ["Mit", "B\u00fc\u00b7chern", "schlu\u00b7gen", "sie", "mich", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und krazten mich und krallten,", "tokens": ["Und", "kraz\u00b7ten", "mich", "und", "krall\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und zogen mich beim Haar herum,", "tokens": ["Und", "zo\u00b7gen", "mich", "beim", "Haar", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Th\u00fcr hinaus mich brallten.", "tokens": ["Zur", "Th\u00fcr", "hin\u00b7aus", "mich", "brall\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie wollte klagen noch viel mehr,", "tokens": ["Sie", "woll\u00b7te", "kla\u00b7gen", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Th\u00fcrlein th\u00e4t erklingen,", "tokens": ["Ein", "Th\u00fcr\u00b7lein", "th\u00e4t", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Critikus kam ganz grad daher,", "tokens": ["Ein", "Cri\u00b7ti\u00b7kus", "kam", "ganz", "grad", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Davon that sie sich schwingen.", "tokens": ["Da\u00b7von", "that", "sie", "sich", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}