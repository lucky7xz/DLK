{"textgrid.poem.62613": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf eben diese Leiche", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch, die zart und kleine Welt", "tokens": ["Der", "Mensch", ",", "die", "zart", "und", "klei\u00b7ne", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Woran Gott seine Kunst bezeiget,", "tokens": ["Wo\u00b7ran", "Gott", "sei\u00b7ne", "Kunst", "be\u00b7zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist vieler Schwachheit ausgestellt,", "tokens": ["Ist", "vie\u00b7ler", "Schwach\u00b7heit", "aus\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die oft das Herz zur Erde beuget.", "tokens": ["Die", "oft", "das", "Herz", "zur", "Er\u00b7de", "beu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nimmt er Verfolgung und Gefahr,", "tokens": ["Nimmt", "er", "Ver\u00b7fol\u00b7gung", "und", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kreuz, Leiden, Noth und Krankheit wahr,", "tokens": ["Kreuz", ",", "Lei\u00b7den", ",", "Noth", "und", "Krank\u00b7heit", "wahr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So \u00fcberf\u00e4llt ihn Angst und Grauen.", "tokens": ["So", "\u00fc\u00b7berf\u00b7\u00e4llt", "ihn", "Angst", "und", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Absonderlich erschrickt sein Geist,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "er\u00b7schrickt", "sein", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn ihm der Tod die Stunde weist,", "tokens": ["Wenn", "ihm", "der", "Tod", "die", "Stun\u00b7de", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da er sein Reich soll helfen bauen.", "tokens": ["Da", "er", "sein", "Reich", "soll", "hel\u00b7fen", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die allerkleinste Creatur", "tokens": ["Die", "al\u00b7ler\u00b7kleins\u00b7te", "Crea\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kr\u00fcmmt sich in Schmerz und Todes-Z\u00fcgen.", "tokens": ["Kr\u00fcmmt", "sich", "in", "Schmerz", "und", "To\u00b7des\u00b7Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist nicht wider die Natur", "tokens": ["Es", "ist", "nicht", "wi\u00b7der", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Grauen vor den Tod zu kriegen.", "tokens": ["Ein", "Grau\u00b7en", "vor", "den", "Tod", "zu", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hiskias, der Gott nie verga\u00df,", "tokens": ["His\u00b7ki\u00b7as", ",", "der", "Gott", "nie", "ver\u00b7ga\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Rang weinend ohne Unterla\u00df", "tokens": ["Rang", "wei\u00b7nend", "oh\u00b7ne", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die H\u00e4nde auf dem Kranken-Bette.", "tokens": ["Die", "H\u00e4n\u00b7de", "auf", "dem", "Kran\u00b7ken\u00b7Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bathe Gott, der Wunder schafft,", "tokens": ["Und", "ba\u00b7the", "Gott", ",", "der", "Wun\u00b7der", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ums Leben, und um neue Kraft,", "tokens": ["Ums", "Le\u00b7ben", ",", "und", "um", "neu\u00b7e", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Damit er ihn zu preisen h\u00e4tte.", "tokens": ["Da\u00b7mit", "er", "ihn", "zu", "prei\u00b7sen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie bitter ist es nicht, die Welt", "tokens": ["Wie", "bit\u00b7ter", "ist", "es", "nicht", ",", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihre Herrlichkeit zu meiden,", "tokens": ["Und", "ih\u00b7re", "Herr\u00b7lich\u00b7keit", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und von der Ehre, Gut und Geld,", "tokens": ["Und", "von", "der", "Eh\u00b7re", ",", "Gut", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht, Ansehn, Gl\u00fcck und Pracht zu scheiden!", "tokens": ["Macht", ",", "An\u00b7sehn", ",", "Gl\u00fcck", "und", "Pracht", "zu", "schei\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie schmerzhaft ist es, wenn ein Mann,", "tokens": ["Wie", "schmerz\u00b7haft", "ist", "es", ",", "wenn", "ein", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sich der Tugend r\u00fchmen kan,", "tokens": ["Der", "sich", "der", "Tu\u00b7gend", "r\u00fch\u00b7men", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So fr\u00fch und zeitig soll erblassen!", "tokens": ["So", "fr\u00fch", "und", "zei\u00b7tig", "soll", "er\u00b7blas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O! ein durchdringend herber Schmerz,", "tokens": ["O", "!", "ein", "durch\u00b7drin\u00b7gend", "her\u00b7ber", "Schmerz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Eltern und sein halbes Herz,", "tokens": ["Die", "El\u00b7tern", "und", "sein", "hal\u00b7bes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die liebsten Freunde zu verlassen!", "tokens": ["Die", "liebs\u00b7ten", "Freun\u00b7de", "zu", "ver\u00b7las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch der Gerechte ist bem\u00fcht", "tokens": ["Doch", "der", "Ge\u00b7rech\u00b7te", "ist", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich solcher Fesseln loszureissen.", "tokens": ["Sich", "sol\u00b7cher", "Fes\u00b7seln", "los\u00b7zu\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Geist, der nach dem Himmel sieht,", "tokens": ["Sein", "Geist", ",", "der", "nach", "dem", "Him\u00b7mel", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will sich getrost im Tod beweisen.", "tokens": ["Will", "sich", "ge\u00b7trost", "im", "Tod", "be\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er wei\u00df gewi\u00df, da\u00df diese Welt", "tokens": ["Er", "wei\u00df", "ge\u00b7wi\u00df", ",", "da\u00df", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nur Angst und Jammer in sich h\u00e4lt,", "tokens": ["Nur", "Angst", "und", "Jam\u00b7mer", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und doch zuletzt im Rauch verschwindet;", "tokens": ["Und", "doch", "zu\u00b7letzt", "im", "Rauch", "ver\u00b7schwin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hingegen trift er Canaan", "tokens": ["Hin\u00b7ge\u00b7gen", "trift", "er", "Ca\u00b7naan"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Und Gosen nach den Sterben an,", "tokens": ["Und", "Go\u00b7sen", "nach", "den", "Ster\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allwo er recht Vergn\u00fcgen findet.", "tokens": ["All\u00b7wo", "er", "recht", "Ver\u00b7gn\u00fc\u00b7gen", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die\u00df Himmels Gl\u00fcck erblickst du nun", "tokens": ["Die\u00df", "Him\u00b7mels", "Gl\u00fcck", "er\u00b7blickst", "du", "nun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erbla\u00dft und hochgeliebte Seele!", "tokens": ["Er\u00b7bla\u00dft", "und", "hoch\u00b7ge\u00b7lieb\u00b7te", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Geist kan nun i