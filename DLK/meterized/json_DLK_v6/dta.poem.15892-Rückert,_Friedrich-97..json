{"dta.poem.15892": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Musterbilder stehn vor euerer Beschauung;", "tokens": ["Zwei", "Mus\u00b7ter\u00b7bil\u00b7der", "stehn", "vor", "eu\u00b7e\u00b7rer", "Be\u00b7schau\u00b7ung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "W\u00e4hlt eurem Sinn gem\u00e4\u00df euch einen zur Erbauung!", "tokens": ["W\u00e4hlt", "eu\u00b7rem", "Sinn", "ge\u00b7m\u00e4\u00df", "euch", "ei\u00b7nen", "zur", "Er\u00b7bau\u00b7ung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PPER", "ART", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der eine kerngesund, der andre tiefverk\u00fcmmert,", "tokens": ["Der", "ei\u00b7ne", "kern\u00b7ge\u00b7sund", ",", "der", "and\u00b7re", "tief\u00b7ver\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eine ganz und rund, der andre ganz zertr\u00fcmmert.", "tokens": ["Der", "ei\u00b7ne", "ganz", "und", "rund", ",", "der", "and\u00b7re", "ganz", "zer\u00b7tr\u00fcm\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADV", "KON", "ADJD", "$,", "PRELS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der kranke Heilige hat, seh' ich, viel gefunden", "tokens": ["Der", "kran\u00b7ke", "Hei\u00b7li\u00b7ge", "hat", ",", "seh'", "ich", ",", "viel", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "VVFIN", "PPER", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anbeter, die ihn fromm vorziehen dem gesunden.", "tokens": ["An\u00b7be\u00b7ter", ",", "die", "ihn", "fromm", "vor\u00b7zie\u00b7hen", "dem", "ge\u00b7sun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit seiner Krankheit wollt ihr eure wol vertreiben,", "tokens": ["Mit", "sei\u00b7ner", "Krank\u00b7heit", "wollt", "ihr", "eu\u00b7re", "wol", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PPOSAT", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Meinthalb! nur la\u00dft gesund mich beim gesunden bleiben.", "tokens": ["Mein\u00b7thalb", "!", "nur", "la\u00dft", "ge\u00b7sund", "mich", "beim", "ge\u00b7sun\u00b7den", "blei\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ADJD", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}