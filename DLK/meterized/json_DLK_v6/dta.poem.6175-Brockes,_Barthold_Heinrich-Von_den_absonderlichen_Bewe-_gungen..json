{"dta.poem.6175": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den absonderlichen Bewe-  \n gungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie w\u00fcrcken unvermerckt, da\u00df man es nicht versp\u00fcret;", "tokens": ["Sie", "w\u00fcr\u00b7cken", "un\u00b7ver\u00b7merckt", ",", "da\u00df", "man", "es", "nicht", "ver\u00b7sp\u00fc\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daher so dann der Wachsthum r\u00fchret.", "tokens": ["Da\u00b7her", "so", "dann", "der", "Wach\u00b7sthum", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}