{"textgrid.poem.67068": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Weisheit des Traurers", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinweg, ihr W\u00fcnsche! Qu\u00e4ler des Unverstands!", "tokens": ["Hin\u00b7weg", ",", "ihr", "W\u00fcn\u00b7sche", "!", "Qu\u00e4\u00b7ler", "des", "Un\u00b7ver\u00b7stands", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hinweg von dieser St\u00e4tte, Verg\u00e4nglichkeit!", "tokens": ["Hin\u00b7weg", "von", "die\u00b7ser", "St\u00e4t\u00b7te", ",", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ernst, wie das Grab, sei meine Seele!", "tokens": ["Ernst", ",", "wie", "das", "Grab", ",", "sei", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heilig mein Sang, wie die Totenglocke!", "tokens": ["Hei\u00b7lig", "mein", "Sang", ",", "wie", "die", "To\u00b7ten\u00b7glo\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Du, stille Weisheit! \u00f6ffne dein Heiligtum.", "tokens": ["Du", ",", "stil\u00b7le", "Weis\u00b7heit", "!", "\u00f6ff\u00b7ne", "dein", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$.", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "La\u00df, wie den Greis am Grabe Cecilias,", "tokens": ["La\u00df", ",", "wie", "den", "Greis", "am", "Gra\u00b7be", "Ce\u00b7ci\u00b7li\u00b7as", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "NE", "$,"], "meter": "---+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mich lauschen deinen G\u00f6tterspr\u00fcchen,", "tokens": ["Mich", "lau\u00b7schen", "dei\u00b7nen", "G\u00f6t\u00b7ter\u00b7spr\u00fc\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ehe der Toten Gericht sie donnert.", "tokens": ["E\u00b7he", "der", "To\u00b7ten", "Ge\u00b7richt", "sie", "don\u00b7nert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Da, unbestochne Richterin, richtest du", "tokens": ["Da", ",", "un\u00b7be\u00b7stoch\u00b7ne", "Rich\u00b7te\u00b7rin", ",", "rich\u00b7test", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Tyrannenfeste, wo sich der H\u00f6flinge", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7fes\u00b7te", ",", "wo", "sich", "der", "H\u00f6f\u00b7lin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Entmanntes Heer zu Trug begeistert,", "tokens": ["Ent\u00b7mann\u00b7tes", "Heer", "zu", "Trug", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo des gesch\u00e4ndeten R\u00f6mers Kehle", "tokens": ["Wo", "des", "ge\u00b7sch\u00e4n\u00b7de\u00b7ten", "R\u00f6\u00b7mers", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die schwei\u00dferrungne Habe des Pfl\u00fcgers stiehlt,", "tokens": ["Die", "schwei\u00b7\u00dfer\u00b7rung\u00b7ne", "Ha\u00b7be", "des", "Pfl\u00fc\u00b7gers", "stiehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo tolle Lust in g\u00fcldnen Pokalen sch\u00e4umt,", "tokens": ["Wo", "tol\u00b7le", "Lust", "in", "g\u00fcld\u00b7nen", "Po\u00b7ka\u00b7len", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ha! des Greuels! an get\u00fcrmten", "tokens": ["Und", "ha", "!", "des", "Greu\u00b7els", "!", "an", "ge\u00b7t\u00fcrm\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$.", "ART", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Silbergef\u00e4\u00dfen des Landes Mark klebt.", "tokens": ["Sil\u00b7ber\u00b7ge\u00b7f\u00e4\u00b7\u00dfen", "des", "Lan\u00b7des", "Mark", "klebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Halt ein! Tyrann! Es f\u00e4hret des W\u00fcrgers Pfeil", "tokens": ["Halt", "ein", "!", "Ty\u00b7rann", "!", "Es", "f\u00e4h\u00b7ret", "des", "W\u00fcr\u00b7gers", "Pfeil"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Daher. Halt ein! es nahet der Rache Tag,", "tokens": ["Da\u00b7her", ".", "Halt", "ein", "!", "es", "na\u00b7het", "der", "Ra\u00b7che", "Tag", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$.", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Da\u00df er, wie Blitz die giftge Staude,", "tokens": ["Da\u00df", "er", ",", "wie", "Blitz", "die", "gift\u00b7ge", "Stau\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nieder den taumelnden Sch\u00e4del schmettre.", "tokens": ["Nie\u00b7der", "den", "tau\u00b7meln\u00b7den", "Sch\u00e4\u00b7del", "schmett\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Doch ach! am grimmen richtenden Saitenspiel", "tokens": ["Doch", "ach", "!", "am", "grim\u00b7men", "rich\u00b7ten\u00b7den", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hinunter wankt die zitternde Rechte mir.", "tokens": ["Hin\u00b7un\u00b7ter", "wankt", "die", "zit\u00b7tern\u00b7de", "Rech\u00b7te", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In lichtre Hallen, gute G\u00f6ttin! \u2013", "tokens": ["In", "licht\u00b7re", "Hal\u00b7len", ",", "gu\u00b7te", "G\u00f6t\u00b7tin", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wandle der Sturm sich in Haingefl\u00fcster!", "tokens": ["Wand\u00b7le", "der", "Sturm", "sich", "in", "Hain\u00b7ge\u00b7fl\u00fcs\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Da schlingst du liebevoll um die Jammernde", "tokens": ["Da", "schlingst", "du", "lie\u00b7be\u00b7voll", "um", "die", "Jam\u00b7mern\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Grabe des Erw\u00e4hlten den Mutterarm,", "tokens": ["Am", "Gra\u00b7be", "des", "Er\u00b7w\u00e4hl\u00b7ten", "den", "Mut\u00b7ter\u00b7arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor Menschentrost dein Kind zu sch\u00fctzen,", "tokens": ["Vor", "Men\u00b7schent\u00b7rost", "dein", "Kind", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schenkest ihr Tr\u00e4nen, und lispelst leise", "tokens": ["Schen\u00b7kest", "ihr", "Tr\u00e4\u00b7nen", ",", "und", "lis\u00b7pelst", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ADJD"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Vom Wiedersehn, vom seligen Einst ins Herz \u2013", "tokens": ["Vom", "Wie\u00b7der\u00b7sehn", ",", "vom", "se\u00b7li\u00b7gen", "Einst", "ins", "Herz", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da schl\u00e4ft in deiner Halle der Jammermann,", "tokens": ["Da", "schl\u00e4ft", "in", "dei\u00b7ner", "Hal\u00b7le", "der", "Jam\u00b7mer\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem Priesterha\u00df das Herz zerfleischet,", "tokens": ["Dem", "Pries\u00b7ter\u00b7ha\u00df", "das", "Herz", "zer\u00b7flei\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ihr Gericht im Gewahrsam foltert,", "tokens": ["Den", "ihr", "Ge\u00b7richt", "im", "Ge\u00b7wahr\u00b7sam", "fol\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Der bleiche J\u00fcngling, der in des Herzens Durst", "tokens": ["Der", "blei\u00b7che", "J\u00fcng\u00b7ling", ",", "der", "in", "des", "Her\u00b7zens", "Durst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach Ehre rastlos klomm auf der Felsenbahn", "tokens": ["Nach", "Eh\u00b7re", "rast\u00b7los", "klomm", "auf", "der", "Fel\u00b7sen\u00b7bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ach umsonst! wie wandelt er so", "tokens": ["Und", "ach", "um\u00b7sonst", "!", "wie", "wan\u00b7delt", "er", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$.", "PWAV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ruhig umher in der stillen Halle.", "tokens": ["Ru\u00b7hig", "um\u00b7her", "in", "der", "stil\u00b7len", "Hal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Mit Brudersinn zu heitern den Kummerblick,", "tokens": ["Mit", "Bru\u00b7der\u00b7sinn", "zu", "hei\u00b7tern", "den", "Kum\u00b7mer\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Kleinen Herz zu leiten am G\u00e4ngelband,", "tokens": ["Der", "Klei\u00b7nen", "Herz", "zu", "lei\u00b7ten", "am", "G\u00e4n\u00b7gel\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sein Haus zu baun, sein Feld zu pfl\u00fcgen,", "tokens": ["Sein", "Haus", "zu", "baun", ",", "sein", "Feld", "zu", "pfl\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ihm Beruf! und die W\u00fcnsche schweigen.", "tokens": ["Wird", "ihm", "Be\u00b7ruf", "!", "und", "die", "W\u00fcn\u00b7sche", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "KON", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Verzeih der bangen Tr\u00e4ne, du G\u00f6ttliche!", "tokens": ["Ver\u00b7zeih", "der", "ban\u00b7gen", "Tr\u00e4\u00b7ne", ",", "du", "G\u00f6tt\u00b7li\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch ich vielleicht! \u2013 zwar gl\u00fchet im Busen mir", "tokens": ["Auch", "ich", "viel\u00b7leicht", "!", "\u2013", "zwar", "gl\u00fc\u00b7het", "im", "Bu\u00b7sen", "mir"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "$.", "$(", "ADV", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Flamme rein und k\u00fchn, und ewig \u2013", "tokens": ["Die", "Flam\u00b7me", "rein", "und", "k\u00fchn", ",", "und", "e\u00b7wig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "KON", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber zur\u00fcck aus den Lorbeerhainen", "tokens": ["A\u00b7ber", "zu\u00b7r\u00fcck", "aus", "den", "Lor\u00b7beer\u00b7hai\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Stie\u00df unerweicht die Ehre den Traurenden,", "tokens": ["Stie\u00df", "un\u00b7er\u00b7weicht", "die", "Eh\u00b7re", "den", "Trau\u00b7ren\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So lang, entflohn dem lachenden Knabenspiel,", "tokens": ["So", "lang", ",", "ent\u00b7flohn", "dem", "la\u00b7chen\u00b7den", "Kna\u00b7ben\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verh\u00f6hnend all die Taumelfreuden,", "tokens": ["Ver\u00b7h\u00f6h\u00b7nend", "all", "die", "Tau\u00b7mel\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Treu und \u00f1 \u2014 \u00f1 mein Herz ihr huldigt.", "tokens": ["Treu", "und", "\u00f1", "\u00f1", "mein", "Herz", "ihr", "hul\u00b7digt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "XY", "$(", "VVIMP", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Drum \u00f6ffne du die Arme dem Traurenden,", "tokens": ["Drum", "\u00f6ff\u00b7ne", "du", "die", "Ar\u00b7me", "dem", "Trau\u00b7ren\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "La\u00df deines Labebechers mich oft und viel", "tokens": ["La\u00df", "dei\u00b7nes", "La\u00b7be\u00b7be\u00b7chers", "mich", "oft", "und", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADV", "KON", "ADV"], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und einzig kosten, nenne Sohn mich!", "tokens": ["Und", "ein\u00b7zig", "kos\u00b7ten", ",", "nen\u00b7ne", "Sohn", "mich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "VVFIN", "NN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00fcrte mit Stolz mich, und Kraft und Wahrheit!", "tokens": ["G\u00fcr\u00b7te", "mit", "Stolz", "mich", ",", "und", "Kraft", "und", "Wahr\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Denn viel der St\u00fcrme harren des J\u00fcnglings noch,", "tokens": ["Denn", "viel", "der", "St\u00fcr\u00b7me", "har\u00b7ren", "des", "J\u00fcng\u00b7lings", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der falschen Gruben viele des Wanderers,", "tokens": ["Der", "fal\u00b7schen", "Gru\u00b7ben", "vie\u00b7le", "des", "Wan\u00b7de\u00b7rers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie alle wird dein Sohn besiegen,", "tokens": ["Sie", "al\u00b7le", "wird", "dein", "Sohn", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So du mit st\u00fctzendem Arm ihn leitest.", "tokens": ["So", "du", "mit", "st\u00fct\u00b7zen\u00b7dem", "Arm", "ihn", "lei\u00b7test", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}