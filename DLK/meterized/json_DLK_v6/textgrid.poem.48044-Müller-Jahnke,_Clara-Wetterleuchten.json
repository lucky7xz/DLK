{"textgrid.poem.48044": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Wetterleuchten", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch die dichtverh\u00fcllten Fenster dringt", "tokens": ["Durch", "die", "dicht\u00b7ver\u00b7h\u00fcll\u00b7ten", "Fens\u00b7ter", "dringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "noch der letzten Blitze mattes Leuchten, \u2013", "tokens": ["noch", "der", "letz\u00b7ten", "Blit\u00b7ze", "mat\u00b7tes", "Leuch\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und die Stunde naht auf regenfeuchten", "tokens": ["und", "die", "Stun\u00b7de", "naht", "auf", "re\u00b7gen\u00b7feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ADJA"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Sohlen, die dich wieder zu mir bringt.", "tokens": ["Soh\u00b7len", ",", "die", "dich", "wie\u00b7der", "zu", "mir", "bringt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Jahre liegen zwischen dir und mir:", "tokens": ["Jah\u00b7re", "lie\u00b7gen", "zwi\u00b7schen", "dir", "und", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "KON", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Herzen, deren Pulse nicht mehr pochen,", "tokens": ["Her\u00b7zen", ",", "de\u00b7ren", "Pul\u00b7se", "nicht", "mehr", "po\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kl\u00fcfte, deren Br\u00fccken abgebrochen \u2013", "tokens": ["Kl\u00fcf\u00b7te", ",", "de\u00b7ren", "Br\u00fc\u00b7cken", "ab\u00b7ge\u00b7bro\u00b7chen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "tote Gluten, Staub und Grabeszier.", "tokens": ["to\u00b7te", "Glu\u00b7ten", ",", "Staub", "und", "Gra\u00b7bes\u00b7zier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber heut umweht von Liedern mich", "tokens": ["A\u00b7ber", "heut", "um\u00b7weht", "von", "Lie\u00b7dern", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "noch ein Ton, die einst so s\u00fc\u00df mir d\u00e4uchten,", "tokens": ["noch", "ein", "Ton", ",", "die", "einst", "so", "s\u00fc\u00df", "mir", "d\u00e4uch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und die Brust durchzuckt's wie Wetterleuchten,", "tokens": ["und", "die", "Brust", "durch\u00b7zuckt's", "wie", "Wet\u00b7ter\u00b7leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "das noch blitzt, wenn schon die Wolke wich.", "tokens": ["das", "noch", "blitzt", ",", "wenn", "schon", "die", "Wol\u00b7ke", "wich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "L\u00e4chle, ob ich finster blicken mag!", "tokens": ["L\u00e4ch\u00b7le", ",", "ob", "ich", "fins\u00b7ter", "bli\u00b7cken", "mag", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Strahle mir mit deiner Augen Sonnen", "tokens": ["Strah\u00b7le", "mir", "mit", "dei\u00b7ner", "Au\u00b7gen", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "neu ins Herz die l\u00e4ngstverrauschten Wonnen,", "tokens": ["neu", "ins", "Herz", "die", "l\u00e4ngst\u00b7ver\u00b7rauschten", "Won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "unserer Liebe heitern Fr\u00fchlingstag.", "tokens": ["un\u00b7se\u00b7rer", "Lie\u00b7be", "hei\u00b7tern", "Fr\u00fch\u00b7lings\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Aus den Schalen duftet Veilchenpracht", "tokens": ["Aus", "den", "Scha\u00b7len", "duf\u00b7tet", "Veil\u00b7chen\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "dir zum Gru\u00df, und volle Kelche sch\u00e4umen:", "tokens": ["dir", "zum", "Gru\u00df", ",", "und", "vol\u00b7le", "Kel\u00b7che", "sch\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "o, noch einmal la\u00df den Traum mich tr\u00e4umen,", "tokens": ["o", ",", "noch", "ein\u00b7mal", "la\u00df", "den", "Traum", "mich", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "ADV", "ADV", "VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "der mein Herz so selig einst gemacht!", "tokens": ["der", "mein", "Herz", "so", "se\u00b7lig", "einst", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}