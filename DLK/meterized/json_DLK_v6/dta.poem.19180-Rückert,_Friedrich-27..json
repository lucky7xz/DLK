{"dta.poem.19180": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Aug' ist \u00fcberm Ohr in allen St\u00fccken, traun,", "tokens": ["Das", "Aug'", "ist", "\u00fc\u00b7berm", "Ohr", "in", "al\u00b7len", "St\u00fc\u00b7cken", ",", "traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "APPR", "PIAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur da\u00df man nicht mit ihm kann um die Ecken schaun.", "tokens": ["Nur", "da\u00df", "man", "nicht", "mit", "ihm", "kann", "um", "die", "E\u00b7cken", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PTKNEG", "APPR", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Aug' ist \u00fcberm Ohr f\u00fcrwahr in allen St\u00fccken,", "tokens": ["Das", "Aug'", "ist", "\u00fc\u00b7berm", "Ohr", "f\u00fcr\u00b7wahr", "in", "al\u00b7len", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur da\u00df man nicht mit ihm kann sehen hinterm R\u00fccken,", "tokens": ["Nur", "da\u00df", "man", "nicht", "mit", "ihm", "kann", "se\u00b7hen", "hin\u00b7term", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PTKNEG", "APPR", "PPER", "VMFIN", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie mit dem Ohre man wol hinterm R\u00fccken h\u00f6rt,", "tokens": ["Wie", "mit", "dem", "Oh\u00b7re", "man", "wol", "hin\u00b7term", "R\u00fc\u00b7cken", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Doch auch nur Schlimmes meist, das unsre Ruhe st\u00f6rt.", "tokens": ["Doch", "auch", "nur", "Schlim\u00b7mes", "meist", ",", "das", "uns\u00b7re", "Ru\u00b7he", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}