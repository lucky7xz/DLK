{"dta.poem.3055": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Haltung fehlt; was hilfts ob ein Gehalt sich findet,", "tokens": ["Die", "Hal\u00b7tung", "fehlt", ";", "was", "hilfts", "ob", "ein", "Ge\u00b7halt", "sich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VVFIN", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, h\u00e4lt er sich nicht fest an Haltung, haltlos schwindet!", "tokens": ["Der", ",", "h\u00e4lt", "er", "sich", "nicht", "fest", "an", "Hal\u00b7tung", ",", "halt\u00b7los", "schwin\u00b7det", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "APPR", "NN", "$,", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der T\u00f6ne F\u00fcll' ist da, doch wenn der eine Ton", "tokens": ["Der", "T\u00f6\u00b7ne", "F\u00fcll'", "ist", "da", ",", "doch", "wenn", "der", "ei\u00b7ne", "Ton"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "$,", "ADV", "KOUS", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht wird gehalten, ist der Einklang auch entflohn.", "tokens": ["Nicht", "wird", "ge\u00b7hal\u00b7ten", ",", "ist", "der", "Ein\u00b7klang", "auch", "ent\u00b7flohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VVPP", "$,", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des Tanzes Wirbel rauscht, der Takt wird nicht gehalten,", "tokens": ["Des", "Tan\u00b7zes", "Wir\u00b7bel", "rauscht", ",", "der", "Takt", "wird", "nicht", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht zur Anmuth kann das Chaos sich entfalten.", "tokens": ["Und", "nicht", "zur", "An\u00b7muth", "kann", "das", "Chaos", "sich", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VMFIN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Der rechte Weg wird falsch, wenn du nicht h\u00e4ltst die Richte;", "tokens": ["Der", "rech\u00b7te", "Weg", "wird", "falsch", ",", "wenn", "du", "nicht", "h\u00e4ltst", "die", "Rich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn du es nicht h\u00e4ltst, wird das Gesetz zunichte.", "tokens": ["Und", "wenn", "du", "es", "nicht", "h\u00e4ltst", ",", "wird", "das", "Ge\u00b7setz", "zu\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Behalt und halte dis bei jeglichem Verhalten:", "tokens": ["Be\u00b7halt", "und", "hal\u00b7te", "dis", "bei", "jeg\u00b7li\u00b7chem", "Ver\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PDS", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Haltung h\u00e4lt die Welt, such' Haltung zu erhalten!", "tokens": ["Die", "Hal\u00b7tung", "h\u00e4lt", "die", "Welt", ",", "such'", "Hal\u00b7tung", "zu", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}