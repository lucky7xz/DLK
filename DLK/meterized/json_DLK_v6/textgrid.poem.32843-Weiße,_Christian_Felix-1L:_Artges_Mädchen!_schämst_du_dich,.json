{"textgrid.poem.32843": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Artges M\u00e4dchen! sch\u00e4mst du dich,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Artges M\u00e4dchen! sch\u00e4mst du dich,", "tokens": ["Art\u00b7ges", "M\u00e4d\u00b7chen", "!", "sch\u00e4mst", "du", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ein leichtes Kleid dich decket?", "tokens": ["Da\u00df", "ein", "leich\u00b7tes", "Kleid", "dich", "de\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4m dich! da\u00df darhinter sich", "tokens": ["Sch\u00e4m", "dich", "!", "da\u00df", "dar\u00b7hin\u00b7ter", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$.", "KOUS", "PAV", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu viel Sch\u00f6nheit noch verstecket!", "tokens": ["Zu", "viel", "Sch\u00f6n\u00b7heit", "noch", "ver\u00b7ste\u00b7cket", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sah man einen Palatin", "tokens": ["Sah", "man", "ei\u00b7nen", "Pa\u00b7la\u00b7tin"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Auf Cytherens Busen schwimmen?", "tokens": ["Auf", "Cy\u00b7the\u00b7rens", "Bu\u00b7sen", "schwim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oder Schnabeleisen gl\u00fchn,", "tokens": ["O\u00b7der", "Schna\u00b7be\u00b7lei\u00b7sen", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um das blonde Haar zu kr\u00fcmmen?", "tokens": ["Um", "das", "blon\u00b7de", "Haar", "zu", "kr\u00fcm\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Frey lie\u00df sie, der Weste Spiel", "tokens": ["Frey", "lie\u00df", "sie", ",", "der", "Wes\u00b7te", "Spiel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es von wei\u00dfen Schultern fallen,", "tokens": ["Es", "von", "wei\u00b7\u00dfen", "Schul\u00b7tern", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der s\u00fc\u00dfen Neugier Ziel", "tokens": ["Und", "der", "s\u00fc\u00b7\u00dfen", "Neu\u00b7gier", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolz die Brust entgegen wallen.", "tokens": ["Stolz", "die", "Brust", "ent\u00b7ge\u00b7gen", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keine Spitze, kein Gewand", "tokens": ["Kei\u00b7ne", "Spit\u00b7ze", ",", "kein", "Ge\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcckte die polirten Glieder,", "tokens": ["Dr\u00fcck\u00b7te", "die", "po\u00b7lir\u00b7ten", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur des G\u00fcrtels leichtes Band", "tokens": ["Nur", "des", "G\u00fcr\u00b7tels", "leich\u00b7tes", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lief von stolzen H\u00fcften nieder:", "tokens": ["Lief", "von", "stol\u00b7zen", "H\u00fcf\u00b7ten", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch war jedes unterthan,", "tokens": ["Doch", "war", "je\u00b7des", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ihr Blick die Herrschaft \u00fcbte,", "tokens": ["Wo", "ihr", "Blick", "die", "Herr\u00b7schaft", "\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Menschen beteten sie an,", "tokens": ["Men\u00b7schen", "be\u00b7te\u00b7ten", "sie", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der ganze Himmel liebte.", "tokens": ["Und", "der", "gan\u00b7ze", "Him\u00b7mel", "lieb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Du, an gleicher Anmuth reich,", "tokens": ["Du", ",", "an", "glei\u00b7cher", "An\u00b7muth", "reich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df dir deine Macht nicht nehmen!", "tokens": ["La\u00df", "dir", "dei\u00b7ne", "Macht", "nicht", "neh\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du bist einer G\u00f6ttin gleich,", "tokens": ["Du", "bist", "ei\u00b7ner", "G\u00f6t\u00b7tin", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kannst dich so menschlich sch\u00e4men?", "tokens": ["Und", "kannst", "dich", "so", "menschlich", "sch\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}