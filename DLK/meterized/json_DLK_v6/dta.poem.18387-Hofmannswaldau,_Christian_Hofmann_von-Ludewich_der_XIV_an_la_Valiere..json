{"dta.poem.18387": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ludewich der  XIV  an la Valiere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein k\u00f6nig Galliens/ nicht aber heisser triebe/", "tokens": ["Ein", "k\u00f6\u00b7nig", "Gal\u00b7li\u00b7ens", "/", "nicht", "a\u00b7ber", "heis\u00b7ser", "trie\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "PTKNEG", "ADV", "ADJD", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Schreibt Valiere/ dir zu klagen seinen stand.", "tokens": ["Schreibt", "Va\u00b7lie\u00b7re", "/", "dir", "zu", "kla\u00b7gen", "sei\u00b7nen", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "PPER", "PTKZU", "VVINF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Der himmel/ der mich kennt/ wei\u00df auch/ da\u00df ich dich liebe/", "tokens": ["Der", "him\u00b7mel", "/", "der", "mich", "kennt", "/", "wei\u00df", "auch", "/", "da\u00df", "ich", "dich", "lie\u00b7be", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und kostete dis wort mir scerter/ cron und land.", "tokens": ["Und", "kos\u00b7te\u00b7te", "dis", "wort", "mir", "scer\u00b7ter", "/", "cron", "und", "land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "PPER", "ADJD", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Liljen meines reichs sind denen nicht zu gleichen/", "tokens": ["Die", "Lil\u00b7jen", "mei\u00b7nes", "reichs", "sind", "de\u00b7nen", "nicht", "zu", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "VAFIN", "PDS", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die dir der bunte lentz auf wang und br\u00fcste tr\u00e4gt.", "tokens": ["Die", "dir", "der", "bun\u00b7te", "lentz", "auf", "wang", "und", "br\u00fcs\u00b7te", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und pflegt kein weibes-bild dis kleinod zu erreichen/", "tokens": ["Und", "pflegt", "kein", "wei\u00b7bes\u00b7bild", "dis", "klei\u00b7nod", "zu", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PDS", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wisse da\u00df sein F\u00fcrst sich dir zum f\u00fcssen legt.", "tokens": ["So", "wis\u00b7se", "da\u00df", "sein", "F\u00fcrst", "sich", "dir", "zum", "f\u00fcs\u00b7sen", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "PPOSAT", "NN", "PRF", "PPER", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wei\u00df nicht was ich bin/ und was ich hiermit werde/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "ich", "bin", "/", "und", "was", "ich", "hier\u00b7mit", "wer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VAFIN", "$(", "KON", "PWS", "PPER", "PAV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich blende meinen glantz/ dich deutlich anzusehn.", "tokens": ["Ich", "blen\u00b7de", "mei\u00b7nen", "glantz", "/", "dich", "deut\u00b7lich", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Carl lie\u00df ein K\u00e4yserthum mit einem ku\u00df/ der erde/", "tokens": ["Carl", "lie\u00df", "ein", "K\u00e4y\u00b7ser\u00b7thum", "mit", "ei\u00b7nem", "ku\u00df", "/", "der", "er\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$(", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich k\u00fcsse noch ein weib mein Franckreich zu verschm\u00e4hn.", "tokens": ["Ich", "k\u00fcs\u00b7se", "noch", "ein", "weib", "mein", "Fran\u00b7ck\u00b7reich", "zu", "ver\u00b7schm\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Du Valiere bist zu meinem trost erkohren;", "tokens": ["Du", "Va\u00b7lie\u00b7re", "bist", "zu", "mei\u00b7nem", "trost", "er\u00b7koh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Mein recht und Parlament halt ich in deiner schoos/", "tokens": ["Mein", "recht", "und", "Par\u00b7la\u00b7ment", "halt", "ich", "in", "dei\u00b7ner", "schoos", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "KON", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich bin so wohl vor dich als dessen hefft gebohren/", "tokens": ["Ich", "bin", "so", "wohl", "vor", "dich", "als", "des\u00b7sen", "hefft", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "KOUS", "PDS", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nur sch\u00e4tze/ wie man pflegt/ mich darzu nicht zu gro\u00df.", "tokens": ["Nur", "sch\u00e4t\u00b7ze", "/", "wie", "man", "pflegt", "/", "mich", "dar\u00b7zu", "nicht", "zu", "gro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWAV", "PIS", "VVFIN", "$(", "PPER", "PAV", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es kr\u00f6nt das regiment nur k\u00f6nig stand und h\u00e4user/", "tokens": ["Es", "kr\u00f6nt", "das", "re\u00b7gi\u00b7ment", "nur", "k\u00f6\u00b7nig", "stand", "und", "h\u00e4u\u00b7ser", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "ADV", "ADJD", "VVFIN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Allein die liebe wei\u00df von keiner Policey/", "tokens": ["Al\u00b7lein", "die", "lie\u00b7be", "wei\u00df", "von", "kei\u00b7ner", "Po\u00b7li\u00b7cey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wer am getreusten liebt/ erwei\u00dft sich einen k\u00e4yser/", "tokens": ["Wer", "am", "ge\u00b7treus\u00b7ten", "liebt", "/", "er\u00b7wei\u00dft", "sich", "ei\u00b7nen", "k\u00e4y\u00b7ser", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "VVFIN", "$(", "VVFIN", "PRF", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und der sich neigen kan erblickt wie gro\u00df er sey.", "tokens": ["Und", "der", "sich", "nei\u00b7gen", "kan", "er\u00b7blickt", "wie", "gro\u00df", "er", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "VVFIN", "VMFIN", "VVPP", "KOKOM", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich aber w\u00fcnsche ietzt ein F\u00fcrst zu seyn im lieben/", "tokens": ["Ich", "a\u00b7ber", "w\u00fcn\u00b7sche", "ietzt", "ein", "F\u00fcrst", "zu", "seyn", "im", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VAINF", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So leg ich ausser dir auch meinen Purpur hin/", "tokens": ["So", "leg", "ich", "aus\u00b7ser", "dir", "auch", "mei\u00b7nen", "Pur\u00b7pur", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "APPR", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Even sch\u00f6nheit ward aus ihres Adams riben", "tokens": ["Der", "E\u00b7ven", "sch\u00f6n\u00b7heit", "ward", "aus", "ih\u00b7res", "A\u00b7dams", "ri\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie wird mein gegenstand nicht gleichfalls was ich bin;", "tokens": ["Wie", "wird", "mein", "ge\u00b7gen\u00b7stand", "nicht", "gleich\u00b7falls", "was", "ich", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da doch die sonne selbst sich nicht kan selber speisen/", "tokens": ["Da", "doch", "die", "son\u00b7ne", "selbst", "sich", "nicht", "kan", "sel\u00b7ber", "spei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "PRF", "PTKNEG", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und meistens unter sich die heissen stralen streckt/", "tokens": ["Und", "meis\u00b7tens", "un\u00b7ter", "sich", "die", "heis\u00b7sen", "stra\u00b7len", "streckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PRF", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der Agtstein ziht nur spreu/ und der Magnet nur Eysen/", "tokens": ["Der", "Agt\u00b7stein", "ziht", "nur", "spreu", "/", "und", "der", "Mag\u00b7net", "nur", "Ey\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$(", "KON", "ART", "NN", "ADV", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Was wunder wenn auch uns der seichte zucker schmeckt.", "tokens": ["Was", "wun\u00b7der", "wenn", "auch", "uns", "der", "seich\u00b7te", "zu\u00b7cker", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KOUS", "ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wiewohl du werthe bist kein irdenes geth\u00f6ne/", "tokens": ["Wie\u00b7wohl", "du", "wert\u00b7he", "bist", "kein", "ir\u00b7de\u00b7nes", "ge\u00b7th\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Perlen-muschel hat dich/ Venus/ uns gebracht.", "tokens": ["Die", "Per\u00b7len\u00b7mu\u00b7schel", "hat", "dich", "/", "Ve\u00b7nus", "/", "uns", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$(", "NE", "$(", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Moren wehlten sich beherrscher nach der sch\u00f6ne/", "tokens": ["Die", "Mo\u00b7ren", "wehl\u00b7ten", "sich", "be\u00b7herr\u00b7scher", "nach", "der", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich wei\u00df/ da\u00df di\u00df allein dich zur regentin macht.", "tokens": ["Ich", "wei\u00df", "/", "da\u00df", "di\u00df", "al\u00b7lein", "dich", "zur", "re\u00b7gen\u00b7tin", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PDS", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "So werde denn noch heut/ regentin meiner seelen/", "tokens": ["So", "wer\u00b7de", "denn", "noch", "heut", "/", "re\u00b7gen\u00b7tin", "mei\u00b7ner", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ADV", "$(", "NE", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was du nach meinen wunsch/ vorl\u00e4ngst gewesen bist/", "tokens": ["Was", "du", "nach", "mei\u00b7nen", "wunsch", "/", "vor\u00b7l\u00e4ngst", "ge\u00b7we\u00b7sen", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "$(", "ADV", "VAPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das ewge feuer brennt zwar nicht in allen h\u00f6len/", "tokens": ["Das", "ew\u00b7ge", "feu\u00b7er", "brennt", "zwar", "nicht", "in", "al\u00b7len", "h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKNEG", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Doch gl\u00e4ube/ da\u00df bey mir was unausl\u00f6schlichs ist.", "tokens": ["Doch", "gl\u00e4u\u00b7be", "/", "da\u00df", "bey", "mir", "was", "un\u00b7aus\u00b7l\u00f6schlichs", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "APPR", "PPER", "PIS", "ADJA", "VAFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.37": {"text": "Mein hertze/ das durch dich in meiner brust entglommen/", "tokens": ["Mein", "hert\u00b7ze", "/", "das", "durch", "dich", "in", "mei\u00b7ner", "brust", "ent\u00b7glom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "APPR", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Hat auch auf ewig dir geschworen seinen brandt.", "tokens": ["Hat", "auch", "auf", "e\u00b7wig", "dir", "ge\u00b7schwo\u00b7ren", "sei\u00b7nen", "brandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJD", "PPER", "VVPP", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Du siehest feur und herd/ wie kan es dir entkommen/", "tokens": ["Du", "sie\u00b7hest", "feur", "und", "herd", "/", "wie", "kan", "es", "dir", "ent\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "NN", "$(", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Verware dies nur selbst/ du hast es in der hand.", "tokens": ["Ver\u00b7wa\u00b7re", "dies", "nur", "selbst", "/", "du", "hast", "es", "in", "der", "hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "ADV", "ADV", "$(", "PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Du bist die Vesta mir die flamme zu erhalten/", "tokens": ["Du", "bist", "die", "Ves\u00b7ta", "mir", "die", "flam\u00b7me", "zu", "er\u00b7hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich aber trage feur den liebes-Ampeln zu.", "tokens": ["Ich", "a\u00b7ber", "tra\u00b7ge", "feur", "den", "lie\u00b7bes\u00b7Am\u00b7peln", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "La\u00df durch verachtung nicht das opffer-\u00f6l erkalten/", "tokens": ["La\u00df", "durch", "ver\u00b7ach\u00b7tung", "nicht", "das", "opf\u00b7fer\u00b7\u00f6l", "er\u00b7kal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "NN", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und weise/ da\u00df dein hertz so lieblich sey als du.", "tokens": ["Und", "wei\u00b7se", "/", "da\u00df", "dein", "hertz", "so", "lieb\u00b7lich", "sey", "als", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "KOUS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ein schatz/ der dein zu seyn viel schl\u00f6sser hat durchbrochen/", "tokens": ["Ein", "schatz", "/", "der", "dein", "zu", "seyn", "viel", "schl\u00f6s\u00b7ser", "hat", "durch\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PPOSAT", "PTKZU", "VAINF", "ADV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Kan dir/ mein auffenthalt/ nicht wol zuwider seyn.", "tokens": ["Kan", "dir", "/", "mein", "auf\u00b7fent\u00b7halt", "/", "nicht", "wol", "zu\u00b7wi\u00b7der", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "PPOSAT", "NN", "$(", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und was dich durch sich selbst zu einen raub bestochen/", "tokens": ["Und", "was", "dich", "durch", "sich", "selbst", "zu", "ei\u00b7nen", "raub", "be\u00b7sto\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Schlist billig deine hand geheimen zimmern ein.", "tokens": ["Schlist", "bil\u00b7lig", "dei\u00b7ne", "hand", "ge\u00b7hei\u00b7men", "zim\u00b7mern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "So liebe/ was dich liebt/ und dich erwehlt f\u00fcr allen/", "tokens": ["So", "lie\u