{"textgrid.poem.36650": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Cori", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Berge sie bl\u00fchn in parthenop\u00e4ischer F\u00fclle,", "tokens": ["Dei\u00b7ne", "Ber\u00b7ge", "sie", "bl\u00fchn", "in", "par\u00b7the\u00b7no\u00b7p\u00e4\u00b7i\u00b7scher", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "S\u00fcdliche L\u00fcfte, wie froh gr\u00fc\u00dft' euch mein Auge, mein Herz!", "tokens": ["S\u00fcd\u00b7li\u00b7che", "L\u00fcf\u00b7te", ",", "wie", "froh", "gr\u00fc\u00dft'", "euch", "mein", "Au\u00b7ge", ",", "mein", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ja, hier bin ich bezaubert, und w\u00e4r's auch ein Wunder, vom Meere", "tokens": ["Ja", ",", "hier", "bin", "ich", "be\u00b7zau\u00b7bert", ",", "und", "w\u00e4r's", "auch", "ein", "Wun\u00b7der", ",", "vom", "Mee\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,", "KON", "VAFIN", "ADV", "ART", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Lockt ja die griechische Fee mich in ihr magisches Netz.", "tokens": ["Lockt", "ja", "die", "grie\u00b7chi\u00b7sche", "Fee", "mich", "in", "ihr", "ma\u00b7gi\u00b7sches", "Netz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}