{"dta.poem.19592": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Mit zwei Worten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Am Gestade Pal\u00e4stina's, auf und nieder, Tag um Tag,", "tokens": ["Am", "Ge\u00b7sta\u00b7de", "Pa\u00b7l\u00e4s\u00b7ti\u00b7na's", ",", "auf", "und", "nie\u00b7der", ",", "Tag", "um", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "APPR", "KON", "PTKVZ", "$,", "NN", "APPR", "NN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "\u201elondon?\u201c frug die Sarazenin, wo ein Schiff vor Anker lag.", "tokens": ["\u201e", "lon\u00b7don", "?", "\u201c", "frug", "die", "Sa\u00b7ra\u00b7ze\u00b7nin", ",", "wo", "ein", "Schiff", "vor", "An\u00b7ker", "lag", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "\u201elondon!\u201c bat sie lang vergebens, nimmer ward sie m\u00fcd und zag,", "tokens": ["\u201e", "lon\u00b7don", "!", "\u201c", "bat", "sie", "lang", "ver\u00b7ge\u00b7bens", ",", "nim\u00b7mer", "ward", "sie", "m\u00fcd", "und", "zag", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "ADJD", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bis zuletzt an Bord sie brachte eines Bootes Ruderschlag.", "tokens": ["Bis", "zu\u00b7letzt", "an", "Bord", "sie", "brach\u00b7te", "ei\u00b7nes", "Boo\u00b7tes", "Ru\u00b7der\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "NN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Sie betrat das Deck des Seglers und ihr wurde nicht gewehrt.", "tokens": ["Sie", "be\u00b7trat", "das", "Deck", "des", "Seg\u00b7lers", "und", "ihr", "wur\u00b7de", "nicht", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Meer und Himmel. \u201eLondon?\u201c frug sie, von der Heimath abgekehrt,", "tokens": ["Meer", "und", "Him\u00b7mel", ".", "\u201e", "Lon\u00b7don", "?", "\u201c", "frug", "sie", ",", "von", "der", "Hei\u00b7math", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "$(", "NE", "$.", "$(", "VVFIN", "PPER", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Suchte, blickte, durch des Schiffers ausgestreckte Hand belehrt,", "tokens": ["Such\u00b7te", ",", "blick\u00b7te", ",", "durch", "des", "Schif\u00b7fers", "aus\u00b7ge\u00b7streck\u00b7te", "Hand", "be\u00b7lehrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nach den K\u00fcsten wo die Sonne sich in Abendgluth verzehrt ...", "tokens": ["Nach", "den", "K\u00fcs\u00b7ten", "wo", "die", "Son\u00b7ne", "sich", "in", "A\u00b7bend\u00b7gluth", "ver\u00b7zehrt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PWAV", "ART", "NN", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u201egilbert?\u201c fragt die Sarazenin im Gedr\u00e4ng der gro\u00dfen Stadt,", "tokens": ["\u201e", "gil\u00b7bert", "?", "\u201c", "fragt", "die", "Sa\u00b7ra\u00b7ze\u00b7nin", "im", "Ge\u00b7dr\u00e4ng", "der", "gro\u00b7\u00dfen", "Stadt", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und die Menge lacht und spottet, bis sie dann Erbarmen hat.", "tokens": ["Und", "die", "Men\u00b7ge", "lacht", "und", "spot\u00b7tet", ",", "bis", "sie", "dann", "Er\u00b7bar\u00b7men", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "\u201etausend Gilbert giebt's in London!\u201c Doch sie schreitet nimmer", "tokens": ["\u201e", "tau\u00b7send", "Gil\u00b7bert", "giebt's", "in", "Lon\u00b7don", "!", "\u201c", "Doch", "sie", "schrei\u00b7tet", "nim\u00b7mer"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "CARD", "NE", "VVFIN", "APPR", "NE", "$.", "$(", "KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "\u201elabe dich mit Trank und Speise!\u201c Doch sie wird von Thr\u00e4nen satt.", "tokens": ["\u201e", "la\u00b7be", "dich", "mit", "Trank", "und", "Spei\u00b7se", "!", "\u201c", "Doch", "sie", "wird", "von", "Thr\u00e4\u00b7nen", "satt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$.", "$(", "KON", "PPER", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "\u201egilbert!\u201c \u201eNichts als Gilbert? wei\u00dft du keine andern", "tokens": ["\u201e", "gil\u00b7bert", "!", "\u201c", "\u201e", "Nichts", "als", "Gil\u00b7bert", "?", "wei\u00dft", "du", "kei\u00b7ne", "an\u00b7dern"], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "$(", "$(", "PIS", "KOKOM", "NE", "$.", "VVFIN", "PPER", "PIAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u201egilbert!\u201c ... \u201eH\u00f6rt, das wird der weiland Pilger Gilbert", "tokens": ["\u201e", "gil\u00b7bert", "!", "\u201c", "...", "\u201e", "H\u00f6rt", ",", "das", "wird", "der", "wei\u00b7land", "Pil\u00b7ger", "Gil\u00b7bert"], "token_info": ["punct", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "$(", "$(", "$(", "VVIMP", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Den gebr\u00e4unt in Sklavenketten gl\u00fcher W\u00fcste Sonnenschein \u2014", "tokens": ["Den", "ge\u00b7br\u00e4unt", "in", "Skla\u00b7ven\u00b7ket\u00b7ten", "gl\u00fc\u00b7her", "W\u00fcs\u00b7te", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJA", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dem die Bande l\u00f6ste heimlich eines Emirs T\u00f6chterlein \u2014\u201c", "tokens": ["Dem", "die", "Ban\u00b7de", "l\u00f6s\u00b7te", "heim\u00b7lich", "ei\u00b7nes", "E\u00b7mirs", "T\u00f6ch\u00b7ter\u00b7lein", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$(", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "\u201epilgrim Gilbert Becket!\u201c dr\u00f6hnt es, braust es l\u00e4ngs der Themse", "tokens": ["\u201e", "pil\u00b7grim", "Gil\u00b7bert", "Be\u00b7cket", "!", "\u201c", "dr\u00f6hnt", "es", ",", "braust", "es", "l\u00e4ngs", "der", "Them\u00b7se"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "NE", "NE", "$.", "$(", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Sieh, da kommt er ihr entgegen, von des Volkes Mund genannt,", "tokens": ["Sieh", ",", "da", "kommt", "er", "ihr", "ent\u00b7ge\u00b7gen", ",", "von", "des", "Vol\u00b7kes", "Mund", "ge\u00b7nannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Ueber seine Schwelle f\u00fchrt er, die das Ziel der Reise fand.", "tokens": ["Ue\u00b7ber", "sei\u00b7ne", "Schwel\u00b7le", "f\u00fchrt", "er", ",", "die", "das", "Ziel", "der", "Rei\u00b7se", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Liebe wandert mit zwei Worten gl\u00e4ubig \u00fcber Meer und Land.", "tokens": ["Lie\u00b7be", "wan\u00b7dert", "mit", "zwei", "Wor\u00b7ten", "gl\u00e4u\u00b7big", "\u00fc\u00b7ber", "Meer", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "CARD", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}