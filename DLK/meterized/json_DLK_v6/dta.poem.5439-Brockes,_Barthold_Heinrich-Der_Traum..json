{"dta.poem.5439": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Traum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lag, voll schwartzer bittrer Sorgen,", "tokens": ["Ich", "lag", ",", "voll", "schwart\u00b7zer", "bit\u00b7trer", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In eines Kerckers Gruft und dunckler Nacht verborgen;", "tokens": ["In", "ei\u00b7nes", "Ker\u00b7ckers", "Gruft", "und", "dunck\u00b7ler", "Nacht", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Fessel dr\u00fcckten mich; doch mehr noch, als die Bande,", "tokens": ["Die", "Fes\u00b7sel", "dr\u00fcck\u00b7ten", "mich", ";", "doch", "mehr", "noch", ",", "als", "die", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADV", "ADV", "ADV", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Furcht der k\u00fcnftigen Verachtung, Straff\u2019 und Schande", "tokens": ["Die", "Furcht", "der", "k\u00fcnf\u00b7ti\u00b7gen", "Ver\u00b7ach\u00b7tung", ",", "Straff'", "und", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da dacht\u2019 ich, welch ein Schatz die g\u00fcldne Freyheit sey.", "tokens": ["Da", "dacht'", "ich", ",", "welch", "ein", "Schatz", "die", "g\u00fcld\u00b7ne", "Frey\u00b7heit", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAT", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich that die Augen auf, und fand mich w\u00fcrcklich frey,", "tokens": ["Ich", "that", "die", "Au\u00b7gen", "auf", ",", "und", "fand", "mich", "w\u00fcrck\u00b7lich", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von aller Furcht erl\u00f6s\u2019t, von aller Pein genesen;", "tokens": ["Von", "al\u00b7ler", "Furcht", "er\u00b7l\u00f6s't", ",", "von", "al\u00b7ler", "Pein", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn meine Noth, (GOtt Lob) war nur ein Traum gewesen.", "tokens": ["Denn", "mei\u00b7ne", "Noth", ",", "(", "Gott", "Lob", ")", "war", "nur", "ein", "Traum", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "$(", "NN", "NN", "$(", "VAFIN", "ADV", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich danckte billig GOtt. Doch dacht\u2019 ich noch dabey,", "tokens": ["Ich", "danck\u00b7te", "bil\u00b7lig", "Gott", ".", "Doch", "dacht'", "ich", "noch", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$.", "KON", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob etwann die Melancholey,", "tokens": ["Ob", "et\u00b7wann", "die", "Me\u00b7lan\u00b7cho\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Womit sich viele Geister qv\u00e4len,", "tokens": ["Wo\u00b7mit", "sich", "vie\u00b7le", "Geis\u00b7ter", "qv\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht einem schweren Traum der Seelen", "tokens": ["Nicht", "ei\u00b7nem", "schwe\u00b7ren", "Traum", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Gar f\u00fcglich zu vergleichen sey?", "tokens": ["Gar", "f\u00fcg\u00b7lich", "zu", "ver\u00b7glei\u00b7chen", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Noch mehr: ob nicht ein Traum uns k\u00f6nn\u2019 ein Vorbild", "tokens": ["Noch", "mehr", ":", "ob", "nicht", "ein", "Traum", "uns", "k\u00f6nn'", "ein", "Vor\u00b7bild"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$.", "KOUS", "PTKNEG", "ART", "NN", "PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "geben", "tokens": ["ge\u00b7ben"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Von einer Seelen-Pein, so gleich nach diesem Leben?", "tokens": ["Von", "ei\u00b7ner", "See\u00b7len\u00b7Pein", ",", "so", "gleich", "nach", "die\u00b7sem", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}