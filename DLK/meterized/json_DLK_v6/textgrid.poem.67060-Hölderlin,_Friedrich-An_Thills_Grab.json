{"textgrid.poem.67060": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Thills Grab", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Leichenreihen wandelte still hinan,", "tokens": ["Der", "Lei\u00b7chen\u00b7rei\u00b7hen", "wan\u00b7del\u00b7te", "still", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Fackelnschimmer schien auf des Teuren Sarg,", "tokens": ["Und", "Fa\u00b7ckeln\u00b7schim\u00b7mer", "schien", "auf", "des", "Teu\u00b7ren", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und du, geliebte, gute Mutter!", "tokens": ["Und", "du", ",", "ge\u00b7lieb\u00b7te", ",", "gu\u00b7te", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schautest entseelt aus der Jammerh\u00fctte,", "tokens": ["Schau\u00b7test", "ent\u00b7seelt", "aus", "der", "Jam\u00b7mer\u00b7h\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Als ich, ein schwacher, stammelnder Knabe noch,", "tokens": ["Als", "ich", ",", "ein", "schwa\u00b7cher", ",", "stam\u00b7meln\u00b7der", "Kna\u00b7be", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Vater! lieber Seliger! dich verlor,", "tokens": ["O", "Va\u00b7ter", "!", "lie\u00b7ber", "Se\u00b7li\u00b7ger", "!", "dich", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da f\u00fchlt ichs nicht, was du mir warst, doch", "tokens": ["Da", "f\u00fchlt", "ichs", "nicht", ",", "was", "du", "mir", "warst", ",", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "$,", "PWS", "PPER", "PPER", "VAFIN", "$,", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mi\u00dfte dich bald der verla\u00dfne Waise.", "tokens": ["Mi\u00df\u00b7te", "dich", "bald", "der", "ver\u00b7la\u00df\u00b7ne", "Wai\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "So weint ich leisen Knabengef\u00fchles schon,", "tokens": ["So", "weint", "ich", "lei\u00b7sen", "Kna\u00b7ben\u00b7ge\u00b7f\u00fch\u00b7les", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Wehmut Tr\u00e4ne \u00fcber dein traurig Los,", "tokens": ["Der", "Weh\u00b7mut", "Tr\u00e4\u00b7ne", "\u00fc\u00b7ber", "dein", "trau\u00b7rig", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "ADJD", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch jetzt, o Thill! jetzt f\u00fchl ichs ernster,", "tokens": ["Doch", "jetzt", ",", "o", "Thill", "!", "jetzt", "f\u00fchl", "ichs", "erns\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NE", "$.", "ADV", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schmerzender jetzt \u00fcber deinem H\u00fcgel,", "tokens": ["Schmer\u00b7zen\u00b7der", "jetzt", "\u00fc\u00b7ber", "dei\u00b7nem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Was hier im Grab den Redlichen Suevias", "tokens": ["Was", "hier", "im", "Grab", "den", "Red\u00b7li\u00b7chen", "Sue\u00b7vi\u00b7as"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPRART", "NN", "ART", "NN", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Verwest, den himmelnahenden Einsamen.", "tokens": ["Ver\u00b7west", ",", "den", "him\u00b7mel\u00b7na\u00b7hen\u00b7den", "Ein\u00b7sa\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und, o mein Thill ! du lie\u00dfst sie Waisen?", "tokens": ["Und", ",", "o", "mein", "Thill", "!", "du", "lie\u00df\u00b7st", "sie", "Wai\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eiltest so fr\u00fche dahin, du Guter?", "tokens": ["Eil\u00b7test", "so", "fr\u00fc\u00b7he", "da\u00b7hin", ",", "du", "Gu\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "PAV", "$,", "PPER", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Ihr stille Schatten seines Holunderbaums!", "tokens": ["Ihr", "stil\u00b7le", "Schat\u00b7ten", "sei\u00b7nes", "Ho\u00b7lun\u00b7der\u00b7baums", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Verbergt mich, da\u00df kein Sp\u00f6tter die Tr\u00e4nen sieht", "tokens": ["Ver\u00b7bergt", "mich", ",", "da\u00df", "kein", "Sp\u00f6t\u00b7ter", "die", "Tr\u00e4\u00b7nen", "sieht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und lacht, wann ich geschmiegt an seinen", "tokens": ["Und", "lacht", ",", "wann", "ich", "ge\u00b7schmiegt", "an", "sei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVPP", "APPR", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcgel die bebenden Wangen trockne.", "tokens": ["H\u00fc\u00b7gel", "die", "be\u00b7ben\u00b7den", "Wan\u00b7gen", "trock\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "O wohl dir! wohl dir, Guter! du schl\u00e4fst so sanft", "tokens": ["O", "wohl", "dir", "!", "wohl", "dir", ",", "Gu\u00b7ter", "!", "du", "schl\u00e4fst", "so", "sanft"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "$.", "ADV", "PPER", "$,", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Im stillen Schatten deines Holunderbaums.", "tokens": ["Im", "stil\u00b7len", "Schat\u00b7ten", "dei\u00b7nes", "Ho\u00b7lun\u00b7der\u00b7baums", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dein Monument ist er, und deine", "tokens": ["Dein", "Mo\u00b7nu\u00b7ment", "ist", "er", ",", "und", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,", "KON", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lieder bewahren des Dorfes Greisen.", "tokens": ["Lie\u00b7der", "be\u00b7wah\u00b7ren", "des", "Dor\u00b7fes", "Grei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "O da\u00df auch mich dein H\u00fcgel umschattete", "tokens": ["O", "da\u00df", "auch", "mich", "dein", "H\u00fc\u00b7gel", "um\u00b7schat\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Hand in Hand wir schliefen, bis Ernte wird,", "tokens": ["Und", "Hand", "in", "Hand", "wir", "schlie\u00b7fen", ",", "bis", "Ern\u00b7te", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PPER", "VVFIN", "$,", "KOUS", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da schielten keine Vorurteile,", "tokens": ["Da", "schiel\u00b7ten", "kei\u00b7ne", "Vor\u00b7ur\u00b7tei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lachte kein Affe des stillen Pilgers.", "tokens": ["Lach\u00b7te", "kein", "Af\u00b7fe", "des", "stil\u00b7len", "Pil\u00b7gers", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "O Thill! Ich zage, denn er ist dornenvoll,", "tokens": ["O", "Thill", "!", "Ich", "za\u00b7ge", ",", "denn", "er", "ist", "dor\u00b7nen\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und noch so fern, der Pfad zur Vollkommenheit;", "tokens": ["Und", "noch", "so", "fern", ",", "der", "Pfad", "zur", "Voll\u00b7kom\u00b7men\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Starken beugen ja ihr Haupt, wie", "tokens": ["Die", "Star\u00b7ken", "beu\u00b7gen", "ja", "ihr", "Haupt", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "PWAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mag ihn erk\u00e4mpfen der schwache J\u00fcngling?", "tokens": ["Mag", "ihn", "er\u00b7k\u00e4mp\u00b7fen", "der", "schwa\u00b7che", "J\u00fcng\u00b7ling", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Doch nein! ich wags! es streitet zur Seite ja", "tokens": ["Doch", "nein", "!", "ich", "wags", "!", "es", "strei\u00b7tet", "zur", "Sei\u00b7te", "ja"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "PPER", "ADV", "$.", "PPER", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein felsentreuer, mutiger Bruder mir.", "tokens": ["Ein", "fel\u00b7sen\u00b7treu\u00b7er", ",", "mu\u00b7ti\u00b7ger", "Bru\u00b7der", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O freut euch, selige Gebeine!", "tokens": ["O", "freut", "euch", ",", "se\u00b7li\u00b7ge", "Ge\u00b7bei\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00dcber dem Namen! Es ist \u2013 mein Neuffer.", "tokens": ["\u00dc\u00b7ber", "dem", "Na\u00b7men", "!", "Es", "ist", "\u2013", "mein", "Neuf\u00b7fer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PPER", "VAFIN", "$(", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}