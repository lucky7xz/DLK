{"dta.poem.11029": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben  \n an  Mademoiselle  Weisen  \n in Merseburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ist mir? trau ich meinen Augen?", "tokens": ["Wie", "ist", "mir", "?", "trau", "ich", "mei\u00b7nen", "Au\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Betrieget mich die ", "tokens": ["Be\u00b7trie\u00b7get", "mich", "die"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ART"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.3": {"text": "Es kann nur die ", "tokens": ["Es", "kann", "nur", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Solch k\u00fcnstlich Werk zu wirken, taugen.", "tokens": ["Solch", "k\u00fcnst\u00b7lich", "Werk", "zu", "wir\u00b7ken", ",", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ADJD", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wie? wer hat sie je so sch\u00f6n,", "tokens": ["Doch", "wie", "?", "wer", "hat", "sie", "je", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PWS", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Zwirn, ihr Werk verrichten sehn!", "tokens": ["Mit", "Zwirn", ",", "ihr", "Werk", "ver\u00b7rich\u00b7ten", "sehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}