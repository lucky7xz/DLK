{"textgrid.poem.55915": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Auszug", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun fortzugehn von alledem Verworrnen,", "tokens": ["Nun", "fort\u00b7zu\u00b7gehn", "von", "al\u00b7le\u00b7dem", "Ver\u00b7worr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "das unser ist und uns doch nicht geh\u00f6rt,", "tokens": ["das", "un\u00b7ser", "ist", "und", "uns", "doch", "nicht", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KON", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das, wie das Wasser in den alten Bornen,", "tokens": ["das", ",", "wie", "das", "Was\u00b7ser", "in", "den", "al\u00b7ten", "Bor\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "uns zitternd spiegelt und das Bild zerst\u00f6rt;", "tokens": ["uns", "zit\u00b7ternd", "spie\u00b7gelt", "und", "das", "Bild", "zer\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VVFIN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "von allem diesen, das sich wie mit Dornen", "tokens": ["von", "al\u00b7lem", "die\u00b7sen", ",", "das", "sich", "wie", "mit", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "PDAT", "$,", "PRELS", "PRF", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "noch einmal an uns anh\u00e4ngt \u2013 fortzugehn", "tokens": ["noch", "ein\u00b7mal", "an", "uns", "an\u00b7h\u00e4ngt", "\u2013", "fort\u00b7zu\u00b7gehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVFIN", "$(", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und Das und Den,", "tokens": ["und", "Das", "und", "Den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KON", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "die man schon nicht mehr sah", "tokens": ["die", "man", "schon", "nicht", "mehr", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "(so t\u00e4glich waren sie und so gew\u00f6hnlich),", "tokens": ["(", "so", "t\u00e4g\u00b7lich", "wa\u00b7ren", "sie", "und", "so", "ge\u00b7w\u00f6hn\u00b7lich", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "PPER", "KON", "ADV", "ADJD", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "auf einmal anzuschauen: sanft, vers\u00f6hnlich", "tokens": ["auf", "ein\u00b7mal", "an\u00b7zu\u00b7schau\u00b7en", ":", "sanft", ",", "ver\u00b7s\u00f6hn\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADV", "VVIZU", "$.", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "und wie an einem Anfang und von nah;", "tokens": ["und", "wie", "an", "ei\u00b7nem", "An\u00b7fang", "und", "von", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "KON", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "und ahnend einzusehn, wie unpers\u00f6nlich,", "tokens": ["und", "ah\u00b7nend", "ein\u00b7zu\u00b7sehn", ",", "wie", "un\u00b7per\u00b7s\u00f6n\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "wie \u00fcber alle hin das Leid geschah,", "tokens": ["wie", "\u00fc\u00b7ber", "al\u00b7le", "hin", "das", "Leid", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "von dem die Kindheit voll war bis zum Rand \u2013:", "tokens": ["von", "dem", "die", "Kind\u00b7heit", "voll", "war", "bis", "zum", "Rand", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADJD", "VAFIN", "APPR", "APPRART", "NN", "$(", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und dann doch fortzugehen, Hand aus Hand,", "tokens": ["Und", "dann", "doch", "fort\u00b7zu\u00b7ge\u00b7hen", ",", "Hand", "aus", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "als ob man ein Geheiltes neu zerrisse,", "tokens": ["als", "ob", "man", "ein", "Ge\u00b7heil\u00b7tes", "neu", "zer\u00b7ris\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "und fortzugehn: wohin? Ins Ungewisse,", "tokens": ["und", "fort\u00b7zu\u00b7gehn", ":", "wo\u00b7hin", "?", "Ins", "Un\u00b7ge\u00b7wis\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PWAV", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "weit in ein unverwandtes warmes Land,", "tokens": ["weit", "in", "ein", "un\u00b7ver\u00b7wand\u00b7tes", "war\u00b7mes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "das hinter allem Handeln wie Kulisse", "tokens": ["das", "hin\u00b7ter", "al\u00b7lem", "Han\u00b7deln", "wie", "Ku\u00b7lis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PIS", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "gleichg\u00fcltig sein wird: Garten oder Wand;", "tokens": ["gleich\u00b7g\u00fcl\u00b7tig", "sein", "wird", ":", "Gar\u00b7ten", "o\u00b7der", "Wand", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAINF", "VAFIN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "und fortzugehn: warum? Aus Drang, aus Artung,", "tokens": ["und", "fort\u00b7zu\u00b7gehn", ":", "wa\u00b7rum", "?", "Aus", "Drang", ",", "aus", "Ar\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PWAV", "$.", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "aus Ungeduld, aus dunkler Erwartung,", "tokens": ["aus", "Un\u00b7ge\u00b7duld", ",", "aus", "dunk\u00b7ler", "Er\u00b7war\u00b7tung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "aus Unverst\u00e4ndlichkeit und Unverstand:", "tokens": ["aus", "Un\u00b7ver\u00b7st\u00e4nd\u00b7lich\u00b7keit", "und", "Un\u00b7ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dies alles auf sich nehmen und vergebens", "tokens": ["Dies", "al\u00b7les", "auf", "sich", "neh\u00b7men", "und", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "APPR", "PRF", "VVINF", "KON", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "vielleicht Gehaltnes fallen lassen, um", "tokens": ["viel\u00b7leicht", "Ge\u00b7halt\u00b7nes", "fal\u00b7len", "las\u00b7sen", ",", "um"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "VVINF", "VVINF", "$,", "KOUI"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "allein zu sterben, wissend nicht warum \u2013", "tokens": ["al\u00b7lein", "zu", "ster\u00b7ben", ",", "wis\u00b7send", "nicht", "wa\u00b7rum", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,", "ADJD", "PTKNEG", "PWAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ist das der Eingang eines neuen Lebens?", "tokens": ["Ist", "das", "der", "Ein\u00b7gang", "ei\u00b7nes", "neu\u00b7en", "Le\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}