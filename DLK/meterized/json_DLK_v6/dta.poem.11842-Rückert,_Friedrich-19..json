{"dta.poem.11842": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soviel ist auf der Welt, was Herzen trennt und einet,", "tokens": ["So\u00b7viel", "ist", "auf", "der", "Welt", ",", "was", "Her\u00b7zen", "trennt", "und", "ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "$,", "PWS", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df kein Verband und kein Zerspalt unm\u00f6glich scheinet.", "tokens": ["Da\u00df", "kein", "Ver\u00b7band", "und", "kein", "Zer\u00b7spalt", "un\u00b7m\u00f6g\u00b7lich", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das unzertrennlich schien und unveruneinbar,", "tokens": ["Das", "un\u00b7zer\u00b7trenn\u00b7lich", "schien", "und", "un\u00b7ver\u00b7un\u00b7ein\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun unvereinbar scheint sich das getrennte Paar.", "tokens": ["Nun", "un\u00b7ver\u00b7ein\u00b7bar", "scheint", "sich", "das", "ge\u00b7trenn\u00b7te", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wieder wenn es sich verbunden wird erkennen,", "tokens": ["Und", "wie\u00b7der", "wenn", "es", "sich", "ver\u00b7bun\u00b7den", "wird", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "PRF", "VVPP", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ists unbegreiflich ihm, wie es sich konnte trennen.", "tokens": ["Ists", "un\u00b7be\u00b7greif\u00b7lich", "ihm", ",", "wie", "es", "sich", "konn\u00b7te", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "$,", "PWAV", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Was Wunder, wenn sich hier so viel bald st\u00f6\u00dft bald zieht,", "tokens": ["Was", "Wun\u00b7der", ",", "wenn", "sich", "hier", "so", "viel", "bald", "st\u00f6\u00dft", "bald", "zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PRF", "ADV", "ADV", "ADV", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Tod und Leben selbst sich ewig sucht und flieht!", "tokens": ["Wo", "Tod", "und", "Le\u00b7ben", "selbst", "sich", "e\u00b7wig", "sucht", "und", "flieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "ADV", "PRF", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}