{"textgrid.poem.60922": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf unersteiglichem Felsen und nicht zug\u00e4nglich der Habsucht,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf unersteiglichem Felsen und nicht zug\u00e4nglich der Habsucht,", "tokens": ["Auf", "un\u00b7er\u00b7steig\u00b7li\u00b7chem", "Fel\u00b7sen", "und", "nicht", "zu\u00b7g\u00e4ng\u00b7lich", "der", "Hab\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Blieb ich in Einfachheit alten Gesetzen getreu.", "tokens": ["Blieb", "ich", "in", "Ein\u00b7fach\u00b7heit", "al\u00b7ten", "Ge\u00b7set\u00b7zen", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Weithin \u00fcber das Meer bis nach den illyrischen Ufern,", "tokens": ["Weit\u00b7hin", "\u00fc\u00b7ber", "das", "Meer", "bis", "nach", "den", "il\u00b7ly\u00b7ri\u00b7schen", "U\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "\u00dcbers Gebirg weithin, wo die Marecchia flie\u00dft", "tokens": ["\u00dc\u00b7bers", "Ge\u00b7birg", "weit\u00b7hin", ",", "wo", "die", "Ma\u00b7rec\u00b7chia", "flie\u00dft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Durch Eichw\u00e4lder und lachende T\u00e4ler und tausenderlei Gr\u00fcn,", "tokens": ["Durch", "Eich\u00b7w\u00e4l\u00b7der", "und", "la\u00b7chen\u00b7de", "T\u00e4\u00b7ler", "und", "tau\u00b7sen\u00b7der\u00b7lei", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "KON", "CARD", "NN", "$,"], "meter": "-+---+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Magst du von mir wegsehn, stehend im Neste des Aars.", "tokens": ["Magst", "du", "von", "mir", "weg\u00b7sehn", ",", "ste\u00b7hend", "im", "Nes\u00b7te", "des", "Aars", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}