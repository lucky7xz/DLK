{"dta.poem.9892": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin schnell wie wasser ist/ ein tyrann der welt und k\u00f6nig/", "tokens": ["Ich", "bin", "schnell", "wie", "was\u00b7ser", "ist", "/", "ein", "ty\u00b7rann", "der", "welt", "und", "k\u00f6\u00b7nig", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ADJD", "VAFIN", "$(", "ART", "NN", "ART", "NN", "KON", "ADJD", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Aller m\u00f6rd- und vater auch: vor mir gilt die st\u00e4rcke wenig/", "tokens": ["Al\u00b7ler", "m\u00f6rd", "und", "va\u00b7ter", "auch", ":", "vor", "mir", "gilt", "die", "st\u00e4r\u00b7cke", "we\u00b7nig", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "TRUNC", "KON", "NN", "ADV", "$.", "APPR", "PPER", "VVFIN", "ART", "VVFIN", "PIS", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Alles theils durch liebe wird/ theils durch zwang von mir", "tokens": ["Al\u00b7les", "theils", "durch", "lie\u00b7be", "wird", "/", "theils", "durch", "zwang", "von", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "ADJA", "VAFIN", "$(", "ADV", "APPR", "VVFIN", "APPR", "PPER"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "regieret/", "tokens": ["re\u00b7gie\u00b7ret", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Tod und leben wird durch mich auf der erden eingef\u00fchret.", "tokens": ["Tod", "und", "le\u00b7ben", "wird", "durch", "mich", "auf", "der", "er\u00b7den", "ein\u00b7ge\u00b7f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VAFIN", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}