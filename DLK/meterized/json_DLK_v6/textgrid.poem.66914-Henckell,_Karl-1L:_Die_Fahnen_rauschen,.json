{"textgrid.poem.66914": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Fahnen rauschen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fahnen rauschen,", "tokens": ["Die", "Fah\u00b7nen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Glocken l\u00e4uten:", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Es tagt in Basel", "tokens": ["Es", "tagt", "in", "Ba\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ein neu Konzil.", "tokens": ["Ein", "neu", "Kon\u00b7zil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das alte M\u00fcnster", "tokens": ["Das", "al\u00b7te", "M\u00fcns\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ragt ob dem Rheine \u2013", "tokens": ["Ragt", "ob", "dem", "Rhei\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NE", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Durch seine Quadern", "tokens": ["Durch", "sei\u00b7ne", "Qua\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "Braust neues Spiel.", "tokens": ["Braust", "neu\u00b7es", "Spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Abgesandten", "tokens": ["Die", "Ab\u00b7ge\u00b7sand\u00b7ten"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der V\u00f6lker reichen", "tokens": ["Der", "V\u00f6l\u00b7ker", "rei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sich fest die H\u00e4nde", "tokens": ["Sich", "fest", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADJD", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Zum heiligen Bund:", "tokens": ["Zum", "hei\u00b7li\u00b7gen", "Bund", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "\u00bbwir wollen zeugen", "tokens": ["\u00bb", "wir", "wol\u00b7len", "zeu\u00b7gen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Aus Macht und Wahrheit \u2013", "tokens": ["Aus", "Macht", "und", "Wahr\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vor Grauen zittert", "tokens": ["Vor", "Grau\u00b7en", "zit\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Erde Grund.", "tokens": ["Der", "Er\u00b7de", "Grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Was Menschen m\u00fchvoll", "tokens": ["Was", "Men\u00b7schen", "m\u00fch\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["PWS", "NN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Schaffen zum Sch\u00f6nen,", "tokens": ["Schaf\u00b7fen", "zum", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schamlos verschlingt es", "tokens": ["Scham\u00b7los", "ver\u00b7schlingt", "es"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Des Wahnsinns Gier \u2013", "tokens": ["Des", "Wahn\u00b7sinns", "Gier", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Zum V\u00f6lkerschlachten", "tokens": ["Zum", "V\u00f6l\u00b7ker\u00b7schlach\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Frevler sch\u00fcren,", "tokens": ["Die", "Frev\u00b7ler", "sch\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Doch Brand und Fackeln", "tokens": ["Doch", "Brand", "und", "Fa\u00b7ckeln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Verl\u00f6schen ", "tokens": ["Ver\u00b7l\u00f6\u00b7schen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Vernehmt, ihr V\u00f6lker,", "tokens": ["Ver\u00b7nehmt", ",", "ihr", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Was wir verk\u00fcnden:", "tokens": ["Was", "wir", "ver\u00b7k\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Gereift sind Saaten", "tokens": ["Ge\u00b7reift", "sind", "Saa\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der neuen Welt.", "tokens": ["Der", "neu\u00b7en", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Der Arbeit Heere", "tokens": ["Der", "Ar\u00b7beit", "Hee\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Fordern Frieden \u2013", "tokens": ["For\u00b7dern", "Frie\u00b7den", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wehe den M\u00f6rdern", "tokens": ["We\u00b7he", "den", "M\u00f6r\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von Firn zu Belt!\u00ab", "tokens": ["Von", "Firn", "zu", "Belt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Die Glocken l\u00e4uten,", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Fahnen rauschen,", "tokens": ["Die", "Fah\u00b7nen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Auflauscht ersch\u00fcttert", "tokens": ["Auf\u00b7lauscht", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Vater Rhein:", "tokens": ["Der", "Va\u00b7ter", "Rhein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Die greisen K\u00e4mpfer,", "tokens": ["Die", "grei\u00b7sen", "K\u00e4mp\u00b7fer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die bl\u00fchenden Kinder,", "tokens": ["Die", "bl\u00fc\u00b7hen\u00b7den", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie ziehn zum M\u00fcnster", "tokens": ["Sie", "ziehn", "zum", "M\u00fcns\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Menschheit ein.", "tokens": ["Der", "Menschheit", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}