{"textgrid.poem.55248": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4hmung", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Gutes zu denken w\u00e4re gut,", "tokens": ["Was", "Gu\u00b7tes", "zu", "den\u00b7ken", "w\u00e4\u00b7re", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKZU", "VVINF", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "F\u00e4nd sich nur immer das gleiche Blut;", "tokens": ["F\u00e4nd", "sich", "nur", "im\u00b7mer", "das", "glei\u00b7che", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dein Gutgedachtes in fremden Adern", "tokens": ["Dein", "Gut\u00b7ge\u00b7dach\u00b7tes", "in", "frem\u00b7den", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird sogleich mit dir selber hadern.", "tokens": ["Wird", "sog\u00b7leich", "mit", "dir", "sel\u00b7ber", "ha\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich w\u00e4r noch gern ein t\u00e4tig Mann,", "tokens": ["Ich", "w\u00e4r", "noch", "gern", "ein", "t\u00e4\u00b7tig", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will aber ruhn:", "tokens": ["Will", "a\u00b7ber", "ruhn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Denn ich soll ja noch immer tun,", "tokens": ["Denn", "ich", "soll", "ja", "noch", "im\u00b7mer", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was immer ungern ich getan.", "tokens": ["Was", "im\u00b7mer", "un\u00b7gern", "ich", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Tr\u00fcge gern noch l\u00e4nger des Lehrers B\u00fcrden,", "tokens": ["Tr\u00fc\u00b7ge", "gern", "noch", "l\u00e4n\u00b7ger", "des", "Leh\u00b7rers", "B\u00fcr\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Wenn Sch\u00fcler nur nicht gleich Lehrer w\u00fcrden.", "tokens": ["Wenn", "Sch\u00fc\u00b7ler", "nur", "nicht", "gleich", "Leh\u00b7rer", "w\u00fcr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PTKNEG", "ADV", "NN", "VAFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}