{"dta.poem.10461": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "So wandl' ich wieder den alten Weg,", "tokens": ["So", "wandl'", "ich", "wie\u00b7der", "den", "al\u00b7ten", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die wohlbekannten Gassen;", "tokens": ["Die", "wohl\u00b7be\u00b7kann\u00b7ten", "Gas\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich komme von meiner Liebsten Haus,", "tokens": ["Ich", "kom\u00b7me", "von", "mei\u00b7ner", "Liebs\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das steht so leer und verlassen.", "tokens": ["Das", "steht", "so", "leer", "und", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Stra\u00dfen sind doch gar zu eng'!", "tokens": ["Die", "Stra\u00b7\u00dfen", "sind", "doch", "gar", "zu", "eng'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Pflaster ist unertr\u00e4glich!", "tokens": ["Das", "Pflas\u00b7ter", "ist", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die H\u00e4user fallen mir auf den Kopf!", "tokens": ["Die", "H\u00e4u\u00b7ser", "fal\u00b7len", "mir", "auf", "den", "Kopf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ich eile so viel als m\u00f6glich!", "tokens": ["Ich", "ei\u00b7le", "so", "viel", "als", "m\u00f6g\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}