{"dta.poem.18492": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines ungerechten Richters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gaben sah ich mehr als die gesetze an/", "tokens": ["Die", "ga\u00b7ben", "sah", "ich", "mehr", "als", "die", "ge\u00b7set\u00b7ze", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PPER", "PIAT", "KOKOM", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprach urtheil nach dem geld/ nicht dem Justinian.", "tokens": ["Sprach", "ur\u00b7theil", "nach", "dem", "geld", "/", "nicht", "dem", "Jus\u00b7ti\u00b7ni\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$(", "PTKNEG", "ART", "NE", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Numehr ist \u00fcber mich da\u00df urtheil auch gesprochen:", "tokens": ["Nu\u00b7mehr", "ist", "\u00fc\u00b7ber", "mich", "da\u00df", "ur\u00b7theil", "auch", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "KOUS", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weh diesem/ der das recht den armen hat gebrochen.", "tokens": ["Weh", "die\u00b7sem", "/", "der", "das", "recht", "den", "ar\u00b7men", "hat", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "$(", "ART", "ART", "NN", "ART", "ADJA", "VAFIN", "VVPP