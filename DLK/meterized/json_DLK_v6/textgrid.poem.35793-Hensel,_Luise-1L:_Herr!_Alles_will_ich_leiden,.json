{"textgrid.poem.35793": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr! Alles will ich leiden,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! Alles will ich leiden,", "tokens": ["Herr", "!", "Al\u00b7les", "will", "ich", "lei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Deine Hand mir giebt,", "tokens": ["Was", "Dei\u00b7ne", "Hand", "mir", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will alle Liebe meiden,", "tokens": ["Will", "al\u00b7le", "Lie\u00b7be", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die, Jesus! Dich nicht liebt.", "tokens": ["Die", ",", "Je\u00b7sus", "!", "Dich", "nicht", "liebt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NE", "$.", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gieb Heiligkeit dem Herzen,", "tokens": ["Gieb", "Hei\u00b7lig\u00b7keit", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gieb einen neuen Geist,", "tokens": ["Gieb", "ei\u00b7nen", "neu\u00b7en", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Dich in Lust und Schmerzen,", "tokens": ["Der", "Dich", "in", "Lust", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Tod und Leben preist.", "tokens": ["In", "Tod", "und", "Le\u00b7ben", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Herr! gieb, da\u00df fest ich glaube,", "tokens": ["Herr", "!", "gieb", ",", "da\u00df", "fest", "ich", "glau\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "$,", "KOUS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gieb Trost auf Erden schon,", "tokens": ["Gieb", "Trost", "auf", "Er\u00b7den", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mir kein Zweifel raube", "tokens": ["Da\u00df", "mir", "kein", "Zwei\u00b7fel", "rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der treuen Knechte Lohn.", "tokens": ["Der", "treu\u00b7en", "Knech\u00b7te", "Lohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich lobsingend dringe", "tokens": ["Da\u00df", "ich", "lob\u00b7sin\u00b7gend", "drin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Nacht zum Morgenroth;", "tokens": ["Durch", "Nacht", "zum", "Mor\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich den Tod bezwinge,", "tokens": ["Da\u00df", "ich", "den", "Tod", "be\u00b7zwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stark durch Dein Lebensbrod.", "tokens": ["Stark", "durch", "Dein", "Le\u00b7bens\u00b7brod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df, vor Deinem Throne", "tokens": ["Ich", "wei\u00df", ",", "vor", "Dei\u00b7nem", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In F\u00fcll' und Herrlichkeit", "tokens": ["In", "F\u00fcll'", "und", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist Allen ja die Krone,", "tokens": ["Ist", "Al\u00b7len", "ja", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch mir ein Ort bereit.", "tokens": ["Auch", "mir", "ein", "Ort", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Bu\u00dfe wird mit Thr\u00e4nen,", "tokens": ["Die", "Bu\u00b7\u00dfe", "wird", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geduld und stiller Pein", "tokens": ["Ge\u00b7duld", "und", "stil\u00b7ler", "Pein"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch mir den Kranz versch\u00f6nen,", "tokens": ["Auch", "mir", "den", "Kranz", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gleich Perl' und Edelstein.", "tokens": ["Gleich", "Perl'", "und", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Soll ich als Zeuge dienen,", "tokens": ["Soll", "ich", "als", "Zeu\u00b7ge", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Herr! nimm hin mein Blut,", "tokens": ["O", "Herr", "!", "nimm", "hin", "mein", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVIMP", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das schm\u00fcckt mir gleich Rubinen", "tokens": ["Das", "schm\u00fcckt", "mir", "gleich", "Ru\u00b7bi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Kranz mit Strahlengluth.", "tokens": ["Den", "Kranz", "mit", "Strah\u00b7len\u00b7gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie prangt die gr\u00fcne Weide!", "tokens": ["Wie", "prangt", "die", "gr\u00fc\u00b7ne", "Wei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Hirte winket hier:", "tokens": ["Mein", "Hir\u00b7te", "win\u00b7ket", "hier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Kleid von wei\u00dfer Seide,", "tokens": ["Ein", "Kleid", "von", "wei\u00b7\u00dfer", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Kr\u00f6nlein zeigt er mir!", "tokens": ["Ein", "Kr\u00f6n\u00b7lein", "zeigt", "er", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Perlen, herbe Thr\u00e4nen!", "tokens": ["O", "Per\u00b7len", ",", "her\u00b7be", "Thr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Herzensblut, Rubin!", "tokens": ["O", "Her\u00b7zens\u00b7blut", ",", "Ru\u00b7bin", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herr, stille dieses Sehnen,", "tokens": ["Herr", ",", "stil\u00b7le", "die\u00b7ses", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herr, nimm mich bald dahin!", "tokens": ["Herr", ",", "nimm", "mich", "bald", "da\u00b7hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}