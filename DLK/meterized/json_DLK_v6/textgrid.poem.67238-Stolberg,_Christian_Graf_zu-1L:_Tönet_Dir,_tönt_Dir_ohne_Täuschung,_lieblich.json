{"textgrid.poem.67238": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: T\u00f6net Dir, t\u00f6nt Dir ohne T\u00e4uschung, lieblich", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6net Dir, t\u00f6nt Dir ohne T\u00e4uschung, lieblich", "tokens": ["T\u00f6\u00b7net", "Dir", ",", "t\u00f6nt", "Dir", "oh\u00b7ne", "T\u00e4u\u00b7schung", ",", "lieb\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN", "$,", "ADJD"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie der Nachtigall Lied, des Todes Name,", "tokens": ["Wie", "der", "Nach\u00b7ti\u00b7gall", "Lied", ",", "des", "To\u00b7des", "Na\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und wird Dir sein rauschender naher Fittig", "tokens": ["Und", "wird", "Dir", "sein", "rau\u00b7schen\u00b7der", "na\u00b7her", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schwanenflug t\u00f6nen?", "tokens": ["Schwa\u00b7nen\u00b7flug", "t\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Blumen umkr\u00e4nzen, wie sie Dir nur bl\u00fchen,", "tokens": ["Blu\u00b7men", "um\u00b7kr\u00e4n\u00b7zen", ",", "wie", "sie", "Dir", "nur", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PWAV", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Deine wallenden Locken, und den Becher,", "tokens": ["Dei\u00b7ne", "wal\u00b7len\u00b7den", "Lo\u00b7cken", ",", "und", "den", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den mit G\u00f6tterwein die Natur Dir immer", "tokens": ["Den", "mit", "G\u00f6t\u00b7ter\u00b7wein", "die", "Na\u00b7tur", "Dir", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ART", "NN", "PPER", "ADV"], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Sch\u00e4umender anf\u00fcllt:", "tokens": ["Sch\u00e4u\u00b7men\u00b7der", "an\u00b7f\u00fcllt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Blumen des Bachs, der Wiese pfl\u00fcckt die Freundschaft", "tokens": ["Blu\u00b7men", "des", "Bachs", ",", "der", "Wie\u00b7se", "pfl\u00fcckt", "die", "Freund\u00b7schaft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dir, den stolzeren Lorbeer Dir die Muse,", "tokens": ["Dir", ",", "den", "stol\u00b7ze\u00b7ren", "Lor\u00b7beer", "Dir", "die", "Mu\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bald auch wird (schon r\u00f6thelt ihr Rosenkn\u00f6spchen!)", "tokens": ["Bald", "auch", "wird", "(", "schon", "r\u00f6\u00b7thelt", "ihr", "Ro\u00b7sen\u00b7kn\u00f6spc\u00b7hen", "!", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Liebe Dich kr\u00e4nzen.", "tokens": ["Lie\u00b7be", "Dich", "kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber, o w\u00e4hnst Du, da\u00df der Liebe Rose,", "tokens": ["A\u00b7ber", ",", "o", "w\u00e4hnst", "Du", ",", "da\u00df", "der", "Lie\u00b7be", "Ro\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "NE", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Selbst der s\u00fc\u00dfesten Liebe, wenn nun endlich,", "tokens": ["Selbst", "der", "s\u00fc\u00b7\u00dfes\u00b7ten", "Lie\u00b7be", ",", "wenn", "nun", "end\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "KOUS", "ADV", "ADV", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Athemlos, mit schmachtendem, feuchten Auge,", "tokens": ["A\u00b7them\u00b7los", ",", "mit", "schmach\u00b7ten\u00b7dem", ",", "feuch\u00b7ten", "Au\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Bebenden Lippen,", "tokens": ["Be\u00b7ben\u00b7den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Die sich zu matten, halbgek\u00fc\u00dften K\u00fcssen", "tokens": ["Die", "sich", "zu", "mat\u00b7ten", ",", "halb\u00b7ge\u00b7k\u00fc\u00df\u00b7ten", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PRF", "PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kaum zu schlie\u00dfen verm\u00f6gen, ach! an Deinem", "tokens": ["Kaum", "zu", "schlie\u00b7\u00dfen", "ver\u00b7m\u00f6\u00b7gen", ",", "ach", "!", "an", "Dei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$,", "ITJ", "$.", "APPR", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Trunknen Busen, sie, die Du liebest, die Dich", "tokens": ["Trunk\u00b7nen", "Bu\u00b7sen", ",", "sie", ",", "die", "Du", "lie\u00b7best", ",", "die", "Dich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Liebet, dahin sinkt;", "tokens": ["Lie\u00b7bet", ",", "da\u00b7hin", "sinkt", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PAV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "W\u00e4hnst Du, sie dufte, diese Rose, st\u00e4rker", "tokens": ["W\u00e4hnst", "Du", ",", "sie", "duf\u00b7te", ",", "die\u00b7se", "Ro\u00b7se", ",", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "PPER", "VVFIN", "$,", "PDAT", "NN", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als das Rankengewebe, das, mit tausend", "tokens": ["Als", "das", "Ran\u00b7ken\u00b7ge\u00b7we\u00b7be", ",", "das", ",", "mit", "tau\u00b7send"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PDS", "$,", "APPR", "CARD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Armen, uns, und kr\u00e4uselnden Sprossen, fester", "tokens": ["Ar\u00b7men", ",", "uns", ",", "und", "kr\u00e4u\u00b7seln\u00b7den", "Spros\u00b7sen", ",", "fes\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPER", "$,", "KON", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Stets uns umschlinget?", "tokens": ["Stets", "uns", "um\u00b7schlin\u00b7get", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Aufgang der Sonne flammet Dir des Todes", "tokens": ["Auf\u00b7gang", "der", "Son\u00b7ne", "flam\u00b7met", "Dir", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Fackel? Sie, die der Ranken keiner schonen", "tokens": ["Fa\u00b7ckel", "?", "Sie", ",", "die", "der", "Ran\u00b7ken", "kei\u00b7ner", "scho\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "$,", "PRELS", "ART", "NN", "PIS", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und austrocknen w\u00fcrde die Borne meines", "tokens": ["Und", "aus\u00b7trock\u00b7nen", "w\u00fcr\u00b7de", "die", "Bor\u00b7ne", "mei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VAFIN", "ART", "NN", "PPOSAT"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lechzenden Lebens?", "tokens": ["Lech\u00b7zen\u00b7den", "Le\u00b7bens", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Da\u00df, den Du w\u00fcnschest, ich nicht f\u00fcrchte, wei\u00dft Du!", "tokens": ["Da\u00df", ",", "den", "Du", "w\u00fcn\u00b7schest", ",", "ich", "nicht", "f\u00fcrch\u00b7te", ",", "wei\u00dft", "Du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kanntest lange den Durst in meinem Herzen,", "tokens": ["Kann\u00b7test", "lan\u00b7ge", "den", "Durst", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Heldentod einst in der gerechten Feldschlacht", "tokens": ["Hel\u00b7den\u00b7tod", "einst", "in", "der", "ge\u00b7rech\u00b7ten", "Feld\u00b7schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Blutig zu sterben!", "tokens": ["Blu\u00b7tig", "zu", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Siehe, schon schwebt er! \u2013 Ha! ich kenne deines", "tokens": ["Sie\u00b7he", ",", "schon", "schwebt", "er", "!", "\u2013", "Ha", "!", "ich", "ken\u00b7ne", "dei\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "$.", "$(", "ITJ", "$.", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fittigs Todesgesang! Mich schreckt nicht, Droher,", "tokens": ["Fit\u00b7tigs", "To\u00b7des\u00b7ge\u00b7sang", "!", "Mich", "schreckt", "nicht", ",", "Dro\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "$,", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Deine Rechte! Trennung von meinen Lieben,", "tokens": ["Dei\u00b7ne", "Rech\u00b7te", "!", "Tren\u00b7nung", "von", "mei\u00b7nen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Droher, die schreckt mich!", "tokens": ["Dro\u00b7her", ",", "die", "schreckt", "mich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Leben, o leben will ich! schwebt gleich manches", "tokens": ["Le\u00b7ben", ",", "o", "le\u00b7ben", "will", "ich", "!", "schwebt", "gleich", "man\u00b7ches"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "FM", "VVINF", "VMFIN", "PPER", "$.", "VVFIN", "ADV", "PIS"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Tr\u00fcbe W\u00f6lkchen heran, ihr Schwestern, Freunde,", "tokens": ["Tr\u00fc\u00b7be", "W\u00f6lk\u00b7chen", "he\u00b7ran", ",", "ihr", "Schwes\u00b7tern", ",", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Leben! Mein braunlockiges Weib, mein Bruder,", "tokens": ["Le\u00b7ben", "!", "Mein", "braun\u00b7lo\u00b7cki\u00b7ges", "Weib", ",", "mein", "Bru\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Leben, o leben!", "tokens": ["Le\u00b7ben", ",", "o", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Aber wenn \u2013 doch der Menschheit Loos verbeut es!", "tokens": ["A\u00b7ber", "wenn", "\u2013", "doch", "der", "Menschheit", "Loos", "ver\u00b7beut", "es", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$(", "ADV", "ART", "NN", "NE", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wenn zugleich dem vertrauten H\u00e4uflein winkte", "tokens": ["Wenn", "zu\u00b7gleich", "dem", "ver\u00b7trau\u00b7ten", "H\u00e4uf\u00b7lein", "wink\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Er, der Ruhegeber; ich s\u00e4h' ihn, l\u00e4chelnd:", "tokens": ["Er", ",", "der", "Ru\u00b7he\u00b7ge\u00b7ber", ";", "ich", "s\u00e4h'", "ihn", ",", "l\u00e4\u00b7chelnd", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "ADJD", "$."], "meter": "--+----+-+-", "measure": "anapaest.init"}, "line.4": {"text": "\u00bbbruder, er schreckt nicht!\u00ab", "tokens": ["\u00bb", "bru\u00b7der", ",", "er", "schreckt", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}