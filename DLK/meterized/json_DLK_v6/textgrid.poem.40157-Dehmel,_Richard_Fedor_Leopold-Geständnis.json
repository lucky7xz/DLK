{"textgrid.poem.40157": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Gest\u00e4ndnis", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df deiner ich so ganz vergessen", "tokens": ["Da\u00df", "dei\u00b7ner", "ich", "so", "ganz", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "in einem wilden Augenblick,", "tokens": ["in", "ei\u00b7nem", "wil\u00b7den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in w\u00fcster Laune mich vermessen", "tokens": ["in", "w\u00fcs\u00b7ter", "Lau\u00b7ne", "mich", "ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zu ", "tokens": ["zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "kannst du's verzeihn?", "tokens": ["kannst", "du's", "ver\u00b7zeihn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Mein Sinn so tr\u00fcb! die Nacht so dunkel!", "tokens": ["Mein", "Sinn", "so", "tr\u00fcb", "!", "die", "Nacht", "so", "dun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$.", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "hei\u00df scho\u00df ins Auge mir das Blut!", "tokens": ["hei\u00df", "scho\u00df", "ins", "Au\u00b7ge", "mir", "das", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "im Strome unten welch Gefunkel!", "tokens": ["im", "Stro\u00b7me", "un\u00b7ten", "welch", "Ge\u00b7fun\u00b7kel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ein weher Schrei, ein Sprung zur Flut!", "tokens": ["ein", "we\u00b7her", "Schrei", ",", "ein", "Sprung", "zur", "Flut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kannst du's verzeihn?", "tokens": ["Kannst", "du's", "ver\u00b7zeihn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Kannst du dem Freunde mit mir ", "tokens": ["Kannst", "du", "dem", "Freun\u00b7de", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "da\u00df Du noch mein, da\u00df Ich noch Dein?", "tokens": ["da\u00df", "Du", "noch", "mein", ",", "da\u00df", "Ich", "noch", "Dein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du Reinste, kannst du ohne Wanken", "tokens": ["Du", "Reins\u00b7te", ",", "kannst", "du", "oh\u00b7ne", "Wan\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dem Frevler noch dein Leben weihn? \u2013", "tokens": ["dem", "Frev\u00b7ler", "noch", "dein", "Le\u00b7ben", "weihn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kannst Du verzeihn?!", "tokens": ["Kannst", "Du", "ver\u00b7zeihn", "?!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}