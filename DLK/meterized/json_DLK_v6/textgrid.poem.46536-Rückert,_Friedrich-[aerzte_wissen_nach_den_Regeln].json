{"textgrid.poem.46536": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[aerzte wissen nach den Regeln]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aerzte wissen nach den Regeln", "tokens": ["A\u00b7erz\u00b7te", "wis\u00b7sen", "nach", "den", "Re\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus der Welt kein Kind zu schaffen,", "tokens": ["Aus", "der", "Welt", "kein", "Kind", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne mit abscheul'chen Egeln", "tokens": ["Oh\u00b7ne", "mit", "ab\u00b7scheul'\u00b7chen", "E\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die Naturkraft hinzuraffen.", "tokens": ["Die", "Na\u00b7tur\u00b7kraft", "hin\u00b7zu\u00b7raf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie mehr werd' ich mich in Quellen", "tokens": ["Nie", "mehr", "werd'", "ich", "mich", "in", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unbefangen spiegeln;", "tokens": ["Un\u00b7be\u00b7fan\u00b7gen", "spie\u00b7geln", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Immer werd' ich in den Wellen", "tokens": ["Im\u00b7mer", "werd'", "ich", "in", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schaudern vor Blutigeln,", "tokens": ["Schau\u00b7dern", "vor", "Blu\u00b7ti\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Die das Leben mit dem Blute", "tokens": ["Die", "das", "Le\u00b7ben", "mit", "dem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Kinds entsogen;", "tokens": ["Mei\u00b7nes", "Kinds", "ent\u00b7so\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "So mi\u00dfhandelt ist das gute", "tokens": ["So", "mi\u00df\u00b7han\u00b7delt", "ist", "das", "gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seelchen, ach, entflogen.", "tokens": ["Seel\u00b7chen", ",", "ach", ",", "ent\u00b7flo\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber nicht aus reinen Quellen,", "tokens": ["A\u00b7ber", "nicht", "aus", "rei\u00b7nen", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sondern styg'schem Sumpfe", "tokens": ["Son\u00b7dern", "sty\u00b7g'\u00b7schem", "Sump\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Holt man diese Blutgesellen", "tokens": ["Holt", "man", "die\u00b7se", "Blut\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Tods Triumphe.", "tokens": ["Zu", "des", "Tods", "Tri\u00b7um\u00b7phe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}