{"textgrid.poem.41486": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Dasein", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein dunkler Feind erheiternder Getr\u00e4nke,", "tokens": ["Ein", "dunk\u00b7ler", "Feind", "er\u00b7hei\u00b7tern\u00b7der", "Ge\u00b7tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Philosoph, trat neulich hin", "tokens": ["Ein", "Phi\u00b7lo\u00b7soph", ",", "trat", "neu\u00b7lich", "hin"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach: Ihr Herren, wi\u00dft, ich bin.", "tokens": ["Und", "sprach", ":", "Ihr", "Her\u00b7ren", ",", "wi\u00dft", ",", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Glaubt mir, ich bin. Ja, ja! Warum? Weil ich gedenke.", "tokens": ["Glaubt", "mir", ",", "ich", "bin", ".", "Ja", ",", "ja", "!", "Wa\u00b7rum", "?", "Weil", "ich", "ge\u00b7den\u00b7ke", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VAFIN", "$.", "PTKANT", "$,", "ADV", "$.", "PWAV", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein S\u00e4ufer kam und taumelt' ihm entgegen,", "tokens": ["Ein", "S\u00e4u\u00b7fer", "kam", "und", "tau\u00b7melt'", "ihm", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schwur bei seinem Wirth und Wein:", "tokens": ["Und", "schwur", "bei", "sei\u00b7nem", "Wirth", "und", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich trink, o darum mu\u00df ich sein.", "tokens": ["Ich", "trink", ",", "o", "da\u00b7rum", "mu\u00df", "ich", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "PAV", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Glaubt mir, ich trink: ich bin. Wer kann mich widerlegen?", "tokens": ["Glaubt", "mir", ",", "ich", "trink", ":", "ich", "bin", ".", "Wer", "kann", "mich", "wi\u00b7der\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "$.", "PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}