{"dta.poem.12298": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Friedenslied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Angenehme Taube,               ", "tokens": ["An\u00b7ge\u00b7neh\u00b7me", "Tau\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die der V\u00e4ter Glaube", "tokens": ["Die", "der", "V\u00e4\u00b7ter", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "L\u00e4ngst gesehen hat;", "tokens": ["L\u00e4ngst", "ge\u00b7se\u00b7hen", "hat", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Lasse dich hernieder,", "tokens": ["Las\u00b7se", "dich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hier sind Jesu Glieder,", "tokens": ["Hier", "sind", "Je\u00b7su", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Hier ist Gottes Stadt;", "tokens": ["Hier", "ist", "Got\u00b7tes", "Stadt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Halte Rast,", "tokens": ["Hal\u00b7te", "Rast", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Erw\u00fcnschter Gast", "tokens": ["Er\u00b7w\u00fcnschter", "Gast"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "In den Herzen,", "tokens": ["In", "den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Die verlangen,", "tokens": ["Die", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Dich jetzt zu empfangen.", "tokens": ["Dich", "jetzt", "zu", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Setze dich auf jeden,", "tokens": ["Set\u00b7ze", "dich", "auf", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und la\u00df deinen Frieden", "tokens": ["Und", "la\u00df", "dei\u00b7nen", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ueberalle seyn;", "tokens": ["Ue\u00b7be\u00b7ral\u00b7le", "seyn", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie du dich erhebest,", "tokens": ["Wie", "du", "dich", "er\u00b7he\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf dem Wasser schwebest,", "tokens": ["Auf", "dem", "Was\u00b7ser", "schwe\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "So kehr bey uns ein.", "tokens": ["So", "kehr", "bey", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Zeig uns hier", "tokens": ["Zeig", "uns", "hier"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Das Oehlblatt f\u00fcr,", "tokens": ["Das", "O\u00b7ehl\u00b7blatt", "f\u00fcr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Als ein h\u00f6chst erw\u00fcnschtes Zeichen,", "tokens": ["Als", "ein", "h\u00f6chst", "er\u00b7w\u00fcnschtes", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Da\u00df die Fluthen weichen.", "tokens": ["Da\u00df", "die", "Flut\u00b7hen", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Was du abgebrochen,", "tokens": ["Was", "du", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist uns l\u00e4ngst versprochen,", "tokens": ["Ist", "uns", "l\u00e4ngst", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und die\u00df edle Blat", "tokens": ["Und", "die\u00df", "ed\u00b7le", "Blat"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ist vom Lebensbaume,", "tokens": ["Ist", "vom", "Le\u00b7bens\u00b7bau\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der in Edens Raume", "tokens": ["Der", "in", "E\u00b7dens", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "L\u00e4ngst gegr\u00fcnet hat.", "tokens": ["L\u00e4ngst", "ge\u00b7gr\u00fc\u00b7net", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Tr\u00e4uft es doch", "tokens": ["Tr\u00e4uft", "es", "doch"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Vom Oehle noch,", "tokens": ["Vom", "O\u00b7eh\u00b7le", "noch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Welches Jesus lassen flie\u00dfen,", "tokens": ["Wel\u00b7ches", "Je\u00b7sus", "las\u00b7sen", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Als er leiden m\u00fcssen.", "tokens": ["Als", "er", "lei\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "O Geruch des Lebens!", "tokens": ["O", "Ge\u00b7ruch", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der uns nicht vergebens", "tokens": ["Der", "uns", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PTKNEG", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unser Herz erquickt;", "tokens": ["Un\u00b7ser", "Herz", "er\u00b7quickt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dieses Oehlblatt k\u00fchlet,", "tokens": ["Die\u00b7ses", "O\u00b7ehl\u00b7blatt", "k\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da\u00df man Lindrung f\u00fchlet,", "tokens": ["Da\u00df", "man", "Lind\u00b7rung", "f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn das Kreuze dr\u00fcckt.", "tokens": ["Wenn", "das", "Kreu\u00b7ze", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Es giebt Kraft und Lebenssaft,", "tokens": ["Es", "giebt", "Kraft", "und", "Le\u00b7bens\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn es wohl wird aufgebunden,", "tokens": ["Wenn", "es", "wohl", "wird", "auf\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Heilt es alle Wunden.", "tokens": ["Heilt", "es", "al\u00b7le", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Bothe von dem Himmel,", "tokens": ["Bo\u00b7the", "von", "dem", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dringe durchs Get\u00fcmmel,", "tokens": ["Drin\u00b7ge", "durchs", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dieser eitlen Welt;", "tokens": ["Die\u00b7ser", "eit\u00b7len", "Welt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und mach eine Stille,", "tokens": ["Und", "mach", "ei\u00b7ne", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df ein Herz, ein Wille,", "tokens": ["Da\u00df", "ein", "Herz", ",", "ein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Uns zusammen h\u00e4lt.", "tokens": ["Uns", "zu\u00b7sam\u00b7men", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "La\u00df das Blat,", "tokens": ["La\u00df", "das", "Blat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Das dein Mund hat,", "tokens": ["Das", "dein", "Mund", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Unser aller Lippen r\u00fchren,", "tokens": ["Un\u00b7ser", "al\u00b7ler", "Lip\u00b7pen", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Deine Stimme f\u00fchren.", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Macht die Feuerschlange,", "tokens": ["Macht", "die", "Feu\u00b7er\u00b7schlan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Uns gleich angst und bange,", "tokens": ["Uns", "gleich", "angst", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hat sie doch nicht Macht,", "tokens": ["Hat", "sie", "doch", "nicht", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Unsern Leib zu t\u00f6dten,", "tokens": ["Un\u00b7sern", "Leib", "zu", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Jetzt in Kriegesn\u00f6then,", "tokens": ["Jetzt", "in", "Krie\u00b7ges\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Weil ja Jesus wacht.", "tokens": ["Weil", "ja", "Je\u00b7sus", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Jesus sch\u00fctzt,", "tokens": ["Je\u00b7sus", "sch\u00fctzt", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wenns kracht und blitzt,", "tokens": ["Wenns", "kracht", "und", "blitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Jesus will die Seinen decken,", "tokens": ["Je\u00b7sus", "will", "die", "Sei\u00b7nen", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn Kanonen schrecken.", "tokens": ["Wenn", "Ka\u00b7no\u00b7nen", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Nun du Himmelstaube,", "tokens": ["Nun", "du", "Him\u00b7mel\u00b7stau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Unser aller Glaube", "tokens": ["Un\u00b7ser", "al\u00b7ler", "Glau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nimmt dich zu uns ein;", "tokens": ["Nimmt", "dich", "zu", "uns", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wohnest du bei keinen,", "tokens": ["Woh\u00b7nest", "du", "bei", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Als nur bey den Reinen,", "tokens": ["Als", "nur", "bey", "den", "Rei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ach so mach uns rein.", "tokens": ["Ach", "so", "mach", "uns", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Taubenart", "tokens": ["Tau\u00b7be\u00b7nart"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Bringt Himmelfahrt,", "tokens": ["Bringt", "Him\u00b7mel\u00b7fahrt", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Bring uns den lieben Frieden", "tokens": ["Bring", "uns", "den", "lie\u00b7ben", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Von dem Sternen-H\u00fcgel.", "tokens": ["Von", "dem", "Ster\u00b7nen\u00b7H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}