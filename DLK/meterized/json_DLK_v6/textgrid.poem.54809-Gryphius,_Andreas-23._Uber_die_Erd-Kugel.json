{"textgrid.poem.54809": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "23. Uber die Erd-Kugel", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Erden rundes Hau\u00df das Vih und Menschen tr\u00e4gt/", "tokens": ["Der", "Er\u00b7den", "run\u00b7des", "Hau\u00df", "das", "Vih", "und", "Men\u00b7schen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist noch nicht gantz beschawt/ doch ist es gantz gemessen.", "tokens": ["Ist", "noch", "nicht", "gantz", "be\u00b7schawt", "/", "doch", "ist", "es", "gantz", "ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was nie der Leib bezwang hat doch der Geist besessen.", "tokens": ["Was", "nie", "der", "Leib", "be\u00b7zwang", "hat", "doch", "der", "Geist", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Land und Wellen Zill hir/ auch abwesend/ legt.", "tokens": ["Der", "Land", "und", "Wel\u00b7len", "Zill", "hir", "/", "auch", "ab\u00b7we\u00b7send", "/", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["AR