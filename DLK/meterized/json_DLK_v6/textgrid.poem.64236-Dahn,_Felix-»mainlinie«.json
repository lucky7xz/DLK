{"textgrid.poem.64236": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbmainlinie\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die raschen Schiffe gleiten", "tokens": ["Die", "ra\u00b7schen", "Schif\u00b7fe", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl hin und her den Main:", "tokens": ["Wohl", "hin", "und", "her", "den", "Main", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hie deutsch zu beiden Seiten: \u2013", "tokens": ["Hie", "deutsch", "zu", "bei\u00b7den", "Sei\u00b7ten", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll das geschieden sein?", "tokens": ["Soll", "das", "ge\u00b7schie\u00b7den", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwei Amseln h\u00f6r' ich singen", "tokens": ["Zwei", "Am\u00b7seln", "h\u00f6r'", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl links und rechts vom Main", "tokens": ["Wohl", "links", "und", "rechts", "vom", "Main"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Ton in Ton sich schlingen: \u2013", "tokens": ["Und", "Ton", "in", "Ton", "sich", "schlin\u00b7gen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll das geschieden sein?", "tokens": ["Soll", "das", "ge\u00b7schie\u00b7den", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zwei Liebste seh' ich gehen", "tokens": ["Zwei", "Liebs\u00b7te", "seh'", "ich", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl links und rechts vom Main,", "tokens": ["Wohl", "links", "und", "rechts", "vom", "Main", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Gru\u00df kann sich verstehen: \u2013", "tokens": ["Ihr", "Gru\u00df", "kann", "sich", "ver\u00b7ste\u00b7hen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Soll da geschieden sein?", "tokens": ["Soll", "da", "ge\u00b7schie\u00b7den", "sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zwei Banner seh' ich fliegen,", "tokens": ["Zwei", "Ban\u00b7ner", "seh'", "ich", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER",