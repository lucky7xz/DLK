{"textgrid.poem.37305": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Geschmacksache", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies f\u00fcr den und das f\u00fcr jenen.", "tokens": ["Dies", "f\u00fcr", "den", "und", "das", "f\u00fcr", "je\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "KON", "PDS", "APPR", "PDAT", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Viele Tische sind gedeckt.", "tokens": ["Vie\u00b7le", "Ti\u00b7sche", "sind", "ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Zunge soll verh\u00f6hnen,", "tokens": ["Kei\u00b7ne", "Zun\u00b7ge", "soll", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der andern Zunge schmeckt.", "tokens": ["Was", "der", "an\u00b7dern", "Zun\u00b7ge", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lasse jedem seine Freuden,", "tokens": ["Las\u00b7se", "je\u00b7dem", "sei\u00b7ne", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6nn ihm, da\u00df er sich erquickt,", "tokens": ["G\u00f6nn", "ihm", ",", "da\u00df", "er", "sich", "er\u00b7quickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er sittsam und bescheiden", "tokens": ["Wenn", "er", "sitt\u00b7sam", "und", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den eignen Teller blickt.", "tokens": ["Auf", "den", "eig\u00b7nen", "Tel\u00b7ler", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn jedoch bei deinem Tisch er", "tokens": ["Wenn", "je\u00b7doch", "bei", "dei\u00b7nem", "Tisch", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unversch\u00e4mt dich neckt und st\u00f6rt,", "tokens": ["Un\u00b7ver\u00b7sch\u00e4mt", "dich", "neckt", "und", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann so gib ihm einen Wischer,", "tokens": ["Dann", "so", "gib", "ihm", "ei\u00b7nen", "Wi\u00b7scher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df er merkt, was sich geh\u00f6rt.", "tokens": ["Da\u00df", "er", "merkt", ",", "was", "sich", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}