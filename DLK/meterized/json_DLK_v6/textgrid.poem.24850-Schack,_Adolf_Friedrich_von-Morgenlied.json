{"textgrid.poem.24850": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Morgenlied", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwache, mein M\u00e4dchen! Im d\u00e4mmernden Blau", "tokens": ["Er\u00b7wa\u00b7che", ",", "mein", "M\u00e4d\u00b7chen", "!", "Im", "d\u00e4m\u00b7mern\u00b7den", "Blau"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Erl\u00f6schen die Sterne gemach;", "tokens": ["Er\u00b7l\u00f6\u00b7schen", "die", "Ster\u00b7ne", "ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Auf schwingt sich die Lerche, noch feucht vom Tau,", "tokens": ["Auf", "schwingt", "sich", "die", "Ler\u00b7che", ",", "noch", "feucht", "vom", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PRF", "ART", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zum leuchtenden Aetherdach.", "tokens": ["Zum", "leuch\u00b7ten\u00b7den", "A\u00b7e\u00b7ther\u00b7dach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie jauchzt im freudezitternden Lied,", "tokens": ["Sie", "jauchzt", "im", "freu\u00b7de\u00b7zit\u00b7tern\u00b7den", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie die Welt so sch\u00f6n, so sch\u00f6n!", "tokens": ["Wie", "die", "Welt", "so", "sch\u00f6n", ",", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn Wonnen, die unten kein Auge sieht,", "tokens": ["Denn", "Won\u00b7nen", ",", "die", "un\u00b7ten", "kein", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Schaut sie in den himmlischen H\u00f6hn.", "tokens": ["Schaut", "sie", "in", "den", "himm\u00b7li\u00b7schen", "H\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und jubelt die Lerche, so jubelt mein Herz", "tokens": ["Und", "ju\u00b7belt", "die", "Ler\u00b7che", ",", "so", "ju\u00b7belt", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Hoch in den L\u00fcften mit ihr", "tokens": ["Hoch", "in", "den", "L\u00fcf\u00b7ten", "mit", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "APPR", "PPOSAT"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und sendet, mein M\u00e4dchen, erdenw\u00e4rts", "tokens": ["Und", "sen\u00b7det", ",", "mein", "M\u00e4d\u00b7chen", ",", "er\u00b7den\u00b7w\u00e4rts"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vieltausend Gr\u00fc\u00dfe zu dir.", "tokens": ["Viel\u00b7tau\u00b7send", "Gr\u00fc\u00b7\u00dfe", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}