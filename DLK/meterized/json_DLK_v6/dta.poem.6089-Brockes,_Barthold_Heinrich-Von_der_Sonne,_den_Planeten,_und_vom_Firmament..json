{"dta.poem.6089": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die sondre Tapfferkeit der fabelhafften Helden", "tokens": ["Die", "sond\u00b7re", "Tapf\u00b7fer\u00b7keit", "der", "fa\u00b7bel\u00b7haff\u00b7ten", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erf\u00fcllt, mit manchem sch\u00f6nen Bilde,", "tokens": ["Er\u00b7f\u00fcllt", ",", "mit", "man\u00b7chem", "sch\u00f6\u00b7nen", "Bil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Himmlische Gefilde:", "tokens": ["Das", "Himm\u00b7li\u00b7sche", "Ge\u00b7fil\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch wenn von ihnen was die weisen Alten melden,", "tokens": ["Doch", "wenn", "von", "ih\u00b7nen", "was", "die", "wei\u00b7sen", "Al\u00b7ten", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPER", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wusten sie den Nutzen zum Vergn\u00fcgen", "tokens": ["So", "wus\u00b7ten", "sie", "den", "Nut\u00b7zen", "zum", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Recht angenehm zu f\u00fcgen.", "tokens": ["Recht", "an\u00b7ge\u00b7nehm", "zu", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es n\u00fctzt uns das Gestirn, wanns aus dem Meere steigt,", "tokens": ["Es", "n\u00fctzt", "uns", "das", "Ge\u00b7stirn", ",", "wanns", "aus", "dem", "Mee\u00b7re", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da es so ordentlich uns Strich und Stunden zeigt.", "tokens": ["Da", "es", "so", "or\u00b7dent\u00b7lich", "uns", "Strich", "und", "Stun\u00b7den", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wofern er darauf merckt, sieht einer, welcher reiset,", "tokens": ["Wo\u00b7fern", "er", "da\u00b7rauf", "merckt", ",", "sieht", "ei\u00b7ner", ",", "wel\u00b7cher", "rei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVFIN", "$,", "VVFIN", "PIS", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df es ihm ebenfalls fast alle Schritte weiset.", "tokens": ["Da\u00df", "es", "ihm", "e\u00b7ben\u00b7falls", "fast", "al\u00b7le", "Schrit\u00b7te", "wei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es zeigt dem Steuermann der Pol-Stern, der allein", "tokens": ["Es", "zeigt", "dem", "Steu\u00b7er\u00b7mann", "der", "Pol\u00b7S\u00b7tern", ",", "der", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nie sich verliert, stets einen sichern Schein.", "tokens": ["Nie", "sich", "ver\u00b7liert", ",", "stets", "ei\u00b7nen", "si\u00b7chern", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Land-Mann kan von seinem Acker-Wesen,", "tokens": ["Der", "Lan\u00b7dMann", "kan", "von", "sei\u00b7nem", "A\u00b7cke\u00b7rWe\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Wenn gute Witterung, zum saen, am Himmel lesen.", "tokens": ["Wenn", "gu\u00b7te", "Wit\u00b7te\u00b7rung", ",", "zum", "saen", ",", "am", "Him\u00b7mel", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "APPRART", "VVINF", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es warnen, da\u00df man sich vom Ufer nicht entferne,", "tokens": ["Es", "war\u00b7nen", ",", "da\u00df", "man", "sich", "vom", "U\u00b7fer", "nicht", "ent\u00b7fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIS", "PRF", "APPRART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die tr\u00fcben Plejades, Orions finstre Sterne.", "tokens": ["Die", "tr\u00fc\u00b7ben", "Ple\u00b7ja\u00b7des", ",", "O\u00b7rions", "finst\u00b7re", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "VVFIN", "NN", "$."], "meter": "-+-+--+---+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Die Zwillinge, der Widder und der Stier", "tokens": ["Die", "Zwil\u00b7lin\u00b7ge", ",", "der", "Wid\u00b7der", "und", "der", "Stier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Verk\u00fcndigen des Fr\u00fchlings holde Zier.", "tokens": ["Ver\u00b7k\u00fcn\u00b7di\u00b7gen", "des", "Fr\u00fch\u00b7lings", "hol\u00b7de", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Es ist ein\u2019 herrlich-gro\u00df\u2019 und nimmerfehlend\u2019 Uhr,", "tokens": ["Es", "ist", "ein'", "herr\u00b7lich\u00b7gro\u00df'", "und", "nim\u00b7mer\u00b7feh\u00b7lend'", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVIMP", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die allen Menschen zeigt die Regel-rechte Spur.", "tokens": ["Die", "al\u00b7len", "Men\u00b7schen", "zeigt", "die", "Re\u00b7gel\u00b7rech\u00b7te", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}