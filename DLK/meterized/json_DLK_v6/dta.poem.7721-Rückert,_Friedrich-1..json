{"dta.poem.7721": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo schroff ein Vorgebirg ins Meer die Stirne schiebt,", "tokens": ["Wo", "schroff", "ein", "Vor\u00b7ge\u00b7birg", "ins", "Meer", "die", "Stir\u00b7ne", "schiebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und am geh\u00f6lten Fu\u00df in Schaum die Brandung stiebt,", "tokens": ["Und", "am", "ge\u00b7h\u00f6l\u00b7ten", "Fu\u00df", "in", "Schaum", "die", "Bran\u00b7dung", "stiebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hat seine Siedelei ein frommer Mann gebaut,", "tokens": ["Hat", "sei\u00b7ne", "Sie\u00b7de\u00b7lei", "ein", "from\u00b7mer", "Mann", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo seinen Horst zu baun der Adler nicht getraut.", "tokens": ["Wo", "sei\u00b7nen", "Horst", "zu", "baun", "der", "Ad\u00b7ler", "nicht", "ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PTKZU", "VVINF", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vom kahlen Baume, den der Fels mit Zittern tr\u00e4gt,", "tokens": ["Vom", "kah\u00b7len", "Bau\u00b7me", ",", "den", "der", "Fels", "mit", "Zit\u00b7tern", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht er dem Abgrund zu, der Todeswogen schl\u00e4gt.", "tokens": ["Sieht", "er", "dem", "Ab\u00b7grund", "zu", ",", "der", "To\u00b7des\u00b7wo\u00b7gen", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So oft er auf der Flut gewahrt ein schwankes Bret", "tokens": ["So", "oft", "er", "auf", "der", "Flut", "ge\u00b7wahrt", "ein", "schwan\u00b7kes", "Bret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Menschenleben, hebt die H\u00e4nd' er zum Gebet.", "tokens": ["Mit", "Men\u00b7schen\u00b7le\u00b7ben", ",", "hebt", "die", "H\u00e4nd'", "er", "zum", "Ge\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Und ehr nicht im Gebet l\u00e4\u00dft er die H\u00e4nde sinken,", "tokens": ["Und", "ehr", "nicht", "im", "Ge\u00b7bet", "l\u00e4\u00dft", "er", "die", "H\u00e4n\u00b7de", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Bis fern das Schiff entflohn den Zacken und den Zinken.", "tokens": ["Bis", "fern", "das", "Schiff", "ent\u00b7flohn", "den", "Za\u00b7cken", "und", "den", "Zin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "NN", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Selbst hat er einst erprobt, das nun um andre tobt,", "tokens": ["Selbst", "hat", "er", "einst", "er\u00b7probt", ",", "das", "nun", "um", "and\u00b7re", "tobt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PRELS", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Meer des Sturms, da hat er dis Gel\u00fcbd gelobt.", "tokens": ["Das", "Meer", "des", "Sturms", ",", "da", "hat", "er", "dis", "Ge\u00b7l\u00fcbd", "ge\u00b7lobt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht schirmen kann er euch, noch warnen vor den Riffen,", "tokens": ["Nicht", "schir\u00b7men", "kann", "er", "euch", ",", "noch", "war\u00b7nen", "vor", "den", "Rif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch beten, da\u00df sie Gott euch gn\u00e4dig lass' umschiffen.", "tokens": ["Doch", "be\u00b7ten", ",", "da\u00df", "sie", "Gott", "euch", "gn\u00e4\u00b7dig", "lass'", "um\u00b7schif\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "PPER", "ADJD", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}