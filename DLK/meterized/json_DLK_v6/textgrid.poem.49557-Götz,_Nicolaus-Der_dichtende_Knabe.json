{"textgrid.poem.49557": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der dichtende Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieh nicht den Amor,", "tokens": ["Flieh", "nicht", "den", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "O zarte Schwester,", "tokens": ["O", "zar\u00b7te", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Flieh nicht den Amor,", "tokens": ["Flieh", "nicht", "den", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Er kriegt dich doch.", "tokens": ["Er", "kriegt", "dich", "doch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich roch im Garten", "tokens": ["Ich", "roch", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "An einer Nelke,", "tokens": ["An", "ei\u00b7ner", "Nel\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In deren Schoose", "tokens": ["In", "de\u00b7ren", "Schoo\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Der Kleine sa\u00df.", "tokens": ["Der", "Klei\u00b7ne", "sa\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mit ihren D\u00fcften,", "tokens": ["Mit", "ih\u00b7ren", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den s\u00fcsen D\u00fcften,", "tokens": ["Den", "s\u00fc\u00b7sen", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die mich vergn\u00fcgten,", "tokens": ["Die", "mich", "ver\u00b7gn\u00fcg\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sog ich ihn ein.", "tokens": ["Sog", "ich", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich armes Kn\u00e4bgen!", "tokens": ["Ich", "ar\u00b7mes", "Kn\u00e4b\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wer kann mir rathen,", "tokens": ["Wer", "kann", "mir", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In meinem Haupte", "tokens": ["In", "mei\u00b7nem", "Haup\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ist er nun Herr.", "tokens": ["Ist", "er", "nun", "Herr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und dieses Liedgen,", "tokens": ["Und", "die\u00b7ses", "Lied\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wie er so kleine,", "tokens": ["Wie", "er", "so", "klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und dieses Liedgen", "tokens": ["Und", "die\u00b7ses", "Lied\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ist schon von ihm!", "tokens": ["Ist", "schon", "von", "ihm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}