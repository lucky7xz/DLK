{"textgrid.poem.41564": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am f\u00fcnfzehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sprach er: \u00bbGehet hin, den Priestern zeiget euch!\u00ab", "tokens": ["Da", "sprach", "er", ":", "\u00bb", "Ge\u00b7het", "hin", ",", "den", "Pries\u00b7tern", "zei\u00b7get", "euch", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und als sie gingen, siehe da, sie wurden rein.", "tokens": ["Und", "als", "sie", "gin\u00b7gen", ",", "sie\u00b7he", "da", ",", "sie", "wur\u00b7den", "rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVIMP", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du meine stolze Seele, nur an Elend reich,", "tokens": ["Du", "mei\u00b7ne", "stol\u00b7ze", "See\u00b7le", ",", "nur", "an", "E\u00b7lend", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An Fehlen gro\u00df, so k\u00f6nnte dir geholfen sein,", "tokens": ["An", "Feh\u00b7len", "gro\u00df", ",", "so", "k\u00f6nn\u00b7te", "dir", "ge\u00b7hol\u00b7fen", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADV", "VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dir, die noch stets verschm\u00e4hte Menschenhand", "tokens": ["Dir", ",", "die", "noch", "stets", "ver\u00b7schm\u00e4h\u00b7te", "Men\u00b7schen\u00b7hand"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und w\u00e4r' sie gottgeweiht und w\u00e4r' sie gottgesandt.", "tokens": ["Und", "w\u00e4r'", "sie", "gott\u00b7ge\u00b7weiht", "und", "w\u00e4r'", "sie", "gott\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "KON", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wohl sprichst du \u00f6fters zu dir selbst in argem Trug:", "tokens": ["Wohl", "sprichst", "du", "\u00f6f\u00b7ters", "zu", "dir", "selbst", "in", "ar\u00b7gem", "Trug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er ist der Starke, so allein mich retten kann;", "tokens": ["Er", "ist", "der", "Star\u00b7ke", ",", "so", "al\u00b7lein", "mich", "ret\u00b7ten", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hilft er mir nicht, dann ist auch Menschenrat ein Lug,", "tokens": ["Hilft", "er", "mir", "nicht", ",", "dann", "ist", "auch", "Men\u00b7schen\u00b7rat", "ein", "Lug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Unmittelbar zu ihm mein Flehen steig' hinan!", "tokens": ["Un\u00b7mit\u00b7tel\u00b7bar", "zu", "ihm", "mein", "Fle\u00b7hen", "steig'", "hi\u00b7nan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und f\u00fchlst es nicht, da\u00df warm und reich gehegt", "tokens": ["Und", "f\u00fchlst", "es", "nicht", ",", "da\u00df", "warm", "und", "reich", "ge\u00b7hegt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADJD", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Hochmut Aussatz an dein t\u00f6richt Herz gelegt.", "tokens": ["Der", "Hoch\u00b7mut", "Aus\u00b7satz", "an", "dein", "t\u00f6\u00b7richt", "Herz", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist denn so fest dein Mut, in reichem Glauben stark,", "tokens": ["Ist", "denn", "so", "fest", "dein", "Mut", ",", "in", "rei\u00b7chem", "Glau\u00b7ben", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df eines Freundes Hand er sich entschlagen darf?", "tokens": ["Da\u00df", "ei\u00b7nes", "Freun\u00b7des", "Hand", "er", "sich", "ent\u00b7schla\u00b7gen", "darf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So klar dein Hirn, so saftig und gesund dein Mark,", "tokens": ["So", "klar", "dein", "Hirn", ",", "so", "saf\u00b7tig", "und", "ge\u00b7sund", "dein", "Mark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df die Erkenntnis dir vor andern Wesen scharf?", "tokens": ["Da\u00df", "die", "Er\u00b7kennt\u00b7nis", "dir", "vor", "an\u00b7dern", "We\u00b7sen", "scharf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O sei dem\u00fctig, sprich es offen aus:", "tokens": ["O", "sei", "de\u00b7m\u00fc\u00b7tig", ",", "sprich", "es", "of\u00b7fen", "aus", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du lebst ein Bettler und in eines Bettlers Haus!", "tokens": ["Du", "lebst", "ein", "Bett\u00b7ler", "und", "in", "ei\u00b7nes", "Bett\u00b7lers", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie arm und schwach du, Seele mein, das meinst du wohl", "tokens": ["Wie", "arm", "und", "schwach", "du", ",", "See\u00b7le", "mein", ",", "das", "meinst", "du", "wohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "PPER", "$,", "NN", "PPOSAT", "$,", "PDS", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu f\u00fchlen, wenn die Lippe matt und klagend spricht;", "tokens": ["Zu", "f\u00fch\u00b7len", ",", "wenn", "die", "Lip\u00b7pe", "matt", "und", "kla\u00b7gend", "spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "PTKVZ", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und doch nur Klang, und doch nur Rauschen, schw\u00fclstig hohl,", "tokens": ["Und", "doch", "nur", "Klang", ",", "und", "doch", "nur", "Rau\u00b7schen", ",", "schw\u00fcls\u00b7tig", "hohl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "NN", "$,", "KON", "ADV", "ADV", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie umgestaltet aus dem Sprachrohr Fl\u00fcstern bricht,", "tokens": ["Wie", "um\u00b7ge\u00b7stal\u00b7tet", "aus", "dem", "Sprach\u00b7rohr", "Fl\u00fcs\u00b7tern", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Aufschrei nur, der willenlos entf\u00e4hrt,", "tokens": ["Ein", "Auf\u00b7schrei", "nur", ",", "der", "wil\u00b7len\u00b7los", "ent\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Indes dein d\u00fcstrer Blick sich stolz nach innen kehrt!", "tokens": ["In\u00b7des", "dein", "d\u00fcst\u00b7rer", "Blick", "sich", "stolz", "nach", "in\u00b7nen", "kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PRF", "ADJD", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was ist da drinnen denn so Herrliches zu schaun?", "tokens": ["Was", "ist", "da", "drin\u00b7nen", "denn", "so", "Herr\u00b7li\u00b7ches", "zu", "schaun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ADV", "ADV", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein krankes Blut, was ach! in eignem Druck erliegt,", "tokens": ["Ein", "kran\u00b7kes", "Blut", ",", "was", "ach", "!", "in", "eig\u00b7nem", "Druck", "er\u00b7liegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "XY", "$.", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was jedes Reizes Sklav' und jeder Stimmung, traun,", "tokens": ["Was", "je\u00b7des", "Rei\u00b7zes", "Skla\u00b7v'", "und", "je\u00b7der", "Stim\u00b7mung", ",", "traun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "NN", "KON", "PIAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bald steht wie ein Morast, bald wie ein Strudel fliegt;", "tokens": ["Bald", "steht", "wie", "ein", "Mo\u00b7rast", ",", "bald", "wie", "ein", "Stru\u00b7del", "fliegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Hirn, von dem dir selber unbekannt,", "tokens": ["Ein", "Hirn", ",", "von", "dem", "dir", "sel\u00b7ber", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ob es dem Wahnsinn oder Frevel mehr verwandt.", "tokens": ["Ob", "es", "dem", "Wahn\u00b7sinn", "o\u00b7der", "Fre\u00b7vel", "mehr", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Dies sind die Sch\u00e4tze, die dich stolz und stark gemacht,", "tokens": ["Dies", "sind", "die", "Sch\u00e4t\u00b7ze", ",", "die", "dich", "stolz", "und", "stark", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du entschlagen dich hast des Geschaffnen Rat;", "tokens": ["Da\u00df", "du", "ent\u00b7schla\u00b7gen", "dich", "hast", "des", "Ge\u00b7schaff\u00b7nen", "Rat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dies sind die Leuchten, die in dumpfen Zweifelns", "tokens": ["Dies", "sind", "die", "Leuch\u00b7ten", ",", "die", "in", "dum\u00b7pfen", "Zwei\u00b7felns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nacht Glorreich bestrahlen sollen den verborgnen Pfad;", "tokens": ["Nacht", "Glor\u00b7reich", "be\u00b7strah\u00b7len", "sol\u00b7len", "den", "ver\u00b7borg\u00b7nen", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Darum, darum baust du auf Gott allein,", "tokens": ["Da\u00b7rum", ",", "da\u00b7rum", "baust", "du", "auf", "Gott", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Menschentadels Dorn dir m\u00f6g' erlassen sein.", "tokens": ["Da\u00df", "Men\u00b7schen\u00b7ta\u00b7dels", "Dorn", "dir", "m\u00f6g'", "er\u00b7las\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Hast anders jemals du des Priesters wohl gedacht,", "tokens": ["Hast", "an\u00b7ders", "je\u00b7mals", "du", "des", "Pries\u00b7ters", "wohl", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der lossprach deine Schuld im heil'gen Sakrament,", "tokens": ["Der", "los\u00b7sprach", "dei\u00b7ne", "Schuld", "im", "heil'\u00b7gen", "Sak\u00b7ra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als wie des Blattes, drauf der Schuldner Rechnung macht", "tokens": ["Als", "wie", "des", "Blat\u00b7tes", ",", "drauf", "der", "Schuld\u00b7ner", "Rech\u00b7nung", "macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "PAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch einzig Gl\u00e4ub'gers Schrift als L\u00f6sung anerkennt?", "tokens": ["Doch", "ein\u00b7zig", "Gl\u00e4ub'\u00b7gers", "Schrift", "als", "L\u00f6\u00b7sung", "an\u00b7er\u00b7kennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ward sichtbar jemals dir in seiner Hand", "tokens": ["Ward", "sicht\u00b7bar", "je\u00b7mals", "dir", "in", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die ernste Waage, drauf dein Tod und Leben stand?", "tokens": ["Die", "erns\u00b7te", "Waa\u00b7ge", ",", "drauf", "dein", "Tod", "und", "Le\u00b7ben", "stand", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PAV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Knie hin, knie hin, doch nicht an jener Gnadenstatt;", "tokens": ["Knie", "hin", ",", "knie", "hin", ",", "doch", "nicht", "an", "je\u00b7ner", "Gna\u00b7den\u00b7statt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "ADV", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nein, vor dem Hirten nur in seiner W\u00fcrde Kraft!", "tokens": ["Nein", ",", "vor", "dem", "Hir\u00b7ten", "nur", "in", "sei\u00b7ner", "W\u00fcr\u00b7de", "Kraft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deine Seele sei vor ihm ein offnes Blatt", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "sei", "vor", "ihm", "ein", "off\u00b7nes", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In aller Eitelkeit und niedern Leidenschaft.", "tokens": ["In", "al\u00b7ler", "Ei\u00b7tel\u00b7keit", "und", "nie\u00b7dern", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wenn du dich vor Menschenhand gebeugt:", "tokens": ["Und", "wenn", "du", "dich", "vor", "Men\u00b7schen\u00b7hand", "ge\u00b7beugt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann schau ob sich am Aussatz nicht ein heilend Fleckchen zeigt!", "tokens": ["Dann", "schau", "ob", "sich", "am", "Aus\u00b7satz", "nicht", "ein", "hei\u00b7lend", "Fleck\u00b7chen", "zeigt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "PRF", "APPRART", "NN", "PTKNEG", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}