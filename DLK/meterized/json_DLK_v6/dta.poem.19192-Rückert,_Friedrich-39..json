{"dta.poem.19192": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willst du erquickenden traumlosen Schlaf genie\u00dfen,", "tokens": ["Willst", "du", "er\u00b7qui\u00b7cken\u00b7den", "traum\u00b7lo\u00b7sen", "Schlaf", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "La\u00df wach dich im Genu\u00df nicht M\u00e4\u00dfigkeit verdrie\u00dfen.", "tokens": ["La\u00df", "wach", "dich", "im", "Ge\u00b7nu\u00df", "nicht", "M\u00e4\u00b7\u00dfig\u00b7keit", "ver\u00b7drie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "APPRART", "NN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und so im Leben auch sei m\u00e4\u00dfig, wenn begraben", "tokens": ["Und", "so", "im", "Le\u00b7ben", "auch", "sei", "m\u00e4\u00b7\u00dfig", ",", "wenn", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "VAFIN", "ADJD", "$,", "KOUS", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du ruhn in Gott willst und nicht b\u00f6se Tr\u00e4ume haben.", "tokens": ["Du", "ruhn", "in", "Gott", "willst", "und", "nicht", "b\u00f6\u00b7se", "Tr\u00e4u\u00b7me", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "APPR", "NN", "VMFIN", "KON", "PTKNEG", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sieh, welchen Lohn der Seel' hat M\u00e4\u00dfigung beschieden!", "tokens": ["Sieh", ",", "wel\u00b7chen", "Lohn", "der", "Seel'", "hat", "M\u00e4\u00b7\u00dfi\u00b7gung", "be\u00b7schie\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAT", "NN", "ART", "NN", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Wachen und im Schlaf, im Tod und Leben Frieden.", "tokens": ["Im", "Wa\u00b7chen", "und", "im", "Schlaf", ",", "im", "Tod", "und", "Le\u00b7ben", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,", "APPRART", "NN", "KON", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}