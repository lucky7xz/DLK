{"dta.poem.18234": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Dreht sich die Welt,", "tokens": ["Dreht", "sich", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ihr Pfad wird breiter,", "tokens": ["Ihr", "Pfad", "wird", "brei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ihr Triebrad schnellt;", "tokens": ["Ihr", "Trie\u00b7brad", "schnellt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Stunden rollen,", "tokens": ["Die", "Stun\u00b7den", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Sonne scheint,", "tokens": ["Die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich bin verschollen", "tokens": ["Ich", "bin", "ver\u00b7schol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und niemand weint!", "tokens": ["Und", "nie\u00b7mand", "weint", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "In Kraut und Kressen", "tokens": ["In", "Kraut", "und", "Kres\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Auf hohem Stein", "tokens": ["Auf", "ho\u00b7hem", "Stein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Lieg ich vergessen", "tokens": ["Lieg", "ich", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVPP"], "meter": "++-+-", "measure": "iambic.di"}, "line.11": {"text": "Und ganz allein;", "tokens": ["Und", "ganz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Nur eine Linde", "tokens": ["Nur", "ei\u00b7ne", "Lin\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Schwingt \u00fcber mir", "tokens": ["Schwingt", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Im Abendwinde", "tokens": ["Im", "Ab\u00b7end\u00b7win\u00b7de"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Ihr gr\u00fcn Panier,", "tokens": ["Ihr", "gr\u00fcn", "Pa\u00b7nier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und leis nur zittert", "tokens": ["Und", "leis", "nur", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Mir ums Gesicht,", "tokens": ["Mir", "ums", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Goldrothumwittert,", "tokens": ["Gol\u00b7dro\u00b7thum\u00b7wit\u00b7tert", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Das Abendlicht.", "tokens": ["Das", "A\u00b7bend\u00b7licht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Welt ging unter,", "tokens": ["Die", "Welt", "ging", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Gott erschuf,", "tokens": ["Die", "Gott", "er\u00b7schuf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur noch mitunter", "tokens": ["Nur", "noch", "mi\u00b7tun\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ein Vogelruf;", "tokens": ["Ein", "Vo\u00b7gel\u00b7ruf", ";"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nur noch zuweilen", "tokens": ["Nur", "noch", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ein irrer Schrei \u2014", "tokens": ["Ein", "ir\u00b7rer", "Schrei"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Wolken eilen", "tokens": ["Die", "Wol\u00b7ken", "ei\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Vorbei, vorbei!", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Was wie ein Stern mir", "tokens": ["Was", "wie", "ein", "Stern", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "KOKOM", "ART", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Brust durchzieht,", "tokens": ["Die", "Brust", "durch\u00b7zieht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Singt nun von fern mir", "tokens": ["Singt", "nun", "von", "fern", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJD", "PPER"], "meter": "+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sein Alphornlied.", "tokens": ["Sein", "Al\u00b7phorn\u00b7lied", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Erinnrung h\u00e4lt mich", "tokens": ["E\u00b7rinn\u00b7rung", "h\u00e4lt", "mich"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In ihrem Bann", "tokens": ["In", "ih\u00b7rem", "Bann"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und pl\u00f6tzlich f\u00e4llt mich", "tokens": ["Und", "pl\u00f6tz\u00b7lich", "f\u00e4llt", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Die Sehnsucht an.", "tokens": ["Die", "Sehn\u00b7sucht", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O Lust von weiland,", "tokens": ["O", "Lust", "von", "wei\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wie liegst du weit!", "tokens": ["Wie", "liegst", "du", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "O selig Eiland", "tokens": ["O", "se\u00b7lig", "Ei\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Jugendzeit!", "tokens": ["Der", "Ju\u00b7gend\u00b7zeit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Blumen bl\u00fchten,", "tokens": ["Die", "Blu\u00b7men", "bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Die Quelle sprang,", "tokens": ["Die", "Quel\u00b7le", "sprang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Sterne gl\u00fchten,", "tokens": ["Die", "Ster\u00b7ne", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Die Amsel sang", "tokens": ["Die", "Am\u00b7sel", "sang"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und mir gab K\u00fcsse", "tokens": ["Und", "mir", "gab", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Zu jeder Stund,", "tokens": ["Zu", "je\u00b7der", "Stund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Als ob er's m\u00fcsse,", "tokens": ["Als", "ob", "er's", "m\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ein M\u00e4dchenmund!", "tokens": ["Ein", "M\u00e4d\u00b7chen\u00b7mund", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Noch stockt der Schmerz mir", "tokens": ["Noch", "stockt", "der", "Schmerz", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In seinem Lauf \u2014", "tokens": ["In", "sei\u00b7nem", "Lauf"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wie ging das Herz mir", "tokens": ["Wie", "ging", "das", "Herz", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In Liedern auf!", "tokens": ["In", "Lie\u00b7dern", "auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Doch wer beschriebe", "tokens": ["Doch", "wer", "be\u00b7schrie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWS", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Die goldne Zeit,", "tokens": ["Die", "gold\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Die erste Liebe,", "tokens": ["Die", "ers\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Das erste Leid?", "tokens": ["Das", "ers\u00b7te", "Leid", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Wie dort die Sonne", "tokens": ["Wie", "dort", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Versinkt in Nacht,", "tokens": ["Ver\u00b7sinkt", "in", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Stirbt Weh und Wonne,", "tokens": ["Stirbt", "Weh", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Eh wir's gedacht.", "tokens": ["Eh", "wir's", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Schon deckt ihr Schleier", "tokens": ["Schon", "deckt", "ihr", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Den Flu\u00df, das Ried \u2014", "tokens": ["Den", "Flu\u00df", ",", "das", "Ried"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Die alte Leier,", "tokens": ["Die", "al\u00b7te", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Das alte Lied!", "tokens": ["Das", "al\u00b7te", "Lied", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}