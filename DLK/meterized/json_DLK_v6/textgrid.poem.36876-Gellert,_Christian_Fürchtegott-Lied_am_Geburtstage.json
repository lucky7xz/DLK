{"textgrid.poem.36876": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Lied am Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir dank ich heute f\u00fcr mein Leben;", "tokens": ["Dir", "dank", "ich", "heu\u00b7te", "f\u00fcr", "mein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Tage, da du mir's gegeben,", "tokens": ["Am", "Ta\u00b7ge", ",", "da", "du", "mir's", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dank ich dir, Gott, daf\u00fcr.", "tokens": ["Dank", "ich", "dir", ",", "Gott", ",", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "$,", "NN", "$,", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch freie Gnad allein bewogen,", "tokens": ["Durch", "frei\u00b7e", "Gnad", "al\u00b7lein", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hast du mich aus dem Nichts gezogen;", "tokens": ["Hast", "du", "mich", "aus", "dem", "Nichts", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch deine G\u00fcte bin ich hier.", "tokens": ["Durch", "dei\u00b7ne", "G\u00fc\u00b7te", "bin", "ich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast mich wunderbar bereitet,", "tokens": ["Du", "hast", "mich", "wun\u00b7der\u00b7bar", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An deiner Rechten mich geleitet,", "tokens": ["An", "dei\u00b7ner", "Rech\u00b7ten", "mich", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis diesen Augenblick.", "tokens": ["Bis", "die\u00b7sen", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du gabst mir tausend frohe Tage,", "tokens": ["Du", "gabst", "mir", "tau\u00b7send", "fro\u00b7he", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verwandeltest selbst meine Klage", "tokens": ["Ver\u00b7wan\u00b7del\u00b7test", "selbst", "mei\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und meine Leiden in mein Gl\u00fcck.", "tokens": ["Und", "mei\u00b7ne", "Lei\u00b7den", "in", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich bin der Treue zu geringe,", "tokens": ["Ich", "bin", "der", "Treu\u00b7e", "zu", "ge\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der du, Herrscher aller Dinge,", "tokens": ["Mit", "der", "du", ",", "Herr\u00b7scher", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stets \u00fcber mich gewacht.", "tokens": ["Stets", "\u00fc\u00b7ber", "mich", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O Gott! damit ich gl\u00fccklich werde,", "tokens": ["O", "Gott", "!", "da\u00b7mit", "ich", "gl\u00fcck\u00b7lich", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hast du an mich, mich Staub und Erde,", "tokens": ["Hast", "du", "an", "mich", ",", "mich", "Staub", "und", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Ewigkeit her schon gedacht!", "tokens": ["Von", "E\u00b7wig\u00b7keit", "her", "schon", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APZR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du sahst und h\u00f6rtest schon mein Sehnen,", "tokens": ["Du", "sahst", "und", "h\u00f6r\u00b7test", "schon", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und z\u00e4hltest alle meine Tr\u00e4nen,", "tokens": ["Und", "z\u00e4hl\u00b7test", "al\u00b7le", "mei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh ich bereitet war;", "tokens": ["Eh", "ich", "be\u00b7rei\u00b7tet", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wogst, eh ich zu sein begonnte,", "tokens": ["Und", "wogst", ",", "eh", "ich", "zu", "sein", "be\u00b7gonn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Eh ich zu dir noch rufen konnte,", "tokens": ["Eh", "ich", "zu", "dir", "noch", "ru\u00b7fen", "konn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir mein bescheiden Teil schon dar.", "tokens": ["Mir", "mein", "be\u00b7schei\u00b7den", "Teil", "schon", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du lie\u00dft mich Gnade vor dir finden;", "tokens": ["Du", "lie\u00dft", "mich", "Gna\u00b7de", "vor", "dir", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sahst doch alle meine S\u00fcnden", "tokens": ["Und", "sahst", "doch", "al\u00b7le", "mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vorher von Ewigkeit.", "tokens": ["Vor\u00b7her", "von", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "O welche Liebe! welch Erbarmen!", "tokens": ["O", "wel\u00b7che", "Lie\u00b7be", "!", "welch", "Er\u00b7bar\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PWAT", "NN", "$.", "PWAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Herr der Welt sorgt f\u00fcr die Armen,", "tokens": ["Der", "Herr", "der", "Welt", "sorgt", "f\u00fcr", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ist ein Vater, der verzeiht.", "tokens": ["Und", "ist", "ein", "Va\u00b7ter", ",", "der", "ver\u00b7zeiht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcr alle Wunder deiner Treue,", "tokens": ["F\u00fcr", "al\u00b7le", "Wun\u00b7der", "dei\u00b7ner", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alles, dessen ich mich freue,", "tokens": ["F\u00fcr", "al\u00b7les", ",", "des\u00b7sen", "ich", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELAT", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lobsinget dir mein Geist.", "tokens": ["Lob\u00b7sin\u00b7get", "dir", "mein", "Geist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er selber ist dein gr\u00f6\u00dft Geschenke;", "tokens": ["Er", "sel\u00b7ber", "ist", "dein", "gr\u00f6\u00dft", "Ge\u00b7schen\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dein ist's, da\u00df ich durch ihn dich denke,", "tokens": ["Dein", "ist's", ",", "da\u00df", "ich", "durch", "ihn", "dich", "den\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$,", "KOUS", "PPER", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und dein, da\u00df er dich heute preist.", "tokens": ["Und", "dein", ",", "da\u00df", "er", "dich", "heu\u00b7te", "preist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df du mein Leben mir gefristet,", "tokens": ["Da\u00df", "du", "mein", "Le\u00b7ben", "mir", "ge\u00b7fris\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit St\u00e4rk und Kraft mich ausger\u00fcstet,", "tokens": ["Mit", "St\u00e4rk", "und", "Kraft", "mich", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies, Vater, dank ich dir;", "tokens": ["Dies", ",", "Va\u00b7ter", ",", "dank", "ich", "dir", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "APPR", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du mich wunderbar gef\u00fchret,", "tokens": ["Da\u00df", "du", "mich", "wun\u00b7der\u00b7bar", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit deinem Geiste mich regieret,", "tokens": ["Mit", "dei\u00b7nem", "Geis\u00b7te", "mich", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies alles, Vater, dank ich dir.", "tokens": ["Dies", "al\u00b7les", ",", "Va\u00b7ter", ",", "dank", "ich", "dir", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "NN", "$,", "APPR", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Soll ich, o Gott! noch l\u00e4nger leben:", "tokens": ["Soll", "ich", ",", "o", "Gott", "!", "noch", "l\u00e4n\u00b7ger", "le\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$.", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wirst du, was mir gut ist, geben;", "tokens": ["So", "wirst", "du", ",", "was", "mir", "gut", "ist", ",", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PWS", "PPER", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du gibst's, ich hoff auf dich.", "tokens": ["Du", "gibst's", ",", "ich", "hoff", "auf", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dir, Gott, befehl ich Leib und Seele.", "tokens": ["Dir", ",", "Gott", ",", "be\u00b7fehl", "ich", "Leib", "und", "See\u00b7le", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "KOUS", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Herr Herr, dem ich sie befehle,", "tokens": ["Der", "Herr", "Herr", ",", "dem", "ich", "sie", "be\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der segne und beh\u00fcte mich!", "tokens": ["Der", "seg\u00b7ne", "und", "be\u00b7h\u00fc\u00b7te", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}