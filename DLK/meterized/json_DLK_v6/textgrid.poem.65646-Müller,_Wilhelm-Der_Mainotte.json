{"textgrid.poem.65646": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Mainotte", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie, nie hat ein Sklavenjoch meinen starken Hals gebogen,", "tokens": ["Nie", ",", "nie", "hat", "ein", "Skla\u00b7ven\u00b7joch", "mei\u00b7nen", "star\u00b7ken", "Hals", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nie hab' ich an meinem Arm eine Kettenlast gewogen.", "tokens": ["Nie", "hab'", "ich", "an", "mei\u00b7nem", "Arm", "ei\u00b7ne", "Ket\u00b7ten\u00b7last", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Frei, wie meiner Berge Strom, wie der Adler in den L\u00fcften,", "tokens": ["Frei", ",", "wie", "mei\u00b7ner", "Ber\u00b7ge", "Strom", ",", "wie", "der", "Ad\u00b7ler", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "NN", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "St\u00fcrz' ich brausend in die Fl\u00e4che, wo die Freiheit liegt in Gr\u00fcften,", "tokens": ["St\u00fcrz'", "ich", "brau\u00b7send", "in", "die", "Fl\u00e4\u00b7che", ",", "wo", "die", "Frei\u00b7heit", "liegt", "in", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Neben altem Heldenstaube, unter grauen Mauertr\u00fcmmern,", "tokens": ["Ne\u00b7ben", "al\u00b7tem", "Hel\u00b7den\u00b7stau\u00b7be", ",", "un\u00b7ter", "grau\u00b7en", "Mau\u00b7er\u00b7tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und mir ist, als h\u00f6rt' ich sie unter mir vernehmlich wimmern.", "tokens": ["Und", "mir", "ist", ",", "als", "h\u00f6rt'", "ich", "sie", "un\u00b7ter", "mir", "ver\u00b7nehm\u00b7lich", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "PPER", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-++-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "R\u00e4uber hei\u00df' ich bei dem Wicht, der den R\u00e4uber nennt Gebieter,", "tokens": ["R\u00e4u\u00b7ber", "hei\u00df'", "ich", "bei", "dem", "Wicht", ",", "der", "den", "R\u00e4u\u00b7ber", "nennt", "Ge\u00b7bie\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Jenen R\u00e4uber, der ihm hat ", "tokens": ["Je\u00b7nen", "R\u00e4u\u00b7ber", ",", "der", "ihm", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Freiheit, Freiheit, Lebensluft, Leibesmark und Seelenschwinge,", "tokens": ["Frei\u00b7heit", ",", "Frei\u00b7heit", ",", "Le\u00b7bens\u00b7luft", ",", "Lei\u00b7bes\u00b7mark", "und", "See\u00b7len\u00b7schwin\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Der geh\u00f6rt mein Herz, mein Arm, meine B\u00fcchs' und meine Klinge,", "tokens": ["Der", "ge\u00b7h\u00f6rt", "mein", "Herz", ",", "mein", "Arm", ",", "mei\u00b7ne", "B\u00fcchs'", "und", "mei\u00b7ne", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Der ich wache, der ich k\u00e4mpfe, der ich lebe, der ich sterbe,", "tokens": ["Der", "ich", "wa\u00b7che", ",", "der", "ich", "k\u00e4mp\u00b7fe", ",", "der", "ich", "le\u00b7be", ",", "der", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Die ich meinen Kindern lasse als mein einig eignes Erde.", "tokens": ["Die", "ich", "mei\u00b7nen", "Kin\u00b7dern", "las\u00b7se", "als", "mein", "ei\u00b7nig", "eig\u00b7nes", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "KOKOM", "PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "R\u00e4uber nennt mich immerhin! Rauben will ich und verheeren", "tokens": ["R\u00e4u\u00b7ber", "nennt", "mich", "im\u00b7mer\u00b7hin", "!", "Rau\u00b7ben", "will", "ich", "und", "ver\u00b7hee\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$.", "NN", "VMFIN", "PPER", "KON", "VVINF"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Herrengut und Sklavenland, und kein Pascha wird es wehren.", "tokens": ["Her\u00b7ren\u00b7gut", "und", "Skla\u00b7ven\u00b7land", ",", "und", "kein", "Pa\u00b7scha", "wird", "es", "weh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$,", "KON", "PIAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Aber h\u00f6rt, ihr Feldbewohner, h\u00f6rt, der R\u00e4uber kann auch geben", "tokens": ["A\u00b7ber", "h\u00f6rt", ",", "ihr", "Feld\u00b7be\u00b7woh\u00b7ner", ",", "h\u00f6rt", ",", "der", "R\u00e4u\u00b7ber", "kann", "auch", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "ART", "NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Mehr, mehr als ihr habt besessen all' in eurem ganzen Leben.", "tokens": ["Mehr", ",", "mehr", "als", "ihr", "habt", "be\u00b7ses\u00b7sen", "all'", "in", "eu\u00b7rem", "gan\u00b7zen", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "KOUS", "PPER", "VAFIN", "VVPP", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Wollt ihr eure Freiheit wieder? Kommt herauf mit scharfen Klingen!", "tokens": ["Wollt", "ihr", "eu\u00b7re", "Frei\u00b7heit", "wie\u00b7der", "?", "Kommt", "her\u00b7auf", "mit", "schar\u00b7fen", "Klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "$.", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Von den Bergen wollen wir sie vereint herunter bringen.", "tokens": ["Von", "den", "Ber\u00b7gen", "wol\u00b7len", "wir", "sie", "ver\u00b7eint", "her\u00b7un\u00b7ter", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}