{"textgrid.poem.58259": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "297. Vereinigung des Willens", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Wille l\u00e4\u00dft die Welt, Vernunft und Sinne dar", "tokens": ["Mein", "Wil\u00b7le", "l\u00e4\u00dft", "die", "Welt", ",", "Ver\u00b7nunft", "und", "Sin\u00b7ne", "dar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich vereinigt sanft mit Gott im stillen Grunde;", "tokens": ["Und", "sich", "ver\u00b7ei\u00b7nigt", "sanft", "mit", "Gott", "im", "stil\u00b7len", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "ADJD", "APPR", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Herzen, nicht im Kopf, macht Gott sich offenbar,", "tokens": ["Im", "Her\u00b7zen", ",", "nicht", "im", "Kopf", ",", "macht", "Gott", "sich", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PTKNEG", "APPRART", "NN", "$,", "VVFIN", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Werd nur ein stilles Kind, so find'st du ihn zur Stunde!", "tokens": ["Werd", "nur", "ein", "stil\u00b7les", "Kind", ",", "so", "fin\u00b7d'st", "du", "ihn", "zur", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}