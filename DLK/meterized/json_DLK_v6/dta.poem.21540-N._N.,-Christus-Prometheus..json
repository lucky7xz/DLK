{"dta.poem.21540": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Christus-Prometheus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Und wieder kam die erste Osternacht,", "tokens": ["Und", "wie\u00b7der", "kam", "die", "ers\u00b7te", "Os\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und \u201eAuferstehen\u201c jauchzt es aller Landen,", "tokens": ["Und", "\u201e", "Auf\u00b7er\u00b7ste\u00b7hen", "\u201c", "jauchzt", "es", "al\u00b7ler", "Lan\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$(", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da f\u00fchrt ein Geist mich fort mit Zaubermacht:", "tokens": ["Da", "f\u00fchrt", "ein", "Geist", "mich", "fort", "mit", "Zau\u00b7ber\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf steilem Felsen liegt ein Mensch in Banden,", "tokens": ["Auf", "stei\u00b7lem", "Fel\u00b7sen", "liegt", "ein", "Mensch", "in", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Allein, sein einziger Genosse ist", "tokens": ["Al\u00b7lein", ",", "sein", "ein\u00b7zi\u00b7ger", "Ge\u00b7nos\u00b7se", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "VAFIN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ein Adler, der an seinem Herzen zehrt und fri\u00dft.", "tokens": ["Ein", "Ad\u00b7ler", ",", "der", "an", "sei\u00b7nem", "Her\u00b7zen", "zehrt", "und", "fri\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Am Felsen festgebannt durch starken Stahl,", "tokens": ["Am", "Fel\u00b7sen", "fest\u00b7ge\u00b7bannt", "durch", "star\u00b7ken", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Mark durchw\u00fchlen Schmerzen nimmergleiche,", "tokens": ["Sein", "Mark", "durch\u00b7w\u00fch\u00b7len", "Schmer\u00b7zen", "nim\u00b7mer\u00b7glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So duldet er Jahrhunderte die Qual.", "tokens": ["So", "dul\u00b7det", "er", "Jahr\u00b7hun\u00b7der\u00b7te", "die", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da, wie ich ihm in\u2019s Angesicht, das bleiche,", "tokens": ["Da", ",", "wie", "ich", "ihm", "in's", "An\u00b7ge\u00b7sicht", ",", "das", "blei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die leidenvollen, edlen Z\u00fcge seh,", "tokens": ["Die", "lei\u00b7den\u00b7vol\u00b7len", ",", "ed\u00b7len", "Z\u00fc\u00b7ge", "seh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erkenn\u2019 ich ihn, den Dulder von Gethsemane.", "tokens": ["Er\u00b7kenn'", "ich", "ihn", ",", "den", "Dul\u00b7der", "von", "Geth\u00b7se\u00b7ma\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Von seinem Munde macht ein Wort sich frei:", "tokens": ["Von", "sei\u00b7nem", "Mun\u00b7de", "macht", "ein", "Wort", "sich", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was klingt ihr, Glocken, in den Ostertagen?", "tokens": ["Was", "klingt", "ihr", ",", "Glo\u00b7cken", ",", "in", "den", "Os\u00b7ter\u00b7ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun l\u00fcgt ihr frech, da\u00df ich erstanden sei", "tokens": ["Nun", "l\u00fcgt", "ihr", "frech", ",", "da\u00df", "ich", "er\u00b7stan\u00b7den", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und habt auf\u2019s Neue mich an\u2019s Kreuz geschlagen,", "tokens": ["Und", "habt", "auf's", "Neu\u00b7e", "mich", "an's", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In meiner Kirche, an dem Weihaltar", "tokens": ["In", "mei\u00b7ner", "Kir\u00b7che", ",", "an", "dem", "Wei\u00b7hal\u00b7tar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da predigst du den Wahn, o Pharis\u00e4erschaar.", "tokens": ["Da", "pre\u00b7digst", "du", "den", "Wahn", ",", "o", "Pha\u00b7ri\u00b7s\u00e4er\u00b7schaar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Die heil\u2019ge Gluth, die ich vom Himmel trug,", "tokens": ["Die", "heil'\u00b7ge", "Gluth", ",", "die", "ich", "vom", "Him\u00b7mel", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Lehre von dem g\u00f6ttlich-gro\u00dfen Lieben,", "tokens": ["Die", "Leh\u00b7re", "von", "dem", "g\u00f6tt\u00b7lich\u00b7gro\u00b7\u00dfen", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewandelt war sie schnell in einen Fluch", "tokens": ["Ge\u00b7wan\u00b7delt", "war", "sie", "schnell", "in", "ei\u00b7nen", "Fluch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Wortverf\u00e4lschern und von Wahrheitsdieben.", "tokens": ["Von", "Wort\u00b7ver\u00b7f\u00e4l\u00b7schern", "und", "von", "Wahr\u00b7heits\u00b7die\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zur L\u00fcge ward verkehrt mein reines Wort,", "tokens": ["Zur", "L\u00fc\u00b7ge", "ward", "ver\u00b7kehrt", "mein", "rei\u00b7nes", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die heil\u2019ge Gluth mi\u00dfbraucht zu Brand und Mord.", "tokens": ["Die", "heil'\u00b7ge", "Gluth", "mi\u00df\u00b7braucht", "zu", "Brand", "und", "Mord", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Adler nun an meinem Herzen zehrt,", "tokens": ["Ein", "Ad\u00b7ler", "nun", "an", "mei\u00b7nem", "Her\u00b7zen", "zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Adler ist der Menschheit Wahn und Hassen;", "tokens": ["Der", "Ad\u00b7ler", "ist", "der", "Menschheit", "Wahn", "und", "Has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kein Mensch, kein Gott, der dem Gewalt\u2019gen wehrt,", "tokens": ["Kein", "Mensch", ",", "kein", "Gott", ",", "der", "dem", "Ge\u00b7walt'\u00b7gen", "wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Herz mir t\u00e4glich zu erfassen.", "tokens": ["Das", "Herz", "mir", "t\u00e4g\u00b7lich", "zu", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und so durchw\u00fchlt von Schmerz und Gram und Noth", "tokens": ["Und", "so", "durch\u00b7w\u00fchlt", "von", "Schmerz", "und", "Gram", "und", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erleid\u2019 ich t\u00e4glich jammervollen Kreuzestod.", "tokens": ["Er\u00b7leid'", "ich", "t\u00e4g\u00b7lich", "jam\u00b7mer\u00b7vol\u00b7len", "Kreu\u00b7ze\u00b7stod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So duld\u2019 ich bis die gold\u2019ne Stunde kommt,", "tokens": ["So", "duld'", "ich", "bis", "die", "gold'\u00b7ne", "Stun\u00b7de", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In der der Mensch erkennt das Wort, das hohe.", "tokens": ["In", "der", "der", "Mensch", "er\u00b7kennt", "das", "Wort", ",", "das", "ho\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur Liebe, Liebe ist es, die uns frommt!", "tokens": ["Nur", "Lie\u00b7be", ",", "Lie\u00b7be", "ist", "es", ",", "die", "uns", "frommt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis aller Orten gl\u00fcht die heil\u2019ge Lohe,", "tokens": ["Bis", "al\u00b7ler", "Or\u00b7ten", "gl\u00fcht", "die", "heil'\u00b7ge", "Lo\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann flieht der Adler, meine Kette bricht,", "tokens": ["Dann", "flieht", "der", "Ad\u00b7ler", ",", "mei\u00b7ne", "Ket\u00b7te", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich werde frei \u2014 es tagt auf Erden und wird Licht.", "tokens": ["Ich", "wer\u00b7de", "frei", "es", "tagt", "auf", "Er\u00b7den", "und", "wird", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$(", "PPER", "VVFIN", "APPR", "NN", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}