{"dta.poem.3026": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die G\u00f6tter lieb' ich nicht, die uns die Sagen gaben,", "tokens": ["Die", "G\u00f6t\u00b7ter", "lieb'", "ich", "nicht", ",", "die", "uns", "die", "Sa\u00b7gen", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die bald zuviel ein Aug' und bald zuwenig haben.", "tokens": ["Die", "bald", "zu\u00b7viel", "ein", "Aug'", "und", "bald", "zu\u00b7we\u00b7nig", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "ART", "NN", "KON", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Gottheit lieb' ich, die mich unsichtbar umflie\u00dft,", "tokens": ["Die", "Got\u00b7theit", "lieb'", "ich", ",", "die", "mich", "un\u00b7sicht\u00b7bar", "um\u00b7flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ew'ger Liebesblick der Sch\u00f6pfung Bl\u00fct' erschlie\u00dft.", "tokens": ["Ein", "ew'\u00b7ger", "Lie\u00b7bes\u00b7blick", "der", "Sch\u00f6p\u00b7fung", "Bl\u00fct'", "er\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Gottheit lieb' ich, die allgegenw\u00e4rtig waltet,", "tokens": ["Die", "Got\u00b7theit", "lieb'", "ich", ",", "die", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gestaltenlos, der Welt Gestalten umgestaltet.", "tokens": ["Ge\u00b7stal\u00b7ten\u00b7los", ",", "der", "Welt", "Ge\u00b7stal\u00b7ten", "um\u00b7ge\u00b7stal\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und nimt sie selbst Gestalt, und es soll mir nicht graun,", "tokens": ["Und", "nimt", "sie", "selbst", "Ge\u00b7stalt", ",", "und", "es", "soll", "mir", "nicht", "graun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "$,", "KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So mu\u00df sie menschlich aus zwei Augen an mich schaun.", "tokens": ["So", "mu\u00df", "sie", "menschlich", "aus", "zwei", "Au\u00b7gen", "an", "mich", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "CARD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}