{"textgrid.poem.65948": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sa\u00df im Schnellzug erster Klasse", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df im Schnellzug erster Klasse", "tokens": ["Ich", "sa\u00df", "im", "Schnell\u00b7zug", "ers\u00b7ter", "Klas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor einigen Tagen ganz allein,", "tokens": ["Vor", "ei\u00b7ni\u00b7gen", "Ta\u00b7gen", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein still beschaulicher Insasse.", "tokens": ["Ein", "still", "be\u00b7schau\u00b7li\u00b7cher", "In\u00b7sas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da stieg bei einer Feldstation", "tokens": ["Da", "stieg", "bei", "ei\u00b7ner", "Feld\u00b7sta\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Ein Herr, zum mindesten ein Baron,", "tokens": ["Ein", "Herr", ",", "zum", "min\u00b7des\u00b7ten", "ein", "Ba\u00b7ron", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit stummem Gru\u00dfe zu mir ein.", "tokens": ["Mit", "stum\u00b7mem", "Gru\u00b7\u00dfe", "zu", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ohne Pfiff und Klinglingling,", "tokens": ["Und", "oh\u00b7ne", "Pfiff", "und", "Kling\u00b7ling\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ganz lautlos ohne Aufsehn ging", "tokens": ["Ganz", "laut\u00b7los", "oh\u00b7ne", "Auf\u00b7sehn", "ging"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Drauf wieder weiter unsre Reise.", "tokens": ["Drauf", "wie\u00b7der", "wei\u00b7ter", "uns\u00b7re", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mich wunderte die seltne Weise,", "tokens": ["Mich", "wun\u00b7der\u00b7te", "die", "selt\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dass so auf freiem Feld im Flug", "tokens": ["Dass", "so", "auf", "frei\u00b7em", "Feld", "im", "Flug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der Eilzug stoppte, nicht genug", "tokens": ["Der", "Eil\u00b7zug", "stopp\u00b7te", ",", "nicht", "ge\u00b7nug"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und steigerte noch meine Meinung", "tokens": ["Und", "stei\u00b7ger\u00b7te", "noch", "mei\u00b7ne", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Von dieser vornehmen Erscheinung,", "tokens": ["Von", "die\u00b7ser", "vor\u00b7neh\u00b7men", "Er\u00b7schei\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Ein Mann von Rang wohl und Gewalt", "tokens": ["Ein", "Mann", "von", "Rang", "wohl", "und", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wie machte sonst der Zug hier Halt.", "tokens": ["Wie", "mach\u00b7te", "sonst", "der", "Zug", "hier", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es war ein schlank gewachsner Mann", "tokens": ["Es", "war", "ein", "schlank", "ge\u00b7wachs\u00b7ner", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit grauem Kaisermantel an,", "tokens": ["Mit", "grau\u00b7em", "Kai\u00b7ser\u00b7man\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kleinem rundem, weichem Hut,", "tokens": ["Und", "klei\u00b7nem", "run\u00b7dem", ",", "wei\u00b7chem", "Hut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wangen blass, wie ohne Blut,", "tokens": ["Die", "Wan\u00b7gen", "blass", ",", "wie", "oh\u00b7ne", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Augen schwarz und ernst und tief,", "tokens": ["Die", "Au\u00b7gen", "schwarz", "und", "ernst", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dar\u00fcber wie ein Buschwall lief", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "wie", "ein", "Busc\u00b7hwall", "lief"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "KOKOM", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Brauen eng vereintes Paar,", "tokens": ["Der", "Brau\u00b7en", "eng", "ver\u00b7ein\u00b7tes", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was, reden alte Weiber wahr,", "tokens": ["Was", ",", "re\u00b7den", "al\u00b7te", "Wei\u00b7ber", "wahr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ja immer auf viel Ungl\u00fcck weist.", "tokens": ["Ja", "im\u00b7mer", "auf", "viel", "Un\u00b7gl\u00fcck", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein vis-\u00e0-vis schien viel gereist.", "tokens": ["Mein", "vis\u00b7\u00e0\u00b7vis", "schien", "viel", "ge\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich schloss das gleich aus seinem Wesen,", "tokens": ["Ich", "schloss", "das", "gleich", "aus", "sei\u00b7nem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das war so ohne Federlesen,", "tokens": ["Das", "war", "so", "oh\u00b7ne", "Fe\u00b7der\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Als w\u00e4r' er im Coupee zu Haus,", "tokens": ["Als", "w\u00e4r'", "er", "im", "Cou\u00b7pee", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sah nicht einmal zum Fenster hinaus,", "tokens": ["Sah", "nicht", "ein\u00b7mal", "zum", "Fens\u00b7ter", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Und rauchte schweigend vor sich hin", "tokens": ["Und", "rauch\u00b7te", "schwei\u00b7gend", "vor", "sich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein feines Kraut, das mir den Sinn", "tokens": ["Ein", "fei\u00b7nes", "Kraut", ",", "das", "mir", "den", "Sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Begehrlich machte. Ob er mir", "tokens": ["Be\u00b7gehr\u00b7lich", "mach\u00b7te", ".", "Ob", "er", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$.", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Las vom Gesicht ab die Begier?", "tokens": ["Las", "vom", "Ge\u00b7sicht", "ab", "die", "Be\u00b7gier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Gleich bot er mir mit H\u00f6flichkeit", "tokens": ["Gleich", "bot", "er", "mir", "mit", "H\u00f6f\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Auch eine solche Cigarette", "tokens": ["Auch", "ei\u00b7ne", "sol\u00b7che", "Ci\u00b7ga\u00b7ret\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und fragte, ob ich Feuer h\u00e4tte,", "tokens": ["Und", "frag\u00b7te", ",", "ob", "ich", "Feu\u00b7er", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und war zu dienen mir bereit.", "tokens": ["Und", "war", "zu", "die\u00b7nen", "mir", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKZU", "VVINF", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ich zog den Hut und stellt mich vor,", "tokens": ["Ich", "zog", "den", "Hut", "und", "stellt", "mich", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Drauf er jedoch kein Wort verlor", "tokens": ["Drauf", "er", "je\u00b7doch", "kein", "Wort", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "PIAT", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Und vornehm nur wie dankend nickte,", "tokens": ["Und", "vor\u00b7nehm", "nur", "wie", "dan\u00b7kend", "nick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KOKOM", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Was in der Meinung mich bestrickte,", "tokens": ["Was", "in", "der", "Mei\u00b7nung", "mich", "be\u00b7strick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Er sei zum wenigsten Baron,", "tokens": ["Er", "sei", "zum", "we\u00b7nigs\u00b7ten", "Ba\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Vielleicht wohl gar ein F\u00fcrstensohn.", "tokens": ["Viel\u00b7leicht", "wohl", "gar", "ein", "F\u00fcrs\u00b7ten\u00b7sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf jeden Fall war sein Tabak", "tokens": ["Auf", "je\u00b7den", "Fall", "war", "sein", "Ta\u00b7bak"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr einen F\u00fcrsten nicht zu schlecht.", "tokens": ["F\u00fcr", "ei\u00b7nen", "F\u00fcrs\u00b7ten", "nicht", "zu", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fein von Aroma und Geschmack.", "tokens": ["Fein", "von", "A\u00b7ro\u00b7ma", "und", "Ge\u00b7schmack", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Behaglich setzt' ich mich zurecht", "tokens": ["Be\u00b7hag\u00b7lich", "setzt'", "ich", "mich", "zu\u00b7recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schwieg beim Rauch der Cigarette", "tokens": ["Und", "schwieg", "beim", "Rauch", "der", "Ci\u00b7ga\u00b7ret\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem Spender um die Wette.", "tokens": ["Mit", "ih\u00b7rem", "Spen\u00b7der", "um", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Doch schlie\u00dflich fasst' ich Mut und sprach", "tokens": ["Doch", "schlie\u00df\u00b7lich", "fasst'", "ich", "Mut", "und", "sprach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von dem und jenem, wie mir's lag,", "tokens": ["Von", "dem", "und", "je\u00b7nem", ",", "wie", "mir's", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "KON", "PDAT", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und er wohl h\u00f6flich Antwort gab,", "tokens": ["Und", "er", "wohl", "h\u00f6f\u00b7lich", "Ant\u00b7wort", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Brach aber stets bald wieder ab.", "tokens": ["Brach", "a\u00b7ber", "stets", "bald", "wie\u00b7der", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sein wortkarg Wesen reizte mich.", "tokens": ["Sein", "wort\u00b7karg", "We\u00b7sen", "reiz\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nun schweigst auch du, gelobte ich,", "tokens": ["Nun", "schweigst", "auch", "du", ",", "ge\u00b7lob\u00b7te", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "VVFIN", "PPER", "$