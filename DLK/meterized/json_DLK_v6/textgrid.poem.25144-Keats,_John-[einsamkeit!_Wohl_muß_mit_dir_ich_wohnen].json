{"textgrid.poem.25144": {"metadata": {"author": {"name": "Keats, John", "birth": "N.A.", "death": "N.A."}, "title": "[einsamkeit! Wohl mu\u00df mit dir ich wohnen]", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsamkeit! Wohl mu\u00df mit dir ich wohnen;", "tokens": ["Ein\u00b7sam\u00b7keit", "!", "Wohl", "mu\u00df", "mit", "dir", "ich", "woh\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VMFIN", "APPR", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch sei es nicht in diesem finsterkalten", "tokens": ["Doch", "sei", "es", "nicht", "in", "die\u00b7sem", "fins\u00b7ter\u00b7kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gewirr von H\u00e4usern; hoch auf Felsgestalten \u2013", "tokens": ["Ge\u00b7wirr", "von", "H\u00e4u\u00b7sern", ";", "hoch", "auf", "Fels\u00b7ge\u00b7stal\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sternwarten der Natur \u2013 da la\u00df uns thronen!", "tokens": ["Stern\u00b7war\u00b7ten", "der", "Na\u00b7tur", "\u2013", "da", "la\u00df", "uns", "thro\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "Wo tief das Tal mit Flu\u00df und W\u00e4lderkronen", "tokens": ["Wo", "tief", "das", "Tal", "mit", "Flu\u00df", "und", "W\u00e4l\u00b7der\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur fu\u00dflang scheint. La\u00df uns Vigilien halten,", "tokens": ["Nur", "fu\u00df\u00b7lang", "scheint", ".", "La\u00df", "uns", "Vi\u00b7gi\u00b7li\u00b7en", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$.", "VVIMP", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dort wo das Reh aus gr\u00fcnen Hinterhalten", "tokens": ["Dort", "wo", "das", "Reh", "aus", "gr\u00fc\u00b7nen", "Hin\u00b7ter\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Biene schreckt von Ginst und Anemonen.", "tokens": ["Die", "Bie\u00b7ne", "schreckt", "von", "Ginst", "und", "A\u00b7ne\u00b7mo\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl m\u00f6cht ich gern mit dir dies alles schauen!", "tokens": ["Wohl", "m\u00f6cht", "ich", "gern", "mit", "dir", "dies", "al\u00b7les", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PDS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch s\u00fc\u00dfre Freude meine Seele kennt,", "tokens": ["Doch", "s\u00fc\u00df\u00b7re", "Freu\u00b7de", "mei\u00b7ne", "See\u00b7le", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und das ist H\u00f6chstes, was mir Sehnsucht nennt:", "tokens": ["Und", "das", "ist", "H\u00f6chs\u00b7tes", ",", "was", "mir", "Sehn\u00b7sucht", "nennt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NE", "$,", "PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit Wahlverwandtem fliehn zu deinen Gauen,", "tokens": ["Mit", "Wahl\u00b7ver\u00b7wand\u00b7tem", "fliehn", "zu", "dei\u00b7nen", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihm, in dem die reine Flamme brennt", "tokens": ["Mit", "ihm", ",", "in", "dem", "die", "rei\u00b7ne", "Flam\u00b7me", "brennt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PRELS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Worte wei\u00df, ihr Wesen zu vertrauen.", "tokens": ["Und", "Wor\u00b7te", "wei\u00df", ",", "ihr", "We\u00b7sen", "zu", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}