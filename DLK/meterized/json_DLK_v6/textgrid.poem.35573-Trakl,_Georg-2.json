{"textgrid.poem.35573": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frieden der Seele. Einsamer Winterabend,", "tokens": ["Frie\u00b7den", "der", "See\u00b7le", ".", "Ein\u00b7sa\u00b7mer", "Win\u00b7ter\u00b7a\u00b7bend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ART", "NN", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die dunklen Gestalten der Hirten am alten Weiher;", "tokens": ["Die", "dunk\u00b7len", "Ge\u00b7stal\u00b7ten", "der", "Hir\u00b7ten", "am", "al\u00b7ten", "Wei\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Kindlein in der H\u00fctte von Stroh; o wie leise", "tokens": ["Kin\u00b7dlein", "in", "der", "H\u00fct\u00b7te", "von", "Stroh", ";", "o", "wie", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR", "NE", "$.", "FM", "KON", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sank in schwarzem Fieber das Antlitz hin", "tokens": ["Sank", "in", "schwar\u00b7zem", "Fie\u00b7ber", "das", "Ant\u00b7litz", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Heilige Nacht.", "tokens": ["Hei\u00b7li\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Oder wenn er an der harten Hand des Vaters", "tokens": ["O\u00b7der", "wenn", "er", "an", "der", "har\u00b7ten", "Hand", "des", "Va\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Stille den finstern Kalvarienberg hinanstieg", "tokens": ["Stil\u00b7le", "den", "fins\u00b7tern", "Kal\u00b7va\u00b7ri\u00b7en\u00b7berg", "hi\u00b7nan\u00b7stieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und in d\u00e4mmernden Felsennischen", "tokens": ["Und", "in", "d\u00e4m\u00b7mern\u00b7den", "Fel\u00b7sen\u00b7ni\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die blaue Gestalt des Menschen durch seine Legende ging,", "tokens": ["Die", "blau\u00b7e", "Ge\u00b7stalt", "des", "Men\u00b7schen", "durch", "sei\u00b7ne", "Le\u00b7gen\u00b7de", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aus der Wunde unter dem Herzen purpurn das Blut rann.", "tokens": ["Aus", "der", "Wun\u00b7de", "un\u00b7ter", "dem", "Her\u00b7zen", "pur\u00b7purn", "das", "Blut", "rann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "NE", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "O wie leise stand in dunkler Seele das Kreuz auf.", "tokens": ["O", "wie", "lei\u00b7se", "stand", "in", "dunk\u00b7ler", "See\u00b7le", "das", "Kreuz", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.3": {"line.1": {"text": "Liebe; da in schwarzen Winkeln der Schnee schmolz,", "tokens": ["Lie\u00b7be", ";", "da", "in", "schwar\u00b7zen", "Win\u00b7keln", "der", "Schnee", "schmolz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ein blaues L\u00fcftchen sich heiter im alten Hollunder fing,", "tokens": ["Ein", "blau\u00b7es", "L\u00fcft\u00b7chen", "sich", "hei\u00b7ter", "im", "al\u00b7ten", "Hol\u00b7lun\u00b7der", "fing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "In dem Schattengew\u00f6lbe des Nu\u00dfbaums;", "tokens": ["In", "dem", "Schat\u00b7ten\u00b7ge\u00b7w\u00f6l\u00b7be", "des", "Nu\u00df\u00b7baums", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und dem Knaben leise sein rosiger Engel erschien.", "tokens": ["Und", "dem", "Kna\u00b7ben", "lei\u00b7se", "sein", "ro\u00b7si\u00b7ger", "En\u00b7gel", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Freude; da in k\u00fchlen Zimmern eine Abendsonate erklang,", "tokens": ["Freu\u00b7de", ";", "da", "in", "k\u00fch\u00b7len", "Zim\u00b7mern", "ei\u00b7ne", "A\u00b7bend\u00b7so\u00b7na\u00b7te", "er\u00b7klang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+--+--+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Im braunen Holzgeb\u00e4lk", "tokens": ["Im", "brau\u00b7nen", "Holz\u00b7ge\u00b7b\u00e4lk"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein blauer Falter aus der silbernen Puppe kroch.", "tokens": ["Ein", "blau\u00b7er", "Fal\u00b7ter", "aus", "der", "sil\u00b7ber\u00b7nen", "Pup\u00b7pe", "kroch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "O die N\u00e4he des Todes. In steinerner Mauer", "tokens": ["O", "die", "N\u00e4\u00b7he", "des", "To\u00b7des", ".", "In", "stei\u00b7ner\u00b7ner", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Neigte sich ein gelbes Haupt, schweigend das Kind,", "tokens": ["Neig\u00b7te", "sich", "ein", "gel\u00b7bes", "Haupt", ",", "schwei\u00b7gend", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Da in jenem M\u00e4rz der Mond verfiel.", "tokens": ["Da", "in", "je\u00b7nem", "M\u00e4rz", "der", "Mond", "ver\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}