{"textgrid.poem.41618": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch Lied wird, einsam Herz, heut abend dir entt\u00f6nen?", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Lied wird, einsam Herz, heut abend dir entt\u00f6nen?", "tokens": ["Welch", "Lied", "wird", ",", "ein\u00b7sam", "Herz", ",", "heut", "a\u00b7bend", "dir", "ent\u00b7t\u00f6\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "ADJD", "NN", "$,", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was wirst du sagen, mein verdorrt und arm Gem\u00fct,", "tokens": ["Was", "wirst", "du", "sa\u00b7gen", ",", "mein", "ver\u00b7dorrt", "und", "arm", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$,", "PPOSAT", "VVPP", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu ihr, der Guten, Teuren, Strahlend-Sch\u00f6nen,", "tokens": ["Zu", "ihr", ",", "der", "Gu\u00b7ten", ",", "Teu\u00b7ren", ",", "Strah\u00b7len\u00b7dSch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor deren heitrem Blick die Seele neu erbl\u00fcht?", "tokens": ["Vor", "de\u00b7ren", "heit\u00b7rem", "Blick", "die", "See\u00b7le", "neu", "er\u00b7bl\u00fcht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "All unser Stolz soll sein, ihr hohes Lob zu singen,", "tokens": ["All", "un\u00b7ser", "Stolz", "soll", "sein", ",", "ihr", "ho\u00b7hes", "Lob", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "VAINF", "$,", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts gleicht an G\u00fcte ihr und anmutvoller Macht,", "tokens": ["Nichts", "gleicht", "an", "G\u00fc\u00b7te", "ihr", "und", "an\u00b7mut\u00b7vol\u00b7ler", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihr durchgeistet Fleisch haucht Duft wie Engelsschwingen,", "tokens": ["Und", "ihr", "durch\u00b7geis\u00b7tet", "Fleisch", "haucht", "Duft", "wie", "En\u00b7gels\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "VVFIN", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Auge webt um uns ein Kleid von Licht und Pracht.", "tokens": ["Ihr", "Au\u00b7ge", "webt", "um", "uns", "ein", "Kleid", "von", "Licht", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei's in der Einsamkeit, wo n\u00e4chtig Dunkel lastet,", "tokens": ["Sei's", "in", "der", "Ein\u00b7sam\u00b7keit", ",", "wo", "n\u00e4ch\u00b7tig", "Dun\u00b7kel", "las\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PWAV", "ADJD", "NN", "VVFIN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Sei's in der Stra\u00dfe, wo die Menge ruhlos hastet,", "tokens": ["Sei's", "in", "der", "Stra\u00b7\u00dfe", ",", "wo", "die", "Men\u00b7ge", "ruh\u00b7los", "has\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihr Bild tanzt in der Luft, wie gl\u00fcher Fackel Schein.", "tokens": ["Ihr", "Bild", "tanzt", "in", "der", "Luft", ",", "wie", "gl\u00fc\u00b7her", "Fa\u00b7ckel", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Oft spricht es: Ich bin sch\u00f6n, euch soll der Liebe Sonne", "tokens": ["Oft", "spricht", "es", ":", "Ich", "bin", "sch\u00f6n", ",", "euch", "soll", "der", "Lie\u00b7be", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$,", "PPER", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchgl\u00fchn, da\u00df ihr um mich die Sch\u00f6nheit liebt allein;", "tokens": ["Durch\u00b7gl\u00fchn", ",", "da\u00df", "ihr", "um", "mich", "die", "Sch\u00f6n\u00b7heit", "liebt", "al\u00b7lein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "PRF", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schutzengel bin ich euch und Muse und Madonne!", "tokens": ["Schut\u00b7zen\u00b7gel", "bin", "ich", "euch", "und", "Mu\u00b7se", "und", "Ma\u00b7don\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "KON", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}