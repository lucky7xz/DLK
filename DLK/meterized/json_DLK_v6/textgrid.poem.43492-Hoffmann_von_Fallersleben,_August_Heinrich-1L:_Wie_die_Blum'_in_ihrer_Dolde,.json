{"textgrid.poem.43492": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Blum' in ihrer Dolde,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Blum' in ihrer Dolde,", "tokens": ["Wie", "die", "Blum'", "in", "ih\u00b7rer", "Dol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Edelstein im Golde,", "tokens": ["Wie", "der", "E\u00b7del\u00b7stein", "im", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhst du, Kindlein, wohlgemuth", "tokens": ["Ruhst", "du", ",", "Kin\u00b7dlein", ",", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PWAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Liebe Pfleg' und Hut.", "tokens": ["In", "der", "Lie\u00b7be", "Pfleg'", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Weinen magst du oder lachen,", "tokens": ["Wei\u00b7nen", "magst", "du", "o\u00b7der", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafen magst du oder wachen:", "tokens": ["Schla\u00b7fen", "magst", "du", "o\u00b7der", "wa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um dich h\u00e4lt die Liebe Wacht", "tokens": ["Um", "dich", "h\u00e4lt", "die", "Lie\u00b7be", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tag f\u00fcr Tag und Nacht f\u00fcr Nacht.", "tokens": ["Tag", "f\u00fcr", "Tag", "und", "Nacht", "f\u00fcr", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wir seh'n an deinem Frieden,", "tokens": ["Und", "wir", "seh'n", "an", "dei\u00b7nem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was der Himmel dir beschieden:", "tokens": ["Was", "der", "Him\u00b7mel", "dir", "be\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vater, Mutter, liebes Kind,", "tokens": ["Va\u00b7ter", ",", "Mut\u00b7ter", ",", "lie\u00b7bes", "Kind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die so gut und gl\u00fccklich sind.", "tokens": ["Die", "so", "gut", "und", "gl\u00fcck\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du l\u00e4chelst, wenn sie fragen,", "tokens": ["Und", "du", "l\u00e4\u00b7chelst", ",", "wenn", "sie", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es ist, als wollt'st du sagen:", "tokens": ["Und", "es", "ist", ",", "als", "wollt'st", "du", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, ich ruhe wohlgemuth", "tokens": ["Ja", ",", "ich", "ru\u00b7he", "wohl\u00b7ge\u00b7muth"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Liebe Pfleg' und Hut.", "tokens": ["In", "der", "Lie\u00b7be", "Pfleg'", "und", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}