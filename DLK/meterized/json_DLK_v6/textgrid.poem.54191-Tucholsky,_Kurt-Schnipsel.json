{"textgrid.poem.54191": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zitate:", "tokens": ["Zi\u00b7ta\u00b7te", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Genie\u00dft der J\u00fcngling ein Vergn\u00fcgen,", "tokens": ["Ge\u00b7nie\u00dft", "der", "J\u00fcng\u00b7ling", "ein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "so sei er dankbar und verschwiegen \u2013", "tokens": ["so", "sei", "er", "dank\u00b7bar", "und", "ver\u00b7schwie\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "ist nicht von Wilhelm Busch.", "tokens": ["ist", "nicht", "von", "Wil\u00b7helm", "Busch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wandelt niemand ungestraft unter Palmen", "tokens": ["Es", "wan\u00b7delt", "nie\u00b7mand", "un\u00b7ge\u00b7straft", "un\u00b7ter", "Pal\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "steht nicht in Lessings ", "tokens": ["steht", "nicht", "in", "Les\u00b7sings"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die Staatsgewalt geht vom Volke aus . . .", "tokens": ["Die", "Staats\u00b7ge\u00b7walt", "geht", "vom", "Vol\u00b7ke", "aus", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "das steht allerdings in der Reichsverfassung.", "tokens": ["das", "steht", "al\u00b7ler\u00b7dings", "in", "der", "Reichs\u00b7ver\u00b7fas\u00b7sung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}