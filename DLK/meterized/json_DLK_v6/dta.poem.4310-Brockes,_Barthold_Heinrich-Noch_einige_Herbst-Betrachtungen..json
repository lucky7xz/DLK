{"dta.poem.4310": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch  \n einige Herbst-Betrachtungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm Herbst, bey tr\u00fcb- und stiller Luft, sa\u00df ich an einem Tisch", "tokens": ["Jm", "Herbst", ",", "bey", "tr\u00fcb", "und", "stil\u00b7ler", "Luft", ",", "sa\u00df", "ich", "an", "ei\u00b7nem", "Tisch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPR", "TRUNC", "KON", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "im Garten,", "tokens": ["im", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Betrachtete die sanfte Sch\u00f6nheit der itzt aufs neu gef\u00e4rbten", "tokens": ["Be\u00b7trach\u00b7te\u00b7te", "die", "sanf\u00b7te", "Sch\u00f6n\u00b7heit", "der", "itzt", "aufs", "neu", "ge\u00b7f\u00e4rb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADV", "APPRART", "ADJD", "VVPP"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Welt,", "tokens": ["Welt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Beschaute den belaubten Steig, besah das schwarz\u2019 und", "tokens": ["Be\u00b7schau\u00b7te", "den", "be\u00b7laub\u00b7ten", "Steig", ",", "be\u00b7sah", "das", "schwa\u00b7rz'", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PDS", "VVFIN", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gr\u00fcne Feld,", "tokens": ["gr\u00fc\u00b7ne", "Feld", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Bewunderte die holde Mischung der Farben von so vielen", "tokens": ["Be\u00b7wun\u00b7der\u00b7te", "die", "hol\u00b7de", "Misc\u00b7hung", "der", "Far\u00b7ben", "von", "so", "vie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ADV", "PIAT"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Arten.", "tokens": ["Ar\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Vor andern fiel mir ins Gesicht vom Dorn-Busch eine", "tokens": ["Vor", "an\u00b7dern", "fiel", "mir", "ins", "Ge\u00b7sicht", "vom", "Dorn\u00b7Busch", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gr\u00fcne Wand,", "tokens": ["gr\u00fc\u00b7ne", "Wand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die der gerade Steig zertheilte, zur linken und zur rechten", "tokens": ["Die", "der", "ge\u00b7ra\u00b7de", "Steig", "zer\u00b7theil\u00b7te", ",", "zur", "lin\u00b7ken", "und", "zur", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,", "APPRART", "ADJA", "KON", "APPRART", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Hand,", "tokens": ["Hand", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Da\u00df jeder eine Laube schien. Recht hinter beyden W\u00e4nden", "tokens": ["Da\u00df", "je\u00b7der", "ei\u00b7ne", "Lau\u00b7be", "schien", ".", "Recht", "hin\u00b7ter", "bey\u00b7den", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$.", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "stand", "tokens": ["stand"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Ein Paar ganz rohter hoher B\u00e4ume, die sich, vor ihrer", "tokens": ["Ein", "Paar", "ganz", "roh\u00b7ter", "ho\u00b7her", "B\u00e4u\u00b7me", ",", "die", "sich", ",", "vor", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA", "ADJA", "NN", "$,", "PRELS", "PRF", "$,", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Bl\u00e4tter Sterben,", "tokens": ["Bl\u00e4t\u00b7ter", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "In einen Feuer-gleichen Glanz bem\u00fchet hatten sie zu f\u00e4rben.", "tokens": ["In", "ei\u00b7nen", "Feu\u00b7er\u00b7glei\u00b7chen", "Glanz", "be\u00b7m\u00fc\u00b7het", "hat\u00b7ten", "sie", "zu", "f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Ein gleiches Paar, nur da\u00df sie klein, stand vor der Hecke,", "tokens": ["Ein", "glei\u00b7ches", "Paar", ",", "nur", "da\u00df", "sie", "klein", ",", "stand", "vor", "der", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "KOUS", "PPER", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so da\u00df man", "tokens": ["so", "da\u00df", "man"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sie auf derselben dunklen Grund noch heller schimmern", "tokens": ["Sie", "auf", "der\u00b7sel\u00b7ben", "dunk\u00b7len", "Grund", "noch", "hel\u00b7ler", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PDAT", "ADJA", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sehen kann.", "tokens": ["se\u00b7hen", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Es nahmen alle rohte Bl\u00e4tter, es nahm sich jeder Zweig und", "tokens": ["Es", "nah\u00b7men", "al\u00b7le", "roh\u00b7te", "Bl\u00e4t\u00b7ter", ",", "es", "nahm", "sich", "je\u00b7der", "Zweig", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PRF", "PIAT", "NN", "KON"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Strau\u00df,", "tokens": ["Strau\u00df", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Bey dieser dunklen Nachbarschaft der Hecke, gar vortrefflich", "tokens": ["Bey", "die\u00b7ser", "dunk\u00b7len", "Nach\u00b7bar\u00b7schaft", "der", "He\u00b7cke", ",", "gar", "vor\u00b7treff\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "aus,", "tokens": ["aus", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "So da\u00df man jedes Blatt fast sah. Noch \u00fcber dieser Wipfeln", "tokens": ["So", "da\u00df", "man", "je\u00b7des", "Blatt", "fast", "sah", ".", "Noch", "\u00fc\u00b7ber", "die\u00b7ser", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PIAT", "NN", "ADV", "VVFIN", "$.", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "schien,", "tokens": ["schien", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "In einem langen gr\u00fcnen Strich, der h\u00f6hern Hecke sch\u00f6nes", "tokens": ["In", "ei\u00b7nem", "lan\u00b7gen", "gr\u00fc\u00b7nen", "Strich", ",", "der", "h\u00f6\u00b7hern", "He\u00b7cke", "sch\u00f6\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Gr\u00fcn,", "tokens": ["Gr\u00fcn", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und unterschiede, dadurch eben, die rohte Sch\u00f6nheit dieser", "tokens": ["Und", "un\u00b7ter\u00b7schie\u00b7de", ",", "da\u00b7durch", "e\u00b7ben", ",", "die", "roh\u00b7te", "Sch\u00f6n\u00b7heit", "die\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PAV", "ADV", "$,", "ART", "ADJA", "NN", "PDAT"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Bl\u00e4tter,", "tokens": ["Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Von der nicht minder sch\u00f6nen R\u00f6hte der hintern B\u00e4ume,", "tokens": ["Von", "der", "nicht", "min\u00b7der", "sch\u00f6\u00b7nen", "R\u00f6h\u00b7te", "der", "hin\u00b7tern", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "die, erhaben,", "tokens": ["die", ",", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ART", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Der andern schon erwehnten Sch\u00f6nheit ein noch viel sch\u00f6ner", "tokens": ["Der", "an\u00b7dern", "schon", "er\u00b7wehn\u00b7ten", "Sch\u00f6n\u00b7heit", "ein", "noch", "viel", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADJA", "NN", "ART", "ADV", "PIAT", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ansehn gaben.", "tokens": ["An\u00b7sehn", "ga\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Weil nun noch hinter diesen rohten noch andre B\u00e4ume", "tokens": ["Weil", "nun", "noch", "hin\u00b7ter", "die\u00b7sen", "roh\u00b7ten", "noch", "and\u00b7re", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PDAT", "ADJA", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "sich erh\u00f6h'n,", "tokens": ["sich", "er\u00b7h\u00f6h'n", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die sich noch nicht entf\u00e4rbet haben, und noch im gr\u00fcnen", "tokens": ["Die", "sich", "noch", "nicht", "ent\u00b7f\u00e4r\u00b7bet", "ha\u00b7ben", ",", "und", "noch", "im", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "PTKNEG", "VVPP", "VAINF", "$,", "KON", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Schmucke steh'n;", "tokens": ["Schmu\u00b7cke", "steh'n", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So machte dieser holde Wechsel und Unterscheid von Roht", "tokens": ["So", "mach\u00b7te", "die\u00b7ser", "hol\u00b7de", "Wech\u00b7sel", "und", "Un\u00b7ter\u00b7scheid", "von", "Roht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "ADJA", "NN", "KON", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "und Gr\u00fcn,", "tokens": ["und", "Gr\u00fcn", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Da stets ein Absatz nach dem andern von Licht und Dunkel-", "tokens": ["Da", "stets", "ein", "Ab\u00b7satz", "nach", "dem", "an\u00b7dern", "von", "Licht", "und", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "heit erschien,", "tokens": ["heit", "er\u00b7schien", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Da\u00df ich mich nicht erinnern konnt\u2019 ein holder Farben-Spiel", "tokens": ["Da\u00df", "ich", "mich", "nicht", "e\u00b7rin\u00b7nern", "konnt'", "ein", "hol\u00b7der", "Fa\u00b7rben\u00b7Spiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "gesehn,", "tokens": ["ge\u00b7sehn", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Mich mehr daran vergn\u00fcgt zu haben. Schien die\u00df nun", "tokens": ["Mich", "mehr", "da\u00b7ran", "ver\u00b7gn\u00fcgt", "zu", "ha\u00b7ben", ".", "Schien", "die\u00df", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "PAV", "VVPP", "PTKZU", "VAINF", "$.", "VVFIN", "PDS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "gleich von ungefehr", "tokens": ["gleich", "von", "un\u00b7ge\u00b7fehr"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "In solchem bunten Schmuck zu stehn; so gab es mir doch ein", "tokens": ["In", "sol\u00b7chem", "bun\u00b7ten", "Schmuck", "zu", "stehn", ";", "so", "gab", "es", "mir", "doch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Vergn\u00fcgen,", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und dieses bracht\u2019 mich allgemach, nach meiner Pflicht, auf", "tokens": ["Und", "die\u00b7ses", "bracht'", "mich", "all\u00b7ge\u00b7mach", ",", "nach", "mei\u00b7ner", "Pflicht", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.16": {"text": "das: Woher", "tokens": ["das", ":", "Wo\u00b7her"], "token_info": ["word", "punct", "word"], "pos": ["PDS", "$.", "PWAV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Entstehet aller Dinge Sch\u00f6nheit? Wer ist von Far-", "tokens": ["Ent\u00b7ste\u00b7het", "al\u00b7ler", "Din\u00b7ge", "Sch\u00f6n\u00b7heit", "?", "Wer", "ist", "von", "Fa\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$.", "PWS", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "ben, Form' und Z\u00fcgen,", "tokens": ["ben", ",", "Form'", "und", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Vom Licht und vom Gesicht der Ursprung? Von wem", "tokens": ["Vom", "Licht", "und", "vom", "Ge\u00b7sicht", "der", "Ur\u00b7sprung", "?", "Von", "wem"], "token_info": ["