{"textgrid.poem.58219": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "257. Jesus zu der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, bleib doch eingekehrt und immer auf der Wacht,", "tokens": ["Ach", ",", "bleib", "doch", "ein\u00b7ge\u00b7kehrt", "und", "im\u00b7mer", "auf", "der", "Wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "VVPP", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du dummes Kind erkennst nicht deinen eignen Schaden;", "tokens": ["Du", "dum\u00b7mes", "Kind", "er\u00b7kennst", "nicht", "dei\u00b7nen", "eig\u00b7nen", "Scha\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Ausspazieren, ach, verwahrlost manche Gnaden,", "tokens": ["Dein", "Aus\u00b7spa\u00b7zie\u00b7ren", ",", "ach", ",", "ver\u00b7wahr\u00b7lost", "man\u00b7che", "Gna\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ITJ", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die dir mein Mutterherz so reichlich zugedacht!", "tokens": ["Die", "dir", "mein", "Mut\u00b7ter\u00b7herz", "so", "reich\u00b7lich", "zu\u00b7ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandr