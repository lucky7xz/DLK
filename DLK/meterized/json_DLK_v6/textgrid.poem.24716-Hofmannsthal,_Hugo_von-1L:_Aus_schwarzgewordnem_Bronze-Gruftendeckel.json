{"textgrid.poem.24716": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus schwarzgewordnem Bronze-Gruftendeckel", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus schwarzgewordnem Bronze-Gruftendeckel", "tokens": ["Aus", "schwarz\u00b7ge\u00b7word\u00b7nem", "Bron\u00b7ze\u00b7G\u00b7ruf\u00b7ten\u00b7de\u00b7ckel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind die ber\u00fchmten schweren alten Verse,", "tokens": ["Sind", "die", "be\u00b7r\u00fchm\u00b7ten", "schwe\u00b7ren", "al\u00b7ten", "Ver\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Kalt anzuf\u00fchlen, unzerst\u00f6rbar, tragend", "tokens": ["Kalt", "an\u00b7zu\u00b7f\u00fch\u00b7len", ",", "un\u00b7zer\u00b7st\u00f6r\u00b7bar", ",", "tra\u00b7gend"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VVIZU", "$,", "ADJD", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Toten-Prunk, schwarzgr\u00fcne Wappenschilde", "tokens": ["Den", "To\u00b7ten\u00b7Prunk", ",", "schwarz\u00b7gr\u00fc\u00b7ne", "Wap\u00b7pen\u00b7schil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und eine Inschrift, ehern auf dem Erz,", "tokens": ["Und", "ei\u00b7ne", "In\u00b7schrift", ",", "e\u00b7hern", "auf", "dem", "Erz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die denken macht, doch keinen Schauer gibt.", "tokens": ["Die", "den\u00b7ken", "macht", ",", "doch", "kei\u00b7nen", "Schau\u00b7er", "gibt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "$,", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du liest und endlich kommst du an ein Wort,", "tokens": ["Du", "liest", "und", "end\u00b7lich", "kommst", "du", "an", "ein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das ist, wie deine Seele oft geahnt", "tokens": ["Das", "ist", ",", "wie", "dei\u00b7ne", "See\u00b7le", "oft", "ge\u00b7ahnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und nie gewu\u00dft zu nennen, was sie meinte.", "tokens": ["Und", "nie", "ge\u00b7wu\u00dft", "zu", "nen\u00b7nen", ",", "was", "sie", "mein\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von da hebt Zauber an. An jedem Sarg", "tokens": ["Von", "da", "hebt", "Zau\u00b7ber", "an", ".", "An", "je\u00b7dem", "Sarg"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "NN", "PTKVZ", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Schl\u00e4gt da von innen mit lebendgen Kn\u00f6cheln", "tokens": ["Schl\u00e4gt", "da", "von", "in\u00b7nen", "mit", "le\u00b7bend\u00b7gen", "Kn\u00f6\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das Leben, Schultern stemmen sich von unten,", "tokens": ["Das", "Le\u00b7ben", ",", "Schul\u00b7tern", "stem\u00b7men", "sich", "von", "un\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Deckel dr\u00f6hnt, wo zwischen Erz und Erz", "tokens": ["Der", "De\u00b7ckel", "dr\u00f6hnt", ",", "wo", "zwi\u00b7schen", "Erz", "und", "Erz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die schmalste Spalte, schieben Menschenfinger", "tokens": ["Die", "schmals\u00b7te", "Spal\u00b7te", ",", "schie\u00b7ben", "Men\u00b7schen\u00b7fin\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Sich durch und aus den Spalten str\u00f6mt ein Licht,", "tokens": ["Sich", "durch", "und", "aus", "den", "Spal\u00b7ten", "str\u00f6mt", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ein Licht, ein wundervolles warmes Licht,", "tokens": ["Ein", "Licht", ",", "ein", "wun\u00b7der\u00b7vol\u00b7les", "war\u00b7mes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Das lang geruht im k\u00fchlen dunklen Grund", "tokens": ["Das", "lang", "ge\u00b7ruht", "im", "k\u00fch\u00b7len", "dunk\u00b7len", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVPP", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und Schweigen in sich sog und tiefen Duft", "tokens": ["Und", "Schwei\u00b7gen", "in", "sich", "sog", "und", "tie\u00b7fen", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PRF", "VVFIN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Von n\u00e4chtigen Fr\u00fcchten \u2013 dieses Licht str\u00f6mt auf,", "tokens": ["Von", "n\u00e4ch\u00b7ti\u00b7gen", "Fr\u00fcch\u00b7ten", "\u2013", "die\u00b7ses", "Licht", "str\u00f6mt", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "PDAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Und auf die Deckel ihrer Gr\u00fcfte steigen,", "tokens": ["Und", "auf", "die", "De\u00b7ckel", "ih\u00b7rer", "Gr\u00fcf\u00b7te", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Den nackten Fu\u00df in goldenen Sandalen,", "tokens": ["Den", "nack\u00b7ten", "Fu\u00df", "in", "gol\u00b7de\u00b7nen", "San\u00b7da\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die tausende Lebendigen und schauen", "tokens": ["Die", "tau\u00b7sen\u00b7de", "Le\u00b7ben\u00b7di\u00b7gen", "und", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Auf dich und auf das Spiel gespenstiger Reihen", "tokens": ["Auf", "dich", "und", "auf", "das", "Spiel", "ge\u00b7spens\u00b7ti\u00b7ger", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Und reden mehr als du begreifen kannst.", "tokens": ["Und", "re\u00b7den", "mehr", "als", "du", "be\u00b7grei\u00b7fen", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}