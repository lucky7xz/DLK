{"textgrid.poem.24006": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Introduktion", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Buntes Gew\u00fchl, es wirheln und flirren", "tokens": ["Bun\u00b7tes", "Ge\u00b7w\u00fchl", ",", "es", "wir\u00b7heln", "und", "flir\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVINF", "KON", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Tausend Farben in tollem Gemenge,", "tokens": ["Tau\u00b7send", "Far\u00b7ben", "in", "tol\u00b7lem", "Ge\u00b7men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Taumelnde, jauchzende T\u00f6ne schwirren,", "tokens": ["Tau\u00b7meln\u00b7de", ",", "jauch\u00b7zen\u00b7de", "T\u00f6\u00b7ne", "schwir\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Suchende gl\u00fchende Blicke irren", "tokens": ["Su\u00b7chen\u00b7de", "gl\u00fc\u00b7hen\u00b7de", "Bli\u00b7cke", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVINF"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Durch das Gedr\u00e4nge.", "tokens": ["Durch", "das", "Ge\u00b7dr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "In all' dem Trubel \u2013 ich suche nur eine:", "tokens": ["In", "all'", "dem", "Tru\u00b7bel", "\u2013", "ich", "su\u00b7che", "nur", "ei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$(", "PPER", "VVFIN", "ADV", "ART", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Reizender Racker, was l\u00e4ufst du so schnelle?", "tokens": ["Rei\u00b7zen\u00b7der", "Ra\u00b7cker", ",", "was", "l\u00e4ufst", "du", "so", "schnel\u00b7le", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Kaum, da\u00df ich wieder zu haben sie meine,", "tokens": ["Kaum", ",", "da\u00df", "ich", "wie\u00b7der", "zu", "ha\u00b7ben", "sie", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PTKZU", "VAINF", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ist sie verschwunden, die zierliche, kleine", "tokens": ["Ist", "sie", "ver\u00b7schwun\u00b7den", ",", "die", "zier\u00b7li\u00b7che", ",", "klei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "ART", "ADJA", "$,", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Pollichinelle.", "tokens": ["Pol\u00b7li\u00b7chi\u00b7nel\u00b7le", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}