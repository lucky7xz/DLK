{"textgrid.poem.47447": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nichts ist dumpfer Gem\u00fcther]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist dumpfer Gem\u00fcther", "tokens": ["Nichts", "ist", "dum\u00b7pfer", "Ge\u00b7m\u00fc\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Tr\u00e4umenden Heerden wunderbar;", "tokens": ["Tr\u00e4u\u00b7men\u00b7den", "Heer\u00b7den", "wun\u00b7der\u00b7bar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch mir d\u00fcnket am Ende", "tokens": ["Doch", "mir", "d\u00fcn\u00b7ket", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Alles auf Erden wunderbar.", "tokens": ["Al\u00b7les", "auf", "Er\u00b7den", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Ist nicht Leben und Athem,", "tokens": ["Ist", "nicht", "Le\u00b7ben", "und", "A\u00b7them", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Himmel und Licht und Rosenflur,", "tokens": ["Him\u00b7mel", "und", "Licht", "und", "Ro\u00b7sen\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ist nicht schwellender Jugend", "tokens": ["Ist", "nicht", "schwel\u00b7len\u00b7der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Liebliches Werden wunderbar?", "tokens": ["Lieb\u00b7li\u00b7ches", "Wer\u00b7den", "wun\u00b7der\u00b7bar", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6nheit, welche Mirakel", "tokens": ["Sch\u00f6n\u00b7heit", ",", "wel\u00b7che", "Mi\u00b7ra\u00b7kel"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAT", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Stellst du betroffnem Auge dar!", "tokens": ["Stellst", "du", "be\u00b7troff\u00b7nem", "Au\u00b7ge", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind nicht, sage, Suleima's", "tokens": ["Sind", "nicht", ",", "sa\u00b7ge", ",", "Su\u00b7lei\u00b7ma's"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PTKNEG", "$,", "VVFIN", "$,", "NE"], "meter": "-----+-", "measure": "unknown.measure.single"}, "line.4": {"text": "Holde Geberden wunderbar?", "tokens": ["Hol\u00b7de", "Ge\u00b7ber\u00b7den", "wun\u00b7der\u00b7bar", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Welch ein Wunder die Liebe!", "tokens": ["Welch", "ein", "Wun\u00b7der", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ach, um ein einzig L\u00e4cheln nur", "tokens": ["Ach", ",", "um", "ein", "ein\u00b7zig", "L\u00e4\u00b7cheln", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUI", "ART", "ADJD", "NN", "ADV"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Tr\u00e4gt die Seele Hafisens", "tokens": ["Tr\u00e4gt", "die", "See\u00b7le", "Ha\u00b7fi\u00b7sens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Welche Beschwerden wunderbar!", "tokens": ["Wel\u00b7che", "Be\u00b7schwer\u00b7den", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}