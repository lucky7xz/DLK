{"textgrid.poem.47606": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[von Liebe spricht der Mystiker]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Liebe spricht der Mystiker,", "tokens": ["Von", "Lie\u00b7be", "spricht", "der", "Mys\u00b7ti\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Von Liebe fl\u00f6tet auch Hafis;", "tokens": ["Von", "Lie\u00b7be", "fl\u00f6\u00b7tet", "auch", "Ha\u00b7fis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "NE", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du aber h\u00f6re diesen nur!", "tokens": ["Du", "a\u00b7ber", "h\u00f6\u00b7re", "die\u00b7sen", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PDAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sein Gesang, sein lieblicher,", "tokens": ["Denn", "sein", "Ge\u00b7sang", ",", "sein", "lieb\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Haucht Leben und Natur;", "tokens": ["Haucht", "Le\u00b7ben", "und", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Mystiker, so prangend auch", "tokens": ["Der", "Mys\u00b7ti\u00b7ker", ",", "so", "pran\u00b7gend", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein ausstudirter Redebrauch", "tokens": ["Sein", "aus\u00b7stu\u00b7dir\u00b7ter", "Re\u00b7de\u00b7brauch"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drischt leere Halme nur.", "tokens": ["Drischt", "lee\u00b7re", "Hal\u00b7me", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}