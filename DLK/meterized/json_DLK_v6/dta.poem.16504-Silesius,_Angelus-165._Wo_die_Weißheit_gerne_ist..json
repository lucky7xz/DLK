{"dta.poem.16504": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "165.  Wo die Wei\u00dfheit gerne ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wei\u00dfheit findt sich gern wo jhre Kinder sind/", "tokens": ["Die", "Wei\u00df\u00b7heit", "findt", "sich", "gern", "wo", "jhre", "Kin\u00b7der", "sind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "PWAV", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Warum\u0304? (O wunder ding!) sie selber ist ein Kind.", "tokens": ["War\u00b7um\u0304", "?", "(", "O", "wun\u00b7der", "ding", "!", ")", "sie", "sel\u00b7ber", "ist", "ein", "Kind", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "$(", "NE", "ADJA", "NN", "$.", "$(", "PPER", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}