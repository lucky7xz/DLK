{"textgrid.poem.48625": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "38. Auf Eines seiner Liebsten Namensged\u00e4chtn\u00fc\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt euch, Liebste, nur zufrieden!", "tokens": ["Gebt", "euch", ",", "Liebs\u00b7te", ",", "nur", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob ich leiblich von euch bin,", "tokens": ["Ob", "ich", "leib\u00b7lich", "von", "euch", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dennoch sind wir ungeschieden.", "tokens": ["den\u00b7noch", "sind", "wir", "un\u00b7ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch bewohnet stets mein Sin,", "tokens": ["Euch", "be\u00b7woh\u00b7net", "stets", "mein", "Sin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und ich trag' in gleichem Schmerzen", "tokens": ["und", "ich", "trag'", "in", "glei\u00b7chem", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "euer Herz' in meinem Herzen.", "tokens": ["eu\u00b7er", "Her\u00b7z'", "in", "mei\u00b7nem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Gott, der helf' uns frisch zusammen!", "tokens": ["Gott", ",", "der", "hel\u00b7f'", "uns", "frisch", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mittels dessen tut wie ich!", "tokens": ["Mit\u00b7tels", "des\u00b7sen", "tut", "wie", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VVFIN", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doppelt eurer Liebe Flammen", "tokens": ["Dop\u00b7pelt", "eu\u00b7rer", "Lie\u00b7be", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "allzeit mehr und mehr auf mich,", "tokens": ["all\u00b7zeit", "mehr", "und", "mehr", "auf", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df sich eure reiche Funken", "tokens": ["da\u00df", "sich", "eu\u00b7re", "rei\u00b7che", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in mein Meer der Tr\u00e4nen dunken!", "tokens": ["in", "mein", "Meer", "der", "Tr\u00e4\u00b7nen", "dun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denkt und braucht der s\u00fc\u00dfen Stunden,", "tokens": ["Denkt", "und", "braucht", "der", "s\u00fc\u00b7\u00dfen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die sich heute geben euch!", "tokens": ["die", "sich", "heu\u00b7te", "ge\u00b7ben", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei hiermit von dem gebunden,", "tokens": ["Sei", "hier\u00b7mit", "von", "dem", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "APPR", "ART", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der nur ist an W\u00fcndschen reich,", "tokens": ["der", "nur", "ist", "an", "W\u00fcnd\u00b7schen", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bis da\u00df einsten seine Gaben", "tokens": ["bis", "da\u00df", "eins\u00b7ten", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PIS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tat und Nachdruck werden haben!", "tokens": ["Tat", "und", "Nach\u00b7druck", "wer\u00b7den", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}