{"textgrid.poem.35192": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit Disteln kehr ich \u00fcm. Es ist/ was bl\u00fcht hierinnen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Disteln kehr ich \u00fcm. Es ist/ was bl\u00fcht hierinnen", "tokens": ["Mit", "Dis\u00b7teln", "kehr", "ich", "\u00fcm", ".", "Es", "ist", "/", "was", "bl\u00fcht", "hie\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "$(", "PWS", "VVFIN", "PAV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht f\u00fcr dein Distelmaul/ mein Kl\u00fcgling. Von dem Sand/", "tokens": ["Nicht", "f\u00fcr", "dein", "Dis\u00b7tel\u00b7maul", "/", "mein", "Kl\u00fcg\u00b7ling", ".", "Von", "dem", "Sand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$.", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Distel-kraut Salat geht lieblich-wohl zurhand", "tokens": ["Ein", "Dis\u00b7tel\u00b7kraut", "Sa\u00b7lat", "geht", "lieb\u00b7lich\u00b7wohl", "zur\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Esel Risel-ein. Nun/ Disteln hier zerrinnen/", "tokens": ["Dem", "E\u00b7sel", "Ri\u00b7sel\u00b7ein", ".", "Nun", "/", "Dis\u00b7teln", "hier", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ADV", "$(", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier findet Blumen-f\u00fcll f\u00fcr Disteln dein Verstand/", "tokens": ["Hier", "fin\u00b7det", "Blu\u00b7men\u00b7f\u00fcll", "f\u00fcr", "Dis\u00b7teln", "dein", "Ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Hochge Ohrter Herr: Es ist kein Distel-rand/", "tokens": ["Mein", "Hoch\u00b7ge", "Ohr\u00b7ter", "Herr", ":", "Es", "ist", "kein", "Dis\u00b7tel\u00b7rand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den unsre Pegnitz w\u00e4scht. Ob Disteln Lust gewinnen/", "tokens": ["Den", "uns\u00b7re", "Peg\u00b7nitz", "w\u00e4scht", ".", "Ob", "Dis\u00b7teln", "Lust", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "KOUS", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Heist Eselhafter Trieb. Dein Distel-hungerband", "tokens": ["Heist", "E\u00b7sel\u00b7haf\u00b7ter", "Trieb", ".", "Dein", "Dis\u00b7tel\u00b7hun\u00b7ger\u00b7band"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Macht dich zum M\u00fcllertier. Es hasst der Disteln Land", "tokens": ["Macht", "dich", "zum", "M\u00fcl\u00b7ler\u00b7tier", ".", "Es", "hasst", "der", "Dis\u00b7teln", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein Blumen-buntes Feld. Auf/ Distelmaul! von hinnen.", "tokens": ["Ein", "Blu\u00b7men\u00b7bun\u00b7tes", "Feld", ".", "Auf", "/", "Dis\u00b7tel\u00b7maul", "!", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "$(", "NN", "$.", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "La\u00df Blumen unbeschnarcht/ die nicht f\u00fcr Distel-Sinne", "tokens": ["La\u00df", "Blu\u00b7men", "un\u00b7be\u00b7schnarcht", "/", "die", "nicht", "f\u00fcr", "Dis\u00b7tel\u00b7Sin\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "ADJD", "$(", "PRELS", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vnd deinen Distelkopf. ", "tokens": ["Vnd", "dei\u00b7nen", "Dis\u00b7tel\u00b7kopf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Nur \u00e4dlen Teutschen ein; du bist ihr Distel-brand:", "tokens": ["Nur", "\u00e4d\u00b7len", "Teut\u00b7schen", "ein", ";", "du", "bist", "ihr", "Dis\u00b7tel\u00b7brand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dr\u00fcm gh\u00f6rt ein Distel-krantz auf deiner Ohren Zinnen", "tokens": ["Dr\u00fcm", "gh\u00f6rt", "ein", "Dis\u00b7tel\u00b7krantz", "auf", "dei\u00b7ner", "Oh\u00b7ren", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "willm\u00fctiger", "tokens": ["will\u00b7m\u00fc\u00b7ti\u00b7ger"], "token_info": ["word"], "pos": ["ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Aechter", "tokens": ["A\u00b7ech\u00b7ter"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}