{"textgrid.poem.26619": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seh' ich euch so vor mir", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seh' ich euch so vor mir", "tokens": ["Seh'", "ich", "euch", "so", "vor", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erbl\u00fchen in J\u00fcnglingskraft,", "tokens": ["Er\u00b7bl\u00fc\u00b7hen", "in", "J\u00fcng\u00b7lings\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Segn' ich euch tief im Herzen,", "tokens": ["Segn'", "ich", "euch", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Still eu're Zukunft bedenkend.", "tokens": ["Still", "eu'\u00b7re", "Zu\u00b7kunft", "be\u00b7den\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Reich hat Natur euch begnadet.", "tokens": ["Reich", "hat", "Na\u00b7tur", "euch", "be\u00b7gna\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sie gab euch des Leibes Wohlgestalt,", "tokens": ["Sie", "gab", "euch", "des", "Lei\u00b7bes", "Wohl\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gab euch bildsam den Geist", "tokens": ["Gab", "euch", "bild\u00b7sam", "den", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und empf\u00e4nglich das Herz.", "tokens": ["Und", "emp\u00b7f\u00e4ng\u00b7lich", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und auch das Leben trat f\u00fcr euch ein.", "tokens": ["Und", "auch", "das", "Le\u00b7ben", "trat", "f\u00fcr", "euch", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Hingestellt hat es euch", "tokens": ["Hin\u00b7ge\u00b7stellt", "hat", "es", "euch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "PPER"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Vorweg auf seine h\u00f6chsten und frei'sten H\u00f6h'n,", "tokens": ["Vor\u00b7weg", "auf", "sei\u00b7ne", "h\u00f6chs\u00b7ten", "und", "frei'\u00b7sten", "H\u00f6h'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wo ihr,", "tokens": ["Wo", "ihr", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "M\u00fchelos", "tokens": ["M\u00fc\u00b7he\u00b7los"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Und unber\u00fchrt von Gemeinem,", "tokens": ["Und", "un\u00b7be\u00b7r\u00fchrt", "von", "Ge\u00b7mei\u00b7nem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Entfalten k\u00f6nnt,", "tokens": ["Ent\u00b7fal\u00b7ten", "k\u00f6nnt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Was ein Gott in euch gelegt.", "tokens": ["Was", "ein", "Gott", "in", "euch", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber gef\u00e4hrlich auch ist die Luft der H\u00f6h'n.", "tokens": ["A\u00b7ber", "ge\u00b7f\u00e4hr\u00b7lich", "auch", "ist", "die", "Luft", "der", "H\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "NE"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Das bedenkt \u2013 und bewahrt euch", "tokens": ["Das", "be\u00b7denkt", "\u2013", "und", "be\u00b7wahrt", "euch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$(", "KON", "VVFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Auf da\u00df niemals", "tokens": ["Auf", "da\u00df", "nie\u00b7mals"], "token_info": ["word", "word", "word"], "pos": ["APPR", "KOUS", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Die Wolke der Selbstsucht", "tokens": ["Die", "Wol\u00b7ke", "der", "Selbst\u00b7sucht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Euch tr\u00fcbe den klaren Blick,", "tokens": ["Euch", "tr\u00fc\u00b7be", "den", "kla\u00b7ren", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Niemals kleinliche R\u00fccksicht", "tokens": ["Nie\u00b7mals", "klein\u00b7li\u00b7che", "R\u00fcck\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Des Gedankens Fittig,", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "Fit\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Die Arme der That euch l\u00e4hme \u2013", "tokens": ["Die", "Ar\u00b7me", "der", "That", "euch", "l\u00e4h\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Und ihr auch haltet,", "tokens": ["Und", "ihr", "auch", "hal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Was ihr versprecht", "tokens": ["Was", "ihr", "ver\u00b7sprecht"], "token_info": ["word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Dem ahnungsvollen Geiste des Dichters.", "tokens": ["Dem", "ah\u00b7nungs\u00b7vol\u00b7len", "Geis\u00b7te", "des", "Dich\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}