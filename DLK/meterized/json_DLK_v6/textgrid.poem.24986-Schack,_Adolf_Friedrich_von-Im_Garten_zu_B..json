{"textgrid.poem.24986": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Im Garten zu B.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich ", "tokens": ["Da\u00df", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Pl\u00e4tze, wiederfinden mu\u00df!", "tokens": ["Pl\u00e4t\u00b7ze", ",", "wie\u00b7der\u00b7fin\u00b7den", "mu\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl noch mit bekannten Lauten", "tokens": ["Wohl", "noch", "mit", "be\u00b7kann\u00b7ten", "Lau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Murmelt der geschw\u00e4tz'ge Flu\u00df,", "tokens": ["Mur\u00b7melt", "der", "ge\u00b7schw\u00e4tz'\u00b7ge", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohl die Knospen bricht der Flieder", "tokens": ["Wohl", "die", "Knos\u00b7pen", "bricht", "der", "Flie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie in jenem sel'gen Jahr, \u2013", "tokens": ["Wie", "in", "je\u00b7nem", "sel'\u00b7gen", "Jahr", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "PDAT", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch nie Fr\u00fchling wird es wieder,", "tokens": ["Doch", "nie", "Fr\u00fch\u00b7ling", "wird", "es", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie es damals Fr\u00fchling war.", "tokens": ["Wie", "es", "da\u00b7mals", "Fr\u00fch\u00b7ling", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie mehr aus dem Gr\u00fcn der Linden", "tokens": ["Nie", "mehr", "aus", "dem", "Gr\u00fcn", "der", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ART", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lacht und duftet so der Mai;", "tokens": ["Lacht", "und", "duf\u00b7tet", "so", "der", "Mai", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie wie damals in den Winden", "tokens": ["Nie", "wie", "da\u00b7mals", "in", "den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hallt des Kuckucks froher Schrei;", "tokens": ["Hallt", "des", "Ku\u00b7ckucks", "fro\u00b7her", "Schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie so an den Bergesh\u00e4ngen", "tokens": ["Nie", "so", "an", "den", "Ber\u00b7ges\u00b7h\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flammt der Fichtensprossen Rot;", "tokens": ["Flammt", "der", "Fich\u00b7tens\u00b7pros\u00b7sen", "Rot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hier in allen Laubeng\u00e4ngen", "tokens": ["Hier", "in", "al\u00b7len", "Lau\u00b7ben\u00b7g\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hingeschritten ist der Tod.", "tokens": ["Hin\u00b7ge\u00b7schrit\u00b7ten", "ist", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Derer, die mir teuer waren,", "tokens": ["De\u00b7rer", ",", "die", "mir", "teu\u00b7er", "wa\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keinen findet mehr mein Blick;", "tokens": ["Kei\u00b7nen", "fin\u00b7det", "mehr", "mein", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit geh\u00e4uftem Gram von Jahren", "tokens": ["Mit", "ge\u00b7h\u00e4uf\u00b7tem", "Gram", "von", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehr' ich noch allein zur\u00fcck,", "tokens": ["Kehr'", "ich", "noch", "al\u00b7lein", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und rings, wie mit Geisterzungen,", "tokens": ["Und", "rings", ",", "wie", "mit", "Geis\u00b7ter\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aus dem Laub, dem Wasserfall,", "tokens": ["Aus", "dem", "Laub", ",", "dem", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "T\u00f6nt von Stimmen, lang verklungen,", "tokens": ["T\u00f6nt", "von", "Stim\u00b7men", ",", "lang", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An mein Ohr der Widerhall.", "tokens": ["An", "mein", "Ohr", "der", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf den Rasen, die verwildern,", "tokens": ["Auf", "den", "Ra\u00b7sen", ",", "die", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht mein Auge thr\u00e4nenschwer", "tokens": ["Sucht", "mein", "Au\u00b7ge", "thr\u00e4\u00b7nen\u00b7schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der G\u00f6tter Marmorbildern,", "tokens": ["Nach", "der", "G\u00f6t\u00b7ter", "Mar\u00b7mor\u00b7bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche einst, olympisch-hehr,", "tokens": ["Wel\u00b7che", "einst", ",", "o\u00b7lym\u00b7pischhehr", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "$,", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Von den Piedestalen schauten;", "tokens": ["Von", "den", "Pie\u00b7de\u00b7sta\u00b7len", "schau\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun von Nesseln \u00fcberdeckt", "tokens": ["Nun", "von", "Nes\u00b7seln", "\u00fc\u00b7berd\u00b7eckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liegen sie und wilden Rauten,", "tokens": ["Lie\u00b7gen", "sie", "und", "wil\u00b7den", "Rau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den Boden hingestreckt.", "tokens": ["Auf", "den", "Bo\u00b7den", "hin\u00b7ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft, halb hoffend und halb zage,", "tokens": ["Oft", ",", "halb", "hof\u00b7fend", "und", "halb", "za\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVPP", "KON", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn des Morgens Rot sich zeigt,", "tokens": ["Wenn", "des", "Mor\u00b7gens", "Rot", "sich", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denk' ich, da\u00df der alten Tage", "tokens": ["Denk'", "ich", ",", "da\u00df", "der", "al\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer neu im Osten steigt;", "tokens": ["Ei\u00b7ner", "neu", "im", "Os\u00b7ten", "steigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoch und h\u00f6her schwingt der reine", "tokens": ["Hoch", "und", "h\u00f6\u00b7her", "schwingt", "der", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Glanz am Himmel sich empor;", "tokens": ["Glanz", "am", "Him\u00b7mel", "sich", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber bald mit blassem Scheine", "tokens": ["A\u00b7ber", "bald", "mit", "blas\u00b7sem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Stirbt er hin in Nebelflor.", "tokens": ["Stirbt", "er", "hin", "in", "Ne\u00b7bel\u00b7flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und erschreckt, wohin ich schreite,", "tokens": ["Und", "er\u00b7schreckt", ",", "wo\u00b7hin", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fahr' ich auf bei jedem Tritt;", "tokens": ["Fahr'", "ich", "auf", "bei", "je\u00b7dem", "Tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schatten schleichen mir zur Seite", "tokens": ["Schat\u00b7ten", "schlei\u00b7chen", "mir", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die Garteng\u00e4nge mit,", "tokens": ["Durch", "die", "Gar\u00b7ten\u00b7g\u00e4n\u00b7ge", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sitzen bei mir auf den B\u00e4nken,", "tokens": ["Sit\u00b7zen", "bei", "mir", "auf", "den", "B\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fl\u00fcstern Worte mir ins Ohr \u2013 \u2013", "tokens": ["Fl\u00fcs\u00b7tern", "Wor\u00b7te", "mir", "ins", "Ohr", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PPER", "APPRART", "NN", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O hinweg! Ich mag's nicht denken,", "tokens": ["O", "hin\u00b7weg", "!", "Ich", "mag's", "nicht", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ich hatt' und nun verlor!", "tokens": ["Was", "ich", "hatt'", "und", "nun", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}