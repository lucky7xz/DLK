{"textgrid.poem.44908": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[grabschrift f\u00fcr Bernhard Freiherrn von Eskeles]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was er gedacht, war Weisheit,", "tokens": ["Was", "er", "ge\u00b7dacht", ",", "war", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was er gewollt, war G\u00fcte,", "tokens": ["Was", "er", "ge\u00b7wollt", ",", "war", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMPP", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was er gef\u00fchlt, war Liebe.", "tokens": ["Was", "er", "ge\u00b7f\u00fchlt", ",", "war", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein F\u00fcrst gab ihm den Adel,", "tokens": ["Sein", "F\u00fcrst", "gab", "ihm", "den", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihm h\u00f6hern gab die Natur.", "tokens": ["Ihm", "h\u00f6\u00b7hern", "gab", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}