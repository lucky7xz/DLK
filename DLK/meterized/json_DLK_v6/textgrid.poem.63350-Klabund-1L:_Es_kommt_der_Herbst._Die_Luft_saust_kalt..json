{"textgrid.poem.63350": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kommt der Herbst. Die Luft saust kalt.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt der Herbst. Die Luft saust kalt.", "tokens": ["Es", "kommt", "der", "Herbst", ".", "Die", "Luft", "saust", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein lieber Gott geht durch den Wald.", "tokens": ["Kein", "lie\u00b7ber", "Gott", "geht", "durch", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein alter Mann von siebenzig", "tokens": ["Ein", "al\u00b7ter", "Mann", "von", "sie\u00b7ben\u00b7zig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht Feuerung f\u00fcr den Winter sich.", "tokens": ["Sucht", "Feu\u00b7e\u00b7rung", "f\u00fcr", "den", "Win\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "PRF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch unser Herz ist ausgeloht", "tokens": ["Auch", "un\u00b7ser", "Herz", "ist", "aus\u00b7ge\u00b7loht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und etwas Feuerung t\u00e4te not.", "tokens": ["Und", "et\u00b7was", "Feu\u00b7e\u00b7rung", "t\u00e4\u00b7te", "not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie runzlig blickt das ganze Land", "tokens": ["Wie", "runz\u00b7lig", "blickt", "das", "gan\u00b7ze", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und riecht nach F\u00e4ulnis penetrant.", "tokens": ["Und", "riecht", "nach", "F\u00e4ul\u00b7nis", "pe\u00b7ne\u00b7trant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Sand verrinnen allgemach", "tokens": ["Im", "Sand", "ver\u00b7rin\u00b7nen", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wittels- und der Fechenbach.", "tokens": ["Der", "Wit\u00b7tels", "und", "der", "Fe\u00b7chen\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Moor, dort, wo man stach den Torf,", "tokens": ["Im", "Moor", ",", "dort", ",", "wo", "man", "stach", "den", "Torf", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "$,", "PWAV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verf\u00e4llt das alte Ludendorff.", "tokens": ["Ver\u00b7f\u00e4llt", "das", "al\u00b7te", "Lu\u00b7den\u00b7dorff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit Halali und mit Geheil", "tokens": ["Mit", "Ha\u00b7la\u00b7li", "und", "mit", "Ge\u00b7heil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Nimmt an der Ebertjagd man teil.", "tokens": ["Nimmt", "an", "der", "E\u00b7bert\u00b7jagd", "man", "teil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PIS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer jetzt nicht liebt Sang, Weib und Wein \u2013", "tokens": ["Wer", "jetzt", "nicht", "liebt", "Sang", ",", "Weib", "und", "Wein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fest steht und treu der Schacht am Rhein.", "tokens": ["Fest", "steht", "und", "treu", "der", "Schacht", "am", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "ART", "NN", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Man leert die Hosentaschen aus.", "tokens": ["Man", "leert", "die", "Ho\u00b7sen\u00b7ta\u00b7schen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Rentenpfennig drin, o Graus.", "tokens": ["Kein", "Ren\u00b7ten\u00b7pfen\u00b7nig", "drin", ",", "o", "Graus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Versuchs und stell' dich auf den Kopf:", "tokens": ["Ver\u00b7suchs", "und", "stell'", "dich", "auf", "den", "Kopf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, kein Gedanke drin, du Tropf!", "tokens": ["Ach", ",", "kein", "Ge\u00b7dan\u00b7ke", "drin", ",", "du", "Tropf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIAT", "NN", "PTKVZ", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Verdreckt, verreckt, verhurt, verlumpt", "tokens": ["Ver\u00b7dreckt", ",", "ver\u00b7reckt", ",", "ver\u00b7hurt", ",", "ver\u00b7lumpt"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 Wer, der uns noch 'nen Taler pumpt?", "tokens": ["\u2013", "Wer", ",", "der", "uns", "noch", "'nen", "Ta\u00b7ler", "pumpt", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$,", "PRELS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es bringt der allgemeine Dalles", "tokens": ["Es", "bringt", "der", "all\u00b7ge\u00b7mei\u00b7ne", "Dal\u00b7les"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch Deutschland, Deutschland unter alles.", "tokens": ["Noch", "Deutschland", ",", "Deutschland", "un\u00b7ter", "al\u00b7les", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "NE", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du kleines K\u00f6hlerm\u00e4dchen, sei", "tokens": ["Du", "klei\u00b7nes", "K\u00f6h\u00b7ler\u00b7m\u00e4d\u00b7chen", ",", "sei"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Moose meine Herbstesfei.", "tokens": ["Im", "Moo\u00b7se", "mei\u00b7ne", "Herbs\u00b7tes\u00b7fei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Regen rinnt. Es weint der Wind,", "tokens": ["Der", "Re\u00b7gen", "rinnt", ".", "Es", "weint", "der", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil wir so schrecklich einsam sind.", "tokens": ["Weil", "wir", "so", "schreck\u00b7lich", "ein\u00b7sam", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es kommt der Herbst. Die Luft saust kalt.", "tokens": ["Es", "kommt", "der", "Herbst", ".", "Die", "Luft", "saust", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Schauer streicht durch Welt und Wald.", "tokens": ["Ein", "Schau\u00b7er", "streicht", "durch", "Welt", "und", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gib mir den Mund. Komm zu mir her.", "tokens": ["Gib", "mir", "den", "Mund", ".", "Komm", "zu", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$.", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umarme mich. Mich friert so sehr.", "tokens": ["Um\u00b7ar\u00b7me", "mich", ".", "Mich", "friert", "so", "sehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}