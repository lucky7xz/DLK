{"textgrid.poem.44486": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Die erstattete Spielschuld", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr neun geliehne Groschen", "tokens": ["F\u00fcr", "neun", "ge\u00b7lieh\u00b7ne", "Gro\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gibst du mir dreimal drei,", "tokens": ["Gibst", "du", "mir", "drei\u00b7mal", "drei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sinds gleich auch nur neune,", "tokens": ["Und", "sinds", "gleich", "auch", "nur", "neu\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Anmut Hand und deine", "tokens": ["Der", "An\u00b7mut", "Hand", "und", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Macht selbst das Alte neu.", "tokens": ["Macht", "selbst", "das", "Al\u00b7te", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und rund umher geschlungen", "tokens": ["Und", "rund", "um\u00b7her", "ge\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKVZ", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein S\u00e4ckel, roten Scheins.", "tokens": ["Ein", "S\u00e4\u00b7ckel", ",", "ro\u00b7ten", "Scheins", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Herz! was k\u00e4me gleicher?", "tokens": ["Ein", "Herz", "!", "was", "k\u00e4\u00b7me", "glei\u00b7cher", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4r sein Inhalt reicher,", "tokens": ["Und", "w\u00e4r", "sein", "In\u00b7halt", "rei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hielt es fast f\u00fcr deins.", "tokens": ["Ich", "hielt", "es", "fast", "f\u00fcr", "deins", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}