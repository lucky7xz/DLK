{"textgrid.poem.60245": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Edle M\u00fctter, die ihr eure Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1756, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Edle M\u00fctter, die ihr eure Seelen", "tokens": ["Ed\u00b7le", "M\u00fct\u00b7ter", ",", "die", "ihr", "eu\u00b7re", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oft zu reisenden S\u00f6hnen gesandt,", "tokens": ["Oft", "zu", "rei\u00b7sen\u00b7den", "S\u00f6h\u00b7nen", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "La\u00dft euch bey der Wiederkunft erz\u00e4hlen,", "tokens": ["La\u00dft", "euch", "bey", "der", "Wie\u00b7der\u00b7kunft", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Welches F\u00fcrstengehorchende Land", "tokens": ["Wel\u00b7ches", "F\u00fcrs\u00b7ten\u00b7ge\u00b7hor\u00b7chen\u00b7de", "Land"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "NN", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ihnen am besten gefallen,", "tokens": ["Ih\u00b7nen", "am", "bes\u00b7ten", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJD", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Welche K\u00f6nigsstadt", "tokens": ["Wel\u00b7che", "K\u00f6\u00b7nigs\u00b7stadt"], "token_info": ["word", "word"], "pos": ["PWAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Unter den K\u00f6nigsst\u00e4dten allen", "tokens": ["Un\u00b7ter", "den", "K\u00f6\u00b7nigs\u00b7st\u00e4d\u00b7ten", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ihre gr\u00f6\u00dfte Bewunderung hat?", "tokens": ["Ih\u00b7re", "gr\u00f6\u00df\u00b7te", "Be\u00b7wun\u00b7de\u00b7rung", "hat", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und sie werden euch sagen,", "tokens": ["Und", "sie", "wer\u00b7den", "euch", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Da\u00df Berlin den Preis", "tokens": ["Da\u00df", "Ber\u00b7lin", "den", "Preis"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Ganz davon getragen.", "tokens": ["Ganz", "da\u00b7von", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "O! der K\u00fcnsteverfeinende Flei\u00df", "tokens": ["O", "!", "der", "K\u00fcns\u00b7te\u00b7ver\u00b7fei\u00b7nen\u00b7de", "Flei\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.13": {"text": "Strebet hier unter dem Schutze des Weisen", "tokens": ["Stre\u00b7bet", "hier", "un\u00b7ter", "dem", "Schut\u00b7ze", "des", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.14": {"text": "Nach dem Gipfel der Vollkommenheit!", "tokens": ["Nach", "dem", "Gip\u00b7fel", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Eure Kinder werden alles preisen.", "tokens": ["Eu\u00b7re", "Kin\u00b7der", "wer\u00b7den", "al\u00b7les", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Meine Liederf\u00e4higkeit", "tokens": ["Mei\u00b7ne", "Lie\u00b7der\u00b7f\u00e4\u00b7hig\u00b7keit"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Ist zu klein, die Wunder zu beschreiben,", "tokens": ["Ist", "zu", "klein", ",", "die", "Wun\u00b7der", "zu", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Die nur eine Kunst enth\u00e4lt. \u2013", "tokens": ["Die", "nur", "ei\u00b7ne", "Kunst", "ent\u00b7h\u00e4lt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Ewig gro\u00df wird Katharina bleiben", "tokens": ["E\u00b7wig", "gro\u00df", "wird", "Ka\u00b7tha\u00b7ri\u00b7na", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "NE", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "Bey der Heldenbewundernden Welt \u2013", "tokens": ["Bey", "der", "Hel\u00b7den\u00b7be\u00b7wun\u00b7dern\u00b7den", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "Ewig sch\u00f6n, und ewig aufbehalten", "tokens": ["E\u00b7wig", "sch\u00f6n", ",", "und", "e\u00b7wig", "auf\u00b7be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Das Geschenke Friedrichs von Berlin.", "tokens": ["Das", "Ge\u00b7schen\u00b7ke", "Fried\u00b7richs", "von", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NE", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Bleiche zitternde Gestalten,", "tokens": ["Blei\u00b7che", "zit\u00b7tern\u00b7de", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Ueberwundner Ottomannen knien", "tokens": ["Ue\u00b7ber\u00b7wund\u00b7ner", "Ot\u00b7to\u00b7man\u00b7nen", "kni\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Vor dem Porcelaingemachten Throne,", "tokens": ["Vor", "dem", "Por\u00b7ce\u00b7lain\u00b7ge\u00b7mach\u00b7ten", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Wei\u00df, wie ein gefallner Schnee;", "tokens": ["Wei\u00df", ",", "wie", "ein", "ge\u00b7fall\u00b7ner", "Schnee", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und in ernster Grazie", "tokens": ["Und", "in", "erns\u00b7ter", "Gra\u00b7zie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.28": {"text": "Unter einer Lorberkrone", "tokens": ["Un\u00b7ter", "ei\u00b7ner", "Lor\u00b7ber\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Blickt die Kaiserinn umher;", "tokens": ["Blickt", "die", "Kai\u00b7se\u00b7rinn", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Auf dem k\u00f6stlichen Tafelgeschirre", "tokens": ["Auf", "dem", "k\u00f6st\u00b7li\u00b7chen", "Ta\u00b7fel\u00b7ge\u00b7schir\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Flammt das brennende Meer,", "tokens": ["Flammt", "das", "bren\u00b7nen\u00b7de", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Fliehn die T\u00fcrken, in der Irre", "tokens": ["Fliehn", "die", "T\u00fcr\u00b7ken", ",", "in", "der", "Ir\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Liegen sie vor den Siegern gestreckt,", "tokens": ["Lie\u00b7gen", "sie", "vor", "den", "Sie\u00b7gern", "ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.34": {"text": "Mit lebendigen Farben gemahlt,", "tokens": ["Mit", "le\u00b7ben\u00b7di\u00b7gen", "Far\u00b7ben", "ge\u00b7mahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-----+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Werden die Grauen des Todes entdeckt,", "tokens": ["Wer\u00b7den", "die", "Grau\u00b7en", "des", "To\u00b7des", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.36": {"text": "Und die Thaten der Reu\u00dfen erz\u00e4hlt.", "tokens": ["Und", "die", "Tha\u00b7ten", "der", "Reu\u00b7\u00dfen", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.37": {"text": "Unnachahmliche Thaten des Krieges,", "tokens": ["Un\u00b7nach\u00b7ahm\u00b7li\u00b7che", "Tha\u00b7ten", "des", "Krie\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.38": {"text": "Dem kein r\u00f6mischer glich,", "tokens": ["Dem", "kein", "r\u00f6\u00b7mi\u00b7scher", "glich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Als vor helfenden G\u00f6ttern des Sieges", "tokens": ["Als", "vor", "hel\u00b7fen\u00b7den", "G\u00f6t\u00b7tern", "des", "Sie\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.40": {"text": "Mithridat von Wald zu Walde wich \u2013", "tokens": ["Mith\u00b7ri\u00b7dat", "von", "Wald", "zu", "Wal\u00b7de", "wich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "Und die f\u00fcrchterlichen Hanniballen", "tokens": ["Und", "die", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Han\u00b7ni\u00b7bal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Und das stolze Karthago zuletzt", "tokens": ["Und", "das", "stol\u00b7ze", "Kar\u00b7tha\u00b7go", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Vor dem r\u00f6mischen Muthe gefallen,", "tokens": ["Vor", "dem", "r\u00f6\u00b7mi\u00b7schen", "Mu\u00b7the", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.44": {"text": "Der Freyheit h\u00f6her, als Leben gesch\u00e4tzt.", "tokens": ["Der", "Frey\u00b7heit", "h\u00f6\u00b7her", ",", "als", "Le\u00b7ben", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}