{"dta.poem.16174": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "228.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist des Geistes Leib? Der K\u00f6rper ist es nicht,", "tokens": ["Was", "ist", "des", "Geis\u00b7tes", "Leib", "?", "Der", "K\u00f6r\u00b7per", "ist", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, aufgebaut aus Staub, in Staub zusammenbricht.", "tokens": ["Der", ",", "auf\u00b7ge\u00b7baut", "aus", "Staub", ",", "in", "Staub", "zu\u00b7sam\u00b7men\u00b7bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ist des Geistes Leib: die Form, die er sich baut,", "tokens": ["Das", "ist", "des", "Geis\u00b7tes", "Leib", ":", "die", "Form", ",", "die", "er", "sich", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der mit Geistesblick ein Geist den andern schaut.", "tokens": ["In", "der", "mit", "Geis\u00b7tes\u00b7blick", "ein", "Geist", "den", "an\u00b7dern", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist der Leib, der jetzt die grobe K\u00f6rperh\u00fclle", "tokens": ["Das", "ist", "der", "Leib", ",", "der", "jetzt", "die", "gro\u00b7be", "K\u00f6r\u00b7per\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchschimmernd, wann sie f\u00e4llt, vortritt in klarer F\u00fclle.", "tokens": ["Durch\u00b7schim\u00b7mernd", ",", "wann", "sie", "f\u00e4llt", ",", "vor\u00b7tritt", "in", "kla\u00b7rer", "F\u00fcl\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In diesem Leib sehn wir uns dort, la\u00dft uns vertrauen:", "tokens": ["In", "die\u00b7sem", "Leib", "sehn", "wir", "uns", "dort", ",", "la\u00dft", "uns", "ver\u00b7trau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Geist hat seinen Leib, um, selbst geschaut, zu schauen.", "tokens": ["Der", "Geist", "hat", "sei\u00b7nen", "Leib", ",", "um", ",", "selbst", "ge\u00b7schaut", ",", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "KOUI", "$,", "ADV", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}