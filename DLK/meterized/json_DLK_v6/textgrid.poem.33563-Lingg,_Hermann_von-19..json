{"textgrid.poem.33563": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O stumm ist die Ferne, da dringt", "tokens": ["O", "stumm", "ist", "die", "Fer\u00b7ne", ",", "da", "dringt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Kein Gru\u00df mehr ans sehnende Herz,", "tokens": ["Kein", "Gru\u00df", "mehr", "ans", "seh\u00b7nen\u00b7de", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und kein Gedanke bezwingt", "tokens": ["Und", "kein", "Ge\u00b7dan\u00b7ke", "be\u00b7zwingt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den t\u00f6dlichen Schmerz.", "tokens": ["Den", "t\u00f6d\u00b7li\u00b7chen", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Kein H\u00e4ndedr\u00fccken, kein Wort", "tokens": ["Kein", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken", ",", "kein", "Wort"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Scheucht vor dem harten Geschick", "tokens": ["Scheucht", "vor", "dem", "har\u00b7ten", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Sorgen, das Bangen mehr fort,", "tokens": ["Die", "Sor\u00b7gen", ",", "das", "Ban\u00b7gen", "mehr", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Kein L\u00e4cheln, kein Blick! \u2013", "tokens": ["Kein", "L\u00e4\u00b7cheln", ",", "kein", "Blick", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Es d\u00e4mmert, es neigt sich der Tag,", "tokens": ["Es", "d\u00e4m\u00b7mert", ",", "es", "neigt", "sich", "der", "Tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der Glanz in den Wolken erblich.", "tokens": ["Der", "Glanz", "in", "den", "Wol\u00b7ken", "er\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wer w\u00e4r' jetzt, o Liebliche, sag,", "tokens": ["Wer", "w\u00e4r'", "jetzt", ",", "o", "Lieb\u00b7li\u00b7che", ",", "sag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wer w\u00e4r' jetzt um dich?", "tokens": ["Wer", "w\u00e4r'", "jetzt", "um", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wer b\u00f6te dir jetzt den Arm", "tokens": ["Wer", "b\u00f6\u00b7te", "dir", "jetzt", "den", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und hie\u00dfe dich tausendmal sein", "tokens": ["Und", "hie\u00b7\u00dfe", "dich", "tau\u00b7send\u00b7mal", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wiegte dann innig und warm", "tokens": ["Und", "wieg\u00b7te", "dann", "in\u00b7nig", "und", "warm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "In Schlummer dich ein?", "tokens": ["In", "Schlum\u00b7mer", "dich", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und wer, seines Gl\u00fcckes bewu\u00dft,", "tokens": ["Und", "wer", ",", "sei\u00b7nes", "Gl\u00fc\u00b7ckes", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wer b\u00f6te, beseligt wie du,", "tokens": ["Wer", "b\u00f6\u00b7te", ",", "be\u00b7se\u00b7ligt", "wie", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "KOKOM", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dir seine hochklopfende Brust", "tokens": ["Dir", "sei\u00b7ne", "hoch\u00b7klop\u00b7fen\u00b7de", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Als Kissen dazu?", "tokens": ["Als", "Kis\u00b7sen", "da\u00b7zu", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Gedenkst du noch sein, mein Kind,", "tokens": ["Ge\u00b7denkst", "du", "noch", "sein", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VAINF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des \u00c4rmsten, der jetzt allein", "tokens": ["Des", "\u00c4rms\u00b7ten", ",", "der", "jetzt", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hinst\u00fcrmt in Wetter und Wind,", "tokens": ["Hin\u00b7st\u00fcrmt", "in", "Wet\u00b7ter", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Gedenkst du noch mein?", "tokens": ["Ge\u00b7denkst", "du", "noch", "mein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}