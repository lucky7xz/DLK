{"dta.poem.16009": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In jeder neuen Lag' ist freilich etwas schlimmer", "tokens": ["In", "je\u00b7der", "neu\u00b7en", "Lag'", "ist", "frei\u00b7lich", "et\u00b7was", "schlim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als in der alten, doch auch etwas besser immer.", "tokens": ["Als", "in", "der", "al\u00b7ten", ",", "doch", "auch", "et\u00b7was", "bes\u00b7ser", "im\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "$,", "ADV", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Soll dir die neue Lag' ertr\u00e4glich seyn, so schlag", "tokens": ["Soll", "dir", "die", "neu\u00b7e", "Lag'", "er\u00b7tr\u00e4g\u00b7lich", "seyn", ",", "so", "schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "VAINF", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Be\u00dfre richtig an, das Schlimmre still ertrag.", "tokens": ["Das", "Be\u00df\u00b7re", "rich\u00b7tig", "an", ",", "das", "Schlimm\u00b7re", "still", "er\u00b7trag", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-