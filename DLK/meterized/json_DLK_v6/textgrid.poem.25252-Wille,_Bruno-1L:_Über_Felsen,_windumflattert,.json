{"textgrid.poem.25252": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber Felsen, windumflattert,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber Felsen, windumflattert,", "tokens": ["\u00dc\u00b7ber", "Fel\u00b7sen", ",", "win\u00b7dum\u00b7flat\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klimm ich hoch hinan zum Freien.", "tokens": ["Klimm", "ich", "hoch", "hi\u00b7nan", "zum", "Frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Droben will ich mich entladen", "tokens": ["Dro\u00b7ben", "will", "ich", "mich", "ent\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Qual, im Sturme baden,", "tokens": ["Die\u00b7ser", "Qual", ",", "im", "Stur\u00b7me", "ba\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Neugeboren meine Seele weihen.", "tokens": ["Neu\u00b7ge\u00b7bo\u00b7ren", "mei\u00b7ne", "See\u00b7le", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Berg, vor deinem Riesenantlitz", "tokens": ["Berg", ",", "vor", "dei\u00b7nem", "Rie\u00b7sen\u00b7ant\u00b7litz"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ja Kleinmut nicht bestehen.", "tokens": ["Kann", "ja", "Klein\u00b7mut", "nicht", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sturm, im Brausen deiner Kraft,", "tokens": ["Sturm", ",", "im", "Brau\u00b7sen", "dei\u00b7ner", "Kraft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die den Forst zusammenrafft,", "tokens": ["Die", "den", "Forst", "zu\u00b7sam\u00b7men\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df mein Seufzer wie ein Staub verwehen.", "tokens": ["Mu\u00df", "mein", "Seuf\u00b7zer", "wie", "ein", "Staub", "ver\u00b7we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}