{"textgrid.poem.55617": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "8", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "D\u00e4mmrung senkte sich von oben,", "tokens": ["D\u00e4mm\u00b7rung", "senk\u00b7te", "sich", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon ist alle N\u00e4he fern;", "tokens": ["Schon", "ist", "al\u00b7le", "N\u00e4\u00b7he", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Doch zuerst emporgehoben", "tokens": ["Doch", "zu\u00b7erst", "em\u00b7por\u00b7ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Holden Lichts der Abendstern!", "tokens": ["Hol\u00b7den", "Lichts", "der", "A\u00b7bends\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Alles schwankt ins Ungewisse,", "tokens": ["Al\u00b7les", "schwankt", "ins", "Un\u00b7ge\u00b7wis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nebel schleichen in die H\u00f6h;", "tokens": ["Ne\u00b7bel", "schlei\u00b7chen", "in", "die", "H\u00f6h", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwarzvertiefte Finsternisse", "tokens": ["Schwarz\u00b7ver\u00b7tief\u00b7te", "Fins\u00b7ter\u00b7nis\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Widerspiegelnd ruht der See.", "tokens": ["Wi\u00b7der\u00b7spie\u00b7gelnd", "ruht", "der", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun im \u00f6stlichen Bereiche", "tokens": ["Nun", "im", "\u00f6st\u00b7li\u00b7chen", "Be\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ahn ich Mondenglanz und -glut,", "tokens": ["Ahn", "ich", "Mon\u00b7den\u00b7glanz", "und", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlanker Weiden Haargezweige", "tokens": ["Schlan\u00b7ker", "Wei\u00b7den", "Haar\u00b7ge\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scherzen auf der n\u00e4chsten Flut.", "tokens": ["Scher\u00b7zen", "auf", "der", "n\u00e4chs\u00b7ten", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch bewegter Schatten Spiele", "tokens": ["Durch", "be\u00b7weg\u00b7ter", "Schat\u00b7ten", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zittert Lunas Zauberschein,", "tokens": ["Zit\u00b7tert", "Lu\u00b7nas", "Zau\u00b7ber\u00b7schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und durchs Auge schleicht die K\u00fchle", "tokens": ["Und", "durchs", "Au\u00b7ge", "schleicht", "die", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00e4nftigend ins Herz hinein.", "tokens": ["S\u00e4nf\u00b7ti\u00b7gend", "ins", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}