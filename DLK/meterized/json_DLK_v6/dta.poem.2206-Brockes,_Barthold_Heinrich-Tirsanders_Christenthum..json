{"dta.poem.2206": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tirsanders Christenthum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem der gr\u00f6\u00dfte Theil der Menschen am Narrenseile", "tokens": ["In\u00b7dem", "der", "gr\u00f6\u00df\u00b7te", "Theil", "der", "Men\u00b7schen", "am", "Nar\u00b7ren\u00b7sei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "wird gelenkt,", "tokens": ["wird", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und alle Thoren insgemein in ihrer Thorheit sich gefallen;", "tokens": ["Und", "al\u00b7le", "Tho\u00b7ren", "ins\u00b7ge\u00b7mein", "in", "ih\u00b7rer", "Thor\u00b7heit", "sich", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Scheint der der gr\u00f6\u00dfte Narr von allen,", "tokens": ["Scheint", "der", "der", "gr\u00f6\u00df\u00b7te", "Narr", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der mit sich selbst zufriedne Narren zu bessern, zu be-", "tokens": ["Der", "mit", "sich", "selbst", "zu\u00b7fried\u00b7ne", "Nar\u00b7ren", "zu", "bes\u00b7sern", ",", "zu", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "PRF", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "kehren denkt.", "tokens": ["keh\u00b7ren", "denkt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}