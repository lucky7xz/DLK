{"textgrid.poem.61594": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "28.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie manchen Blick du frei und freier", "tokens": ["Wie", "man\u00b7chen", "Blick", "du", "frei", "und", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Walten der Natur getan,", "tokens": ["Ins", "Wal\u00b7ten", "der", "Na\u00b7tur", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufs neue hinter jedem Schleier", "tokens": ["Aufs", "neu\u00b7e", "hin\u00b7ter", "je\u00b7dem", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht doch die alte Sphinx dich an.", "tokens": ["Sieht", "doch", "die", "al\u00b7te", "Sphinx", "dich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du kannst ihr nimmer Antwort geben,", "tokens": ["Du", "kannst", "ihr", "nim\u00b7mer", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie die letzte Frag' entbot;", "tokens": ["Wenn", "sie", "die", "letz\u00b7te", "Frag'", "ent\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ewig R\u00e4tsel ist das Leben,", "tokens": ["Ein", "e\u00b7wig", "R\u00e4t\u00b7sel", "ist", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein Geheimnis bleibt der Tod.", "tokens": ["Und", "ein", "Ge\u00b7heim\u00b7nis", "bleibt", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}