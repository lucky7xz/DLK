{"textgrid.poem.36959": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heil uns! Heute Morgen gegen drei Viertel auf Elfen,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil uns! Heute Morgen gegen drei Viertel auf Elfen,", "tokens": ["Heil", "uns", "!", "Heu\u00b7te", "Mor\u00b7gen", "ge\u00b7gen", "drei", "Vier\u00b7tel", "auf", "El\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "NN", "APPR", "CARD", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Einem l\u00e4ngstgef\u00fchlten Bed\u00fcrfni\u00dfe abzuhelfen,", "tokens": ["Ei\u00b7nem", "l\u00e4ngst\u00b7ge\u00b7f\u00fchl\u00b7ten", "Be\u00b7d\u00fcrf\u00b7ni\u00b7\u00dfe", "ab\u00b7zu\u00b7hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ist dem Volke ein Prinz geboren,", "tokens": ["Ist", "dem", "Vol\u00b7ke", "ein", "Prinz", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zu Gl\u00fcck und Segen erkoren!", "tokens": ["Zu", "Gl\u00fcck", "und", "Se\u00b7gen", "er\u00b7ko\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Eine Kanone verk\u00fcndet's durch ganze Land:", "tokens": ["Ei\u00b7ne", "Ka\u00b7no\u00b7ne", "ver\u00b7k\u00fcn\u00b7det's", "durch", "gan\u00b7ze", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Ein Prinz ist geboren von Zacke- Zicke- Zuckerkant", "tokens": ["Ein", "Prinz", "ist", "ge\u00b7bo\u00b7ren", "von", "Za\u00b7cke", "Zi\u00b7cke", "Zu\u00b7cker\u00b7kant"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "TRUNC", "TRUNC", "NN"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Heil uns! Seine Durchlaucht geruhten bereits zu schreien,", "tokens": ["Heil", "uns", "!", "Sei\u00b7ne", "Durch\u00b7laucht", "ge\u00b7ruh\u00b7ten", "be\u00b7reits", "zu", "schrei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und der Natur H\u00f6chstihr erstes Opfer zu weihen,", "tokens": ["Und", "der", "Na\u00b7tur", "H\u00f6chs\u00b7tihr", "ers\u00b7tes", "Op\u00b7fer", "zu", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "H\u00f6chstsie sind bereits zum Major ernannt,", "tokens": ["H\u00f6chst\u00b7sie", "sind", "be\u00b7reits", "zum", "Ma\u00b7jor", "er\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPRART", "NE", "VVPP", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und tragen das breite W\u00fcrdenband,", "tokens": ["Und", "tra\u00b7gen", "das", "brei\u00b7te", "W\u00fcr\u00b7den\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Sie haben H\u00f6chstselbst an der Brust schon gesogen,", "tokens": ["Sie", "ha\u00b7ben", "H\u00f6chst\u00b7selbst", "an", "der", "Brust", "schon", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und bleiben dem Reiche in Gnaden gewogen.", "tokens": ["Und", "blei\u00b7ben", "dem", "Rei\u00b7che", "in", "Gna\u00b7den", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Heil uns! Seine Durchlaucht lassen in diesen Tagen", "tokens": ["Heil", "uns", "!", "Sei\u00b7ne", "Durch\u00b7laucht", "las\u00b7sen", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN", "VVINF", "APPR", "PDAT", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In H\u00f6chstihren Appartements herum Sich tragen,", "tokens": ["In", "H\u00f6chs\u00b7tih\u00b7ren", "Ap\u00b7par\u00b7te\u00b7ments", "he\u00b7rum", "Sich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APZR", "PRF", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Bald wird der H\u00f6chste Lutschbeutel genommen,", "tokens": ["Bald", "wird", "der", "H\u00f6chs\u00b7te", "Lutschbeu\u00b7tel", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und bald werden H\u00f6chstsie ", "tokens": ["Und", "bald", "wer\u00b7den", "H\u00f6chst\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Heil uns!", "tokens": ["Heil", "uns", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Mit Freuden wollen wir neue Abgaben geben,", "tokens": ["Mit", "Freu\u00b7den", "wol\u00b7len", "wir", "neu\u00b7e", "Ab\u00b7ga\u00b7ben", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Erh\u00e4lt der H\u00f6chste H\u00f6chstsie uns am Leben!", "tokens": ["Er\u00b7h\u00e4lt", "der", "H\u00f6chs\u00b7te", "H\u00f6chst\u00b7sie", "uns", "am", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Heil uns! Heil uns! Heil uns!", "tokens": ["Heil", "uns", "!", "Heil", "uns", "!", "Heil", "uns", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "PPER", "$.", "NN", "PPER", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}