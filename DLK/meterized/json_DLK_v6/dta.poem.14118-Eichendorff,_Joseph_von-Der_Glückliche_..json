{"dta.poem.14118": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fcckliche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein Liebchen lieb recht von Herzen.", "tokens": ["Ich", "hab'", "ein", "Lieb\u00b7chen", "lieb", "recht", "von", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hellfrische Augen hat's wie zwei Kerzen,", "tokens": ["Hell\u00b7fri\u00b7sche", "Au\u00b7gen", "hat's", "wie", "zwei", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "KOKOM", "CARD", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und wo sie spielend streifen das Feld,", "tokens": ["Und", "wo", "sie", "spie\u00b7lend", "strei\u00b7fen", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ach wie so lustig gl\u00e4nzet die Welt!", "tokens": ["Ach", "wie", "so", "lus\u00b7tig", "gl\u00e4n\u00b7zet", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}