{"textgrid.poem.67765": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Zum Schutz des weiblichen Geschlechts", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O M\u00e4nner, schm\u00e4ht sie nicht mit Bosheitsfreuden,", "tokens": ["O", "M\u00e4n\u00b7ner", ",", "schm\u00e4ht", "sie", "nicht", "mit", "Bos\u00b7heits\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Weiber arm Geschlecht!", "tokens": ["Der", "Wei\u00b7ber", "arm", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das arme Weib, gemacht zu Leiden,", "tokens": ["Das", "ar\u00b7me", "Weib", ",", "ge\u00b7macht", "zu", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist uns zur Lust, zur Hilfe unterthan,", "tokens": ["Ist", "uns", "zur", "Lust", ",", "zur", "Hil\u00b7fe", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es mu\u00df f\u00fcr ihre kleinen Freuden,", "tokens": ["Es", "mu\u00df", "f\u00fcr", "ih\u00b7re", "klei\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcrs Wohl der Welt", "tokens": ["F\u00fcrs", "Wohl", "der", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Geburt und Schwangerschaft und Todesschmerzen leiden.", "tokens": ["Ge\u00b7burt", "und", "Schwan\u00b7ger\u00b7schaft", "und", "To\u00b7des\u00b7schmer\u00b7zen", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es rei\u00dfet uns zur Welt sich von dem Herzen los,", "tokens": ["Es", "rei\u00b7\u00dfet", "uns", "zur", "Welt", "sich", "von", "dem", "Her\u00b7zen", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Macht uns mit M\u00fch und Thr\u00e4nen gro\u00df,", "tokens": ["Macht", "uns", "mit", "M\u00fch", "und", "Thr\u00e4\u00b7nen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Zieht uns zu Menschen auf von Thieren,", "tokens": ["Zieht", "uns", "zu", "Men\u00b7schen", "auf", "von", "Thie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Mu\u00df M\u00e4nnersorg' in ihrem Schoo\u00dfe f\u00fchren.", "tokens": ["Mu\u00df", "M\u00e4n\u00b7ner\u00b7sor\u00b7g'", "in", "ih\u00b7rem", "Schoo\u00b7\u00dfe", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "O M\u00e4nner, schm\u00e4ht sie nicht mit Bosheitsfreuden!", "tokens": ["O", "M\u00e4n\u00b7ner", ",", "schm\u00e4ht", "sie", "nicht", "mit", "Bos\u00b7heits\u00b7freu\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}