{"dta.poem.3566": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er. Du Dienerl, du nett's,               ", "tokens": ["Er", ".", "Du", "Die\u00b7nerl", ",", "du", "nett's", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$.", "PPER", "NN", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du liegst mir im Herz;", "tokens": ["Du", "liegst", "mir", "im", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Du k\u00f6mmst mir' nicht raus,", "tokens": ["Du", "k\u00f6mmst", "mir'", "nicht", "raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bis die Liebe ist aus.", "tokens": ["Bis", "die", "Lie\u00b7be", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sie. Aus ist sie mit dir,               ", "tokens": ["Sie", ".", "Aus", "ist", "sie", "mit", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "APPR", "VAFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Im ganzen Revier;", "tokens": ["Im", "gan\u00b7zen", "Re\u00b7vier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn die Donau eintrocknet,", "tokens": ["Wenn", "die", "Do\u00b7nau", "ein\u00b7trock\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann heurathen wir.", "tokens": ["Dann", "heu\u00b7ra\u00b7then", "wir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Er. Sie trocknet nit ein,               ", "tokens": ["Er", ".", "Sie", "trock\u00b7net", "nit", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Bleibt alleweil na\u00df;", "tokens": ["Bleibt", "al\u00b7le\u00b7weil", "na\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Jezt mu\u00df ich halt schauen,", "tokens": ["Jezt", "mu\u00df", "ich", "halt", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Um ein anderen Schatz.", "tokens": ["Um", "ein", "an\u00b7de\u00b7ren", "Schatz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}