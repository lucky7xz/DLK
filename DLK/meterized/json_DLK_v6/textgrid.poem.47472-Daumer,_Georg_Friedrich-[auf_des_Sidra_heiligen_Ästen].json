{"textgrid.poem.47472": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[auf des Sidra heiligen \u00c4sten]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf des Sidra heiligen \u00c4sten", "tokens": ["Auf", "des", "Sid\u00b7ra", "hei\u00b7li\u00b7gen", "\u00c4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hoch im himmlichen Revier", "tokens": ["Hoch", "im", "himm\u00b7li\u00b7chen", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Nistete mein Seelenvogel", "tokens": ["Nis\u00b7te\u00b7te", "mein", "See\u00b7len\u00b7vo\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonder irdische Begier.", "tokens": ["Son\u00b7der", "ir\u00b7di\u00b7sche", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einen einzigen Blick, Geliebte,", "tokens": ["Ei\u00b7nen", "ein\u00b7zi\u00b7gen", "Blick", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sandtest du zum Himmel auf,", "tokens": ["Sand\u00b7test", "du", "zum", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und getroffen sank der Vogel", "tokens": ["Und", "ge\u00b7trof\u00b7fen", "sank", "der", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmel-ab zu Fu\u00dfe dir.", "tokens": ["Him\u00b7mel\u00b7ab", "zu", "Fu\u00b7\u00dfe", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Strebt er in die H\u00f6he wieder? \u2013", "tokens": ["Strebt", "er", "in", "die", "H\u00f6\u00b7he", "wie\u00b7der", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach sein einzig Leben ist", "tokens": ["Ach", "sein", "ein\u00b7zig", "Le\u00b7ben", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "ADJD", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So der Erde Staub zu k\u00fcssen,", "tokens": ["So", "der", "Er\u00b7de", "Staub", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So im Staub zu sterben hier.", "tokens": ["So", "im", "Staub", "zu", "ster\u00b7ben", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKZU", "VVINF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}