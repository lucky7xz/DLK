{"dta.poem.14215": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da zog er heimlich fort. \u2014 Seit jenem Morgen", "tokens": ["Da", "zog", "er", "heim\u00b7lich", "fort", ".", "Seit", "je\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$(", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da hatt' ich Noth, hatt' heimlich was zu sorgen.", "tokens": ["Da", "hatt'", "ich", "Noth", ",", "hatt'", "heim\u00b7lich", "was", "zu", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "VAFIN", "ADJD", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn n\u00e4chtlich unten lag die stille Runde,", "tokens": ["Wenn", "n\u00e4cht\u00b7lich", "un\u00b7ten", "lag", "die", "stil\u00b7le", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einf\u00f6rmig Rauschen herkam von den W\u00e4ldern,", "tokens": ["Ein\u00b7f\u00f6r\u00b7mig", "Rau\u00b7schen", "her\u00b7kam", "von", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Pfeifend der Wind strich durch die \u00f6den Felder", "tokens": ["Pfei\u00b7fend", "der", "Wind", "strich", "durch", "die", "\u00f6\u00b7den", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und hin und her in D\u00f6rfern bellten Hunde,", "tokens": ["Und", "hin", "und", "her", "in", "D\u00f6r\u00b7fern", "bell\u00b7ten", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "ADV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach! wenn kein gl\u00fccklich Herz auf Erden wacht", "tokens": ["Ach", "!", "wenn", "kein", "gl\u00fcck\u00b7lich", "Herz", "auf", "Er\u00b7den", "wacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PIAT", "ADJD", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Begr\u00fc\u00dften die verweinten Augen manche Nacht!", "tokens": ["Be\u00b7gr\u00fc\u00df\u00b7ten", "die", "ver\u00b7wein\u00b7ten", "Au\u00b7gen", "man\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}