{"textgrid.poem.28454": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt fault in sich selbst, und ihre Sitten stincken;", "tokens": ["Die", "Welt", "fault", "in", "sich", "selbst", ",", "und", "ih\u00b7re", "Sit\u00b7ten", "stin\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "ADV", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Haus steht auff dem Fall und hebt schon an zu sincken;", "tokens": ["Ihr", "Haus", "steht", "auff", "dem", "Fall", "und", "hebt", "schon", "an", "zu", "sin\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo dich, Herr Christ, man kan im Glantz und Klarheit schauen,", "tokens": ["Wo", "dich", ",", "Herr", "Christ", ",", "man", "kan", "im", "Glantz", "und", "Klar\u00b7heit", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "NN", "NE", "$,", "PIS", "VMFIN", "APPRART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da ist es gut zu seyn und H\u00fctten auff zu bauen.", "tokens": ["Da", "ist", "es", "gut", "zu", "seyn", "und", "H\u00fct\u00b7ten", "auff", "zu", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKZU", "VAINF", "KON", "NN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}