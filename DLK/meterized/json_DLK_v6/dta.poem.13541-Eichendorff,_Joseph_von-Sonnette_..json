{"dta.poem.13541": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So viele Quellen von den Bergen rauschen,", "tokens": ["So", "vie\u00b7le", "Quel\u00b7len", "von", "den", "Ber\u00b7gen", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die brechen zornig aus der Felsenhalle,", "tokens": ["Die", "bre\u00b7chen", "zor\u00b7nig", "aus", "der", "Fel\u00b7sen\u00b7hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die andern plaudern in melod'schem Falle", "tokens": ["Die", "an\u00b7dern", "plau\u00b7dern", "in", "me\u00b7lo\u00b7d'\u00b7schem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mit Nymphen, die im Gr\u00fcn vertraulich lauschen", "tokens": ["Mit", "Nym\u00b7phen", ",", "die", "im", "Gr\u00fcn", "ver\u00b7trau\u00b7lich", "lau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch wie sie irrend auch die Bahn vertauschen,", "tokens": ["Doch", "wie", "sie", "ir\u00b7rend", "auch", "die", "Bahn", "ver\u00b7tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie treffen endlich doch zusammen alle,", "tokens": ["Sie", "tref\u00b7fen", "end\u00b7lich", "doch", "zu\u00b7sam\u00b7men", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Strom, mit br\u00fcderlicher Wogen Schwalle", "tokens": ["Ein", "Strom", ",", "mit", "br\u00fc\u00b7der\u00b7li\u00b7cher", "Wo\u00b7gen", "Schwal\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erfrischend durch das sch\u00f6ne Land zu rauschen.", "tokens": ["Er\u00b7fri\u00b7schend", "durch", "das", "sch\u00f6\u00b7ne", "Land", "zu", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}