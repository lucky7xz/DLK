{"textgrid.poem.41926": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin von aller Ruh geschieden", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin von aller Ruh geschieden", "tokens": ["Ich", "bin", "von", "al\u00b7ler", "Ruh", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und treib' umher auf wilder Fluth;", "tokens": ["Und", "treib'", "um\u00b7her", "auf", "wil\u00b7der", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An einem Ort nur find' ich Frieden,", "tokens": ["An", "ei\u00b7nem", "Ort", "nur", "find'", "ich", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der Ort, wo alles ruht.", "tokens": ["Das", "ist", "der", "Ort", ",", "wo", "al\u00b7les", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn die Wind' auch schaurig sausen,", "tokens": ["Und", "wenn", "die", "Wind'", "auch", "schau\u00b7rig", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kalt der Regen niederf\u00e4llt,", "tokens": ["Und", "kalt", "der", "Re\u00b7gen", "nie\u00b7der\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch mag ich dort viel lieber hausen", "tokens": ["Doch", "mag", "ich", "dort", "viel", "lie\u00b7ber", "hau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als in der unbest\u00e4nd'gen Welt.", "tokens": ["Als", "in", "der", "un\u00b7be\u00b7st\u00e4n\u00b7d'\u00b7gen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Denn wie die Tr\u00e4ume spurlos schweben,", "tokens": ["Denn", "wie", "die", "Tr\u00e4u\u00b7me", "spur\u00b7los", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und einer schnell den andern treibt,", "tokens": ["Und", "ei\u00b7ner", "schnell", "den", "an\u00b7dern", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Spielt mit sich selbst das irre Leben,", "tokens": ["Spielt", "mit", "sich", "selbst", "das", "ir\u00b7re", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und jedes naht und keines bleibt.", "tokens": ["Und", "je\u00b7des", "naht", "und", "kei\u00b7nes", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nie will die falsche Hoffnung weichen,", "tokens": ["Nie", "will", "die", "fal\u00b7sche", "Hoff\u00b7nung", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nie mit der Hoffnung Furcht und M\u00fch.", "tokens": ["Nie", "mit", "der", "Hoff\u00b7nung", "Furcht", "und", "M\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Die Ewigstummen, Ewigbleichen,", "tokens": ["Die", "E\u00b7wigs\u00b7tum\u00b7men", ",", "E\u00b7wig\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verhei\u00dfen und versagen nie.", "tokens": ["Ver\u00b7hei\u00b7\u00dfen", "und", "ver\u00b7sa\u00b7gen", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht weck' ich sie mit meinen Schritten", "tokens": ["Nicht", "weck'", "ich", "sie", "mit", "mei\u00b7nen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVIMP", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrer dunklen Einsamkeit;", "tokens": ["In", "ih\u00b7rer", "dunk\u00b7len", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wissen nicht, was ich gelitten,", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "was", "ich", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und keinen st\u00f6rt mein tiefes Leid.", "tokens": ["Und", "kei\u00b7nen", "st\u00f6rt", "mein", "tie\u00b7fes", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort kann die Seele freyer klagen", "tokens": ["Dort", "kann", "die", "See\u00b7le", "frey\u00b7er", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bey Jener, die ich treu geliebt;", "tokens": ["Bey", "Je\u00b7ner", ",", "die", "ich", "treu", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht wird der kalte Stein mir sagen,", "tokens": ["Nicht", "wird", "der", "kal\u00b7te", "Stein", "mir", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ach, da\u00df auch sie mein Schmerz betr\u00fcbt!", "tokens": ["Ach", ",", "da\u00df", "auch", "sie", "mein", "Schmerz", "be\u00b7tr\u00fcbt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}