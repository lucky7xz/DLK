{"textgrid.poem.47773": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sch\u00f6n ist der Erde Thal]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist der Erde Thal;", "tokens": ["Sch\u00f6n", "ist", "der", "Er\u00b7de", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Smaragdner Gr\u00fcne Pracht,", "tokens": ["Sma\u00b7ragd\u00b7ner", "Gr\u00fc\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die goldne Rose lacht,", "tokens": ["Die", "gold\u00b7ne", "Ro\u00b7se", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es laben Ambrad\u00fcfte,", "tokens": ["Es", "la\u00b7ben", "Am\u00b7bra\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es woget durch die L\u00fcfte", "tokens": ["Es", "wo\u00b7get", "durch", "die", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Nachtigallent\u00f6nen", "tokens": ["Von", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein lieblicher Choral;", "tokens": ["Ein", "lieb\u00b7li\u00b7cher", "Cho\u00b7ral", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es blinket der Pokal", "tokens": ["Es", "blin\u00b7ket", "der", "Po\u00b7kal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und tausende von Sch\u00f6nen,", "tokens": ["Und", "tau\u00b7sen\u00b7de", "von", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die dieses Lustgewimmel", "tokens": ["Die", "die\u00b7ses", "Lust\u00b7ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["ART", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mit ihrem Reize kr\u00f6nen,", "tokens": ["Mit", "ih\u00b7rem", "Rei\u00b7ze", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Erm\u00fcden deine Wahl.", "tokens": ["Er\u00b7m\u00fc\u00b7den", "dei\u00b7ne", "Wahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Was also willst du weiter,", "tokens": ["Was", "al\u00b7so", "willst", "du", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Betrogne Phantasei?", "tokens": ["Be\u00b7trog\u00b7ne", "Phan\u00b7ta\u00b7sei", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Es ist, ich sag' es frei,", "tokens": ["Es", "ist", ",", "ich", "sag'", "es", "frei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Vonn\u00f6then uns kein Himmel", "tokens": ["Von\u00b7n\u00f6\u00b7then", "uns", "kein", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und keine Himmelsleiter,", "tokens": ["Und", "kei\u00b7ne", "Him\u00b7mels\u00b7lei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Nur da\u00df die Seele heiter,", "tokens": ["Nur", "da\u00df", "die", "See\u00b7le", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Nur da\u00df die Sehe frei,", "tokens": ["Nur", "da\u00df", "die", "Se\u00b7he", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Zu blicken ohne H\u00fclle", "tokens": ["Zu", "bli\u00b7cken", "oh\u00b7ne", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "In diese reiche F\u00fclle", "tokens": ["In", "die\u00b7se", "rei\u00b7che", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Von Gl\u00fcck und Wonne sei.", "tokens": ["Von", "Gl\u00fcck", "und", "Won\u00b7ne", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}