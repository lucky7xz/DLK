{"textgrid.poem.39402": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Freundes Zunge sprach zu der Geliebten Lippen:", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Freundes Zunge sprach zu der Geliebten Lippen:", "tokens": ["Des", "Freun\u00b7des", "Zun\u00b7ge", "sprach", "zu", "der", "Ge\u00b7lieb\u00b7ten", "Lip\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ist's, das, mich zu bannen, euch bewegt?", "tokens": ["Was", "ist's", ",", "das", ",", "mich", "zu", "ban\u00b7nen", ",", "euch", "be\u00b7wegt", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PDS", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00dft mich nur Einmal euren Nektar nippen!", "tokens": ["La\u00dft", "mich", "nur", "Ein\u00b7mal", "eu\u00b7ren", "Nek\u00b7tar", "nip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin ein Pfeil der keine Wunde schl\u00e4gt.", "tokens": ["Ich", "bin", "ein", "Pfeil", "der", "kei\u00b7ne", "Wun\u00b7de", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich bin beredt, ich lisple Huldigungen,", "tokens": ["Ich", "bin", "be\u00b7redt", ",", "ich", "lisp\u00b7le", "Hul\u00b7di\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich fleh' um Gunst und innigen Verein;", "tokens": ["Ich", "fleh'", "um", "Gunst", "und", "in\u00b7ni\u00b7gen", "Ver\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und das vermag ich auch allein.", "tokens": ["Und", "das", "ver\u00b7mag", "ich", "auch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch das Gekose schwesterlicher Zungen,", "tokens": ["Doch", "das", "Ge\u00b7ko\u00b7se", "schwes\u00b7ter\u00b7li\u00b7cher", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die, insgeheim, sich liebevoll umschlungen,", "tokens": ["Die", ",", "ins\u00b7ge\u00b7heim", ",", "sich", "lie\u00b7be\u00b7voll", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "$,", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wird \u00fcberschwenglicher als alle Worte sein:", "tokens": ["Wird", "\u00fc\u00b7bersc\u00b7hweng\u00b7li\u00b7cher", "als", "al\u00b7le", "Wor\u00b7te", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es athmet Flammen und entz\u00fcckte Pein.", "tokens": ["Es", "ath\u00b7met", "Flam\u00b7men", "und", "ent\u00b7z\u00fcck\u00b7te", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}