{"textgrid.poem.39152": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[wenn auf zu den Wolken ich schaue]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf zu den Wolken ich schaue", "tokens": ["Wenn", "auf", "zu", "den", "Wol\u00b7ken", "ich", "schau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "APPR", "ART", "NN", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ins feucht umw\u00f6lkte Blau,", "tokens": ["Ins", "feucht", "um\u00b7w\u00f6lk\u00b7te", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann denk' ich an Deine Augen,", "tokens": ["Dann", "denk'", "ich", "an", "Dei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du wundersch\u00f6ne Frau!", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die weinenden Wolken", "tokens": ["Und", "wenn", "die", "wei\u00b7nen\u00b7den", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Hinst\u00e4uben den Morgentau,", "tokens": ["Hin\u00b7st\u00e4u\u00b7ben", "den", "Mor\u00b7gen\u00b7tau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dann denk' ich an Deine Tr\u00e4nen,", "tokens": ["Dann", "denk'", "ich", "an", "Dei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Du wundersch\u00f6ne Frau!", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und schau' ich zwei Wolken innig", "tokens": ["Und", "schau'", "ich", "zwei", "Wol\u00b7ken", "in\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "CARD", "NN", "ADJD"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zusammenrinnen in Grau,", "tokens": ["Zu\u00b7sam\u00b7men\u00b7rin\u00b7nen", "in", "Grau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dann denk' ich an unsre Liebe,", "tokens": ["Dann", "denk'", "ich", "an", "uns\u00b7re", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du wundersch\u00f6ne Frau!", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und tobt in der Wolken Busen", "tokens": ["Und", "tobt", "in", "der", "Wol\u00b7ken", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Grimm der Orkane rauh,", "tokens": ["Der", "Grimm", "der", "Or\u00b7ka\u00b7ne", "rauh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "ADJD", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.7": {"text": "Dann denk' ich an unsre Schmerzen,", "tokens": ["Dann", "denk'", "ich", "an", "uns\u00b7re", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Du wundersch\u00f6ne Frau!", "tokens": ["Du", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}