{"dta.poem.7761": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig von Lahor', in seines Reiches Mitte,", "tokens": ["Der", "K\u00f6\u00b7nig", "von", "La\u00b7hor'", ",", "in", "sei\u00b7nes", "Rei\u00b7ches", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat aus Freigebigkeit erfunden eine Sitte.", "tokens": ["Hat", "aus", "Frei\u00b7ge\u00b7big\u00b7keit", "er\u00b7fun\u00b7den", "ei\u00b7ne", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "An jedem Monat l\u00e4\u00dft er sich einmal aufw\u00e4gen", "tokens": ["An", "je\u00b7dem", "Mo\u00b7nat", "l\u00e4\u00dft", "er", "sich", "ein\u00b7mal", "auf\u00b7w\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+--+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit M\u00fcnzen gro\u00df und klein von eigenen Gepr\u00e4gen.", "tokens": ["Mit", "M\u00fcn\u00b7zen", "gro\u00df", "und", "klein", "von", "ei\u00b7ge\u00b7nen", "Ge\u00b7pr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In eine Wagschal' ist er als Gewicht gethan,", "tokens": ["In", "ei\u00b7ne", "Wag\u00b7schal'", "ist", "er", "als", "Ge\u00b7wicht", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in die andre Geld, genau auf Unz' und Gran.", "tokens": ["Und", "in", "die", "and\u00b7re", "Geld", ",", "ge\u00b7nau", "auf", "Un\u00b7z'", "und", "Gran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.4": {"line.1": {"text": "Wenn einfach gn\u00e4dig nur, ists Silber, wenn er hold", "tokens": ["Wenn", "ein\u00b7fach", "gn\u00e4\u00b7dig", "nur", ",", "ists", "Sil\u00b7ber", ",", "wenn", "er", "hold"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "ADV", "$,", "VAFIN", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besonders seyn will, wird gemischt darunter Gold.", "tokens": ["Be\u00b7son\u00b7ders", "seyn", "will", ",", "wird", "ge\u00b7mischt", "da\u00b7run\u00b7ter", "Gold", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "VMFIN", "$,", "VAFIN", "VVPP", "PAV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und soviel als er wog, soviel theilt er gewogen", "tokens": ["Und", "so\u00b7viel", "als", "er", "wog", ",", "so\u00b7viel", "theilt", "er", "ge\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "VVPP"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den Armen aus, davon wird ihnen nichts entzogen.", "tokens": ["Den", "Ar\u00b7men", "aus", ",", "da\u00b7von", "wird", "ih\u00b7nen", "nichts", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PAV", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Armen beten, da\u00df ihr F\u00fcrst auf seine Wage,", "tokens": ["Die", "Ar\u00b7men", "be\u00b7ten", ",", "da\u00df", "ihr", "F\u00fcrst", "auf", "sei\u00b7ne", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt jeden Monat, doch sich setz' an jedem Tage.", "tokens": ["Statt", "je\u00b7den", "Mo\u00b7nat", ",", "doch", "sich", "setz'", "an", "je\u00b7dem", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "ADV", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie beten, da\u00df ihr F\u00fcrst fett werde, dick und schwer,", "tokens": ["Sie", "be\u00b7ten", ",", "da\u00df", "ihr", "F\u00fcrst", "fett", "wer\u00b7de", ",", "dick", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der leider magrer wird und leichter immermehr.", "tokens": ["Der", "lei\u00b7der", "mag\u00b7rer", "wird", "und", "leich\u00b7ter", "im\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Bald wird ein Federchen des F\u00fcrsten Leib aufwiegen,", "tokens": ["Bald", "wird", "ein", "Fe\u00b7der\u00b7chen", "des", "F\u00fcrs\u00b7ten", "Leib", "auf\u00b7wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann werden weder Gold noch Silber Arme kriegen.", "tokens": ["Dann", "wer\u00b7den", "we\u00b7der", "Gold", "noch", "Sil\u00b7ber", "Ar\u00b7me", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "KON", "NN", "ADV", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}