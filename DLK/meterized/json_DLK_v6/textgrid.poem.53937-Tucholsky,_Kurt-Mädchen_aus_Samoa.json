{"textgrid.poem.53937": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchen aus Samoa", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein M\u00e4dchen aus Samoa. Wir gingen", "tokens": ["Ich", "bin", "ein", "M\u00e4d\u00b7chen", "aus", "Sa\u00b7moa", ".", "Wir", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit Schmuckketten und einem Schurz bekleidet,", "tokens": ["mit", "Schmuck\u00b7ket\u00b7ten", "und", "ei\u00b7nem", "Schurz", "be\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Tiere des Waldes haben uns um unsere Sch\u00f6nheit beneidet \u2013", "tokens": ["die", "Tie\u00b7re", "des", "Wal\u00b7des", "ha\u00b7ben", "uns", "um", "un\u00b7se\u00b7re", "Sch\u00f6n\u00b7heit", "be\u00b7nei\u00b7det", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "wir waren frei wie sie.", "tokens": ["wir", "wa\u00b7ren", "frei", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann aber sind die wei\u00dfen Fremden in unser Land gekommen", "tokens": ["Dann", "a\u00b7ber", "sind", "die", "wei\u00b7\u00dfen", "Frem\u00b7den", "in", "un\u00b7ser", "Land", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "und haben uns unsere G\u00f6tter und unsere Felder fortgenommen \u2013", "tokens": ["und", "ha\u00b7ben", "uns", "un\u00b7se\u00b7re", "G\u00f6t\u00b7ter", "und", "un\u00b7se\u00b7re", "Fel\u00b7der", "fort\u00b7ge\u00b7nom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+--+--+--+-+-+-", "measure": "amphibrach.penta.plus"}, "line.7": {"text": "was haben sie uns daf\u00fcr gegeben?", "tokens": ["was", "ha\u00b7ben", "sie", "uns", "da\u00b7f\u00fcr", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "PAV", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ihre Missionare gaben uns einen Aberglauben und Plappergebete;", "tokens": ["Ih\u00b7re", "Mis\u00b7si\u00b7o\u00b7na\u00b7re", "ga\u00b7ben", "uns", "ei\u00b7nen", "A\u00b7berg\u00b7lau\u00b7ben", "und", "Plap\u00b7per\u00b7ge\u00b7be\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "KON", "NN", "$."], "meter": "+-+--+-+--+-+-+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "ihre Kaufleute gaben uns Whisky, bedruckten Kattun und Eisenger\u00e4te \u2013", "tokens": ["ih\u00b7re", "Kauf\u00b7leu\u00b7te", "ga\u00b7ben", "uns", "Whis\u00b7ky", ",", "be\u00b7druck\u00b7ten", "Kat\u00b7tun", "und", "Ei\u00b7sen\u00b7ge\u00b7r\u00e4\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NE", "$,", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+--+--+--+-+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "seit wir es kennen, brauchen wir das.", "tokens": ["seit", "wir", "es", "ken\u00b7nen", ",", "brau\u00b7chen", "wir", "das", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PDS", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ihre Soldaten gaben uns eine neue Art, zu morden und zu henken;", "tokens": ["Ih\u00b7re", "Sol\u00b7da\u00b7ten", "ga\u00b7ben", "uns", "ei\u00b7ne", "neu\u00b7e", "Art", ",", "zu", "mor\u00b7den", "und", "zu", "hen\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}, "line.5": {"text": "ihre M\u00e4nner gaben uns die Syphilis benebst einigen andern Geschenken \u2013", "tokens": ["ih\u00b7re", "M\u00e4n\u00b7ner", "ga\u00b7ben", "uns", "die", "Sy\u00b7phi\u00b7lis", "be\u00b7nebst", "ei\u00b7ni\u00b7gen", "an\u00b7dern", "Ge\u00b7schen\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NE", "VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-+--+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "In meinen tiefen Augen liegt noch die Sch\u00f6nheit unserer Allmutter Natur;", "tokens": ["In", "mei\u00b7nen", "tie\u00b7fen", "Au\u00b7gen", "liegt", "noch", "die", "Sch\u00f6n\u00b7heit", "un\u00b7se\u00b7rer", "A\u00b7llmut\u00b7ter", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "um meine Beine schlottert schon der Rock der Zivilisation \u2013 wartet nur:", "tokens": ["um", "mei\u00b7ne", "Bei\u00b7ne", "schlot\u00b7tert", "schon", "der", "Rock", "der", "Zi\u00b7vi\u00b7li\u00b7sa\u00b7ti\u00b7on", "\u2013", "war\u00b7tet", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$(", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+--+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "noch bin ich halb.", "tokens": ["noch", "bin", "ich", "halb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Eines Tages aber werden wir alle die europ\u00e4ischen Gaben gegen die Ausbeuter wenden,", "tokens": ["Ei\u00b7nes", "Ta\u00b7ges", "a\u00b7ber", "wer\u00b7den", "wir", "al\u00b7le", "die", "eu\u00b7ro\u00b7p\u00e4\u00b7i\u00b7schen", "Ga\u00b7ben", "ge\u00b7gen", "die", "Aus\u00b7beu\u00b7ter", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPER", "PIS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Telegrafen und Automobile bedienen wir mit unsern braunen H\u00e4nden;", "tokens": ["Te\u00b7le\u00b7gra\u00b7fen", "und", "Au\u00b7to\u00b7mo\u00b7bi\u00b7le", "be\u00b7die\u00b7nen", "wir", "mit", "un\u00b7sern", "brau\u00b7nen", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "eines Tagesk\u00e4mpfen wir, braune und gelbe Arbeiter f\u00fcr unser eigenes Leben:", "tokens": ["ei\u00b7nes", "Ta\u00b7ges\u00b7k\u00e4mp\u00b7fen", "wir", ",", "brau\u00b7ne", "und", "gel\u00b7be", "Ar\u00b7bei\u00b7ter", "f\u00fcr", "un\u00b7ser", "ei\u00b7ge\u00b7nes", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "VVFIN", "KON", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+--+--+--+-+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "eines Tages werden die Kontinente sich ihre Freiheit geben \u2013!", "tokens": ["ei\u00b7nes", "Ta\u00b7ges", "wer\u00b7den", "die", "Kon\u00b7ti\u00b7nen\u00b7te", "sich", "ih\u00b7re", "Frei\u00b7heit", "ge\u00b7ben", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PRF", "PPOSAT", "NN", "VVINF", "$(", "$."], "meter": "+-+-+--+----+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Denn ", "tokens": ["Denn"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "aus schwer arbeitender Brust ein Gekeuch:", "tokens": ["aus", "schwer", "ar\u00b7bei\u00b7ten\u00b7der", "Brust", "ein", "Ge\u00b7keuch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Proletarier aller L\u00e4nder, vereinigt euch!", "tokens": ["Pro\u00b7le\u00b7ta\u00b7rier", "al\u00b7ler", "L\u00e4n\u00b7der", ",", "ver\u00b7ei\u00b7nigt", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}}}}