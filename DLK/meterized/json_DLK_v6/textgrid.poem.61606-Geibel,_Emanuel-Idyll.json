{"textgrid.poem.61606": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Idyll", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch auf des Eilands schroffem Vorgeb\u00fcrg,", "tokens": ["Hoch", "auf", "des", "Ei\u00b7lands", "schrof\u00b7fem", "Vor\u00b7ge\u00b7b\u00fcrg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom himmelblauen Meer umg\u00fcrtet liegt", "tokens": ["Vom", "him\u00b7mel\u00b7blau\u00b7en", "Meer", "um\u00b7g\u00fcr\u00b7tet", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Kloster, dessen offnen Bogengang", "tokens": ["Das", "Klos\u00b7ter", ",", "des\u00b7sen", "off\u00b7nen", "Bo\u00b7gen\u00b7gang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit wei\u00dfem Glanz die Morgensonne f\u00fcllt.", "tokens": ["Mit", "wei\u00b7\u00dfem", "Glanz", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch k\u00fchl noch ist's im Garten, wo der Hauch", "tokens": ["Doch", "k\u00fchl", "noch", "ist's", "im", "Gar\u00b7ten", ",", "wo", "der", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VAFIN", "APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der See gelind die schwarzen Riesenwipfel", "tokens": ["Der", "See", "ge\u00b7lind", "die", "schwar\u00b7zen", "Rie\u00b7sen\u00b7wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der hundertj\u00e4hrigen Zypressen wiegt,", "tokens": ["Der", "hun\u00b7dert\u00b7j\u00e4h\u00b7ri\u00b7gen", "Zyp\u00b7res\u00b7sen", "wiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und frisch vom Tau der Nacht die Rose bl\u00fcht.", "tokens": ["Und", "frisch", "vom", "Tau", "der", "Nacht", "die", "Ro\u00b7se", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dort wandelt ernst im dunkeln Ordenskleid", "tokens": ["Dort", "wan\u00b7delt", "ernst", "im", "dun\u00b7keln", "Or\u00b7dens\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein alter M\u00f6nch; die tiefgefurchte Stirn,", "tokens": ["Ein", "al\u00b7ter", "M\u00f6nch", ";", "die", "tief\u00b7ge\u00b7furch\u00b7te", "Stirn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Zug ged\u00e4mpfter Wehmut um den Mund", "tokens": ["Der", "Zug", "ge\u00b7d\u00e4mpf\u00b7ter", "Weh\u00b7mut", "um", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Verraten, da\u00df er einst die Welt gekannt,", "tokens": ["Ver\u00b7ra\u00b7ten", ",", "da\u00df", "er", "einst", "die", "Welt", "ge\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und da\u00df er erst gescheitert ihr entsagt.", "tokens": ["Und", "da\u00df", "er", "erst", "ge\u00b7schei\u00b7tert", "ihr", "ent\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Jetzt \u00fcbt er treulich jede fromme Pflicht", "tokens": ["Jetzt", "\u00fcbt", "er", "treu\u00b7lich", "je\u00b7de", "from\u00b7me", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und wallt, der Rosen und des lichten Meers", "tokens": ["Und", "wallt", ",", "der", "Ro\u00b7sen", "und", "des", "lich\u00b7ten", "Meers"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Kaum achtend, hin, vertieft in sein Brevier.", "tokens": ["Kaum", "ach\u00b7tend", ",", "hin", ",", "ver\u00b7tieft", "in", "sein", "Bre\u00b7vier", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PTKVZ", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Doch als ein sch\u00f6ner blauer Schmetterling", "tokens": ["Doch", "als", "ein", "sch\u00f6\u00b7ner", "blau\u00b7er", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Sich ihm aufs Buch setzt, l\u00e4chelt er und wagt", "tokens": ["Sich", "ihm", "aufs", "Buch", "setzt", ",", "l\u00e4\u00b7chelt", "er", "und", "wagt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PPER", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Den leichtbeschwingten Gast nicht fortzuscheuchen", "tokens": ["Den", "leicht\u00b7be\u00b7schwing\u00b7ten", "Gast", "nicht", "fort\u00b7zu\u00b7scheu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und schaut dem Zwinkern seiner Fl\u00fcgel zu,", "tokens": ["Und", "schaut", "dem", "Zwin\u00b7kern", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der Zeit gedenkend, da er selbst noch froh", "tokens": ["Der", "Zeit", "ge\u00b7den\u00b7kend", ",", "da", "er", "selbst", "noch", "froh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Geflattert durch des Lebens Sonnenschein.", "tokens": ["Ge\u00b7flat\u00b7tert", "durch", "des", "Le\u00b7bens", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}