{"textgrid.poem.44159": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo, Amor, kommstu denn erst heute", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo, Amor, kommstu denn erst heute", "tokens": ["Wo", ",", "A\u00b7mor", ",", "komms\u00b7tu", "denn", "erst", "heu\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "NE", "$,", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schnell auf einmahl wieder her?", "tokens": ["So", "schnell", "auf", "ein\u00b7mahl", "wie\u00b7der", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schwur dir ja nicht ohngefehr:", "tokens": ["Ich", "schwur", "dir", "ja", "nicht", "ohn\u00b7ge\u00b7fehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun sind wir zwey geschiedne Leute.", "tokens": ["Nun", "sind", "wir", "zwey", "ge\u00b7schied\u00b7ne", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verschone meiner Sorgen Lauf", "tokens": ["Ver\u00b7scho\u00b7ne", "mei\u00b7ner", "Sor\u00b7gen", "Lauf"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und leg die Pfeile nicht erst auf;", "tokens": ["Und", "leg", "die", "Pfei\u00b7le", "nicht", "erst", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du siehst, ich bin nicht mehr derselbe,", "tokens": ["Du", "siehst", ",", "ich", "bin", "nicht", "mehr", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "PDAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Filindrene k\u00fcst und dr\u00fcckt,", "tokens": ["Den", "Fi\u00b7lind\u00b7re\u00b7ne", "k\u00fcst", "und", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "Noch der, den Weistriz, Plei\u00df und Elbe", "tokens": ["Noch", "der", ",", "den", "Wei\u00b7striz", ",", "Plei\u00df", "und", "El\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "ART", "NN", "$,", "NN", "KON", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "An ihrem Ufer oft entz\u00fcckt.", "tokens": ["An", "ih\u00b7rem", "U\u00b7fer", "oft", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Alter kommt mir vor den Jahren,", "tokens": ["Das", "Al\u00b7ter", "kommt", "mir", "vor", "den", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe zeitig ausgedient,", "tokens": ["Ich", "ha\u00b7be", "zei\u00b7tig", "aus\u00b7ge\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Fr\u00fchling ist in Angst vergr\u00fcnt", "tokens": ["Mein", "Fr\u00fch\u00b7ling", "ist", "in", "Angst", "ver\u00b7gr\u00fcnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und als ein Strom dahingefahren.", "tokens": ["Und", "als", "ein", "Strom", "da\u00b7hin\u00b7ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Auge, de\u00dfen feurig Spiel", "tokens": ["Mein", "Au\u00b7ge", ",", "de\u00b7\u00dfen", "feu\u00b7rig", "Spiel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Sch\u00f6nen in das Auge fiel,", "tokens": ["Den", "Sch\u00f6\u00b7nen", "in", "das", "Au\u00b7ge", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hat manchen Siegeskranz empfangen;", "tokens": ["Hat", "man\u00b7chen", "Sie\u00b7ges\u00b7kranz", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dies Auge sieht jezt l\u00e4\u00dfig zu", "tokens": ["Dies", "Au\u00b7ge", "sieht", "jezt", "l\u00e4\u00b7\u00dfig", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "ADV", "ADJD", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und winckt mit thr\u00e4nendem Verlangen", "tokens": ["Und", "winckt", "mit", "thr\u00e4\u00b7nen\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der in der Welt versagten Ruh.", "tokens": ["Der", "in", "der", "Welt", "ver\u00b7sag\u00b7ten", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geh, loser Dieb, mit deinen Flammen", "tokens": ["Geh", ",", "lo\u00b7ser", "Dieb", ",", "mit", "dei\u00b7nen", "Flam\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schmelze Florens harten Sinn.", "tokens": ["Und", "schmel\u00b7ze", "Flo\u00b7rens", "har\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie giebt der Zeit die Sch\u00f6nheit hin", "tokens": ["Sie", "giebt", "der", "Zeit", "die", "Sch\u00f6n\u00b7heit", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will sich selbst zur Noth verdammen.", "tokens": ["Und", "will", "sich", "selbst", "zur", "Noth", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Geh, bring ihr die Empfindung bey,", "tokens": ["Geh", ",", "bring", "ihr", "die", "Emp\u00b7fin\u00b7dung", "bey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Warum sie jung und artig sey", "tokens": ["Wa\u00b7rum", "sie", "jung", "und", "ar\u00b7tig", "sey"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn und wie man lieben solle!", "tokens": ["Und", "wenn", "und", "wie", "man", "lie\u00b7ben", "sol\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "KON", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geh, fl\u00f6\u00df ihr deine Klugheit ein;", "tokens": ["Geh", ",", "fl\u00f6\u00df", "ihr", "dei\u00b7ne", "Klug\u00b7heit", "ein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es wird ihr mehr als eine Rolle", "tokens": ["Es", "wird", "ihr", "mehr", "als", "ei\u00b7ne", "Rol\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Verliebter Sclaven Weihrauch streun.", "tokens": ["Ver\u00b7lieb\u00b7ter", "Scla\u00b7ven", "Weih\u00b7rauch", "streun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist gewehlt, wie Kinder pflegen,", "tokens": ["Du", "bist", "ge\u00b7wehlt", ",", "wie", "Kin\u00b7der", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und liebst ein aufger\u00e4umt Quartier;", "tokens": ["Und", "liebst", "ein", "auf\u00b7ge\u00b7r\u00e4umt", "Quar\u00b7tier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies aber suche nicht bey mir,", "tokens": ["Dies", "a\u00b7ber", "su\u00b7che", "nicht", "bey", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich m\u00fcste dich auf Dornen legen.", "tokens": ["Ich", "m\u00fcs\u00b7te", "dich", "auf", "Dor\u00b7nen", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Gram erf\u00fcllt auch schon mein Herz,", "tokens": ["Der", "Gram", "er\u00b7f\u00fcllt", "auch", "schon", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du sch\u00e4ckerst, dies verbeuth mein Schmerz,", "tokens": ["Du", "sch\u00e4\u00b7ckerst", ",", "dies", "ver\u00b7beuth", "mein", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mu\u00df ich dann und wann noch lachen,", "tokens": ["Und", "mu\u00df", "ich", "dann", "und", "wann", "noch", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "KON", "PWAV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geschieht es nur aus Bitterkeit,", "tokens": ["Ge\u00b7schieht", "es", "nur", "aus", "Bit\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem P\u00f6bel keine Lust zu machen,", "tokens": ["Dem", "P\u00f6\u00b7bel", "kei\u00b7ne", "Lust", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der \u00fcber meine Muse schreyt.", "tokens": ["Der", "\u00fc\u00b7ber", "mei\u00b7ne", "Mu\u00b7se", "schreyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Lauf, Amor, lauf mit List und Stricken,", "tokens": ["Lauf", ",", "A\u00b7mor", ",", "lauf", "mit", "List", "und", "Stri\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und such ein Kind von guter Art,", "tokens": ["Und", "such", "ein", "Kind", "von", "gu\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An welchem Gl\u00fcck und Zucht nichts spart,", "tokens": ["An", "wel\u00b7chem", "Gl\u00fcck", "und", "Zucht", "nichts", "spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stand, Glieder, Wiz und Herz zu schm\u00fccken!", "tokens": ["Stand", ",", "Glie\u00b7der", ",", "Wiz", "und", "Herz", "zu", "schm\u00fc\u00b7cken", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dies unschuldsvolle Sch\u00f6nheitsbild", "tokens": ["Dies", "un\u00b7schulds\u00b7vol\u00b7le", "Sch\u00f6n\u00b7heits\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nimm, wenn auch Freund und Mutter schilt,", "tokens": ["Nimm", ",", "wenn", "auch", "Freund", "und", "Mut\u00b7ter", "schilt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und las ihm von den Charitinnen", "tokens": ["Und", "las", "ihm", "von", "den", "Cha\u00b7ri\u00b7tin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr reizend Ein ich weis nicht was", "tokens": ["Ihr", "rei\u00b7zend", "Ein", "ich", "weis", "nicht", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "ART", "PPER", "PTKVZ", "PTKNEG", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In alle Wort und Mienen rinnen;", "tokens": ["In", "al\u00b7le", "Wort", "und", "Mie\u00b7nen", "rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Denn Sch\u00f6nheit ohne dies ist Glas.", "tokens": ["Denn", "Sch\u00f6n\u00b7heit", "oh\u00b7ne", "dies", "ist", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du fragst, f\u00fcr wen ich so viel Gaben", "tokens": ["Du", "fragst", ",", "f\u00fcr", "wen", "ich", "so", "viel", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PWS", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und so was Seltnes fodern kan?", "tokens": ["Und", "so", "was", "Selt\u00b7nes", "fo\u00b7dern", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lauf, stecke Beuchelts Sehnsucht an,", "tokens": ["Lauf", ",", "ste\u00b7cke", "Beu\u00b7chelts", "Sehn\u00b7sucht", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "NE", "NN", "PTKVZ", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem gilt die Wahl, der soll es haben!", "tokens": ["Dem", "gilt", "die", "Wahl", ",", "der", "soll", "es", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word