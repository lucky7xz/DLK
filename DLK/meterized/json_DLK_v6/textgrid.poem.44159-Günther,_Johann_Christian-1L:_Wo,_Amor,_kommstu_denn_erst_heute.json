, "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "ADV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier wirft dein Scherzamt mehr Erg\u00f6zen,", "tokens": ["Hier", "wirft", "dein", "Scherz\u00b7amt", "mehr", "Er\u00b7g\u00f6\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mehr Herrligkeit und Opfer ab,", "tokens": ["Mehr", "Herr\u00b7lig\u00b7keit", "und", "Op\u00b7fer", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Cypern seiner Mutter gab.", "tokens": ["Als", "Cy\u00b7pern", "sei\u00b7ner", "Mut\u00b7ter", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier wird man dich in Rosen sezen,", "tokens": ["Hier", "wird", "man", "dich", "in", "Ro\u00b7sen", "se\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hier wird der Sch\u00f6nen Ku\u00df dein Wein,", "tokens": ["Hier", "wird", "der", "Sch\u00f6\u00b7nen", "Ku\u00df", "dein", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr Mund dein Ganymedes seyn,", "tokens": ["Ihr", "Mund", "dein", "Ga\u00b7ny\u00b7me\u00b7des", "seyn", ","], "token_info": ["word", "word", "word", "wo