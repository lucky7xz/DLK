{"textgrid.poem.32466": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen bekannten Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den nennt der Dichter Mars, und die nennt er Cythere;", "tokens": ["Den", "nennt", "der", "Dich\u00b7ter", "Mars", ",", "und", "die", "nennt", "er", "Cy\u00b7the\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,", "KON", "ART", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hier kommen Grazien, hier Musen ihm die Quere.", "tokens": ["Hier", "kom\u00b7men", "Gra\u00b7zi\u00b7en", ",", "hier", "Mu\u00b7sen", "ihm", "die", "Que\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADV", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Apoll, Minerva, Zeus versch\u00f6nern was er spricht;", "tokens": ["A\u00b7poll", ",", "Mi\u00b7ner\u00b7va", ",", "Zeus", "ver\u00b7sch\u00f6\u00b7nern", "was", "er", "spricht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "VVINF", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wen er zum Gott nicht macht, den lobt er lieber nicht.", "tokens": ["Wen", "er", "zum", "Gott", "nicht", "macht", ",", "den", "lobt", "er", "lie\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$,", "ART", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr, die ihr ihn der Welt verachtungswert gewiesen,", "tokens": ["Ihr", ",", "die", "ihr", "ihn", "der", "Welt", "ver\u00b7ach\u00b7tungs\u00b7wert", "ge\u00b7wie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "ART", "NN", "VVPP", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Trotz allen Tugenden, die er verstellt gepriesen;", "tokens": ["Trotz", "al\u00b7len", "Tu\u00b7gen\u00b7den", ",", "die", "er", "ver\u00b7stellt", "ge\u00b7prie\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Wenn er die G\u00f6tter all auf fertger Zunge tr\u00e4gt,", "tokens": ["Wenn", "er", "die", "G\u00f6t\u00b7ter", "all", "auf", "fert\u00b7ger", "Zun\u00b7ge", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PIAT", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was wunderts euch, da\u00df er im Herzen keinen hegt?", "tokens": ["Was", "wun\u00b7derts", "euch", ",", "da\u00df", "er", "im", "Her\u00b7zen", "kei\u00b7nen", "hegt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}