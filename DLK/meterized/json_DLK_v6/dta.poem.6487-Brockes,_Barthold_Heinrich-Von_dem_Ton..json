{"dta.poem.6487": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wie die Regeln allgemein", "tokens": ["So", "wie", "die", "Re\u00b7geln", "all\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey andern C\u00f6rpern seyn;", "tokens": ["Bey", "an\u00b7dern", "C\u00f6r\u00b7pern", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So haben T\u00f6n\u2019 auch ihr\u2019 Abwechselung,", "tokens": ["So", "ha\u00b7ben", "T\u00f6n'", "auch", "ih\u00b7r'", "Ab\u00b7wech\u00b7se\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie haben Gleichheit, Ma\u00df, auch Abstand, Aenderung", "tokens": ["Sie", "ha\u00b7ben", "Gleich\u00b7heit", ",", "Ma\u00df", ",", "auch", "Ab\u00b7stand", ",", "A\u00b7en\u00b7de\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "NN", "$,", "ADV", "NN", "$,", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und sonderbare Eigenschafft,", "tokens": ["Und", "son\u00b7der\u00b7ba\u00b7re", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja nach verschiednem Stand, auch unterschiedne Krafft.", "tokens": ["Ja", "nach", "ver\u00b7schied\u00b7nem", "Stand", ",", "auch", "un\u00b7ter\u00b7schied\u00b7ne", "Krafft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}