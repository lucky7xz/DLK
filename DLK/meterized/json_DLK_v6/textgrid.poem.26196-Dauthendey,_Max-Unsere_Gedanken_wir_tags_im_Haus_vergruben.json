{"textgrid.poem.26196": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Unsere Gedanken wir tags im Haus vergruben", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsere Gedanken wir tags im Haus vergruben", "tokens": ["Un\u00b7se\u00b7re", "Ge\u00b7dan\u00b7ken", "wir", "tags", "im", "Haus", "ver\u00b7gru\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVINF"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Hinter hei\u00dfen Fensterladen und in kellerdunklen Stuben.", "tokens": ["Hin\u00b7ter", "hei\u00b7\u00dfen", "Fens\u00b7ter\u00b7la\u00b7den", "und", "in", "kel\u00b7ler\u00b7dunk\u00b7len", "Stu\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "W\u00e4hrend drau\u00dfen der Flu\u00dfspiegel wie Hexensilber tanzt,", "tokens": ["W\u00e4h\u00b7rend", "drau\u00b7\u00dfen", "der", "Flu\u00df\u00b7spie\u00b7gel", "wie", "He\u00b7xen\u00b7sil\u00b7ber", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+--+---+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und die Sonne das Haus umschanzt mit Mittagbrand,", "tokens": ["Und", "die", "Son\u00b7ne", "das", "Haus", "um\u00b7schanzt", "mit", "Mit\u00b7tag\u00b7brand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Liegt drinnen mein Leben leicht in deiner Hand.", "tokens": ["Liegt", "drin\u00b7nen", "mein", "Le\u00b7ben", "leicht", "in", "dei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Am Abend, wenn der Vollmond die Flu\u00dfbr\u00fccke bleicht", "tokens": ["Am", "A\u00b7bend", ",", "wenn", "der", "Voll\u00b7mond", "die", "Flu\u00df\u00b7br\u00fc\u00b7cke", "bleicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-++-++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und dir einen goldenen Schleier \u00fcber die Fensterbank reicht,", "tokens": ["Und", "dir", "ei\u00b7nen", "gol\u00b7de\u00b7nen", "Schlei\u00b7er", "\u00fc\u00b7ber", "die", "Fens\u00b7ter\u00b7bank", "reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dann gehst du neben mir herzwandelnd dem Dunkel nach", "tokens": ["Dann", "gehst", "du", "ne\u00b7ben", "mir", "herz\u00b7wan\u00b7delnd", "dem", "Dun\u00b7kel", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "VVPP", "ART", "NN", "APPR"], "meter": "-+-+--++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und lockst wie das Abendl\u00e4uten die guten Geister unter das Dach.", "tokens": ["Und", "lockst", "wie", "das", "A\u00b7bend\u00b7l\u00e4u\u00b7ten", "die", "gu\u00b7ten", "Geis\u00b7ter", "un\u00b7ter", "das", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-+-+--+", "measure": "iambic.septa.relaxed"}}}}}