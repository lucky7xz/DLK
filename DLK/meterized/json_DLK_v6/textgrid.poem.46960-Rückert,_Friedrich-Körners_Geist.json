{"textgrid.poem.46960": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6rners Geist", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bedeckt von Moos und Schorfe,", "tokens": ["Be\u00b7deckt", "von", "Moos", "und", "Schor\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Eichbaum hoch und stark", "tokens": ["Ein", "Eich\u00b7baum", "hoch", "und", "stark"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steht bei W\u00f6bblin, dem Dorfe,", "tokens": ["Steht", "bei", "W\u00f6bb\u00b7lin", ",", "dem", "Dor\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Mecklenburger Mark.", "tokens": ["In", "Meck\u00b7len\u00b7bur\u00b7ger", "Mark", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Darunter ist von Steine", "tokens": ["Da\u00b7run\u00b7ter", "ist", "von", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein neues Grab gemacht,", "tokens": ["Ein", "neu\u00b7es", "Grab", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Draus steigt im Mondenscheine", "tokens": ["Draus", "steigt", "im", "Mon\u00b7den\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Geist um Mitternacht.", "tokens": ["Ein", "Geist", "um", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er richtet auf die Rinden", "tokens": ["Er", "rich\u00b7tet", "auf", "die", "Rin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Baums den Blick und liest", "tokens": ["Des", "Baums", "den", "Blick", "und", "liest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Namen, der zu finden", "tokens": ["Den", "Na\u00b7men", ",", "der", "zu", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort eingegraben ist.", "tokens": ["Dort", "ein\u00b7ge\u00b7gra\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann sucht er mit den H\u00e4nden", "tokens": ["Dann", "sucht", "er", "mit", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schwert, das liegt am Ort,", "tokens": ["Ein", "Schwert", ",", "das", "liegt", "am", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und g\u00fcrtet um die Lenden", "tokens": ["Und", "g\u00fcr\u00b7tet", "um", "die", "Len\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich dieses Schwert sofort.", "tokens": ["Sich", "die\u00b7ses", "Schwert", "so\u00b7fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Langt dann nach einer Leier,", "tokens": ["Langt", "dann", "nach", "ei\u00b7ner", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nimmt sie vom Ast herab,", "tokens": ["Nimmt", "sie", "vom", "Ast", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und setzt in stiller Feier", "tokens": ["Und", "setzt", "in", "stil\u00b7ler", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich singend auf sein Grab:", "tokens": ["Sich", "sin\u00b7gend", "auf", "sein", "Grab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich war in Jugendbrause", "tokens": ["Ich", "war", "in", "Ju\u00b7gend\u00b7brau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein rascher Reitersmann,", "tokens": ["Ein", "ra\u00b7scher", "Rei\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis hier im dunklen Hause", "tokens": ["Bis", "hier", "im", "dunk\u00b7len", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich Ruh' und Rast gewann.", "tokens": ["Ich", "Ruh'", "und", "Rast", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich war ein freier J\u00e4ger", "tokens": ["Ich", "war", "ein", "frei\u00b7er", "J\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In L\u00fctzows wilder Schar,", "tokens": ["In", "L\u00fct\u00b7zows", "wil\u00b7der", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch ein Zitherschl\u00e4ger,", "tokens": ["Und", "auch", "ein", "Zi\u00b7ther\u00b7schl\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Schwertlied klang so klar.", "tokens": ["Mein", "Schwert\u00b7lied", "klang", "so", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nun reiten die Genossen", "tokens": ["Nun", "rei\u00b7ten", "die", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allein auf ihrer Fahrt,", "tokens": ["Al\u00b7lein", "auf", "ih\u00b7rer", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-----+", "measure": "unknown.measure.single"}, "line.3": {"text": "Da ich vom Ro\u00df geschossen", "tokens": ["Da", "ich", "vom", "Ro\u00df", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hier begraben ward.", "tokens": ["Und", "hier", "be\u00b7gra\u00b7ben", "ward", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr m\u00f6gt nur weiter traben,", "tokens": ["Ihr", "m\u00f6gt", "nur", "wei\u00b7ter", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bis da\u00df ihr kommt ans Ziel.", "tokens": ["Bis", "da\u00df", "ihr", "kommt", "ans", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr habet mich begraben,", "tokens": ["Ihr", "ha\u00b7bet", "mich", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie es mir wohlgefiel.", "tokens": ["Wie", "es", "mir", "wohl\u00b7ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es sind die beiden Lieben,", "tokens": ["Es", "sind", "die", "bei\u00b7den", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mir im Leben wert,", "tokens": ["Die", "mir", "im", "Le\u00b7ben", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Tode mir geblieben,", "tokens": ["Im", "To\u00b7de", "mir", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Leier und das Schwert.", "tokens": ["Die", "Lei\u00b7er", "und", "das", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich seh' auch meinen Namen,", "tokens": ["Ich", "seh'", "auch", "mei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er unsterblich sei,", "tokens": ["Da\u00df", "er", "uns\u00b7terb\u00b7lich", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geschnitten in den Rahmen", "tokens": ["Ge\u00b7schnit\u00b7ten", "in", "den", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Eiche sch\u00f6n und frei.", "tokens": ["Der", "Ei\u00b7che", "sch\u00f6n", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es sind die sch\u00f6nsten Kr\u00e4nze", "tokens": ["Es", "sind", "die", "sch\u00f6ns\u00b7ten", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gegeben meiner Gruft,", "tokens": ["Ge\u00b7ge\u00b7ben", "mei\u00b7ner", "Gruft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sich in jedem Lenze", "tokens": ["Die", "sich", "in", "je\u00b7dem", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erneu'n mit frischem Duft.", "tokens": ["Er\u00b7neu'n", "mit", "fri\u00b7schem", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Eich' ob meiner Scheitel,", "tokens": ["Die", "Eich'", "ob", "mei\u00b7ner", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ist der Kranz so gro\u00df;", "tokens": ["Wie", "ist", "der", "Kranz", "so", "gro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Ringen war nicht eitel,", "tokens": ["Mein", "Rin\u00b7gen", "war", "nicht", "ei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich ruh' in ihrem Scho\u00df.", "tokens": ["Ich", "ruh'", "in", "ih\u00b7rem", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Man hat in F\u00fcrstengr\u00fcften", "tokens": ["Man", "hat", "in", "F\u00fcrs\u00b7ten\u00b7gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bestatten mich gewollt;", "tokens": ["Be\u00b7stat\u00b7ten", "mich", "ge\u00b7wollt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VMPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier in den frischen D\u00fcften", "tokens": ["Hier", "in", "den", "fri\u00b7schen", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr ruhn mich lassen sollt.", "tokens": ["Ihr", "ruhn", "mich", "las\u00b7sen", "sollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Hier sei noch oft mit Kr\u00e4useln", "tokens": ["Hier", "sei", "noch", "oft", "mit", "Kr\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Eiche Laub bewegt,", "tokens": ["Der", "Ei\u00b7che", "Laub", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn in des Windes S\u00e4useln", "tokens": ["Wenn", "in", "des", "Win\u00b7des", "S\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Geist die Saiten schl\u00e4gt.", "tokens": ["Mein", "Geist", "die", "Sai\u00b7ten", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}