{"textgrid.poem.56535": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinter Capri steht der Vollmond.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinter Capri steht der Vollmond.", "tokens": ["Hin\u00b7ter", "Cap\u00b7ri", "steht", "der", "Voll\u00b7mond", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Um die blauen Wasser webt", "tokens": ["Um", "die", "blau\u00b7en", "Was\u00b7ser", "webt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er ein breites Silberband.", "tokens": ["Er", "ein", "brei\u00b7tes", "Sil\u00b7ber\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Rosenduft die Nacht durchschwebt.", "tokens": ["Ro\u00b7sen\u00b7duft", "die", "Nacht", "durch\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "An dem Maste lehnt die Sklavin.", "tokens": ["An", "dem", "Mas\u00b7te", "lehnt", "die", "Skla\u00b7vin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um das Haar den Kranz aus Rosen", "tokens": ["Um", "das", "Haar", "den", "Kranz", "aus", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leimt sie, strahlend, prachtvoll, sch\u00f6n,", "tokens": ["Leimt", "sie", ",", "strah\u00b7lend", ",", "pracht\u00b7voll", ",", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Diana des Lysipp.", "tokens": ["Wie", "Di\u00b7a\u00b7na", "des", "Ly\u00b7sipp", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ihre feinen, schlanken Finger", "tokens": ["Ih\u00b7re", "fei\u00b7nen", ",", "schlan\u00b7ken", "Fin\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlagen der Kithara Saiten.", "tokens": ["Schla\u00b7gen", "der", "Ki\u00b7tha\u00b7ra", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Musik ist ihr Gewand,", "tokens": ["Die", "Mu\u00b7sik", "ist", "ihr", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Das der Mond mit Gold umwindet.", "tokens": ["Das", "der", "Mond", "mit", "Gold", "um\u00b7win\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich, der eben noch mit buhlnden Blicken", "tokens": ["Ich", ",", "der", "e\u00b7ben", "noch", "mit", "buhln\u00b7den", "Bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihren wei\u00dfen Leib umkoste,", "tokens": ["Ih\u00b7ren", "wei\u00b7\u00dfen", "Leib", "um\u00b7kos\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Beuge mich den Harmonien.", "tokens": ["Beu\u00b7ge", "mich", "den", "Har\u00b7mo\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Seele sang sie frei.", "tokens": ["Ih\u00b7re", "See\u00b7le", "sang", "sie", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}