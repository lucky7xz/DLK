{"textgrid.poem.25192": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Umhaucht vom Silberdufte", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umhaucht vom Silberdufte", "tokens": ["Um\u00b7haucht", "vom", "Sil\u00b7ber\u00b7duf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des \u00fcppig bl\u00fchenden Mondes,", "tokens": ["Des", "\u00fcp\u00b7pig", "bl\u00fc\u00b7hen\u00b7den", "Mon\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erschauert leise des Parkes", "tokens": ["Er\u00b7schau\u00b7ert", "lei\u00b7se", "des", "Par\u00b7kes"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gl\u00e4nzendes Laubgespro\u00df \u2013", "tokens": ["Gl\u00e4n\u00b7zen\u00b7des", "Laub\u00b7ge\u00b7spro\u00df", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wie tr\u00e4umende Seelenjugend", "tokens": ["Wie", "tr\u00e4u\u00b7men\u00b7de", "See\u00b7len\u00b7ju\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Im Kusse lichter Gedanken.", "tokens": ["Im", "Kus\u00b7se", "lich\u00b7ter", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00dcber den Wipfeln fern das Nachtgew\u00f6lk", "tokens": ["\u00dc\u00b7ber", "den", "Wip\u00b7feln", "fern", "das", "Nacht\u00b7ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Flammt bisweilen von Blitzen \u2013", "tokens": ["Flammt", "bis\u00b7wei\u00b7len", "von", "Blit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Dem dumpfen Schl\u00e4fer gleich,", "tokens": ["Dem", "dum\u00b7pfen", "Schl\u00e4\u00b7fer", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Den hei\u00dfe Leidenschaft", "tokens": ["Den", "hei\u00b7\u00dfe", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Zuckend r\u00fchrt.", "tokens": ["Zu\u00b7ckend", "r\u00fchrt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Aus B\u00fcschen und frischen Halmen", "tokens": ["Aus", "B\u00fc\u00b7schen", "und", "fri\u00b7schen", "Hal\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Atmet der s\u00fc\u00dfe Mai;", "tokens": ["At\u00b7met", "der", "s\u00fc\u00b7\u00dfe", "Mai", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "In lauschiger Bl\u00e4ttertiefe", "tokens": ["In", "lau\u00b7schi\u00b7ger", "Bl\u00e4t\u00b7ter\u00b7tie\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Dichtet tr\u00e4umend die Nachtigall;", "tokens": ["Dich\u00b7tet", "tr\u00e4u\u00b7mend", "die", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Und vom stolzen bleichen Hause", "tokens": ["Und", "vom", "stol\u00b7zen", "blei\u00b7chen", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "An des Parkes Saum", "tokens": ["An", "des", "Par\u00b7kes", "Saum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.18": {"text": "Aus erhellten Fenstern", "tokens": ["Aus", "er\u00b7hell\u00b7ten", "Fens\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Klingt Musik", "tokens": ["Klingt", "Mu\u00b7sik"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+--", "measure": "dactylic.init"}, "line.20": {"text": "Wie perlendes Gl\u00fcck.", "tokens": ["Wie", "per\u00b7len\u00b7des", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Im Garten aber am Eisengitter", "tokens": ["Im", "Gar\u00b7ten", "a\u00b7ber", "am", "Ei\u00b7sen\u00b7git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Steht ein schimmernder Bl\u00fctenbusch", "tokens": ["Steht", "ein", "schim\u00b7mern\u00b7der", "Bl\u00fc\u00b7ten\u00b7busch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Traurig \u00fcber die St\u00e4be geneigt;", "tokens": ["Trau\u00b7rig", "\u00fc\u00b7ber", "die", "St\u00e4\u00b7be", "ge\u00b7neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die wei\u00dfen Bl\u00fcten blicken", "tokens": ["Die", "wei\u00b7\u00dfen", "Bl\u00fc\u00b7ten", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie bange Kinderaugen", "tokens": ["Wie", "ban\u00b7ge", "Kin\u00b7der\u00b7au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf ein dunkles Menschenbild,", "tokens": ["Auf", "ein", "dunk\u00b7les", "Men\u00b7schen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das zu des Busches F\u00fc\u00dfen", "tokens": ["Das", "zu", "des", "Bu\u00b7sches", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Drau\u00dfen am Gittersockel", "tokens": ["Drau\u00b7\u00dfen", "am", "Git\u00b7ter\u00b7so\u00b7ckel"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Reglos kauert.", "tokens": ["Reg\u00b7los", "kau\u00b7ert", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Durch bebende Zweige f\u00e4llt", "tokens": ["Durch", "be\u00b7ben\u00b7de", "Zwei\u00b7ge", "f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zerrissenes Mondlicht", "tokens": ["Zer\u00b7ris\u00b7se\u00b7nes", "Mond\u00b7licht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und huscht mit Scheu", "tokens": ["Und", "huscht", "mit", "Scheu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "\u00dcber des kauernden Mannes", "tokens": ["\u00dc\u00b7ber", "des", "kau\u00b7ern\u00b7den", "Man\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "W\u00fcsten Rock und wirres Haar.", "tokens": ["W\u00fcs\u00b7ten", "Rock", "und", "wir\u00b7res", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seufzend streift vorbei der Nachtwind,", "tokens": ["Seuf\u00b7zend", "streift", "vor\u00b7bei", "der", "Nacht\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der wei\u00dfe Bl\u00fctenbusch", "tokens": ["Und", "der", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7ten\u00b7busch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sinnt in tr\u00e4umender Trauer:", "tokens": ["Sinnt", "in", "tr\u00e4u\u00b7men\u00b7der", "Trau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.4": {"line.1": {"text": "\u00bbarme Menschenbl\u00fcte,", "tokens": ["\u00bb", "ar\u00b7me", "Men\u00b7schen\u00b7bl\u00fc\u00b7te", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die du gefallen liegst,", "tokens": ["Die", "du", "ge\u00b7fal\u00b7len", "liegst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verloren f\u00fcr die Sonne,", "tokens": ["Ver\u00b7lo\u00b7ren", "f\u00fcr", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Angesicht verw\u00fcstet,", "tokens": ["Das", "An\u00b7ge\u00b7sicht", "ver\u00b7w\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf Stein und Staub!", "tokens": ["Auf", "Stein", "und", "Staub", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Welch liebeloser G\u00e4rtner", "tokens": ["Welch", "lie\u00b7be\u00b7lo\u00b7ser", "G\u00e4rt\u00b7ner"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lie\u00df so dich darben, d\u00fcrsten,", "tokens": ["Lie\u00df", "so", "dich", "dar\u00b7ben", ",", "d\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PAV", "$,", "VMFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Da\u00df du verwelkt, gesunken,", "tokens": ["Da\u00df", "du", "ver\u00b7welkt", ",", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zertreten bist in Staub und Stein?\u00ab", "tokens": ["Zer\u00b7tre\u00b7ten", "bist", "in", "Staub", "und", "Stein", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So sinnt in tr\u00e4umender Trauer", "tokens": ["So", "sinnt", "in", "tr\u00e4u\u00b7men\u00b7der", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der wei\u00dfe Bl\u00fctenbusch ...", "tokens": ["Der", "wei\u00b7\u00dfe", "Bl\u00fc\u00b7ten\u00b7busch", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Himmel aber flammt es", "tokens": ["Am", "Him\u00b7mel", "a\u00b7ber", "flammt", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und rollt und grollt,", "tokens": ["Und", "rollt", "und", "grollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Als r\u00fcsteten sich ferne Wetter", "tokens": ["Als", "r\u00fcs\u00b7te\u00b7ten", "sich", "fer\u00b7ne", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu hei\u00dfem Zorne.", "tokens": ["Zu", "hei\u00b7\u00dfem", "Zor\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Das zarte Mondlicht fl\u00fcchtet", "tokens": ["Das", "zar\u00b7te", "Mond\u00b7licht", "fl\u00fcch\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinter finster ragende Wolken,", "tokens": ["Hin\u00b7ter", "fins\u00b7ter", "ra\u00b7gen\u00b7de", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Und die Nachtigall verstummt ...", "tokens": ["Und", "die", "Nach\u00b7ti\u00b7gall", "ver\u00b7stummt", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur vom stolzen Hause", "tokens": ["Nur", "vom", "stol\u00b7zen", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "An des Parkes Saum", "tokens": ["An", "des", "Par\u00b7kes", "Saum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Aus erhellten Fenstern", "tokens": ["Aus", "er\u00b7hell\u00b7ten", "Fens\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Klingt Musik wie perlendes Gl\u00fcck.", "tokens": ["Klingt", "Mu\u00b7sik", "wie", "per\u00b7len\u00b7des", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Aus der Th\u00fcr des Hauses tritt", "tokens": ["Aus", "der", "Th\u00fcr", "des", "Hau\u00b7ses", "tritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Herr in feiner Tracht,", "tokens": ["Ein", "Herr", "in", "fei\u00b7ner", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gr\u00fc\u00dft zur\u00fcck", "tokens": ["Gr\u00fc\u00dft", "zu\u00b7r\u00fcck"], "token_info": ["word", "word"], "pos": ["VVFIN", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "\u00bbgute Nacht!\u00ab", "tokens": ["\u00bb", "gu\u00b7te", "Nacht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Und kommt gegangen,", "tokens": ["Und", "kommt", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Leise tr\u00e4llernd.", "tokens": ["Lei\u00b7se", "tr\u00e4l\u00b7lernd", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+---", "measure": "dactylic.init"}, "line.7": {"text": "Mit kaltem Blicke", "tokens": ["Mit", "kal\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Streift er die Gestalt am Gitter", "tokens": ["Streift", "er", "die", "Ge\u00b7stalt", "am", "Git\u00b7ter"], "token_info": ["wor