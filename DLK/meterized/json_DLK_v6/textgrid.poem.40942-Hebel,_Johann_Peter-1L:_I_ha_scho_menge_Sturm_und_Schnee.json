{"textgrid.poem.40942": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: I ha scho menge Sturm und Schnee", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "I ha scho menge Sturm und Schnee", "tokens": ["I", "ha", "scho", "men\u00b7ge", "Sturm", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN", "KON", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "i ha scho menge Fr\u00fcehlig gseh,", "tokens": ["i", "ha", "scho", "men\u00b7ge", "Fr\u00fceh\u00b7lig", "gseh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "und Chrieg und Elend \u00fcberal", "tokens": ["und", "Chrieg", "und", "E\u00b7lend", "\u00fc\u00b7be\u00b7ral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Rebland und im Wiesetal.", "tokens": ["im", "Re\u00b7bland", "und", "im", "Wie\u00b7se\u00b7tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An so ne Zit, wo alles singt", "tokens": ["An", "so", "ne", "Zit", ",", "wo", "al\u00b7les", "singt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und jung und alt in Freude springt,", "tokens": ["und", "jung", "und", "alt", "in", "Freu\u00b7de", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "an so ne Tag, wie Gott ein schenkt,", "tokens": ["an", "so", "ne", "Tag", ",", "wie", "Gott", "ein", "schenkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,", "PWAV", "NN", "ART", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "an so ne Freud het niemes denkt.", "tokens": ["an", "so", "ne", "Freud", "het", "nie\u00b7mes", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VAFIN", "PIS", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.9": {"text": "O w\u00e4r er do, o ch\u00f6nnt er's seh,", "tokens": ["O", "w\u00e4r", "er", "do", ",", "o", "ch\u00f6nnt", "er's", "seh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "der liebi F\u00fcrst, Gott het en ge!", "tokens": ["der", "lie\u00b7bi", "F\u00fcrst", ",", "Gott", "het", "en", "ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "VAFIN", "FM", "FM", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Er isch so gn\u00e4dig, isch so guet,", "tokens": ["Er", "isch", "so", "gn\u00e4\u00b7dig", ",", "isch", "so", "guet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "$,", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "'s wird Wohltat, was er denkt und tuet.", "tokens": ["'s", "wird", "Wohl\u00b7tat", ",", "was", "er", "denkt", "und", "tu\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "\u00bbdu, Gott im Himmel, sei sein Lohn,", "tokens": ["\u00bb", "du", ",", "Gott", "im", "Him\u00b7mel", ",", "sei", "sein", "Lohn", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NN", "APPRART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "und schirme seinen F\u00fcrstenthron.\u00ab", "tokens": ["und", "schir\u00b7me", "sei\u00b7nen", "F\u00fcrs\u00b7ten\u00b7thron", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Siehsch, Friederli, sel Engelsbild?", "tokens": ["Sieh\u00b7sch", ",", "Frie\u00b7der\u00b7li", ",", "sel", "En\u00b7gels\u00b7bild", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wie luegt's ein a so lieb und mild!", "tokens": ["Wie", "luegt's", "ein", "a", "so", "lieb", "und", "mild", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NE", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Es isch di F\u00fcrst, wo sorgt und wacht;", "tokens": ["Es", "isch", "di", "F\u00fcrst", ",", "wo", "sorgt", "und", "wacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NE", "NN", "$,", "PWAV", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "er het is alli gl\u00fccklich gmacht.", "tokens": ["er", "het", "is", "al\u00b7li", "gl\u00fcck\u00b7lich", "gmacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM", "FM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das lohnt em Gott, und uf si Hus", "tokens": ["Das", "lohnt", "em", "Gott", ",", "und", "uf", "si", "Hus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$,", "KON", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "gie\u00dft Gott si Huld und Segen us.", "tokens": ["gie\u00dft", "Gott", "si", "Huld", "und", "Se\u00b7gen", "us", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NE", "NE", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "O Chind, de bisch so jung und zart,", "tokens": ["O", "Chind", ",", "de", "bisch", "so", "jung", "und", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "und wenn di Lebe Gott bewahrt,", "tokens": ["und", "wenn", "di", "Le\u00b7be", "Gott", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "und bisch emol dim Vater glich,", "tokens": ["und", "bisch", "e\u00b7mol", "dim", "Va\u00b7ter", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "so wohnt di F\u00fcrst im Himmelrich,", "tokens": ["so", "wohnt", "di", "F\u00fcrst", "im", "Him\u00b7mel\u00b7rich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "und andere Zite ch\u00f6mme no.", "tokens": ["und", "an\u00b7de\u00b7re", "Zi\u00b7te", "ch\u00f6m\u00b7me", "no", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NE", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Doch blibt si Geist und Liebi do,", "tokens": ["Doch", "blibt", "si", "Geist", "und", "Lie\u00b7bi", "do", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "und tr\u00f6stet wieder treu und mild,", "tokens": ["und", "tr\u00f6s\u00b7tet", "wie\u00b7der", "treu", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "und segnet in sim Ebebild.", "tokens": ["und", "seg\u00b7net", "in", "sim", "E\u00b7be\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}