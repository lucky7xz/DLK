{"dta.poem.335": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine Grausahme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sie t\u00f6dtet mich mein Licht/ mein Schatz/ mein s\u00fcsses Leben/", "tokens": ["Sie", "t\u00f6d\u00b7tet", "mich", "mein", "Licht", "/", "mein", "Schatz", "/", "mein", "s\u00fcs\u00b7ses", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie machet da\u00df ich auf den matten Geist mu\u00df geben/", "tokens": ["Sie", "ma\u00b7chet", "da\u00df", "ich", "auf", "den", "mat\u00b7ten", "Geist", "mu\u00df", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Augen heisser Blitz/", "tokens": ["Der", "Au\u00b7gen", "heis\u00b7ser", "Blitz", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das kr&#367;fftige Gesch\u00fctz/", "tokens": ["Das", "kr", "&#367;", "ff\u00b7ti\u00b7ge", "Ge\u00b7sch\u00fctz", "/"], "token_info": ["word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Setzt mein verliebtes Hertz in solche Feuer-Gluhten", "tokens": ["Setzt", "mein", "ver\u00b7lieb\u00b7tes", "Hertz", "in", "sol\u00b7che", "Feu\u00b7er\u00b7Gluh\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die niemand l\u00f6schen mag mit allen Wasser-Fluhten.", "tokens": ["Die", "nie\u00b7mand", "l\u00f6\u00b7schen", "mag", "mit", "al\u00b7len", "Was\u00b7ser\u00b7Fluh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es gehet Flei\u00df und Schwei\u00df vergebens in den Wind", "tokens": ["Es", "ge\u00b7het", "Flei\u00df", "und", "Schwei\u00df", "ver\u00b7ge\u00b7bens", "in", "den", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So lange/ bis man sie auch bey der Rettung findt.", "tokens": ["So", "lan\u00b7ge", "/", "bis", "man", "sie", "auch", "bey", "der", "Ret\u00b7tung", "findt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie machet meinen Brand/ und will mich doch nicht retten/", "tokens": ["Sie", "ma\u00b7chet", "mei\u00b7nen", "Brand", "/", "und", "will", "mich", "doch", "nicht", "ret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie denckt zu gr\u00f6ssern Schmertz mich in die H\u00f6ll zu betten.", "tokens": ["Sie", "denckt", "zu", "gr\u00f6s\u00b7sern", "Schmertz", "mich", "in", "die", "H\u00f6ll", "zu", "bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie h\u00e4uffet Feur auf Feur", "tokens": ["Sie", "h\u00e4uf\u00b7fet", "Feur", "auf", "Feur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als dort das Ungeheur/", "tokens": ["Als", "dort", "das", "Un\u00b7ge\u00b7heur", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit welchem ", "tokens": ["Mit", "wel\u00b7chem"], "token_info": ["word", "word"], "pos": ["APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Doch dieser kunte noch desselben Flammen d\u00e4mpffen/", "tokens": ["Doch", "die\u00b7ser", "kun\u00b7te", "noch", "des\u00b7sel\u00b7ben", "Flam\u00b7men", "d\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er trug den Sieg davon/ ich aber stege nicht/", "tokens": ["Er", "trug", "den", "Sieg", "da\u00b7von", "/", "ich", "a\u00b7ber", "ste\u00b7ge", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "$(", "PPER", "ADV", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein K\u00e4mpffen hat noch nichts/ o Himmel ausgericht.", "tokens": ["Mein", "K\u00e4mpf\u00b7fen", "hat", "noch", "nichts", "/", "o", "Him\u00b7mel", "aus\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PIS", "$(", "FM", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich m\u00fche mich umsonst ihr Hertze zubesiegen/", "tokens": ["Ich", "m\u00fc\u00b7he", "mich", "um\u00b7sonst", "ihr", "Hert\u00b7ze", "zu\u00b7be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Unternehmen kan ich nicht zum Ende kriegen.", "tokens": ["Mein", "Un\u00b7ter\u00b7neh\u00b7men", "kan", "ich", "nicht", "zum", "En\u00b7de", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Streiten stellet mir", "tokens": ["Mein", "Strei\u00b7ten", "stel\u00b7let", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Danaiden f\u00fcr.", "tokens": ["Die", "Da\u00b7na\u00b7i\u00b7den", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn so wie diese nur in steter Arbeit schweben", "tokens": ["Denn", "so", "wie", "die\u00b7se", "nur", "in", "ste\u00b7ter", "Ar\u00b7beit", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "PDS", "ADV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in dem Siebe nicht das schwere Wasser heben/", "tokens": ["Und", "in", "dem", "Sie\u00b7be", "nicht", "das", "schwe\u00b7re", "Was\u00b7ser", "he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So schl\u00e4gt bey ihrer Brust auch mein Beginnen an/", "tokens": ["So", "schl\u00e4gt", "bey", "ih\u00b7rer", "Brust", "auch", "mein", "Be\u00b7gin\u00b7nen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich habe viel/ und doch noch nichtes nicht gethan.", "tokens": ["Ich", "ha\u00b7be", "viel", "/", "und", "doch", "noch", "nich\u00b7tes", "nicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "KON", "ADV", "ADV", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Geister sind schon halb aus meiner Brust verlohren/", "tokens": ["Die", "Geis\u00b7ter", "sind", "schon", "halb", "aus", "mei\u00b7ner", "Brust", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr Hertz\u2019 hat sich so gar auf meinen Fall verschworen.", "tokens": ["Ihr", "Hertz'", "hat", "sich", "so", "gar", "auf", "mei\u00b7nen", "Fall", "ver\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ohren sind verstopfft", "tokens": ["Die", "Oh\u00b7ren", "sind", "ver\u00b7stopfft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So das vergebens klopfft", "tokens": ["So", "das", "ver\u00b7ge\u00b7bens", "klopfft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bey ihrem Hertzen an mitleydiges Erbarmen/", "tokens": ["Bey", "ih\u00b7rem", "Hert\u00b7zen", "an", "mit\u00b7ley\u00b7di\u00b7ges", "Er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie stellt sich taub und hart vor einen krancken Armen/", "tokens": ["Sie", "stellt", "sich", "taub", "und", "hart", "vor", "ei\u00b7nen", "kran\u00b7cken", "Ar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr Hertz verkehret sich in Stahl und Marmor-Stein/", "tokens": ["Ihr", "Hertz", "ver\u00b7keh\u00b7ret", "sich", "in", "Stahl", "und", "Mar\u00b7mor\u00b7Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und will/ o Ungel\u00fcck! mir unbeweglich seyn.", "tokens": ["Und", "will", "/", "o", "Un\u00b7ge\u00b7l\u00fcck", "!", "mir", "un\u00b7be\u00b7weg\u00b7lich", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$(", "FM", "NN", "$.", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ach Unbeweglichste! ihr Hart-seyn l\u00e4\u00dft mich sterben/", "tokens": ["Ach", "Un\u00b7be\u00b7weg\u00b7lichs\u00b7te", "!", "ihr", "Har\u00b7tseyn", "l\u00e4\u00dft", "mich", "ster\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr weigern l\u00e4sset mich/ doch viel zu fr\u00fch/ verderben.", "tokens": ["Ihr", "wei\u00b7gern", "l\u00e4s\u00b7set", "mich", "/", "doch", "viel", "zu", "fr\u00fch", "/", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "$(", "ADV", "ADV", "PTKA", "ADJD", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Geist begehrt die Lufft", "tokens": ["Der", "Geist", "be\u00b7gehrt", "die", "Lufft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Leib verlangt die Grufft.", "tokens": ["Der", "Leib", "ver\u00b7langt", "die", "Grufft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und zeiget da\u00df sie mehr/ als diese/ grausahm ist", "tokens": ["Und", "zei\u00b7get", "da\u00df", "sie", "mehr", "/", "als", "die\u00b7se", "/", "grau\u00b7sahm", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PPER", "ADV", "$(", "KOUS", "PDAT", "$(", "VVFIN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da sonst die Grausamkeit sich mit der ", "tokens": ["Da", "sonst", "die", "Grau\u00b7sam\u00b7keit", "sich", "mit", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So mu\u00df ich nun also in steten Sterben leben/", "tokens": ["So", "mu\u00df", "ich", "nun", "al\u00b7so", "in", "ste\u00b7ten", "Ster\u00b7ben", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lebend allezeit den matten Geist auffgeben/", "tokens": ["Und", "le\u00b7bend", "al\u00b7le\u00b7zeit", "den", "mat\u00b7ten", "Geist", "auff\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lebe/ und bin todt", "tokens": ["Ich", "le\u00b7be", "/", "und", "bin", "todt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "KON", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Halb Wohl/ und halb in Noht.", "tokens": ["Halb", "Wohl", "/", "und", "halb", "in", "Noht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$(", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Noht/ wenn ich mich mu\u00df mit ihren Streuben qu\u00e4hlen", "tokens": ["In", "Noht", "/", "wenn", "ich", "mich", "mu\u00df", "mit", "ih\u00b7ren", "Streu\u00b7ben", "qu\u00e4h\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "KOUS", "PPER", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Wohl/ dieweil ich wei\u00df/ da\u00df sie mich wird entseelen.", "tokens": ["Und", "Wohl", "/", "die\u00b7weil", "ich", "wei\u00df", "/", "da\u00df", "sie", "mich", "wird", "ent\u00b7see\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "KOUS", "PPER", "VVFIN", "$(", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So liege ich im Grab\u2019 und bin auch in der Welt/", "tokens": ["So", "lie\u00b7ge", "ich", "im", "Grab'", "und", "bin", "auch", "in", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "KON", "VAFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Himmel weis wie schwehr mir dieser Jammer f\u00e4llt.", "tokens": ["Der", "Him\u00b7mel", "weis", "wie", "schwehr", "mir", "die\u00b7ser", "Jam\u00b7mer", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KOKOM", "ADV", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}