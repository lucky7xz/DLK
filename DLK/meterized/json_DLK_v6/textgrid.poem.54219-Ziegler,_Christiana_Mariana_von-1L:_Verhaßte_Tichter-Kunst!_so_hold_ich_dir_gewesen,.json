{"textgrid.poem.54219": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verha\u00dfte Tichter-Kunst! so hold ich dir gewesen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verha\u00dfte Tichter-Kunst! so hold ich dir gewesen,", "tokens": ["Ver\u00b7ha\u00df\u00b7te", "Tich\u00b7ter\u00b7Kunst", "!", "so", "hold", "ich", "dir", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "ADJD", "PPER", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gram und spinne-feind werd ich dir k\u00fcnfftig hin,", "tokens": ["So", "gram", "und", "spin\u00b7ne\u00b7feind", "werd", "ich", "dir", "k\u00fcnff\u00b7tig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du kanst den Unmuth leicht aus Aug und Minen lesen,", "tokens": ["Du", "kanst", "den", "Un\u00b7muth", "leicht", "aus", "Aug", "und", "Mi\u00b7nen", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und siehst, wie traurig ich und mi\u00dfvergn\u00fcget bin.", "tokens": ["Und", "siehst", ",", "wie", "trau\u00b7rig", "ich", "und", "mi\u00df\u00b7ver\u00b7gn\u00fc\u00b7get", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "KON", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht frage nach den Quell und Ursprung meiner Schmertzen;", "tokens": ["Nicht", "fra\u00b7ge", "nach", "den", "Quell", "und", "Ur\u00b7sprung", "mei\u00b7ner", "Schmert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "K\u00fc\u00dft ich deswegen dich, da\u00df ich nun mit Verdru\u00df,", "tokens": ["K\u00fc\u00dft", "ich", "des\u00b7we\u00b7gen", "dich", ",", "da\u00df", "ich", "nun", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Derjenigen, die mir so tieff in meinen Hertzen,", "tokens": ["Der\u00b7je\u00b7ni\u00b7gen", ",", "die", "mir", "so", "tieff", "in", "mei\u00b7nen", "Hert\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als in dem Sinne, sa\u00df, ein Grab-Lied singen mu\u00df?", "tokens": ["Als", "in", "dem", "Sin\u00b7ne", ",", "sa\u00df", ",", "ein", "Grab\u00b7Lied", "sin\u00b7gen", "mu\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "VVFIN", "$,", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Erbla\u00dfte Bre\u00dflerin, mein Engel, mein Vergn\u00fcgen,", "tokens": ["Er\u00b7bla\u00df\u00b7te", "Bre\u00df\u00b7le\u00b7rin", ",", "mein", "En\u00b7gel", ",", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ists m\u00f6glich? da\u00df ein Brieff mit Schrecken mir entdeckt,", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "da\u00df", "ein", "Brieff", "mit", "Schre\u00b7cken", "mir", "ent\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "KOUS", "ART", "NN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Budorgis s\u00e4he dich auf Toden-Brettern liegen,", "tokens": ["Bu\u00b7dor\u00b7gis", "s\u00e4\u00b7he", "dich", "auf", "To\u00b7den\u00b7Bret\u00b7tern", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Worauf das Schicksal dich bethaurens-werth gestreckt;", "tokens": ["Wo\u00b7rauf", "das", "Schick\u00b7sal", "dich", "be\u00b7thau\u00b7rens\u00b7werth", "ge\u00b7streckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach Freundin, brichst du schon so fr\u00fch den Bund der Liebe?", "tokens": ["Ach", "Freun\u00b7din", ",", "brichst", "du", "schon", "so", "fr\u00fch", "den", "Bund", "der", "Lie\u00b7be", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den unsre Freundschafft erst vor kurtzen stifften hie\u00df,", "tokens": ["Den", "uns\u00b7re", "Freund\u00b7schafft", "erst", "vor", "kurt\u00b7zen", "stiff\u00b7ten", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo bleibt die Schwesterschafft, und die so zarten Triebe,", "tokens": ["Wo", "bleibt", "die", "Schwes\u00b7ter\u00b7schafft", ",", "und", "die", "so", "zar\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die deine Zuschrifft mir in allen Bl\u00e4ttern wie\u00df?", "tokens": ["Die", "dei\u00b7ne", "Zu\u00b7schrifft", "mir", "in", "al\u00b7len", "Bl\u00e4t\u00b7tern", "wie\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie freudig griff ich sonst nach meinen Spiel der Sayten,", "tokens": ["Wie", "freu\u00b7dig", "griff", "ich", "sonst", "nach", "mei\u00b7nen", "Spiel", "der", "Say\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wiewohl es auch zugleich mit Schaam und Furcht geschah,", "tokens": ["Wie\u00b7wohl", "es", "auch", "zu\u00b7gleich", "mit", "Schaam", "und", "Furcht", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So offt mein Ph\u00f6bus mich, dich hie\u00df darmit begleiten,", "tokens": ["So", "offt", "mein", "Ph\u00f6\u00b7bus", "mich", ",", "dich", "hie\u00df", "dar\u00b7mit", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PPER", "$,", "PPER", "VVFIN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und ich von deiner Hand ein Freundschaffts-Zeichen sah.", "tokens": ["Und", "ich", "von", "dei\u00b7ner", "Hand", "ein", "Freund\u00b7schaffts\u00b7Zei\u00b7chen", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch da dein Fall mich hei\u00dft ein Toden-Opffer bringen,", "tokens": ["Doch", "da", "dein", "Fall", "mich", "hei\u00dft", "ein", "To\u00b7den\u00b7Opf\u00b7fer", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So f\u00e4llt die Feder mir vor Wehmuth aus der Hand,", "tokens": ["So", "f\u00e4llt", "die", "Fe\u00b7der", "mir", "vor", "Weh\u00b7muth", "aus", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die heischre Muse wei\u00df vor Thr\u00e4nen nichts zu singen,", "tokens": ["Die", "heischre", "Mu\u00b7se", "wei\u00df", "vor", "Thr\u00e4\u00b7nen", "nichts", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.24": {"text": "Als di\u00df: Mir wird durch dich was treffliches entwandt.", "tokens": ["Als", "di\u00df", ":", "Mir", "wird", "durch", "dich", "was", "treff\u00b7li\u00b7ches", "ent\u00b7wandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$.", "PPER", "VAFIN", "APPR", "PPER", "PWS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das Gl\u00fccke lie\u00df mir zwar dein Antlitz niemahls lachen,", "tokens": ["Das", "Gl\u00fc\u00b7cke", "lie\u00df", "mir", "zwar", "dein", "Ant\u00b7litz", "nie\u00b7mahls", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn die Entfernung lie\u00df es leider nicht geschehn,", "tokens": ["Denn", "die", "Ent\u00b7fer\u00b7nung", "lie\u00df", "es", "lei\u00b7der", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch kont ich mir von dir ein solches Bildni\u00df machen,", "tokens": ["Doch", "kont", "ich", "mir", "von", "dir", "ein", "sol\u00b7ches", "Bild\u00b7ni\u00df", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "PPER", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das dir mehr als so gleich und \u00e4hnlich hat gesehn.", "tokens": ["Das", "dir", "mehr", "als", "so", "gleich", "und", "\u00e4hn\u00b7lich", "hat", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "KOKOM", "ADV", "ADV", "KON", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ach Freundin, der Verlust steht gar nicht auszusprechen;", "tokens": ["Ach", "Freun\u00b7din", ",", "der", "Ver\u00b7lust", "steht", "gar", "nicht", "aus\u00b7zu\u00b7spre\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Raub ist allzu gro\u00df, der mich anjetzt betrifft;", "tokens": ["Der", "Raub", "ist", "all\u00b7zu", "gro\u00df", ",", "der", "mich", "an\u00b7jetzt", "be\u00b7tr\u00b7ifft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Die Mi\u00dfgunst konte sich an mir nicht \u00e4rger r\u00e4chen,", "tokens": ["Die", "Mi\u00df\u00b7gunst", "kon\u00b7te", "sich", "an", "mir", "nicht", "\u00e4r\u00b7ger", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Als da\u00df sie zwischen uns den Scheide-Brief gestifft.", "tokens": ["Als", "da\u00df", "sie", "zwi\u00b7schen", "uns", "den", "Schei\u00b7de\u00b7Brief", "ge\u00b7stifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich kannte deine Treu und ungef\u00e4rbtes Wesen,", "tokens": ["Ich", "kann\u00b7te", "dei\u00b7ne", "Treu", "und", "un\u00b7ge\u00b7f\u00e4rb\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das, d\u00fcnckt mich, heut zu Tag ein rarer Ph\u00f6nix hei\u00dft,", "tokens": ["Das", ",", "d\u00fcnckt", "mich", ",", "heut", "zu", "Tag", "ein", "ra\u00b7rer", "Ph\u00f6\u00b7nix", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$,", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Kein einges W\u00f6rtgen lie\u00df mir was geschmincktes lesen,", "tokens": ["Kein", "ein\u00b7ges", "W\u00f6rt\u00b7gen", "lie\u00df", "mir", "was", "ge\u00b7schminck\u00b7tes", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "PIS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dergleichen F\u00fcrni\u00df sonst die Welt gar h\u00e4uffig wei\u00dft.", "tokens": ["Derg\u00b7lei\u00b7chen", "F\u00fcr\u00b7ni\u00df", "sonst", "die", "Welt", "gar", "h\u00e4uf\u00b7fig", "wei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und eben dieses macht, da\u00df mir dein fr\u00fches Scheiden", "tokens": ["Und", "e\u00b7ben", "die\u00b7ses", "macht", ",", "da\u00df", "mir", "dein", "fr\u00fc\u00b7hes", "Schei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDS", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ein St\u00fcck vom Hertzen rei\u00dft, und in Verwirrung setzt;", "tokens": ["Ein", "St\u00fcck", "vom", "Hert\u00b7zen", "rei\u00dft", ",", "und", "in", "Ver\u00b7wir\u00b7rung", "setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Du weist nicht, was darbey mu\u00df meine Seele leiden,", "tokens": ["Du", "weist", "nicht", ",", "was", "dar\u00b7bey", "mu\u00df", "mei\u00b7ne", "See\u00b7le", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PAV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die gantz besonders dich vor andern hochgesch\u00e4tzt;", "tokens": ["Die", "gantz", "be\u00b7son\u00b7ders", "dich", "vor", "an\u00b7dern", "hoch\u00b7ge\u00b7sch\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PRF", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Doch h\u00f6rst du nicht allein mich deine Flucht beklagen,", "tokens": ["Doch", "h\u00f6rst", "du", "nicht", "al\u00b7lein", "mich", "dei\u00b7ne", "Flucht", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Es singt gantz Schlesien mit mir ein Klage-Lied;", "tokens": ["Es", "singt", "gantz", "Schle\u00b7si\u00b7en", "mit", "mir", "ein", "Kla\u00b7ge\u00b7Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Dein Breslau kan nunmehr am allerbesten sagen,", "tokens": ["Dein", "Bres\u00b7lau", "kan", "nun\u00b7mehr", "am", "al\u00b7ler\u00b7bes\u00b7ten", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Was durch die Bre\u00dflerin das Schicksal ihm entzieht.", "tokens": ["Was", "durch", "die", "Bre\u00df\u00b7le\u00b7rin", "das", "Schick\u00b7sal", "ihm", "ent\u00b7zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die Welt bewunderte dein Grund-gelehrtes Wissen,", "tokens": ["Die", "Welt", "be\u00b7wun\u00b7der\u00b7te", "dein", "Grun\u00b7dge\u00b7lehr\u00b7tes", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und deine Treflichkeit war weit und breit bekant;", "tokens": ["Und", "dei\u00b7ne", "Tref\u00b7lich\u00b7keit", "war", "weit", "und", "breit", "be\u00b7kant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Man wird nach sp\u00e4ther Zeit noch deine Lieder k\u00fcssen,", "tokens": ["Man", "wird", "nach", "sp\u00e4t\u00b7her", "Zeit", "noch", "dei\u00b7ne", "Lie\u00b7der", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Sie zeigen Majest\u00e4t, Geist, Feuer und Verstand.", "tokens": ["Sie", "zei\u00b7gen", "Ma\u00b7jes\u00b7t\u00e4t", ",", "Geist", ",", "Feu\u00b7er", "und", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "O Schade, da\u00df die Hand dem Moder soll ver