 ["ART", "NN", "VMFIN", "PIS", "APPR", "PPER", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Der Kern verbleibt der Welt in deiner Tugend Lauff,", "tokens": ["Der", "Kern", "ver\u00b7bleibt", "der", "Welt", "in", "dei\u00b7ner", "Tu\u00b7gend", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Mnemosyne l\u00e4st nicht dein Angedencken sterben,", "tokens": ["Mne\u00b7mo\u00b7sy\u00b7ne", "l\u00e4st", "nicht", "dein", "An\u00b7ge\u00b7den\u00b7cken", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.56": {"text": "Und Pallas h\u00e4ngt von dir den Kiel im Tempel auf.", "tokens": ["Und", "Pal\u00b7las", "h\u00e4ngt", "von", "dir", "den", "Kiel", "im", "Tem\u00b7pel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "ART", "NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}