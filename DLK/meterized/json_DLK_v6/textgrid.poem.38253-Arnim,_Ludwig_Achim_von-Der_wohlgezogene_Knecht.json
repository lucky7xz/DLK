{"textgrid.poem.38253": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der wohlgezogene Knecht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gieng ein wohlgezogner Knecht", "tokens": ["Es", "gieng", "ein", "wohl\u00b7ge\u00b7zog\u00b7ner", "Knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u00fcber die breite Aue,", "tokens": ["Wohl", "\u00fc\u00b7ber", "die", "brei\u00b7te", "Au\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da sah er einen sch\u00f6nen Tanz", "tokens": ["Da", "sah", "er", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von adlichen Jungfrauen;", "tokens": ["Von", "ad\u00b7li\u00b7chen", "Jung\u00b7frau\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Tanz, den wollt er schauen.", "tokens": ["Den", "Tanz", ",", "den", "wollt", "er", "schau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sprach der wohlerzogne Knecht:", "tokens": ["Da", "sprach", "der", "woh\u00b7ler\u00b7zog\u00b7ne", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbgott gr\u00fc\u00df euch Jungfraun alle!\u00ab", "tokens": ["\u00bb", "gott", "gr\u00fc\u00df", "euch", "Jung\u00b7fraun", "al\u00b7le", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ADV", "PIS", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da sprach das Fr\u00e4ulein Rosenthal:", "tokens": ["Da", "sprach", "das", "Fr\u00e4u\u00b7lein", "Ro\u00b7sen\u00b7thal", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbda\u00df dir ein Ohr abfalle,", "tokens": ["\u00bb", "da\u00df", "dir", "ein", "Ohr", "ab\u00b7fal\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Eh ich dir wohlgefalle.\u00ab", "tokens": ["Eh", "ich", "dir", "wohl\u00b7ge\u00b7fal\u00b7le", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da sprach der wohlerzogne Knecht:", "tokens": ["Da", "sprach", "der", "woh\u00b7ler\u00b7zog\u00b7ne", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbihr seyd ein grobe Maide.\u00ab", "tokens": ["\u00bb", "ihr", "seyd", "ein", "gro\u00b7be", "Mai\u00b7de", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", 