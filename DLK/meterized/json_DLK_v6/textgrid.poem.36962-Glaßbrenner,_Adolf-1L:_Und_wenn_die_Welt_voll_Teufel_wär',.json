{"textgrid.poem.36962": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und wenn die Welt voll Teufel w\u00e4r',", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn die Welt voll Teufel w\u00e4r',", "tokens": ["Und", "wenn", "die", "Welt", "voll", "Teu\u00b7fel", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir w\u00fcrden sie besiegen!", "tokens": ["Wir", "w\u00fcr\u00b7den", "sie", "be\u00b7sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir w\u00fcrden sie beim langen Schweif", "tokens": ["Wir", "w\u00fcr\u00b7den", "sie", "beim", "lan\u00b7gen", "Schweif"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bei den H\u00f6rnern kriegen.", "tokens": ["Und", "bei", "den", "H\u00f6r\u00b7nern", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie sollten unsre Sinne nun", "tokens": ["Sie", "soll\u00b7ten", "uns\u00b7re", "Sin\u00b7ne", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nimmermehr beth\u00f6ren;", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", "be\u00b7th\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir schmei\u00dfen in die H\u00f6lle sie,", "tokens": ["Wir", "schmei\u00b7\u00dfen", "in", "die", "H\u00f6l\u00b7le", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da, wo sie hingeh\u00f6ren!", "tokens": ["Da", ",", "wo", "sie", "hin\u00b7ge\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}