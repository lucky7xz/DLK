{"textgrid.poem.64176": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Richard und Sir Hugh", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnun zieh' ich ins gelobte Land, der heil'ge Christ hat Not,", "tokens": ["\u00bb", "nun", "zieh'", "ich", "ins", "ge\u00b7lob\u00b7te", "Land", ",", "der", "heil'\u00b7ge", "Christ", "hat", "Not", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Jetzt helf' ich ihm mit ", "tokens": ["Jetzt", "hel\u00b7f'", "ich", "ihm", "mit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dir, Sir Hugh, empfehl' ich all' mein Volk und was es hat,", "tokens": ["Und", "dir", ",", "Sir", "Hugh", ",", "emp\u00b7fehl'", "ich", "all'", "mein", "Volk", "und", "was", "es", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "FM", "$,", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Schlo\u00df Dover, meines Reiches Wall, und London, meine Stadt.", "tokens": ["Schlo\u00df", "Do\u00b7ver", ",", "mei\u00b7nes", "Rei\u00b7ches", "Wall", ",", "und", "Lon\u00b7don", ",", "mei\u00b7ne", "Stadt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "NE", "$,", "KON", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Ich kenne dich von edlem Mut: ich wei\u00df, treu wahrest du", "tokens": ["Ich", "ken\u00b7ne", "dich", "von", "ed\u00b7lem", "Mut", ":", "ich", "wei\u00df", ",", "treu", "wah\u00b7rest", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Noch treuer als dein h\u00f6chstes Gut mein K\u00f6nigsrecht, Sir Hugh.", "tokens": ["Noch", "treu\u00b7er", "als", "dein", "h\u00f6chs\u00b7tes", "Gut", "mein", "K\u00f6\u00b7nigs\u00b7recht", ",", "Sir", "Hugh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Mein Vetter Frankreich ist ein Schelm, mein Bruder John dazu:", "tokens": ["Mein", "Vet\u00b7ter", "Fran\u00b7kreich", "ist", "ein", "Schelm", ",", "mein", "Bru\u00b7der", "John", "da\u00b7zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VAFIN", "ART", "NN", "$,", "PPOSAT", "NN", "NE", "PAV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Sei du Altenglands Schild und Helm an meiner Statt, Sir Hugh.\u00ab", "tokens": ["Sei", "du", "Al\u00b7ten\u00b7glands", "Schild", "und", "Helm", "an", "mei\u00b7ner", "Statt", ",", "Sir", "Hugh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NN", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,", "NN", "NE", "$.", "$("], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig Richard sprach's und stieg an Bord mit seinem Heer:", "tokens": ["Der", "K\u00f6\u00b7nig", "Ric\u00b7hard", "sprach's", "und", "stieg", "an", "Bord", "mit", "sei\u00b7nem", "Heer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "KON", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "In seinen Fahnen flog der Sieg und Schreck zog vor ihm her;", "tokens": ["In", "sei\u00b7nen", "Fah\u00b7nen", "flog", "der", "Sieg", "und", "Schreck", "zog", "vor", "ihm", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Vorauf dem Kreuzheer stritt der Held und hell erklang wie Erz", "tokens": ["Vor\u00b7auf", "dem", "Kreuz\u00b7heer", "stritt", "der", "Held", "und", "hell", "er\u00b7klang", "wie", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "KON", "ADJD", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Durch Christenland und Heidenwelt der Name: L\u00f6wenherz.", "tokens": ["Durch", "Chris\u00b7ten\u00b7land", "und", "Hei\u00b7den\u00b7welt", "der", "Na\u00b7me", ":", "L\u00f6\u00b7wen\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Sir Hugh indes des Rechtes pflag und hielt das Reich in acht:", "tokens": ["Sir", "Hugh", "in\u00b7des", "des", "Rech\u00b7tes", "pflag", "und", "hielt", "das", "Reich", "in", "acht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Dem Staat geh\u00f6rt' der laute Tag, der Lieb' die stille Nacht.", "tokens": ["Dem", "Staat", "ge\u00b7h\u00f6rt'", "der", "lau\u00b7te", "Tag", ",", "der", "Lieb'", "die", "stil\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Denn einst, als er zu angeln ging am Severn blau und breit,", "tokens": ["Denn", "einst", ",", "als", "er", "zu", "an\u00b7geln", "ging", "am", "Se\u00b7vern", "blau", "und", "breit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Sir Hugh als s\u00fc\u00dfe Beute fing die allersch\u00f6nste Maid.", "tokens": ["Sir", "Hugh", "als", "s\u00fc\u00b7\u00dfe", "Beu\u00b7te", "fing", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KOUS", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Das war das junge Fischerkind, nicht sechzehn Winter alt,", "tokens": ["Das", "war", "das", "jun\u00b7ge", "Fi\u00b7scher\u00b7kind", ",", "nicht", "sech\u00b7zehn", "Win\u00b7ter", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PTKNEG", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ihr golden Haar so seidenlind, so wonnig die Gestalt;", "tokens": ["Ihr", "gol\u00b7den", "Haar", "so", "sei\u00b7den\u00b7lind", ",", "so", "won\u00b7nig", "die", "Ge\u00b7stalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "$,", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "In gr\u00fcner Einsamkeit erbl\u00fcht, gleichwie die Wasserros',", "tokens": ["In", "gr\u00fc\u00b7ner", "Ein\u00b7sam\u00b7keit", "er\u00b7bl\u00fcht", ",", "gleich\u00b7wie", "die", "Was\u00b7ser\u00b7ros'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Die an dem Rand des Severn gl\u00fcht, von Schilf versteckt und Moos.", "tokens": ["Die", "an", "dem", "Rand", "des", "Se\u00b7vern", "gl\u00fcht", ",", "von", "Schilf", "ver\u00b7steckt", "und", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Manch' goldnen Abend fuhren sie, wann s\u00fc\u00df der H\u00e4nfling sang,", "tokens": ["Man\u00b7ch'", "gold\u00b7nen", "A\u00b7bend", "fuh\u00b7ren", "sie", ",", "wann", "s\u00fc\u00df", "der", "H\u00e4nf\u00b7ling", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wohl Mund an Mund und Knie an Knie den stillen Strom entlang.", "tokens": ["Wohl", "Mund", "an", "Mund", "und", "Knie", "an", "Knie", "den", "stil\u00b7len", "Strom", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "O waldumfriedet Glostershire, du erlengr\u00fcnes Land,", "tokens": ["O", "wal\u00b7dum\u00b7frie\u00b7det", "Glos\u00b7ter\u00b7shi\u00b7re", ",", "du", "er\u00b7len\u00b7gr\u00fc\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Welch' stille Freuden schautet ihr, ihr Buchten an dem Strand!", "tokens": ["Welch'", "stil\u00b7le", "Freu\u00b7den", "schau\u00b7tet", "ihr", ",", "ihr", "Buch\u00b7ten", "an", "dem", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Das Ruder ruht, \u2013 sie treiben leis, \u2013 vorauf der wilde Schwan \u2013", "tokens": ["Das", "Ru\u00b7der", "ruht", ",", "\u2013", "sie", "trei\u00b7ben", "leis", ",", "\u2013", "vor\u00b7auf", "der", "wil\u00b7de", "Schwan", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$(", "PPER", "VVFIN", "ADJD", "$,", "$(", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Und Bl\u00fcten streuet rot und wei\u00df der Maiwind in den Kahn.", "tokens": ["Und", "Bl\u00fc\u00b7ten", "streu\u00b7et", "rot", "und", "wei\u00df", "der", "Mai\u00b7wind", "in", "den", "Kahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Seit Monden ruht der flinke Kahn, umsonst der Vogel schl\u00e4gt,", "tokens": ["Seit", "Mon\u00b7den", "ruht", "der", "flin\u00b7ke", "Kahn", ",", "um\u00b7sonst", "der", "Vo\u00b7gel", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Kein Liebespaar auf blauer Bahn der stille Severn tr\u00e4gt:", "tokens": ["Kein", "Lie\u00b7be\u00b7spaar", "auf", "blau\u00b7er", "Bahn", "der", "stil\u00b7le", "Se\u00b7vern", "tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Sir Hugh zog aus mit Mann und Ro\u00df f\u00fcr K\u00f6nig Richards Thron,", "tokens": ["Sir", "Hugh", "zog", "aus", "mit", "Mann", "und", "Ro\u00df", "f\u00fcr", "K\u00f6\u00b7nig", "Ric\u00b7hards", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "APPR", "NN", "KON", "NN", "APPR", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Denn Frankreich griff nach Dover-Schlo\u00df, nach London griff Prinz John.", "tokens": ["Denn", "Fran\u00b7kreich", "griff", "nach", "Do\u00b7ver\u00b7Schlo\u00df", ",", "nach", "Lon\u00b7don", "griff", "Prinz", "John", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "NE", "$,", "APPR", "NE", "VVFIN", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Und manchen Tag stand er im Feld, es wuchs und wuchs der Feind,", "tokens": ["Und", "man\u00b7chen", "Tag", "stand", "er", "im", "Feld", ",", "es", "wuchs", "und", "wuchs", "der", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Schon vor dem Tor von London h\u00e4lt er seine Macht vereint.", "tokens": ["Schon", "vor", "dem", "Tor", "von", "Lon\u00b7don", "h\u00e4lt", "er", "sei\u00b7ne", "Macht", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und morgen will in blut'ger Schlacht Sir Hugh die Stadt befrein,", "tokens": ["Und", "mor\u00b7gen", "will", "in", "blut'\u00b7ger", "Schlacht", "Sir", "Hugh", "die", "Stadt", "be\u00b7fr\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "APPR", "ADJA", "NN", "NN", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Da st\u00fcrzt ins Zelt bei tiefer Nacht sein treuster Knapp' herein:", "tokens": ["Da", "st\u00fcrzt", "ins", "Zelt", "bei", "tie\u00b7fer", "Nacht", "sein", "treus\u00b7ter", "Knapp'", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.10": {"line.1": {"text": "\u00bbdu bist betrogen! folge mir nach Haus, Sir Hugh, nach Haus!", "tokens": ["\u00bb", "du", "bist", "be\u00b7tro\u00b7gen", "!", "fol\u00b7ge", "mir", "nach", "Haus", ",", "Sir", "Hugh", ",", "nach", "Haus", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$.", "VVFIN", "PPER", "APPR", "NN", "$,", "FM", "FM", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Du k\u00e4mpfst f\u00fcr K\u00f6nig Richard hier, vieltreuer Mann, den Strau\u00df:", "tokens": ["Du", "k\u00e4mpfst", "f\u00fcr", "K\u00f6\u00b7nig", "Ric\u00b7hard", "hier", ",", "viel\u00b7treu\u00b7er", "Mann", ",", "den", "Strau\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NE", "ADV", "$,", "ADJA", "NN", "$,", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und K\u00f6nig Richard ist zur\u00fcck, und stiehlt dir wie ein Dieb", "tokens": ["Und", "K\u00f6\u00b7nig", "Ric\u00b7hard", "ist", "zu\u00b7r\u00fcck", ",", "und", "stiehlt", "dir", "wie", "ein", "Dieb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NE", "VAFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Im Wald von Glostershire dein Gl\u00fcck und herzt und kos't dein Lieb.", "tokens": ["Im", "Wald", "von", "Glos\u00b7ter\u00b7shi\u00b7re", "dein", "Gl\u00fcck", "und", "herzt", "und", "kos't", "dein", "Lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "PPOSAT", "NN", "KON", "VVPP", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}, "stanza.11": {"line.1": {"text": "Sie sitzt auf seinem Scho\u00df in Ruh', \u2013 oft k\u00fc\u00dft er ihren Mund,", "tokens": ["Sie", "sitzt", "auf", "sei\u00b7nem", "Scho\u00df", "in", "Ruh'", ",", "\u2013", "oft", "k\u00fc\u00dft", "er", "ih\u00b7ren", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "NN", "$,", "$(", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ich hab's gesehen \u2013 ich schw\u00f6r' dir's zu \u2013 zur Rache fort, zur Stund'!\u00ab", "tokens": ["Ich", "hab's", "ge\u00b7se\u00b7hen", "\u2013", "ich", "schw\u00f6r'", "dir's", "zu", "\u2013", "zur", "Ra\u00b7che", "fort", ",", "zur", "Stund'", "!", "\u00ab"], "token_info": ["word", 