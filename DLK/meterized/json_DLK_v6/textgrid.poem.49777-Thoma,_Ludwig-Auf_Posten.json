{"textgrid.poem.49777": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf Posten", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es prangen in den Stra\u00dfen", "tokens": ["Es", "pran\u00b7gen", "in", "den", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Reichen auf und ab,", "tokens": ["Die", "Rei\u00b7chen", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das mu\u00df mich denken lassen,", "tokens": ["Das", "mu\u00df", "mich", "den\u00b7ken", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich kein Geld nicht hab'.", "tokens": ["Da\u00df", "ich", "kein", "Geld", "nicht", "hab'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die M\u00e4dchen promenieren", "tokens": ["Die", "M\u00e4d\u00b7chen", "pro\u00b7me\u00b7nie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich stolz an mir vorbei,", "tokens": ["Sich", "stolz", "an", "mir", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da mu\u00df ich es versp\u00fcren,", "tokens": ["Da", "mu\u00df", "ich", "es", "ver\u00b7sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ich alleine sei.", "tokens": ["Wie", "ich", "al\u00b7lei\u00b7ne", "sei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich m\u00f6chte, Mond und Sterne", "tokens": ["Ich", "m\u00f6ch\u00b7te", ",", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00e4r lauter bares Geld,", "tokens": ["W\u00e4r", "lau\u00b7ter", "ba\u00b7res", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das h\u00e4tt' ich wohl so gerne", "tokens": ["Das", "h\u00e4tt'", "ich", "wohl", "so", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00e4r ein feiner Held.", "tokens": ["Und", "w\u00e4r", "ein", "fei\u00b7ner", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Gl\u00fcck mu\u00df andern winken,", "tokens": ["Das", "Gl\u00fcck", "mu\u00df", "an\u00b7dern", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommt aber nicht zu mir,", "tokens": ["Kommt", "a\u00b7ber", "nicht", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Geld nicht zum Vertrinken,", "tokens": ["Kein", "Geld", "nicht", "zum", "Ver\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein M\u00e4dchen zum Pl\u00e4sier.", "tokens": ["Kein", "M\u00e4d\u00b7chen", "zum", "Pl\u00e4\u00b7sier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}