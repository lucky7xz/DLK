{"dta.poem.534": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die falsche  Lysilis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.57", "fr:0.28", "en:0.14"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So meynet Lysilis, da\u00df ich des Todes sey", "tokens": ["So", "mey\u00b7net", "Ly\u00b7si\u00b7lis", ",", "da\u00df", "ich", "des", "To\u00b7des", "sey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihre Gunst mir fehlt/ und sie mir ungetreu?", "tokens": ["Wenn", "ih\u00b7re", "Gunst", "mir", "fehlt", "/", "und", "sie", "mir", "un\u00b7ge\u00b7treu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$(", "KON", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So meynt sie/ da\u00df mein Geist vor Uberdru\u00df verschmachtet/", "tokens": ["So", "meynt", "sie", "/", "da\u00df", "mein", "Geist", "vor", "U\u00b7berd\u00b7ru\u00df", "ver\u00b7schmach\u00b7tet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn sie mich h\u00f6nisch schillt/ und ohne Schuld verachtet?", "tokens": ["Wenn", "sie", "mich", "h\u00f6\u00b7nisch", "schillt", "/", "und", "oh\u00b7ne", "Schuld", "ver\u00b7ach\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$(", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach Nein! die Meynung triegt/ ich bin noch unverzagt/", "tokens": ["Ach", "Nein", "!", "die", "Mey\u00b7nung", "triegt", "/", "ich", "bin", "noch", "un\u00b7ver\u00b7zagt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PTKANT", "$.", "ART", "NN", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich f\u00fchle nicht/ da\u00df mich um sie ein Kummer plagt/", "tokens": ["Ich", "f\u00fch\u00b7le", "nicht", "/", "da\u00df", "mich", "um", "sie", "ein", "Kum\u00b7mer", "plagt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will ihr gleich meine Waar aus Hochmuth nicht gefallen?", "tokens": ["Will", "ihr", "gleich", "mei\u00b7ne", "Waar", "aus", "Hoch\u00b7muth", "nicht", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie wird darum doch nicht verkleineret von allen.", "tokens": ["Sie", "wird", "da\u00b7rum", "doch", "nicht", "ver\u00b7klei\u00b7ne\u00b7ret", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "PTKNEG", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Will sie nicht K\u00e4uffer seyn/ so sind derselben mehr", "tokens": ["Will", "sie", "nicht", "K\u00e4uf\u00b7fer", "seyn", "/", "so", "sind", "der\u00b7sel\u00b7ben", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "NN", "VAINF", "$(", "ADV", "VAFIN", "PDAT", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie reissen sich um sie ja auch nicht allzusehr.", "tokens": ["Sie", "reis\u00b7sen", "sich", "um", "sie", "ja", "auch", "nicht", "all\u00b7zu\u00b7sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ADV", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn sie nicht bessern Stoff vor ihren Laden leget", "tokens": ["Wenn", "sie", "nicht", "bes\u00b7sern", "Stoff", "vor", "ih\u00b7ren", "La\u00b7den", "le\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Alsdenn sie keinen Mann in ihrer Buden heget.", "tokens": ["Als\u00b7denn", "sie", "kei\u00b7nen", "Mann", "in", "ih\u00b7rer", "Bu\u00b7den", "he\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Fahr hin du bist verlacht du dunckles Augen-Licht/", "tokens": ["Fahr", "hin", "du", "bist", "ver\u00b7lacht", "du", "dunck\u00b7les", "Au\u00b7gen\u00b7Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VAFIN", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dein Spiegel schmeichelt sehr/ und gibt dir falsch Bericht/", "tokens": ["Dein", "Spie\u00b7gel", "schmei\u00b7chelt", "sehr", "/", "und", "gibt", "dir", "falsch", "Be\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "KON", "VVFIN", "PPER", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie meynt/ sie w\u00e4re zart/ ", "tokens": ["Sie", "meynt", "/", "sie", "w\u00e4\u00b7re", "zart", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Sie meynt/ da\u00df man mit recht sie als die beste kr\u00f6hne/", "tokens": ["Sie", "meynt", "/", "da\u00df", "man", "mit", "recht", "sie", "als", "die", "bes\u00b7te", "kr\u00f6h\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PIS", "APPR", "ADJD", "PPER", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch Nein! sie ist nicht sch\u00f6n/ wie es ihr Sinn begehrt/", "tokens": ["Doch", "Nein", "!", "sie", "ist", "nicht", "sch\u00f6n", "/", "wie", "es", "ihr", "Sinn", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PPER", "VAFIN", "PTKNEG", "ADJD", "$(", "PWAV", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die falsche Sch\u00f6nheit wird durch Einbildung ernehrt.", "tokens": ["Die", "fal\u00b7sche", "Sch\u00f6n\u00b7heit", "wird", "durch", "Ein\u00b7bil\u00b7dung", "er\u00b7nehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Fahrt hin/ ich acht eur nicht ihr falschen Liebes-Sterne/", "tokens": ["Fahrt", "hin", "/", "ich", "acht", "eur", "nicht", "ihr", "fal\u00b7schen", "Lie\u00b7bes\u00b7S\u00b7ter\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$(", "PPER", "CARD", "PPOSAT", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Jhr d\u00fcstert in der N\u00e4h\u2019 und gl\u00e4ntzet nur von ferne.", "tokens": ["Ihr", "d\u00fcs\u00b7tert", "in", "der", "N\u00e4h'", "und", "gl\u00e4nt\u00b7zet", "nur", "von", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ADV", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jhr falsches Hertze ist ein Boden-loser See/", "tokens": ["Ihr", "fal\u00b7sches", "Hert\u00b7ze", "ist", "ein", "Bo\u00b7den\u00b7lo\u00b7ser", "See", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ihre Treu vergeht wie der Aprillen-Schnee/", "tokens": ["Und", "ih\u00b7re", "Treu", "ver\u00b7geht", "wie", "der", "A\u00b7pril\u00b7len\u00b7Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sie ist ein Wetterhahn/ den jeder Wind verdrehet/", "tokens": ["Sie", "ist", "ein", "Wet\u00b7ter\u00b7hahn", "/", "den", "je\u00b7der", "Wind", "ver\u00b7dre\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und bald nach Ost und West/ nach ", "tokens": ["Und", "bald", "nach", "Ost", "und", "West", "/", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "APPR", "NE", "KON", "NN", "$(", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}