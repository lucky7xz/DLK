{"dta.poem.10298": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Bist du es wirklich, sitz\u2019 ich neben dir,", "tokens": ["Bist", "du", "es", "wirk\u00b7lich", ",", "sitz'", "ich", "ne\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "VVIMP", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sto\u00dfen aneinander unsre Gl\u00e4ser,", "tokens": ["Und", "sto\u00b7\u00dfen", "an\u00b7ein\u00b7an\u00b7der", "uns\u00b7re", "Gl\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Spielt irgendwo versteckt ein Fl\u00f6tenbl\u00e4ser", "tokens": ["Spielt", "ir\u00b7gend\u00b7wo", "ver\u00b7steckt", "ein", "Fl\u00f6\u00b7ten\u00b7bl\u00e4\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein sanftes Sch\u00e4ferst\u00fcckchen, dir und mir?", "tokens": ["Sein", "sanf\u00b7tes", "Sch\u00e4\u00b7fer\u00b7st\u00fcck\u00b7chen", ",", "dir", "und", "mir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und sitzen in der alten Halle wir,", "tokens": ["Und", "sit\u00b7zen", "in", "der", "al\u00b7ten", "Hal\u00b7le", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am Pfeiler dort der Kranz der Aehrenleser,", "tokens": ["Am", "Pfei\u00b7ler", "dort", "der", "Kranz", "der", "A\u00b7eh\u00b7ren\u00b7le\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch unverwelkt die Blumen und die Gr\u00e4ser,", "tokens": ["Noch", "un\u00b7ver\u00b7welkt", "die", "Blu\u00b7men", "und", "die", "Gr\u00e4\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War gestern unser letztes Erntebier?", "tokens": ["War", "ge\u00b7stern", "un\u00b7ser", "letz\u00b7tes", "Ern\u00b7te\u00b7bier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie Gru\u00df aus Gr\u00fcften ruft der Regenpfeifer,", "tokens": ["Wie", "Gru\u00df", "aus", "Gr\u00fcf\u00b7ten", "ruft", "der", "Re\u00b7gen\u00b7pfei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00e4\u00dflich her\u00fcber schreit das M\u00f6wenheer,", "tokens": ["H\u00e4\u00df\u00b7lich", "her\u00b7\u00fc\u00b7ber", "schreit", "das", "M\u00f6\u00b7wen\u00b7heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der seeenttauchten Bank Besitzergreifer.", "tokens": ["Der", "see\u00b7ent\u00b7tauch\u00b7ten", "Bank", "Be\u00b7sit\u00b7zer\u00b7grei\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Langweilig, \u00f6de, glei\u00dft das Wattenmeer,", "tokens": ["Lang\u00b7wei\u00b7lig", ",", "\u00f6\u00b7de", ",", "glei\u00dft", "das", "Wat\u00b7ten\u00b7meer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gezwungen schl\u00e4ft das Schiff, der Wellenschweifer,", "tokens": ["Ge\u00b7zwun\u00b7gen", "schl\u00e4ft", "das", "Schiff", ",", "der", "Wel\u00b7len\u00b7schwei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und einsam ist die Erde, w\u00fcst und leer.", "tokens": ["Und", "ein\u00b7sam", "ist", "die", "Er\u00b7de", ",", "w\u00fcst", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}