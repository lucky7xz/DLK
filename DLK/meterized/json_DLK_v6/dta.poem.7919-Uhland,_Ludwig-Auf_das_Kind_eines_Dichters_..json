{"dta.poem.7919": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Kind eines Dichters .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Sey uns willkommen, Dichterkind,", "tokens": ["Sey", "uns", "will\u00b7kom\u00b7men", ",", "Dich\u00b7ter\u00b7kind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deines Lebens goldner Pforte!", "tokens": ["An", "dei\u00b7nes", "Le\u00b7bens", "gold\u00b7ner", "Pfor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ziemen dir zum Angebind", "tokens": ["Wohl", "zie\u00b7men", "dir", "zum", "An\u00b7ge\u00b7bind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Lieder und prophet\u2019sche Worte.", "tokens": ["Sich", "Lie\u00b7der", "und", "pro\u00b7phet'\u00b7sche", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In gro\u00dfer Zeit erbl\u00fchest du,", "tokens": ["In", "gro\u00b7\u00dfer", "Zeit", "er\u00b7bl\u00fc\u00b7hest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ernsten Tagen, wundervollen,", "tokens": ["In", "erns\u00b7ten", "Ta\u00b7gen", ",", "wun\u00b7der\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo \u00fcber deiner kind\u2019schen Ruh", "tokens": ["Wo", "\u00fc\u00b7ber", "dei\u00b7ner", "kin\u00b7d'\u00b7schen", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Des heil\u2019gen Krieges Donner rollen.", "tokens": ["Des", "heil'\u00b7gen", "Krie\u00b7ges", "Don\u00b7ner", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du aber schlummre selig hin", "tokens": ["Du", "a\u00b7ber", "schlumm\u00b7re", "se\u00b7lig", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In angestammten Dichtertr\u00e4umen", "tokens": ["In", "an\u00b7ge\u00b7stamm\u00b7ten", "Dich\u00b7ter\u00b7tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Himmelsglanz und Waldesgr\u00fcn,", "tokens": ["Von", "Him\u00b7mels\u00b7glanz", "und", "Wal\u00b7des\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sternen, Blumen, Bl\u00fcthenb\u00e4umen!", "tokens": ["Von", "Ster\u00b7nen", ",", "Blu\u00b7men", ",", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Derweil verrauschet der Orkan,", "tokens": ["Der\u00b7weil", "ver\u00b7rau\u00b7schet", "der", "Or\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es weicht der blut\u2019gen Zeiten Tr\u00fcbe;", "tokens": ["Es", "weicht", "der", "blut'\u00b7gen", "Zei\u00b7ten", "Tr\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl bl\u00fchst als ", "tokens": ["Wohl", "bl\u00fchst", "als"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Du k\u00fcndest so das Reich der Liebe.", "tokens": ["Du", "k\u00fcn\u00b7dest", "so", "das", "Reich", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was einst als Ahnung, Sehnsucht nur", "tokens": ["Was", "einst", "als", "Ah\u00b7nung", ",", "Sehn\u00b7sucht", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "KOUS", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchdrungen deines Vaters Lieder,", "tokens": ["Durch\u00b7drun\u00b7gen", "dei\u00b7nes", "Va\u00b7ters", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das sinkt von sel\u2019ger Himmelsflur", "tokens": ["Das", "sinkt", "von", "sel'\u00b7ger", "Him\u00b7mels\u00b7flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als reiches Leben dir hernieder.", "tokens": ["Als", "rei\u00b7ches", "Le\u00b7ben", "dir", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}