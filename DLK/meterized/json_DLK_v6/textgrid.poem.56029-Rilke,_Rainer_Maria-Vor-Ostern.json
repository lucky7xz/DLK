{"textgrid.poem.56029": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Vor-Ostern", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morgen wird in diesen tiefgekerbten", "tokens": ["Mor\u00b7gen", "wird", "in", "die\u00b7sen", "tief\u00b7ge\u00b7kerb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PDAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gassen, die sich durch get\u00fcrmtes Wohnen", "tokens": ["Gas\u00b7sen", ",", "die", "sich", "durch", "ge\u00b7t\u00fcrm\u00b7tes", "Woh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "unten dunkel nach dem Hafen dr\u00e4ngen,", "tokens": ["un\u00b7ten", "dun\u00b7kel", "nach", "dem", "Ha\u00b7fen", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "hell das Gold der Prozessionen rollen;", "tokens": ["hell", "das", "Gold", "der", "Pro\u00b7zes\u00b7si\u00b7o\u00b7nen", "rol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "statt der Fetzen werden die ererbten", "tokens": ["statt", "der", "Fet\u00b7zen", "wer\u00b7den", "die", "er\u00b7erb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bettbez\u00fcge, welche wehen wollen,", "tokens": ["Bett\u00b7be\u00b7z\u00fc\u00b7ge", ",", "wel\u00b7che", "we\u00b7hen", "wol\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "von den immer h\u00f6heren Balkonen", "tokens": ["von", "den", "im\u00b7mer", "h\u00f6\u00b7he\u00b7ren", "Bal\u00b7ko\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "(wie in Flie\u00dfendem gespiegelt) h\u00e4ngen.", "tokens": ["(", "wie", "in", "Flie\u00b7\u00dfen\u00b7dem", "ge\u00b7spie\u00b7gelt", ")", "h\u00e4n\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KOKOM", "APPR", "NN", "VVPP", "$(", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Aber heute h\u00e4mmert an den Klopfern", "tokens": ["A\u00b7ber", "heu\u00b7te", "h\u00e4m\u00b7mert", "an", "den", "Klop\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "jeden Augenblick ein voll Bepackter,", "tokens": ["je\u00b7den", "Au\u00b7gen\u00b7blick", "ein", "voll", "Be\u00b7pack\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und sie schleppen immer neue K\u00e4ufe;", "tokens": ["und", "sie", "schlep\u00b7pen", "im\u00b7mer", "neu\u00b7e", "K\u00e4u\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "dennoch stehen strotzend noch die St\u00e4nde.", "tokens": ["den\u00b7noch", "ste\u00b7hen", "strot\u00b7zend", "noch", "die", "St\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "An der Ecke zeigt ein aufgehackter", "tokens": ["An", "der", "E\u00b7cke", "zeigt", "ein", "auf\u00b7ge\u00b7hack\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ochse seine frischen Innenw\u00e4nde,", "tokens": ["O\u00b7chse", "sei\u00b7ne", "fri\u00b7schen", "In\u00b7nen\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und in F\u00e4hnchen enden alle L\u00e4ufe.", "tokens": ["und", "in", "F\u00e4hn\u00b7chen", "en\u00b7den", "al\u00b7le", "L\u00e4u\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und ein Vorrat wie von tausend Opfern", "tokens": ["Und", "ein", "Vor\u00b7rat", "wie", "von", "tau\u00b7send", "Op\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KOKOM", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "dr\u00e4ngt auf B\u00e4nken, h\u00e4ngt sich rings um Pfl\u00f6cke,", "tokens": ["dr\u00e4ngt", "auf", "B\u00e4n\u00b7ken", ",", "h\u00e4ngt", "sich", "rings", "um", "Pfl\u00f6\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "PRF", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "zw\u00e4ngt sich, w\u00f6lbt sich, w\u00e4lzt sich aus dem D\u00e4mmer", "tokens": ["zw\u00e4ngt", "sich", ",", "w\u00f6lbt", "sich", ",", "w\u00e4lzt", "sich", "aus", "dem", "D\u00e4m\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "PRF", "$,", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "aller T\u00fcren, und vor dem Geg\u00e4hne", "tokens": ["al\u00b7ler", "T\u00fc\u00b7ren", ",", "und", "vor", "dem", "Ge\u00b7g\u00e4h\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "der Melonen strecken sich die Brote.", "tokens": ["der", "Me\u00b7lo\u00b7nen", "stre\u00b7cken", "sich", "die", "Bro\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Voller Gier und Handlung ist das Tote;", "tokens": ["Vol\u00b7ler", "Gier", "und", "Hand\u00b7lung", "ist", "das", "To\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "doch viel stiller sind die jungen H\u00e4hne", "tokens": ["doch", "viel", "stil\u00b7ler", "sind", "die", "jun\u00b7gen", "H\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "und die abgeh\u00e4ngten Ziegenb\u00f6cke", "tokens": ["und", "die", "ab\u00b7ge\u00b7h\u00e4ng\u00b7ten", "Zie\u00b7gen\u00b7b\u00f6\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "und am allerleisesten die L\u00e4mmer,", "tokens": ["und", "am", "al\u00b7ler\u00b7lei\u00b7ses\u00b7ten", "die", "L\u00e4m\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "ART", "NN", "$,"], "meter": "--+-+---+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "die die Knaben um die Schultern nehmen", "tokens": ["die", "die", "Kna\u00b7ben", "um", "die", "Schul\u00b7tern", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und die willig von den Schritten nicken;", "tokens": ["und", "die", "wil\u00b7lig", "von", "den", "Schrit\u00b7ten", "ni\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "w\u00e4hrend in der Mauer der verglasten", "tokens": ["w\u00e4h\u00b7rend", "in", "der", "Mau\u00b7er", "der", "ver\u00b7glas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "spanischen Madonna die Agraffe", "tokens": ["spa\u00b7ni\u00b7schen", "Ma\u00b7don\u00b7na", "die", "A\u00b7graf\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "und das Silber in den Diademen", "tokens": ["und", "das", "Sil\u00b7ber", "in", "den", "Di\u00b7a\u00b7de\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "von dem Lichter-Vorgef\u00fchl begl\u00e4nzter", "tokens": ["von", "dem", "Lich\u00b7ter\u00b7Vor\u00b7ge\u00b7f\u00fchl", "be\u00b7gl\u00e4nz\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "schimmert. Aber dr\u00fcber in dem Fenster", "tokens": ["schim\u00b7mert", ".", "A\u00b7ber", "dr\u00fc\u00b7ber", "in", "dem", "Fens\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "PAV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "zeigt sich blickverschwenderisch ein Affe", "tokens": ["zeigt", "sich", "blick\u00b7ver\u00b7schwen\u00b7de\u00b7risch", "ein", "Af\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "und f\u00fchrt rasch in einer angema\u00dften", "tokens": ["und", "f\u00fchrt", "rasch", "in", "ei\u00b7ner", "an\u00b7ge\u00b7ma\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Haltung Gesten aus, die sich nicht schicken.", "tokens": ["Hal\u00b7tung", "Ges\u00b7ten", "aus", ",", "die", "sich", "nicht", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,", "PRELS", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}