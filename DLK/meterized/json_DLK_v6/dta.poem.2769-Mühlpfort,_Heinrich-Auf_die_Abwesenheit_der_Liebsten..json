{"dta.poem.2769": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Abwesenheit der Liebsten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kan nicht deine Augen k\u00fcssen/", "tokens": ["Ich", "kan", "nicht", "dei\u00b7ne", "Au\u00b7gen", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dir ist jetzt mein Mund versagt.", "tokens": ["Und", "dir", "ist", "jetzt", "mein", "Mund", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach Schatz/ da\u00df wir so lieben m\u00fcssen/", "tokens": ["Ach", "Schatz", "/", "da\u00df", "wir", "so", "lie\u00b7ben", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich dem Himmel offt geklagt.", "tokens": ["Hab", "ich", "dem", "Him\u00b7mel", "offt", "ge\u00b7klagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umbsonst/ er zeigt nur Donnerblicke/", "tokens": ["Um\u00b7bsonst", "/", "er", "zeigt", "nur", "Don\u00b7ner\u00b7bli\u00b7cke", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und st\u00f6\u00dft den heissen Wunsch zur\u00fccke.", "tokens": ["Und", "st\u00f6\u00dft", "den", "heis\u00b7sen", "Wunsch", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ich will dich zwar im Hertzen tragen/", "tokens": ["Ich", "will", "dich", "zwar", "im", "Hert\u00b7zen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So lange mich die Erde tr\u00e4gt.", "tokens": ["So", "lan\u00b7ge", "mich", "die", "Er\u00b7de", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Geist soll deine Seele fragen/", "tokens": ["Mein", "Geist", "soll", "dei\u00b7ne", "See\u00b7le", "fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ob sie noch gleichen Zunder h\u00e4gt.", "tokens": ["Ob", "sie", "noch", "glei\u00b7chen", "Zun\u00b7der", "h\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du lebst und schwebst mir in Gedancken/", "tokens": ["Du", "lebst", "und", "schwebst", "mir", "in", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Doch nicht in eines Landes Schrancken.", "tokens": ["Doch", "nicht", "in", "ei\u00b7nes", "Lan\u00b7des", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Gefangne hoffen frey zu werden/", "tokens": ["Ge\u00b7fang\u00b7ne", "hof\u00b7fen", "frey", "zu", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich hoffe dich nicht mehr zu sehn.", "tokens": ["Ich", "hof\u00b7fe", "dich", "nicht", "mehr", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Wind kan jetzt in frembder Erden", "tokens": ["Der", "Wind", "kan", "jetzt", "in", "fremb\u00b7der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mir deine Seuffzer nicht zuwehn;", "tokens": ["Mir", "dei\u00b7ne", "Seuff\u00b7zer", "nicht", "zu\u00b7wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und dennoch baut der Liebe St\u00e4rcke", "tokens": ["Und", "den\u00b7noch", "baut", "der", "Lie\u00b7be", "St\u00e4r\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Jm Hertzen grosse Wunderwercke.", "tokens": ["Jm", "Hert\u00b7zen", "gros\u00b7se", "Wun\u00b7der\u00b7wer\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wenn mich der Schlaf nur eingewieget;", "tokens": ["Wenn", "mich", "der", "Schlaf", "nur", "ein\u00b7ge\u00b7wie\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "(wo auch die Liebe schlaffen l\u00e4st.)", "tokens": ["(", "wo", "auch", "die", "Lie\u00b7be", "schlaf\u00b7fen", "l\u00e4st", ".", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Hat sich ein Both im Traum verf\u00fcget/", "tokens": ["Hat", "sich", "ein", "Both", "im", "Traum", "ver\u00b7f\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der spricht/ die ", "tokens": ["Der", "spricht", "/", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$(", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Sie liebt/ und schickt dir dieses Schreiben/", "tokens": ["Sie", "liebt", "/", "und", "schickt", "dir", "die\u00b7ses", "Schrei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PPER", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und will auf ewig deine bleiben.", "tokens": ["Und", "will", "auf", "e\u00b7wig", "dei\u00b7ne", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJD", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Bald seh ich sie vorm Spiegel stehen/", "tokens": ["Bald", "seh", "ich", "sie", "vorm", "Spie\u00b7gel", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Wie sie das Haar zu Felde schl\u00e4gt;", "tokens": ["Wie", "sie", "das", "Haar", "zu", "Fel\u00b7de", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Bald mit beliebten Tritten gehen;", "tokens": ["Bald", "mit", "be\u00b7lieb\u00b7ten", "Trit\u00b7ten", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Bald wie sie sich zu Bette legt/", "tokens": ["Bald", "wie", "sie", "sich", "zu", "Bet\u00b7te", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und meine treue Lieder singet/", "tokens": ["Und", "mei\u00b7ne", "treu\u00b7e", "Lie\u00b7der", "sin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Bi\u00df sie der m\u00fcde Schlaf bezwinget.", "tokens": ["Bi\u00df", "sie", "der", "m\u00fc\u00b7de", "Schlaf", "be\u00b7zwin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Ach/ denck ich/ solt ich bey dir ligen/", "tokens": ["Ach", "/", "denck", "ich", "/", "solt", "ich", "bey", "dir", "li\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VVIMP", "PPER", "$(", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Sollt ich den s\u00fc\u00df-bethauten Mund", "tokens": ["Sollt", "ich", "den", "s\u00fc\u00df\u00b7be\u00b7thau\u00b7ten", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Mit einem solchen Ku\u00df vergn\u00fcgen/", "tokens": ["Mit", "ei\u00b7nem", "sol\u00b7chen", "Ku\u00df", "ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Der nur den Treu-verliebten kund/", "tokens": ["Der", "nur", "den", "Treu\u00b7ver\u00b7lieb\u00b7ten", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "So w\u00fcrd ich mich verg\u00f6ttert nennen/", "tokens": ["So", "w\u00fcrd", "ich", "mich", "ver\u00b7g\u00f6t\u00b7tert", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und keine Sterblichkeit mehr kennen.", "tokens": ["Und", "kei\u00b7ne", "Sterb\u00b7lich\u00b7keit", "mehr", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Ich wiederhole jene Zeiten/", "tokens": ["Ich", "wie\u00b7der\u00b7ho\u00b7le", "je\u00b7ne", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Da ich umb deinen Hal\u00df geschr\u00e4nckt/", "tokens": ["Da", "ich", "umb", "dei\u00b7nen", "Hal\u00df", "ge\u00b7schr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und mehr als tausend Lieblichkeiten/", "tokens": ["Und", "mehr", "als", "tau\u00b7send", "Lieb\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "CARD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Mich mit dem Nectar-Safft getr\u00e4nckt/", "tokens": ["Mich", "mit", "dem", "Nec\u00b7ta\u00b7r\u00b7S\u00b7afft", "ge\u00b7tr\u00e4nckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.41": {"text": "Der Lipp\u2019und Br\u00fcste holde Gaben/", "tokens": ["Der", "Lipp'und", "Br\u00fcs\u00b7te", "hol\u00b7de", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Verm\u00f6gen noch mein Hertz\u2019 zu laben.", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "noch", "mein", "Hertz'", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "So offt der West den Flor erhebet/", "tokens": ["So", "offt", "der", "West", "den", "Flor", "er\u00b7he\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Der deine Lilgen Br\u00fcste deckt/", "tokens": ["Der", "dei\u00b7ne", "Lil\u00b7gen", "Br\u00fcs\u00b7te", "deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "So dencke/ da\u00df mein Geist da schwebet/", "tokens": ["So", "den\u00b7cke", "/", "da\u00df", "mein", "Geist", "da", "schwe\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Da\u00df mein Hertz unter deinem steckt.", "tokens": ["Da\u00df", "mein", "Hertz", "un\u00b7ter", "dei\u00b7nem", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Wie dieser zarte Schneevoll Flammen/", "tokens": ["Wie", "die\u00b7ser", "zar\u00b7te", "Schnee\u00b7voll", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Wie Gluth und Blut sich f\u00fcgt zusammen.", "tokens": ["Wie", "Gluth", "und", "Blut", "sich", "f\u00fcgt", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Kommt wo ein Jungfern-Bild gegangen", "tokens": ["Kommt", "wo", "ein", "Jung\u00b7fern\u00b7Bild", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PWAV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Das Wunder-holde Sch\u00f6nheit ziert.", "tokens": ["Das", "Wun\u00b7der\u00b7hol\u00b7de", "Sch\u00f6n\u00b7heit", "ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Erkenn ich da\u00df der Liebsten Wangen", "tokens": ["Er\u00b7kenn", "ich", "da\u00df", "der", "Liebs\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Allein der Lorbeer-Krantz geb\u00fchrt.", "tokens": ["Al\u00b7lein", "der", "Lor\u00b7beer\u00b7Krantz", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Mein Spiegel-Gla\u00df sind sch\u00f6ne Frauen/", "tokens": ["Mein", "Spie\u00b7gel\u00b7Gla\u00df", "sind", "sch\u00f6\u00b7ne", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Worinn ich ", "tokens": ["Wo\u00b7rinn", "ich"], "token_info": ["word", "