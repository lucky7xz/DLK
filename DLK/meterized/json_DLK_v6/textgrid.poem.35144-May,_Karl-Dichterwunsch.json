{"textgrid.poem.35144": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Dichterwunsch", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat meine Stunde einst geschlagen,", "tokens": ["Hat", "mei\u00b7ne", "Stun\u00b7de", "einst", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ernsteste, die es wohl giebt,", "tokens": ["Die", "erns\u00b7tes\u00b7te", ",", "die", "es", "wohl", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So soll kein Herze um mich klagen,", "tokens": ["So", "soll", "kein", "Her\u00b7ze", "um", "mich", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn es noch so sehr mich liebt.", "tokens": ["Und", "wenn", "es", "noch", "so", "sehr", "mich", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich habe mich dann durchgerungen", "tokens": ["Ich", "ha\u00b7be", "mich", "dann", "durch\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und werf das enge Kleid von mir,", "tokens": ["Und", "werf", "das", "en\u00b7ge", "Kleid", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hab meine Seele freigesungen,", "tokens": ["Hab", "mei\u00b7ne", "See\u00b7le", "frei\u00b7ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Geh heim, doch noch nicht fort von hier.", "tokens": ["Geh", "heim", ",", "doch", "noch", "nicht", "fort", "von", "hier", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADV", "ADV", "PTKNEG", "PTKVZ", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es lag in mir ein doppelt Leben;", "tokens": ["Es", "lag", "in", "mir", "ein", "dop\u00b7pelt", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das eine kennt die Erde nicht;", "tokens": ["Das", "ei\u00b7ne", "kennt", "die", "Er\u00b7de", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das andre hab ich euch gegeben;", "tokens": ["Das", "and\u00b7re", "hab", "ich", "euch", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es wurde f\u00fcr euch zum Gedicht.", "tokens": ["Es", "wur\u00b7de", "f\u00fcr", "euch", "zum", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Macht dieses Leben euch zu Eigen;", "tokens": ["Macht", "die\u00b7ses", "Le\u00b7ben", "euch", "zu", "Ei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denkt und empfindet euch hinein,", "tokens": ["Denkt", "und", "emp\u00b7fin\u00b7det", "euch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So werde ich die Hand euch reichen", "tokens": ["So", "wer\u00b7de", "ich", "die", "Hand", "euch", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und niemals ferne von euch sein.", "tokens": ["Und", "nie\u00b7mals", "fer\u00b7ne", "von", "euch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum trauert nicht, wenn mir die Stunde,", "tokens": ["Drum", "trau\u00b7ert", "nicht", ",", "wenn", "mir", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich zum Vater ruft, einst schl\u00e4gt.", "tokens": ["Die", "mich", "zum", "Va\u00b7ter", "ruft", ",", "einst", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bringt mir ja die frohe Kunde,", "tokens": ["Sie", "bringt", "mir", "ja", "die", "fro\u00b7he", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach der mein Herz Verlangen tr\u00e4gt.", "tokens": ["Nach", "der", "mein", "Herz", "Ver\u00b7lan\u00b7gen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Ernst wird mir die Wangen bleichen,", "tokens": ["Ihr", "Ernst", "wird", "mir", "die", "Wan\u00b7gen", "blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch wenn ihr um mich steht und bebt,", "tokens": ["Doch", "wenn", "ihr", "um", "mich", "steht", "und", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wird sich auch mein Glaube zeigen:", "tokens": ["So", "wird", "sich", "auch", "mein", "Glau\u00b7be", "zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbich wei\u00df, da\u00df mein Erl\u00f6ser lebt.\u00ab", "tokens": ["\u00bb", "ich", "wei\u00df", ",", "da\u00df", "mein", "Er\u00b7l\u00f6\u00b7ser", "lebt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann, wenn ihr seht, da\u00df ich geschieden,", "tokens": ["Dann", ",", "wenn", "ihr", "seht", ",", "da\u00df", "ich", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich, man sagt, gestorben bin,", "tokens": ["Da\u00df", "ich", ",", "man", "sagt", ",", "ge\u00b7stor\u00b7ben", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PIS", "VVFIN", "$,", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So st\u00f6rt mir nicht den Himmelsfrieden,", "tokens": ["So", "st\u00f6rt", "mir", "nicht", "den", "Him\u00b7mels\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Begrabt mich nur nach meinem Sinn.", "tokens": ["Be\u00b7grabt", "mich", "nur", "nach", "mei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch todt will ich die H\u00e4nde halten", "tokens": ["Auch", "todt", "will", "ich", "die", "H\u00e4n\u00b7de", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So fromm, wie ihr es t\u00e4glich seht.", "tokens": ["So", "fromm", ",", "wie", "ihr", "es", "t\u00e4g\u00b7lich", "seht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich bitte euch, sie mir zu falten,", "tokens": ["Ich", "bit\u00b7te", "euch", ",", "sie", "mir", "zu", "fal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als l\u00e4ge still ich im Gebet.", "tokens": ["Als", "l\u00e4\u00b7ge", "still", "ich", "im", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Legt eine einzge, kleine Blume", "tokens": ["Legt", "ei\u00b7ne", "einz\u00b7ge", ",", "klei\u00b7ne", "Blu\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir auf die eingesunkne Brust.", "tokens": ["Mir", "auf", "die", "ein\u00b7ge\u00b7sunk\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr wi\u00dft, ich hielt nichts von dem Ruhme,", "tokens": ["Ihr", "wi\u00dft", ",", "ich", "hielt", "nichts", "von", "dem", "Ruh\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich war der Fehler mir bewu\u00dft.", "tokens": ["Ich", "war", "der", "Feh\u00b7ler", "mir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Tragt mich hinaus, nicht mit Gepr\u00e4nge;", "tokens": ["Tragt", "mich", "hin\u00b7aus", ",", "nicht", "mit", "Ge\u00b7pr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APZR", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Es ist des S\u00fcnders letzter Gang.", "tokens": ["Es", "ist", "des", "S\u00fcn\u00b7ders", "letz\u00b7ter", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vermeidet prahlerische Kl\u00e4nge;", "tokens": ["Ver\u00b7mei\u00b7det", "prah\u00b7le\u00b7ri\u00b7sche", "Kl\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "W\u00e4hlt einen ernsten Bu\u00dfgesang.", "tokens": ["W\u00e4hlt", "ei\u00b7nen", "erns\u00b7ten", "Bu\u00df\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann sollt ihr in das Grab mich legen,", "tokens": ["Dann", "sollt", "ihr", "in", "das", "Grab", "mich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Handvoll Erde mit hinein,", "tokens": ["Die", "Hand\u00b7voll", "Er\u00b7de", "mit", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und eines Priesters Gottessegen,", "tokens": ["Und", "ei\u00b7nes", "Pries\u00b7ters", "Got\u00b7tes\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der soll und wird mein Helfer sein.", "tokens": ["Der", "soll", "und", "wird", "mein", "Hel\u00b7fer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KON", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Denkmal ist euch streng verboten,", "tokens": ["Ein", "Denk\u00b7mal", "ist", "euch", "streng", "ver\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch sei der H\u00fcgel gr\u00fcn berankt.", "tokens": ["Doch", "sei", "der", "H\u00fc\u00b7gel", "gr\u00fcn", "be\u00b7rankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Erz und Stein dankt man den Todten;", "tokens": ["Mit", "Erz", "und", "Stein", "dankt", "man", "den", "Tod\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich wei\u00df, da\u00df ihr mir besser dankt.", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "ihr", "mir", "bes\u00b7ser", "dankt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich will ja nicht von hinnen scheiden,", "tokens": ["Ich", "will", "ja", "nicht", "von", "hin\u00b7nen", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ihr, ihr la\u00dft mich auch nicht fort;", "tokens": ["Und", "ihr", ",", "ihr", "la\u00dft", "mich", "auch", "nicht", "fort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tod wird zwar mich anders kleiden,", "tokens": ["Der", "Tod", "wird", "zwar", "mich", "an\u00b7ders", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wechsele ich nicht den Ort.", "tokens": ["Doch", "wech\u00b7se\u00b7le", "ich", "nicht", "den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den K\u00f6rper tr\u00e4gt man wohl zu Grabe,", "tokens": ["Den", "K\u00f6r\u00b7per", "tr\u00e4gt", "man", "wohl", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Menschen und den Dichter nicht.", "tokens": ["Den", "Men\u00b7schen", "und", "den", "Dich\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Eine sei euch Himmelsgabe;", "tokens": ["Der", "Ei\u00b7ne", "sei", "euch", "Him\u00b7mels\u00b7ga\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Andre bleib euch \u2013 \u2013 kein Gedicht!", "tokens": ["Der", "And\u00b7re", "bleib", "euch", "\u2013", "\u2013", "kein", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "$(", "$(", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}