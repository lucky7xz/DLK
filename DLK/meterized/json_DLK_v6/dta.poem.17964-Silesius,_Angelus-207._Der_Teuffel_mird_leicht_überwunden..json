{"dta.poem.17964": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "207.  Der Teuffel mird leicht \u00fcberwunden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Christ bi\u00df nur nicht verzagt/ mit wachen fasten bethen", "tokens": ["Christ", "bi\u00df", "nur", "nicht", "ver\u00b7zagt", "/", "mit", "wa\u00b7chen", "fas\u00b7ten", "be\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "PTKNEG", "VVPP", "$(", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kanstu das gantze Heer der Teuffel unterthreten.", "tokens": ["Kans\u00b7tu", "das", "gant\u00b7ze", "Heer", "der", "Teuf\u00b7fel", "un\u00b7ter\u00b7thre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}