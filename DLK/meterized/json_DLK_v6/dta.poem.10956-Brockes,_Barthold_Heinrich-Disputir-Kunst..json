{"dta.poem.10956": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Disputir-Kunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Willt du ruhig und beliebt, \u00fcberall gelitten seyn:", "tokens": ["Willt", "du", "ru\u00b7hig", "und", "be\u00b7liebt", ",", "\u00fc\u00b7be\u00b7rall", "ge\u00b7lit\u00b7ten", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Disputir mit keinem Menschen, er sey vornehm oder klein.", "tokens": ["Dis\u00b7pu\u00b7tir", "mit", "kei\u00b7nem", "Men\u00b7schen", ",", "er", "sey", "vor\u00b7nehm", "o\u00b7der", "klein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn, du wirst entweder siegen,", "tokens": ["Denn", ",", "du", "wirst", "ent\u00b7we\u00b7der", "sie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder aber unten liegen.", "tokens": ["O\u00b7der", "a\u00b7ber", "un\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Letzters kann dir nicht gefallen; ersters thut dem andern weh,", "tokens": ["Letz\u00b7ters", "kann", "dir", "nicht", "ge\u00b7fal\u00b7len", ";", "ers\u00b7ters", "thut", "dem", "an\u00b7dern", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PTKNEG", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Mehr, als einer Ursach halber. Sein Verstand soll von der", "tokens": ["Mehr", ",", "als", "ei\u00b7ner", "Ur\u00b7sach", "hal\u00b7ber", ".", "Sein", "Ver\u00b7stand", "soll", "von", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "APPO", "$.", "PPOSAT", "NN", "VMFIN", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Drauf er sich geglaubt, herunter. Dieses ist ihm \u00e4rgerlich;", "tokens": ["Drauf", "er", "sich", "ge\u00b7glaubt", ",", "her\u00b7un\u00b7ter", ".", "Die\u00b7ses", "ist", "ihm", "\u00e4r\u00b7ger\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVPP", "$,", "PTKVZ", "$.", "PDS", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und er thuts mit Widerwillen. Aber, da\u00df zugleich er dich", "tokens": ["Und", "er", "thuts", "mit", "Wi\u00b7der\u00b7wil\u00b7len", ".", "A\u00b7ber", ",", "da\u00df", "zu\u00b7gleich", "er", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$.", "KON", "$,", "KOUS", "ADV", "PPER", "PRF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Noch vern\u00fcnftiger, als sich,", "tokens": ["Noch", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", ",", "als", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Halten und erkennen soll; sind zween Schl\u00e4g in einer Wunden,", "tokens": ["Hal\u00b7ten", "und", "er\u00b7ken\u00b7nen", "soll", ";", "sind", "zween", "Schl\u00e4g", "in", "ei\u00b7ner", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$.", "VAFIN", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.11": {"text": "Die von seinem lieben Ich gar zu heftig sind empfunden.", "tokens": ["Die", "von", "sei\u00b7nem", "lie\u00b7ben", "Ich", "gar", "zu", "hef\u00b7tig", "sind", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Und er wird, ohn allen Zweifel, dich, nebst deiner Wahrheit,", "tokens": ["Und", "er", "wird", ",", "ohn", "al\u00b7len", "Zwei\u00b7fel", ",", "dich", ",", "nebst", "dei\u00b7ner", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUI", "PIAT", "NN", "$,", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "Soll er aber, sonder Ha\u00df, sich von dir besiegen lassen:", "tokens": ["Soll", "er", "a\u00b7ber", ",", "son\u00b7der", "Ha\u00df", ",", "sich", "von", "dir", "be\u00b7sie\u00b7gen", "las\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KON", "NN", "$,", "PRF", "APPR", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Mu\u00dft du erst sein Herz gewinnen, und dazu beliebter Minen,", "tokens": ["Mu\u00dft", "du", "erst", "sein", "Herz", "ge\u00b7win\u00b7nen", ",", "und", "da\u00b7zu", "be\u00b7lieb\u00b7ter", "Mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,", "KON", "PAV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Sanfter T\u00f6ne, s\u00fcsser W\u00f6rter, holden Ausdrucks dich bedienen.", "tokens": ["Sanf\u00b7ter", "T\u00f6\u00b7ne", ",", "s\u00fcs\u00b7ser", "W\u00f6r\u00b7ter", ",", "hol\u00b7den", "Aus\u00b7drucks", "dich", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Bey der Tonkunst brauche ja eines freundlichen Gesichts,", "tokens": ["Bey", "der", "Ton\u00b7kunst", "brau\u00b7che", "ja", "ei\u00b7nes", "freund\u00b7li\u00b7chen", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Und bem\u00fch dich, deine Schl\u00fcsse, wie mit Honig, zu vers\u00fcssen.", "tokens": ["Und", "be\u00b7m\u00fch", "dich", ",", "dei\u00b7ne", "Schl\u00fcs\u00b7se", ",", "wie", "mit", "Ho\u00b7nig", ",", "zu", "ver\u00b7s\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PWAV", "APPR", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Sonsten richtest du bey allen mit den st\u00e4rksten Schl\u00fcssen nichts;", "tokens": ["Sons\u00b7ten", "rich\u00b7test", "du", "bey", "al\u00b7len", "mit", "den", "st\u00e4rks\u00b7ten", "Schl\u00fcs\u00b7sen", "nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PIS", "APPR", "ART", "ADJA", "NN", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Und dir wird aus deiner Klugheit nichts, als bittre Wermuth,", "tokens": ["Und", "dir", "wird", "aus", "dei\u00b7ner", "Klug\u00b7heit", "nichts", ",", "als", "bitt\u00b7re", "Wer\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PIS", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}}}}