{"textgrid.poem.42175": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Marys Kochschule", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df in deinem Engelsk\u00f6pfchen", "tokens": ["Da\u00df", "in", "dei\u00b7nem", "En\u00b7gels\u00b7k\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So viel Teufelei rumort,", "tokens": ["So", "viel", "Teu\u00b7fe\u00b7lei", "ru\u00b7mort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tt ich nimmer ahnen k\u00f6nnen;", "tokens": ["H\u00e4tt", "ich", "nim\u00b7mer", "ah\u00b7nen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber deine K\u00fcsse brennen,", "tokens": ["A\u00b7ber", "dei\u00b7ne", "K\u00fcs\u00b7se", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie kein H\u00f6llenfeuer schmort.", "tokens": ["Wie", "kein", "H\u00f6l\u00b7len\u00b7feu\u00b7er", "schmort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deiner Seele hei\u00dfe Sauce", "tokens": ["Dei\u00b7ner", "See\u00b7le", "hei\u00b7\u00dfe", "Sau\u00b7ce"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gie\u00dft sich prasselnd auf mich aus;", "tokens": ["Gie\u00dft", "sich", "pras\u00b7selnd", "auf", "mich", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den neusten Apparaten", "tokens": ["Mit", "den", "neus\u00b7ten", "Ap\u00b7pa\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werd ich \u00c4rmster ausgebraten,", "tokens": ["Werd", "ich", "\u00c4rms\u00b7ter", "aus\u00b7ge\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein bejammernswerter Schmaus.", "tokens": ["Ein", "be\u00b7jam\u00b7merns\u00b7wer\u00b7ter", "Schmaus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlie\u00dflich \u00f6ffnest du die Brust mir", "tokens": ["Schlie\u00df\u00b7lich", "\u00f6ff\u00b7nest", "du", "die", "Brust", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und transchierst mein dampfend Herz,", "tokens": ["Und", "tran\u00b7schierst", "mein", "damp\u00b7fend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weidest dich an seinem Pochen,", "tokens": ["Wei\u00b7dest", "dich", "an", "sei\u00b7nem", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie's zerrissen und zerstochen", "tokens": ["Wie's", "zer\u00b7ris\u00b7sen", "und", "zer\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVPP", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in St\u00fccke sprang vor Schmerz.", "tokens": ["Und", "in", "St\u00fc\u00b7cke", "sprang", "vor", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}