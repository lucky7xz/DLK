{"textgrid.poem.57492": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Auf eines werthen Freundes Magisterpromotion in Halle", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund! dem die Natur mehr Gaben eingepr\u00e4gt,", "tokens": ["Mein", "Freund", "!", "dem", "die", "Na\u00b7tur", "mehr", "Ga\u00b7ben", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als mancher andrer sich kaum halb zu w\u00fcnschen pflegt;", "tokens": ["Als", "man\u00b7cher", "an\u00b7drer", "sich", "kaum", "halb", "zu", "w\u00fcn\u00b7schen", "pflegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "PRF", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dessen muntrer Kopf in wenig Vierthelstunden", "tokens": ["Und", "des\u00b7sen", "mun\u00b7trer", "Kopf", "in", "we\u00b7nig", "Vier\u00b7thel\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mehr Witziges erdacht, als Mops im Jahr erfunden:", "tokens": ["Mehr", "Wit\u00b7zi\u00b7ges", "er\u00b7dacht", ",", "als", "Mops", "im", "Jahr", "er\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "KOUS", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Freund! wenn ich bisher ganz unempfindlich blieb,", "tokens": ["Mein", "Freund", "!", "wenn", "ich", "bis\u00b7her", "ganz", "un\u00b7emp\u00b7find\u00b7lich", "blieb", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und kein verdientes Lied zu deinem Ruhme schrieb:", "tokens": ["Und", "kein", "ver\u00b7dien\u00b7tes", "Lied", "zu", "dei\u00b7nem", "Ruh\u00b7me", "schrieb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So mochtest du vielleicht von deinem Freunde denken,", "tokens": ["So", "moch\u00b7test", "du", "viel\u00b7leicht", "von", "dei\u00b7nem", "Freun\u00b7de", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er wolle dir aus Stolz kein Lobgedichte schenken.", "tokens": ["Er", "wol\u00b7le", "dir", "aus", "Stolz", "kein", "Lob\u00b7ge\u00b7dich\u00b7te", "schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Allein, das war gefehlt. Dein Argwohn tr\u00fcget sich;", "tokens": ["Al\u00b7lein", ",", "das", "war", "ge\u00b7fehlt", ".", "Dein", "Arg\u00b7wohn", "tr\u00fc\u00b7get", "sich", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PDS", "VAFIN", "VVPP", "$.", "PPOSAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Hochmuth hat nicht Schuld, ein Zweifel schreckte mich.", "tokens": ["Mein", "Hoch\u00b7muth", "hat", "nicht", "Schuld", ",", "ein", "Zwei\u00b7fel", "schreck\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wollte mich nicht gern auf den Parnassus dringen,", "tokens": ["Ich", "woll\u00b7te", "mich", "nicht", "gern", "auf", "den", "Par\u00b7nas\u00b7sus", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und durch ein hartes Lied von deiner Ehre singen.", "tokens": ["Und", "durch", "ein", "har\u00b7tes", "Lied", "von", "dei\u00b7ner", "Eh\u00b7re", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich kenne meine Kraft und meine Schw\u00e4che schon;", "tokens": ["Ich", "ken\u00b7ne", "mei\u00b7ne", "Kraft", "und", "mei\u00b7ne", "Schw\u00e4\u00b7che", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bin nicht so begl\u00fcckt, als mancher Musensohn,", "tokens": ["Ich", "bin", "nicht", "so", "be\u00b7gl\u00fcckt", ",", "als", "man\u00b7cher", "Mu\u00b7sen\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der ungezweifelt glaubt, da\u00df seiner Cyther Seyten", "tokens": ["Der", "un\u00b7ge\u00b7zwei\u00b7felt", "glaubt", ",", "da\u00df", "sei\u00b7ner", "Cy\u00b7ther", "Sey\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit aller Alten Kunst um Rang und Vorzug streiten;", "tokens": ["Mit", "al\u00b7ler", "Al\u00b7ten", "Kunst", "um", "Rang", "und", "Vor\u00b7zug", "strei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der in Gedanken steht, Homerus und Virgil,", "tokens": ["Der", "in", "Ge\u00b7dan\u00b7ken", "steht", ",", "Ho\u00b7me\u00b7rus", "und", "Vir\u00b7gil", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Die Wunder der Natur, beneiden ihm sein Spiel:", "tokens": ["Die", "Wun\u00b7der", "der", "Na\u00b7tur", ",", "be\u00b7nei\u00b7den", "ihm", "sein", "Spiel", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil Midas und sein Chor, die seine Lieder h\u00f6ren,", "tokens": ["Weil", "Mi\u00b7das", "und", "sein", "Chor", ",", "die", "sei\u00b7ne", "Lie\u00b7der", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Aus lauter Unverstand den harten Ton verehren.", "tokens": ["Aus", "lau\u00b7ter", "Un\u00b7ver\u00b7stand", "den", "har\u00b7ten", "Ton", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach nein, gelehrter Freund! so gl\u00fccklich bin ich nicht:", "tokens": ["Ach", "nein", ",", "ge\u00b7lehr\u00b7ter", "Freund", "!", "so", "gl\u00fcck\u00b7lich", "bin", "ich", "nicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "ADJA", "NN", "$.", "ADV", "ADJD", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So bald ich dichten soll, err\u00f6thet mein Gesicht.", "tokens": ["So", "bald", "ich", "dich\u00b7ten", "soll", ",", "er\u00b7r\u00f6t\u00b7het", "mein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mu\u00df mich vor mir selbst und meinen Versen sch\u00e4men;", "tokens": ["Ich", "mu\u00df", "mich", "vor", "mir", "selbst", "und", "mei\u00b7nen", "Ver\u00b7sen", "sch\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "ADV", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drum mag ich auch den Kiel nicht in die H\u00e4nde nehmen.", "tokens": ["Drum", "mag", "ich", "auch", "den", "Kiel", "nicht", "in", "die", "H\u00e4n\u00b7de", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ART", "NE", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sieht ja mit Verdru\u00df, da\u00df Squenz, der Sylbenheld,", "tokens": ["Man", "sieht", "ja", "mit", "Ver\u00b7dru\u00df", ",", "da\u00df", "Squenz", ",", "der", "Syl\u00b7ben\u00b7held", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "$,", "KOUS", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn Pallas ihrer Schaar die Kr\u00e4nze zugestellt,", "tokens": ["Wenn", "Pal\u00b7las", "ih\u00b7rer", "Schaar", "die", "Kr\u00e4n\u00b7ze", "zu\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An statt des Pegasus sich auf ein Maulthier schwinget,", "tokens": ["An", "statt", "des", "Pe\u00b7ga\u00b7sus", "sich", "auf", "ein", "Mault\u00b7hier", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NE", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dann mit aller Kraft gezwungne Lieder singet.", "tokens": ["Und", "dann", "mit", "al\u00b7ler", "Kraft", "ge\u00b7zwung\u00b7ne", "Lie\u00b7der", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Daher k\u00f6mmt mancher Vers, wo alles mager sieht,", "tokens": ["Da\u00b7her", "k\u00f6mmt", "man\u00b7cher", "Vers", ",", "wo", "al\u00b7les", "ma\u00b7ger", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo lauter Aberwitz die Bl\u00e4tter \u00fcberzieht;", "tokens": ["Wo", "lau\u00b7ter", "A\u00b7ber\u00b7witz", "die", "Bl\u00e4t\u00b7ter", "\u00fc\u00b7berz\u00b7ieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo unverst\u00e4ndlich Zeug die leeren Zeilen f\u00fcllet;", "tokens": ["Wo", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "Zeug", "die", "lee\u00b7ren", "Zei\u00b7len", "f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo manche Redensart von falscher Hoheit schwillet;", "tokens": ["Wo", "man\u00b7che", "Re\u00b7den\u00b7sart", "von", "fal\u00b7scher", "Ho\u00b7heit", "schwil\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo fast kein einzig Wort sich recht zur Sache schickt;", "tokens": ["Wo", "fast", "kein", "ein\u00b7zig", "Wort", "sich", "recht", "zur", "Sa\u00b7che", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "ADJD", "NN", "PRF", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo man den deutschen Rock mit w\u00e4lschen Lumpen flickt;", "tokens": ["Wo", "man", "den", "deut\u00b7schen", "Rock", "mit", "w\u00e4l\u00b7schen", "Lum\u00b7pen", "flickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja, wo die Reime selbst nicht auf einander treffen,", "tokens": ["Ja", ",", "wo", "die", "Rei\u00b7me", "selbst", "nicht", "auf", "ein\u00b7an\u00b7der", "tref\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "ADV", "PTKNEG", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So, da\u00df auch Kinder oft den dummen Dichter \u00e4ffen.", "tokens": ["So", ",", "da\u00df", "auch", "Kin\u00b7der", "oft", "den", "dum\u00b7men", "Dich\u00b7ter", "\u00e4f\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein solch Exempel hat mich so verzagt gemacht,", "tokens": ["Ein", "solch", "Ex\u00b7em\u00b7pel", "hat", "mich", "so", "ver\u00b7zagt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Da\u00df ich bisher ges\u00e4umt. Ich habe mich bedacht,", "tokens": ["Da\u00df", "ich", "bis\u00b7her", "ge\u00b7s\u00e4umt", ".", "Ich", "ha\u00b7be", "mich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ob meine Finger auch, bey deinen Freudentagen,", "tokens": ["Ob", "mei\u00b7ne", "Fin\u00b7ger", "auch", ",", "bey", "dei\u00b7nen", "Freu\u00b7den\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verm\u00f6gend w\u00fcrden seyn, ein reines Lied zu schlagen.", "tokens": ["Ver\u00b7m\u00f6\u00b7gend", "w\u00fcr\u00b7den", "seyn", ",", "ein", "rei\u00b7nes", "Lied", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "VAINF", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sprach: Wo 