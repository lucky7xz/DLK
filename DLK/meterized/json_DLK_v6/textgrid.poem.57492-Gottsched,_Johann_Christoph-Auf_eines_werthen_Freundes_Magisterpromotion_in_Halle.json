"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warf Blatt und Feder weg. Der herrschende Verstand", "tokens": ["Warf", "Blatt", "und", "Fe\u00b7der", "weg", ".", "Der", "herr\u00b7schen\u00b7de", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bezwang f\u00fcr diesesmal die St\u00e4rke meiner Triebe,", "tokens": ["Be\u00b7zwang", "f\u00fcr", "die\u00b7ses\u00b7mal", "die", "St\u00e4r\u00b7ke", "mei\u00b7ner", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der eigne Ruhm galt mehr, als meines Freundes Liebe.", "tokens": ["Der", "eig\u00b7ne", "Ruhm", "galt", "mehr", ",", "als", "mei\u00b7nes", "Freun\u00b7des", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Wallung des Gebl\u00fcts verlohr sich allgemach;", "tokens": ["Die", "Wal\u00b7lung", "des", "Ge\u00b7bl\u00fcts", "ver\u00b7lohr", "sich", "all\u00b7ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Herz ward wieder kalt und seine Schl\u00e4ge schwach.", "tokens": ["Mein", "Herz", "ward", "wie\u00b7der", "kalt", "und", "sei\u00b7ne", "Schl\u00e4\u00b7ge", "schwach", "."], "token_info": ["word", "word", "word