{"textgrid.poem.26203": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und auch den \u00c4ckern gingen Augen auf", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und auch den \u00c4ckern gingen Augen auf.", "tokens": ["Und", "auch", "den", "\u00c4\u00b7ckern", "gin\u00b7gen", "Au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kornblumen, die stahlblauen, stummen,", "tokens": ["Korn\u00b7blu\u00b7men", ",", "die", "stahl\u00b7blau\u00b7en", ",", "stum\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Betrachten wie Augen der Sonne Lauf.", "tokens": ["Be\u00b7trach\u00b7ten", "wie", "Au\u00b7gen", "der", "Son\u00b7ne", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ihre Farbe ist ehrf\u00fcrchtig und tief.", "tokens": ["Ih\u00b7re", "Far\u00b7be", "ist", "ehr\u00b7f\u00fcrch\u00b7tig", "und", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sie wohnen ernst auf den Ackerkrumen,", "tokens": ["Sie", "woh\u00b7nen", "ernst", "auf", "den", "A\u00b7cker\u00b7kru\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die blaue Ruhe sie aus der Erde rief;", "tokens": ["Die", "blau\u00b7e", "Ru\u00b7he", "sie", "aus", "der", "Er\u00b7de", "rief", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Himmel st\u00fcckweis auf der Erd' einzieht", "tokens": ["Der", "Him\u00b7mel", "st\u00fcck\u00b7weis", "auf", "der", "Erd'", "ein\u00b7zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und gr\u00fcnende \u00c4hren \u00fcber ihm summen,", "tokens": ["Und", "gr\u00fc\u00b7nen\u00b7de", "\u00c4h\u00b7ren", "\u00fc\u00b7ber", "ihm", "sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und eine Kornblum' der andern wie Aug' ins Auge sieht.", "tokens": ["Und", "ei\u00b7ne", "Korn\u00b7blum'", "der", "an\u00b7dern", "wie", "Aug'", "ins", "Au\u00b7ge", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "PIS", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}