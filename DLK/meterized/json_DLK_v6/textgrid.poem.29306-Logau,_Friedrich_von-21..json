{"textgrid.poem.29306": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wehr-, Lehr-, N\u00e4hr-Stand, ieder Stand hat sein eigen Ehr in sich;", "tokens": ["Wehr", ",", "Lehr", ",", "N\u00e4hr\u00b7Stand", ",", "ie\u00b7der", "Stand", "hat", "sein", "ei\u00b7gen", "Ehr", "in", "sich", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "$,", "NN", "$,", "PIAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PRF", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nimm W L und N weg, lehrt der Name solches dich;", "tokens": ["Nimm", "W", "L", "und", "N", "weg", ",", "lehrt", "der", "Na\u00b7me", "sol\u00b7ches", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "XY", "XY", "KON", "XY", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PIAT", "PRF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Nur der Her-Stand, der bi\u00dfher andrer St\u00e4nde Hencker war,", "tokens": ["Nur", "der", "Her\u00b7Stand", ",", "der", "bi\u00df\u00b7her", "an\u00b7drer", "St\u00e4n\u00b7de", "Hen\u00b7cker", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "NE", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Hat bey St\u00e4nden keinen Stand, ist an Ehr und Namen baar.", "tokens": ["Hat", "bey", "St\u00e4n\u00b7den", "kei\u00b7nen", "Stand", ",", "ist", "an", "Ehr", "und", "Na\u00b7men", "baar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PIAT", "NN", "$,", "VAFIN", "APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}