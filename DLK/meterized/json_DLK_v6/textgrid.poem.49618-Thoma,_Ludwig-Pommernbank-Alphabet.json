{"textgrid.poem.49618": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Pommernbank-Alphabet", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfer ", "tokens": ["Ein", "gro\u00b7\u00dfer"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wenn man ihn nicht f\u00fcr Kirchen macht.", "tokens": ["Wenn", "man", "ihn", "nicht", "f\u00fcr", "Kir\u00b7chen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Bau der Kirche war ", "tokens": ["Der", "Bau", "der", "Kir\u00b7che", "war"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und trotzdem durchaus ", "tokens": ["Und", "trotz\u00b7dem", "durc\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So gut wie unten zu entbehren.", "tokens": ["So", "gut", "wie", "un\u00b7ten", "zu", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein allzu gro\u00dfes ", "tokens": ["Ein", "all\u00b7zu", "gro\u00b7\u00dfes"], "token_info": ["word", "word", "word"], "pos": ["ART", "PTKA", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Gereicht nicht immerdar zum Ruhm.", "tokens": ["Ge\u00b7reicht", "nicht", "im\u00b7mer\u00b7dar", "zum", "Ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Versp\u00e4tung wirkt nicht angenehm.", "tokens": ["Ver\u00b7sp\u00e4\u00b7tung", "wirkt", "nicht", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ein glattes ", "tokens": ["Ein", "glat\u00b7tes"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Hat durch die ", "tokens": ["Hat", "durch", "die"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Nach Herkunft fr\u00e4gt die hohe Welt", "tokens": ["Nach", "Her\u00b7kunft", "fr\u00e4gt", "die", "ho\u00b7he", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur einen Mann, niemals das ", "tokens": ["Nur", "ei\u00b7nen", "Mann", ",", "nie\u00b7mals", "das"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Vor dem ", "tokens": ["Vor", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.10": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Nicht jede ", "tokens": ["Nicht", "je\u00b7de"], "token_info": ["word", "word"], "pos": ["PTKNEG", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.11": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Gereicht dem Himmlischen zu Nutz.", "tokens": ["Ge\u00b7reicht", "dem", "Himm\u00b7li\u00b7schen", "zu", "Nutz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Von Mirbach achtet nicht den Spott;", "tokens": ["Von", "Mir\u00b7bach", "ach\u00b7tet", "nicht", "den", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er tat es f\u00fcr den lieben ", "tokens": ["Er", "tat", "es", "f\u00fcr", "den", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und mancher wird dadurch verf\u00fchrt.", "tokens": ["Und", "man\u00b7cher", "wird", "da\u00b7durch", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Nicht jeder, der im Glauben ", "tokens": ["Nicht", "je\u00b7der", ",", "der", "im", "Glau\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PIS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist mit Erkenntnistau befeuchtet.", "tokens": ["Ist", "mit", "Er\u00b7kennt\u00b7ni\u00b7stau", "be\u00b7feuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Bei M da denkt man so herum", "tokens": ["Bei", "M", "da", "denkt", "man", "so", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADV", "VVFIN", "PIS", "ADV", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}}, "stanza.16": {"line.1": {"text": "Im Bankproze\u00df weht eine Luft,", "tokens": ["Im", "Bank\u00b7pro\u00b7ze\u00df", "weht", "ei\u00b7ne", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ganz so sch\u00f6n wie ", "tokens": ["Nicht", "ganz", "so", "sch\u00f6n", "wie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "KOKOM"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.17": {"line.1": {"text": "Man ist sich ", "tokens": ["Man", "ist", "sich"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VAFIN", "PRF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Doch uns ward manches ", "tokens": ["Doch", "uns", "ward", "man\u00b7ches"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.18": {"line.1": {"text": "Nicht jeder Adelsschild ist blank,", "tokens": ["Nicht", "je\u00b7der", "A\u00b7dels\u00b7schild", "ist", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fleck kam von der ", "tokens": ["Der", "Fleck", "kam", "von", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.19": {"line.1": {"text": "Wer fr\u00e4gt nach ", "tokens": ["Wer", "fr\u00e4gt", "nach"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wenn nur das ", "tokens": ["Wenn", "nur", "das"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.20": {"line.1": {"text": "Streng ist man gegen die, die streiken,", "tokens": ["Streng", "ist", "man", "ge\u00b7gen", "die", ",", "die", "strei\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "APPR", "ART", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch sanft mit Schultz und mit ", "tokens": ["Doch", "sanft", "mit", "Schultz", "und", "mit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Jetzt kam ein b\u00f6ses Interim.", "tokens": ["Jetzt", "kam", "ein", "b\u00f6\u00b7ses", "In\u00b7te\u00b7rim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.22": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Es kostet viel und geht oft schief.", "tokens": ["Es", "kos\u00b7tet", "viel", "und", "geht", "oft", "schief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.23": {"line.1": {"text": "Es k\u00e4men noch das U, V, W,", "tokens": ["Es", "k\u00e4\u00b7men", "noch", "das", "U", ",", "V", ",", "W", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "XY", "$,", "CARD", "$,", "XY", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das X in diesem Abc,", "tokens": ["Das", "X", "in", "die\u00b7sem", "Abc", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "XY", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch die Blamage langt wohl schon", "tokens": ["Doch", "die", "Bla\u00b7ma\u00b7ge", "langt", "wohl", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ohne Z und Y.", "tokens": ["Auch", "oh\u00b7ne", "Z", "und", "Y."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "APPR", "NN", "KON", "NE"], "meter": "-+-+-", "measure": "iambic.di"}}}}}