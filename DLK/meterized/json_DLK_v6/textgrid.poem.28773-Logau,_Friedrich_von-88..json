{"textgrid.poem.28773": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem ich ein gesundes Jahr w\u00fcntsche, wei\u00df mir ieder Danck;", "tokens": ["Wem", "ich", "ein", "ge\u00b7sun\u00b7des", "Jahr", "w\u00fcnt\u00b7sche", ",", "wei\u00df", "mir", "ie\u00b7der", "Danck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nur der Doctor wil nicht dran: andrer frisch das ist sein kranck.", "tokens": ["Nur", "der", "Doc\u00b7tor", "wil", "nicht", "dran", ":", "an\u00b7drer", "frisch", "das", "ist", "sein", "kranck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "PTKNEG", "PTKVZ", "$.", "ADJA", "ADJD", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}