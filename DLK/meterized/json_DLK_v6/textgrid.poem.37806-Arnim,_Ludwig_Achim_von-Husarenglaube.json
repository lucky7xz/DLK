{"textgrid.poem.37806": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Husarenglaube", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nichts lustger auf der Welt,", "tokens": ["Es", "ist", "nichts", "lust\u00b7ger", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auch nichts so geschwind,", "tokens": ["Und", "auch", "nichts", "so", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADV", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Als wir Husaren in dem Feld,", "tokens": ["Als", "wir", "Hu\u00b7sa\u00b7ren", "in", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir beym Schlachten sind.", "tokens": ["Wenn", "wir", "beym", "Schlach\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenns blitzt und kracht dem Donner gleich", "tokens": ["Wenns", "blitzt", "und", "kracht", "dem", "Don\u00b7ner", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir schie\u00dfen rosenroth,", "tokens": ["Wir", "schie\u00b7\u00dfen", "ro\u00b7sen\u00b7roth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenns Blut uns in die Augen l\u00e4uft,", "tokens": ["Wenns", "Blut", "uns", "in", "die", "Au\u00b7gen", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sind wir sternhagelvoll.", "tokens": ["Sind", "wir", "stern\u00b7ha\u00b7gel\u00b7voll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da heists: Husaren insgemein", "tokens": ["Da", "heists", ":", "Hu\u00b7sa\u00b7ren", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schlagt die Pistolen an,", "tokens": ["Schlagt", "die", "Pis\u00b7to\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Greift durch, den S\u00e4bel in der Hand", "tokens": ["Greift", "durch", ",", "den", "S\u00e4\u00b7bel", "in", "der", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Haut durch den n\u00e4chsten Mann.", "tokens": ["Haut", "durch", "den", "n\u00e4chs\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wenn ihr das Fransche nicht versteht,", "tokens": ["Wenn", "ihr", "das", "Fran\u00b7sche", "nicht", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So macht es euch bequem,", "tokens": ["So", "macht", "es", "euch", "be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Reden ihm sogleich vergeht,", "tokens": ["Das", "Re\u00b7den", "ihm", "sog\u00b7leich", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ihr den Kopf abm\u00e4ht.", "tokens": ["Wie", "ihr", "den", "Kopf", "ab\u00b7m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn gleich mein treuer Kammerad,", "tokens": ["Wenn", "gleich", "mein", "treu\u00b7er", "Kam\u00b7me\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df bleiben in dem Streit,", "tokens": ["Mu\u00df", "blei\u00b7ben", "in", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Husaren fragen nichts darnach,", "tokens": ["Hu\u00b7sa\u00b7ren", "fra\u00b7gen", "nichts", "dar\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PAV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sind auch dazu bereit;", "tokens": ["Sind", "auch", "da\u00b7zu", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Leib verweset in der Gruft,", "tokens": ["Der", "Leib", "ver\u00b7we\u00b7set", "in", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Rock bleibt in der Welt,", "tokens": ["Der", "Rock", "bleibt", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Seele schwingt sich durch die Luft", "tokens": ["Die", "See\u00b7le", "schwingt", "sich", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ins blaue Himmelszelt.", "tokens": ["Ins", "blau\u00b7e", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}