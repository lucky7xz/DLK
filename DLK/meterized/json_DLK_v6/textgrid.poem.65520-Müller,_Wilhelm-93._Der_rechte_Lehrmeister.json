{"textgrid.poem.65520": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "93. Der rechte Lehrmeister", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Folg' als J\u00fcnger nicht dem Lehrer, dessen Saal ist immer voll,", "tokens": ["Fol\u00b7g'", "als", "J\u00fcn\u00b7ger", "nicht", "dem", "Leh\u00b7rer", ",", "des\u00b7sen", "Saal", "ist", "im\u00b7mer", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "PTKNEG", "ART", "NN", "$,", "PRELAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Weil im Spiel er alle Sch\u00fcler zu Doktoren machen soll.", "tokens": ["Weil", "im", "Spiel", "er", "al\u00b7le", "Sch\u00fc\u00b7ler", "zu", "Dok\u00b7to\u00b7ren", "ma\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "PIAT", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der mit M\u00fch' dem Doktor lehret, da\u00df er nur ein Sch\u00fcler ist,", "tokens": ["Der", "mit", "M\u00fch'", "dem", "Dok\u00b7tor", "leh\u00b7ret", ",", "da\u00df", "er", "nur", "ein", "Sch\u00fc\u00b7ler", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dessen kleine Pforte suche, eh' zu gro\u00df du worden bist.", "tokens": ["Des\u00b7sen", "klei\u00b7ne", "Pfor\u00b7te", "su\u00b7che", ",", "eh'", "zu", "gro\u00df", "du", "wor\u00b7den", "bist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PTKA", "ADJD", "PPER", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}