{"dta.poem.616": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Jacobus   des Andern/ gewesenen K\u00f6nigs  \n in Engelland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT war ich nicht getreu/ wie jener Jacob war", "tokens": ["GoTT", "war", "ich", "nicht", "ge\u00b7treu", "/", "wie", "je\u00b7ner", "Ja\u00b7cob", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "ADJD", "$(", "KOKOM", "PDAT", "NE", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum muste ich zur Straff mein Engelland verlassen.", "tokens": ["Drum", "mus\u00b7te", "ich", "zur", "Straff", "mein", "En\u00b7gel\u00b7land", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Pabst und Franckreich gab mir Volck und Geld zwar dar/", "tokens": ["Der", "Pabst", "und", "Fran\u00b7ck\u00b7reich", "gab", "mir", "Volck", "und", "Geld", "zwar", "dar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NE", "VVFIN", "PPER", "NN", "KON", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch dem hilfft keiner nicht/ den GOTT und Himmel hassen.", "tokens": ["Doch", "dem", "hilfft", "kei\u00b7ner", "nicht", "/", "den", "GoTT", "und", "Him\u00b7mel", "has\u00b7sen", "."], "token_i