{"textgrid.poem.49732": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Niederbayrischer Kooperator", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir, mir, mir Niedaboarn,", "tokens": ["Mir", ",", "mir", ",", "mir", "Nied\u00b7a\u00b7bo\u00b7arn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hamm a den Eid sch\u00f6 g'schwoarn,", "tokens": ["Hamm", "a", "den", "Eid", "sch\u00f6", "g'\u00b7schwoarn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "D\u00f6s hot si glei geb\u00fcahrt,", "tokens": ["D\u00f6s", "hot", "si", "glei", "ge\u00b7b\u00fcahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df 'n a jeda schw\u00fcart.", "tokens": ["Da\u00df", "'n", "a", "je\u00b7da", "schw\u00fcart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Net g'rad mit oana Hand,", "tokens": ["Net", "g'\u00b7rad", "mit", "o\u00b7a\u00b7na", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit all zwoa mitanand,", "tokens": ["Mit", "all", "zwoa", "mi\u00b7ta\u00b7nand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "G'wasch'n san s' aa net g'wen.", "tokens": ["G'\u00b7wasch'n", "san", "s'", "aa", "net", "g'\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Zweg'na wos denn?", "tokens": ["Zweg'\u00b7na", "wos", "denn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mir, mir, mir Niedaboarn,", "tokens": ["Mir", ",", "mir", ",", "mir", "Nied\u00b7a\u00b7bo\u00b7arn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "M\u00fca\u00df ma'r an Himmi foahrn,", "tokens": ["M\u00fc\u00b7a\u00df", "ma'r", "an", "Him\u00b7mi", "fo\u00b7ahrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Pfeigr\u00e4d als geischtli Herrn,", "tokens": ["Pfei\u00b7gr\u00e4d", "als", "geischt\u00b7li", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "D\u00f6s hamm mir alle gern;", "tokens": ["D\u00f6s", "hamm", "mir", "al\u00b7le", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sagt da Papscht, was a wui,", "tokens": ["Sagt", "da", "Papscht", ",", "was", "a", "wui", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,", "PWS", "FM", "FM", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Ins is koan Eid net z'vui,", "tokens": ["Ins", "is", "koan", "Eid", "net", "z'\u00b7vui", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mir hamm koan Wiss'nsdurscht,", "tokens": ["Mir", "hamm", "koan", "Wiss'ns\u00b7durscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ins is all's wurscht.", "tokens": ["Ins", "is", "all's", "wurscht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Mir, mir, mir Niedaboarn,", "tokens": ["Mir", ",", "mir", ",", "mir", "Nied\u00b7a\u00b7bo\u00b7arn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So san ma'r aufzog'n woarn,", "tokens": ["So", "san", "ma'r", "auf\u00b7zo\u00b7g'n", "woarn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df ma koan Angst net g'sp\u00fcart,", "tokens": ["Da\u00df", "ma", "koan", "Angst", "net", "g'\u00b7sp\u00fcart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "NE", "NE", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Da\u00df si koa Zweifi r\u00fcahrt,", "tokens": ["Da\u00df", "si", "koa", "Zwei\u00b7fi", "r\u00fc\u00b7ahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM.it", "FM.it", "FM.it", "VVFIN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Da\u00df mir scho allesamm", "tokens": ["Da\u00df", "mir", "scho", "al\u00b7le\u00b7samm"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "XY", "XY"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "So an sch\u00f6n Glaab'n hamm,", "tokens": ["So", "an", "sch\u00f6n", "Glaab'n", "hamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Da gibt's scho gar nix mehr.", "tokens": ["Da", "gibt's", "scho", "gar", "nix", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Hau a Pris her!", "tokens": ["Hau", "a", "Pris", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "PTKVZ", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}