{"textgrid.poem.39424": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier schlugen dir die Flammen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier schlugen dir die Flammen", "tokens": ["Hier", "schlu\u00b7gen", "dir", "die", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch \u00fcberm Haupt zusammen,", "tokens": ["Hoch", "\u00fc\u00b7berm", "Haupt", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du heil'ges Heldenweib!", "tokens": ["Du", "heil'\u00b7ges", "Hel\u00b7den\u00b7weib", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch ihrer Lohe Qualmen", "tokens": ["Doch", "ih\u00b7rer", "Lo\u00b7he", "Qual\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durchwehten Himmelspalmen,", "tokens": ["Durch\u00b7weh\u00b7ten", "Him\u00b7mel\u00b7spal\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "K\u00fchlend den reinen Leib.", "tokens": ["K\u00fch\u00b7lend", "den", "rei\u00b7nen", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Kein Wahn war das Gesichte,", "tokens": ["Kein", "Wahn", "war", "das", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das einst in innerm Lichte", "tokens": ["Das", "einst", "in", "in\u00b7nerm", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dich machte glaubensstark:", "tokens": ["Dich", "mach\u00b7te", "glau\u00b7bens\u00b7stark", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie 's dich zum Sieg erlesen,", "tokens": ["Wie", "'s", "dich", "zum", "Sieg", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollt du im Tod genesen,", "tokens": ["Sollt", "du", "im", "Tod", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Edle Johanna d'Arc!", "tokens": ["Ed\u00b7le", "Jo\u00b7han\u00b7na", "d'\u00b7A\u00b7rc", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit g\u00f6ttlichem Erbarmen", "tokens": ["Mit", "g\u00f6tt\u00b7li\u00b7chem", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und liebend offnen Armen", "tokens": ["Und", "lie\u00b7bend", "off\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Lehnt sich Maria hin,", "tokens": ["Lehnt", "sich", "Ma\u00b7ria", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und Engelsfl\u00fcgel schlagen", "tokens": ["Und", "En\u00b7gels\u00b7fl\u00fc\u00b7gel", "schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu ihr empor zu tragen", "tokens": ["Zu", "ihr", "em\u00b7por", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die holde Dulderin.", "tokens": ["Die", "hol\u00b7de", "Dul\u00b7de\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O falscher Lohn der Erde!", "tokens": ["O", "fal\u00b7scher", "Lohn", "der", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer strebt, da\u00df der ihm werde,", "tokens": ["Wer", "strebt", ",", "da\u00df", "der", "ihm", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie tr\u00fcgt ihn seine Wahl!", "tokens": ["Wie", "tr\u00fcgt", "ihn", "sei\u00b7ne", "Wahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Frankreich hat errettet,", "tokens": ["Die", "Fran\u00b7kreich", "hat", "er\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Seht hier sie festgekettet", "tokens": ["Seht", "hier", "sie", "fest\u00b7ge\u00b7ket\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am schn\u00f6den Marterpfahl.", "tokens": ["Am", "schn\u00f6\u00b7den", "Mar\u00b7ter\u00b7pfahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zum Dank f\u00fcr hohe Thaten", "tokens": ["Zum", "Dank", "f\u00fcr", "ho\u00b7he", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom eignen Volk verrathen", "tokens": ["Vom", "eig\u00b7nen", "Volk", "ver\u00b7ra\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gespielt in Feindes Hand;", "tokens": ["Ge\u00b7spielt", "in", "Fein\u00b7des", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Beschuldigt und gerichtet", "tokens": ["Be\u00b7schul\u00b7digt", "und", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach dem, was Bosheit dichtet,", "tokens": ["Nach", "dem", ",", "was", "Bos\u00b7heit", "dich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In L\u00fcgenkunst gewandt.", "tokens": ["In", "L\u00fc\u00b7gen\u00b7kunst", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dennoch die Magd unschuldig", "tokens": ["Den\u00b7noch", "die", "Magd", "un\u00b7schul\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blieb standhaft und geduldig,", "tokens": ["Blieb", "stand\u00b7haft", "und", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Err\u00f6thend nur der Schmach,", "tokens": ["Er\u00b7r\u00f6\u00b7thend", "nur", "der", "Schmach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie sich von w\u00fcsten Rotten", "tokens": ["Wie", "sich", "von", "w\u00fcs\u00b7ten", "Rot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "APPR", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Heiland lie\u00df verspotten,", "tokens": ["Der", "Hei\u00b7land", "lie\u00df", "ver\u00b7spot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und betend f\u00fcr sie sprach.", "tokens": ["Und", "be\u00b7tend", "f\u00fcr", "sie", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die\u00df ist der Arm, der muthig", "tokens": ["Die\u00df", "ist", "der", "Arm", ",", "der", "mut\u00b7hig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Banner trug, doch blutig", "tokens": ["Das", "Ban\u00b7ner", "trug", ",", "doch", "blu\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gef\u00e4rbet nie das Schwert.", "tokens": ["Ge\u00b7f\u00e4r\u00b7bet", "nie", "das", "Schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die\u00df ist die Brust, das Herze,", "tokens": ["Die\u00df", "ist", "die", "Brust", ",", "das", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So schwellend unterm Erze", "tokens": ["So", "schwel\u00b7lend", "un\u00b7term", "Er\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur keuschen Trieb gen\u00e4hrt.", "tokens": ["Nur", "keu\u00b7schen", "Trieb", "ge\u00b7n\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Verflogen nun zu Aschen,", "tokens": ["Ver\u00b7flo\u00b7gen", "nun", "zu", "A\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Flu\u00df hinweggewaschen.", "tokens": ["Vom", "Flu\u00df", "hin\u00b7weg\u00b7ge\u00b7wa\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus diesem S\u00fcndenland;", "tokens": ["Aus", "die\u00b7sem", "S\u00fcn\u00b7den\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des hohen Geistes Spuren", "tokens": ["Des", "ho\u00b7hen", "Geis\u00b7tes", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus den erl\u00f6s'ten Fluren", "tokens": ["Aus", "den", "er\u00b7l\u00f6s'\u00b7ten", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Leichtsinn l\u00e4ngst verbannt.", "tokens": ["Vom", "Leicht\u00b7sinn", "l\u00e4ngst", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Dichter, nein, ein Schm\u00e4her", "tokens": ["Ein", "Dich\u00b7ter", ",", "nein", ",", "ein", "Schm\u00e4\u00b7her"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PTKANT", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der frommen Gottesseher,", "tokens": ["Der", "from\u00b7men", "Got\u00b7tes\u00b7se\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00f6hnt das reine Weib:", "tokens": ["Ver\u00b7h\u00f6hnt", "das", "rei\u00b7ne", "Weib", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Glorie der Geschichte", "tokens": ["Die", "Glo\u00b7rie", "der", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dient euch im Schandgedichte", "tokens": ["Dient", "euch", "im", "Schand\u00b7ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu eklem Zeitvertreib.", "tokens": ["Zu", "ek\u00b7lem", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "F\u00fchllos Geschlecht, verme\u00dfen", "tokens": ["F\u00fchl\u00b7los", "Ge\u00b7schlecht", ",", "ver\u00b7me\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "NN", "$,", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "In eitelm Selbstverge\u00dfen", "tokens": ["In", "ei\u00b7telm", "Selbst\u00b7ver\u00b7ge\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und kalter Schwindelei!", "tokens": ["Und", "kal\u00b7ter", "Schwin\u00b7de\u00b7lei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Treue fremd, dem Rechte,", "tokens": ["Der", "Treu\u00b7e", "fremd", ",", "dem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bald Dr\u00e4nger und bald Knechte,", "tokens": ["Bald", "Dr\u00e4n\u00b7ger", "und", "bald", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch niemals mild' und frei!", "tokens": ["Doch", "nie\u00b7mals", "mild'", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}