{"textgrid.poem.34800": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Beginn schuf Gott die Sonne,", "tokens": ["Im", "Be\u00b7ginn", "schuf", "Gott", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann die n\u00e4chtlichen Gestirne;", "tokens": ["Dann", "die", "n\u00e4cht\u00b7li\u00b7chen", "Ge\u00b7stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hierauf schuf er auch die Ochsen,", "tokens": ["Hier\u00b7auf", "schuf", "er", "auch", "die", "Och\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Aus dem Schwei\u00dfe seiner Stirne.", "tokens": ["Aus", "dem", "Schwei\u00b7\u00dfe", "sei\u00b7ner", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sp\u00e4ter schuf er wilde Bestien,", "tokens": ["Sp\u00e4\u00b7ter", "schuf", "er", "wil\u00b7de", "Be\u00b7sti\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "L\u00f6wen mit den grimmen Tatzen;", "tokens": ["L\u00f6\u00b7wen", "mit", "den", "grim\u00b7men", "Tat\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach des L\u00f6wen Ebenbilde", "tokens": ["Nach", "des", "L\u00f6\u00b7wen", "E\u00b7ben\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuf er h\u00fcbsche kleine Katzen.", "tokens": ["Schuf", "er", "h\u00fcb\u00b7sche", "klei\u00b7ne", "Kat\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zur Bev\u00f6lkerung der Wildnis", "tokens": ["Zur", "Be\u00b7v\u00f6l\u00b7ke\u00b7rung", "der", "Wild\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward hernach der Mensch erschaffen;", "tokens": ["Ward", "her\u00b7nach", "der", "Mensch", "er\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach des Menschen holdem Bildnis", "tokens": ["Nach", "des", "Men\u00b7schen", "hol\u00b7dem", "Bild\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuf er intressante Affen.", "tokens": ["Schuf", "er", "in\u00b7tres\u00b7san\u00b7te", "Af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Satan sah dem zu und lachte:", "tokens": ["Sa\u00b7tan", "sah", "dem", "zu", "und", "lach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "APPR", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbei, der Herr kopiert sich selber!", "tokens": ["\u00bb", "ei", ",", "der", "Herr", "ko\u00b7piert", "sich", "sel\u00b7ber", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ART", "NN", "VVFIN", "PRF", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach dem Bilde seiner Ochsen", "tokens": ["Nach", "dem", "Bil\u00b7de", "sei\u00b7ner", "Och\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht er noch am Ende K\u00e4lber!\u00ab", "tokens": ["Macht", "er", "noch", "am", "En\u00b7de", "K\u00e4l\u00b7ber", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}