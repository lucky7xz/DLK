{"textgrid.poem.26198": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Du findest die Menschen im Dunkel, wenn deine Augen warten", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Papierlaternen rote und gelbe, die h\u00e4ngen im Ufergarten.", "tokens": ["Pa\u00b7pier\u00b7la\u00b7ter\u00b7nen", "ro\u00b7te", "und", "gel\u00b7be", ",", "die", "h\u00e4n\u00b7gen", "im", "U\u00b7ferg\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "KON", "ADJA", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Du findest die Menschen im Dunkel, wenn deine Augen warten.", "tokens": ["Du", "fin\u00b7dest", "die", "Men\u00b7schen", "im", "Dun\u00b7kel", ",", "wenn", "dei\u00b7ne", "Au\u00b7gen", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+--+--+-+-+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Manch Busch manchmal wie ein Mensch und zwei manchmal wie ein Busch.", "tokens": ["Manch", "Busch", "manch\u00b7mal", "wie", "ein", "Mensch", "und", "zwei", "manch\u00b7mal", "wie", "ein", "Busch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "KOKOM", "ART", "NN", "KON", "CARD", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-+----+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Aufblitzend im Himmel knattert laut einer Rakete Gehusch.", "tokens": ["Auf\u00b7blit\u00b7zend", "im", "Him\u00b7mel", "knat\u00b7tert", "laut", "ei\u00b7ner", "Ra\u00b7ke\u00b7te", "Ge\u00b7husch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "Alle Laternen mild leuchten, und wie die Weltkugeln sich dr\u00e4ngen", "tokens": ["Al\u00b7le", "La\u00b7ter\u00b7nen", "mild", "leuch\u00b7ten", ",", "und", "wie", "die", "Welt\u00b7ku\u00b7geln", "sich", "dr\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "VVFIN", "$,", "KON", "PWAV", "ART", "NN", "PRF", "VVFIN"], "meter": "--+---+--+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und wie die bunten Gestirne nahe den Ufern h\u00e4ngen.", "tokens": ["Und", "wie", "die", "bun\u00b7ten", "Ge\u00b7stir\u00b7ne", "na\u00b7he", "den", "U\u00b7fern", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Es spielt die Nacht mit Feuern, und alle Wesen verschwanden,", "tokens": ["Es", "spielt", "die", "Nacht", "mit", "Feu\u00b7ern", ",", "und", "al\u00b7le", "We\u00b7sen", "ver\u00b7schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Und nur Papierlaternen, wie Monde, im Garten entstanden;", "tokens": ["Und", "nur", "Pa\u00b7pier\u00b7la\u00b7ter\u00b7nen", ",", "wie", "Mon\u00b7de", ",", "im", "Gar\u00b7ten", "ent\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "PWAV", "NE", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Hell, manchmal in einem Kahne, zwei Menschengesichter landen.", "tokens": ["Hell", ",", "manch\u00b7mal", "in", "ei\u00b7nem", "Kah\u00b7ne", ",", "zwei", "Men\u00b7schen\u00b7ge\u00b7sich\u00b7ter", "lan\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "APPR", "ART", "NN", "$,", "CARD", "NN", "VVINF", "$."], "meter": "-+--+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}