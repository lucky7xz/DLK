{"textgrid.poem.34519": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mich selber hab ich nun bezwungen", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich selber hab ich nun bezwungen", "tokens": ["Mich", "sel\u00b7ber", "hab", "ich", "nun", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und bin ein halber Engel schon,", "tokens": ["und", "bin", "ein", "hal\u00b7ber", "En\u00b7gel", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Palme hab ich mir errungen,", "tokens": ["die", "Pal\u00b7me", "hab", "ich", "mir", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "geniesse meiner Reinheit Lohn!", "tokens": ["ge\u00b7nies\u00b7se", "mei\u00b7ner", "Rein\u00b7heit", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O S\u00fcnde an der heilgen Jugend,", "tokens": ["O", "S\u00fcn\u00b7de", "an", "der", "heil\u00b7gen", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der toten Sitte schn\u00f6der Sieg,", "tokens": ["der", "to\u00b7ten", "Sit\u00b7te", "schn\u00f6\u00b7der", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "o schm\u00e4hlicher Triumph der Tugend", "tokens": ["o", "schm\u00e4h\u00b7li\u00b7cher", "Tri\u00b7umph", "der", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "in feigem, w\u00fcrdelosem Krieg!", "tokens": ["in", "fei\u00b7gem", ",", "w\u00fcr\u00b7de\u00b7lo\u00b7sem", "Krieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was frommt es, selber sich zu zwingen,", "tokens": ["Was", "frommt", "es", ",", "sel\u00b7ber", "sich", "zu", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da schon die Welt genug uns zwingt,", "tokens": ["da", "schon", "die", "Welt", "ge\u00b7nug", "uns", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "was frommt es, Opfer ihr zu bringen,", "tokens": ["was", "frommt", "es", ",", "Op\u00b7fer", "ihr", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die selber uns zum Opfer bringt!", "tokens": ["die", "sel\u00b7ber", "uns", "zum", "Op\u00b7fer", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}