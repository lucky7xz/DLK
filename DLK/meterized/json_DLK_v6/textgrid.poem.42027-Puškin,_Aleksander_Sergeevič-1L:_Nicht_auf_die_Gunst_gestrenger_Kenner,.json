{"textgrid.poem.42027": {"metadata": {"author": {"name": "Pu\u0161kin, Aleksander Sergeevi\u010d", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht auf die Gunst gestrenger Kenner,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht auf die Gunst gestrenger Kenner,", "tokens": ["Nicht", "auf", "die", "Gunst", "ge\u00b7stren\u00b7ger", "Ken\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf warmen Anteil nur bedacht,", "tokens": ["Auf", "war\u00b7men", "An\u00b7teil", "nur", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei dir allein, als treuem G\u00f6nner,", "tokens": ["Sei", "dir", "al\u00b7lein", ",", "als", "treu\u00b7em", "G\u00f6n\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Pfand der Freundschaft dargebracht.", "tokens": ["Dies", "Pfand", "der", "Freund\u00b7schaft", "dar\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir, dessen Geist seit Jugendtagen,", "tokens": ["Dir", ",", "des\u00b7sen", "Geist", "seit", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von heil'ger Phantasie belebt", "tokens": ["Von", "heil'\u00b7ger", "Phan\u00b7ta\u00b7sie", "be\u00b7lebt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und von der Dichtkunst Hauch getragen,", "tokens": ["Und", "von", "der", "Dicht\u00b7kunst", "Hauch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In lautrem Ernst zur H\u00f6he strebt.", "tokens": ["In", "laut\u00b7rem", "Ernst", "zur", "H\u00f6\u00b7he", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wohlan denn, la\u00df ihn dir behagen,", "tokens": ["Wo\u00b7hlan", "denn", ",", "la\u00df", "ihn", "dir", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVIMP", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Den anspruchslosen, bunten Strau\u00df", "tokens": ["Den", "an\u00b7spruchs\u00b7lo\u00b7sen", ",", "bun\u00b7ten", "Strau\u00df"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Von oft so tr\u00fcb', oft heitren Kl\u00e4ngen,", "tokens": ["Von", "oft", "so", "tr\u00fcb'", ",", "oft", "hei\u00b7tren", "Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "VVFIN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Volksweisen, Idealges\u00e4ngen,", "tokens": ["Volks\u00b7wei\u00b7sen", ",", "I\u00b7deal\u00b7ge\u00b7s\u00e4n\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Wie meinem Hirn sie wirr und kraus", "tokens": ["Wie", "mei\u00b7nem", "Hirn", "sie", "wirr", "und", "kraus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bei fl\u00fccht'gem Musenspiel entsprossen:", "tokens": ["Bei", "fl\u00fccht'\u00b7gem", "Mu\u00b7sen\u00b7spiel", "ent\u00b7spros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Aus Tr\u00e4umen ferner Jugendzeit,", "tokens": ["Aus", "Tr\u00e4u\u00b7men", "fer\u00b7ner", "Ju\u00b7gend\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dem Unmut bittrer Lebensglossen", "tokens": ["Dem", "Un\u00b7mut", "bit\u00b7trer", "Le\u00b7bens\u00b7glos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Und meines Herzens tiefstem Leid!", "tokens": ["Und", "mei\u00b7nes", "Her\u00b7zens", "tiefs\u00b7tem", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}