{"textgrid.poem.58137": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "175. Immer dasselbe Werk", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein einzig Werk sei, Gott zu kontentieren", "tokens": ["Dein", "ein\u00b7zig", "Werk", "sei", ",", "Gott", "zu", "kon\u00b7ten\u00b7tie\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "$,", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dich von ihm in allem lassen f\u00fchren;", "tokens": ["Und", "dich", "von", "ihm", "in", "al\u00b7lem", "las\u00b7sen", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "APPR", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In allem Tun dein Werk dies eine sei,", "tokens": ["In", "al\u00b7lem", "Tun", "dein", "Werk", "dies", "ei\u00b7ne", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "PPOSAT", "NN", "PDS", "ART", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wirst du nicht zerstreut durch mancherlei!", "tokens": ["So", "wirst", "du", "nicht", "zer\u00b7streut", "durch", "man\u00b7cher\u00b7lei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "VVPP", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}