{"dta.poem.18991": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "18.  \n  An eine/ sich alt zu werden beklagende/  \n Sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Nein. Jhr seit noch nicht alt. So zart/ so sch\u00f6n/", "tokens": ["Nein", ".", "Ihr", "seit", "noch", "nicht", "alt", ".", "So", "zart", "/", "so", "sch\u00f6n", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "$.", "ADV", "ADJD", "$(", "ADV", "ADJD", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "so klar/", "tokens": ["so", "klar", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Pfleg ich stehts ewer flaisch/ farb vnd aug zuer-", "tokens": ["Pfleg", "ich", "stehts", "e\u00b7wer", "flaisch", "/", "farb", "vnd", "aug", "zu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "PPOSAT", "ADJD", "$(", "VVFIN", "KON", "NE", "TRUNC"], "meter": "+-+--++-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "fahren/", "tokens": ["fah\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Dz jhr mir billich jung. frisch/ hurtig/ s\u00fc\u00df ist zwar", "tokens": ["Dz", "jhr", "mir", "bil\u00b7lich", "jung", ".", "frisch", "/", "hur\u00b7tig", "/", "s\u00fc\u00df", "ist", "zwar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJD", "$.", "ADJD", "$(", "ADJD", "$(", "ADJD", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der glatten jugend Lieb/ vn\u0303 Fr\u00fcling vnsrer jahren.", "tokens": ["Der", "glat\u00b7ten", "ju\u00b7gend", "Lieb", "/", "v\u00f1", "Fr\u00fc\u00b7ling", "vns\u00b7rer", "jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df vnser Som\u0303er auch gantz liebreich sey/ ist wahr;", "tokens": ["Da\u00df", "vn\u00b7ser", "Som\u0303er", "auch", "gantz", "lieb\u00b7reich", "sey", "/", "ist", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VAFIN", "$(", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch ist die hitz so gro\u00df/ da\u00df sein gedranck zu", "tokens": ["Doch", "ist", "die", "hitz", "so", "gro\u00df", "/", "da\u00df", "sein", "ge\u00b7dranck", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "$(", "KOUS", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "spahren", "tokens": ["spah\u00b7ren"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Jhm kaum kan m\u00f6glich sein: Daher er matt vnd", "tokens": ["Jhm", "kaum", "kan", "m\u00f6g\u00b7lich", "sein", ":", "Da\u00b7her", "er", "matt", "vnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$.", "PAV", "PPER", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "bahr", "tokens": ["bahr"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Durch der Lieb starcke brunst in taussenten ge-", "tokens": ["Durch", "der", "Lieb", "star\u00b7cke", "brunst", "in", "taus\u00b7sen\u00b7ten", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "TRUNC"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "fahren.", "tokens": ["fah\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Wan nu/ weil noch zu jung/ fruchtlo\u00df die Fr\u00fchlings", "tokens": ["Wan", "nu", "/", "weil", "noch", "zu", "jung", "/", "frucht\u00b7lo\u00df", "die", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "$(", "KOUS", "ADV", "PTKA", "ADJD", "$(", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "zeit;", "tokens": ["zeit", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der Sommer vil zu heissz: Ist weder zu ver-", "tokens": ["Der", "Som\u00b7mer", "vil", "zu", "heissz", ":", "Ist", "we\u00b7der", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "$.", "VAFIN", "KON", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schweigen/", "tokens": ["schwei\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Noch zu erh\u00f6b\u1ebd gnug des Herbsts lust-reiche beut.", "tokens": ["Noch", "zu", "er\u00b7h\u00f6\u00b7b\u1ebd", "gnug", "des", "Herbsts", "lust\u00b7rei\u00b7che", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADV", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dan Er erg\u00f6tzet Vns mit so Lieb-reiffen Feigen/", "tokens": ["Dan", "Er", "er\u00b7g\u00f6t\u00b7zet", "Vns", "mit", "so", "Lie\u00b7breif\u00b7fen", "Fei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit solcher Wollusts frucht/ da\u00df er ohn allen", "tokens": ["Mit", "sol\u00b7cher", "Wol\u00b7lusts", "frucht", "/", "da\u00df", "er", "ohn", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "$(", "KOUS", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "streit", "tokens": ["streit"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die ander vbertreff/ jhm gnug an zweyen Zeugen.", "tokens": ["Die", "an\u00b7der", "vbert\u00b7reff", "/", "jhm", "gnug", "an", "zwe\u00b7yen", "Zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$(", "PPER", "ADV", "APPR", "VVFIN", "NN", "$."], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}