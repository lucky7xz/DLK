{"textgrid.poem.46922": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn dein Herz Gedanken nachh\u00e4ngt]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dein Herz Gedanken nachh\u00e4ngt,", "tokens": ["Wenn", "dein", "Herz", "Ge\u00b7dan\u00b7ken", "nach\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deren einer anderm nachdr\u00e4ngt,", "tokens": ["De\u00b7ren", "ei\u00b7ner", "an\u00b7derm", "nach\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wie es anders konnte gehn,", "tokens": ["Wie", "es", "an\u00b7ders", "konn\u00b7te", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als es leider ist geschehn,", "tokens": ["Als", "es", "lei\u00b7der", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie, was so ein Zufall wendet,", "tokens": ["Wie", ",", "was", "so", "ein", "Zu\u00b7fall", "wen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Anders konnt' ein Zufall wenden;", "tokens": ["An\u00b7ders", "konnt'", "ein", "Zu\u00b7fall", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Werden nie die Zweifel enden,", "tokens": ["Wer\u00b7den", "nie", "die", "Zwei\u00b7fel", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie nicht dein Glauben endet:", "tokens": ["Wenn", "sie", "nicht", "dein", "Glau\u00b7ben", "en\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nicht dem Zufall preisgegeben", "tokens": ["Nicht", "dem", "Zu\u00b7fall", "preis\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist des Menschen Lebensgl\u00fcck;", "tokens": ["Ist", "des", "Men\u00b7schen", "Le\u00b7bens\u00b7gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn der Zufall selbst ist eben", "tokens": ["Denn", "der", "Zu\u00b7fall", "selbst", "ist", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Von Nothwendigkeit ein St\u00fcck", "tokens": ["Von", "Noth\u00b7wen\u00b7dig\u00b7keit", "ein", "St\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "In den ewigen Geweben,", "tokens": ["In", "den", "e\u00b7wi\u00b7gen", "Ge\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Deren Netz die Welt umspannt,", "tokens": ["De\u00b7ren", "Netz", "die", "Welt", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Deren F\u00e4den unverwandt,", "tokens": ["De\u00b7ren", "F\u00e4\u00b7den", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Nach dem Plan dir unbekannt,", "tokens": ["Nach", "dem", "Plan", "dir", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "All aus einer Mitte streben", "tokens": ["All", "aus", "ei\u00b7ner", "Mit\u00b7te", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und in eine gehn zur\u00fcck.", "tokens": ["Und", "in", "ei\u00b7ne", "gehn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}