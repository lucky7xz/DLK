{"textgrid.poem.32646": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unerschrocken taucht der Fischer", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unerschrocken taucht der Fischer", "tokens": ["Un\u00b7er\u00b7schro\u00b7cken", "taucht", "der", "Fi\u00b7scher"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Meeresflut,", "tokens": ["In", "die", "Mee\u00b7res\u00b7flut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo auf tiefem, feuchtem Grunde", "tokens": ["Wo", "auf", "tie\u00b7fem", ",", "feuch\u00b7tem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Still die Perle ruht.", "tokens": ["Still", "die", "Per\u00b7le", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Gerne hab' ich volle Kr\u00fcge", "tokens": ["Ger\u00b7ne", "hab'", "ich", "vol\u00b7le", "Kr\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und vom Wein ein Meer,", "tokens": ["Und", "vom", "Wein", "ein", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sind in meinen Liedern Perlen,", "tokens": ["Sind", "in", "mei\u00b7nen", "Lie\u00b7dern", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Holt' ich da sie her.", "tokens": ["Holt'", "ich", "da", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da die Welt von S\u00fcnd' umnachtet,", "tokens": ["Da", "die", "Welt", "von", "S\u00fcnd'", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam die Sintflut wild,", "tokens": ["Kam", "die", "Sint\u00b7flut", "wild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die Nacht ward von den Wassern", "tokens": ["Und", "die", "Nacht", "ward", "von", "den", "Was\u00b7sern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00e4h hinweggesp\u00fclt!", "tokens": ["J\u00e4h", "hin\u00b7weg\u00b7ge\u00b7sp\u00fclt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Auch mein Herz ist oft umnachtet,", "tokens": ["Auch", "mein", "Herz", "ist", "oft", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch von Gram und Pein,", "tokens": ["Doch", "von", "Gram", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Machtlos aber ist das Wasser,", "tokens": ["Macht\u00b7los", "a\u00b7ber", "ist", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir hilft nur der Wein!", "tokens": ["Mir", "hilft", "nur", "der", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "M\u00fcde in der Sommerhitze", "tokens": ["M\u00fc\u00b7de", "in", "der", "Som\u00b7mer\u00b7hit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht das Bl\u00fcmelein,", "tokens": ["Steht", "das", "Bl\u00fc\u00b7me\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch ihm fl\u00f6\u00dft ein milder Regen", "tokens": ["Doch", "ihm", "fl\u00f6\u00dft", "ein", "mil\u00b7der", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neues Leben ein:", "tokens": ["Neu\u00b7es", "Le\u00b7ben", "ein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn verzagt ich und erm\u00fcdet,", "tokens": ["Wenn", "ver\u00b7zagt", "ich", "und", "er\u00b7m\u00fc\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht der Wein mich frei,", "tokens": ["Macht", "der", "Wein", "mich", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und nach einem feinen Tropfen", "tokens": ["Und", "nach", "ei\u00b7nem", "fei\u00b7nen", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gl\u00fcht's in mir aufs neu'.", "tokens": ["Gl\u00fcht's", "in", "mir", "aufs", "neu'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPRART", "ADJA", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ob Verstellung, ob Betr\u00fcbnis,", "tokens": ["Ob", "Ver\u00b7stel\u00b7lung", ",", "ob", "Be\u00b7tr\u00fcb\u00b7nis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was es immer ist,", "tokens": ["Was", "es", "im\u00b7mer", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alter Brauch, da\u00df um die Toten", "tokens": ["Al\u00b7ter", "Brauch", ",", "da\u00df", "um", "die", "To\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4nen man vergie\u00dft;", "tokens": ["Tr\u00e4\u00b7nen", "man", "ver\u00b7gie\u00dft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ich verbitte mir das Wasser:", "tokens": ["Ich", "ver\u00b7bit\u00b7te", "mir", "das", "Was\u00b7ser", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn es Tr\u00e4nen gilt,", "tokens": ["Wenn", "es", "Tr\u00e4\u00b7nen", "gilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "M\u00f6gen sie dem Kelch entstr\u00f6men,", "tokens": ["M\u00f6\u00b7gen", "sie", "dem", "Kelch", "ent\u00b7str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der mit Wein gef\u00fcllt!", "tokens": ["Der", "mit", "Wein", "ge\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}