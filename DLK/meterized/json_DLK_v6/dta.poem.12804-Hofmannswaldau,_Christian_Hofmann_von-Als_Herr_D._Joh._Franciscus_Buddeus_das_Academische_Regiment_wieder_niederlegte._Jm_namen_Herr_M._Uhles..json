{"dta.poem.12804": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dis ist die furcht, nach der er sich und die vertraute jugend", "tokens": ["Dis", "ist", "die", "furcht", ",", "nach", "der", "er", "sich", "und", "die", "ver\u00b7trau\u00b7te", "ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "KON", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "leitet,", "tokens": ["lei\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df sie nicht aus den schrancken schreitet.", "tokens": ["Da\u00df", "sie", "nicht", "aus", "den", "schran\u00b7cken", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}