{"textgrid.poem.38917": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlauf! es ruft der Sonnenschein", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf! es ruft der Sonnenschein", "tokens": ["Wohl\u00b7auf", "!", "es", "ruft", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus in Gottes freie Welt!", "tokens": ["Hin\u00b7aus", "in", "Got\u00b7tes", "frei\u00b7e", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht munter in das Land hinein", "tokens": ["Geht", "mun\u00b7ter", "in", "das", "Land", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wandelt \u00fcber Berg und Feld!", "tokens": ["Und", "wan\u00b7delt", "\u00fc\u00b7ber", "Berg", "und", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es bleibt der Strom nicht ruhig stehn,", "tokens": ["Es", "bleibt", "der", "Strom", "nicht", "ru\u00b7hig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar lustig rauscht er fort;", "tokens": ["Gar", "lus\u00b7tig", "rauscht", "er", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00f6rst du des Windes muntres Wehn?", "tokens": ["H\u00f6rst", "du", "des", "Win\u00b7des", "mun\u00b7tres", "Wehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er braust von Ort zu Ort.", "tokens": ["Er", "braust", "von", "Ort", "zu", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es reist der Mond wohl hin und her,", "tokens": ["Es", "reist", "der", "Mond", "wohl", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne ab und auf,", "tokens": ["Die", "Son\u00b7ne", "ab", "und", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Guckt \u00fcber'n Berg und geht in's Meer,", "tokens": ["Guckt", "\u00fc\u00b7ber'n", "Berg", "und", "geht", "in's", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie matt in ihrem Lauf.", "tokens": ["Nie", "matt", "in", "ih\u00b7rem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und, Mensch, du sitzest stets daheim,", "tokens": ["Und", ",", "Mensch", ",", "du", "sit\u00b7zest", "stets", "da\u00b7heim", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sehnst dich nach der Fern:", "tokens": ["Und", "sehnst", "dich", "nach", "der", "Fern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei frisch und wandle durch den Hain,", "tokens": ["Sei", "frisch", "und", "wand\u00b7le", "durch", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sich die Fremde gern.", "tokens": ["Und", "sich", "die", "Frem\u00b7de", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer wei\u00df, wo dir dein Gl\u00fccke bl\u00fcht,", "tokens": ["Wer", "wei\u00df", ",", "wo", "dir", "dein", "Gl\u00fc\u00b7cke", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So geh und such es nur,", "tokens": ["So", "geh", "und", "such", "es", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abend kommt, der Morgen flieht,", "tokens": ["Der", "A\u00b7bend", "kommt", ",", "der", "Mor\u00b7gen", "flieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Betrete bald die Spur.", "tokens": ["Be\u00b7tre\u00b7te", "bald", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00df Sorgen sein und Bangigkeit,", "tokens": ["La\u00df", "Sor\u00b7gen", "sein", "und", "Ban\u00b7gig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "VAINF", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist doch der Himmel blau,", "tokens": ["Ist", "doch", "der", "Him\u00b7mel", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wechselt Freude stets mit Leid,", "tokens": ["Es", "wech\u00b7selt", "Freu\u00b7de", "stets", "mit", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Gl\u00fccke nur vertrau.", "tokens": ["Dem", "Gl\u00fc\u00b7cke", "nur", "ver\u00b7trau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So weit dich schlie\u00dft der Himmel ein", "tokens": ["So", "weit", "dich", "schlie\u00dft", "der", "Him\u00b7mel", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ger\u00e4th der Liebe Frucht,", "tokens": ["Ge\u00b7r\u00e4\u00b7th", "der", "Lie\u00b7be", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und jedes Herz wird gl\u00fccklich sein,", "tokens": ["Und", "je\u00b7des", "Herz", "wird", "gl\u00fcck\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und finden was es sucht.", "tokens": ["Und", "fin\u00b7den", "was", "es", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}