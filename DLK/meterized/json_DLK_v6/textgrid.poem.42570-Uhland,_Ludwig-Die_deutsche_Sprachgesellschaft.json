{"textgrid.poem.42570": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Sprachgesellschaft", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gelehrte deutsche M\u00e4nner,", "tokens": ["Ge\u00b7lehr\u00b7te", "deut\u00b7sche", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der deutschen Rede Kenner,", "tokens": ["Der", "deut\u00b7schen", "Re\u00b7de", "Ken\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie reichen sich die Hand,", "tokens": ["Sie", "rei\u00b7chen", "sich", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Sprache zu ergr\u00fcnden,", "tokens": ["Die", "Spra\u00b7che", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu regeln und zu r\u00fcnden", "tokens": ["Zu", "re\u00b7geln", "und", "zu", "r\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In emsigem Verband.", "tokens": ["In", "em\u00b7si\u00b7gem", "Ver\u00b7band", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Indes nun diese walten,", "tokens": ["In\u00b7des", "nun", "die\u00b7se", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PDAT", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bestimmen und gestalten", "tokens": ["Be\u00b7stim\u00b7men", "und", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Sprache Form und Zier:", "tokens": ["Der", "Spra\u00b7che", "Form", "und", "Zier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So schaffe du inwendig,", "tokens": ["So", "schaf\u00b7fe", "du", "in\u00b7wen\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tatkr\u00e4ftig und lebendig,", "tokens": ["Tat\u00b7kr\u00e4f\u00b7tig", "und", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gesamtes Volk, an ihr!", "tokens": ["Ge\u00b7sam\u00b7tes", "Volk", ",", "an", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja! gib ihr du die Reinheit,", "tokens": ["Ja", "!", "gib", "ihr", "du", "die", "Rein\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVIMP", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Klarheit und die Feinheit,", "tokens": ["Die", "Klar\u00b7heit", "und", "die", "Fein\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die aus dem Herzen stammt!", "tokens": ["Die", "aus", "dem", "Her\u00b7zen", "stammt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gib ihr den Schwung, die St\u00e4rke,", "tokens": ["Gib", "ihr", "den", "Schwung", ",", "die", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Glut, an der man merke,", "tokens": ["Die", "Glut", ",", "an", "der", "man", "mer\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df sie vom Geiste flammt!", "tokens": ["Da\u00df", "sie", "vom", "Geis\u00b7te", "flammt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "An deiner Sprache r\u00fcge", "tokens": ["An", "dei\u00b7ner", "Spra\u00b7che", "r\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du sch\u00e4rfer nichts denn L\u00fcge,", "tokens": ["Du", "sch\u00e4r\u00b7fer", "nichts", "denn", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PIS", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wahrheit sei ihr Hort!", "tokens": ["Die", "Wahr\u00b7heit", "sei", "ihr", "Hort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verpflanz auf deine Jugend", "tokens": ["Ver\u00b7pflanz", "auf", "dei\u00b7ne", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die deutsche Treu und Tugend", "tokens": ["Die", "deut\u00b7sche", "Treu", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zugleich mit deutschem Wort!", "tokens": ["Zu\u00b7gleich", "mit", "deut\u00b7schem", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu buhlerischem Girren", "tokens": ["Zu", "buh\u00b7le\u00b7ri\u00b7schem", "Gir\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "La\u00df du ihn niemals kirren,", "tokens": ["La\u00df", "du", "ihn", "nie\u00b7mals", "kir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der ernsten Sprache Klang!", "tokens": ["Der", "erns\u00b7ten", "Spra\u00b7che", "Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie sei dir Wort der Treue,", "tokens": ["Sie", "sei", "dir", "Wort", "der", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sei Stimme zarter Scheue,", "tokens": ["Sei", "Stim\u00b7me", "zar\u00b7ter", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sei echter Minne Sang!", "tokens": ["Sei", "ech\u00b7ter", "Min\u00b7ne", "Sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie diene nie am Hofe", "tokens": ["Sie", "die\u00b7ne", "nie", "am", "Ho\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als Gauklerin, als Zofe,", "tokens": ["Als", "Gauk\u00b7le\u00b7rin", ",", "als", "Zo\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Lispeln taugt ihr nicht;", "tokens": ["Das", "Lis\u00b7peln", "taugt", "ihr", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie t\u00f6ne stolz, sie weihe", "tokens": ["Sie", "t\u00f6\u00b7ne", "stolz", ",", "sie", "wei\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sich dahin, wo der Freie", "tokens": ["Sich", "da\u00b7hin", ",", "wo", "der", "Frei\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PAV", "$,", "PWAV", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "F\u00fcr Recht, f\u00fcr Freiheit spricht!", "tokens": ["F\u00fcr", "Recht", ",", "f\u00fcr", "Frei\u00b7heit", "spricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn so der Sprache Mehrung,", "tokens": ["Wenn", "so", "der", "Spra\u00b7che", "Meh\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verbesserung und Kl\u00e4rung", "tokens": ["Ver\u00b7bes\u00b7se\u00b7rung", "und", "Kl\u00e4\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei dir vonstatten geht:", "tokens": ["Bei", "dir", "von\u00b7stat\u00b7ten", "geht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wird man sagen m\u00fcssen,", "tokens": ["So", "wird", "man", "sa\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df, wo sich Deutsche gr\u00fc\u00dfen,", "tokens": ["Da\u00df", ",", "wo", "sich", "Deut\u00b7sche", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Atem Gottes weht.", "tokens": ["Der", "A\u00b7tem", "Got\u00b7tes", "weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}