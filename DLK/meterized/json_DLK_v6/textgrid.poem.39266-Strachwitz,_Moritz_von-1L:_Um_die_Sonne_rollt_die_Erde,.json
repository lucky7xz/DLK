{"textgrid.poem.39266": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um die Sonne rollt die Erde,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um die Sonne rollt die Erde,", "tokens": ["Um", "die", "Son\u00b7ne", "rollt", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rasch im ruhelosen Wirken,", "tokens": ["Rasch", "im", "ru\u00b7he\u00b7lo\u00b7sen", "Wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es immer lichter werde", "tokens": ["Da\u00df", "es", "im\u00b7mer", "lich\u00b7ter", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den irdischen Bezirken.", "tokens": ["In", "den", "ir\u00b7di\u00b7schen", "Be\u00b7zir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Um der Wahrheit Sonnenrunde", "tokens": ["Um", "der", "Wahr\u00b7heit", "Son\u00b7nen\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kreist die Menschheit la\u00df und schleichend", "tokens": ["Kreist", "die", "Menschheit", "la\u00df", "und", "schlei\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVIMP", "KON", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Einen Zoll in jeder Stunde,", "tokens": ["Ei\u00b7nen", "Zoll", "in", "je\u00b7der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwitzend, st\u00f6hnend, m\u00fcrrisch keuchend.", "tokens": ["Schwit\u00b7zend", ",", "st\u00f6h\u00b7nend", ",", "m\u00fcr\u00b7risch", "keu\u00b7chend", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die Erde, die gewalt'ge,", "tokens": ["Und", "die", "Er\u00b7de", ",", "die", "ge\u00b7walt'\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Kann die Sonne nicht erringen:", "tokens": ["Kann", "die", "Son\u00b7ne", "nicht", "er\u00b7rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wird der Mensch, der staubgestalt'ge,", "tokens": ["Wird", "der", "Mensch", ",", "der", "staub\u00b7ge\u00b7stalt'\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Je zum gro\u00dfen Ziele dringen?", "tokens": ["Je", "zum", "gro\u00b7\u00dfen", "Zie\u00b7le", "drin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}