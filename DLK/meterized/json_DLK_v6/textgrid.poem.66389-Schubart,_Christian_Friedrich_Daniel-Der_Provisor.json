{"textgrid.poem.66389": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der Provisor", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein liebes deutsches Vaterland", "tokens": ["Mein", "lie\u00b7bes", "deut\u00b7sches", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat bei so mancher Zierde,", "tokens": ["Hat", "bei", "so", "man\u00b7cher", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch keinen ehrenvollern Stand,", "tokens": ["Doch", "kei\u00b7nen", "eh\u00b7ren\u00b7vol\u00b7lern", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als die Provisorsw\u00fcrde.", "tokens": ["Als", "die", "Pro\u00b7vi\u00b7sors\u00b7w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum freu' ich mich", "tokens": ["Drum", "freu'", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Oft k\u00f6niglich,", "tokens": ["Oft", "k\u00f6\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wenn mich die Leute m\u00fcssen", "tokens": ["Wenn", "mich", "die", "Leu\u00b7te", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Herrn Provisor gr\u00fc\u00dfen.", "tokens": ["Als", "Herrn", "Pro\u00b7vi\u00b7sor", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar gibt's oft manchen sauren Tag,", "tokens": ["Zwar", "gibt's", "oft", "man\u00b7chen", "sau\u00b7ren", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Lehrstand hat viel Plagen;", "tokens": ["Der", "Lehr\u00b7stand", "hat", "viel", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gibt man dem Buben einen Schlag,", "tokens": ["Gibt", "man", "dem", "Bu\u00b7ben", "ei\u00b7nen", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich geht's an ein Verklagen.", "tokens": ["Gleich", "geht's", "an", "ein", "Ver\u00b7kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das A, be, ab,", "tokens": ["Das", "A", ",", "be", ",", "ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NE", "$,", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Das E, be, eb,", "tokens": ["Das", "E", ",", "be", ",", "eb", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Das tr\u00e4ge Buchstabiren,", "tokens": ["Das", "tr\u00e4\u00b7ge", "Buch\u00b7sta\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kann weidlich uns vexiren.", "tokens": ["Kann", "weid\u00b7lich", "uns", "ve\u00b7xi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch welch ein Seelengaudium!", "tokens": ["Doch", "welch", "ein", "See\u00b7len\u00b7gau\u00b7di\u00b7um", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich die Orgel spiele.", "tokens": ["Wenn", "ich", "die", "Or\u00b7gel", "spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und weidlich im Pr\u00e4ludium", "tokens": ["Und", "weid\u00b7lich", "im", "Pr\u00e4\u00b7lu\u00b7di\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit H\u00e4nd' und F\u00fc\u00dfen w\u00fchle.", "tokens": ["Mit", "H\u00e4nd'", "und", "F\u00fc\u00b7\u00dfen", "w\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Dudeldum", "tokens": ["Mein", "Du\u00b7del\u00b7dum"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kann weit herum,", "tokens": ["Kann", "weit", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Doch ohne Ruhm zu melden,", "tokens": ["Doch", "oh\u00b7ne", "Ruhm", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Virtuos mir schelten.", "tokens": ["Kein", "Vir\u00b7tu\u00b7os", "mir", "schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ha! welche Freude wird's erst sein,", "tokens": ["Ha", "!", "wel\u00b7che", "Freu\u00b7de", "wird's", "erst", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAT", "NN", "VAFIN", "ADV", "VAINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wenn ich ", "tokens": ["Wenn", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "++", "measure": "spondeus"}, "line.3": {"text": "Und mich im schwarzen Rocke fein", "tokens": ["Und", "mich", "im", "schwar\u00b7zen", "Ro\u00b7cke", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein Pr\u00e4lat geberde.", "tokens": ["Wie", "ein", "Pr\u00e4\u00b7lat", "ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Weib dazu", "tokens": ["Ein", "Weib", "da\u00b7zu"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zur Pfleg' und Ruh',", "tokens": ["Zur", "Pfleg'", "und", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Macht mir das Leben s\u00fc\u00dfer.", "tokens": ["Macht", "mir", "das", "Le\u00b7ben", "s\u00fc\u00b7\u00dfer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Es leben die Proviser!", "tokens": ["Es", "le\u00b7ben", "die", "Pro\u00b7vi\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}