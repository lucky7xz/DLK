{"dta.poem.20030": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Winter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stets, wann Winter und Sturm unfreundlich tobt auf der", "tokens": ["Stets", ",", "wann", "Win\u00b7ter", "und", "Sturm", "un\u00b7freund\u00b7lich", "tobt", "auf", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN", "ADJD", "VVFIN", "APPR", "ART"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Erde,", "tokens": ["Er\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Glaub' ich, o Liebe! Du seyst doppelt entfernet von", "tokens": ["Glaub'", "ich", ",", "o", "Lie\u00b7be", "!", "Du", "seyst", "dop\u00b7pelt", "ent\u00b7fer\u00b7net", "von"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$.", "PPER", "VAFIN", "ADJD", "VVFIN", "APPR"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "mir,", "tokens": ["mir", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Aber, wann Fr\u00fchling und Lust, wann Sonn' und Mond", "tokens": ["A\u00b7ber", ",", "wann", "Fr\u00fch\u00b7ling", "und", "Lust", ",", "wann", "Sonn'", "und", "Mond"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "NN", "KON", "NN", "$,", "PWAV", "NN", "KON", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "mich umspielen,", "tokens": ["mich", "um\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Glaub' ich wohl alles, nur nicht, da\u00df Du so ferne mir", "tokens": ["Glaub'", "ich", "wohl", "al\u00b7les", ",", "nur", "nicht", ",", "da\u00df", "Du", "so", "fer\u00b7ne", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$,", "ADV", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "ADJA", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "bist.", "tokens": ["bist", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "-", "measure": "single.down"}}}}}