{"textgrid.poem.24122": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Sp\u00e4tsommerphilosophie", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ueber die Wiesen septembert der Wind,", "tokens": ["Ue\u00b7ber", "die", "Wie\u00b7sen", "sep\u00b7tem\u00b7bert", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Bl\u00e4tter wolln sich verf\u00e4rben;", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "wolln", "sich", "ver\u00b7f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jetzt gehts erst langsam, dann geschwind", "tokens": ["Jetzt", "gehts", "erst", "lang\u00b7sam", ",", "dann", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ans Sterben.", "tokens": ["Ans", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Unsinn! Jetzt wird die Welt erst bunt!", "tokens": ["Un\u00b7sinn", "!", "Jetzt", "wird", "die", "Welt", "erst", "bunt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Glaub nicht an die Unken!", "tokens": ["Glaub", "nicht", "an", "die", "Un\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wir sind verliebt und frech gesund;", "tokens": ["Wir", "sind", "ver\u00b7liebt", "und", "frech", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald wird getrunken!", "tokens": ["Bald", "wird", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Pa\u00dft auf: Das Jahr giebt guten Wein!", "tokens": ["Pa\u00dft", "auf", ":", "Das", "Jahr", "giebt", "gu\u00b7ten", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie lachen die Trauben!", "tokens": ["Wie", "la\u00b7chen", "die", "Trau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sonst mag ich kein Prophete sein;", "tokens": ["Sonst", "mag", "ich", "kein", "Pro\u00b7phe\u00b7te", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das k\u00f6nnt ihr mir glauben.", "tokens": ["Das", "k\u00f6nnt", "ihr", "mir", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und schlie\u00dflich: K\u00e4me der Senserich", "tokens": ["Und", "schlie\u00df\u00b7lich", ":", "K\u00e4\u00b7me", "der", "Sen\u00b7se\u00b7rich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit der Rippenweste", "tokens": ["Mit", "der", "Rip\u00b7pen\u00b7wes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und spr\u00e4che zu mir: Jetzt hol ich dich", "tokens": ["Und", "spr\u00e4\u00b7che", "zu", "mir", ":", "Jetzt", "hol", "ich", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom sch\u00e4umenden Feste.", "tokens": ["Vom", "sch\u00e4u\u00b7men\u00b7den", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Es w\u00e4re mir, ich gesteh es euch gern,", "tokens": ["Es", "w\u00e4\u00b7re", "mir", ",", "ich", "ge\u00b7steh", "es", "euch", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht eben erfreulich,", "tokens": ["Nicht", "e\u00b7ben", "er\u00b7freu\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Indessen, ich folgte dem kalkigen Herrn", "tokens": ["In\u00b7des\u00b7sen", ",", "ich", "folg\u00b7te", "dem", "kal\u00b7ki\u00b7gen", "Herrn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Getreulich.", "tokens": ["Ge\u00b7treu\u00b7lich", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Ich kenne den Mann \u00fcber drei\u00dfig Jahr;", "tokens": ["Ich", "ken\u00b7ne", "den", "Mann", "\u00fc\u00b7ber", "drei\u00b7\u00dfig", "Jahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es ist wohl der Gleiche,", "tokens": ["Es", "ist", "wohl", "der", "Glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Der damals so liebensw\u00fcrdig war,", "tokens": ["Der", "da\u00b7mals", "so", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mich in die Reiche", "tokens": ["Mich", "in", "die", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NE"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Des Lebens zu rufen aus wei\u00df nicht woher.", "tokens": ["Des", "Le\u00b7bens", "zu", "ru\u00b7fen", "aus", "wei\u00df", "nicht", "wo\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er scheint es zu lieben,", "tokens": ["Er", "scheint", "es", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Zwischen Sein und Nichtsein uns hin und her", "tokens": ["Zwi\u00b7schen", "Sein", "und", "Nich\u00b7tsein", "uns", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gem\u00e4chlich zu schieben.", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich", "zu", "schie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein etwas unverst\u00e4ndlicher Sport.", "tokens": ["Ein", "et\u00b7was", "un\u00b7ver\u00b7st\u00e4nd\u00b7li\u00b7cher", "Sport", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Sportsman indessen", "tokens": ["Der", "Sports\u00b7man", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Scheint m\u00e4chtig. Zu mucken wider sein Wort,", "tokens": ["Scheint", "m\u00e4ch\u00b7tig", ".", "Zu", "mu\u00b7cken", "wi\u00b7der", "sein", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das w\u00e4re vermessen.", "tokens": ["Das", "w\u00e4\u00b7re", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Was n\u00fctzt es der Kegelkugel, die", "tokens": ["Was", "n\u00fctzt", "es", "der", "Ke\u00b7gel\u00b7ku\u00b7gel", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich str\u00e4ubte, zu rollen?", "tokens": ["Sich", "str\u00e4ub\u00b7te", ",", "zu", "rol\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es giebt ein paar Punkte, da fragt man nicht, wie", "tokens": ["Es", "giebt", "ein", "paar", "Punk\u00b7te", ",", "da", "fragt", "man", "nicht", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "$,", "ADV", "VVFIN", "PIS", "PTKNEG", "$,", "PWAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wir W\u00fcrmerchen wollen.", "tokens": ["Wir", "W\u00fcr\u00b7mer\u00b7chen", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Und also sag ich: Der Wein wird gut", "tokens": ["Und", "al\u00b7so", "sag", "ich", ":", "Der", "Wein", "wird", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und werde getrunken!", "tokens": ["Und", "wer\u00b7de", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Schief setzt ich auf Hallodrioh meinen Hut", "tokens": ["Schief", "setzt", "ich", "auf", "Hal\u00b7lo\u00b7dri\u00b7oh", "mei\u00b7nen", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und pfeif auf die Unken.", "tokens": ["Und", "pfeif", "auf", "die", "Un\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}