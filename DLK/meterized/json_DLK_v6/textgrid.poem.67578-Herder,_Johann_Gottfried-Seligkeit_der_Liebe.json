{"textgrid.poem.67578": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Seligkeit der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, Liebe! la\u00df kein Mi\u00dfbehagen", "tokens": ["Auf", ",", "Lie\u00b7be", "!", "la\u00df", "kein", "Mi\u00df\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$.", "VVIMP", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Anfechten unsre Himmelsruh,", "tokens": ["An\u00b7fech\u00b7ten", "uns\u00b7re", "Him\u00b7mels\u00b7ruh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht Thorenfurcht noch Stolz uns plagen", "tokens": ["Nicht", "Tho\u00b7ren\u00b7furcht", "noch", "Stolz", "uns", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ADV", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schlie\u00dfen Gottes Eden zu!", "tokens": ["Und", "schlie\u00b7\u00dfen", "Got\u00b7tes", "E\u00b7den", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie? weil uns F\u00fcrsten nicht beschweren", "tokens": ["Wie", "?", "weil", "uns", "F\u00fcrs\u00b7ten", "nicht", "be\u00b7schwe\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "KOUS", "PPER", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Adeltiteln unser Blut?", "tokens": ["Mit", "A\u00b7del\u00b7ti\u00b7teln", "un\u00b7ser", "Blut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So gl\u00e4nzen wir in bessren Ehren,", "tokens": ["So", "gl\u00e4n\u00b7zen", "wir", "in", "bess\u00b7ren", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sind wahrlich edel \u2013 denn sind gut!", "tokens": ["Sind", "wahr\u00b7lich", "e\u00b7del", "\u2013", "denn", "sind", "gut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$(", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo immer unser Nam' erschalle,", "tokens": ["Wo", "im\u00b7mer", "un\u00b7ser", "Nam'", "er\u00b7schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird immer er erschallen s\u00fc\u00df;", "tokens": ["Wird", "im\u00b7mer", "er", "er\u00b7schal\u00b7len", "s\u00fc\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bewundern selbst die Gro\u00dfen alle", "tokens": ["Be\u00b7wun\u00b7dern", "selbst", "die", "Gro\u00b7\u00dfen", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies kleine hohe V\u00f6lklein, dies!", "tokens": ["Dies", "klei\u00b7ne", "ho\u00b7he", "V\u00f6l\u00b7klein", ",", "dies", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADJA", "ADJA", "NN", "$,", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie? weil Gl\u00fcckes Narreng\u00fcte", "tokens": ["Und", "wie", "?", "weil", "Gl\u00fc\u00b7ckes", "Nar\u00b7ren\u00b7g\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "$.", "KOUS", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns keine Kr\u00f6sussch\u00e4tze leiht?", "tokens": ["Uns", "kei\u00b7ne", "Kr\u00f6\u00b7sus\u00b7sch\u00e4t\u00b7ze", "leiht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In M\u00e4\u00dfigung ist Lebensbl\u00fcthe;", "tokens": ["In", "M\u00e4\u00b7\u00dfi\u00b7gung", "ist", "Le\u00b7bens\u00b7bl\u00fct\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Tugend selbst ist M\u00e4\u00dfigkeit!", "tokens": ["Die", "Tu\u00b7gend", "selbst", "ist", "M\u00e4\u00b7\u00dfig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Uns wird jedwedes Wiederkehren", "tokens": ["Uns", "wird", "jed\u00b7we\u00b7des", "Wie\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Jahres W\u00fcnschen g'nug verleihn,", "tokens": ["Des", "Jah\u00b7res", "W\u00fcn\u00b7schen", "g'\u00b7nug", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Leben der Vernunft zu Ehren;", "tokens": ["Und", "Le\u00b7ben", "der", "Ver\u00b7nunft", "zu", "Eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welch Leben kann sonst Leben sein!", "tokens": ["Welch", "Le\u00b7ben", "kann", "sonst", "Le\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch Jugend, Alter in die Wette", "tokens": ["Durch", "Ju\u00b7gend", ",", "Al\u00b7ter", "in", "die", "Wet\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns liebend, wallen wir daher,", "tokens": ["Uns", "lie\u00b7bend", ",", "wal\u00b7len", "wir", "da\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Ruh die H\u00fctte, unser Bette", "tokens": ["Mit", "Ruh", "die", "H\u00fct\u00b7te", ",", "un\u00b7ser", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gekr\u00f6nt mit Kindern lieblicher!", "tokens": ["Ge\u00b7kr\u00f6nt", "mit", "Kin\u00b7dern", "lieb\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Wie will ich meine Kleinen herzen,", "tokens": ["Wie", "will", "ich", "mei\u00b7ne", "Klei\u00b7nen", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir angeschlungen um mein Knie!", "tokens": ["Mir", "an\u00b7ge\u00b7schlun\u00b7gen", "um", "mein", "Knie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr L\u00e4cheln \u2013 ja, der Mutter Scherzen!", "tokens": ["Ihr", "L\u00e4\u00b7cheln", "\u2013", "ja", ",", "der", "Mut\u00b7ter", "Scher\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PTKANT", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Stammeln \u2013 ja, die Mutter \u2013 sie!", "tokens": ["Ihr", "Stam\u00b7meln", "\u2013", "ja", ",", "die", "Mut\u00b7ter", "\u2013", "sie", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PTKANT", "$,", "ART", "NN", "$(", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Neideszeit einst fortgerissen,", "tokens": ["Von", "Nei\u00b7des\u00b7zeit", "einst", "fort\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch werden wir nie freudlos sein;", "tokens": ["Noch", "wer\u00b7den", "wir", "nie", "freud\u00b7los", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du siehst Dich jung in T\u00f6chterk\u00fcssen,", "tokens": ["Du", "siehst", "Dich", "jung", "in", "T\u00f6ch\u00b7ter\u00b7k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich geh' in meinen Buben frein.", "tokens": ["Ich", "geh'", "in", "mei\u00b7nen", "Bu\u00b7ben", "frein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}