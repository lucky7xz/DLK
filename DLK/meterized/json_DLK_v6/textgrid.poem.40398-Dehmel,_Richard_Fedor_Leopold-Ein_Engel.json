{"textgrid.poem.40398": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Engel", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Engel werden,", "tokens": ["Ich", "kann", "ein", "En\u00b7gel", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "wenn ich gestorben bin.", "tokens": ["wenn", "ich", "ge\u00b7stor\u00b7ben", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann jagt wohl mit Wolkenpferden,", "tokens": ["Dann", "jagt", "wohl", "mit", "Wol\u00b7ken\u00b7pfer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die wir nicht sehn auf Erden,", "tokens": ["die", "wir", "nicht", "sehn", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "meine Kraft durchs Luftmeer hin.", "tokens": ["mei\u00b7ne", "Kraft", "durchs", "Luft\u00b7meer", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Fl\u00fcgel sind wohl die St\u00fcrme,", "tokens": ["Mei\u00b7ne", "Fl\u00fc\u00b7gel", "sind", "wohl", "die", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "der Blitzstrahl wohl mein Pfad.", "tokens": ["der", "Blitz\u00b7strahl", "wohl", "mein", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df es nicht, ich leuchte nur;", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "ich", "leuch\u00b7te", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mich treibt ein Geist zur Tat,", "tokens": ["mich", "treibt", "ein", "Geist", "zur", "Tat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "der braucht wohl meine Kraft.", "tokens": ["der", "braucht", "wohl", "mei\u00b7ne", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich leuchte in tausend Gestalten,", "tokens": ["Ich", "leuch\u00b7te", "in", "tau\u00b7send", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "vielleicht wo die Sonne loht,", "tokens": ["viel\u00b7leicht", "wo", "die", "Son\u00b7ne", "loht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "vielleicht wo Sterne erkalten,", "tokens": ["viel\u00b7leicht", "wo", "Ster\u00b7ne", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die bleich noch Nachtwache halten,", "tokens": ["die", "bleich", "noch", "Nacht\u00b7wa\u00b7che", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "vielleicht im Morgenrot.", "tokens": ["viel\u00b7leicht", "im", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da darf ich \u00fcberall wirken,", "tokens": ["Da", "darf", "ich", "\u00fc\u00b7be\u00b7rall", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und bin doch vor dem Geist,", "tokens": ["und", "bin", "doch", "vor", "dem", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der mich und all die andern Engel", "tokens": ["der", "mich", "und", "all", "die", "an\u00b7dern", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KON", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "zu Seinem Werk hinrei\u00dft,", "tokens": ["zu", "Sei\u00b7nem", "Werk", "hin\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "nur ein kleiner Held.", "tokens": ["nur", "ein", "klei\u00b7ner", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}