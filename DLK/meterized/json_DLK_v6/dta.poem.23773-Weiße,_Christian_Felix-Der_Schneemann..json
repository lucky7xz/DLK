{"dta.poem.23773": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Schneemann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6ne Schneemann &#8212; ey wie", "tokens": ["Der", "sch\u00f6\u00b7ne", "Schnee\u00b7mann", "&#8212;", "ey", "wie"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "NE", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "gro\u00df,", "tokens": ["gro\u00df", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Ein riesenm\u00e4\u00dfiger Colo\u00df!", "tokens": ["Ein", "rie\u00b7sen\u00b7m\u00e4\u00b7\u00dfi\u00b7ger", "Co\u00b7lo\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ach! die liebe Sonne scheint,", "tokens": ["Doch", "ach", "!", "die", "lie\u00b7be", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und er zerrinnt, eh mans gemeint.", "tokens": ["Und", "er", "zer\u00b7rinnt", ",", "eh", "mans", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhm gleicht ein eitler leerer Kopf,", "tokens": ["Jhm", "gleicht", "ein", "eit\u00b7ler", "lee\u00b7rer", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von weitem gl\u00e4nzt der arme Tropf,", "tokens": ["Von", "wei\u00b7tem", "gl\u00e4nzt", "der", "ar\u00b7me", "Tropf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch der Verstand beleucht ihn nur,", "tokens": ["Doch", "der", "Ver\u00b7stand", "be\u00b7leucht", "ihn", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schmilzt die schimmernde Figur.", "tokens": ["So", "schmilzt", "die", "schim\u00b7mern\u00b7de", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}