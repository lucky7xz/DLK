{"textgrid.poem.41924": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht im Leben, nicht im Lieben", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht im Leben, nicht im Lieben", "tokens": ["Nicht", "im", "Le\u00b7ben", ",", "nicht", "im", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PTKNEG", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoff' ich Ruhe mehr und Gl\u00fcck,", "tokens": ["Hoff'", "ich", "Ru\u00b7he", "mehr", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur zwey stille W\u00fcnsche blieben", "tokens": ["Nur", "zwey", "stil\u00b7le", "W\u00fcn\u00b7sche", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch im Herzen mir zur\u00fcck.", "tokens": ["Noch", "im", "Her\u00b7zen", "mir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch weil man bey zarten Frauen", "tokens": ["Doch", "weil", "man", "bey", "zar\u00b7ten", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Lieber Blick als Stimme fragt,", "tokens": ["Lie\u00b7ber", "Blick", "als", "Stim\u00b7me", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Will ich leisem Klang vertrauen,", "tokens": ["Will", "ich", "lei\u00b7sem", "Klang", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was mein Mund zu nennen zagt.", "tokens": ["Was", "mein", "Mund", "zu", "nen\u00b7nen", "zagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00f6chtest du aus bunten Bl\u00fcthen", "tokens": ["M\u00f6ch\u00b7test", "du", "aus", "bun\u00b7ten", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich den Liedern, die ich sang,", "tokens": ["Gleich", "den", "Lie\u00b7dern", ",", "die", "ich", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich einen Kranz mir bieten,", "tokens": ["Freund\u00b7lich", "ei\u00b7nen", "Kranz", "mir", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfen Duft f\u00fcr s\u00fc\u00dfen Klang;", "tokens": ["S\u00fc\u00b7\u00dfen", "Duft", "f\u00fcr", "s\u00fc\u00b7\u00dfen", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wenn dann in guter Stunde", "tokens": ["Und", "wenn", "dann", "in", "gu\u00b7ter", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Mir die G\u00f6tter Muth verleihn,", "tokens": ["Mir", "die", "G\u00f6t\u00b7ter", "Muth", "ver\u00b7leihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "M\u00f6g' ein Ku\u00df von deinem Munde", "tokens": ["M\u00f6g'", "ein", "Ku\u00df", "von", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem Kranz die Rose seyn.", "tokens": ["In", "dem", "Kranz", "die", "Ro\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}