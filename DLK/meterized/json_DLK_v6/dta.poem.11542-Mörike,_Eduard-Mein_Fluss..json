{"dta.poem.11542": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Mein Fluss.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "O Flu\u00df, mein Flu\u00df im Morgenstrahl!", "tokens": ["O", "Flu\u00df", ",", "mein", "Flu\u00df", "im", "Mor\u00b7gen\u00b7strahl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfange nun, empfange", "tokens": ["Emp\u00b7fan\u00b7ge", "nun", ",", "emp\u00b7fan\u00b7ge"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den sehnsuchtsvollen Leib einmal", "tokens": ["Den", "sehn\u00b7suchts\u00b7vol\u00b7len", "Leib", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fcsse Brust und Wange!", "tokens": ["Und", "k\u00fcs\u00b7se", "Brust", "und", "Wan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u2014 Er f\u00fchlt mir schon herauf die Brust,", "tokens": ["Er", "f\u00fchlt", "mir", "schon", "her\u00b7auf", "die", "Brust", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er k\u00fchlt mit Liebesschauerlust", "tokens": ["Er", "k\u00fchlt", "mit", "Lie\u00b7bes\u00b7schau\u00b7er\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und jauchzendem Gesange.", "tokens": ["Und", "jauch\u00b7zen\u00b7dem", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es schl\u00fcpft der goldne Sonnenschein", "tokens": ["Es", "schl\u00fcpft", "der", "gold\u00b7ne", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Tropfen an mir nieder,", "tokens": ["In", "Trop\u00b7fen", "an", "mir", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Woge wieget aus und ein", "tokens": ["Die", "Wo\u00b7ge", "wie\u00b7get", "aus", "und", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die hingegeb'nen Glieder;", "tokens": ["Die", "hin\u00b7ge\u00b7ge\u00b7b'\u00b7nen", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die Arme hab' ich ausgespannt,", "tokens": ["Die", "Ar\u00b7me", "hab'", "ich", "aus\u00b7ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie kommt auf mich herzugerannt,", "tokens": ["Sie", "kommt", "auf", "mich", "her\u00b7zu\u00b7ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie fa\u00dft und l\u00e4\u00dft mich wieder.", "tokens": ["Sie", "fa\u00dft", "und", "l\u00e4\u00dft", "mich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du murmelst so, mein Flu\u00df, warum?", "tokens": ["Du", "mur\u00b7melst", "so", ",", "mein", "Flu\u00df", ",", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du tr\u00e4gst seit alten Tagen", "tokens": ["Du", "tr\u00e4gst", "seit", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein seltsam M\u00e4hrchen mit dir um,", "tokens": ["Ein", "selt\u00b7sam", "M\u00e4hr\u00b7chen", "mit", "dir", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und m\u00fch'st dich, es zu sagen;", "tokens": ["Und", "m\u00fch'st", "dich", ",", "es", "zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du eilst so sehr und l\u00e4ufst so sehr,", "tokens": ["Du", "eilst", "so", "sehr", "und", "l\u00e4ufst", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als m\u00fc\u00dftest du im Land umher,", "tokens": ["Als", "m\u00fc\u00df\u00b7test", "du", "im", "Land", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man wei\u00df nicht, wen? drum fragen.", "tokens": ["Man", "wei\u00df", "nicht", ",", "wen", "?", "drum", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "PWS", "$.", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Himmel blau und kinderrein,", "tokens": ["Der", "Him\u00b7mel", "blau", "und", "kin\u00b7der\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Worin die Wellen singen,", "tokens": ["Wo\u00b7rin", "die", "Wel\u00b7len", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel ist die Seele dein:", "tokens": ["Der", "Him\u00b7mel", "ist", "die", "See\u00b7le", "dein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O la\u00df mich ihn durchdringen!", "tokens": ["O", "la\u00df", "mich", "ihn", "durch\u00b7drin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich tauche mich mit Geist und Sinn", "tokens": ["Ich", "tau\u00b7che", "mich", "mit", "Geist", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch die vertiefte Bl\u00e4ue hin,", "tokens": ["Durch", "die", "ver\u00b7tief\u00b7te", "Bl\u00e4u\u00b7e", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kann sie nicht erschwingen!", "tokens": ["Und", "kann", "sie", "nicht", "er\u00b7schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was ist so tief, so tief wie sie?", "tokens": ["Was", "ist", "so", "tief", ",", "so", "tief", "wie", "sie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Liebe nur alleine.", "tokens": ["Die", "Lie\u00b7be", "nur", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wird nicht satt und s\u00e4ttigt nie", "tokens": ["Sie", "wird", "nicht", "satt", "und", "s\u00e4t\u00b7tigt", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihrem Wechselscheine.", "tokens": ["Mit", "ih\u00b7rem", "Wech\u00b7sel\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u2014 Schwill an, mein Flu\u00df, und hebe dich!", "tokens": ["Schwill", "an", ",", "mein", "Flu\u00df", ",", "und", "he\u00b7be", "dich", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mit Grausen \u00fcbergie\u00dfe mich!", "tokens": ["Mit", "Grau\u00b7sen", "\u00fc\u00b7berg\u00b7ie\u00b7\u00dfe", "mich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Leben um das deine!", "tokens": ["Mein", "Le\u00b7ben", "um", "das", "dei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PRELS", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du weisest schmeichelnd mich zur\u00fcck", "tokens": ["Du", "wei\u00b7sest", "schmei\u00b7chelnd", "mich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu deiner Blumenschwelle;", "tokens": ["Zu", "dei\u00b7ner", "Blu\u00b7men\u00b7schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So trage denn allein dein Gl\u00fcck,", "tokens": ["So", "tra\u00b7ge", "denn", "al\u00b7lein", "dein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieg' auf deiner Welle", "tokens": ["Und", "wieg'", "auf", "dei\u00b7ner", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Sonne Pracht, des Mondes Ruh,", "tokens": ["Der", "Son\u00b7ne", "Pracht", ",", "des", "Mon\u00b7des", "Ruh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die lieben Sterne f\u00fchre du", "tokens": ["Die", "lie\u00b7ben", "Ster\u00b7ne", "f\u00fch\u00b7re", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur ew'gen Mutterquelle!", "tokens": ["Zur", "ew'\u00b7gen", "Mut\u00b7ter\u00b7quel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}