{"textgrid.poem.26591": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das aber ist des Alters Sch\u00f6ne,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das aber ist des Alters Sch\u00f6ne,", "tokens": ["Das", "a\u00b7ber", "ist", "des", "Al\u00b7ters", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df es die Saiten reiner stimmt,", "tokens": ["Da\u00df", "es", "die", "Sai\u00b7ten", "rei\u00b7ner", "stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df es der Lust die grellen T\u00f6ne,", "tokens": ["Da\u00df", "es", "der", "Lust", "die", "grel\u00b7len", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Schmerz den herbsten Stachel nimmt.", "tokens": ["Dem", "Schmerz", "den", "herbs\u00b7ten", "Sta\u00b7chel", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ermessen l\u00e4\u00dft sich und verstehen", "tokens": ["Er\u00b7mes\u00b7sen", "l\u00e4\u00dft", "sich", "und", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die eig'ne mit der fremden Schuld,", "tokens": ["Die", "eig'\u00b7ne", "mit", "der", "frem\u00b7den", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie auch rings die Dinge gehen,", "tokens": ["Und", "wie", "auch", "rings", "die", "Din\u00b7ge", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du lernst dich fassen in Geduld.", "tokens": ["Du", "lernst", "dich", "fas\u00b7sen", "in", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Ruhe kommt erf\u00fcllten Strebens,", "tokens": ["Die", "Ru\u00b7he", "kommt", "er\u00b7f\u00fcll\u00b7ten", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es schwindet des verfehlten Pein \u2013", "tokens": ["Es", "schwin\u00b7det", "des", "ver\u00b7fehl\u00b7ten", "Pein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und also wird der Rest des Lebens", "tokens": ["Und", "al\u00b7so", "wird", "der", "Rest", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sanftes R\u00fcckerinnern sein.", "tokens": ["Ein", "sanf\u00b7tes", "R\u00fc\u00b7cke\u00b7rin\u00b7nern", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}