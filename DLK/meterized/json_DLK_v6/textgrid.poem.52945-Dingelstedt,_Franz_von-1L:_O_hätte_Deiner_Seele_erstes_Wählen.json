{"textgrid.poem.52945": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O h\u00e4tte Deiner Seele erstes W\u00e4hlen", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4tte Deiner Seele erstes W\u00e4hlen", "tokens": ["O", "h\u00e4t\u00b7te", "Dei\u00b7ner", "See\u00b7le", "ers\u00b7tes", "W\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Statt meiner einen Besseren getroffen", "tokens": ["Statt", "mei\u00b7ner", "ei\u00b7nen", "Bes\u00b7se\u00b7ren", "ge\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00e4tten wir uns nimmermehr gefunden!", "tokens": ["Und", "h\u00e4t\u00b7ten", "wir", "uns", "nim\u00b7mer\u00b7mehr", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Fr\u00fchling soll dem Herbst sich nicht verm\u00e4hlen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "soll", "dem", "Herbst", "sich", "nicht", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und die Entt\u00e4uschung nicht dem gl\u00e4ubgen Hoffen;", "tokens": ["Und", "die", "Ent\u00b7t\u00e4u\u00b7schung", "nicht", "dem", "gl\u00e4ub\u00b7gen", "Hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie wirst Du, wenn Dein kurzer Rausch entschwunden,", "tokens": ["Wie", "wirst", "Du", ",", "wenn", "Dein", "kur\u00b7zer", "Rausch", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Erwachen, wann gesunden?", "tokens": ["Er\u00b7wa\u00b7chen", ",", "wann", "ge\u00b7sun\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du wei\u00dft nicht, was Du tust. Stets fester rankst Du", "tokens": ["Du", "wei\u00dft", "nicht", ",", "was", "Du", "tust", ".", "Stets", "fes\u00b7ter", "rankst", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$.", "ADV", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Im jungen Triebe Dich um Schutt und Steine;", "tokens": ["Im", "jun\u00b7gen", "Trie\u00b7be", "Dich", "um", "Schutt", "und", "Stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wenn diese brechen \u00fcber Nacht, dann schwankst Du", "tokens": ["Wenn", "die\u00b7se", "bre\u00b7chen", "\u00fc\u00b7ber", "Nacht", ",", "dann", "schwankst", "Du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Zerrissen hin und schutzlos, arme Kleine!", "tokens": ["Zer\u00b7ris\u00b7sen", "hin", "und", "schutz\u00b7los", ",", "ar\u00b7me", "Klei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADJD", "$,", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nein, Rosen sollen nicht aus Tr\u00fcmmern sprossen, \u2013", "tokens": ["Nein", ",", "Ro\u00b7sen", "sol\u00b7len", "nicht", "aus", "Tr\u00fcm\u00b7mern", "spros\u00b7sen", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "NN", "VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Geh, such Dir einen anderen Genossen!", "tokens": ["Geh", ",", "such", "Dir", "ei\u00b7nen", "an\u00b7de\u00b7ren", "Ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}