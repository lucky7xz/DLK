{"textgrid.poem.63393": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Es ist genug", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist genug. Mein tr\u00fcbes Licht", "tokens": ["Es", "ist", "ge\u00b7nug", ".", "Mein", "tr\u00fc\u00b7bes", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereit' sich zu erl\u00f6schen.", "tokens": ["Be\u00b7reit'", "sich", "zu", "er\u00b7l\u00f6\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' vertan mein Recht und Pflicht", "tokens": ["Ich", "hab'", "ver\u00b7tan", "mein", "Recht", "und", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meiner Seel' vergessen.", "tokens": ["Und", "mei\u00b7ner", "Seel'", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ist genug. Es weht ein Wind,", "tokens": ["Es", "ist", "ge\u00b7nug", ".", "Es", "weht", "ein", "Wind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weht nicht von Ost noch Norden.", "tokens": ["Weht", "nicht", "von", "Ost", "noch", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NE", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf der Milchstra\u00dfe wandert ein wei\u00dfes Kind,", "tokens": ["Auf", "der", "Milch\u00b7stra\u00b7\u00dfe", "wan\u00b7dert", "ein", "wei\u00b7\u00dfes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ist nicht geboren worden.", "tokens": ["Ist", "nicht", "ge\u00b7bo\u00b7ren", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du \u00fcber den H\u00e4usern heller Schein,", "tokens": ["Du", "\u00fc\u00b7ber", "den", "H\u00e4u\u00b7sern", "hel\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wovon bist du so helle?", "tokens": ["Wo\u00b7von", "bist", "du", "so", "hel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJA", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Stehst du um die Stirn einer Jungfrau rein", "tokens": ["Stehst", "du", "um", "die", "Stirn", "ei\u00b7ner", "Jung\u00b7frau", "rein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Oder brennt ein S\u00fcnder zur H\u00f6lle?", "tokens": ["O\u00b7der", "brennt", "ein", "S\u00fcn\u00b7der", "zur", "H\u00f6l\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}