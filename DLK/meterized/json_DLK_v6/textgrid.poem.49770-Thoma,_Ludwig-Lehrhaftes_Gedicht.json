{"textgrid.poem.49770": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lehrhaftes Gedicht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Adolf war der Sprosse guter Leute,", "tokens": ["A\u00b7dolf", "war", "der", "Spros\u00b7se", "gu\u00b7ter", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ehelichen Ursprungs, legitim;", "tokens": ["E\u00b7he\u00b7li\u00b7chen", "Ur\u00b7sprungs", ",", "le\u00b7gi\u00b7tim", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Anders Jakob, denn sein Vater scheute", "tokens": ["An\u00b7ders", "Ja\u00b7kob", ",", "denn", "sein", "Va\u00b7ter", "scheu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NE", "$,", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sich und sagt', er w\u00e4re nicht von ihm.", "tokens": ["Sich", "und", "sagt'", ",", "er", "w\u00e4\u00b7re", "nicht", "von", "ihm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbs\u00fc\u00dfes Wunder\u00ab hie\u00df der Eltern Liebe", "tokens": ["\u00bb", "s\u00fc\u00b7\u00dfes", "Wun\u00b7der", "\u00ab", "hie\u00df", "der", "El\u00b7tern", "Lie\u00b7be"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$(", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unsern Adolf, der \u00bbvon Gott gesandt\u00ab;", "tokens": ["Un\u00b7sern", "A\u00b7dolf", ",", "der", "\u00bb", "von", "Gott", "ge\u00b7sandt", "\u00ab", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "$,", "ART", "$(", "APPR", "NN", "VVPP", "$(", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbdie unsel'ge Frucht verbotner Triebe\u00ab", "tokens": ["\u00bb", "die", "un\u00b7sel'\u00b7ge", "Frucht", "ver\u00b7bot\u00b7ner", "Trie\u00b7be", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wurde Jakob meistenteils genannt.", "tokens": ["Wur\u00b7de", "Ja\u00b7kob", "meis\u00b7ten\u00b7teils", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Adolf konnte man den Freunden zeigen;", "tokens": ["A\u00b7dolf", "konn\u00b7te", "man", "den", "Freun\u00b7den", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Man entdeckt' an ihm des Vaters Art.", "tokens": ["Man", "ent\u00b7deckt'", "an", "ihm", "des", "Va\u00b7ters", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcber Jakob herrschte tiefes Schweigen,", "tokens": ["\u00dc\u00b7ber", "Ja\u00b7kob", "herrschte", "tie\u00b7fes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Von ihm sprechen galt als wenig zart.", "tokens": ["Von", "ihm", "spre\u00b7chen", "galt", "als", "we\u00b7nig", "zart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VVFIN", "KOKOM", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dieser Unterschied verblieb im Leben;", "tokens": ["Die\u00b7ser", "Un\u00b7ter\u00b7schied", "ver\u00b7blieb", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Adolfs Laufbahn war solid und leicht.", "tokens": ["A\u00b7dolfs", "Lauf\u00b7bahn", "war", "so\u00b7lid", "und", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Zwar Talent war ihm nicht viel gegeben,", "tokens": ["Zwar", "Ta\u00b7lent", "war", "ihm", "nicht", "viel", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "F\u00fcr den Staatsdienst hat es doch gereicht.", "tokens": ["F\u00fcr", "den", "Staats\u00b7dienst", "hat", "es", "doch", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Jakob war, so wie er einst geboren,", "tokens": ["Ja\u00b7kob", "war", ",", "so", "wie", "er", "einst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ADV", "KOKOM", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stets der Tante Minna ihr Mal\u00f6r.", "tokens": ["Stets", "der", "Tan\u00b7te", "Min\u00b7na", "ihr", "Ma\u00b7l\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Feine Kreise gaben ihn verloren,", "tokens": ["Fei\u00b7ne", "Krei\u00b7se", "ga\u00b7ben", "ihn", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und er wurde sp\u00e4ter Redakteur.", "tokens": ["Und", "er", "wur\u00b7de", "sp\u00e4\u00b7ter", "Re\u00b7dak\u00b7teur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}