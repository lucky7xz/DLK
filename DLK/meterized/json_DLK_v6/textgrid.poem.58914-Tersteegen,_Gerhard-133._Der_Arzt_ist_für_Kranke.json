{"textgrid.poem.58914": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "133. Der Arzt ist f\u00fcr Kranke", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht nur will Jesus S\u00fcnden decken,", "tokens": ["Nicht", "nur", "will", "Je\u00b7sus", "S\u00fcn\u00b7den", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er machet auch von S\u00fcnden frei;", "tokens": ["Er", "ma\u00b7chet", "auch", "von", "S\u00fcn\u00b7den", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer seine Gnade recht soll schmecken,", "tokens": ["Wer", "sei\u00b7ne", "Gna\u00b7de", "recht", "soll", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der schmeckt zuvor, was S\u00fcnde sei.", "tokens": ["Der", "schmeckt", "zu\u00b7vor", ",", "was", "S\u00fcn\u00b7de", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}