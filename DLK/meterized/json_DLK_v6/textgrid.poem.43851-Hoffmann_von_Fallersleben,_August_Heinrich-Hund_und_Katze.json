{"textgrid.poem.43851": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Hund und Katze", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu willst mich kratzen, Katze?", "tokens": ["\u00bb", "du", "willst", "mich", "krat\u00b7zen", ",", "Kat\u00b7ze", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PRF", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich kratzen, Katze du?", "tokens": ["Mich", "krat\u00b7zen", ",", "Kat\u00b7ze", "du", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Birg, Katze, deine Tatze,", "tokens": ["Birg", ",", "Kat\u00b7ze", ",", "dei\u00b7ne", "Tat\u00b7ze", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst, Katze, patsch' ich zu.\u00ab", "tokens": ["Sonst", ",", "Kat\u00b7ze", ",", "patsch'", "ich", "zu", ".", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So sprach der Hund zur Katze", "tokens": ["So", "sprach", "der", "Hund", "zur", "Kat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sah sie patzig an.", "tokens": ["Und", "sah", "sie", "pat\u00b7zig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit einer s\u00fc\u00dfen Fratze", "tokens": ["Mit", "ei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Frat\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Katze drauf begann:", "tokens": ["Die", "Kat\u00b7ze", "drauf", "be\u00b7gann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Miau, miau, miau, miau!", "tokens": ["Mi\u00b7au", ",", "mi\u00b7au", ",", "mi\u00b7au", ",", "mi\u00b7au", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Katze drauf begann.", "tokens": ["Die", "Kat\u00b7ze", "drauf", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lieb H\u00fcndlein, mu\u00dft mir schmeicheln", "tokens": ["Lieb", "H\u00fcnd\u00b7lein", ",", "mu\u00dft", "mir", "schmei\u00b7cheln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und thun recht sanft und zart,", "tokens": ["Und", "thun", "recht", "sanft", "und", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du mu\u00dft mich krau'n und streicheln:", "tokens": ["Du", "mu\u00dft", "mich", "krau'n", "und", "strei\u00b7cheln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So will es meine Art.", "tokens": ["So", "will", "es", "mei\u00b7ne", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Glaub' mir, da\u00df ich nicht murre,", "tokens": ["Glaub'", "mir", ",", "da\u00df", "ich", "nicht", "mur\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Glaub' mir es, meiner Six!", "tokens": ["Glaub'", "mir", "es", ",", "mei\u00b7ner", "Six", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich schmiege mich und schnurre", "tokens": ["Ich", "schmie\u00b7ge", "mich", "und", "schnur\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mache manchen Knix,", "tokens": ["Und", "ma\u00b7che", "man\u00b7chen", "Knix", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Miau, miau, miau, miau!", "tokens": ["Mi\u00b7au", ",", "mi\u00b7au", ",", "mi\u00b7au", ",", "mi\u00b7au", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und mache manchen Knix.", "tokens": ["Und", "ma\u00b7che", "man\u00b7chen", "Knix", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da sprach der Hund zur Katze:", "tokens": ["Da", "sprach", "der", "Hund", "zur", "Kat\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbich geb' dir keinen Schmatz,", "tokens": ["\u00bb", "ich", "geb'", "dir", "kei\u00b7nen", "Schmatz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fcrchte deine Tatze:", "tokens": ["Ich", "f\u00fcrch\u00b7te", "dei\u00b7ne", "Tat\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bist ein falscher Schatz.\u00ab", "tokens": ["Du", "bist", "ein", "fal\u00b7scher", "Schatz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So sprach der Hund zur Katze:", "tokens": ["So", "sprach", "der", "Hund", "zur", "Kat\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbich geb' dir keinen Schmatz,", "tokens": ["\u00bb", "ich", "geb'", "dir", "kei\u00b7nen", "Schmatz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich f\u00fcrchte deine Tatze:", "tokens": ["Ich", "f\u00fcrch\u00b7te", "dei\u00b7ne", "Tat\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du bist ein falscher Schatz,", "tokens": ["Du", "bist", "ein", "fal\u00b7scher", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wauwau, wauwau, wauwau, wauwau!", "tokens": ["Wau\u00b7wau", ",", "wau\u00b7wau", ",", "wau\u00b7wau", ",", "wau\u00b7wau", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Du bist ein falscher Schatz.\u00ab", "tokens": ["Du", "bist", "ein", "fal\u00b7scher", "Schatz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}