{"textgrid.poem.33919": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigend sinkt die Nacht hernieder,", "tokens": ["Schwei\u00b7gend", "sinkt", "die", "Nacht", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in tiefster Dunkelheit", "tokens": ["Und", "in", "tiefs\u00b7ter", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00f6s't das M\u00e4dchen ihre Glieder", "tokens": ["L\u00f6s't", "das", "M\u00e4d\u00b7chen", "ih\u00b7re", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem engen Sonntagskleid.", "tokens": ["Aus", "dem", "en\u00b7gen", "Sonn\u00b7tags\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Aber ihre H\u00e4nde irren", "tokens": ["A\u00b7ber", "ih\u00b7re", "H\u00e4n\u00b7de", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei den Locken dann und wann,", "tokens": ["Bei", "den", "Lo\u00b7cken", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und um diese zu entwirren,", "tokens": ["Und", "um", "die\u00b7se", "zu", "ent\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Z\u00fcndet sie ihr L\u00e4mpchen an.", "tokens": ["Z\u00fcn\u00b7det", "sie", "ihr", "L\u00e4mp\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00fcchtern nun bei seinem Strale", "tokens": ["Sch\u00fcch\u00b7tern", "nun", "bei", "sei\u00b7nem", "Stra\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut sie in des Spiegels Rund,", "tokens": ["Schaut", "sie", "in", "des", "Spie\u00b7gels", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr thut zum ersten Male", "tokens": ["Und", "ihr", "thut", "zum", "ers\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Sch\u00f6nheit Macht sich kund.", "tokens": ["Ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Macht", "sich", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tief err\u00f6thend, dennoch zaudernd,", "tokens": ["Tief", "er\u00b7r\u00f6\u00b7thend", ",", "den\u00b7noch", "zau\u00b7dernd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blickt sie fort und fort hinein;", "tokens": ["Blickt", "sie", "fort", "und", "fort", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann, wie vor sich selbst erschaudernd,", "tokens": ["Dann", ",", "wie", "vor", "sich", "selbst", "er\u00b7schau\u00b7dernd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00f6scht sie schnell der Lampe Schein.", "tokens": ["L\u00f6scht", "sie", "schnell", "der", "Lam\u00b7pe", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leise in sich selbst versinkend", "tokens": ["Lei\u00b7se", "in", "sich", "selbst", "ver\u00b7sin\u00b7kend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und aus eig'nen Zaubers Glanz", "tokens": ["Und", "aus", "eig'\u00b7nen", "Zau\u00b7bers", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Inniges Gen\u00fcgen trinkend,", "tokens": ["In\u00b7ni\u00b7ges", "Ge\u00b7n\u00fc\u00b7gen", "trin\u00b7kend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ist sie still und selig ganz.", "tokens": ["Ist", "sie", "still", "und", "se\u00b7lig", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch sie will die Lust bezwingen,", "tokens": ["Doch", "sie", "will", "die", "Lust", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil sie aus ihr selber quillt,", "tokens": ["Weil", "sie", "aus", "ihr", "sel\u00b7ber", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da verkl\u00e4rt dies holde Ringen", "tokens": ["Da", "ver\u00b7kl\u00e4rt", "dies", "hol\u00b7de", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mailich s\u00fc\u00df ihr frommes Bild.", "tokens": ["Mai\u00b7lich", "s\u00fc\u00df", "ihr", "from\u00b7mes", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und sie sieht's mit halbem Bangen,", "tokens": ["Und", "sie", "sieht's", "mit", "hal\u00b7bem", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df, je mehr sie sich verdammt,", "tokens": ["Da\u00df", ",", "je", "mehr", "sie", "sich", "ver\u00b7dammt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "ADV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihr's von Stirn und Mund und Wangen", "tokens": ["Ih\u00b7r's", "von", "Stirn", "und", "Mund", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Immer sternenhafter flammt.", "tokens": ["Im\u00b7mer", "ster\u00b7nen\u00b7haf\u00b7ter", "flammt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gottes eig'ner Finger leuchtet", "tokens": ["Got\u00b7tes", "eig'\u00b7ner", "Fin\u00b7ger", "leuch\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Golden durch ihr Angesicht,", "tokens": ["Gol\u00b7den", "durch", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und so wie ihr Blick sich feuchtet,", "tokens": ["Und", "so", "wie", "ihr", "Blick", "sich", "feuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00f6scht ihr Hauch zugleich das Licht.", "tokens": ["L\u00f6scht", "ihr", "Hauch", "zu\u00b7gleich", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}