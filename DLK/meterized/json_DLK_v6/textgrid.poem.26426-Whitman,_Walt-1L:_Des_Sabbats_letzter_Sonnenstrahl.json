{"textgrid.poem.26426": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Sabbats letzter Sonnenstrahl", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Sabbats letzter Sonnenstrahl", "tokens": ["Des", "Sab\u00b7bats", "letz\u00b7ter", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verglimmt hier auf dem Steinpflaster,", "tokens": ["Ver\u00b7glimmt", "hier", "auf", "dem", "Stein\u00b7pflas\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und dr\u00fcben f\u00e4llt er nieder", "tokens": ["Und", "dr\u00fc\u00b7ben", "f\u00e4llt", "er", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ein frisches Doppelgrab.", "tokens": ["Auf", "ein", "fri\u00b7sches", "Dop\u00b7pel\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Mond geht auf", "tokens": ["Der", "Mond", "geht", "auf"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Herrlich im Osten \u00fcber den H\u00e4usern,", "tokens": ["Herr\u00b7lich", "im", "Os\u00b7ten", "\u00fc\u00b7ber", "den", "H\u00e4u\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Runder silberbleicher Geistermond,", "tokens": ["Run\u00b7der", "sil\u00b7ber\u00b7blei\u00b7cher", "Geis\u00b7ter\u00b7mond", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gro\u00dfer, stiller Mond.", "tokens": ["Gro\u00b7\u00dfer", ",", "stil\u00b7ler", "Mond", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sehe einen Trauerzug,", "tokens": ["Ich", "se\u00b7he", "ei\u00b7nen", "Trau\u00b7er\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6re den vollen Schall der n\u00e4herkommenden H\u00f6rner,", "tokens": ["H\u00f6\u00b7re", "den", "vol\u00b7len", "Schall", "der", "n\u00e4\u00b7her\u00b7kom\u00b7men\u00b7den", "H\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Durch alle Stra\u00dfen der Stadt flutet es n\u00e4her,", "tokens": ["Durch", "al\u00b7le", "Stra\u00b7\u00dfen", "der", "Stadt", "flu\u00b7tet", "es", "n\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+--++--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wie von Stimmen und Tr\u00e4nen.", "tokens": ["Wie", "von", "Stim\u00b7men", "und", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6re die Schl\u00e4ge der gro\u00dfen Trommel", "tokens": ["Ich", "h\u00f6\u00b7re", "die", "Schl\u00e4\u00b7ge", "der", "gro\u00b7\u00dfen", "Trom\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und den best\u00e4ndigen Wirbel der kleinen Trommeln,", "tokens": ["Und", "den", "be\u00b7st\u00e4n\u00b7di\u00b7gen", "Wir\u00b7bel", "der", "klei\u00b7nen", "Trom\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jeder Schlag der gro\u00dfen Trommel", "tokens": ["Und", "je\u00b7der", "Schlag", "der", "gro\u00b7\u00dfen", "Trom\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durchbebt und ersch\u00fcttert mich.", "tokens": ["Durch\u00b7bebt", "und", "er\u00b7sch\u00fct\u00b7tert", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Denn man bringt den Sohn mit dem Vater,", "tokens": ["Denn", "man", "bringt", "den", "Sohn", "mit", "dem", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Beide zugleich beim st\u00fcrmischen Angriff gefallen,", "tokens": ["Bei\u00b7de", "zu\u00b7gleich", "beim", "st\u00fcr\u00b7mi\u00b7schen", "An\u00b7griff", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+--++-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Vater und Sohn in den vordersten Reihn.", "tokens": ["Va\u00b7ter", "und", "Sohn", "in", "den", "vor\u00b7ders\u00b7ten", "Reihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ein Grab harret nun beider.", "tokens": ["Ein", "Grab", "har\u00b7ret", "nun", "bei\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "N\u00e4her blasen die H\u00f6rner,", "tokens": ["N\u00e4\u00b7her", "bla\u00b7sen", "die", "H\u00f6r\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und die Trommeln schlagen ersch\u00fctternder;", "tokens": ["Und", "die", "Trom\u00b7meln", "schla\u00b7gen", "er\u00b7sch\u00fct\u00b7tern\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Verglommen ist das Tageslicht auf dem Pflaster,", "tokens": ["Ver\u00b7glom\u00b7men", "ist", "das", "Ta\u00b7ges\u00b7licht", "auf", "dem", "Pflas\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und der starke Trauermarsch umf\u00e4ngt mich.", "tokens": ["Und", "der", "star\u00b7ke", "Trau\u00b7er\u00b7marsch", "um\u00b7f\u00e4ngt", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "H\u00f6her steigt im Osten", "tokens": ["H\u00f6\u00b7her", "steigt", "im", "Os\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das gro\u00dfe, traurig leuchtende Phantom,", "tokens": ["Das", "gro\u00b7\u00dfe", ",", "trau\u00b7rig", "leuch\u00b7ten\u00b7de", "Phan\u00b7tom", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich einer Mutter durchschimmerndes Antlitz,", "tokens": ["Gleich", "ei\u00b7ner", "Mut\u00b7ter", "durch\u00b7schim\u00b7mern\u00b7des", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das heller im Himmel ergl\u00fcht.", "tokens": ["Das", "hel\u00b7ler", "im", "Him\u00b7mel", "er\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "O starker Trauermarsch! Du tr\u00f6stest mich.", "tokens": ["O", "star\u00b7ker", "Trau\u00b7er\u00b7marsch", "!", "Du", "tr\u00f6s\u00b7test", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gro\u00dfer Mond mit deinem Silbergesicht, Du beruhigst mich.", "tokens": ["Gro\u00b7\u00dfer", "Mond", "mit", "dei\u00b7nem", "Sil\u00b7ber\u00b7ge\u00b7sicht", ",", "Du", "be\u00b7ru\u00b7higst", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O meine Soldaten, meine Veteranen,", "tokens": ["O", "mei\u00b7ne", "Sol\u00b7da\u00b7ten", ",", "mei\u00b7ne", "Ve\u00b7te\u00b7ra\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Was ich habe, gebe ich euch!", "tokens": ["Was", "ich", "ha\u00b7be", ",", "ge\u00b7be", "ich", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Der Mond gibt euch Licht,", "tokens": ["Der", "Mond", "gibt", "euch", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "H\u00f6rner und Trommeln die Trauermusik,", "tokens": ["H\u00f6r\u00b7ner", "und", "Trom\u00b7meln", "die", "Trau\u00b7er\u00b7mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und mein Herz, o meine Veteranen,", "tokens": ["Und", "mein", "Herz", ",", "o", "mei\u00b7ne", "Ve\u00b7te\u00b7ra\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mein Herz gibt euch Liebe.", "tokens": ["Mein", "Herz", "gibt", "euch", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}