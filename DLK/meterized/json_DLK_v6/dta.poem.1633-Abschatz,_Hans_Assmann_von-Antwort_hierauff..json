{"dta.poem.1633": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort hierauff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du Stieffkind der Natur/ der eignen Mutter Schande/", "tokens": ["Du", "Stieff\u00b7kind", "der", "Na\u00b7tur", "/", "der", "eig\u00b7nen", "Mut\u00b7ter", "Schan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Freund der Uppigkeit/ und Feind verg\u00f6nnter Lust/", "tokens": ["Du", "Freund", "der", "Up\u00b7pig\u00b7keit", "/", "und", "Feind", "ver\u00b7g\u00f6nn\u00b7ter", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$(", "KON", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hassest was dir nicht erlaubet noch bewust:", "tokens": ["Du", "has\u00b7sest", "was", "dir", "nicht", "er\u00b7lau\u00b7bet", "noch", "be\u00b7wust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "PTKNEG", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind alle so gesinnt in deinem Vaterlande/", "tokens": ["Sind", "al\u00b7le", "so", "ge\u00b7sinnt", "in", "dei\u00b7nem", "Va\u00b7ter\u00b7lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So haben sie nicht mehr der Edlen Francken Sinnen/", "tokens": ["So", "ha\u00b7ben", "sie", "nicht", "mehr", "der", "Ed\u00b7len", "Fran\u00b7cken", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wird man nur zu sehr der Bastardirung innen.", "tokens": ["Und", "wird", "man", "nur", "zu", "sehr", "der", "Bas\u00b7tar\u00b7di\u00b7rung", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ADV", "PTKA", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}