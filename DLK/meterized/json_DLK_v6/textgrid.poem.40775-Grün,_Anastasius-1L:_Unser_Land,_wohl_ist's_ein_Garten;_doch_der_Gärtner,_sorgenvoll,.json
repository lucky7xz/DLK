{"textgrid.poem.40775": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unser Land, wohl ist's ein Garten; doch der G\u00e4rtner, sorgenvoll,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unser Land, wohl ist's ein Garten; doch der G\u00e4rtner, sorgenvoll,", "tokens": ["Un\u00b7ser", "Land", ",", "wohl", "ist's", "ein", "Gar\u00b7ten", ";", "doch", "der", "G\u00e4rt\u00b7ner", ",", "sor\u00b7gen\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$.", "ADV", "ART", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zog ein starres Eisengitter, das ihn rings verschlie\u00dfen soll;", "tokens": ["Zog", "ein", "star\u00b7res", "Ei\u00b7sen\u00b7git\u00b7ter", ",", "das", "ihn", "rings", "ver\u00b7schlie\u00b7\u00dfen", "soll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch auch drau\u00dfen wohnen Leute, solch ein Garten lockt herein;", "tokens": ["Doch", "auch", "drau\u00b7\u00dfen", "woh\u00b7nen", "Leu\u00b7te", ",", "solch", "ein", "Gar\u00b7ten", "lockt", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "$,", "PIAT", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wer sich freut an sch\u00f6nen Fluren, kann der schlimmste Gast nicht sein!", "tokens": ["Wer", "sich", "freut", "an", "sch\u00f6\u00b7nen", "Flu\u00b7ren", ",", "kann", "der", "schlimms\u00b7te", "Gast", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Schwarz und gelbe Schranken halten unsre Grenzen rings umspannt,", "tokens": ["Schwarz", "und", "gel\u00b7be", "Schran\u00b7ken", "hal\u00b7ten", "uns\u00b7re", "Gren\u00b7zen", "rings", "um\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schergenwacht und Mauthner h\u00fcten so bei Tag als Nacht das Land,", "tokens": ["Scher\u00b7gen\u00b7wacht", "und", "Mauth\u00b7ner", "h\u00fc\u00b7ten", "so", "bei", "Tag", "als", "Nacht", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "APPR", "NN", "KOUS", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sitzen unter Tags vorm Zollhaus, liegen Nachts im feuchten Gras,", "tokens": ["Sit\u00b7zen", "un\u00b7ter", "Tags", "vorm", "Zoll\u00b7haus", ",", "lie\u00b7gen", "Nachts", "im", "feuch\u00b7ten", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "$,", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Still und lauschend auf dem Bauche, sp\u00e4hend rings ohn' Unterla\u00df.", "tokens": ["Still", "und", "lau\u00b7schend", "auf", "dem", "Bau\u00b7che", ",", "sp\u00e4\u00b7hend", "rings", "ohn'", "Un\u00b7ter\u00b7la\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$,", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Da\u00df sich ja kein fremder Kr\u00e4mer, fremder Knaster, fremder Wein,", "tokens": ["Da\u00df", "sich", "ja", "kein", "frem\u00b7der", "Kr\u00e4\u00b7mer", ",", "frem\u00b7der", "Knas\u00b7ter", ",", "frem\u00b7der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "PIAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Fremde Seide, fremde Linnen schleichen in das Land herein!", "tokens": ["Frem\u00b7de", "Sei\u00b7de", ",", "frem\u00b7de", "Lin\u00b7nen", "schlei\u00b7chen", "in", "das", "Land", "her\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Der Gedanke, der entsprossen fremdem Boden, fremdem Licht!", "tokens": ["Der", "Ge\u00b7dan\u00b7ke", ",", "der", "ent\u00b7spros\u00b7sen", "frem\u00b7dem", "Bo\u00b7den", ",", "frem\u00b7dem", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Endlich wird's den W\u00e4chtern bange, wenn die Geisterstunde kreist,", "tokens": ["End\u00b7lich", "wird's", "den", "W\u00e4ch\u00b7tern", "ban\u00b7ge", ",", "wenn", "die", "Geis\u00b7ter\u00b7stun\u00b7de", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Denn in unserm guten Lande graut es Manchem vor dem Geist;", "tokens": ["Denn", "in", "un\u00b7serm", "gu\u00b7ten", "Lan\u00b7de", "graut", "es", "Man\u00b7chem", "vor", "dem", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Kalt und schneidend weht die Nachtluft, Mattheit rieselt durchs Gebein,", "tokens": ["Kalt", "und", "schnei\u00b7dend", "weht", "die", "Nacht\u00b7luft", ",", "Mat\u00b7theit", "rie\u00b7selt", "durchs", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN", "$,", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "In die Schenke zieh'n die W\u00e4chter, Herz und Leib erquickt der Wein!", "tokens": ["In", "die", "Schen\u00b7ke", "zieh'n", "die", "W\u00e4ch\u00b7ter", ",", "Herz", "und", "Leib", "er\u00b7quickt", "der", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NE", "$,", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Sieh, da tauchen aus den B\u00fcschen, aus den Nebeln rings der Nacht,", "tokens": ["Sieh", ",", "da", "tau\u00b7chen", "aus", "den", "B\u00fc\u00b7schen", ",", "aus", "den", "Ne\u00b7beln", "rings", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "M\u00e4nner, schwere Last am R\u00fccken, Karren, schwer von reicher Fracht,", "tokens": ["M\u00e4n\u00b7ner", ",", "schwe\u00b7re", "Last", "am", "R\u00fc\u00b7cken", ",", "Kar\u00b7ren", ",", "schwer", "von", "rei\u00b7cher", "Fracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "APPRART", "NN", "$,", "NN", "$,", "ADJD", "APPR", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Leise, wie die Nebel, schleichen sie die fahlen Steg' entlang,", "tokens": ["Lei\u00b7se", ",", "wie", "die", "Ne\u00b7bel", ",", "schlei\u00b7chen", "sie", "die", "fah\u00b7len", "Steg'", "ent\u00b7lang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sieh, da wallt auch der Gedanke seiner Sendung heil'gen Gang.", "tokens": ["Sieh", ",", "da", "wallt", "auch", "der", "Ge\u00b7dan\u00b7ke", "sei\u00b7ner", "Sen\u00b7dung", "heil'\u00b7gen", "Gang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Mit den Schmugglern mu\u00df er reisen, \u2013 er versteckt und hehlt doch nichts!", "tokens": ["Mit", "den", "Schmug\u00b7glern", "mu\u00df", "er", "rei\u00b7sen", ",", "\u2013", "er", "ver\u00b7steckt", "und", "hehlt", "doch", "nichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "$(", "PPER", "VVPP", "KON", "VVFIN", "ADV", "PIS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mit den dunkeln Nebeln schleichen, \u2013 er, der Sohn des Tags und Lichts! \u2013", "tokens": ["Mit", "den", "dun\u00b7keln", "Ne\u00b7beln", "schlei\u00b7chen", ",", "\u2013", "er", ",", "der", "Sohn", "des", "Tags", "und", "Lichts", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,", "$(", "PPER", "$,", "ART", "NN", "ART", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "O heraus, ihr durst'gen Zecher! M\u00fcde W\u00e4chter, flink herbei!", "tokens": ["O", "he\u00b7raus", ",", "ihr", "dur\u00b7st'\u00b7gen", "Ze\u00b7cher", "!", "M\u00fc\u00b7de", "W\u00e4ch\u00b7ter", ",", "flink", "her\u00b7bei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$.", "NN", "NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Stellt euch auf in blanken Waffen, schnurgerad in Glied und Reih'!", "tokens": ["Stellt", "euch", "auf", "in", "blan\u00b7ken", "Waf\u00b7fen", ",", "schnur\u00b7ge\u00b7rad", "in", "Glied", "und", "Reih'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Pr\u00e4sentiret die Gewehre, senkt die Fahne feierlich!", "tokens": ["Pr\u00e4\u00b7sen\u00b7ti\u00b7ret", "die", "Ge\u00b7weh\u00b7re", ",", "senkt", "die", "Fah\u00b7ne", "fei\u00b7er\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "La\u00dft die Trommeln fr\u00f6hlich wirbeln, und die Schranke \u00f6ffne sich!", "tokens": ["La\u00dft", "die", "Trom\u00b7meln", "fr\u00f6h\u00b7lich", "wir\u00b7beln", ",", "und", "die", "Schran\u00b7ke", "\u00f6ff\u00b7ne", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF", "$,", "KON", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df mit gr\u00fcner Palme siegreich, stolz und frei im Lichtgewand,", "tokens": ["Da\u00df", "mit", "gr\u00fc\u00b7ner", "Pal\u00b7me", "sieg\u00b7reich", ",", "stolz", "und", "frei", "im", "Licht\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Leuchtend der Gedanke wandle in das gastlich sch\u00f6ne Land!", "tokens": ["Leuch\u00b7tend", "der", "Ge\u00b7dan\u00b7ke", "wand\u00b7le", "in", "das", "gast\u00b7lich", "sch\u00f6\u00b7ne", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}