{"textgrid.poem.59156": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "375. Abh\u00e4ngig und ergeben \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lass' mich stille wickeln ein,", "tokens": ["Ich", "lass'", "mich", "stil\u00b7le", "wi\u00b7ckeln", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lass' mich legen, wenden, heben;", "tokens": ["Ich", "lass'", "mich", "le\u00b7gen", ",", "wen\u00b7den", ",", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00dft du auch abh\u00e4ngig sein", "tokens": ["So", "mu\u00dft", "du", "auch", "ab\u00b7h\u00e4n\u00b7gig", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dich in meine Bande geben.", "tokens": ["Und", "dich", "in", "mei\u00b7ne", "Ban\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}