{"textgrid.poem.57754": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die b\u00f6se Sieben", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Wirtshaus an der Stra\u00dfe", "tokens": ["Am", "Wirts\u00b7haus", "an", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sieben Birkenb\u00e4ume stehn;", "tokens": ["Sie\u00b7ben", "Bir\u00b7ken\u00b7b\u00e4u\u00b7me", "stehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die sieben gr\u00fcnen B\u00e4ume,", "tokens": ["Die", "sie\u00b7ben", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die will ich gar nicht sehn.", "tokens": ["Die", "will", "ich", "gar", "nicht", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sieben, ja die Sieben", "tokens": ["Die", "Sie\u00b7ben", ",", "ja", "die", "Sie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist eine b\u00f6se Zahl;", "tokens": ["Ist", "ei\u00b7ne", "b\u00f6\u00b7se", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieben wundersch\u00f6ne M\u00e4dchen,", "tokens": ["Sie\u00b7ben", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die liebte ich einmal.", "tokens": ["Die", "lieb\u00b7te", "ich", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sechs Rosen ohne Dornen", "tokens": ["Sechs", "Ro\u00b7sen", "oh\u00b7ne", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die waren mein f\u00fcrwahr;", "tokens": ["Die", "wa\u00b7ren", "mein", "f\u00fcr\u00b7wahr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die siebte, die ich pfl\u00fcckte,", "tokens": ["Die", "sieb\u00b7te", ",", "die", "ich", "pfl\u00fcck\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Dorn und Distel war.", "tokens": ["Voll", "Dorn", "und", "Dis\u00b7tel", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die siebte von den Sieben", "tokens": ["Die", "sieb\u00b7te", "von", "den", "Sie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kunst sie wohl verstand;", "tokens": ["Die", "Kunst", "sie", "wohl", "ver\u00b7stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fchrt mich zum Altare", "tokens": ["Sie", "f\u00fchrt", "mich", "zum", "Al\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihrer wei\u00dfen Hand.", "tokens": ["Mit", "ih\u00b7rer", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die sieben Birkenb\u00e4ume,", "tokens": ["Die", "sie\u00b7ben", "Bir\u00b7ken\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die gehen hin und her;", "tokens": ["Die", "ge\u00b7hen", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ade, ihr roten Rosen,", "tokens": ["A\u00b7de", ",", "ihr", "ro\u00b7ten", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ich pfl\u00fccke keine mehr.", "tokens": ["Ich", "pfl\u00fc\u00b7cke", "kei\u00b7ne", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}