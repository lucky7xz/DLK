{"dta.poem.9433": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Euch T\u00f6chter Jerusalem will ich beschw\u00f6ren", "tokens": ["Euch", "T\u00f6ch\u00b7ter", "Je\u00b7ru\u00b7sa\u00b7lem", "will", "ich", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "NE", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bey allen den Hindin und Rehen jtzund/", "tokens": ["Bey", "al\u00b7len", "den", "Hin\u00b7din", "und", "Re\u00b7hen", "jt\u00b7zund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "KON", "NN", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Jhr sollet im schlaaffe nicht also verst\u00f6ren", "tokens": ["Ihr", "sol\u00b7let", "im", "schlaaf\u00b7fe", "nicht", "al\u00b7so", "ver\u00b7st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "VVFIN", "PTKNEG", "ADV", "VVINF"], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die/ welche mein Hertze verwundt/", "tokens": ["Die", "/", "wel\u00b7che", "mein", "Hert\u00b7ze", "ver\u00b7wundt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELS", "PPOSAT", "VVFIN", "VVPP", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "und la\u00dfet Sie liegen", "tokens": ["und", "la\u00b7\u00dfet", "Sie", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nach jhrem gon\u00fcgen/", "tokens": ["Nach", "jhrem", "go\u00b7n\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bi\u00df sie sich auffmacht", "tokens": ["Bi\u00df", "sie", "sich", "auff\u00b7macht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "und selbsten erwacht.", "tokens": ["und", "selbs\u00b7ten", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ach sihe? wer ist es doch die ich von weiten", "tokens": ["Ach", "si\u00b7he", "?", "wer", "ist", "es", "doch", "die", "ich", "von", "wei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "ADV", "ART", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Kan sehen aus jenem geb\u00fcsche rausgehn/", "tokens": ["Kan", "se\u00b7hen", "aus", "je\u00b7nem", "ge\u00b7b\u00fc\u00b7sche", "raus\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "PDAT", "ADJA", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich sehe mein Leben/ die Zierde der zeiten/", "tokens": ["Ich", "se\u00b7he", "mein", "Le\u00b7ben", "/", "die", "Zier\u00b7de", "der", "zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich seh Sie wie weyhrauch und myrrhen entstehn.", "tokens": ["Ich", "seh", "Sie", "wie", "wey\u00b7hrauch", "und", "myr\u00b7rhen", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Die herrlich sich zeiget", "tokens": ["Die", "herr\u00b7lich", "sich", "zei\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie Benzoe steiget", "tokens": ["Wie", "Ben\u00b7zoe", "stei\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NE", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Nach jhrem gebrauch/", "tokens": ["Nach", "jhrem", "ge\u00b7brauch", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "---+", "measure": "unknown.measure.single"}, "line.8": {"text": "Wie jrgend ein rauch.", "tokens": ["Wie", "jr\u00b7gend", "ein", "rauch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}