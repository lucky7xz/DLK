{"textgrid.poem.60981": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Rheinsage", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Rhein, am gr\u00fcnen Rheine,", "tokens": ["Am", "Rhein", ",", "am", "gr\u00fc\u00b7nen", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "$,", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ist so mild die Nacht,", "tokens": ["Da", "ist", "so", "mild", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Rebenh\u00fcgel liegen", "tokens": ["Die", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", "lie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In goldner Mondenpracht.", "tokens": ["In", "gold\u00b7ner", "Mon\u00b7den\u00b7pracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und an den H\u00fcgeln wandelt", "tokens": ["Und", "an", "den", "H\u00fc\u00b7geln", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein hoher Schatten her", "tokens": ["Ein", "ho\u00b7her", "Schat\u00b7ten", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Schwert und Purpurmantel,", "tokens": ["Mit", "Schwert", "und", "Pur\u00b7pur\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Krone von Golde schwer.", "tokens": ["Die", "Kro\u00b7ne", "von", "Gol\u00b7de", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das ist der Karl, der Kaiser,", "tokens": ["Das", "ist", "der", "Karl", ",", "der", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der mit gewalt'ger Hand", "tokens": ["Der", "mit", "ge\u00b7walt'\u00b7ger", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor vielen hundert Jahren", "tokens": ["Vor", "vie\u00b7len", "hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geherrscht im deutschen Land.", "tokens": ["Ge\u00b7herrscht", "im", "deut\u00b7schen", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er ist heraufgestiegen", "tokens": ["Er", "ist", "her\u00b7auf\u00b7ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Aachen aus der Gruft", "tokens": ["Zu", "Aa\u00b7chen", "aus", "der", "Gruft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und segnet seine Reben", "tokens": ["Und", "seg\u00b7net", "sei\u00b7ne", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und atmet Traubenduft.", "tokens": ["Und", "at\u00b7met", "Trau\u00b7ben\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bei R\u00fcdesheim da funkelt", "tokens": ["Bei", "R\u00fc\u00b7des\u00b7heim", "da", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Mond ins Wasser hinein", "tokens": ["Der", "Mond", "ins", "Was\u00b7ser", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "APZR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und baut eine goldene Br\u00fccke", "tokens": ["Und", "baut", "ei\u00b7ne", "gol\u00b7de\u00b7ne", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wohl \u00fcber den gr\u00fcnen Rhein.", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "gr\u00fc\u00b7nen", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Kaiser geht hin\u00fcber", "tokens": ["Der", "Kai\u00b7ser", "geht", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schreitet langsam fort", "tokens": ["Und", "schrei\u00b7tet", "lang\u00b7sam", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und segnet l\u00e4ngs dem Strome", "tokens": ["Und", "seg\u00b7net", "l\u00e4ngs", "dem", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Reben an jedem Ort.", "tokens": ["Die", "Re\u00b7ben", "an", "je\u00b7dem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Dann kehrt er heim nach Aachen", "tokens": ["Dann", "kehrt", "er", "heim", "nach", "Aa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schl\u00e4ft in seiner Gruft,", "tokens": ["Und", "schl\u00e4ft", "in", "sei\u00b7ner", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis ihn im neuen Jahre", "tokens": ["Bis", "ihn", "im", "neu\u00b7en", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erweckt der Trauben Duft.", "tokens": ["Er\u00b7weckt", "der", "Trau\u00b7ben", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir aber f\u00fcllen die R\u00f6mer", "tokens": ["Wir", "a\u00b7ber", "f\u00fcl\u00b7len", "die", "R\u00f6\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und trinken im goldenen Saft", "tokens": ["Und", "trin\u00b7ken", "im", "gol\u00b7de\u00b7nen", "Saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Uns deutsches Heldenfeuer", "tokens": ["Uns", "deut\u00b7sches", "Hel\u00b7den\u00b7feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deutsche Heldenkraft.", "tokens": ["Und", "deut\u00b7sche", "Hel\u00b7den\u00b7kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}