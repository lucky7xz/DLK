{"textgrid.poem.42796": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Schlummerlied", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will du auf T\u00f6pfchen?", "tokens": ["Will", "du", "auf", "T\u00f6pf\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "F\u00fchlst du ein D\u00fcrstchen?", "tokens": ["F\u00fchlst", "du", "ein", "D\u00fcr\u00b7stchen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Oder ein W\u00fcrstchen?", "tokens": ["O\u00b7der", "ein", "W\u00fcr\u00b7stchen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Senke dein K\u00f6pfchen.", "tokens": ["Sen\u00b7ke", "dein", "K\u00f6pf\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Drau\u00dfen die schwarze, kalte", "tokens": ["Drau\u00b7\u00dfen", "die", "schwar\u00b7ze", ",", "kal\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "ADJA", "$,", "ADJA"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Nacht ist b\u00f6se und fremd.", "tokens": ["Nacht", "ist", "b\u00f6\u00b7se", "und", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Deine H\u00e4nde falte.", "tokens": ["Dei\u00b7ne", "H\u00e4n\u00b7de", "fal\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der liebe Gott k\u00fc\u00dft dein Hemd.", "tokens": ["Der", "lie\u00b7be", "Gott", "k\u00fc\u00dft", "dein", "Hemd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Gute Ruh!", "tokens": ["Gu\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ich bin da,", "tokens": ["Ich", "bin", "da", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Deine Mutter, Mama;", "tokens": ["Dei\u00b7ne", "Mut\u00b7ter", ",", "Ma\u00b7ma", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "M\u00fcde wie du.", "tokens": ["M\u00fc\u00b7de", "wie", "du", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Nichts mehr sagen \u2013", "tokens": ["Nichts", "mehr", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Nicht fragen \u2013", "tokens": ["Nicht", "fra\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nichts wissen \u2013", "tokens": ["Nichts", "wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Augen zu.", "tokens": ["Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Horch in dein Kissen:", "tokens": ["Horch", "in", "dein", "Kis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Es atmet wie du.", "tokens": ["Es", "at\u00b7met", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}