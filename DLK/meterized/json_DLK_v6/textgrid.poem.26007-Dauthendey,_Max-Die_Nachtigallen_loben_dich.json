{"textgrid.poem.26007": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtigallen loben dich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Wolken an der Erde h\u00e4ngen Tag und Nacht,", "tokens": ["Wie", "die", "Wol\u00b7ken", "an", "der", "Er\u00b7de", "h\u00e4n\u00b7gen", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "So umdr\u00e4ngen dich meine Gedanken.", "tokens": ["So", "um\u00b7dr\u00e4n\u00b7gen", "dich", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Die Nachtigallen loben dich,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal\u00b7len", "lo\u00b7ben", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ich schreibe ihre Lieder ab.", "tokens": ["Und", "ich", "schrei\u00b7be", "ih\u00b7re", "Lie\u00b7der", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Du stehst wie eine Anemone in den Steinfeldern,", "tokens": ["Du", "stehst", "wie", "ei\u00b7ne", "A\u00b7ne\u00b7mo\u00b7ne", "in", "den", "Stein\u00b7fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Ihre Bl\u00fctenwangen ziehen meine H\u00e4nde an.", "tokens": ["Ih\u00b7re", "Bl\u00fc\u00b7ten\u00b7wan\u00b7gen", "zie\u00b7hen", "mei\u00b7ne", "H\u00e4n\u00b7de", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Nie haben sich Bienen so s\u00fc\u00df gen\u00e4hrt", "tokens": ["Nie", "ha\u00b7ben", "sich", "Bie\u00b7nen", "so", "s\u00fc\u00df", "ge\u00b7n\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wie meine Lippen.", "tokens": ["Wie", "mei\u00b7ne", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}