{"textgrid.poem.63228": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Venedig", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Norden", "tokens": ["Im", "Nor\u00b7den"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Frieren die G\u00f6tter.", "tokens": ["Frie\u00b7ren", "die", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Hier", "tokens": ["Hier"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Strahlt jeder Gauner: ein heisser Gott.", "tokens": ["Strahlt", "je\u00b7der", "Gau\u00b7ner", ":", "ein", "heis\u00b7ser", "Gott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Seines Tempels Stufen", "tokens": ["Sei\u00b7nes", "Tem\u00b7pels", "Stu\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Steigen aus dem Canale grande.", "tokens": ["Stei\u00b7gen", "aus", "dem", "Ca\u00b7na\u00b7le", "gran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Er opfert", "tokens": ["Er", "op\u00b7fert"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sein s\u00fcdliches Herz sich selbst.", "tokens": ["Sein", "s\u00fcd\u00b7li\u00b7ches", "Herz", "sich", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Sbirren schleichen", "tokens": ["Die", "Sbir\u00b7ren", "schlei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Zur D\u00e4mmerung.", "tokens": ["Zur", "D\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Am Himmel segelt", "tokens": ["Am", "Him\u00b7mel", "se\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Eine Gondel.", "tokens": ["Ei\u00b7ne", "Gon\u00b7del", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Die Adria", "tokens": ["Die", "Ad\u00b7ria"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Brandet an meine Brust.", "tokens": ["Bran\u00b7det", "an", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Markusplatz", "tokens": ["Der", "Mar\u00b7kus\u00b7platz"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "T\u00f6nt wie eine Harfe.", "tokens": ["T\u00f6nt", "wie", "ei\u00b7ne", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "An vergitterten Fenstern,", "tokens": ["An", "ver\u00b7git\u00b7ter\u00b7ten", "Fens\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "An freigelassenen Menschen vorbei:", "tokens": ["An", "frei\u00b7ge\u00b7las\u00b7se\u00b7nen", "Men\u00b7schen", "vor\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Auf einer weissen Piazza", "tokens": ["Auf", "ei\u00b7ner", "weis\u00b7sen", "Piaz\u00b7za"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entfaltet sich wie eine rote Mantille dein L\u00e4cheln.", "tokens": ["Ent\u00b7fal\u00b7tet", "sich", "wie", "ei\u00b7ne", "ro\u00b7te", "Man\u00b7til\u00b7le", "dein", "L\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KOKOM", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ists Tag? So ist die Sonne,", "tokens": ["Ists", "Tag", "?", "So", "ist", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ists Nacht? So ist der Mond", "tokens": ["Ists", "Nacht", "?", "So", "ist", "der", "Mond"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Herzen", "tokens": ["Am", "Her\u00b7zen"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Aufgegangen.", "tokens": ["Auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}