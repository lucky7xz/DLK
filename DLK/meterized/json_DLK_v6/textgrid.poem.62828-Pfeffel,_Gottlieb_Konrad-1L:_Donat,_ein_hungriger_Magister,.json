{"textgrid.poem.62828": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Donat, ein hungriger Magister,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Donat, ein hungriger Magister,", "tokens": ["Do\u00b7nat", ",", "ein", "hung\u00b7ri\u00b7ger", "Ma\u00b7gis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein Thier zu stolz f\u00fcr einen K\u00fcster,", "tokens": ["Ein", "Thier", "zu", "stolz", "f\u00fcr", "ei\u00b7nen", "K\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKA", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch zu dumm f\u00fcr etwas mehr,", "tokens": ["Und", "doch", "zu", "dumm", "f\u00fcr", "et\u00b7was", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKA", "ADJD", "APPR", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschlo\u00df nach Art der neuern Weisen,", "tokens": ["Be\u00b7schlo\u00df", "nach", "Art", "der", "neu\u00b7ern", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Welt empfindsam zu bereisen,", "tokens": ["Die", "Welt", "emp\u00b7find\u00b7sam", "zu", "be\u00b7rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und zog mit Stab und Sack umher.", "tokens": ["Und", "zog", "mit", "Stab", "und", "Sack", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst kam er mit betr\u00fcbtem Schritte", "tokens": ["Einst", "kam", "er", "mit", "be\u00b7tr\u00fcb\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Nachts vor eines Bauers H\u00fctte", "tokens": ["Des", "Nachts", "vor", "ei\u00b7nes", "Bau\u00b7ers", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hielt um eine Zehrung an:", "tokens": ["Und", "hielt", "um", "ei\u00b7ne", "Zeh\u00b7rung", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft, rief er, eines edeln Armen,", "tokens": ["La\u00dft", ",", "rief", "er", ",", "ei\u00b7nes", "e\u00b7deln", "Ar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00dft eines Meisters euch erbarmen,", "tokens": ["La\u00dft", "ei\u00b7nes", "Meis\u00b7ters", "euch", "er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der sieben freye K\u00fcnste kann!", "tokens": ["Der", "sie\u00b7ben", "frey\u00b7e", "K\u00fcns\u00b7te", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit sieben K\u00fcnsten betteln gehen?", "tokens": ["Mit", "sie\u00b7ben", "K\u00fcns\u00b7ten", "bet\u00b7teln", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach Hans, das kann ich nicht verstehen:", "tokens": ["Sprach", "Hans", ",", "das", "kann", "ich", "nicht", "ver\u00b7ste\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich dummer Bauer wei\u00df genug,", "tokens": ["Ich", "dum\u00b7mer", "Bau\u00b7er", "wei\u00df", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um Weib und Kind mit Gott und Ehren", "tokens": ["Um", "Weib", "und", "Kind", "mit", "Gott", "und", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von meinem Pfluge zu ern\u00e4hren;", "tokens": ["Von", "mei\u00b7nem", "Pflu\u00b7ge", "zu", "er\u00b7n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Drum rath ich euch, ergreift den Pflug.", "tokens": ["Drum", "rath", "ich", "euch", ",", "er\u00b7greift", "den", "Pflug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}