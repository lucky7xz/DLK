{"textgrid.poem.25332": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Baldurs Traum", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch alle Lande leuchteten die Opferbr\u00e4nde.", "tokens": ["Durch", "al\u00b7le", "Lan\u00b7de", "leuch\u00b7te\u00b7ten", "die", "Op\u00b7fer\u00b7br\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Rauschen trug die Luft, ein Lied der Kraft;", "tokens": ["Ein", "Rau\u00b7schen", "trug", "die", "Luft", ",", "ein", "Lied", "der", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das klang, als r\u00fcttelte durch Orgelpfeifen", "tokens": ["Das", "klang", ",", "als", "r\u00fct\u00b7tel\u00b7te", "durch", "Or\u00b7gel\u00b7pfei\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Nordsturm, der aus Gischt und Brandung sich", "tokens": ["Ein", "Nord\u00b7sturm", ",", "der", "aus", "Gischt", "und", "Bran\u00b7dung", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Atem borgte. Wie ein Schlachtruf sprangs", "tokens": ["Den", "A\u00b7tem", "borg\u00b7te", ".", "Wie", "ein", "Schlach\u00b7truf", "sprangs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus Grab und Traum aufjauchzend in die Welt.", "tokens": ["Aus", "Grab", "und", "Traum", "auf\u00b7jauch\u00b7zend", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und war", "tokens": ["Und", "war"], "token_info": ["word", "word"], "pos": ["KON", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Ein Lied des Trotzes und ein Lied der Jugend \u2013", "tokens": ["Ein", "Lied", "des", "Trot\u00b7zes", "und", "ein", "Lied", "der", "Ju\u00b7gend", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sonnwendnacht!", "tokens": ["Sonn\u00b7wend\u00b7nacht", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Durch alle Lande leuchteten die Opferbr\u00e4nde.", "tokens": ["Durch", "al\u00b7le", "Lan\u00b7de", "leuch\u00b7te\u00b7ten", "die", "Op\u00b7fer\u00b7br\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Nacht gebar den Streit.", "tokens": ["Die", "Nacht", "ge\u00b7bar", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Hei\u00df stob die Schlacht.", "tokens": ["Hei\u00df", "stob", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Das Blut schlang taumelnd seine Reigen durch die Reihn,", "tokens": ["Das", "Blut", "schlang", "tau\u00b7melnd", "sei\u00b7ne", "Rei\u00b7gen", "durch", "die", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bis zwei noch standen, ineinander tief", "tokens": ["Bis", "zwei", "noch", "stan\u00b7den", ",", "in\u00b7ein\u00b7an\u00b7der", "tief"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "CARD", "ADV", "VVFIN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Den gl\u00fchen Blick getaucht,", "tokens": ["Den", "gl\u00fc\u00b7hen", "Blick", "ge\u00b7taucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Gott und Schwarzalbe.", "tokens": ["Gott", "und", "Schwar\u00b7zal\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.17": {"text": "Wild griff der Gott zum Bogen, seine Finger", "tokens": ["Wild", "griff", "der", "Gott", "zum", "Bo\u00b7gen", ",", "sei\u00b7ne", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Krampften die Saiten auf:", "tokens": ["Krampf\u00b7ten", "die", "Sai\u00b7ten", "auf", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "Die st\u00f6hnten, sangen \u2013", "tokens": ["Die", "st\u00f6hn\u00b7ten", ",", "san\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Zerklirrten pfeifend ... Schmetternd schlug", "tokens": ["Zer\u00b7klirr\u00b7ten", "pfei\u00b7fend", "...", "Schmet\u00b7ternd", "schlug"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$(", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "In j\u00e4hem Schwung des Andern Keule nieder.", "tokens": ["In", "j\u00e4\u00b7hem", "Schwung", "des", "An\u00b7dern", "Keu\u00b7le", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mit dem Siegsgebr\u00fcll des Schwarzen fiel", "tokens": ["Und", "mit", "dem", "Siegs\u00b7ge\u00b7br\u00fcll", "des", "Schwar\u00b7zen", "fiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Eisige Nacht aus Wolken in die Welt,", "tokens": ["Ei\u00b7si\u00b7ge", "Nacht", "aus", "Wol\u00b7ken", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und auf den Bergen loschen alle Feuer ...", "tokens": ["Und", "auf", "den", "Ber\u00b7gen", "lo\u00b7schen", "al\u00b7le", "Feu\u00b7er", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}