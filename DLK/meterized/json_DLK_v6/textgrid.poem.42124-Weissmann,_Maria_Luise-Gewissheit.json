{"textgrid.poem.42124": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Gewissheit", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei geduldig! Nimm gelassen,", "tokens": ["Sei", "ge\u00b7dul\u00b7dig", "!", "Nimm", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$.", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Tage Kette gleitet:", "tokens": ["Wie", "der", "Ta\u00b7ge", "Ket\u00b7te", "glei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Anfang, der nicht zu erfassen,", "tokens": ["An\u00b7fang", ",", "der", "nicht", "zu", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ende nicht. Und die sich breitet,", "tokens": ["En\u00b7de", "nicht", ".", "Und", "die", "sich", "brei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$.", "KON", "ART", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mitte, ziellos, offner, enger,", "tokens": ["Mit\u00b7te", ",", "ziel\u00b7los", ",", "off\u00b7ner", ",", "en\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df sie z\u00f6gern, la\u00df sie schweifen:", "tokens": ["La\u00df", "sie", "z\u00f6\u00b7gern", ",", "la\u00df", "sie", "schwei\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unerbittlich wird ein strenger", "tokens": ["Un\u00b7er\u00b7bitt\u00b7lich", "wird", "ein", "stren\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kreis zuletzt Dich einbegreifen.", "tokens": ["Kreis", "zu\u00b7letzt", "Dich", "ein\u00b7be\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}