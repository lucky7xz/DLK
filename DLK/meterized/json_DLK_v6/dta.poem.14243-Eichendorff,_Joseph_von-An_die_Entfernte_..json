{"dta.poem.14243": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' es oft in Deiner Brust gelesen:", "tokens": ["Ich", "hab'", "es", "oft", "in", "Dei\u00b7ner", "Brust", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie hast Du recht mich in mir selbst gefunden,", "tokens": ["Nie", "hast", "Du", "recht", "mich", "in", "mir", "selbst", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "PRF", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fremd blieb, zu keck und treibend Dir mein Wesen,", "tokens": ["Fremd", "blieb", ",", "zu", "keck", "und", "trei\u00b7bend", "Dir", "mein", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PTKA", "ADJD", "KON", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und so bin ich im Strome Dir verschwunden.", "tokens": ["Und", "so", "bin", "ich", "im", "Stro\u00b7me", "Dir", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O nenn' drum nicht die sch\u00f6ne Jugend wilde,", "tokens": ["O", "nenn'", "drum", "nicht", "die", "sch\u00f6\u00b7ne", "Ju\u00b7gend", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PAV", "PTKNEG", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die mit dem Leben und mit seinen Schmerzen", "tokens": ["Die", "mit", "dem", "Le\u00b7ben", "und", "mit", "sei\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mag unbek\u00fcmmert scherzen,", "tokens": ["Mag", "un\u00b7be\u00b7k\u00fcm\u00b7mert", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Weil sie die Brust reich f\u00fchlt und ernst und milde!", "tokens": ["Weil", "sie", "die", "Brust", "reich", "f\u00fchlt", "und", "ernst", "und", "mil\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVFIN", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}