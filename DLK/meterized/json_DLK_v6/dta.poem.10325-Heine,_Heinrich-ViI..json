{"dta.poem.10325": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Nun hast du das Kaufgeld, nun z\u00f6gerst du doch?", "tokens": ["Nun", "hast", "du", "das", "Kauf\u00b7geld", ",", "nun", "z\u00f6\u00b7gerst", "du", "doch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Blutfinstrer Gesell, was z\u00f6gerst du noch?", "tokens": ["Blut\u00b7finst\u00b7rer", "Ge\u00b7sell", ",", "was", "z\u00f6\u00b7gerst", "du", "noch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "++--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Schon sitze ich harrend im K\u00e4mmerlein traut,", "tokens": ["Schon", "sit\u00b7ze", "ich", "har\u00b7rend", "im", "K\u00e4m\u00b7mer\u00b7lein", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Mitternacht nah't schon, \u2014 es fehlt nur die Braut.", "tokens": ["Und", "Mit\u00b7ter\u00b7nacht", "nah't", "schon", ",", "es", "fehlt", "nur", "die", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Viel schauernde L\u00fcftchen vom Kirchhofe wehn;", "tokens": ["Viel", "schau\u00b7ern\u00b7de", "L\u00fcft\u00b7chen", "vom", "Kirch\u00b7ho\u00b7fe", "wehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr L\u00fcftchen! habt ihr mein Br\u00e4utchen gesehn?", "tokens": ["Ihr", "L\u00fcft\u00b7chen", "!", "habt", "ihr", "mein", "Br\u00e4ut\u00b7chen", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel blasse Larven gestalten sich da,", "tokens": ["Viel", "blas\u00b7se", "Lar\u00b7ven", "ge\u00b7stal\u00b7ten", "sich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umknixen mich grinsend, und nicken: O ja!", "tokens": ["Um\u00b7kni\u00b7xen", "mich", "grin\u00b7send", ",", "und", "ni\u00b7cken", ":", "O", "ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "KON", "VVINF", "$.", "ITJ", "ITJ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Pack' aus, was bringst du f\u00fcr Bothschafterei,", "tokens": ["Pack'", "aus", ",", "was", "bringst", "du", "f\u00fcr", "Both\u00b7schaf\u00b7te\u00b7rei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du schwarzer Schlingel in Feuerlivrei?", "tokens": ["Du", "schwar\u00b7zer", "Schlin\u00b7gel", "in", "Feu\u00b7er\u00b7liv\u00b7rei", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u201edie gn\u00e4dige Herrschaft meldet sich an,", "tokens": ["\u201e", "die", "gn\u00e4\u00b7di\u00b7ge", "Herr\u00b7schaft", "mel\u00b7det", "sich", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gleich kommt sie gefahren im Drachengespann.\u201c", "tokens": ["Gleich", "kommt", "sie", "ge\u00b7fah\u00b7ren", "im", "Dra\u00b7chen\u00b7ge\u00b7spann", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Du lieb grau M\u00e4nnchen, was ist dein Begehr?", "tokens": ["Du", "lieb", "grau", "M\u00e4nn\u00b7chen", ",", "was", "ist", "dein", "Be\u00b7gehr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "NN", "$,", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein todter Magister, was treibt dich her?", "tokens": ["Mein", "tod\u00b7ter", "Ma\u00b7gis\u00b7ter", ",", "was", "treibt", "dich", "her", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er schaut mich mit schweigend tr\u00fcbseligem Blick,", "tokens": ["Er", "schaut", "mich", "mit", "schwei\u00b7gend", "tr\u00fcb\u00b7se\u00b7li\u00b7gem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sch\u00fcttelt das Haupt, und wandelt zur\u00fcck.", "tokens": ["Und", "sch\u00fct\u00b7telt", "das", "Haupt", ",", "und", "wan\u00b7delt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Was winselt und wedelt mein zott'ger Gesell?", "tokens": ["Was", "win\u00b7selt", "und", "we\u00b7delt", "mein", "zott'\u00b7ger", "Ge\u00b7sell", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Was glimmert schwarz Katers Auge so hell?", "tokens": ["Was", "glim\u00b7mert", "schwarz", "Ka\u00b7ters", "Au\u00b7ge", "so", "hell", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was heulen die Weiber mit fliegendem Haar?", "tokens": ["Was", "heu\u00b7len", "die", "Wei\u00b7ber", "mit", "flie\u00b7gen\u00b7dem", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was lullt mir Frau Amme mein Wiegenlied gar?", "tokens": ["Was", "lullt", "mir", "Frau", "Am\u00b7me", "mein", "Wie\u00b7gen\u00b7lied", "gar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "NE", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+--+---", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Frau Amme bleib heut mit dem Singsang zu Haus,", "tokens": ["Frau", "Am\u00b7me", "bleib", "heut", "mit", "dem", "Sings\u00b7ang", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Das Eiapopeia ist lange schon aus;", "tokens": ["Das", "Ei\u00b7a\u00b7po\u00b7pei\u00b7a", "ist", "lan\u00b7ge", "schon", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich fey're gar heute mein Hochzeitfest, \u2014", "tokens": ["Ich", "fey'\u00b7re", "gar", "heu\u00b7te", "mein", "Hoch\u00b7zeit\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da schau' mal, dort kommen schon zierliche G\u00e4st'.", "tokens": ["Da", "schau'", "mal", ",", "dort", "kom\u00b7men", "schon", "zier\u00b7li\u00b7che", "G\u00e4st'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Da schau' mal! Ihr Herren, das nenn' ich galant!", "tokens": ["Da", "schau'", "mal", "!", "Ihr", "Her\u00b7ren", ",", "das", "nenn'", "ich", "ga\u00b7lant", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ihr tragt, statt der H\u00fcte, die K\u00f6pf' in der Hand!", "tokens": ["Ihr", "tragt", ",", "statt", "der", "H\u00fc\u00b7te", ",", "die", "K\u00f6pf'", "in", "der", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr Zappelbein-Leutchen im Galgen-Ornat,", "tokens": ["Ihr", "Zap\u00b7pel\u00b7bein\u00b7Leut\u00b7chen", "im", "Gal\u00b7gen\u00b7Or\u00b7nat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Wind ist still, was kommt Ihr so spat?", "tokens": ["Der", "Wind", "ist", "still", ",", "was", "kommt", "Ihr", "so", "spat", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PWS", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Da kommt auch alt Besenstielm\u00fctterchen schon,", "tokens": ["Da", "kommt", "auch", "alt", "Be\u00b7sen\u00b7stiel\u00b7m\u00fct\u00b7ter\u00b7chen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ach segne mich, M\u00fctterchen, bin ja dein Sohn;", "tokens": ["Ach", "seg\u00b7ne", "mich", ",", "M\u00fct\u00b7ter\u00b7chen", ",", "bin", "ja", "dein", "Sohn", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "NN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da zittert der Mund im wei\u00dfen Gesicht:", "tokens": ["Da", "zit\u00b7tert", "der", "Mund", "im", "wei\u00b7\u00dfen", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u201ein Ewigkeit Amen!\u201c alt M\u00fctterchen spricht.", "tokens": ["\u201e", "in", "E\u00b7wig\u00b7keit", "A\u00b7men", "!", "\u201c", "alt", "M\u00fct\u00b7ter\u00b7chen", "spricht", "."], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "NN", "$.", "$(", "ADJD", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Zw\u00f6lf windd\u00fcrre Musiker schlendern herein;", "tokens": ["Zw\u00f6lf", "wind\u00b7d\u00fcr\u00b7re", "Mu\u00b7si\u00b7ker", "schlen\u00b7dern", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Blind Fidelweib holpert wohl hintendrein.", "tokens": ["Blind", "Fi\u00b7del\u00b7weib", "hol\u00b7pert", "wohl", "hin\u00b7ten\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da schleppt der Hanswurst, in buntscheckiger Jack',", "tokens": ["Da", "schleppt", "der", "Hans\u00b7wurst", ",", "in", "bunt\u00b7sche\u00b7cki\u00b7ger", "Jack'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Todtengr\u00e4ber huckepack.", "tokens": ["Den", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "hu\u00b7cke\u00b7pack", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da tanzen zw\u00f6lf Klosterjungfrauen herein;", "tokens": ["Da", "tan\u00b7zen", "zw\u00f6lf", "Klos\u00b7ter\u00b7jung\u00b7frau\u00b7en", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die schielende Kupplerin f\u00fchret den Reih\u2019n.", "tokens": ["Die", "schie\u00b7len\u00b7de", "Kupp\u00b7le\u00b7rin", "f\u00fch\u00b7ret", "den", "Reih'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da folgen zw\u00f6lf l\u00fcsterne Pf\u00e4ffelein schon,", "tokens": ["Da", "fol\u00b7gen", "zw\u00f6lf", "l\u00fcs\u00b7ter\u00b7ne", "Pf\u00e4f\u00b7fe\u00b7lein", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und pfeifen ein Schandlied im Kirchenton\u2019.", "tokens": ["Und", "pfei\u00b7fen", "ein", "Schand\u00b7lied", "im", "Kir\u00b7chen\u00b7ton'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Herr Tr\u00f6dler, o schrei dir nicht blau das Gesicht.", "tokens": ["Herr", "Tr\u00f6d\u00b7ler", ",", "o", "schrei", "dir", "nicht", "blau", "das", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "FM", "FM", "PPER", "PTKNEG", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Fegfeuer n\u00fctzt mir dein Pelzr\u00f6ckel nicht;", "tokens": ["Im", "Feg\u00b7feu\u00b7er", "n\u00fctzt", "mir", "dein", "Pel\u00b7zr\u00f6\u00b7ckel", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Dort heizet man gratis jahraus, jahrein,", "tokens": ["Dort", "hei\u00b7zet", "man", "gra\u00b7tis", "ja\u00b7hraus", ",", "ja\u00b7hrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$,", "PTKANT", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Statt mit Holz, mit F\u00fcrsten- und Bettlergebein.", "tokens": ["Statt", "mit", "Holz", ",", "mit", "F\u00fcrs\u00b7ten", "und", "Bett\u00b7ler\u00b7ge\u00b7bein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "Die Blumenm\u00e4dchen sind bucklicht und krumm,", "tokens": ["Die", "Blu\u00b7men\u00b7m\u00e4d\u00b7chen", "sind", "buck\u00b7licht", "und", "krumm", ","], "token_info": ["word", "word", "word"