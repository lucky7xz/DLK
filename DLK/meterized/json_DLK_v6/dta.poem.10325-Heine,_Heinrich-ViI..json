"token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "An mein Herze pocht das Herze der Maid.", "tokens": ["An", "mein", "Her\u00b7ze", "pocht", "das", "Her\u00b7ze", "der", "Maid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sie pochen wohl beide vor Lust und vor Weh,", "tokens": ["Sie", "po\u00b7chen", "wohl", "bei\u00b7de", "vor", "Lust", "und", "vor", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und schweben hinauf in die Himmelsh\u00f6h'.", "tokens": ["Und", "schwe\u00b7ben", "hin\u00b7auf", "in", "die", "Him\u00b7mels\u00b7h\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.19": {"line.1": {"text": "Die Herzlein schwammen im Freudensee,", "tokens": ["Die", "Herz\u00b7lein", "schwam\u00b7men", "im", "Freu\u00b7den\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort oben in Gottes heil'ger H\u00f6h';", "tokens": ["Dort", "o\u00b7ben", "in", "Got\u00b7tes", "heil'\u00b7ger", "H\u00f6h'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "li