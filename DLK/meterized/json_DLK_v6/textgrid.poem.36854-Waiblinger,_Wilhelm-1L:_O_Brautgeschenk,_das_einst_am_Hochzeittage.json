{"textgrid.poem.36854": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Brautgeschenk, das einst am Hochzeittage", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Brautgeschenk, das einst am Hochzeittage", "tokens": ["O", "Braut\u00b7ge\u00b7schenk", ",", "das", "einst", "am", "Hoch\u00b7zeit\u00b7ta\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Proserpinen der gro\u00dfe Vater gab,", "tokens": ["Pro\u00b7ser\u00b7pi\u00b7nen", "der", "gro\u00b7\u00dfe", "Va\u00b7ter", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Ceres Liebe wie der Ceres Klage,", "tokens": ["Der", "Ce\u00b7res", "Lie\u00b7be", "wie", "der", "Ce\u00b7res", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dianens Wieg' und der Giganten Grab,", "tokens": ["Di\u00b7a\u00b7nens", "Wieg'", "und", "der", "Gi\u00b7gan\u00b7ten", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O sch\u00f6nste Heimath frommer G\u00f6ttersage,", "tokens": ["O", "sch\u00f6ns\u00b7te", "Hei\u00b7math", "from\u00b7mer", "G\u00f6t\u00b7ter\u00b7sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem K\u00f6nigsscepter und dem Hirtenstab,", "tokens": ["Dem", "K\u00f6\u00b7nigs\u00b7scep\u00b7ter", "und", "dem", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Nachtigall, dem Veilchen und Cyanen,", "tokens": ["Der", "Nach\u00b7ti\u00b7gall", ",", "dem", "Veil\u00b7chen", "und", "Cy\u00b7a\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Flamme heilig und des Bergs Orkanen;", "tokens": ["Der", "Flam\u00b7me", "hei\u00b7lig", "und", "des", "Bergs", "Or\u00b7ka\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O Eiland, mir geliebt seitdem ich liebe,", "tokens": ["O", "Ei\u00b7land", ",", "mir", "ge\u00b7liebt", "seit\u00b7dem", "ich", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVPP", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir werth seit ich f\u00fcr Heldenkraft entgl\u00fcht,", "tokens": ["Mir", "werth", "seit", "ich", "f\u00fcr", "Hel\u00b7den\u00b7kraft", "ent\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Seit an der Lipp', entflammt von gr\u00f6\u00dfrem Triebe,", "tokens": ["Seit", "an", "der", "Lipp'", ",", "ent\u00b7flammt", "von", "gr\u00f6\u00df\u00b7rem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Dichtkunst G\u00f6tterbecher mir gebl\u00fcht,", "tokens": ["Der", "Dicht\u00b7kunst", "G\u00f6t\u00b7ter\u00b7be\u00b7cher", "mir", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Seit ich die priesterlichen Pflichten \u00fcbe,", "tokens": ["Seit", "ich", "die", "pries\u00b7ter\u00b7li\u00b7chen", "Pflich\u00b7ten", "\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Feuer h\u00fctend, das in Delfi spr\u00fcht,", "tokens": ["Das", "Feu\u00b7er", "h\u00fc\u00b7tend", ",", "das", "in", "Del\u00b7fi", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Seit ich gelernt, wie gro\u00dfe M\u00e4nner werden,", "tokens": ["Seit", "ich", "ge\u00b7lernt", ",", "wie", "gro\u00b7\u00dfe", "M\u00e4n\u00b7ner", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und lehre was ich nie gelernt auf Erden.", "tokens": ["Und", "leh\u00b7re", "was", "ich", "nie", "ge\u00b7lernt", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch einmal, theures Eiland, la\u00df mich denken,", "tokens": ["Noch", "ein\u00b7mal", ",", "theu\u00b7res", "Ei\u00b7land", ",", "la\u00df", "mich", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was deine Berg' und Meere mir gezeigt.", "tokens": ["Was", "dei\u00b7ne", "Ber\u00b7g'", "und", "Mee\u00b7re", "mir", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Purpurglanz mit ihr mich zu versenken,", "tokens": ["Im", "Pur\u00b7pur\u00b7glanz", "mit", "ihr", "mich", "zu", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sonne, die zum goldnen Bad sich neigt,", "tokens": ["Der", "Son\u00b7ne", ",", "die", "zum", "gold\u00b7nen", "Bad", "sich", "neigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NE", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verm\u00f6cht' ich der Gestirne Lauf zu lenken \u2013", "tokens": ["Ver\u00b7m\u00f6cht'", "ich", "der", "Ge\u00b7stir\u00b7ne", "Lauf", "zu", "len\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O Lust, und wenn sie wieder ihm entsteigt,", "tokens": ["O", "Lust", ",", "und", "wenn", "sie", "wie\u00b7der", "ihm", "ent\u00b7steigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "KOUS", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dich wiedersehn am Morgen! Doch vergebens;", "tokens": ["Dich", "wie\u00b7der\u00b7sehn", "am", "Mor\u00b7gen", "!", "Doch", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mir blieb Erinn'rung nur, der Mond des Lebens.", "tokens": ["Mir", "blieb", "Er\u00b7inn'\u00b7rung", "nur", ",", "der", "Mond", "des", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch sterblich ist dem sterblichen Geschlechte", "tokens": ["Doch", "sterb\u00b7lich", "ist", "dem", "sterb\u00b7li\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Gl\u00fccks Geschenk, des Augenblickes Lust.", "tokens": ["Des", "Gl\u00fccks", "Ge\u00b7schenk", ",", "des", "Au\u00b7gen\u00b7bli\u00b7ckes", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur in der Gegenwart sind ihre Rechte", "tokens": ["Nur", "in", "der", "Ge\u00b7gen\u00b7wart", "sind", "ih\u00b7re", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Gegenwart verg\u00f6nnt, nur unbewu\u00dft;", "tokens": ["Der", "Ge\u00b7gen\u00b7wart", "ver\u00b7g\u00f6nnt", ",", "nur", "un\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du denkest, und schon nahn die strengen M\u00e4chte,", "tokens": ["Du", "den\u00b7kest", ",", "und", "schon", "nahn", "die", "stren\u00b7gen", "M\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Selbst das Gef\u00fchl bleibt nicht in deiner Brust,", "tokens": ["Selbst", "das", "Ge\u00b7f\u00fchl", "bleibt", "nicht", "in", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und du gewahrst mit Freuden oder Trauer,", "tokens": ["Und", "du", "ge\u00b7wahrst", "mit", "Freu\u00b7den", "o\u00b7der", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur die Erinnerung hat ew'ge Dauer.", "tokens": ["Nur", "die", "E\u00b7rin\u00b7ne\u00b7rung", "hat", "ew'\u00b7ge", "Dau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So nimm mein Lebewohl, o Blumenwiege", "tokens": ["So", "nimm", "mein", "Le\u00b7be\u00b7wohl", ",", "o", "Blu\u00b7men\u00b7wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "$,", "FM", "NN"], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Der Heldenfabel, wunderreichstes Land;", "tokens": ["Der", "Hel\u00b7den\u00b7fa\u00b7bel", ",", "wun\u00b7der\u00b7reichs\u00b7tes", "Land", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So je noch griech'sches Ufer mir erstiege,", "tokens": ["So", "je", "noch", "griech'\u00b7sches", "U\u00b7fer", "mir", "er\u00b7stie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begr\u00fc\u00df' ich's als vertrauten Heimathsstrand,", "tokens": ["Be\u00b7gr\u00fc\u00df'", "ich's", "als", "ver\u00b7trau\u00b7ten", "Hei\u00b7maths\u00b7strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So je ich das Verh\u00e4ngni\u00df noch besiege,", "tokens": ["So", "je", "ich", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "noch", "be\u00b7sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Das manchen Kranz um meine Schl\u00e4fe wand,", "tokens": ["Das", "man\u00b7chen", "Kranz", "um", "mei\u00b7ne", "Schl\u00e4\u00b7fe", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So mag die Mutter freundlich mich empfangen;", "tokens": ["So", "mag", "die", "Mut\u00b7ter", "freund\u00b7lich", "mich", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn ihre Tochter k\u00fc\u00dft' ich auf die Wangen.", "tokens": ["Denn", "ih\u00b7re", "Toch\u00b7ter", "k\u00fc\u00dft'", "ich", "auf", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ja, stolz und freudig darf ich's euch gestehen,", "tokens": ["Ja", ",", "stolz", "und", "freu\u00b7dig", "darf", "ich's", "euch", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "KON", "ADJD", "VMFIN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geweihte, die ihr fromm die Vorwelt ehrt,", "tokens": ["Ge\u00b7weih\u00b7te", ",", "die", "ihr", "fromm", "die", "Vor\u00b7welt", "ehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die G\u00f6ttinnen auf Ennas Felsenh\u00f6hen,", "tokens": ["Die", "G\u00f6t\u00b7tin\u00b7nen", "auf", "En\u00b7nas", "Fel\u00b7sen\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Huld sind sie zu mir zur\u00fcckgekehrt.", "tokens": ["Voll", "Huld", "sind", "sie", "zu", "mir", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Proserpinen, Dianen durft' ich sehen,", "tokens": ["Pro\u00b7ser\u00b7pi\u00b7nen", ",", "Di\u00b7a\u00b7nen", "durft'", "ich", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Athene hat mich weisen Rath gelehrt;", "tokens": ["A\u00b7the\u00b7ne", "hat", "mich", "wei\u00b7sen", "Rath", "ge\u00b7lehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.7": {"text": "Der Seher, der der Erde sich entwunden,", "tokens": ["Der", "Se\u00b7her", ",", "der", "der", "Er\u00b7de", "sich", "ent\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hat stets das Himmlische sich nah gefunden.", "to