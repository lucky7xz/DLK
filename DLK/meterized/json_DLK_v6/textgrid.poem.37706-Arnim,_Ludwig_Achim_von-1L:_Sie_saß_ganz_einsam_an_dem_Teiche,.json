{"textgrid.poem.37706": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie sa\u00df ganz einsam an dem Teiche,", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie sa\u00df ganz einsam an dem Teiche,", "tokens": ["Sie", "sa\u00df", "ganz", "ein\u00b7sam", "an", "dem", "Tei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich war versteckt von gr\u00fcnem Zweige,", "tokens": ["Ich", "war", "ver\u00b7steckt", "von", "gr\u00fc\u00b7nem", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wu\u00dfte nicht mich zu erkl\u00e4ren,", "tokens": ["Und", "wu\u00df\u00b7te", "nicht", "mich", "zu", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Ihr Fr\u00fchst\u00fcck sollt' sie erst verzehren,", "tokens": ["Ihr", "Fr\u00fch\u00b7st\u00fcck", "sollt'", "sie", "erst", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hatte da des Brods so viel,", "tokens": ["Sie", "hat\u00b7te", "da", "des", "Brods", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sie's verkr\u00fcmelte zum Spiel,", "tokens": ["Da\u00df", "sie's", "ver\u00b7kr\u00fc\u00b7mel\u00b7te", "zum", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die kleinen Fische blinkten munter,", "tokens": ["Die", "klei\u00b7nen", "Fi\u00b7sche", "blink\u00b7ten", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sprangen aufw\u00e4rts, tauchten unter,", "tokens": ["Und", "spran\u00b7gen", "auf\u00b7w\u00e4rts", ",", "tauch\u00b7ten", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "VVFIN", "APPR", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie sah in Ruh' dem Dr\u00e4ngen zu", "tokens": ["Sie", "sah", "in", "Ruh'", "dem", "Dr\u00e4n\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und warf mit ihrem seidnen Schuh", "tokens": ["Und", "warf", "mit", "ih\u00b7rem", "seid\u00b7nen", "Schuh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Statt Brod viel kleine Steine nieder,", "tokens": ["Statt", "Brod", "viel", "klei\u00b7ne", "Stei\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das that mir leid, war mir zuwider,", "tokens": ["Das", "that", "mir", "leid", ",", "war", "mir", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die stummen Thierchen so zu necken,", "tokens": ["Die", "stum\u00b7men", "Thier\u00b7chen", "so", "zu", "ne\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Ich unterlie\u00df mich zu entdecken,", "tokens": ["Ich", "un\u00b7ter\u00b7lie\u00df", "mich", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ich meinte tief in sie zu blicken,", "tokens": ["Ich", "mein\u00b7te", "tief", "in", "sie", "zu", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Begl\u00fccken kann sie nicht, nur zwicken,", "tokens": ["Be\u00b7gl\u00fc\u00b7cken", "kann", "sie", "nicht", ",", "nur", "zwi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Sie f\u00fchrte jeden lieber an,", "tokens": ["Sie", "f\u00fchr\u00b7te", "je\u00b7den", "lie\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als da\u00df sie einen n\u00e4hm' zum Mann.", "tokens": ["Als", "da\u00df", "sie", "ei\u00b7nen", "n\u00e4hm'", "zum", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}