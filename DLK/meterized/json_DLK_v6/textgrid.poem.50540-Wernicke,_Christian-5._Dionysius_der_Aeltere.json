{"textgrid.poem.50540": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "5. Dionysius der Aeltere", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Wie dass ein edles ", "tokens": ["Wie", "dass", "ein", "ed\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Hast du woll je von mir geh\u00f6rt,", "tokens": ["Hast", "du", "woll", "je", "von", "mir", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fragt' er, dass ich, noch jung, solch eine That begangen?", "tokens": ["Fragt'", "er", ",", "dass", "ich", ",", "noch", "jung", ",", "solch", "ei\u00b7ne", "That", "be\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "ADV", "ADJD", "$,", "PIAT", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Sohn sprach: dieses ist nicht ohn',", "tokens": ["Der", "Sohn", "sprach", ":", "die\u00b7ses", "ist", "nicht", "ohn'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PDS", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr waret aber auch nicht eines ", "tokens": ["Ihr", "wa\u00b7ret", "a\u00b7ber", "auch", "nicht", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und du, antwortet' er, mit zornigen Geb\u00e4hrden,", "tokens": ["Und", "du", ",", "ant\u00b7wor\u00b7tet'", "er", ",", "mit", "zor\u00b7ni\u00b7gen", "Ge\u00b7b\u00e4hr\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wirst keines ", "tokens": ["Wirst", "kei\u00b7nes"], "token_info": ["word", "word"], "pos": ["VAFIN", "PIS"], "meter": "-+-", "measure": "amphibrach.single"}}}}}