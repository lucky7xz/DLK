{"dta.poem.15934": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "139.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Freunde schweigen still; kein Laut hat mir entdeckt,", "tokens": ["Die", "Freun\u00b7de", "schwei\u00b7gen", "still", ";", "kein", "Laut", "hat", "mir", "ent\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$.", "PIAT", "APPR", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie, was ich Neustes aufgetischt hab', ihnen schmeckt.", "tokens": ["Wie", ",", "was", "ich", "Neus\u00b7tes", "auf\u00b7ge\u00b7tischt", "hab'", ",", "ih\u00b7nen", "schmeckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "PPER", "NN", "VVPP", "VAFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur Fremde h\u00f6r' ich, die dort auf dem Markte schelten,", "tokens": ["Nur", "Frem\u00b7de", "h\u00f6r'", "ich", ",", "die", "dort", "auf", "dem", "Mark\u00b7te", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Winkel loben dort; was kann mir beides gelten?", "tokens": ["Im", "Win\u00b7kel", "lo\u00b7ben", "dort", ";", "was", "kann", "mir", "bei\u00b7des", "gel\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "$.", "PWS", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich bin zu alt, um neu zu modeln meine Sachen,", "tokens": ["Ich", "bin", "zu", "alt", ",", "um", "neu", "zu", "mo\u00b7deln", "mei\u00b7ne", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weder Tadel kann noch Lob mich besser machen.", "tokens": ["Und", "we\u00b7der", "Ta\u00b7del", "kann", "noch", "Lob", "mich", "bes\u00b7ser", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "VMFIN", "ADV", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verbitten will ich mir ganz alle Zeitungspost,", "tokens": ["Ver\u00b7bit\u00b7ten", "will", "ich", "mir", "ganz", "al\u00b7le", "Zei\u00b7tungs\u00b7post", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst zufrieden seyn mit meiner Hausmannskost.", "tokens": ["Und", "selbst", "zu\u00b7frie\u00b7den", "seyn", "mit", "mei\u00b7ner", "Haus\u00b7manns\u00b7kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}