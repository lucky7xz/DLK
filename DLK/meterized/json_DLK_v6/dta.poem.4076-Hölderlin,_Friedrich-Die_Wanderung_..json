{"dta.poem.4076": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wanderung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcckselig Sunvien, meine Mutter!", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "Sun\u00b7vi\u00b7en", ",", "mei\u00b7ne", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auch du, der gl\u00e4nzenderen, der Schwester", "tokens": ["Auch", "du", ",", "der", "gl\u00e4n\u00b7zen\u00b7de\u00b7ren", ",", "der", "Schwes\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "$,", "ART", "ADJA", "$,", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Lombarda dr\u00fcben gleich,", "tokens": ["Lom\u00b7bar\u00b7da", "dr\u00fc\u00b7ben", "gleich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Von hundert B\u00e4chen durchflossen!", "tokens": ["Von", "hun\u00b7dert", "B\u00e4\u00b7chen", "durch\u00b7flos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und B\u00e4ume genug, wei\u00dfbl\u00fchend und r\u00f6thlich,", "tokens": ["Und", "B\u00e4u\u00b7me", "ge\u00b7nug", ",", "wei\u00df\u00b7bl\u00fc\u00b7hend", "und", "r\u00f6th\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und dunklere, wild, tief gr\u00fcnendes Laub's voll \u2014", "tokens": ["Und", "dunk\u00b7le\u00b7re", ",", "wild", ",", "tief", "gr\u00fc\u00b7nen\u00b7des", "Laub's", "voll"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "ADJD", "$,", "ADJD", "ADJA", "NN", "ADJD", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und Alpengebirg auch \u00fcberschattet,", "tokens": ["Und", "Al\u00b7pen\u00b7ge\u00b7birg", "auch", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Uraltes, dich; denn nah dem Herde des Hauses", "tokens": ["Ur\u00b7al\u00b7tes", ",", "dich", ";", "denn", "nah", "dem", "Her\u00b7de", "des", "Hau\u00b7ses"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "$.", "KON", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wohnst du, und h\u00f6rst, wie drinnen", "tokens": ["Wohnst", "du", ",", "und", "h\u00f6rst", ",", "wie", "drin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KON", "VVFIN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Aus silbernen Opferschalen", "tokens": ["Aus", "sil\u00b7ber\u00b7nen", "Op\u00b7fer\u00b7scha\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Der Quell rauscht, ausgesch\u00fcttet", "tokens": ["Der", "Quell", "rauscht", ",", "aus\u00b7ge\u00b7sch\u00fct\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von reinen H\u00e4nden, wenn ber\u00fchrt", "tokens": ["Von", "rei\u00b7nen", "H\u00e4n\u00b7den", ",", "wenn", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von warmen Stralen", "tokens": ["Von", "war\u00b7men", "Stra\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Krystallenes Eis, und umgest\u00fcrzt", "tokens": ["Krys\u00b7tal\u00b7le\u00b7nes", "Eis", ",", "und", "um\u00b7ge\u00b7st\u00fcrzt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Vom leichtanregenden Lichte", "tokens": ["Vom", "leich\u00b7tan\u00b7re\u00b7gen\u00b7den", "Lich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Der schneeige Gipfel \u00fcbergie\u00dft die Erde", "tokens": ["Der", "schne\u00b7ei\u00b7ge", "Gip\u00b7fel", "\u00fc\u00b7berg\u00b7ie\u00dft", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Mit reinestem Wasser. Darum ist", "tokens": ["Mit", "rei\u00b7nes\u00b7tem", "Was\u00b7ser", ".", "Da\u00b7rum", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PAV", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Dir angeboren die Treue. Schwer verl\u00e4\u00dft", "tokens": ["Dir", "an\u00b7ge\u00b7bo\u00b7ren", "die", "Treu\u00b7e", ".", "Schwer", "ver\u00b7l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVPP", "ART", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Was nahe dem Ursprung wohnet, den Ort.", "tokens": ["Was", "na\u00b7he", "dem", "Ur\u00b7sprung", "woh\u00b7net", ",", "den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und deine Kinder, die St\u00e4dte", "tokens": ["Und", "dei\u00b7ne", "Kin\u00b7der", ",", "die", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Am weithind\u00e4mmernden See,", "tokens": ["Am", "weit\u00b7hin\u00b7d\u00e4m\u00b7mern\u00b7den", "See", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "An Neckars Weiden, am Rheine,", "tokens": ["An", "Ne\u00b7ckars", "Wei\u00b7den", ",", "am", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "APPRART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Sie alle meinen, es w\u00e4re", "tokens": ["Sie", "al\u00b7le", "mei\u00b7nen", ",", "es", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Sonst nirgend besser zu wohnen.", "tokens": ["Sonst", "nir\u00b7gend", "bes\u00b7ser", "zu", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Ich aber will dem Kaukasos zu!", "tokens": ["Ich", "a\u00b7ber", "will", "dem", "Kau\u00b7ka\u00b7sos", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.26": {"text": "Denn sagen h\u00f6rt' ich", "tokens": ["Denn", "sa\u00b7gen", "h\u00f6rt'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Noch heut in den L\u00fcften:", "tokens": ["Noch", "heut", "in", "den", "L\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Frei sey'n, wie Schwalben, die Dichter.", "tokens": ["Frei", "sey'n", ",", "wie", "Schwal\u00b7ben", ",", "die", "Dich\u00b7ter."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "abbreviation"], "pos": ["ADJD", "VAINF", "$,", "PWAV", "NN", "$,", "ART", "NN"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Auch hat in j\u00fcngern Tagen", "tokens": ["Auch", "hat", "in", "j\u00fcn\u00b7gern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Sonst Eines mir vertraut:", "tokens": ["Sonst", "Ei\u00b7nes", "mir", "ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Es seyen vor alter Zeit", "tokens": ["Es", "se\u00b7yen", "vor", "al\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Die Unsrigen einst, ein sinnig Geschlecht,", "tokens": ["Die", "Uns\u00b7ri\u00b7gen", "einst", ",", "ein", "sin\u00b7nig", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.33": {"text": "Still fortgezogen von Wellen der Donau,", "tokens": ["Still", "fort\u00b7ge\u00b7zo\u00b7gen", "von", "Wel\u00b7len", "der", "Do\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "ART", "NE", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Dort mit der Sonne Kindern", "tokens": ["Dort", "mit", "der", "Son\u00b7ne", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Am Sommertage, da diese", "tokens": ["Am", "Som\u00b7mer\u00b7ta\u00b7ge", ",", "da", "die\u00b7se"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PDAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.36": {"text": "Sich Schatten suchten, zusammen", "tokens": ["Sich", "Schat\u00b7ten", "such\u00b7ten", ",", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PRF", "NN", "VVFIN", "$,", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Am schwarzen Meere gekommen,", "tokens": ["Am", "schwar\u00b7zen", "Mee\u00b7re", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Und nicht umsonst sey die\u00df", "tokens": ["Und", "nicht", "um\u00b7sonst", "sey", "die\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADV", "VAFIN", "PDS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Das gastfreundliche genennet.", "tokens": ["Das", "gast\u00b7freund\u00b7li\u00b7che", "ge\u00b7nen\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Denn als ihr Staunen vor\u00fcber war,", "tokens": ["Denn", "als", "ihr", "Stau\u00b7nen", "vor\u00b7\u00fc\u00b7ber", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Da nahten die Andern zuerst; dann setzten auch", "tokens": ["Da", "nah\u00b7ten", "die", "An\u00b7dern", "zu\u00b7erst", ";", "dann", "setz\u00b7ten", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADV", "$.", "ADV", "VVFIN", "ADV"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "Die Unseren sich neugierig unter den Oelbaum.", "tokens": ["Die", "Un\u00b7se\u00b7ren", "sich", "neu\u00b7gie\u00b7rig", "un\u00b7ter", "den", "O\u00b7el\u00b7baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.43": {"text": "Doch, als sich ihre Gewande ber\u00fchrt,", "tokens": ["Doch", ",", "als", "sich", "ih\u00b7re", "Ge\u00b7wan\u00b7de", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.44": {"text": "Und Keiner vernehmen konnte", "tokens": ["Und", "Kei\u00b7ner", "ver\u00b7neh\u00b7men", "konn\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "VMFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Die eigene Rede des Andern, w\u00e4re wohl", "tokens": ["Die", "ei\u00b7ge\u00b7ne", "Re\u00b7de", "des", "An\u00b7dern", ",", "w\u00e4\u00b7re", "wohl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "$,", "VAFIN", "ADV"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.46": {"text": "Entstanden ein Zwist, wenn nicht aus Zweigen", "tokens": ["Ent\u00b7stan\u00b7den", "ein", "Zwist", ",", "wenn", "nicht", "aus", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PTKNEG", "APPR", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.47": {"text": "herunter", "tokens": ["her\u00b7un\u00b7ter"], "token_info": ["word"], "pos": ["PTKVZ"], "meter": "-+-", "measure": "amphibrach.single"}, "line.48": {"text": "Gekommen w\u00e4re die K\u00fchlung,", "tokens": ["Ge\u00b7kom\u00b7men", "w\u00e4\u00b7re", "die", "K\u00fch\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.49": {"text": "Die L\u00e4cheln \u00fcber das Angesicht", "tokens": ["Die", "L\u00e4\u00b7cheln", "\u00fc\u00b7ber", "das", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.50": {"text": "Der Streitenden \u00f6fters breitet; und eine Weile", "tokens": ["Der", "Strei\u00b7ten\u00b7den", "\u00f6f\u00b7ters", "brei\u00b7tet", ";", "und", "ei\u00b7ne", "Wei\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "$.", "KON", "ART", "NN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.51": {"text": "Sah'n still sie auf. Dann reichten sie sich", "tokens": ["Sah'n", "still", "sie", "auf", ".", "Dann", "reich\u00b7ten", "sie", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.52": {"text": "Die H\u00e4nde liebend einander. Und bald", "tokens": ["Die", "H\u00e4n\u00b7de", "lie\u00b7bend", "ein\u00b7an\u00b7der", ".", "Und", "bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADV", "$.", "KON", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.53": {"text": "Vertauschten sie Waffen und all'", "tokens": ["Ver\u00b7tauschten", "sie", "Waf\u00b7fen", "und", "all'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "KON", "PIS"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.54": {"text": "Die lieben G\u00fcter des Hauses,", "tokens": ["Die", "lie\u00b7ben", "G\u00fc\u00b7ter", "des", "Hau\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.55": {"text": "Vertauschten das Wort auch und es w\u00fcnschten", "tokens": ["Ver\u00b7tauschten", "das", "Wort", "auch", "und", "es", "w\u00fcnschten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "KON", "PPER", "VVFIN"], "meter"