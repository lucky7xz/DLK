{"textgrid.poem.50949": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Gerichte rief der Fr\u00fchling.", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Gerichte rief der Fr\u00fchling.", "tokens": ["Zum", "Ge\u00b7rich\u00b7te", "rief", "der", "Fr\u00fch\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit Strenge zu verfahren", "tokens": ["Und", "mit", "Stren\u00b7ge", "zu", "ver\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen ketzerisch verstockte", "tokens": ["Ge\u00b7gen", "ket\u00b7ze\u00b7risch", "ver\u00b7stock\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcbelsinnige Verzweiflung,", "tokens": ["\u00dc\u00b7bel\u00b7sin\u00b7ni\u00b7ge", "Ver\u00b7zwei\u00b7flung", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Haben Seine Heiligkeit", "tokens": ["Ha\u00b7ben", "Sei\u00b7ne", "Hei\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bei der Sonne Glanz geschworen!", "tokens": ["Bei", "der", "Son\u00b7ne", "Glanz", "ge\u00b7schwo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und in gr\u00fcnem Feuer flammen", "tokens": ["Und", "in", "gr\u00fc\u00b7nem", "Feu\u00b7er", "flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle B\u00e4ume nun auf Erden;", "tokens": ["Al\u00b7le", "B\u00e4u\u00b7me", "nun", "auf", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Baum ist eine Flamme!", "tokens": ["Je\u00b7der", "Baum", "ist", "ei\u00b7ne", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gesch\u00fcrt sind alle Gluten,", "tokens": ["Und", "ge\u00b7sch\u00fcrt", "sind", "al\u00b7le", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Angefacht gl\u00fchn alle Rosen,", "tokens": ["An\u00b7ge\u00b7facht", "gl\u00fchn", "al\u00b7le", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4hrend die schismatisch grauen", "tokens": ["W\u00e4h\u00b7rend", "die", "schis\u00b7ma\u00b7tisch", "grau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Aufgel\u00f6sten Nebelflocken", "tokens": ["Auf\u00b7ge\u00b7l\u00f6s\u00b7ten", "Ne\u00b7bel\u00b7flo\u00b7cken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Klagend durch die L\u00fcfte flattern,", "tokens": ["Kla\u00b7gend", "durch", "die", "L\u00fcf\u00b7te", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gleich verbrannter Ketzer Asche!", "tokens": ["Gleich", "ver\u00b7brann\u00b7ter", "Ket\u00b7zer", "A\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Doch der heilig ernste Himmel", "tokens": ["Doch", "der", "hei\u00b7lig", "erns\u00b7te", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.11": {"text": "L\u00e4\u00dft sie ohne Spur verschwinden,", "tokens": ["L\u00e4\u00dft", "sie", "oh\u00b7ne", "Spur", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und er schaut ins gr\u00fcne Feuer", "tokens": ["Und", "er", "schaut", "ins", "gr\u00fc\u00b7ne", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mit erbarmungsloser Bl\u00e4ue.", "tokens": ["Mit", "er\u00b7bar\u00b7mungs\u00b7lo\u00b7ser", "Bl\u00e4u\u00b7e", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Habt ihr jetzo unter euch", "tokens": ["Habt", "ihr", "jet\u00b7zo", "un\u00b7ter", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen schlimmen und verschrobnen,", "tokens": ["Ei\u00b7nen", "schlim\u00b7men", "und", "ver\u00b7schrob\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heuchlerischen und verstockten", "tokens": ["Heuch\u00b7le\u00b7ri\u00b7schen", "und", "ver\u00b7stock\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verbohrten Hypochonder,", "tokens": ["Und", "ver\u00b7bohr\u00b7ten", "Hy\u00b7po\u00b7chon\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der da zwischen Gut und B\u00f6se", "tokens": ["Der", "da", "zwi\u00b7schen", "Gut", "und", "B\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eigensinnig schwankt und zweifelt,", "tokens": ["Ei\u00b7gen\u00b7sin\u00b7nig", "schwankt", "und", "zwei\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weder warm noch kalt kann werden", "tokens": ["We\u00b7der", "warm", "noch", "kalt", "kann", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADJD", "VMFIN", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder zu gerechtem Argwohn", "tokens": ["O\u00b7der", "zu", "ge\u00b7rech\u00b7tem", "Arg\u00b7wohn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Grund gibt, da\u00df sein schwarzes Innres", "tokens": ["Grund", "gibt", ",", "da\u00df", "sein", "schwar\u00b7zes", "Inn\u00b7res"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wohl ein ungeheures hohles,", "tokens": ["Wohl", "ein", "un\u00b7ge\u00b7heu\u00b7res", "hoh\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aufgeblasnes Schisma berge:", "tokens": ["Auf\u00b7ge\u00b7blas\u00b7nes", "Schis\u00b7ma", "ber\u00b7ge", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Diesen legt nun auf die Folter,", "tokens": ["Die\u00b7sen", "legt", "nun", "auf", "die", "Fol\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen lasset nun bekennen!", "tokens": ["Die\u00b7sen", "las\u00b7set", "nun", "be\u00b7ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bindet ihn mit jungem Efeu,", "tokens": ["Bin\u00b7det", "ihn", "mit", "jun\u00b7gem", "E\u00b7feu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Werft ihn nieder auf die Rosen!", "tokens": ["Werft", "ihn", "nie\u00b7der", "auf", "die", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gie\u00dft ihm Wein auf seine Zunge,", "tokens": ["Gie\u00dft", "ihm", "Wein", "auf", "sei\u00b7ne", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fl\u00fcssig hei\u00dfes Gold des Weines,", "tokens": ["Fl\u00fcs\u00b7sig", "hei\u00b7\u00dfes", "Gold", "des", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das den Mann zum Beichten zwingt,", "tokens": ["Das", "den", "Mann", "zum", "Beich\u00b7ten", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Glas auf Glas, bis er bekennt!", "tokens": ["Glas", "auf", "Glas", ",", "bis", "er", "be\u00b7kennt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zeiget sich ein Hoffnungsfunken,", "tokens": ["Zei\u00b7get", "sich", "ein", "Hoff\u00b7nungs\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein Funken heitren Glaubens,", "tokens": ["Nur", "ein", "Fun\u00b7ken", "hei\u00b7tren", "Glau\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur ein Strahl des guten Geistes:", "tokens": ["Nur", "ein", "Strahl", "des", "gu\u00b7ten", "Geis\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O so stellt ihn auf zur Linken,", "tokens": ["O", "so", "stellt", "ihn", "auf", "zur", "Lin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zur Belehrung und zur Be\u00dfrung,", "tokens": ["Zur", "Be\u00b7leh\u00b7rung", "und", "zur", "Be\u00df\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O so stellt ihn, wo das Herz schl\u00e4gt,", "tokens": ["O", "so", "stellt", "ihn", ",", "wo", "das", "Herz", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf der Menschheit frohe Linke,", "tokens": ["Auf", "der", "Menschheit", "fro\u00b7he", "Lin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Auf des Fr\u00fchlings gro\u00dfe Seite!", "tokens": ["Auf", "des", "Fr\u00fch\u00b7lings", "gro\u00b7\u00dfe", "Sei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sollt es sich jedoch ereignen,", "tokens": ["Sollt", "es", "sich", "je\u00b7doch", "er\u00b7eig\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Da\u00df das peinliche Verfahren", "tokens": ["Da\u00df", "das", "pein\u00b7li\u00b7che", "Ver\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts enth\u00fcllte, nichts verriete,", "tokens": ["Nichts", "ent\u00b7h\u00fcll\u00b7te", ",", "nichts", "ver\u00b7rie\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was da nur der Rede wert \u2013", "tokens": ["Was", "da", "nur", "der", "Re\u00b7de", "wert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Delirium des Rausches", "tokens": ["Das", "De\u00b7li\u00b7ri\u00b7um", "des", "Rau\u00b7sches"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbst nur eine dunkle Leere", "tokens": ["Selbst", "nur", "ei\u00b7ne", "dunk\u00b7le", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor den Richtern offenbarte:", "tokens": ["Vor", "den", "Rich\u00b7tern", "of\u00b7fen\u00b7bar\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schleunig la\u00dft den S\u00fcnder laufen!", "tokens": ["Schleu\u00b7nig", "la\u00dft", "den", "S\u00fcn\u00b7der", "lau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jagt ihn stracks zur schn\u00f6den Rechten,", "tokens": ["Jagt", "ihn", "stracks", "zur", "schn\u00f6\u00b7den", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo Geheul und Z\u00e4hneklappen,", "tokens": ["Wo", "Ge\u00b7heul", "und", "Z\u00e4h\u00b7ne\u00b7klap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dummheit und Verdammnis wohnen!", "tokens": ["Dumm\u00b7heit", "und", "Ver\u00b7damm\u00b7nis", "woh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}