{"textgrid.poem.52390": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbkarl Zunkel\u00ab, seggt de Herr von Schwutscher,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbkarl Zunkel\u00ab, seggt de Herr von Schwutscher,", "tokens": ["\u00bb", "karl", "Zun\u00b7kel", "\u00ab", ",", "seggt", "de", "Herr", "von", "Schwutsc\u00b7her", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NN", "$(", "$,", "VVFIN", "NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich mache dich zu meinem Kutscher", "tokens": ["\u00bb", "ich", "ma\u00b7che", "dich", "zu", "mei\u00b7nem", "Kut\u00b7scher"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hoffe, da\u00df du jederzeit", "tokens": ["Und", "hof\u00b7fe", ",", "da\u00df", "du", "je\u00b7der\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Anstand und durch Sauberkeit", "tokens": ["Durch", "An\u00b7stand", "und", "durch", "Sau\u00b7ber\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der gelb-gr\u00fcn-roten Liveree", "tokens": ["Der", "gelb\u00b7gr\u00fcn\u00b7ro\u00b7ten", "Li\u00b7ve\u00b7ree"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Derer von Schwutscher Ehre machst", "tokens": ["De\u00b7rer", "von", "Schwutsc\u00b7her", "Eh\u00b7re", "machst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und \u00fcber deine Haltung wachst.", "tokens": ["Und", "\u00fc\u00b7ber", "dei\u00b7ne", "Hal\u00b7tung", "wachst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Reck dich mal aus! Kopf in die H\u00f6h! \u2013", "tokens": ["Reck", "dich", "mal", "aus", "!", "Kopf", "in", "die", "H\u00f6h", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "$.", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "So, nun ist's gut, nun zieh dich an", "tokens": ["So", ",", "nun", "ist's", "gut", ",", "nun", "zieh", "dich", "an"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und schirr dann auf das Kutschgespann.\u00ab", "tokens": ["Und", "schirr", "dann", "auf", "das", "Kutschge\u00b7spann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Na, Korl, de sitt denn stramm un stiw", "tokens": ["Na", ",", "Korl", ",", "de", "sitt", "denn", "stramm", "un", "stiw"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "$,", "NE", "VVFIN", "KON", "FM", "FM", "FM"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bald in den blanken nigen Stat", "tokens": ["Bald", "in", "den", "blan\u00b7ken", "ni\u00b7gen", "Stat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un reckt sick ut so pil un grad,", "tokens": ["Un", "reckt", "sick", "ut", "so", "pil", "un", "grad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "As stek en Pal em d\u00f6rch dat Liw;", "tokens": ["As", "stek", "en", "Pal", "em", "d\u00f6rch", "dat", "Liw", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hei treckt de Halsbin'n \u00fcmmer knasser", "tokens": ["Hei", "treckt", "de", "Hals\u00b7bin'n", "\u00fcm\u00b7mer", "knas\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Un Vaderm\u00fcrder bet an't Uhr,", "tokens": ["Un", "Va\u00b7der\u00b7m\u00fcr\u00b7der", "bet", "an't", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von unn'n bet baben insnallt was'e", "tokens": ["Von", "unn'n", "bet", "ba\u00b7ben", "ins\u00b7nallt", "was'e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Un rinneprems't in de Mondur.", "tokens": ["Un", "rin\u00b7ne\u00b7prems't", "in", "de", "Mon\u00b7dur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Un nu wo stolz in sinen Sinn,", "tokens": ["Un", "nu", "wo", "stolz", "in", "si\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PWAV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Wat is'e v\u00f6rnehm nu un sch\u00f6n!", "tokens": ["Wat", "i\u00b7s'e", "v\u00f6r\u00b7nehm", "nu", "un", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So k\u00fcmmt hei in den Stall herin,", "tokens": ["So", "k\u00fcmmt", "hei", "in", "den", "Stall", "he\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "De N\u00e4s' so hoch bet an den B\u00e4hn,", "tokens": ["De", "N\u00e4s'", "so", "hoch", "bet", "an", "den", "B\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "As wir hei nu en Wunnerdird,", "tokens": ["As", "wir", "hei", "nu", "en", "Wun\u00b7ner\u00b7dird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un ward den Backenbort sick striken.", "tokens": ["Un", "ward", "den", "Ba\u00b7cken\u00b7bort", "sick", "stri\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "De Knechts, de stahn \u00fcm em un kiken", "tokens": ["De", "Knechts", ",", "de", "stahn", "\u00fcm", "em", "un", "ki\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "FM", "FM", "VVINF"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.16": {"text": "Un wunnern, lachen st\u00f6ten sich.", "tokens": ["Un", "wun\u00b7nern", ",", "la\u00b7chen", "st\u00f6\u00b7ten", "sich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "VVINF", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch Korl, de steiht un s\u00fcht dat nich,", "tokens": ["Doch", "Korl", ",", "de", "steiht", "un", "s\u00fcht", "dat", "nich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "FM", "FM", "FM", "FM", "FM", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hei kickt herup nah sinen B\u00e4hn", "tokens": ["Hei", "kickt", "he\u00b7rup", "nah", "si\u00b7nen", "B\u00e4hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "ADJA", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Un seggt tauletzt: \u00bbDau, Willem D\u00e4hn!", "tokens": ["Un", "seggt", "tau\u00b7letzt", ":", "\u00bb", "Dau", ",", "Wil\u00b7lem", "D\u00e4hn", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "$(", "NE", "$,", "NE", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Kik mal nah unn'n in't Emmer rin!", "tokens": ["Kik", "mal", "nah", "unn'n", "in't", "Em\u00b7mer", "rin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "APPR", "NE", "NE", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Heww'n mine Pird' noch Water in?\u00ab", "tokens": ["Hew\u00b7w'n", "mi\u00b7ne", "Pird'", "noch", "Wa\u00b7ter", "in", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "NN", "APPR", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}