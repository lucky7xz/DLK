{"textgrid.poem.55555": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe wider Willen", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df es wohl und spotte viel:", "tokens": ["Ich", "wei\u00df", "es", "wohl", "und", "spot\u00b7te", "viel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr M\u00e4dchen seid voll Wankelmut!", "tokens": ["Ihr", "M\u00e4d\u00b7chen", "seid", "voll", "Wan\u00b7kel\u00b7mut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr liebet, wie im Kartenspiel,", "tokens": ["Ihr", "lie\u00b7bet", ",", "wie", "im", "Kar\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den David und den Alexander;", "tokens": ["Den", "Da\u00b7vid", "und", "den", "A\u00b7lex\u00b7an\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sind ja Forcen miteinander,", "tokens": ["Sie", "sind", "ja", "For\u00b7cen", "mi\u00b7tein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und die sind miteinander gut.", "tokens": ["Und", "die", "sind", "mi\u00b7tein\u00b7an\u00b7der", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch bin ich elend wie zuvor,", "tokens": ["Doch", "bin", "ich", "e\u00b7lend", "wie", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit misanthropischem Gesicht,", "tokens": ["Mit", "mi\u00b7san\u00b7th\u00b7ro\u00b7pi\u00b7schem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der Liebe Sklav, ein armer Tor!", "tokens": ["Der", "Lie\u00b7be", "Sklav", ",", "ein", "ar\u00b7mer", "Tor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gern w\u00e4r ich sie los, die Schmerzen!", "tokens": ["Wie", "gern", "w\u00e4r", "ich", "sie", "los", ",", "die", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PPER", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein es sitzt zu tief im Herzen,", "tokens": ["Al\u00b7lein", "es", "sitzt", "zu", "tief", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Spott vertreibt die Liebe nicht.", "tokens": ["Und", "Spott", "ver\u00b7treibt", "die", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}