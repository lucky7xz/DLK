{"dta.poem.21553": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr armen Dichter, die ihr \u201ePhilomele\u201c,", "tokens": ["Ihr", "ar\u00b7men", "Dich\u00b7ter", ",", "die", "ihr", "\u201e", "Phi\u00b7lo\u00b7me\u00b7le", "\u201c", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "$(", "NE", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jedem Lenze rythmisch angeschw\u00e4rmt,", "tokens": ["In", "je\u00b7dem", "Len\u00b7ze", "ryth\u00b7misch", "an\u00b7ge\u00b7schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O wenn ihr w\u00fc\u00dftet, wie sich meine Seele", "tokens": ["O", "wenn", "ihr", "w\u00fc\u00df\u00b7tet", ",", "wie", "sich", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um ihre gottverlassnen Schwestern h\u00e4rmt!", "tokens": ["Um", "ih\u00b7re", "gott\u00b7ver\u00b7lass\u00b7nen", "Schwes\u00b7tern", "h\u00e4rmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dreht ihr auch noch so ernsthaft eure Phrase,", "tokens": ["Dreht", "ihr", "auch", "noch", "so", "ernst\u00b7haft", "eu\u00b7re", "Phra\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Teufel setzt sie lustig in Musik,", "tokens": ["Der", "Teu\u00b7fel", "setzt", "sie", "lus\u00b7tig", "in", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn eine ungeheuer lange Nase", "tokens": ["Denn", "ei\u00b7ne", "un\u00b7ge\u00b7heu\u00b7er", "lan\u00b7ge", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat seine Gro\u00dfmama, die Frau Kritik.", "tokens": ["Hat", "sei\u00b7ne", "Gro\u00df\u00b7ma\u00b7ma", ",", "die", "Frau", "Kri\u00b7tik", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}