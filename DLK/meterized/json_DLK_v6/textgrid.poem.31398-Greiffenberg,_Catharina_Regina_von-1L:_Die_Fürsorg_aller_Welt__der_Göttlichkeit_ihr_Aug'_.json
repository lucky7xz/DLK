{"textgrid.poem.31398": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die F\u00fcrsorg aller Welt / der G\u00f6ttlichkeit ihr Aug' /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die F\u00fcrsorg aller Welt / der G\u00f6ttlichkeit ihr Aug' /", "tokens": ["Die", "F\u00fcr\u00b7sorg", "al\u00b7ler", "Welt", "/", "der", "G\u00f6tt\u00b7lich\u00b7keit", "ihr", "Aug'", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in die mein ganzer Sinn treuglaubig sich versenkte /", "tokens": ["in", "die", "mein", "gan\u00b7zer", "Sinn", "treug\u00b7lau\u00b7big", "sich", "ver\u00b7senk\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "ADJA", "NN", "ADJD", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den Widder / wider all mein Angedenken / schenkte.", "tokens": ["den", "Wid\u00b7der", "/", "wi\u00b7der", "all", "mein", "An\u00b7ge\u00b7den\u00b7ken", "/", "schenk\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PIAT", "PPOSAT", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie / wie ich mich versehn / versahe was da taug.", "tokens": ["Sie", "/", "wie", "ich", "mich", "ver\u00b7sehn", "/", "ver\u00b7sa\u00b7he", "was", "da", "taug", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PWAV", "PPER", "PRF", "VVINF", "$(", "VVFIN", "PWS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Viel Trost- und Wei\u00dfheit S\u00e4fft aus dieser Traub man saug /", "tokens": ["Viel", "Trost", "und", "Wei\u00df\u00b7heit", "S\u00e4fft", "aus", "die\u00b7ser", "Traub", "man", "saug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "NN", "APPR", "PDAT", "NN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie Gott zum guten End / sein Ziel und Willen lenkte /", "tokens": ["wie", "Gott", "zum", "gu\u00b7ten", "End", "/", "sein", "Ziel", "und", "Wil\u00b7len", "lenk\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und Wundertreu auf die / so ihm vertrauen / denkte.", "tokens": ["und", "Wun\u00b7der\u00b7treu", "auf", "die", "/", "so", "ihm", "ver\u00b7trau\u00b7en", "/", "denk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "$(", "ADV", "PPER", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Balsam ist das Werk / in scharffer Tr\u00fcbsals Laug.", "tokens": ["Ein", "Bal\u00b7sam", "ist", "das", "Werk", "/", "in", "scharf\u00b7fer", "Tr\u00fcb\u00b7sals", "Laug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer ganz entselbstet sich in Gottes H\u00e4nde schwinget /", "tokens": ["Wer", "ganz", "ent\u00b7selbs\u00b7tet", "sich", "in", "Got\u00b7tes", "H\u00e4n\u00b7de", "schwin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PRF", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ihm gibet hin / was Gott ihm selbst gegeben hat /", "tokens": ["Ihm", "gi\u00b7bet", "hin", "/", "was", "Gott", "ihm", "selbst", "ge\u00b7ge\u00b7ben", "hat", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "PWS", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "aufs st\u00e4rkeste mit Gott durch Hertzens Demut ringet:", "tokens": ["aufs", "st\u00e4r\u00b7kes\u00b7te", "mit", "Gott", "durch", "Hert\u00b7zens", "De\u00b7mut", "rin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "NN", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "g'winnt den entsatzten Sieg / vor treuen Sinn die That.", "tokens": ["g'\u00b7winnt", "den", "ent\u00b7satz\u00b7ten", "Sieg", "/", "vor", "treu\u00b7en", "Sinn", "die", "That", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Den Gott ergebenen es lederzeit gelinget:", "tokens": ["Den", "Gott", "er\u00b7ge\u00b7be\u00b7nen", "es", "le\u00b7der\u00b7zeit", "ge\u00b7lin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "weil zu erh\u00f6ren sie / ist de\u00dfen hoher Raht.", "tokens": ["weil", "zu", "er\u00b7h\u00f6\u00b7ren", "sie", "/", "ist", "de\u00b7\u00dfen", "ho\u00b7her", "Raht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVFIN", "PPER", "$(", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}