{"textgrid.poem.23965": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingszuruf", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sich die Knospen aus den Zweigen dr\u00e4ngen,", "tokens": ["Nun", "sich", "die", "Knos\u00b7pen", "aus", "den", "Zwei\u00b7gen", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bl\u00fchende Kr\u00e4fte morsche Bande sprengen,", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "Kr\u00e4f\u00b7te", "mor\u00b7sche", "Ban\u00b7de", "spren\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wohin du siehst, wacht alles fr\u00f6hlich auf \u2013:", "tokens": ["Wo\u00b7hin", "du", "siehst", ",", "wacht", "al\u00b7les", "fr\u00f6h\u00b7lich", "auf", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PIS", "ADJD", "APPR", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun sei in deiner Seele rein und heiter,", "tokens": ["Nun", "sei", "in", "dei\u00b7ner", "See\u00b7le", "rein", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Erzengel rechts und links dir als Begleiter,", "tokens": ["Er\u00b7zen\u00b7gel", "rechts", "und", "links", "dir", "als", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nimm in den Morgen fr\u00f6hlich deinen Lauf!", "tokens": ["Nimm", "in", "den", "Mor\u00b7gen", "fr\u00f6h\u00b7lich", "dei\u00b7nen", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Schwingen streifen dich an beiden Seiten,", "tokens": ["Die", "Schwin\u00b7gen", "strei\u00b7fen", "dich", "an", "bei\u00b7den", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um dich der Engel Atem im Geleiten,", "tokens": ["Um", "dich", "der", "En\u00b7gel", "A\u00b7tem", "im", "Ge\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie mu\u00df dein Schritt jetzt frei und kr\u00e4ftig sein!", "tokens": ["Wie", "mu\u00df", "dein", "Schritt", "jetzt", "frei", "und", "kr\u00e4f\u00b7tig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schreit aus und glaube: Dir erklang das Werde!", "tokens": ["Schreit", "aus", "und", "glau\u00b7be", ":", "Dir", "er\u00b7klang", "das", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$.", "PPER", "VVFIN", "PDS", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schick deine Blicke aus: Die ganze Erde", "tokens": ["Schick", "dei\u00b7ne", "Bli\u00b7cke", "aus", ":", "Die", "gan\u00b7ze", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bl\u00fcht dir ans Herz: Was sch\u00f6n ist, das ist dein!", "tokens": ["Bl\u00fcht", "dir", "ans", "Herz", ":", "Was", "sch\u00f6n", "ist", ",", "das", "ist", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$.", "PWS", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn der ist K\u00f6nig \u00fcber alle Dinge,", "tokens": ["Denn", "der", "ist", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "al\u00b7le", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und den ber\u00fchrt der Engel goldene Schwinge,", "tokens": ["Und", "den", "be\u00b7r\u00fchrt", "der", "En\u00b7gel", "gol\u00b7de\u00b7ne", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der seine Blicke so aussenden kann,", "tokens": ["Der", "sei\u00b7ne", "Bli\u00b7cke", "so", "aus\u00b7sen\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df sie wie Adler Beute heimw\u00e4rts tragen,", "tokens": ["Da\u00df", "sie", "wie", "Ad\u00b7ler", "Beu\u00b7te", "heim\u00b7w\u00e4rts", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und dem die Morgenstunden leuchtend sagen:", "tokens": ["Und", "dem", "die", "Mor\u00b7gen\u00b7stun\u00b7den", "leuch\u00b7tend", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du Mensch mit hellen Augen, nimm uns an!", "tokens": ["Du", "Mensch", "mit", "hel\u00b7len", "Au\u00b7gen", ",", "nimm", "uns", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$,", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}