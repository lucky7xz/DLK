{"textgrid.poem.24631": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die W\u00fcrckung der Music in der Harmonie der Gem\u00fcther bey einem vergn\u00fcgten Hochzeit-Feste", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sagt/ da\u00df die ", "tokens": ["Man", "sagt", "/", "da\u00df", "die"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "KOUS", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und bringe der Vernunfft das Urtheil selber bey:", "tokens": ["Und", "brin\u00b7ge", "der", "Ver\u00b7nunfft", "das", "Ur\u00b7theil", "sel\u00b7ber", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df habe von der Welt den Ursprung nicht genommen/", "tokens": ["Di\u00df", "ha\u00b7be", "von", "der", "Welt", "den", "Ur\u00b7sprung", "nicht", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dessen Wunder-Krafft ein G\u00f6ttlichs Wesen sey.", "tokens": ["In", "des\u00b7sen", "Wun\u00b7der\u00b7Krafft", "ein", "G\u00f6tt\u00b7lichs", "We\u00b7sen", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob man die Schmeicheley nun gleich hierinnen schmecket:", "tokens": ["Ob", "man", "die", "Schmei\u00b7che\u00b7ley", "nun", "gleich", "hie\u00b7rin\u00b7nen", "schme\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "ADV", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gnug/ da\u00df sie unser Hertz zu aller Regung zwingt/", "tokens": ["Gnug", "/", "da\u00df", "sie", "un\u00b7ser", "Hertz", "zu", "al\u00b7ler", "Re\u00b7gung", "zwingt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und den erstorbnen Geist so sch\u00f6n in uns erwecket/", "tokens": ["Und", "den", "ers\u00b7torb\u00b7nen", "Geist", "so", "sch\u00f6n", "in", "uns", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja gleichsam durch den Thon ein neues Leben bringt.", "tokens": ["Ja", "gleich\u00b7sam", "durch", "den", "Thon", "ein", "neu\u00b7es", "Le\u00b7ben", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So gar halb todte wei\u00df ", "tokens": ["So", "gar", "halb", "tod\u00b7te", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADJA", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die die Tarantula durch ihren Gifft verletzt.", "tokens": ["Die", "die", "Ta\u00b7ran\u00b7tu\u00b7la", "durch", "ih\u00b7ren", "Gifft", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie jagt die Grillen aus und bey den schwersten Sachen", "tokens": ["Sie", "jagt", "die", "Gril\u00b7len", "aus", "und", "bey", "den", "schwers\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist sie die Artzeney/ die Leib und Seel ergetzt.", "tokens": ["Ist", "sie", "die", "Art\u00b7ze\u00b7ney", "/", "die", "Leib", "und", "Seel", "er\u00b7getzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die W\u00fcrckung hat sie auch bey unvern\u00fcnfftgen Thieren:", "tokens": ["Die", "W\u00fcr\u00b7ckung", "hat", "sie", "auch", "bey", "un\u00b7ver\u00b7n\u00fcnfft\u00b7gen", "Thie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Elephanten Wuht wird durch ", "tokens": ["Der", "E\u00b7le\u00b7phan\u00b7ten", "Wuht", "wird", "durch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Delphin wird verliebt/ wenn wir die Saiten r\u00fchren/", "tokens": ["Der", "Del\u00b7phin", "wird", "ver\u00b7liebt", "/", "wenn", "wir", "die", "Sai\u00b7ten", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df er den Arion durch Meer und Wellen tr\u00e4gt.", "tokens": ["Da\u00df", "er", "den", "A\u00b7rion", "durch", "Meer", "und", "Wel\u00b7len", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.17": {"text": "Ein Beyspiel/ da\u00df sich wohl zu dieser Liebe schicket/", "tokens": ["Ein", "Bey\u00b7spiel", "/", "da\u00df", "sich", "wohl", "zu", "die\u00b7ser", "Lie\u00b7be", "schi\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PRF", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die diesen sch\u00f6nen Tag zur s\u00fc\u00dfen Freude nimmt.", "tokens": ["Die", "die\u00b7sen", "sch\u00f6\u00b7nen", "Tag", "zur", "s\u00fc\u00b7\u00dfen", "Freu\u00b7de", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "An dem zwo Seelen sind durch den Accord entz\u00fccket/", "tokens": ["An", "dem", "zwo", "See\u00b7len", "sind", "durch", "den", "Ac\u00b7cord", "ent\u00b7z\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Den wahre Liebe sch\u00f6n in gleichen Hertzen stimmt.", "tokens": ["Den", "wah\u00b7re", "Lie\u00b7be", "sch\u00f6n", "in", "glei\u00b7chen", "Hert\u00b7zen", "stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "An dem ein Arion/ der wohl auf Saiten spielet/", "tokens": ["An", "dem", "ein", "A\u00b7rion", "/", "der", "wohl", "auf", "Sai\u00b7ten", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "$(", "ART", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Ein der ", "tokens": ["Ein", "der"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Da\u00df es durch ihn den Trieb zum Ehestande f\u00fchlet/", "tokens": ["Da\u00df", "es", "durch", "ihn", "den", "Trieb", "zum", "E\u00b7hes\u00b7tan\u00b7de", "f\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und ihn vergn\u00fcgt durchs Meer der reinsten Liebe tr\u00e4gt.", "tokens": ["Und", "ihn", "ver\u00b7gn\u00fcgt", "durchs", "Meer", "der", "reins\u00b7ten", "Lie\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Herr ", "tokens": ["Herr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Nimmt zum Vergn\u00fcgen sich die Jungfer \u2013 \u2013", "tokens": ["Nimmt", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "sich", "die", "Jung\u00b7fer", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "So geht/ die von dem Stamm recht edler Musen kommen/", "tokens": ["So", "geht", "/", "die", "von", "dem", "Stamm", "recht", "ed\u00b7ler", "Mu\u00b7sen", "kom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ART", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Tugendhaffte Braut/ zu Musen wieder hin.", "tokens": ["Die", "Tu\u00b7gend\u00b7haff\u00b7te", "Braut", "/", "zu", "Mu\u00b7sen", "wie\u00b7der", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Das sch\u00f6nste Sinnen Bild von gleich gesinnten Hertzen", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Sin\u00b7nen", "Bild", "von", "gleich", "ge\u00b7sinn\u00b7ten", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bleibt nichts als die ", "tokens": ["Bleibt", "nichts", "als", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "KOKOM", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "So s\u00fc\u00df und rein durchaus zwo gleiche Lauten schertzen/", "tokens": ["So", "s\u00fc\u00df", "und", "rein", "durc\u00b7haus", "zwo", "glei\u00b7che", "Lau\u00b7ten", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "CARD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So wundersch\u00f6n auch stets ein gleiches Paar sich k\u00fc\u00dft.", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", "auch", "stets", "ein", "glei\u00b7ches", "Paar", "sich", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADV", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wohldann/ du wehrtes Paar/ das Tugend stets gezieret/", "tokens": ["Wohl\u00b7dann", "/", "du", "wehr\u00b7tes", "Paar", "/", "das", "Tu\u00b7gend", "stets", "ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "PPER", "ADJA", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Von dessen Leben nur ein reiner Thon erklingt/", "tokens": ["Von", "des\u00b7sen", "Le\u00b7ben", "nur", "ein", "rei\u00b7ner", "Thon", "er\u00b7klingt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Das durch die ", "tokens": ["Das", "durch", "die"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Zum ewigen ", "tokens": ["Zum", "e\u00b7wi\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.37": {"text": "Der Himmel/ welcher euch so sch\u00f6n zusammen stimmt/", "tokens": ["Der", "Him\u00b7mel", "/", "wel\u00b7cher", "euch", "so", "sch\u00f6n", "zu\u00b7sam\u00b7men", "stimmt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "La\u00df eure Hertzen stets wohl ", "tokens": ["La\u00df", "eu\u00b7re", "Hert\u00b7zen", "stets", "wohl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.39": {"text": "Und wie uns die ", "tokens": ["Und", "wie", "uns", "die"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.40": {"text": "So stelle sich bey euch niemahls ein Kummer ein.", "tokens": ["So", "stel\u00b7le", "sich", "bey", "euch", "nie\u00b7mahls", "ein", "Kum\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Will die ", "tokens": ["Will", "die"], "token_info": ["word", "word"], "pos": ["VMFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.42": {"text": "So wird bey euch daf\u00fcr in Tugend ", "tokens": ["So", "wird", "bey", "euch", "da\u00b7f\u00fcr", "in", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "PAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Und aller Feinde Ha\u00df ist dieser Ergetzen/", "tokens": ["Und", "al\u00b7ler", "Fein\u00b7de", "Ha\u00df", "ist", "die\u00b7ser", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VAFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.44": {"text": "Die die Zufriedenbeit in Gott allzeit gef\u00fchrt.", "tokens": ["Die", "die", "Zu\u00b7frie\u00b7den\u00b7beit", "in", "Gott", "all\u00b7zeit", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Weil die ", "tokens": ["Weil", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.46": {"text": "So mu\u00df euch Gl\u00fcck und Wohl stets wie im Lentzen bl\u00fchn/", "tokens": ["So", "mu\u00df", "euch", "Gl\u00fcck", "und", "Wohl", "stets", "wie", "im", "Lent\u00b7zen", "bl\u00fchn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "ADV", "ADV", "KOKOM", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Und was sonst Regen/ Sturm/ Wind Schnee und Nebel bringet/", "tokens": ["Und", "was", "sonst", "Re\u00b7gen", "/", "Sturm", "/", "Wind", "Schnee", "und", "Ne\u00b7bel", "brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "A