{"textgrid.poem.48763": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "50. Bei der Musik, so dem Herrn Gesandten auf seinen Namenstag vor Terky in Zirkassen vorbracht wurde", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie steckt ihr volles Licht zu unsrer Musik an,", "tokens": ["Sie", "steckt", "ihr", "vol\u00b7les", "Licht", "zu", "uns\u00b7rer", "Mu\u00b7sik", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die ganze Zynthie, und hei\u00dft nach unserm Singen", "tokens": ["die", "gan\u00b7ze", "Zynt\u00b7hie", ",", "und", "hei\u00dft", "nach", "un\u00b7serm", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "durch Junons blauen Saal die frohen Sternen springen.", "tokens": ["durch", "Ju\u00b7nons", "blau\u00b7en", "Saal", "die", "fro\u00b7hen", "Ster\u00b7nen", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schaut, Jupiter trit selbst mit Venus an den Plan.", "tokens": ["Schaut", ",", "Ju\u00b7pi\u00b7ter", "trit", "selbst", "mit", "Ve\u00b7nus", "an", "den", "Plan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVFIN", "ADV", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Die Erde folgt der Luft, den beiden der Hyrkan,", "tokens": ["Die", "Er\u00b7de", "folgt", "der", "Luft", ",", "den", "bei\u00b7den", "der", "Hyr\u00b7kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der seine Doris hier so sanfte wei\u00df zu schwingen", "tokens": ["der", "sei\u00b7ne", "Do\u00b7ris", "hier", "so", "sanf\u00b7te", "wei\u00df", "zu", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NE", "ADV", "ADV", "VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um euer edles Schiff, dem nichts kan misgelingen,", "tokens": ["um", "eu\u00b7er", "ed\u00b7les", "Schiff", ",", "dem", "nichts", "kan", "mis\u00b7ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "als was der Himmel selbst wil haben nicht getan.", "tokens": ["als", "was", "der", "Him\u00b7mel", "selbst", "wil", "ha\u00b7ben", "nicht", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADV", "VMFIN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Herr, la\u00dft den s\u00fc\u00dfen Schlaf den s\u00fc\u00dfern Freuden weichen,", "tokens": ["Herr", ",", "la\u00dft", "den", "s\u00fc\u00b7\u00dfen", "Schlaf", "den", "s\u00fc\u00b7\u00dfern", "Freu\u00b7den", "wei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die allen Kummer euch aus euren Augen streichen,", "tokens": ["die", "al\u00b7len", "Kum\u00b7mer", "euch", "aus", "eu\u00b7ren", "Au\u00b7gen", "strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und habt ein g\u00fcnstigs Ohr auf unsrer Saiten Ton.", "tokens": ["und", "habt", "ein", "g\u00fcns\u00b7tigs", "Ohr", "auf", "uns\u00b7rer", "Sai\u00b7ten", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein Mensch kan mit mehr nicht als Menschenstimmen ehren.", "tokens": ["Ein", "Mensch", "kan", "mit", "mehr", "nicht", "als", "Men\u00b7schen\u00b7stim\u00b7men", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ADV", "PTKNEG", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein weit viel bessers Lied das l\u00e4\u00dft Eagers Sohn", "tokens": ["Ein", "weit", "viel", "bes\u00b7sers", "Lied", "das", "l\u00e4\u00dft", "E\u00b7a\u00b7gers", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PIAT", "ADJA", "NN", "PDS", "VVFIN", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf diesen euren Tag f\u00fcr allen G\u00f6ttern h\u00f6ren.", "tokens": ["auf", "die\u00b7sen", "eu\u00b7ren", "Tag", "f\u00fcr", "al\u00b7len", "G\u00f6t\u00b7tern", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den 4. November 1636.", "tokens": ["Den", "4.", "No\u00b7vem\u00b7ber", "16\u00b736", "."], "token_info": ["word", "ordinal", "word", "number", "punct"], "pos": ["ART", "ADJA", "NN", "CARD", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}