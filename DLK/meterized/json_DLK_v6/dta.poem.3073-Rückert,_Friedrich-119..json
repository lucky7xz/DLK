{"dta.poem.3073": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "119.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was Menschen Vorsicht hei\u00dft, ist schlecht von Menschen denken;", "tokens": ["Was", "Men\u00b7schen", "Vor\u00b7sicht", "hei\u00dft", ",", "ist", "schlecht", "von", "Men\u00b7schen", "den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie woll', o Vorsicht, mir die schlechte Vorsicht schenken!", "tokens": ["Nie", "woll'", ",", "o", "Vor\u00b7sicht", ",", "mir", "die", "schlech\u00b7te", "Vor\u00b7sicht", "schen\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "FM", "NN", "$,", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Vorsicht blickt herab, du schau zu ihr empor!", "tokens": ["Die", "Vor\u00b7sicht", "blickt", "her\u00b7ab", ",", "du", "schau", "zu", "ihr", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorsichtig ohne sie, bist du ein bl\u00f6der Thor.", "tokens": ["Vor\u00b7sich\u00b7tig", "oh\u00b7ne", "sie", ",", "bist", "du", "ein", "bl\u00f6\u00b7der", "Thor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}