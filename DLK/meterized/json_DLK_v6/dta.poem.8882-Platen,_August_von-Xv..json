{"dta.poem.8882": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Nach sch\u00f6nen Augen f\u00fchlt und sch\u00f6nen Haaren,", "tokens": ["Nach", "sch\u00f6\u00b7nen", "Au\u00b7gen", "f\u00fchlt", "und", "sch\u00f6\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den mahn' ich ab, der nur zu viel erfahren", "tokens": ["Den", "mahn'", "ich", "ab", ",", "der", "nur", "zu", "viel", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "ADV", "PTKA", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Schmerz und Qual durch eitles Unterfangen.", "tokens": ["Von", "Schmerz", "und", "Qual", "durch", "eit\u00b7les", "Un\u00b7ter\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dem j\u00e4hen Abgrund nur mit Noth entgangen,", "tokens": ["Dem", "j\u00e4\u00b7hen", "Ab\u00b7grund", "nur", "mit", "Noth", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was blieb mir aus unendlichen Gefahren?", "tokens": ["Was", "blieb", "mir", "aus", "un\u00b7end\u00b7li\u00b7chen", "Ge\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Aug' die Spur von hingeweinten Jahren,", "tokens": ["Im", "Aug'", "die", "Spur", "von", "hin\u00b7ge\u00b7wein\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in der Brust ein ungeheures Bangen.", "tokens": ["Und", "in", "der", "Brust", "ein", "un\u00b7ge\u00b7heu\u00b7res", "Ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Naht nicht der j\u00e4hen Tiefe, junge Herzen!", "tokens": ["Naht", "nicht", "der", "j\u00e4\u00b7hen", "Tie\u00b7fe", ",", "jun\u00b7ge", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Ufers Lilien gl\u00fchn von falschem Feuer,", "tokens": ["Des", "U\u00b7fers", "Li\u00b7li\u00b7en", "gl\u00fchn", "von", "fal\u00b7schem", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn ach, sie locken in das Meer der Schmerzen!", "tokens": ["Denn", "ach", ",", "sie", "lo\u00b7cken", "in", "das", "Meer", "der", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur Jenen ist das Leben sch\u00f6n und theuer,", "tokens": ["Nur", "Je\u00b7nen", "ist", "das", "Le\u00b7ben", "sch\u00f6n", "und", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die frank und ungefesselt mit ihm scherzen,", "tokens": ["Die", "frank", "und", "un\u00b7ge\u00b7fes\u00b7selt", "mit", "ihm", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ihnen ruft ein Gott: Die Welt ist euer.", "tokens": ["Und", "ih\u00b7nen", "ruft", "ein", "Gott", ":", "Die", "Welt", "ist", "eu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}