{"textgrid.poem.29737": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "152.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Welt ist unsre Wiege, drinnen liegen wir als Kinder;", "tokens": ["Die\u00b7se", "Welt", "ist", "uns\u00b7re", "Wie\u00b7ge", ",", "drin\u00b7nen", "lie\u00b7gen", "wir", "als", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Was wir wissen, ist nur St\u00fcckwerck; sind wir was, so sind wir S\u00fcnder.", "tokens": ["Was", "wir", "wis\u00b7sen", ",", "ist", "nur", "St\u00fcck\u00b7werck", ";", "sind", "wir", "was", ",", "so", "sind", "wir", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "$,", "VAFIN", "ADV", "NN", "$.", "VAFIN", "PPER", "PIS", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}