{"textgrid.poem.57336": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welche Bemerkung war's? des Dichterohres?", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche Bemerkung war's? des Dichterohres?", "tokens": ["Wel\u00b7che", "Be\u00b7mer\u00b7kung", "wa\u00b7r's", "?", "des", "Dich\u00b7te\u00b7roh\u00b7res", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "VAFIN", "$.", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Oder war es zugleich des Untersuchers,", "tokens": ["O\u00b7der", "war", "es", "zu\u00b7gleich", "des", "Un\u00b7ter\u00b7su\u00b7chers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die der Deutschen Heldenges\u00e4ngen sanfte", "tokens": ["Die", "der", "Deut\u00b7schen", "Hel\u00b7den\u00b7ge\u00b7s\u00e4n\u00b7gen", "sanf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Rithmosbewegung", "tokens": ["Rith\u00b7mos\u00b7be\u00b7we\u00b7gung"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Oft zur Gef\u00e4hrtin gab? In ihrer Sprache", "tokens": ["Oft", "zur", "Ge\u00b7f\u00e4hr\u00b7tin", "gab", "?", "In", "ih\u00b7rer", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Waltet st\u00e4rkerer Klang: sie dachten Sch\u00f6nheit,", "tokens": ["Wal\u00b7tet", "st\u00e4r\u00b7ke\u00b7rer", "Klang", ":", "sie", "dach\u00b7ten", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Da sie, ihn zu mildern, ihm mitgeh\u00f6rtes", "tokens": ["Da", "sie", ",", "ihn", "zu", "mil\u00b7dern", ",", "ihm", "mit\u00b7ge\u00b7h\u00f6r\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "PPER", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sanftes vereinten.", "tokens": ["Sanf\u00b7tes", "ver\u00b7ein\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Also erfrischt, bey hoher Fr\u00fchlingssonne,", "tokens": ["Al\u00b7so", "er\u00b7frischt", ",", "bey", "ho\u00b7her", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dichter Ulmen Gew\u00f6lbe, oder jene", "tokens": ["Dich\u00b7ter", "Ul\u00b7men", "Ge\u00b7w\u00f6l\u00b7be", ",", "o\u00b7der", "je\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "KON", "PDAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Luft des ersten Mays, die vom Wasserfalle", "tokens": ["Luft", "des", "ers\u00b7ten", "Mays", ",", "die", "vom", "Was\u00b7ser\u00b7fal\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NE", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Lieblich einherweht.", "tokens": ["Lieb\u00b7lich", "ein\u00b7her\u00b7weht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Starkes ert\u00f6nt nicht herschend in des Griechen", "tokens": ["Star\u00b7kes", "er\u00b7t\u00f6nt", "nicht", "her\u00b7schend", "in", "des", "Grie\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "VVPP", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sprache, Sanftes ert\u00f6nt; drum f\u00fchrt er seltner", "tokens": ["Spra\u00b7che", ",", "Sanf\u00b7tes", "er\u00b7t\u00f6nt", ";", "drum", "f\u00fchrt", "er", "selt\u00b7ner"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN", "$.", "PAV", "VVFIN", "PPER", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Zu des Schattens K\u00fchlungen, in der hohen", "tokens": ["Zu", "des", "Schat\u00b7tens", "K\u00fch\u00b7lun\u00b7gen", ",", "in", "der", "ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "--+-++-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Quelle Ges\u00e4usel.", "tokens": ["Quel\u00b7le", "Ge\u00b7s\u00e4u\u00b7sel", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Seltner noch, als der Grieche f\u00fchrt der neue", "tokens": ["Selt\u00b7ner", "noch", ",", "als", "der", "Grie\u00b7che", "f\u00fchrt", "der", "neu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "R\u00f6mer, wenn er, wie seiner stolzen V\u00e4ter", "tokens": ["R\u00f6\u00b7mer", ",", "wenn", "er", ",", "wie", "sei\u00b7ner", "stol\u00b7zen", "V\u00e4\u00b7ter"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u00dcberwinder, je sich erk\u00fchnt zu schweben", "tokens": ["\u00dc\u00b7berw\u00b7in\u00b7der", ",", "je", "sich", "er\u00b7k\u00fchnt", "zu", "schwe\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "PRF", "VVPP", "PTKZU", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "T\u00e4nze des Liedes.", "tokens": ["T\u00e4n\u00b7ze", "des", "Lie\u00b7des", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}