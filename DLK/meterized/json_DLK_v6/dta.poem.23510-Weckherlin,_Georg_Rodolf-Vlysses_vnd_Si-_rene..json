{"dta.poem.23510": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol/ Vlysses/ ich kom zu dir/", "tokens": ["Wol", "/", "Vlys\u00b7ses", "/", "ich", "kom", "zu", "dir", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NE", "$(", "PPER", "VVFIN", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil du ja nicht wilt zu mir kommen;", "tokens": ["Weil", "du", "ja", "nicht", "wilt", "zu", "mir", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd weil dein hertz nicht wirt von mir/", "tokens": ["Vnd", "weil", "dein", "hertz", "nicht", "wirt", "von", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PTKNEG", "VVPP", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wirt mein hertz von dir eingenommen:", "tokens": ["Wirt", "mein", "hertz", "von", "dir", "ein\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Zwar ist mein verlust mehr mein pracht/", "tokens": ["Zwar", "ist", "mein", "ver\u00b7lust", "mehr", "mein", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Dan so ich auch dich thet erwerben;", "tokens": ["Dan", "so", "ich", "auch", "dich", "thet", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dan Sch\u00f6nheit ist allein gemacht", "tokens": ["Dan", "Sch\u00f6n\u00b7heit", "ist", "al\u00b7lein", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu verd\u00f6rben oder verderben.", "tokens": ["Zu", "ver\u00b7d\u00f6r\u00b7ben", "o\u00b7der", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}}}}