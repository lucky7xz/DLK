{"dta.poem.12847": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Melinden wegen seiner l", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du tadelst meine lieb', unbillige Melinde!", "tokens": ["Du", "ta\u00b7delst", "mei\u00b7ne", "lieb'", ",", "un\u00b7bil\u00b7li\u00b7ge", "Me\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr lallen und ihr schertz erwecket dir verdru\u00df", "tokens": ["Ihr", "lal\u00b7len", "und", "ihr", "schertz", "er\u00b7we\u00b7cket", "dir", "ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "KON", "PPER", "ADJD", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein mund verlanget nichts, als einen k\u00fchlen ku\u00df;", "tokens": ["Dein", "mund", "ver\u00b7lan\u00b7get", "nichts", ",", "als", "ei\u00b7nen", "k\u00fch\u00b7len", "ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch wer begehret wol so viel von einem kinde?", "tokens": ["Doch", "wer", "be\u00b7ge\u00b7hret", "wol", "so", "viel", "von", "ei\u00b7nem", "kin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von m\u00e4nnern fordert man mit rechte solche sachen;", "tokens": ["Von", "m\u00e4n\u00b7nern", "for\u00b7dert", "man", "mit", "rech\u00b7te", "sol\u00b7che", "sa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PIS", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn ernst und klugheit zieht nicht bey den kindern ein:", "tokens": ["Denn", "ernst", "und", "klug\u00b7heit", "zieht", "nicht", "bey", "den", "kin\u00b7dern", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Soll meine liebe nun klug und verst\u00e4ndig seyn,", "tokens": ["Soll", "mei\u00b7ne", "lie\u00b7be", "nun", "klug", "und", "ver\u00b7st\u00e4n\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "So must du sie zuvor zu einem manne machen.", "tokens": ["So", "must", "du", "sie", "zu\u00b7vor", "zu", "ei\u00b7nem", "man\u00b7ne", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}