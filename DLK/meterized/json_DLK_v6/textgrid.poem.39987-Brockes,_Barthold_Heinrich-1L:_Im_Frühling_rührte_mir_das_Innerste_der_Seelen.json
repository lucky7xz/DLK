{"textgrid.poem.39987": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Fr\u00fchling r\u00fchrte mir das Innerste der Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fchling r\u00fchrte mir das Innerste der Seelen", "tokens": ["Im", "Fr\u00fch\u00b7ling", "r\u00fchr\u00b7te", "mir", "das", "In\u00b7ners\u00b7te", "der", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der B\u00fcsche K\u00f6niginn, die holde Nachtigall,", "tokens": ["Der", "B\u00fc\u00b7sche", "K\u00f6\u00b7ni\u00b7ginn", ",", "die", "hol\u00b7de", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die, aus so enger Brust, und mit so kleiner K\u00e4len,", "tokens": ["Die", ",", "aus", "so", "en\u00b7ger", "Brust", ",", "und", "mit", "so", "klei\u00b7ner", "K\u00e4\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "KON", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gr\u00f6\u00dften W\u00e4lder f\u00fcllt, durch ihren Wunder-Schall.", "tokens": ["Die", "gr\u00f6\u00df\u00b7ten", "W\u00e4l\u00b7der", "f\u00fcllt", ",", "durch", "ih\u00b7ren", "Wun\u00b7der\u00b7Schall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Derselben Fertigkeit, die Kunst, der Flei\u00df, die St\u00e4rcke,", "tokens": ["Der\u00b7sel\u00b7ben", "Fer\u00b7tig\u00b7keit", ",", "die", "Kunst", ",", "der", "Flei\u00df", ",", "die", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ver\u00e4nderung und Ton sind lauter Wunder-Wercke", "tokens": ["Ver\u00b7\u00e4n\u00b7de\u00b7rung", "und", "Ton", "sind", "lau\u00b7ter", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der wirckenden Natur, die solchen starcken Klang", "tokens": ["Der", "wir\u00b7cken\u00b7den", "Na\u00b7tur", ",", "die", "sol\u00b7chen", "star\u00b7cken", "Klang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In ein Paar Federchen, die kaum zu sehen, sencket,", "tokens": ["In", "ein", "Paar", "Fe\u00b7der\u00b7chen", ",", "die", "kaum", "zu", "se\u00b7hen", ",", "sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.9": {"text": "Und einen das Geh\u00f6r bezaubernden Gesang", "tokens": ["Und", "ei\u00b7nen", "das", "Ge\u00b7h\u00f6r", "be\u00b7zau\u00b7bern\u00b7den", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "In solche d\u00fcnne Haut und zarten Schnabel schrencket.", "tokens": ["In", "sol\u00b7che", "d\u00fcn\u00b7ne", "Haut", "und", "zar\u00b7ten", "Schna\u00b7bel", "schren\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr H\u00e4lschen ist am Ton so unersch\u00f6pflich reich,", "tokens": ["Ihr", "H\u00e4l\u00b7schen", "ist", "am", "Ton", "so", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df sie tief, hoch, gelind' und starck auf einmahl singet.", "tokens": ["Da\u00df", "sie", "tief", ",", "hoch", ",", "ge\u00b7lind'", "und", "starck", "auf", "ein\u00b7mahl", "sin\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "APPR", "ADV", "VVFIN", "$."], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.13": {"text": "Die kleine Gurgel lockt, schnarrt, zischt und pfeift zugleich,", "tokens": ["Die", "klei\u00b7ne", "Gur\u00b7gel", "lockt", ",", "schnarrt", ",", "zischt", "und", "pfeift", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df sie, wie Quellen, rauscht, wie helle Glocken, klinget.", "tokens": ["Da\u00df", "sie", ",", "wie", "Quel\u00b7len", ",", "rauscht", ",", "wie", "hel\u00b7le", "Glo\u00b7cken", ",", "klin\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "NN", "$,", "VVFIN", "$,", "PWAV", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie zwitschert, stimmt und schl\u00e4g't mit solcher Anmuht an,", "tokens": ["Sie", "zwit\u00b7schert", ",", "stimmt", "und", "schl\u00e4g't", "mit", "sol\u00b7cher", "An\u00b7muht", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mit solchem nach der Kunst gekr\u00e4uselten Geschwirre;", "tokens": ["Mit", "sol\u00b7chem", "nach", "der", "Kunst", "ge\u00b7kr\u00e4u\u00b7sel\u00b7ten", "Ge\u00b7schwir\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.17": {"text": "Da\u00df man darob erstaunt, und nicht begreiffen kann,", "tokens": ["Da\u00df", "man", "da\u00b7rob", "er\u00b7staunt", ",", "und", "nicht", "be\u00b7greif\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVPP", "$,", "KON", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ob sie nicht seuftzend lach', ob sie nicht lachend girre.", "tokens": ["Ob", "sie", "nicht", "seuft\u00b7zend", "lach'", ",", "ob", "sie", "nicht", "la\u00b7chend", "gir\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ihr Stimmchen ziehet sich in einer holen L\u00e4nge", "tokens": ["Ihr", "Stimm\u00b7chen", "zie\u00b7het", "sich", "in", "ei\u00b7ner", "ho\u00b7len", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von unten in die H\u00f6h, f\u00e4llt, steigt aufs neu' empor,", "tokens": ["Von", "un\u00b7ten", "in", "die", "H\u00f6h", ",", "f\u00e4llt", ",", "steigt", "aufs", "neu'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "APPRART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und schweb't nach Maa\u00df' und Zeit; bald drenget eine Menge", "tokens": ["Und", "schwe\u00b7b't", "nach", "Maa\u00df'", "und", "Zeit", ";", "bald", "dren\u00b7get", "ei\u00b7ne", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Verschied'ner T\u00f6n' aus ihr, als wie ein Strom, hervor,", "tokens": ["Ver\u00b7schie\u00b7d'\u00b7ner", "T\u00f6n'", "aus", "ihr", ",", "als", "wie", "ein", "Strom", ",", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Zuweilen seuftzet sie, und winselt, da\u00df man meynet,", "tokens": ["Zu\u00b7wei\u00b7len", "seuft\u00b7zet", "sie", ",", "und", "win\u00b7selt", ",", "da\u00df", "man", "mey\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie werde sterben; aber bald", "tokens": ["Sie", "wer\u00b7de", "ster\u00b7ben", ";", "a\u00b7ber", "bald"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "$.", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Erhebet sie, mit feuriger Gewalt,", "tokens": ["Er\u00b7he\u00b7bet", "sie", ",", "mit", "feu\u00b7ri\u00b7ger", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Den reinen Ton aufs neu. Dann eben scheinet,", "tokens": ["Den", "rei\u00b7nen", "Ton", "aufs", "neu", ".", "Dann", "e\u00b7ben", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJD", "$.", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Es woll' ihr lieblich-scharfes Singen,", "tokens": ["Es", "woll'", "ihr", "lieb\u00b7lich\u00b7scha\u00b7rfes", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Als wie ein Pfeil, uns in die Seele dringen.", "tokens": ["Als", "wie", "ein", "Pfeil", ",", "uns", "in", "die", "See\u00b7le", "drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwitschern, seuftzen, lachen, singen,", "tokens": ["Zwit\u00b7schern", ",", "seuft\u00b7zen", ",", "la\u00b7chen", ",", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Girren, st\u00f6hnen, gurgeln, klingen,", "tokens": ["Gir\u00b7ren", ",", "st\u00f6h\u00b7nen", ",", "gur\u00b7geln", ",", "klin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Locken, schmeicheln, pfeifen, zucken,", "tokens": ["Lo\u00b7cken", ",", "schmei\u00b7cheln", ",", "pfei\u00b7fen", ",", "zu\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00f6then, schlagen, zischen, glucken", "tokens": ["Fl\u00f6\u00b7then", ",", "schla\u00b7gen", ",", "zi\u00b7schen", ",", "glu\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist der holden Nachtigall", "tokens": ["Ist", "der", "hol\u00b7den", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wunderbar gemischter Schall.", "tokens": ["Wun\u00b7der\u00b7bar", "ge\u00b7mischter", "Schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Es scheint so gar der Nam' allein", "tokens": ["Es", "scheint", "so", "gar", "der", "Nam'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Inbegriff der Fr\u00fchlings-Lust zu seyn.", "tokens": ["Ein", "In\u00b7be\u00b7griff", "der", "Fr\u00fch\u00b7lings\u00b7Lust", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn etwa jemand spricht: es sang die Nachtigall;", "tokens": ["Wenn", "et\u00b7wa", "je\u00b7mand", "spricht", ":", "es", "sang", "die", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kann fast des blossen Wortes Schall", "tokens": ["Kann", "fast", "des", "blos\u00b7sen", "Wor\u00b7tes", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So viel zu wircken taugen,", "tokens": ["So", "viel", "zu", "wir\u00b7cken", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df in der meisten H\u00f6rer Augen", "tokens": ["Da\u00df", "in", "der", "meis\u00b7ten", "H\u00f6\u00b7rer", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich ein geheim Vergn\u00fcgen zeiget.", "tokens": ["Sich", "ein", "ge\u00b7heim", "Ver\u00b7gn\u00fc\u00b7gen", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie dreht und dehnt den Schall, zerreisst und f\u00fcg't ihn wieder;", "tokens": ["Sie", "dreht", "und", "dehnt", "den", "Schall", ",", "zer\u00b7reisst", "und", "f\u00fcg't", "ihn", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Singt sanft, singt ungest\u00fcm, bald grob, bald klar und hell.", "tokens": ["Singt", "sanft", ",", "singt", "un\u00b7ge\u00b7st\u00fcm", ",", "bald", "grob", ",", "bald", "klar", "und", "hell", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-