{"dta.poem.20144": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n Lob-schrift  \n Von lautern Troch \u00e6 is  \n auf  \n Den Kupfertittel  \n dieses Poetisch-Historischen Lustgartens.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer ist eigendlich zu sehen/ wie Herr Neu-\nmark angeleget/", "tokens": ["Hjer", "ist", "ei\u00b7gend\u00b7lich", "zu", "se\u00b7hen", "/", "wie", "Herr", "Neu", "mark", "an\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$(", "KOKOM", "NN", "TRUNC", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Seinen Garten an der Lotten/ der viel Sinnen-", "tokens": ["Sei\u00b7nen", "Gar\u00b7ten", "an", "der", "Lot\u00b7ten", "/", "der", "viel", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$(", "ART", "PIAT", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Tr\u00e4get jemand ein Verlangen/ sich mit Floren zu", "tokens": ["Tr\u00e4\u00b7get", "je\u00b7mand", "ein", "Ver\u00b7lan\u00b7gen", "/", "sich", "mit", "Flo\u00b7ren", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "$(", "PRF", "APPR", "NN", "PTKZU"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Die geschr\u00e4nkte Blumen-Felder k\u00f6nnen ihn in", "tokens": ["Die", "ge\u00b7schr\u00e4nk\u00b7te", "Blu\u00b7men\u00b7Fel\u00b7der", "k\u00f6n\u00b7nen", "ihn", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "APPR"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Wer die Augen wil erfrischen/ an des Grases", "tokens": ["Wer", "die", "Au\u00b7gen", "wil", "er\u00b7fri\u00b7schen", "/", "an", "des", "Gra\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VMFIN", "VVINF", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Mit Obst-b\u00e4umen wol durchschattet/ kan Sie hier", "tokens": ["Mit", "Obst\u00b7b\u00e4u\u00b7men", "wol", "durch\u00b7schat\u00b7tet", "/", "kan", "Sie", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "$(", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Es steht gleichfals zu betrachten/ was in den", "tokens": ["Es", "steht", "gleich\u00b7fals", "zu", "be\u00b7trach\u00b7ten", "/", "was", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$(", "PWS", "APPR", "ART"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wie doch alles was gepflantzet zum Confect und", "tokens": ["Wie", "doch", "al\u00b7les", "was", "ge\u00b7pflant\u00b7zet", "zum", "Con\u00b7fect", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIS", "PWS", "VVPP", "APPRART", "NN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Man kan auch mit Lust ansehen/ wie die Lotte sich", "tokens": ["Man", "kan", "auch", "mit", "Lust", "an\u00b7se\u00b7hen", "/", "wie", "die", "Lot\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$(", "KOKOM", "ART", "NE", "PRF"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und mit ihrem Strohm geschl\u00e4ngelt/ \u00fcm den", "tokens": ["Und", "mit", "ih\u00b7rem", "Strohm", "ge\u00b7schl\u00e4n\u00b7gelt", "/", "\u00fcm", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$(", "ADV", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Wie in ihrem Flu\u00df die Schmerlen oft sich hin", "tokens": ["Wie", "in", "ih\u00b7rem", "Flu\u00df", "die", "Schmer\u00b7len", "oft", "sich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "PRF", "APZR"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Auch zur Zeit auf ihrem R\u00fckken die schnee-wei\u00dfe", "tokens": ["Auch", "zur", "Zeit", "auf", "ih\u00b7rem", "R\u00fck\u00b7ken", "die", "schnee\u00b7wei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Wenn ein Regen unversehens/ oder sonst ein Wet-", "tokens": ["Wenn", "ein", "Re\u00b7gen", "un\u00b7ver\u00b7se\u00b7hens", "/", "o\u00b7der", "sonst", "ein", "Wet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "$(", "KON", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "Ist man in dem Gartenhause von dergleichen", "tokens": ["Ist", "man", "in", "dem", "Gar\u00b7ten\u00b7hau\u00b7se", "von", "derg\u00b7lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "APPR", "ART", "NN", "APPR", "PIS"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Da man sich kan unter dessen/ mit Gespr\u00e4chen er-", "tokens": ["Da", "man", "sich", "kan", "un\u00b7ter", "des\u00b7sen", "/", "mit", "Ge\u00b7spr\u00e4\u00b7chen", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "VMFIN", "APPR", "PRELAT", "$(", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "Oder in den B\u00fcchern lesen/ auch bi\u00dfweilen musi-", "tokens": ["O\u00b7der", "in", "den", "B\u00fc\u00b7chern", "le\u00b7sen", "/", "auch", "bi\u00df\u00b7wei\u00b7len", "mu\u00b7si"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$(", "ADV", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Welch\u2019 Ergetzung in dem Zim\u0303er mehrmals nach", "tokens": ["Welch'", "Er\u00b7get\u00b7zung", "in", "dem", "Zim\u0303er", "mehr\u00b7mals", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "ADV", "APPR"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.18": {"text": "Und den wol-erkandtein Freunden zu Gebot und", "tokens": ["Und", "den", "wol\u00b7er\u00b7kand\u00b7tein", "Freun\u00b7den", "zu", "Ge\u00b7bot", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "APPR", "NN", "KON"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.19": {"text": "Summa/ hier in diesem Garten ist zu sp\u00fchren und", "tokens": ["Sum\u00b7ma", "/", "hier", "in", "die\u00b7sem", "Gar\u00b7ten", "ist", "zu", "sp\u00fch\u00b7ren", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADV", "APPR", "PDAT", "NN", "VAFIN", "PTKZU", "VVINF", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "Da\u00df der Nutz und die Ergetzung sich gew\u00fcnscht", "tokens": ["Da\u00df", "der", "Nutz", "und", "die", "Er\u00b7get\u00b7zung", "sich", "ge\u00b7w\u00fcnscht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "PRF", "VVPP"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "Nun mu\u00df alles die\u00df vergehen alles dieses endlich", "tokens": ["Nun", "mu\u00df", "al\u00b7les", "die\u00df", "ver\u00b7ge\u00b7hen", "al\u00b7les", "die\u00b7ses", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "PDS", "VVINF", "PIS", "PDAT", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.22": {"text": "Und als ein verg\u00e4nglichs Wesen/ mit der Folge-", "tokens": ["Und", "als", "ein", "ver\u00b7g\u00e4ng\u00b7lichs", "We\u00b7sen", "/", "mit", "der", "Fol\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$(", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "Aber dieses sch\u00f6ne Schriftwerk/ dieser angelegte", "tokens": ["A\u00b7ber", "die\u00b7ses", "sch\u00f6\u00b7ne", "Schrift\u00b7werk", "/", "die\u00b7ser", "an\u00b7ge\u00b7leg\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN", "$(", "PDAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "Den Apollo selbst hilft pflantzen/ den die Musen", "tokens": ["Den", "A\u00b7pol\u00b7lo", "selbst", "hilft", "pflant\u00b7zen", "/", "den", "die", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "VVFIN", "VVINF", "$(", "ART", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Den Herr Neumark auch erbauet/ der steht sicher", "tokens": ["Den", "Herr", "Neu\u00b7mark", "auch", "er\u00b7bau\u00b7et", "/", "der", "steht", "si\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ADV", "VVFIN", "$(", "ART", "VVFIN", "ADJD"], "meter": "-++-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Jhn kan weder Frost noch Hitze/ weder Zeit noch", "tokens": ["Jhn", "kan", "we\u00b7der", "Frost", "noch", "Hit\u00b7ze", "/", "we\u00b7der", "Zeit", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KON", "NN", "ADV", "NN", "$(", "KON", "NN", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.27": {"text": "Dieser sch\u00e4rfet den Verstand/ und verbessert das", "tokens": ["Die\u00b7ser", "sch\u00e4r\u00b7fet", "den", "Ver\u00b7stand", "/", "und", "ver\u00b7bes\u00b7sert", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "ART"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Jener bringt dem Leid\u2019 Ergetzen/ und erfrischet", "tokens": ["Je\u00b7ner", "bringt", "dem", "Leid'", "Er\u00b7get\u00b7zen", "/", "und", "er\u00b7fri\u00b7schet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "VVFIN", "ART", "NN", "NN", "$(", "KON", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.29": {"text": "Dru\u0304m mu\u00df dieser G\u00e4rtner haben billich zum ver-", "tokens": ["Dr\u016bm", "mu\u00df", "die\u00b7ser", "G\u00e4rt\u00b7ner", "ha\u00b7ben", "bil\u00b7lich", "zum", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PDAT", "NN", "VAFIN", "ADJD", "APPRART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.30": {"text": "Vor so gute zwiefach\u2019 Arbeit/ eine zwiefach\u2019", "tokens": ["Vor", "so", "gu\u00b7te", "zwie\u00b7fach'", "Ar\u00b7beit", "/", "ei\u00b7ne", "zwie\u00b7fach'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "PIAT", "NN", "$(", "ART", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}