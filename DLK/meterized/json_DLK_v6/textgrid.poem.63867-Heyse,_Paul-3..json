{"textgrid.poem.63867": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also, da\u00df der Mensch vom Affen abstammt, d\u00fcnkt dir zweifelhaft,", "tokens": ["Al\u00b7so", ",", "da\u00df", "der", "Mensch", "vom", "Af\u00b7fen", "ab\u00b7stammt", ",", "d\u00fcnkt", "dir", "zwei\u00b7fel\u00b7haft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Leugnest mit den strengen Pfaffen Darwin's tiefe Wissenschaft?", "tokens": ["Leug\u00b7nest", "mit", "den", "stren\u00b7gen", "Pfaf\u00b7fen", "Dar\u00b7win's", "tie\u00b7fe", "Wis\u00b7sen\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Problematisch bleibt's; indessen, klar ist der Familienzug.", "tokens": ["Prob\u00b7le\u00b7ma\u00b7tisch", "bleibt's", ";", "in\u00b7des\u00b7sen", ",", "klar", "ist", "der", "Fa\u00b7mi\u00b7li\u00b7en\u00b7zug", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "ADV", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sieh nur, wie statt blanker Waffen auch der Mensch vom Boden rafft,", "tokens": ["Sieh", "nur", ",", "wie", "statt", "blan\u00b7ker", "Waf\u00b7fen", "auch", "der", "Mensch", "vom", "Bo\u00b7den", "rafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "ADJD", "ADJA", "NN", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Was ihm kommt an faulen \u00c4pfeln, eklem Unrat in die Hand,", "tokens": ["Was", "ihm", "kommt", "an", "fau\u00b7len", "\u00c4p\u00b7feln", ",", "ek\u00b7lem", "Un\u00b7rat", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Nur um Hilfe sich zu schaffen gegen \u00fcberlegne Kraft.", "tokens": ["Nur", "um", "Hil\u00b7fe", "sich", "zu", "schaf\u00b7fen", "ge\u00b7gen", "\u00fc\u00b7berl\u00b7eg\u00b7ne", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PRF", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Eilen nicht, wenn ein erhabnes Vorbild ragt vor ihrem Blick,", "tokens": ["Ei\u00b7len", "nicht", ",", "wenn", "ein", "er\u00b7hab\u00b7nes", "Vor\u00b7bild", "ragt", "vor", "ih\u00b7rem", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Nachzust\u00fcmpern eitle Laffen seine Werke t\u00f6lpelhaft?", "tokens": ["Nach\u00b7zu\u00b7st\u00fcm\u00b7pern", "eit\u00b7le", "Laf\u00b7fen", "sei\u00b7ne", "Wer\u00b7ke", "t\u00f6l\u00b7pel\u00b7haft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Und die edlen Menschenkinder, die sich d\u00fcnken g\u00f6ttergleich,", "tokens": ["Und", "die", "ed\u00b7len", "Men\u00b7schen\u00b7kin\u00b7der", ",", "die", "sich", "d\u00fcn\u00b7ken", "g\u00f6t\u00b7ter\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Wie sie achselzuckend gaffen, wenn des Helden Mut erschlafft,", "tokens": ["Wie", "sie", "ach\u00b7sel\u00b7zu\u00b7ckend", "gaf\u00b7fen", ",", "wenn", "des", "Hel\u00b7den", "Mut", "er\u00b7schlafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$,", "KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Gassenbubenhaft frohlocken, wenn er endlich niedersinkt,", "tokens": ["Gas\u00b7sen\u00b7bu\u00b7ben\u00b7haft", "froh\u00b7lo\u00b7cken", ",", "wenn", "er", "end\u00b7lich", "nie\u00b7der\u00b7sinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Weil ihm tiefe Wunden klaffen, draus entstr\u00f6mt der Lebenssaft?", "tokens": ["Weil", "ihm", "tie\u00b7fe", "Wun\u00b7den", "klaf\u00b7fen", ",", "draus", "ent\u00b7str\u00f6mt", "der", "Le\u00b7bens\u00b7saft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Auf die B\u00e4ume nur zu klettern h\u00fctet sich ein Weibessohn,", "tokens": ["Auf", "die", "B\u00e4u\u00b7me", "nur", "zu", "klet\u00b7tern", "h\u00fc\u00b7tet", "sich", "ein", "Wei\u00b7bes\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Denn sein Fu\u00df ist so beschaffen, da\u00df die Erd' ihn h\u00e4lt in Haft.", "tokens": ["Denn", "sein", "Fu\u00df", "ist", "so", "be\u00b7schaf\u00b7fen", ",", "da\u00df", "die", "Erd'", "ihn", "h\u00e4lt", "in", "Haft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Doch ihr schadenfr\u00f6hlich Grinsen, wagt er's doch und f\u00e4llt herab,", "tokens": ["Doch", "ihr", "scha\u00b7den\u00b7fr\u00f6h\u00b7lich", "Grin\u00b7sen", ",", "wagt", "er's", "doch", "und", "f\u00e4llt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "$,", "VVFIN", "PIS", "ADV", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Schwerbem\u00fcht, sich aufzuraffen \u2013 ist's nicht v\u00f6llig affenhaft?", "tokens": ["Schwer\u00b7be\u00b7m\u00fcht", ",", "sich", "auf\u00b7zu\u00b7raf\u00b7fen", "\u2013", "ist's", "nicht", "v\u00f6l\u00b7lig", "af\u00b7fen\u00b7haft", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRF", "VVIZU", "$(", "VAFIN", "PTKNEG", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}