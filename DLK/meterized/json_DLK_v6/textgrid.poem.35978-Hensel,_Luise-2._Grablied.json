{"textgrid.poem.35978": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "2. Grablied", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein H\u00fcttchen dir gebaut,", "tokens": ["Ich", "hab'", "ein", "H\u00fctt\u00b7chen", "dir", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lind (?) und gr\u00fcn und k\u00fchl,", "tokens": ["So", "lind", "(", "?", ")", "und", "gr\u00fcn", "und", "k\u00fchl", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "$.", "$(", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wohnst du so allein und traut,", "tokens": ["Da", "wohnst", "du", "so", "al\u00b7lein", "und", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist es d\u00e4mmernd still.", "tokens": ["Da", "ist", "es", "d\u00e4m\u00b7mernd", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Komm, wei\u00dfe Rose, geh hinein,", "tokens": ["Komm", ",", "wei\u00b7\u00dfe", "Ro\u00b7se", ",", "geh", "hin\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das H\u00fcttchen ist bereit,", "tokens": ["Das", "H\u00fctt\u00b7chen", "ist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe blanke Perlen drein", "tokens": ["Ich", "ha\u00b7be", "blan\u00b7ke", "Per\u00b7len", "drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Blumen genug gestreut.", "tokens": ["Und", "Blu\u00b7men", "ge\u00b7nug", "ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab' ein Bettchen dir gemacht,", "tokens": ["Ich", "hab'", "ein", "Bett\u00b7chen", "dir", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tief im gr\u00fcnen Gras,", "tokens": ["So", "tief", "im", "gr\u00fc\u00b7nen", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da tr\u00e4umst du s\u00fc\u00df, da schl\u00e4fst du sacht,", "tokens": ["Da", "tr\u00e4umst", "du", "s\u00fc\u00df", ",", "da", "schl\u00e4fst", "du", "sacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist's auch von Tr\u00e4nen na\u00df.", "tokens": ["Ist's", "auch", "von", "Tr\u00e4\u00b7nen", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich schm\u00fccke dir dein K\u00e4mmerlein", "tokens": ["Ich", "schm\u00fc\u00b7cke", "dir", "dein", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blumen und mit Klee,", "tokens": ["Mit", "Blu\u00b7men", "und", "mit", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm', stille Braut, komm', geh' hinein,", "tokens": ["Komm'", ",", "stil\u00b7le", "Braut", ",", "komm'", ",", "geh'", "hin\u00b7ein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ist kein Schmerz, kein Weh.", "tokens": ["Da", "ist", "kein", "Schmerz", ",", "kein", "Weh", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bald werden bleiche Blumen hier", "tokens": ["Bald", "wer\u00b7den", "blei\u00b7che", "Blu\u00b7men", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rings um dein H\u00e4uslein bl\u00fchn;", "tokens": ["Rings", "um", "dein", "H\u00e4us\u00b7lein", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df vor deiner Kammert\u00fcr", "tokens": ["Ich", "mu\u00df", "vor", "dei\u00b7ner", "Kam\u00b7mer\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch wenig Stunden knien.", "tokens": ["Noch", "we\u00b7nig", "Stun\u00b7den", "kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und bis das letzte Bl\u00fcmlein schwand,", "tokens": ["Und", "bis", "das", "letz\u00b7te", "Bl\u00fcm\u00b7lein", "schwand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich geduldig sein;", "tokens": ["Will", "ich", "ge\u00b7dul\u00b7dig", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann reichst du mir die wei\u00dfe Hand,", "tokens": ["Dann", "reichst", "du", "mir", "die", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ziehst du mich hinein.", "tokens": ["Dann", "ziehst", "du", "mich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}