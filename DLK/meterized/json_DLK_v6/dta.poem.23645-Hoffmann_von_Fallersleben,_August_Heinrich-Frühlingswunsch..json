{"dta.poem.23645": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingswunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn jetzt in diesen langen Tagen", "tokens": ["Wenn", "jetzt", "in", "die\u00b7sen", "lan\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen wieder bl\u00fchn,", "tokens": ["Die", "Blu\u00b7men", "wie\u00b7der", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn jetzt die Nachtigallen schlagen", "tokens": ["Wenn", "jetzt", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im frischen Waldesgr\u00fcn;", "tokens": ["Im", "fri\u00b7schen", "Wal\u00b7des\u00b7gr\u00fcn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn bei dem Klange der Schallmeien", "tokens": ["Wenn", "bei", "dem", "Klan\u00b7ge", "der", "Schall\u00b7mei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.2": {"text": "Die Kinder gro\u00df und klein", "tokens": ["Die", "Kin\u00b7der", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier in den D\u00f6rfern, dort im Freien", "tokens": ["Hier", "in", "den", "D\u00f6r\u00b7fern", ",", "dort", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich froh zum Tanze reihn \u2014", "tokens": ["Sich", "froh", "zum", "Tan\u00b7ze", "reihn"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann mahnen T\u00e4nze, Kl\u00e4ng' und Lieder", "tokens": ["Dann", "mah\u00b7nen", "T\u00e4n\u00b7ze", ",", "Kl\u00e4ng'", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An dich, o Heimath, mich:", "tokens": ["An", "dich", ",", "o", "Hei\u00b7math", ",", "mich", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann preis' ich dich doch gl\u00fccklich wieder,", "tokens": ["Wann", "preis'", "ich", "dich", "doch", "gl\u00fcck\u00b7lich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann bist du frei wie ich?", "tokens": ["Wann", "bist", "du", "frei", "wie", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}