{"textgrid.poem.31761": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ich wollt, ich w\u00e4r Polizeiminister", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt, ich w\u00e4r Polizeiminister,", "tokens": ["Ich", "wollt", ",", "ich", "w\u00e4r", "Po\u00b7li\u00b7zei\u00b7mi\u00b7nis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da lie\u00df ich alle Leut arretieren,", "tokens": ["Da", "lie\u00df", "ich", "al\u00b7le", "Leut", "ar\u00b7re\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da lie\u00df ich die sch\u00f6nsten Frauen schier", "tokens": ["Da", "lie\u00df", "ich", "die", "sch\u00f6ns\u00b7ten", "Frau\u00b7en", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADJA"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mir all ins Gef\u00e4ngnis f\u00fchren.", "tokens": ["Mir", "all", "ins", "Ge\u00b7f\u00e4ng\u00b7nis", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da sollten sie schmachten in Seide und Samt,", "tokens": ["Da", "soll\u00b7ten", "sie", "schmach\u00b7ten", "in", "Sei\u00b7de", "und", "Samt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In Troddel und goldener Frange,", "tokens": ["In", "Trod\u00b7del", "und", "gol\u00b7de\u00b7ner", "Fran\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da sollten sie essen Ambrosia", "tokens": ["Da", "soll\u00b7ten", "sie", "es\u00b7sen", "A\u00b7mbro\u00b7sia"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und trinken den Wein der Champagne.", "tokens": ["Und", "trin\u00b7ken", "den", "Wein", "der", "Cham\u00b7pag\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und ach, im Verh\u00f6re geheimnisvoll,", "tokens": ["Und", "ach", ",", "im", "Ver\u00b7h\u00f6\u00b7re", "ge\u00b7heim\u00b7nis\u00b7voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wie w\u00fcrden sie leis mir verk\u00fcnden", "tokens": ["Wie", "w\u00fcr\u00b7den", "sie", "leis", "mir", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die lange, die liebliche Litanei", "tokens": ["Die", "lan\u00b7ge", ",", "die", "lieb\u00b7li\u00b7che", "Li\u00b7ta\u00b7nei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von ihren galanten S\u00fcnden.", "tokens": ["Von", "ih\u00b7ren", "ga\u00b7lan\u00b7ten", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Blonden g\u00e4be ich vierzig K\u00fc\u00df,", "tokens": ["Der", "Blon\u00b7den", "g\u00e4\u00b7be", "ich", "vier\u00b7zig", "K\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und mit achtzig straft ich die Braune,", "tokens": ["Und", "mit", "acht\u00b7zig", "straft", "ich", "die", "Brau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch ein schwarzes Weib verdammt ich zumeist \u2013", "tokens": ["Doch", "ein", "schwar\u00b7zes", "Weib", "ver\u00b7dammt", "ich", "zu\u00b7meist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$("], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Gerichtsschreiber lachten wie Faune.", "tokens": ["Die", "Ge\u00b7richts\u00b7schrei\u00b7ber", "lach\u00b7ten", "wie", "Fau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Und der Herr Prokurator w\u00fcrdevoll,", "tokens": ["Und", "der", "Herr", "Pro\u00b7ku\u00b7ra\u00b7tor", "w\u00fcr\u00b7de\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der spr\u00e4che mit vielem Pathos:", "tokens": ["Der", "spr\u00e4\u00b7che", "mit", "vie\u00b7lem", "Pa\u00b7thos", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PIS", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbich wasch meine H\u00e4nde in Regenwasser", "tokens": ["\u00bb", "ich", "wasch", "mei\u00b7ne", "H\u00e4n\u00b7de", "in", "Re\u00b7gen\u00b7was\u00b7ser"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "ADJD", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und in Unschuld wie Pontius Pilatos.\u00ab", "tokens": ["Und", "in", "Un\u00b7schuld", "wie", "Pon\u00b7ti\u00b7us", "Pi\u00b7la\u00b7tos", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "NN", "KOKOM", "NE", "NE", "$.", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Ja, staatsgef\u00e4hrlicher als Rebelln", "tokens": ["Ja", ",", "staats\u00b7ge\u00b7f\u00e4hr\u00b7li\u00b7cher", "als", "Re\u00b7belln"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "KOKOM", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist \u2013 Oh, ins Gef\u00e4ngnis schickt sie! \u2013", "tokens": ["Ist", "\u2013", "Oh", ",", "ins", "Ge\u00b7f\u00e4ng\u00b7nis", "schickt", "sie", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$(", "ITJ", "$,", "APPRART", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eine einzige kleine Frau mit ihrem", "tokens": ["Ei\u00b7ne", "ein\u00b7zi\u00b7ge", "klei\u00b7ne", "Frau", "mit", "ih\u00b7rem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "S\u00fc\u00dfen Corpus delicti.", "tokens": ["S\u00fc\u00b7\u00dfen", "Cor\u00b7pus", "de\u00b7lic\u00b7ti", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}