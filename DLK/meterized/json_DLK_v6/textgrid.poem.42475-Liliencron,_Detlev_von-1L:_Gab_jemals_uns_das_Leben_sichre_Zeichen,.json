{"textgrid.poem.42475": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gab jemals uns das Leben sichre Zeichen,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gab jemals uns das Leben sichre Zeichen,", "tokens": ["Gab", "je\u00b7mals", "uns", "das", "Le\u00b7ben", "sich\u00b7re", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wann wir das Ziel und ob wir es erreichen?", "tokens": ["Wann", "wir", "das", "Ziel", "und", "ob", "wir", "es", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "KON", "KOUS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wohl bl\u00fchen gr\u00fcne B\u00e4ume viel am Wege,", "tokens": ["Wohl", "bl\u00fc\u00b7hen", "gr\u00fc\u00b7ne", "B\u00e4u\u00b7me", "viel", "am", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sahst du je die stillen Friedenseichen?", "tokens": ["Doch", "sahst", "du", "je", "die", "stil\u00b7len", "Frie\u00b7den\u00b7sei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein blaues Schlo\u00df, das Gl\u00fcck, blitzt, eine Sonne", "tokens": ["Ein", "blau\u00b7es", "Schlo\u00df", ",", "das", "Gl\u00fcck", ",", "blitzt", ",", "ei\u00b7ne", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus fernen H\u00f6hen wie aus Zauberreichen,", "tokens": ["Aus", "fer\u00b7nen", "H\u00f6\u00b7hen", "wie", "aus", "Zau\u00b7ber\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und fiebernd, wie von Angst getrieben, sto\u00dfen", "tokens": ["Und", "fie\u00b7bernd", ",", "wie", "von", "Angst", "ge\u00b7trie\u00b7ben", ",", "sto\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVPP", "$,", "PWAV", "APPR", "NN", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Rosse wir die Sporen in die Weichen:", "tokens": ["Dem", "Ros\u00b7se", "wir", "die", "Spo\u00b7ren", "in", "die", "Wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Sonne zu erjagen. Doch mitnichten;", "tokens": ["Die", "Son\u00b7ne", "zu", "er\u00b7ja\u00b7gen", ".", "Doch", "mit\u00b7nich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Dicht vor uns wird sie wie ein Stern erbleichen.", "tokens": ["Dicht", "vor", "uns", "wird", "sie", "wie", "ein", "Stern", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Es knirscht das Weltenrad, das ungeheure,", "tokens": ["Es", "knirscht", "das", "Wel\u00b7ten\u00b7rad", ",", "das", "un\u00b7ge\u00b7heu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Dich ruhig tot, ger\u00e4tst du in die Speichen.", "tokens": ["Dich", "ru\u00b7hig", "tot", ",", "ge\u00b7r\u00e4tst", "du", "in", "die", "Spei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So bleibt barmherzig dir der Trostgedanke:", "tokens": ["So", "bleibt", "barm\u00b7her\u00b7zig", "dir", "der", "Trost\u00b7ge\u00b7dan\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Du z\u00e4hlst als Leiche nur zu andern Leichen.", "tokens": ["Du", "z\u00e4hlst", "als", "Lei\u00b7che", "nur", "zu", "an\u00b7dern", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}