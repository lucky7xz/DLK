{"dta.poem.9880": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Celadons abschied und seiner sch\u00e4ferin  \n weh klage.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eleonora die betr\u00fcbte", "tokens": ["E\u00b7leo\u00b7no\u00b7ra", "die", "be\u00b7tr\u00fcb\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieng in dem gr\u00fcnen auf und ab/", "tokens": ["Gieng", "in", "dem", "gr\u00fc\u00b7nen", "auf", "und", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ihr der sch\u00e4ffer/ der geliebte/", "tokens": ["Als", "ihr", "der", "sch\u00e4f\u00b7fer", "/", "der", "ge\u00b7lieb\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "$(", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den letzten ku\u00df mit thr\u00e4nen gab.", "tokens": ["Den", "letz\u00b7ten", "ku\u00df", "mit", "thr\u00e4\u00b7nen", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! sprach sie/ da\u00df ich ietzt mu\u00df leben/", "tokens": ["Ach", "!", "sprach", "sie", "/", "da\u00df", "ich", "ietzt", "mu\u00df", "le\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und meinem sch\u00e4ffer abschied geben.", "tokens": ["Und", "mei\u00b7nem", "sch\u00e4f\u00b7fer", "ab\u00b7schied", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie rung die liljen-weisse h\u00e4nde/", "tokens": ["Sie", "rung", "die", "lil\u00b7jen\u00b7weis\u00b7se", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schrie mit lauter weh und ach!", "tokens": ["Sie", "schrie", "mit", "lau\u00b7ter", "weh", "und", "ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "PTKVZ", "KON", "XY", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach ich! ach ich! ach ich elende!", "tokens": ["Ach", "ich", "!", "ach", "ich", "!", "ach", "ich", "e\u00b7len\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "$.", "XY", "PPER", "$.", "XY", "PPER", "ADJA", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Ich bin f\u00fcr meine noth zu schwach!", "tokens": ["Ich", "bin", "f\u00fcr", "mei\u00b7ne", "noth", "zu", "schwach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Celadon will von mir scheiden/", "tokens": ["Mein", "Ce\u00b7la\u00b7don", "will", "von", "mir", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der mit mir pflag allhier zu weiden.", "tokens": ["Der", "mit", "mir", "pflag", "all\u00b7hier", "zu", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie schrie und sanck vor ohnmacht nieder/", "tokens": ["Sie", "schrie", "und", "san\u00b7ck", "vor", "ohn\u00b7macht", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "APPR", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Red\u2019 und bewegung starb in ihr.", "tokens": ["Red'", "und", "be\u00b7we\u00b7gung", "starb", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der athem fand sich endlich wieder;", "tokens": ["Der", "at\u00b7hem", "fand", "sich", "end\u00b7lich", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch blieb die wehmuth f\u00fcr und f\u00fcr:", "tokens": ["Doch", "blieb", "die", "weh\u00b7muth", "f\u00fcr", "und", "f\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie schrie; doch sah man von der auen", "tokens": ["Sie", "schrie", ";", "doch", "sah", "man", "von", "der", "au\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie gantz erstarrt gen himmel schauen.", "tokens": ["Sie", "gantz", "er\u00b7starrt", "gen", "him\u00b7mel", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die sch\u00e4flein selbst die sah man klagen/", "tokens": ["Die", "sch\u00e4f\u00b7lein", "selbst", "die", "sah", "man", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "VVFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und traurig in dem grase gehn.", "tokens": ["Und", "trau\u00b7rig", "in", "dem", "gra\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Klagt/ sprach sie/ denn in wenig tagen", "tokens": ["Klagt", "/", "sprach", "sie", "/", "denn", "in", "we\u00b7nig", "ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "$(", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird man nicht mehr mich weiden sehn.", "tokens": ["Wird", "man", "nicht", "mehr", "mich", "wei\u00b7den", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PTKNEG", "ADV", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nehmt diesen crantz und diese myrthen", "tokens": ["Nehmt", "die\u00b7sen", "crantz", "und", "die\u00b7se", "myr\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "KON", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sucht euch einen andern hirten.", "tokens": ["Und", "sucht", "euch", "ei\u00b7nen", "an\u00b7dern", "hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der sch\u00e4ffer lieff sie auffzurichten/", "tokens": ["Der", "sch\u00e4f\u00b7fer", "lieff", "sie", "auff\u00b7zu\u00b7rich\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schlo\u00df sich ihm um hals und mund;", "tokens": ["Sie", "schlo\u00df", "sich", "ihm", "um", "hals", "und", "mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zeugt ihr/ sprach sie/ ihr nahen fichten/", "tokens": ["Zeugt", "ihr", "/", "sprach", "sie", "/", "ihr", "na\u00b7hen", "fich\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Was euch von meinen schmertzen kund.", "tokens": ["Was", "euch", "von", "mei\u00b7nen", "schmert\u00b7zen", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So offt es nur beginnt zu tagen/", "tokens": ["So", "offt", "es", "nur", "be\u00b7ginnt", "zu", "ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6rt ihr Eleonoren klagen.", "tokens": ["H\u00f6rt", "ihr", "E\u00b7leo\u00b7no\u00b7ren", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Celadon du machst die schmertzen/", "tokens": ["Mein", "Ce\u00b7la\u00b7don", "du", "machst", "die", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "ART", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Fieng sie zu ihrem sch\u00e4fer an:", "tokens": ["Fi\u00b7eng", "sie", "zu", "ih\u00b7rem", "sch\u00e4\u00b7fer", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du bist ein theil von meinem hertzen/", "tokens": ["Du", "bist", "ein", "theil", "von", "mei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ich auch nimmer missen kan.", "tokens": ["So", "ich", "auch", "nim\u00b7mer", "mis\u00b7sen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du aber wilst ietzt von mir ziehen/", "tokens": ["Du", "a\u00b7ber", "wilst", "ietzt", "von", "mir", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und die verliebte h\u00fcrden fliehen.", "tokens": ["Und", "die", "ver\u00b7lieb\u00b7te", "h\u00fcr\u00b7den", "flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die schlancken arme wurden m\u00fcde/", "tokens": ["Die", "schlan\u00b7cken", "ar\u00b7me", "wur\u00b7den", "m\u00fc\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Celadon von selben frey;", "tokens": ["Und", "Ce\u00b7la\u00b7don", "von", "sel\u00b7ben", "frey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie klagst du/ sprach er/ zum abschiede;", "tokens": ["Wie", "klagst", "du", "/", "sprach", "er", "/", "zum", "ab\u00b7schie\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "VVFIN", "PPER", "$(", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin ich doch auch abwesend treu.", "tokens": ["Bin", "ich", "doch", "auch", "ab\u00b7we\u00b7send", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schwere dir bey meiner heerde/", "tokens": ["Ich", "schwe\u00b7re", "dir", "bey", "mei\u00b7ner", "heer\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich bich ewig lieben werde.", "tokens": ["Da\u00df", "ich", "bich", "e\u00b7wig", "lie\u00b7ben", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sie k\u00fcsten sich/ und weinten beyde.", "tokens": ["Sie", "k\u00fcs\u00b7ten", "sich", "/", "und", "wein\u00b7ten", "bey\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der sch\u00e4fer fieng an fort zu gehn.", "tokens": ["Der", "sch\u00e4\u00b7fer", "fi\u00b7eng", "an", "fort", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie lief ihm nach die gantze heyde/", "tokens": ["Sie", "lief", "ihm", "nach", "die", "gant\u00b7ze", "hey\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um Celadon noch eins zu sehn/", "tokens": ["Um", "Ce\u00b7la\u00b7don", "noch", "eins", "zu", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ADV", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Woselbst sie mit gebrochner zungen/", "tokens": ["Wo\u00b7selbst", "sie", "mit", "ge\u00b7broch\u00b7ner", "zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhm ferner also nachgesungen;", "tokens": ["Jhm", "fer\u00b7ner", "al\u00b7so", "nach\u00b7ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Zeuch hin/ GOtt sey dein gut geleite/", "tokens": ["Zeuch", "hin", "/", "Gott", "sey", "dein", "gut", "ge\u00b7lei\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "NN", "VAFIN", "PPOSAT", "ADJD", "ADJA", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Es kommt mir nun nicht mehr die zeit/", "tokens": ["Es", "kommt", "mir", "nun", "nicht", "mehr", "die", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich den fr\u00fchling um dich spreite.", "tokens": ["Da\u00df", "ich", "den", "fr\u00fch\u00b7ling", "um", "dich", "sprei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zeuch hin/ o meine liebligkeit!", "tokens": ["Zeuch", "hin", "/", "o", "mei\u00b7ne", "lieb\u00b7lig\u00b7kei