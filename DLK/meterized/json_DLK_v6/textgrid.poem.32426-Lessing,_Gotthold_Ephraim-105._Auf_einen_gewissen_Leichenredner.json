{"textgrid.poem.32426": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "105. Auf einen gewissen Leichenredner", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Redner! dein Gesicht zieht j\u00e4mmerliche Falten,", "tokens": ["O", "Red\u00b7ner", "!", "dein", "Ge\u00b7sicht", "zieht", "j\u00e4m\u00b7mer\u00b7li\u00b7che", "Fal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem dein Maul erb\u00e4rmlich spricht.", "tokens": ["In\u00b7dem", "dein", "Maul", "er\u00b7b\u00e4rm\u00b7lich", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh du mir sollst die Leichenrede halten,", "tokens": ["Eh", "du", "mir", "sollst", "die", "Lei\u00b7chen\u00b7re\u00b7de", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wahrhaftig, lieber sterb' ich nicht!", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "lie\u00b7ber", "sterb'", "ich", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}