{"textgrid.poem.67835": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "7. Wilhelm und Margreth", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es traf sich an ein'm Sommertag,", "tokens": ["Es", "traf", "sich", "an", "ein'm", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Liebende sassen drauss'n;", "tokens": ["Zwei", "Lie\u00b7ben\u00b7de", "sas\u00b7sen", "draus\u00b7s'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie sassen zusammen den langen Tag,", "tokens": ["Sie", "sas\u00b7sen", "zu\u00b7sam\u00b7men", "den", "lan\u00b7gen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und sprachen sich noch nicht aus.", "tokens": ["Und", "spra\u00b7chen", "sich", "noch", "nicht", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich seh kein Leid an dir, Margreth,", "tokens": ["Ich", "seh", "kein", "Leid", "an", "dir", ",", "Mar\u00b7greth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirsts an mir nicht sehn;", "tokens": ["Du", "wirsts", "an", "mir", "nicht", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor eilf Uhr Morgens wird vor dir", "tokens": ["Vor", "eilf", "Uhr", "Mor\u00b7gens", "wird", "vor", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADV", "VAFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein' reiche Hochzeit gehn.", "tokens": ["Ein'", "rei\u00b7che", "Hoch\u00b7zeit", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n Gretchen sa\u00df am Fenster daheim,", "tokens": ["Sch\u00f6n", "Gret\u00b7chen", "sa\u00df", "am", "Fens\u00b7ter", "da\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und k\u00e4mmt ihr goldnes Haar,", "tokens": ["Und", "k\u00e4mmt", "ihr", "gold\u00b7nes", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als sie lieb-Will'm und seine Braut", "tokens": ["Als", "sie", "lie\u00b7bWill'm", "und", "sei\u00b7ne", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anreitend ward gewahr.", "tokens": ["An\u00b7rei\u00b7tend", "ward", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann legt sie nieder ihren beinen Kamm,", "tokens": ["Dann", "legt", "sie", "nie\u00b7der", "ih\u00b7ren", "bei\u00b7nen", "Kamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und flocht ihr Haar in Zweyn,", "tokens": ["Und", "flocht", "ihr", "Haar", "in", "Zweyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie ging wohl lebend aus ihrem Haus,", "tokens": ["Sie", "ging", "wohl", "le\u00b7bend", "aus", "ih\u00b7rem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kam nimmer lebend hinein.", "tokens": ["Kam", "nim\u00b7mer", "le\u00b7bend", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Als Tag war um und die Nacht war da,", "tokens": ["Als", "Tag", "war", "um", "und", "die", "Nacht", "war", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "APPR", "KON", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und alles schlafen th\u00e4t,", "tokens": ["Und", "al\u00b7les", "schla\u00b7fen", "th\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam der Geist der sch\u00f6n'n Margreth,", "tokens": ["Da", "kam", "der", "Geist", "der", "sch\u00f6n'n", "Mar\u00b7greth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stand an Wilhelms Bett.", "tokens": ["Und", "stand", "an", "Wil\u00b7helms", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwachst du noch, s\u00fcsser Wilhelm, sprach sie,", "tokens": ["\u00bb", "wachst", "du", "noch", ",", "s\u00fcs\u00b7ser", "Wil\u00b7helm", ",", "sprach", "sie", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NE", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lieb Wilhelm, oder schl\u00e4fst?", "tokens": ["Lieb", "Wil\u00b7helm", ",", "o\u00b7der", "schl\u00e4fst", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott geb dir Gl\u00fcck zum Brautbett dein,", "tokens": ["Gott", "geb", "dir", "Gl\u00fcck", "zum", "Braut\u00b7bett", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mir zur Leichenst\u00e4t!\u00ab", "tokens": ["Und", "mir", "zur", "Lei\u00b7chen\u00b7st\u00e4t", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Als Nacht war um und der Tag brach an,", "tokens": ["Als", "Nacht", "war", "um", "und", "der", "Tag", "brach", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "APPR", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und aufwacht Herr und Knecht,", "tokens": ["Und", "auf\u00b7wacht", "Herr", "und", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Br\u00e4utgam zu sein'r Lieben sprach:", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "zu", "sein'r", "Lie\u00b7ben", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbach, Schaz, ich weinen m\u00f6cht.", "tokens": ["\u00bb", "ach", ",", "Schaz", ",", "ich", "wei\u00b7nen", "m\u00f6cht", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich tr\u00e4umt ein'n Traum, mein liebes Weib,", "tokens": ["Ich", "tr\u00e4umt", "ein'n", "Traum", ",", "mein", "lie\u00b7bes", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tr\u00e4um'n ist nimmer gut;", "tokens": ["So", "tr\u00e4um'n", "ist", "nim\u00b7mer", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich tr\u00e4umt' mein Haus voll rothem Vieh,", "tokens": ["Ich", "tr\u00e4umt'", "mein", "Haus", "voll", "ro\u00b7them", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Brautbett voll von Blut.\u00ab", "tokens": ["Mein", "Braut\u00b7bett", "voll", "von", "Blut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbso ein Traum, so ein Traum, mein herzer Herr,", "tokens": ["\u00bb", "so", "ein", "Traum", ",", "so", "ein", "Traum", ",", "mein", "her\u00b7zer", "Herr", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "So tr\u00e4um'n ist nimmer gut;", "tokens": ["So", "tr\u00e4um'n", "ist", "nim\u00b7mer", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu tr\u00e4um'n das Haus voll rothem Vieh,", "tokens": ["Zu", "tr\u00e4um'n", "das", "Haus", "voll", "ro\u00b7them", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Brautbett voll von Blut.\u00ab", "tokens": ["Das", "Braut\u00b7bett", "voll", "von", "Blut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auf rief er all seine wackre Leut,", "tokens": ["Auf", "rief", "er", "all", "sei\u00b7ne", "wack\u00b7re", "Leut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PIAT", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bei Eins und Zwei und Drey'n,", "tokens": ["Bei", "Eins", "und", "Zwei", "und", "Drey'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "CARD", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sprach: \u00bbich mu\u00df hin zu Margreths Haus,", "tokens": ["Sprach", ":", "\u00bb", "ich", "mu\u00df", "hin", "zu", "Mar\u00b7greths", "Haus", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VMFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du l\u00e4\u00dft mich, Liebe mein!\u00ab", "tokens": ["Du", "l\u00e4\u00dft", "mich", ",", "Lie\u00b7be", "mein", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und als er kam vor Margreths Haus,", "tokens": ["Und", "als", "er", "kam", "vor", "Mar\u00b7greths", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er zog wohl an die Klink';", "tokens": ["Er", "zog", "wohl", "an", "die", "Klink'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer so schnell, als ihre sieben Br\u00fcder,", "tokens": ["Und", "wer", "so", "schnell", ",", "als", "ih\u00b7re", "sie\u00b7ben", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "$,", "KOUS", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu lassen Wilhelm in?", "tokens": ["Zu", "las\u00b7sen", "Wil\u00b7helm", "in", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NE", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann hob er auf das Leichentuch:", "tokens": ["Dann", "hob", "er", "auf", "das", "Lei\u00b7chen\u00b7tuch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbbitt', la\u00dft mich sehn die Leich',", "tokens": ["\u00bb", "bitt'", ",", "la\u00dft", "mich", "sehn", "die", "Leich'", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "VVIMP", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich d\u00fcnkt, ihr liebes Roth ist weg,", "tokens": ["Mich", "d\u00fcnkt", ",", "ihr", "lie\u00b7bes", "Roth", "ist", "weg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich d\u00fcnkt, sie sieht so bleich.", "tokens": ["Mich", "d\u00fcnkt", ",", "sie", "sieht", "so", "bleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ich will, lieb Gretchen, um dich thun,", "tokens": ["Ich", "will", ",", "lieb", "Gret\u00b7chen", ",", "um", "dich", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADJD", "NE", "$,", "KOUI", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was keiner thut um dich,", "tokens": ["Was", "kei\u00b7ner", "thut", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will k\u00fcssen deine Lippen bla\u00df,", "tokens": ["Will", "k\u00fcs\u00b7sen", "dei\u00b7ne", "Lip\u00b7pen", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht l\u00e4chelnd mehr auf mich.\u00ab", "tokens": ["Nicht", "l\u00e4\u00b7chelnd", "mehr", "auf", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Einsprachen da die sieben Br\u00fcder,", "tokens": ["Ein\u00b7spra\u00b7chen", "da", "die", "sie\u00b7ben", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gar traurig sprachen sie drein:", "tokens": ["Gar", "trau\u0