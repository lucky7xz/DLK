{"dta.poem.21578": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Die letzte Nacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab\u2019 zur Nacht gesessen", "tokens": ["Ich", "hab'", "zur", "Nacht", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit euch im goldnen Saal;", "tokens": ["Mit", "euch", "im", "gold\u00b7nen", "Saal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus blanken R\u00f6mern scho\u00df der Wein,", "tokens": ["Aus", "blan\u00b7ken", "R\u00f6\u00b7mern", "scho\u00df", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "S\u00fc\u00df duftete das Mahl.", "tokens": ["S\u00fc\u00df", "duf\u00b7te\u00b7te", "das", "Mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Luft ging schwer, die Ampel warf", "tokens": ["Die", "Luft", "ging", "schwer", ",", "die", "Am\u00b7pel", "warf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tr\u00fcb ihren letzten Schein \u2014 \u2014", "tokens": ["Tr\u00fcb", "ih\u00b7ren", "letz\u00b7ten", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Fenster auf! und k\u00fchl und scharf", "tokens": ["Die", "Fens\u00b7ter", "auf", "!", "und", "k\u00fchl", "und", "scharf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schl\u00e4gt Morgenwind herein.", "tokens": ["Schl\u00e4gt", "Mor\u00b7gen\u00b7wind", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aufschreckt vom Scho\u00df des Buhlen", "tokens": ["Auf\u00b7schreckt", "vom", "Scho\u00df", "des", "Buh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die leichtgesch\u00fcrzte Dirn,", "tokens": ["Die", "leicht\u00b7ge\u00b7sch\u00fcrz\u00b7te", "Dirn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Bursch springt auf und st\u00f6\u00dft die Faust", "tokens": ["Der", "Bursch", "springt", "auf", "und", "st\u00f6\u00dft", "die", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hohnlachend an die Stirn.", "tokens": ["Hohn\u00b7la\u00b7chend", "an", "die", "Stirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Dirne rei\u00dft er dann empor,", "tokens": ["Die", "Dir\u00b7ne", "rei\u00dft", "er", "dann", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00fc\u00dft sie lang und hei\u00df,", "tokens": ["Und", "k\u00fc\u00dft", "sie", "lang", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schwarz f\u00e4llt sein Haar wie Trauerflor", "tokens": ["Schwarz", "f\u00e4llt", "sein", "Haar", "wie", "Trau\u00b7er\u00b7flor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf ihres Nackens Wei\u00df.", "tokens": ["Auf", "ih\u00b7res", "Na\u00b7ckens", "Wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "\u201ef\u00fcllt einmal noch die Becher,", "tokens": ["\u201e", "f\u00fcllt", "ein\u00b7mal", "noch", "die", "Be\u00b7cher", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Genossen dieser Nacht.", "tokens": ["Ge\u00b7nos\u00b7sen", "die\u00b7ser", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sto\u00dft mit mir an, frisch, auf den Tod,", "tokens": ["Sto\u00dft", "mit", "mir", "an", ",", "frisch", ",", "auf", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "ADJD", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Glas sei ihm gebracht.", "tokens": ["Dies", "Glas", "sei", "ihm", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du trinkst der Liebe, du der Lust \u2014", "tokens": ["Du", "trinkst", "der", "Lie\u00b7be", ",", "du", "der", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das all ist Tand und Schall,", "tokens": ["Das", "all", "ist", "Tand", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Hauch in fieberkranker Brust,", "tokens": ["Ein", "Hauch", "in", "fie\u00b7ber\u00b7kran\u00b7ker", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Tod besiegt das all.", "tokens": ["Der", "Tod", "be\u00b7siegt", "das", "all", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wann hab ich nicht die Locken", "tokens": ["Wann", "hab", "ich", "nicht", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Kr\u00e4nzen mir geschm\u00fcckt,", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", "mir", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann sah ich je ein bl\u00fchend Weib,", "tokens": ["Wann", "sah", "ich", "je", "ein", "bl\u00fc\u00b7hend", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das nicht mein Gold ber\u00fcckt!", "tokens": ["Das", "nicht", "mein", "Gold", "be\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Begehrt\u2019 ich Ruhm, begehrt\u2019 ich Macht,", "tokens": ["Be\u00b7gehrt'", "ich", "Ruhm", ",", "be\u00b7gehrt'", "ich", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon lag\u2019s zu F\u00fc\u00dfen mir,", "tokens": ["Schon", "lag's", "zu", "F\u00fc\u00b7\u00dfen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Tag war Gluth und Gluth die Nacht \u2014", "tokens": ["Mein", "Tag", "war", "Gluth", "und", "Gluth", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Eins aber qu\u00e4lt mich hier.", "tokens": ["Eins", "a\u00b7ber", "qu\u00e4lt", "mich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das eine macht mich m\u00fcde,", "tokens": ["Das", "ei\u00b7ne", "macht", "mich", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Macht schaal mir Bett und Wein,", "tokens": ["Macht", "schaal", "mir", "Bett", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das grinst mich an aus jedem Aug\u2019", "tokens": ["Das", "grinst", "mich", "an", "aus", "je\u00b7dem", "Aug'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie marklos Todtenbein.", "tokens": ["Wie", "mark\u00b7los", "Tod\u00b7ten\u00b7bein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das l\u00f6scht am Himmel Licht und Tag,", "tokens": ["Das", "l\u00f6scht", "am", "Him\u00b7mel", "Licht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das zehrt die letzte Ruh, \u2014", "tokens": ["Das", "zehrt", "die", "letz\u00b7te", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Frage ist\u2019s, die tolle Frag\u2019,", "tokens": ["Die", "Fra\u00b7ge", "ist's", ",", "die", "tol\u00b7le", "Frag'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wozu dies all, wozu?", "tokens": ["Wo\u00b7zu", "dies", "all", ",", "wo\u00b7zu", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PDS", "PIAT", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo ist ein Lenz ohn\u2019 Winter,", "tokens": ["Wo", "ist", "ein", "Lenz", "ohn'", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Lieben ohne End\u2019, \u2014", "tokens": ["Ein", "Lie\u00b7ben", "oh\u00b7ne", "End'", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ist ein Feuer, das nicht matt", "tokens": ["Wo", "ist", "ein", "Feu\u00b7er", ",", "das", "nicht", "matt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Kohl\u2019 und Asche brennt.", "tokens": ["Zu", "Kohl'", "und", "A\u00b7sche", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ehern steht kein Fels, kein Land,", "tokens": ["So", "e\u00b7hern", "steht", "kein", "Fels", ",", "kein", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem nicht die S\u00fcndfluth droht \u2014", "tokens": ["Dem", "nicht", "die", "S\u00fcnd\u00b7fluth", "droht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nur eins lebt ewig, eins h\u00e4lt Stand,", "tokens": ["Nur", "eins", "lebt", "e\u00b7wig", ",", "eins", "h\u00e4lt", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "$,", "PIS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Leben ist der Tod.\u201c", "tokens": ["Das", "Le\u00b7ben", "ist", "der", "Tod", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er ruft\u2019s und wie am Grabe", "tokens": ["Er", "ruft's", "und", "wie", "am", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4lt pl\u00f6tzlich alles Ruh, \u2014", "tokens": ["H\u00e4lt", "pl\u00f6tz\u00b7lich", "al\u00b7les", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da zuckt ein Blitz, da f\u00e4llt ein Schu\u00df,", "tokens": ["Da", "zuckt", "ein", "Blitz", ",", "da", "f\u00e4llt", "ein", "Schu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leise haucht\u2019s Wozu?", "tokens": ["Und", "lei\u00b7se", "haucht's", "Wo\u00b7zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Dirne st\u00fcrzt zur Th\u00fcr und schreit,", "tokens": ["Die", "Dir\u00b7ne", "st\u00fcrzt", "zur", "Th\u00fcr", "und", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wirft klirrend den Pokal, \u2014", "tokens": ["Wirft", "klir\u00b7rend", "den", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und durch die Fenster hell und breit", "tokens": ["Und", "durch", "die", "Fens\u00b7ter", "hell", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gl\u00fcht auf des Morgens Strahl.", "tokens": ["Gl\u00fcht", "auf", "des", "Mor\u00b7gens", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}