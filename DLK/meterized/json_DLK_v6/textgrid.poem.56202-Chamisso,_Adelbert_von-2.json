{"textgrid.poem.56202": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter, Mutter! meine Puppe", "tokens": ["Mut\u00b7ter", ",", "Mut\u00b7ter", "!", "mei\u00b7ne", "Pup\u00b7pe"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich in den Schlaf gewiegt,", "tokens": ["Hab", "ich", "in", "den", "Schlaf", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gute Mutter, komm und siehe,", "tokens": ["Gu\u00b7te", "Mut\u00b7ter", ",", "komm", "und", "sie\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "VVIMP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so englisch sie da liegt.", "tokens": ["Wie", "so", "eng\u00b7lisch", "sie", "da", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Vater wies mich ab und sagte:", "tokens": ["Va\u00b7ter", "wies", "mich", "ab", "und", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbgeh, du bist ein dummes Kind\u00ab;", "tokens": ["\u00bb", "geh", ",", "du", "bist", "ein", "dum\u00b7mes", "Kind", "\u00ab", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du nur, Mutter, kannst begreifen,", "tokens": ["Du", "nur", ",", "Mut\u00b7ter", ",", "kannst", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche meine Freuden sind.", "tokens": ["Wel\u00b7che", "mei\u00b7ne", "Freu\u00b7den", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie du mit den kleinen Kindern,", "tokens": ["Wie", "du", "mit", "den", "klei\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich alles mit ihr tun,", "tokens": ["Will", "ich", "al\u00b7les", "mit", "ihr", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie soll in ihrer Wiege", "tokens": ["Und", "sie", "soll", "in", "ih\u00b7rer", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neben meinem Bette ruhn.", "tokens": ["Ne\u00b7ben", "mei\u00b7nem", "Bet\u00b7te", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schl\u00e4ft sie, werd ich von ihr tr\u00e4umen,", "tokens": ["Schl\u00e4ft", "sie", ",", "werd", "ich", "von", "ihr", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreit sie auf, erwach ich gleich, \u2013", "tokens": ["Schreit", "sie", "auf", ",", "er\u00b7wach", "ich", "gleich", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "ADV", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine himmlisch gute Mutter,", "tokens": ["Mei\u00b7ne", "himm\u00b7lisch", "gu\u00b7te", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie bin ich doch so reich!", "tokens": ["O", "wie", "bin", "ich", "doch", "so", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}