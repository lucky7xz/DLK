{"textgrid.poem.57827": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Jungfernkranz", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und da\u00df ich eine Jungfer bin", "tokens": ["Und", "da\u00df", "ich", "ei\u00b7ne", "Jung\u00b7fer", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und habe keinen Mann,", "tokens": ["Und", "ha\u00b7be", "kei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und noch nicht wei\u00df, was Liebe ist,", "tokens": ["Und", "noch", "nicht", "wei\u00df", ",", "was", "Lie\u00b7be", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das steht mir wenig an.", "tokens": ["Das", "steht", "mir", "we\u00b7nig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was hilft mir denn mein Jungfernkranz,", "tokens": ["Was", "hilft", "mir", "denn", "mein", "Jung\u00b7fern\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich ihn ganz allein,", "tokens": ["Hab'", "ich", "ihn", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich trug ihn zwanzig Jahre lang,", "tokens": ["Ich", "trug", "ihn", "zwan\u00b7zig", "Jah\u00b7re", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald wird verwelkt er sein.", "tokens": ["Bald", "wird", "ver\u00b7welkt", "er", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verwelken aber soll er nicht", "tokens": ["Ver\u00b7wel\u00b7ken", "a\u00b7ber", "soll", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Sonne und vor Wind,", "tokens": ["Vor", "Son\u00b7ne", "und", "vor", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich h\u00e4ng' ihn abends in den Tau,", "tokens": ["Ich", "h\u00e4ng'", "ihn", "a\u00b7bends", "in", "den", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis da\u00df ihn einer find't.", "tokens": ["Bis", "da\u00df", "ihn", "ei\u00b7ner", "find'", "t."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wer ihn find't, das sag' ich frei,", "tokens": ["Und", "wer", "ihn", "find't", ",", "das", "sag'", "ich", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn auch behalten kann;", "tokens": ["Ihn", "auch", "be\u00b7hal\u00b7ten", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich trug ihn zwanzig Jahre lang,", "tokens": ["Ich", "trug", "ihn", "zwan\u00b7zig", "Jah\u00b7re", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir liegt nichts mehr daran.", "tokens": ["Mir", "liegt", "nichts", "mehr", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}