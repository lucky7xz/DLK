{"textgrid.poem.34789": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flaschen sind leer, das Fr\u00fchst\u00fcck ist gut,", "tokens": ["Die", "Fla\u00b7schen", "sind", "leer", ",", "das", "Fr\u00fch\u00b7st\u00fcck", "ist", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die D\u00e4mchen sind rosig erhitzet;", "tokens": ["Die", "D\u00e4m\u00b7chen", "sind", "ro\u00b7sig", "er\u00b7hit\u00b7zet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie l\u00fcften das Mieder mit \u00dcbermut,", "tokens": ["Sie", "l\u00fcf\u00b7ten", "das", "Mie\u00b7der", "mit", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich glaube, sie sind bespitzet.", "tokens": ["Ich", "glau\u00b7be", ",", "sie", "sind", "be\u00b7spit\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Schulter wie wei\u00df, die Br\u00fcstchen wie nett!", "tokens": ["Die", "Schul\u00b7ter", "wie", "wei\u00df", ",", "die", "Br\u00fcst\u00b7chen", "wie", "nett", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "VVFIN", "$,", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Herz erbebet vor Schrecken.", "tokens": ["Mein", "Herz", "er\u00b7be\u00b7bet", "vor", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun werfen sie lachend sich aufs Bett,", "tokens": ["Nun", "wer\u00b7fen", "sie", "la\u00b7chend", "sich", "aufs", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und h\u00fcllen sich ein mit den Decken.", "tokens": ["Und", "h\u00fcl\u00b7len", "sich", "ein", "mit", "den", "De\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Sie ziehen nun gar die Gardinen vor,", "tokens": ["Sie", "zie\u00b7hen", "nun", "gar", "die", "Gar\u00b7di\u00b7nen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schnarchen am End' um die Wette.", "tokens": ["Und", "schnar\u00b7chen", "am", "End'", "um", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da steh ich im Zimmer, ein einsamer Tor,", "tokens": ["Da", "steh", "ich", "im", "Zim\u00b7mer", ",", "ein", "ein\u00b7sa\u00b7mer", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Betrachte verlegen das Bette.", "tokens": ["Be\u00b7trach\u00b7te", "ver\u00b7le\u00b7gen", "das", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}