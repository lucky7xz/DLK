{"textgrid.poem.45580": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[widmungsblatt f\u00fcr den Hofschauspieler Joseph Schmidt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie fromm in Baden unsre Lebensweise,", "tokens": ["Wie", "fromm", "in", "Ba\u00b7den", "uns\u00b7re", "Le\u00b7bens\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Merkt, wer verfolgt dort unsers Wandels Spur:", "tokens": ["Merkt", ",", "wer", "ver\u00b7folgt", "dort", "un\u00b7sers", "Wan\u00b7dels", "Spur", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sankt Anna gab uns unsres Leibes-Speise,", "tokens": ["Sankt", "An\u00b7na", "gab", "uns", "uns\u00b7res", "Lei\u00b7bes\u00b7Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Geistestrank die heilige Natur.", "tokens": ["Den", "Geis\u00b7tes\u00b7trank", "die", "hei\u00b7li\u00b7ge", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch hat der Himmel uns zu f\u00fchlbar fast gesegnet,", "tokens": ["Doch", "hat", "der", "Him\u00b7mel", "uns", "zu", "f\u00fchl\u00b7bar", "fast", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "PTKA", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da es in unsre Wallfahrt meist geregnet.", "tokens": ["Da", "es", "in", "uns\u00b7re", "Wall\u00b7fahrt", "meist", "ge\u00b7reg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}