{"textgrid.poem.37200": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Statue einer Nymphe", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am pl\u00e4tschernden Ger\u00e4usch der Welle", "tokens": ["Am", "pl\u00e4t\u00b7schern\u00b7den", "Ge\u00b7r\u00e4usch", "der", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entschlief ich Nymphe dieser Quelle.", "tokens": ["Ent\u00b7schlief", "ich", "Nym\u00b7phe", "die\u00b7ser", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du, der herab zum baden steigt,", "tokens": ["Du", ",", "der", "her\u00b7ab", "zum", "ba\u00b7den", "steigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "APPRART", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du, der zu trinken, o schweigt!", "tokens": ["Du", ",", "der", "zu", "trin\u00b7ken", ",", "o", "schweigt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PTKZU", "VVINF", "$,", "FM", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}