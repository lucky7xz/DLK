{"textgrid.poem.39799": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Kirchenbesuch am 18. October 1814", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne lichtet sich in Morgenbl\u00e4ue,", "tokens": ["Die", "Son\u00b7ne", "lich\u00b7tet", "sich", "in", "Mor\u00b7gen\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will hell ihr siegreich freies Land bescheinen;", "tokens": ["Will", "hell", "ihr", "sieg\u00b7reich", "frei\u00b7es", "Land", "be\u00b7schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O dumpfe Stadt, wirst du dich nicht vereinen,", "tokens": ["O", "dump\u00b7fe", "Stadt", ",", "wirst", "du", "dich", "nicht", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "VAFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Herrn zu preisen f\u00fcr die ew'ge Treue?", "tokens": ["Den", "Herrn", "zu", "prei\u00b7sen", "f\u00fcr", "die", "ew'\u00b7ge", "Treu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verschlossen stehen deine Tempelb\u00e4ue,", "tokens": ["Ver\u00b7schlos\u00b7sen", "ste\u00b7hen", "dei\u00b7ne", "Tem\u00b7pel\u00b7b\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Glocke ruft zur sch\u00f6nsten Feier Keinen;", "tokens": ["Die", "Glo\u00b7cke", "ruft", "zur", "sch\u00f6ns\u00b7ten", "Fei\u00b7er", "Kei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch tret' ich in die Kirche, \u2013 Dank dem kleinen", "tokens": ["Doch", "tret'", "ich", "in", "die", "Kir\u00b7che", ",", "\u2013", "Dank", "dem", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "$(", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Halb offnen Pf\u00f6rtlein dort \u2013 da\u00df ich mich freue.", "tokens": ["Halb", "off\u00b7nen", "Pf\u00f6rt\u00b7lein", "dort", "\u2013", "da\u00df", "ich", "mich", "freu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADV", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr Heil'genbilder rings, so fromme, schlichte,", "tokens": ["Ihr", "Heil'\u00b7gen\u00b7bil\u00b7der", "rings", ",", "so", "from\u00b7me", ",", "schlich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ADV", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00dft euch durch meinen leisen Tritt nicht st\u00f6ren,", "tokens": ["La\u00dft", "euch", "durch", "mei\u00b7nen", "lei\u00b7sen", "Tritt", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr feiert betend wohl des Tags Geschichte!", "tokens": ["Ihr", "fei\u00b7ert", "be\u00b7tend", "wohl", "des", "Tags", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Kirche, sel'ger, als bei vollen Ch\u00f6ren,", "tokens": ["O", "Kir\u00b7che", ",", "sel'\u00b7ger", ",", "als", "bei", "vol\u00b7len", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "$,", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Gott erf\u00fcllt, durchstralt vom Morgenlichte!", "tokens": ["Von", "Gott", "er\u00b7f\u00fcllt", ",", "durch\u00b7stralt", "vom", "Mor\u00b7gen\u00b7lich\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer ist's, der ", "tokens": ["Wer", "ist's", ",", "der"], "token_info": ["word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS"], "meter": "-+-", "measure": "amphibrach.single"}}}}}