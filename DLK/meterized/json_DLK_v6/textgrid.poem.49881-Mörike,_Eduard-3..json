{"textgrid.poem.49881": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Irrsal kam in die Mondscheing\u00e4rten", "tokens": ["Ein", "Irr\u00b7sal", "kam", "in", "die", "Mond\u00b7schein\u00b7g\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Einer einst heiligen Liebe.", "tokens": ["Ei\u00b7ner", "einst", "hei\u00b7li\u00b7gen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Schaudernd entdeckt ich verj\u00e4hrten Betrug.", "tokens": ["Schau\u00b7dernd", "ent\u00b7deckt", "ich", "ver\u00b7j\u00e4hr\u00b7ten", "Be\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und mit weinendem Blick, doch grausam,", "tokens": ["Und", "mit", "wei\u00b7nen\u00b7dem", "Blick", ",", "doch", "grau\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Hie\u00df ich das schlanke,", "tokens": ["Hie\u00df", "ich", "das", "schlan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zauberhafte M\u00e4dchen", "tokens": ["Zau\u00b7ber\u00b7haf\u00b7te", "M\u00e4d\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ferne gehen von mir.", "tokens": ["Fer\u00b7ne", "ge\u00b7hen", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Ach, ihre hohe Stirn,", "tokens": ["Ach", ",", "ih\u00b7re", "ho\u00b7he", "Stirn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "War gesenkt, denn sie liebte mich;", "tokens": ["War", "ge\u00b7senkt", ",", "denn", "sie", "lieb\u00b7te", "mich", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Aber sie zog mit Schweigen", "tokens": ["A\u00b7ber", "sie", "zog", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "Fort in die graue", "tokens": ["Fort", "in", "die", "grau\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA"], "meter": "++-+-", "measure": "iambic.di"}, "line.12": {"text": "Welt hinaus.", "tokens": ["Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Krank seitdem,", "tokens": ["Krank", "seit\u00b7dem", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Wund ist und wehe mein Herz.", "tokens": ["Wund", "ist", "und", "we\u00b7he", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Nimmer wird es genesen!", "tokens": ["Nim\u00b7mer", "wird", "es", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Als ginge, luftgesponnen, ein Zauberfaden", "tokens": ["Als", "gin\u00b7ge", ",", "luft\u00b7ge\u00b7spon\u00b7nen", ",", "ein", "Zau\u00b7ber\u00b7fa\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "VVFIN", "$,", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Von ihr zu mir, ein \u00e4ngstig Band,", "tokens": ["Von", "ihr", "zu", "mir", ",", "ein", "\u00e4ngs\u00b7tig", "Band", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So zieht es, zieht mich schmachtend ihr nach!", "tokens": ["So", "zieht", "es", ",", "zieht", "mich", "schmach\u00b7tend", "ihr", "nach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "\u2013 Wie? wenn ich eines Tags auf meiner Schwelle", "tokens": ["\u2013", "Wie", "?", "wenn", "ich", "ei\u00b7nes", "Tags", "auf", "mei\u00b7ner", "Schwel\u00b7le"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "$.", "KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sie sitzen f\u00e4nde, wie einst, im Morgen-Zwielicht,", "tokens": ["Sie", "sit\u00b7zen", "f\u00e4n\u00b7de", ",", "wie", "einst", ",", "im", "Mor\u00b7gen\u00b7Zwie\u00b7licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "$,", "PWAV", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Wanderb\u00fcndel neben ihr,", "tokens": ["Das", "Wan\u00b7der\u00b7b\u00fcn\u00b7del", "ne\u00b7ben", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ihr Auge, treuherzig zu mir aufschauend,", "tokens": ["Und", "ihr", "Au\u00b7ge", ",", "treu\u00b7her\u00b7zig", "zu", "mir", "auf\u00b7schau\u00b7end", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Sagte, da bin ich wieder", "tokens": ["Sag\u00b7te", ",", "da", "bin", "ich", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Hergekommen aus weiter Welt!", "tokens": ["Her\u00b7ge\u00b7kom\u00b7men", "aus", "wei\u00b7ter", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}