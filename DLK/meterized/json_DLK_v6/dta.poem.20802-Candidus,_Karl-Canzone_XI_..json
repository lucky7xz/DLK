{"dta.poem.20802": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   XI .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Der Welt will d\u00fcnken da\u00df zweitausend Jahre", "tokens": ["Der", "Welt", "will", "d\u00fcn\u00b7ken", "da\u00df", "zweit\u00b7au\u00b7send", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF", "KOUS", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Trauer und der Treue mehr als schicklich.", "tokens": ["Der", "Trau\u00b7er", "und", "der", "Treu\u00b7e", "mehr", "als", "schick\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr bist du tot, ja l\u00e4ngst tot, und sie findet", "tokens": ["Ihr", "bist", "du", "tot", ",", "ja", "l\u00e4ngst", "tot", ",", "und", "sie", "fin\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "ADV", "ADV", "ADJD", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein zu gedenken nimmermehr erquicklich.", "tokens": ["Dein", "zu", "ge\u00b7den\u00b7ken", "nim\u00b7mer\u00b7mehr", "er\u00b7quick\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKZU", "VVINF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich aber wei\u00df, Geliebter! da\u00df die Bahre", "tokens": ["Ich", "a\u00b7ber", "wei\u00df", ",", "Ge\u00b7lieb\u00b7ter", "!", "da\u00df", "die", "Bah\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "NN", "$.", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dich nicht erbeutet und kein Grab dich bindet.", "tokens": ["Dich", "nicht", "er\u00b7beu\u00b7tet", "und", "kein", "Grab", "dich", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "KON", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du lebst und nimmer schwindet", "tokens": ["Du", "lebst", "und", "nim\u00b7mer", "schwin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In deinen Freundesarmen mein Gedenken", "tokens": ["In", "dei\u00b7nen", "Freun\u00b7des\u00b7ar\u00b7men", "mein", "Ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "An alle Todesqual die du gelitten", "tokens": ["An", "al\u00b7le", "To\u00b7des\u00b7qual", "die", "du", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als du mich dir erstritten", "tokens": ["Als", "du", "mich", "dir", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und neustets mu\u00df ich mich darein versenken.", "tokens": ["Und", "neus\u00b7tets", "mu\u00df", "ich", "mich", "da\u00b7rein", "ver\u00b7sen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Welch roher Sinn, wenn dein ich je verg\u00e4\u00dfe", "tokens": ["Welch", "ro\u00b7her", "Sinn", ",", "wenn", "dein", "ich", "je", "ver\u00b7g\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und meine Dankbarkeit nach Jahren m\u00e4\u00dfe!", "tokens": ["Und", "mei\u00b7ne", "Dank\u00b7bar\u00b7keit", "nach", "Jah\u00b7ren", "m\u00e4\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Menschheit hast du Ewiges erblutet,", "tokens": ["Der", "Menschheit", "hast", "du", "E\u00b7wi\u00b7ges", "er\u00b7blu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJA", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und unser Dank, nicht darf er sein verg\u00e4nglich.", "tokens": ["Und", "un\u00b7ser", "Dank", ",", "nicht", "darf", "er", "sein", "ver\u00b7g\u00e4ng\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PTKNEG", "VMFIN", "PPER", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, w\u00e4re doch Einbu\u00dfe sein Erkalten,", "tokens": ["Ach", ",", "w\u00e4\u00b7re", "doch", "Ein\u00b7bu\u00b7\u00dfe", "sein", "Er\u00b7kal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verlust des Heils womit du \u00fcberschw\u00e4nglich", "tokens": ["Ver\u00b7lust", "des", "Heils", "wo\u00b7mit", "du", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Uns ringsum wie mit einem Meer umflutet!", "tokens": ["Uns", "ring\u00b7sum", "wie", "mit", "ei\u00b7nem", "Meer", "um\u00b7flu\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Darum erkor ja Tod dein klares Walten,", "tokens": ["Da\u00b7rum", "er\u00b7kor", "ja", "Tod", "dein", "kla\u00b7res", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df du uns seist erhalten", "tokens": ["Da\u00df", "du", "uns", "seist", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In brennendem, erkennendem Ged\u00e4chtni\u00df.", "tokens": ["In", "bren\u00b7nen\u00b7dem", ",", "er\u00b7ken\u00b7nen\u00b7dem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie heilig, wenn der Kirche Glocken rufen", "tokens": ["Wie", "hei\u00b7lig", ",", "wenn", "der", "Kir\u00b7che", "Glo\u00b7cken", "ru\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zu deines Tisches Stufen,", "tokens": ["Zu", "dei\u00b7nes", "Ti\u00b7sches", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie heilig soll mir sein, Herr, dein Verm\u00e4chtni\u00df!", "tokens": ["Wie", "hei\u00b7lig", "soll", "mir", "sein", ",", "Herr", ",", "dein", "Ver\u00b7m\u00e4cht\u00b7ni\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VAINF", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Stets m\u00f6ge da in stillem Liebestrauern", "tokens": ["Stets", "m\u00f6\u00b7ge", "da", "in", "stil\u00b7lem", "Lie\u00b7bes\u00b7trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ein neues Ma\u00df des Heils mich \u00fcberschauern.", "tokens": ["Ein", "neu\u00b7es", "Ma\u00df", "des", "Heils", "mich", "\u00fc\u00b7bersc\u00b7hau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da will ich alles deines Bluts gedenken", "tokens": ["Da", "will", "ich", "al\u00b7les", "dei\u00b7nes", "Bluts", "ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das uns zu gut geflossen gleich dem Weine.", "tokens": ["Das", "uns", "zu", "gut", "ge\u00b7flos\u00b7sen", "gleich", "dem", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKA", "ADJD", "VVPP", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Ahnen der Propheten aller V\u00f6lker,", "tokens": ["Das", "Ah\u00b7nen", "der", "Pro\u00b7phe\u00b7ten", "al\u00b7ler", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du warst's ja selbst, und ", "tokens": ["Du", "war\u00b7st's", "ja", "selbst", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "KON"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Von Anbeginn ja wolltest du uns lenken.", "tokens": ["Von", "An\u00b7be\u00b7ginn", "ja", "woll\u00b7test", "du", "uns", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Allbildungswort, ", "tokens": ["All\u00b7bil\u00b7dungs\u00b7wort", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du bist auch Schwung und Anlauf der Geschichte.", "tokens": ["Du", "bist", "auch", "Schwung", "und", "An\u00b7lauf", "der", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein Blut von Alters her sind die Entdecker,", "tokens": ["Dein", "Blut", "von", "Al\u00b7ters", "her", "sind", "die", "Ent\u00b7de\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APZR", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Erfinder und Erwecker", "tokens": ["Er\u00b7fin\u00b7der", "und", "Er\u00b7we\u00b7cker"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die durch ihr Leuchten zeugten von dem Lichte.", "tokens": ["Die", "durch", "ihr", "Leuch\u00b7ten", "zeug\u00b7ten", "von", "dem", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Du blutetest so oft sie mochten leiden,", "tokens": ["Du", "blu\u00b7te\u00b7test", "so", "oft", "sie", "moch\u00b7ten", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie alle darf ich nicht vom Mittler scheiden.", "tokens": ["Sie", "al\u00b7le", "darf", "ich", "nicht", "vom", "Mitt\u00b7ler", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Menschen Jesus dann zertratst der Schlange", "tokens": ["Im", "Men\u00b7schen", "Je\u00b7sus", "dann", "zer\u00b7tratst", "der", "Schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Kopf du und sie stach dich in die Ferse,", "tokens": ["Den", "Kopf", "du", "und", "sie", "stach", "dich", "in", "die", "Fer\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und purpurn quoll, derweil die Knechte flohen,", "tokens": ["Und", "pur\u00b7purn", "quoll", ",", "der\u00b7weil", "die", "Knech\u00b7te", "flo\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des S\u00e4manns Blut in's Feld bei Pflug und Herse.", "tokens": ["Des", "S\u00e4\u00b7manns", "Blut", "in's", "Feld", "bei", "Pflug", "und", "Her\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O eine Nacht der H\u00f6lle, eine bange", "tokens": ["O", "ei\u00b7ne", "Nacht", "der", "H\u00f6l\u00b7le", ",", "ei\u00b7ne", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nacht des Verrats erfa\u00dfte meinen Hohen", "tokens": ["Nacht", "des", "Ver\u00b7rats", "er\u00b7fa\u00df\u00b7te", "mei\u00b7nen", "Ho\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und gab ihn preis der rohen", "tokens": ["Und", "gab", "ihn", "preis", "der", "ro\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gottlosigkeit des P\u00f6bels aller St\u00e4nde!", "tokens": ["Gott\u00b7lo\u00b7sig\u00b7keit", "des", "P\u00f6\u00b7bels", "al\u00b7ler", "St\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wer kann den Qualkelch singen den du trankest,", "tokens": ["Wer", "kann", "den", "Qual\u00b7kelch", "sin\u00b7gen", "den", "du", "tran\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVFIN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "O Herr, und wie du trankest!", "tokens": ["O", "Herr", ",", "und", "wie", "du", "tran\u00b7kest", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das Kreuz war deiner Marter nur das Ende.", "tokens": ["Das", "Kreuz", "war", "dei\u00b7ner", "Mar\u00b7ter", "nur", "das", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Erhabner! dein gedenk' ich voller Demut", "tokens": ["Er\u00b7hab\u00b7ner", "!", "dein",