{"textgrid.poem.63805": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16. Juno Ludovisi", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie ein Gesang des Homer\u00ab? Und was denn sagte dies Antlitz", "tokens": ["\u00bb", "wie", "ein", "Ge\u00b7sang", "des", "Ho\u00b7mer", "\u00ab", "?", "Und", "was", "denn", "sag\u00b7te", "dies", "Ant\u00b7litz"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOKOM", "ART", "NN", "ART", "NE", "$(", "$.", "KON", "PWS", "ADV", "VVFIN", "PDS", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mir vom Zorn des Achill, von der Sirenen Gesang?", "tokens": ["Mir", "vom", "Zorn", "des", "A\u00b7chill", ",", "von", "der", "Si\u00b7re\u00b7nen", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NE", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nein, kein dichtender Geist, kein irdischer Zauber beseelt dich:", "tokens": ["Nein", ",", "kein", "dich\u00b7ten\u00b7der", "Geist", ",", "kein", "ir\u00b7di\u00b7scher", "Zau\u00b7ber", "be\u00b7seelt", "dich", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIAT", "ADJA", "NN", "$,", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "So unnahbar und k\u00fchl leuchtet der \u00c4ther allein.", "tokens": ["So", "un\u00b7nah\u00b7bar", "und", "k\u00fchl", "leuch\u00b7tet", "der", "\u00c4\u00b7ther", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}}}}}