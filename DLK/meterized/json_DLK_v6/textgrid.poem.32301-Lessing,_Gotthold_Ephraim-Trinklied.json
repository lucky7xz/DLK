{"textgrid.poem.32301": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voll, voll, voll,", "tokens": ["Voll", ",", "voll", ",", "voll", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Freunde, macht euch voll!", "tokens": ["Freun\u00b7de", ",", "macht", "euch", "voll", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wein, Wein, Wein,", "tokens": ["Wein", ",", "Wein", ",", "Wein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Freunde, schenkt ihn ein!", "tokens": ["Freun\u00b7de", ",", "schenkt", "ihn", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "K\u00fc\u00dft, k\u00fc\u00dft, k\u00fc\u00dft,", "tokens": ["K\u00fc\u00dft", ",", "k\u00fc\u00dft", ",", "k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Die euch wieder k\u00fc\u00dft!", "tokens": ["Die", "euch", "wie\u00b7der", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Voll von Wein,", "tokens": ["Voll", "von", "Wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Voll von Liebe,", "tokens": ["Voll", "von", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Voll von Wein und Liebe,", "tokens": ["Voll", "von", "Wein", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Freunde, voll zu sein,", "tokens": ["Freun\u00b7de", ",", "voll", "zu", "sein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "K\u00fc\u00dft und schenket ein!", "tokens": ["K\u00fc\u00dft", "und", "schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}