{"textgrid.poem.48717": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Auf Herrn Paul Christof Lindners Namenstag 1631", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der derbe Rausch, den du mir beigebracht,", "tokens": ["Der", "der\u00b7be", "Rausch", ",", "den", "du", "mir", "bei\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bestellet mir das Uhrwerk meiner Sinnen.", "tokens": ["be\u00b7stel\u00b7let", "mir", "das", "Uhr\u00b7werk", "mei\u00b7ner", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein' erste Lust, die deutschen Kastalinnen", "tokens": ["Mein'", "ers\u00b7te", "Lust", ",", "die", "deut\u00b7schen", "Kas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sind noch bei mir anitzt nicht aufgewacht.", "tokens": ["sind", "noch", "bei", "mir", "a\u00b7nitzt", "nicht", "auf\u00b7ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So hab' ich auch den lieben Tag bedacht", "tokens": ["So", "hab'", "ich", "auch", "den", "lie\u00b7ben", "Tag", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nicht eh' als itzt. Was soll ich dichten k\u00fcnnen,", "tokens": ["nicht", "eh'", "als", "itzt", ".", "Was", "soll", "ich", "dich\u00b7ten", "k\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KOKOM", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "weil Kastalis kein Z\u00e4rlein l\u00e4sset rinnen,", "tokens": ["weil", "Kas\u00b7ta\u00b7lis", "kein", "Z\u00e4r\u00b7lein", "l\u00e4s\u00b7set", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIAT", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "weil mir die Dunst noch tr\u00fcbe Sinnen macht?", "tokens": ["weil", "mir", "die", "Dunst", "noch", "tr\u00fc\u00b7be", "Sin\u00b7nen", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So sieht mich auch kein Plutus freundlich an,", "tokens": ["So", "sieht", "mich", "auch", "kein", "Plu\u00b7tus", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df ich dir Gold und Sch\u00e4tze schenken kan.", "tokens": ["da\u00df", "ich", "dir", "Gold", "und", "Sch\u00e4t\u00b7ze", "schen\u00b7ken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch halt', ich kan noch etwas bei mir finden,", "tokens": ["Doch", "halt'", ",", "ich", "kan", "noch", "et\u00b7was", "bei", "mir", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "das, wie ich wei\u00df, dich mehr als Gold ergetzt,", "tokens": ["das", ",", "wie", "ich", "wei\u00df", ",", "dich", "mehr", "als", "Gold", "er\u00b7getzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "VVFIN", "$,", "PPER", "ADV", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das mich in dich und dich in mich versetzt.", "tokens": ["das", "mich", "in", "dich", "und", "dich", "in", "mich", "ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "KON", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gieb her die Hand, di\u00df Herze soll sie binden!", "tokens": ["Gieb", "her", "die", "Hand", ",", "di\u00df", "Her\u00b7ze", "soll", "sie", "bin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "PDS", "VVFIN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}