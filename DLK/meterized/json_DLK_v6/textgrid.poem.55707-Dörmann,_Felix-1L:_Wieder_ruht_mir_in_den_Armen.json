{"textgrid.poem.55707": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder ruht mir in den Armen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder ruht mir in den Armen", "tokens": ["Wie\u00b7der", "ruht", "mir", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der geliebte braune K\u00f6rper", "tokens": ["Der", "ge\u00b7lieb\u00b7te", "brau\u00b7ne", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem dunklen Kranz der Haare", "tokens": ["Mit", "dem", "dunk\u00b7len", "Kranz", "der", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der hei\u00dfen, vollen Brust.", "tokens": ["Auf", "der", "hei\u00b7\u00dfen", ",", "vol\u00b7len", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wieder bett ich meine Lippen", "tokens": ["Wie\u00b7der", "bett", "ich", "mei\u00b7ne", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mein tr\u00e4nenfeuchtes Antlitz", "tokens": ["Und", "mein", "tr\u00e4\u00b7nen\u00b7feuch\u00b7tes", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In das warme, duftdurchstr\u00f6mte", "tokens": ["In", "das", "war\u00b7me", ",", "duft\u00b7durch\u00b7str\u00f6m\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebenslust-durchgl\u00fchte Fleisch.", "tokens": ["Le\u00b7bens\u00b7lust\u00b7durch\u00b7gl\u00fch\u00b7te", "Fleisch", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und die braunen, starken Arme", "tokens": ["Und", "die", "brau\u00b7nen", ",", "star\u00b7ken", "Ar\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pressen sich um meinen Nacken", "tokens": ["Pres\u00b7sen", "sich", "um", "mei\u00b7nen", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Lachen, unter Tr\u00e4nen,", "tokens": ["Un\u00b7ter", "La\u00b7chen", ",", "un\u00b7ter", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leidenschaftlich, wild und hei\u00df.", "tokens": ["Lei\u00b7den\u00b7schaft\u00b7lich", ",", "wild", "und", "hei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, Du bist der Born der Liebe,", "tokens": ["Ja", ",", "Du", "bist", "der", "Born", "der", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist der Anfang und das Ende,", "tokens": ["Bist", "der", "An\u00b7fang", "und", "das", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der m\u00fcde Wandrer wendet", "tokens": ["Und", "der", "m\u00fc\u00b7de", "Wand\u00b7rer", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der Fahrten tr\u00fcbem Irrsal,", "tokens": ["Nach", "der", "Fahr\u00b7ten", "tr\u00fc\u00b7bem", "Irr\u00b7sal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freudig wieder heim zu Dir.", "tokens": ["Freu\u00b7dig", "wie\u00b7der", "heim", "zu", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKVZ", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}