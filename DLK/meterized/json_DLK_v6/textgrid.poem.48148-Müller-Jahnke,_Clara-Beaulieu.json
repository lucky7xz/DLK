{"textgrid.poem.48148": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Beaulieu", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter Oliven und Pinien,", "tokens": ["Un\u00b7ter", "O\u00b7li\u00b7ven", "und", "Pi\u00b7ni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+----", "measure": "unknown.measure.tri"}, "line.2": {"text": "wie leuchtet das Meer so nah!", "tokens": ["wie", "leuch\u00b7tet", "das", "Meer", "so", "nah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In starken, reinen Linien", "tokens": ["In", "star\u00b7ken", ",", "rei\u00b7nen", "Li\u00b7ni\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "gr\u00fc\u00dft Cap Ferrat.", "tokens": ["gr\u00fc\u00dft", "Cap", "Fer\u00b7rat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die marmornen Villen funkeln", "tokens": ["Die", "mar\u00b7mor\u00b7nen", "Vil\u00b7len", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "rot auf im letzten Licht, \u2013", "tokens": ["rot", "auf", "im", "letz\u00b7ten", "Licht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "silberne Schatten dunkeln", "tokens": ["sil\u00b7ber\u00b7ne", "Schat\u00b7ten", "dun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u00fcber dein Gesicht.", "tokens": ["\u00fc\u00b7ber", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Unter Oliven und Pinien", "tokens": ["Un\u00b7ter", "O\u00b7li\u00b7ven", "und", "Pi\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+----", "measure": "unknown.measure.tri"}, "line.2": {"text": "f\u00fchle, was du mir bist!", "tokens": ["f\u00fch\u00b7le", ",", "was", "du", "mir", "bist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "PPER", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In gro\u00dfen, klaren Linien", "tokens": ["In", "gro\u00b7\u00dfen", ",", "kla\u00b7ren", "Li\u00b7ni\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "l\u00e4uft unsres Lebens Frist.", "tokens": ["l\u00e4uft", "uns\u00b7res", "Le\u00b7bens", "Frist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Rot auf lodern die Gluten", "tokens": ["Rot", "auf", "lo\u00b7dern", "die", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "uns einmal noch durchs Herz, \u2013", "tokens": ["uns", "ein\u00b7mal", "noch", "durchs", "Herz", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "silberne Schatten fluten", "tokens": ["sil\u00b7ber\u00b7ne", "Schat\u00b7ten", "flu\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "lautlos erdenw\u00e4rts.", "tokens": ["laut\u00b7los", "er\u00b7den\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}