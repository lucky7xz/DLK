{"textgrid.poem.43099": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du schaust mein Antlitz ohne Maske,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du schaust mein Antlitz ohne Maske,", "tokens": ["Du", "schaust", "mein", "Ant\u00b7litz", "oh\u00b7ne", "Mas\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch verl\u00e4ssest du mich nicht;", "tokens": ["Und", "doch", "ver\u00b7l\u00e4s\u00b7sest", "du", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So reizt dich noch das thr\u00e4nenblasse", "tokens": ["So", "reizt", "dich", "noch", "das", "thr\u00e4\u00b7nen\u00b7blas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verh\u00e4rmte Leidensangesicht?", "tokens": ["Ver\u00b7h\u00e4rm\u00b7te", "Lei\u00b7den\u00b7san\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du dr\u00e4ngst dich auch zu dem Gelage,", "tokens": ["Du", "dr\u00e4ngst", "dich", "auch", "zu", "dem", "Ge\u00b7la\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das meiner Wiederkehr geweiht;", "tokens": ["Das", "mei\u00b7ner", "Wie\u00b7der\u00b7kehr", "ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du h\u00f6rst es still und seltsam l\u00e4chelnd,", "tokens": ["Du", "h\u00f6rst", "es", "still", "und", "selt\u00b7sam", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Jeder meinen Namen schreit.", "tokens": ["Wie", "Je\u00b7der", "mei\u00b7nen", "Na\u00b7men", "schreit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du sprichst so ruhig mit den M\u00e4nnern,", "tokens": ["Du", "sprichst", "so", "ru\u00b7hig", "mit", "den", "M\u00e4n\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die frech mir Mund und Arme k\u00fcssen;", "tokens": ["Die", "frech", "mir", "Mund", "und", "Ar\u00b7me", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du stimmst mit ein in die Toaste,", "tokens": ["Du", "stimmst", "mit", "ein", "in", "die", "Toas\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Womit sie jauchzend mich begr\u00fc\u00dfen.", "tokens": ["Wo\u00b7mit", "sie", "jauch\u00b7zend", "mich", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was willst du mit den starren Blicken \u2013", "tokens": ["Was", "willst", "du", "mit", "den", "star\u00b7ren", "Bli\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist's Spott, der mir entgegenschaut?", "tokens": ["Ist's", "Spott", ",", "der", "mir", "ent\u00b7ge\u00b7gen\u00b7schaut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df ab von mir! Du bist ein Wesen,", "tokens": ["La\u00df", "ab", "von", "mir", "!", "Du", "bist", "ein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPER", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dem mir bangt, vor dem mir graut!", "tokens": ["Vor", "dem", "mir", "bangt", ",", "vor", "dem", "mir", "graut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}