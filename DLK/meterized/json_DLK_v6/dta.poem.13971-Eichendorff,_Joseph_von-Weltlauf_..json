{"dta.poem.13971": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Weltlauf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00dft's doch bl\u00fch'n und rauschen lassen,", "tokens": ["Mu\u00dft's", "doch", "bl\u00fch'n", "und", "rau\u00b7schen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "KON", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den todten L\u00f6wen fassen", "tokens": ["Und", "den", "tod\u00b7ten", "L\u00f6\u00b7wen", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Knaben nun nach Jungen-Art", "tokens": ["Kna\u00b7ben", "nun", "nach", "Jun\u00b7gen\u00b7Art"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ungestraft an M\u00e4hn' und Bart.", "tokens": ["Un\u00b7ge\u00b7straft", "an", "M\u00e4hn'", "und", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}