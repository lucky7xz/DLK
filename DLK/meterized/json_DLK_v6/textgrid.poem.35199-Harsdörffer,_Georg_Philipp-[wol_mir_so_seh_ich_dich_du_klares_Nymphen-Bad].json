{"textgrid.poem.35199": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[wol mir/ so seh ich dich/ du klares Nymphen-Bad]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol mir/ so seh ich dich/ du klares Nymphen-Bad/", "tokens": ["Wol", "mir", "/", "so", "seh", "ich", "dich", "/", "du", "kla\u00b7res", "Nym\u00b7phen\u00b7Bad", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "ADV", "VVFIN", "PPER", "PRF", "$(", "PPER", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du Haus der Najaden/ so seh ich wieder fliessen", "tokens": ["Du", "Haus", "der", "Na\u00b7ja\u00b7den", "/", "so", "seh", "ich", "wie\u00b7der", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dich Weltbeschreiten Strand/ von dem ich ward gerissen.", "tokens": ["Dich", "Welt\u00b7be\u00b7schrei\u00b7ten", "Strand", "/", "von", "dem", "ich", "ward", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "$(", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Himmel/ euch geb\u00fcrt der Dank f\u00fcr diese Gnad/", "tokens": ["Ihr", "Him\u00b7mel", "/", "euch", "ge\u00b7b\u00fcrt", "der", "Dank", "f\u00fcr", "die\u00b7se", "Gnad", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Dank/ da\u00df mich bi\u00dfher unschadhaft trug der Pfad/", "tokens": ["Der", "Dank", "/", "da\u00df", "mich", "bi\u00df\u00b7her", "un\u00b7schad\u00b7haft", "trug", "der", "Pfad", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Da\u00df euer Schutz mich wolt f\u00fcr Vnfall sicher wissen.", "tokens": ["Da\u00df", "eu\u00b7er", "Schutz", "mich", "wolt", "f\u00fcr", "Vn\u00b7fall", "si\u00b7cher", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VMFIN", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd du ber\u00fcmter Flu\u00df/ nimm dieses mein Begr\u00fcssen.", "tokens": ["Vnd", "du", "be\u00b7r\u00fcm\u00b7ter", "Flu\u00df", "/", "nimm", "die\u00b7ses", "mein", "Be\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$(", "VVIMP", "PDAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du bist es/ der mich erst so wol bewirtet hat/", "tokens": ["Du", "bist", "es", "/", "der", "mich", "erst", "so", "wol", "be\u00b7wir\u00b7tet", "hat", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "PRELS", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du bist es/ du allein/ der meiner hat gepflogen/", "tokens": ["Du", "bist", "es", "/", "du", "al\u00b7lein", "/", "der", "mei\u00b7ner", "hat", "ge\u00b7pflo\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "PPER", "ADV", "$(", "ART", "PPOSAT", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als meine Kindheit noch an Br\u00fcsten ward erzogen:", "tokens": ["Als", "mei\u00b7ne", "Kind\u00b7heit", "noch", "an", "Br\u00fcs\u00b7ten", "ward", "er\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wo aber bleibt die Pflicht/ mit der ich dir verpf\u00e4ndet?", "tokens": ["Wo", "a\u00b7ber", "bleibt", "die", "Pflicht", "/", "mit", "der", "ich", "dir", "ver\u00b7pf\u00e4n\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "$(", "APPR", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Min Leben lebt ja dir/ das ich dir schuldig bin.", "tokens": ["Min", "Le\u00b7ben", "lebt", "ja", "dir", "/", "das", "ich", "dir", "schul\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PPER", "$(", "PRELS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gieb/ was du hast Strephon mit Sch\u00e4ffeln zugewendet/", "tokens": ["Gieb", "/", "was", "du", "hast", "Stre\u00b7phon", "mit", "Sch\u00e4f\u00b7feln", "zu\u00b7ge\u00b7wen\u00b7det", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PWS", "PPER", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann nimm von meiner Hand viel tausend Lieder hin.", "tokens": ["Dann", "nimm", "von", "mei\u00b7ner", "Hand", "viel", "tau\u00b7send", "Lie\u00b7der", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "PPOSAT", "NN", "ADV", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr Felder dieses Orts/ ihr Kleebereichten Brachen/", "tokens": ["Ihr", "Fel\u00b7der", "die\u00b7ses", "Orts", "/", "ihr", "Klee\u00b7be\u00b7reich\u00b7ten", "Bra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Auen/ meine Lust/ wie hoch hat euch begl\u00fckkt/", "tokens": ["Ihr", "Au\u00b7en", "/", "mei\u00b7ne", "Lust", "/", "wie", "hoch", "hat", "euch", "be\u00b7gl\u00fckkt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PWAV", "ADJD", "VAFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Euch/ Strephons grosser Geist/ der bunte Reimen stikkt/", "tokens": ["Euch", "/", "Stre\u00b7phons", "gros\u00b7ser", "Geist", "/", "der", "bun\u00b7te", "Rei\u00b7men", "stikkt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NE", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der euch oft anvertraut/ voll Witzes Hirtensachen.", "tokens": ["Der", "euch", "oft", "an\u00b7ver\u00b7traut", "/", "voll", "Wit\u00b7zes", "Hir\u00b7ten\u00b7sa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$(", "ADJD", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du Linde/ kanst nun wohl in deinen Zweigen lachen/", "tokens": ["Du", "Lin\u00b7de", "/", "kanst", "nun", "wohl", "in", "dei\u00b7nen", "Zwei\u00b7gen", "la\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$(", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil Strephons Kunst-Geb\u00e4nd von deinen Rinden blikkt.", "tokens": ["Weil", "Stre\u00b7phons", "Kunst\u00b7Ge\u00b7b\u00e4nd", "von", "dei\u00b7nen", "Rin\u00b7den", "blikkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wird dein Lentzenzelt von lieber Luft erquikkt/", "tokens": ["Es", "wird", "dein", "Lent\u00b7zen\u00b7zelt", "von", "lie\u00b7ber", "Luft", "er\u00b7quikkt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch sprech ich/ da\u00df bey dir gelehrte Winde wachen.", "tokens": ["Noch", "sprech", "ich", "/", "da\u00df", "bey", "dir", "ge\u00b7lehr\u00b7te", "Win\u00b7de", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "APPR", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Gebt her/ ihr L\u00fcfte/ das/ womit Er euch begabt/", "tokens": ["Gebt", "her", "/", "ihr", "L\u00fcf\u00b7te", "/", "das", "/", "wo\u00b7mit", "Er", "euch", "be\u00b7gabt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$(", "PPOSAT", "NN", "$(", "ART", "$(", "PWAV", "PPER", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Schenkt/ was er euch geschenkt/ was ihr besitzt und habt/", "tokens": ["Schenkt", "/", "was", "er", "euch", "ge\u00b7schenkt", "/", "was", "ihr", "be\u00b7sitzt", "und", "habt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWS", "PPER", "PPER", "VVPP", "$(", "PWS", "PPER", "VVFIN", "KON", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So werd ich solches euch mit Wucher wieder senden.", "tokens": ["So", "werd", "ich", "sol\u00b7ches", "euch", "mit", "Wu\u00b7cher", "wie\u00b7der", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VMFIN", "PPER", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hinfort/ wann meinen Wunsch ein Schlu\u00df der Sternen h\u00e4lt/", "tokens": ["Hin\u00b7fort", "/", "wann", "mei\u00b7nen", "Wunsch", "ein", "Schlu\u00df", "der", "Ster\u00b7nen", "h\u00e4lt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Werd ich besuchen oft/ ihr Felder/ eure Welt/", "tokens": ["Werd", "ich", "be\u00b7su\u00b7chen", "oft", "/", "ihr", "Fel\u00b7der", "/", "eu\u00b7re", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ADV", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann lasst sich euren Schatz zu meinen Sinnen wenden.", "tokens": ["Dann", "lasst", "sich", "eu\u00b7ren", "Schatz", "zu", "mei\u00b7nen", "Sin\u00b7nen", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}