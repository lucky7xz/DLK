{"textgrid.poem.43774": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rt' ich nicht ein V\u00f6glein singen?", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt' ich nicht ein V\u00f6glein singen?", "tokens": ["H\u00f6rt'", "ich", "nicht", "ein", "V\u00f6\u00b7glein", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja, es sang zum letzten Mal,", "tokens": ["Ja", ",", "es", "sang", "zum", "letz\u00b7ten", "Mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollte seinen Dank nur bringen", "tokens": ["Woll\u00b7te", "sei\u00b7nen", "Dank", "nur", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den letzten Sonnenstrahl.", "tokens": ["F\u00fcr", "den", "letz\u00b7ten", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber keine Bl\u00e4tter rauschten,", "tokens": ["A\u00b7ber", "kei\u00b7ne", "Bl\u00e4t\u00b7ter", "rauschten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00e4um' und Str\u00e4uche waren kahl;", "tokens": ["B\u00e4um'", "und", "Str\u00e4u\u00b7che", "wa\u00b7ren", "kahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine lieben Blumen lauschten,", "tokens": ["Kei\u00b7ne", "lie\u00b7ben", "Blu\u00b7men", "lauschten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie starben allzumal.", "tokens": ["Denn", "sie", "star\u00b7ben", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch das V\u00f6glein wollte singen,", "tokens": ["Doch", "das", "V\u00f6\u00b7glein", "woll\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' es schied aus unserm Thal,", "tokens": ["Eh'", "es", "schied", "aus", "un\u00b7serm", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollte seinen Dank nur bringen", "tokens": ["Woll\u00b7te", "sei\u00b7nen", "Dank", "nur", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr den letzten Sonnenstrahl.", "tokens": ["F\u00fcr", "den", "letz\u00b7ten", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}