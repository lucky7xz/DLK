{"textgrid.poem.28267": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Achiver langem Weiber-Kriege", "tokens": ["In", "der", "A\u00b7chi\u00b7ver", "lan\u00b7gem", "Wei\u00b7ber\u00b7Krie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Halff letzlich noch ein h\u00f6ltznes Pferd zum Siege.", "tokens": ["Halff", "letz\u00b7lich", "noch", "ein", "h\u00f6ltz\u00b7nes", "Pferd", "zum", "Sie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was gilts, ob Krieg ietzt auch nicht wehren werde,", "tokens": ["Was", "gilts", ",", "ob", "Krieg", "ietzt", "auch", "nicht", "weh\u00b7ren", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NN", "ADV", "ADV", "PTKNEG", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bi\u00df sonst kein Pferd mehr bleib, als Kinder-Pferde?", "tokens": ["Bi\u00df", "sonst", "kein", "Pferd", "mehr", "bleib", ",", "als", "Kin\u00b7der\u00b7P\u00b7fer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "ADV", "VVFIN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}