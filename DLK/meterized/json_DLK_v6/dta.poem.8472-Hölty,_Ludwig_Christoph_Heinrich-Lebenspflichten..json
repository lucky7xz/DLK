{"dta.poem.8472": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lebenspflichten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und des Harms vergessen!", "tokens": ["Und", "des", "Harms", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Eine kurze Spanne Zeit", "tokens": ["Ei\u00b7ne", "kur\u00b7ze", "Span\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward uns zugemessen.", "tokens": ["Ward", "uns", "zu\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Heute h\u00fcpft im Fr\u00fchlingstanz", "tokens": ["Heu\u00b7te", "h\u00fcpft", "im", "Fr\u00fch\u00b7lings\u00b7tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch der frohe Knabe;", "tokens": ["Noch", "der", "fro\u00b7he", "Kna\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Morgen weht der Todtenkranz", "tokens": ["Mor\u00b7gen", "weht", "der", "Tod\u00b7ten\u00b7kranz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schon auf seinem Grabe.", "tokens": ["Schon", "auf", "sei\u00b7nem", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wonne f\u00fchrt die junge Braut", "tokens": ["Won\u00b7ne", "f\u00fchrt", "die", "jun\u00b7ge", "Braut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute zum Altare;", "tokens": ["Heu\u00b7te", "zum", "Al\u00b7ta\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eh die Abendwolke thaut,", "tokens": ["Eh", "die", "Ab\u00b7end\u00b7wol\u00b7ke", "thaut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruht sie auf der Bahre.", "tokens": ["Ruht", "sie", "auf", "der", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gebt denn Harm und Grillensang,", "tokens": ["Gebt", "denn", "Harm", "und", "Gril\u00b7len\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gebet ihn den Winden;", "tokens": ["Ge\u00b7bet", "ihn", "den", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Ruht bei hellem Becherklang", "tokens": ["Ruht", "bei", "hel\u00b7lem", "Be\u00b7cher\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unter gr\u00fcnen Linden.", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7nen", "Lin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Lasset keine Nachtigall", "tokens": ["Las\u00b7set", "kei\u00b7ne", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unbehorcht verstummen,", "tokens": ["Un\u00b7be\u00b7horcht", "ver\u00b7stum\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Keine Bien' im Fr\u00fchlingsthal", "tokens": ["Kei\u00b7ne", "Bi\u00b7en'", "im", "Fr\u00fch\u00b7lings\u00b7thal"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Unbelauscht entsummen.", "tokens": ["Un\u00b7be\u00b7lauscht", "ent\u00b7sum\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schmeckt, so lang' es Gott erlaubt,", "tokens": ["Schmeckt", ",", "so", "lang'", "es", "Gott", "er\u00b7laubt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADV", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kuss und s\u00fcsse Trauben,", "tokens": ["Kuss", "und", "s\u00fcs\u00b7se", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Bis der Tod, der alles raubt,", "tokens": ["Bis", "der", "Tod", ",", "der", "al\u00b7les", "raubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kommt, auch sie zu rauben.", "tokens": ["Kommt", ",", "auch", "sie", "zu", "rau\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Unserm schlummernden Gebein,", "tokens": ["Un\u00b7serm", "schlum\u00b7mern\u00b7den", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Tod' umd\u00fcstert,", "tokens": ["Von", "dem", "Tod'", "um\u00b7d\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Duftet nicht der Rosenhain,", "tokens": ["Duf\u00b7tet", "nicht", "der", "Ro\u00b7sen\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der am Grabe fl\u00fcstert,", "tokens": ["Der", "am", "Gra\u00b7be", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "T\u00f6net nicht der Wonneklang", "tokens": ["T\u00f6\u00b7net", "nicht", "der", "Won\u00b7ne\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Angestossner Becher,", "tokens": ["An\u00b7ge\u00b7stoss\u00b7ner", "Be\u00b7cher", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Noch der frohe Rundgesang", "tokens": ["Noch", "der", "fro\u00b7he", "Rund\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Weinbelaubter Zecher.", "tokens": ["Wein\u00b7be\u00b7laub\u00b7ter", "Ze\u00b7cher", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}