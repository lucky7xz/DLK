{"textgrid.poem.24768": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "Erlebnis", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit silbergrauem Dufte war das Tal", "tokens": ["Mit", "sil\u00b7ber\u00b7grau\u00b7em", "Duf\u00b7te", "war", "das", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der D\u00e4mmerung erf\u00fcllt, wie wenn der Mond", "tokens": ["Der", "D\u00e4m\u00b7me\u00b7rung", "er\u00b7f\u00fcllt", ",", "wie", "wenn", "der", "Mond"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch Wolken sickert. Doch es war nicht Nacht.", "tokens": ["Durch", "Wol\u00b7ken", "si\u00b7ckert", ".", "Doch", "es", "war", "nicht", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$.", "KON", "PPER", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit silbergrauem Duft des dunklen Tales", "tokens": ["Mit", "sil\u00b7ber\u00b7grau\u00b7em", "Duft", "des", "dunk\u00b7len", "Ta\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verschwammen meine d\u00e4mmernden Gedanken,", "tokens": ["Ver\u00b7schwam\u00b7men", "mei\u00b7ne", "d\u00e4m\u00b7mern\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und still versank ich in dem webenden,", "tokens": ["Und", "still", "ver\u00b7sank", "ich", "in", "dem", "we\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Durchsichtgen Meere und verlie\u00df das Leben.", "tokens": ["Durch\u00b7sicht\u00b7gen", "Mee\u00b7re", "und", "ver\u00b7lie\u00df", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wie wunderbare Blumen waren da", "tokens": ["Wie", "wun\u00b7der\u00b7ba\u00b7re", "Blu\u00b7men", "wa\u00b7ren", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Mit Kelchen dunkelgl\u00fchend! Pflanzendickicht,", "tokens": ["Mit", "Kel\u00b7chen", "dun\u00b7kel\u00b7gl\u00fc\u00b7hend", "!", "Pflan\u00b7zen\u00b7di\u00b7ckicht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$.", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Durch das ein gelbrot Licht wie von Topasen", "tokens": ["Durch", "das", "ein", "gel\u00b7brot", "Licht", "wie", "von", "To\u00b7pa\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "KOKOM", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "In warmen Str\u00f6men drang und glomm. Das Ganze", "tokens": ["In", "war\u00b7men", "Str\u00f6\u00b7men", "drang", "und", "glomm", ".", "Das", "Gan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "War angef\u00fcllt mit einem tiefen Schwellen", "tokens": ["War", "an\u00b7ge\u00b7f\u00fcllt", "mit", "ei\u00b7nem", "tie\u00b7fen", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Schwerm\u00fctiger Musik. Und dieses wu\u00dft ich,", "tokens": ["Schwer\u00b7m\u00fc\u00b7ti\u00b7ger", "Mu\u00b7sik", ".", "Und", "die\u00b7ses", "wu\u00dft", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Obgleich ich's nicht begreife, doch ich wu\u00dft es:", "tokens": ["Ob\u00b7gleich", "ich's", "nicht", "be\u00b7grei\u00b7fe", ",", "doch", "ich", "wu\u00dft", "es", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVFIN", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Das ist der Tod. Der ist Musik geworden,", "tokens": ["Das", "ist", "der", "Tod", ".", "Der", "ist", "Mu\u00b7sik", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$.", "PDS", "VAFIN", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Gewaltig sehnend, s\u00fc\u00df und dunkelgl\u00fchend,", "tokens": ["Ge\u00b7wal\u00b7tig", "seh\u00b7nend", ",", "s\u00fc\u00df", "und", "dun\u00b7kel\u00b7gl\u00fc\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Verwandt der tiefsten Schwermut.", "tokens": ["Ver\u00b7wandt", "der", "tiefs\u00b7ten", "Schwer\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Aber seltsam!", "tokens": ["A\u00b7ber", "selt\u00b7sam", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Ein namenloses Heimweh weinte lautlos", "tokens": ["Ein", "na\u00b7men\u00b7lo\u00b7ses", "Heim\u00b7weh", "wein\u00b7te", "laut\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "In meiner Seele nach dem Leben, weinte,", "tokens": ["In", "mei\u00b7ner", "See\u00b7le", "nach", "dem", "Le\u00b7ben", ",", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Wie einer weint, wenn er auf gro\u00dfem Seeschiff", "tokens": ["Wie", "ei\u00b7ner", "weint", ",", "wenn", "er", "auf", "gro\u00b7\u00dfem", "See\u00b7schiff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Mit gelben Riesensegeln gegen Abend", "tokens": ["Mit", "gel\u00b7ben", "Rie\u00b7sen\u00b7se\u00b7geln", "ge\u00b7gen", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Auf dunkelblauem Wasser an der Stadt,", "tokens": ["Auf", "dun\u00b7kel\u00b7blau\u00b7em", "Was\u00b7ser", "an", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der Vaterstadt, vor\u00fcberf\u00e4hrt. Da sieht er", "tokens": ["Der", "Va\u00b7ter\u00b7stadt", ",", "vor\u00b7\u00fc\u00b7berf\u00b7\u00e4hrt", ".", "Da", "sieht", "er"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die Gassen, h\u00f6rt die Brunnen rauschen, riecht", "tokens": ["Die", "Gas\u00b7sen", ",", "h\u00f6rt", "die", "Brun\u00b7nen", "rau\u00b7schen", ",", "riecht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Den Duft der Fliederb\u00fcsche, sieht sich selber,", "tokens": ["Den", "Duft", "der", "Flie\u00b7der\u00b7b\u00fc\u00b7sche", ",", "sieht", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Ein Kind, am Ufer stehn, mit Kindesaugen,", "tokens": ["Ein", "Kind", ",", "am", "U\u00b7fer", "stehn", ",", "mit", "Kin\u00b7des\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVINF", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Die \u00e4ngstlich sind und weinen wollen, sieht", "tokens": ["Die", "\u00e4ngst\u00b7lich", "sind", "und", "wei\u00b7nen", "wol\u00b7len", ",", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "VAFIN", "KON", "VVINF", "VMFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Durchs offne Fenster Licht in seinem Zimmer \u2013", "tokens": ["Durchs", "off\u00b7ne", "Fens\u00b7ter", "Licht", "in", "sei\u00b7nem", "Zim\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Das gro\u00dfe Seeschiff aber tr\u00e4gt ihn weiter", "tokens": ["Das", "gro\u00b7\u00dfe", "See\u00b7schiff", "a\u00b7ber", "tr\u00e4gt", "ihn", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Auf dunkelblauem Wasser lautlos gleitend", "tokens": ["Auf", "dun\u00b7kel\u00b7blau\u00b7em", "Was\u00b7ser", "laut\u00b7los", "glei\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Mit gelben fremdgeformten Riesensegeln.", "tokens": ["Mit", "gel\u00b7ben", "fremd\u00b7ge\u00b7form\u00b7ten", "Rie\u00b7sen\u00b7se\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}