{"dta.poem.20656": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Warnung  \n an den jungen Herrn von H*st.  \n 1764 .  \n Als derselbe der Mahlerey den Vorzug vor der Dichtkunst  \n ertheilte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Du den Pinsel des Apelles                 ", "tokens": ["Der", "Du", "den", "Pin\u00b7sel", "des", "A\u00b7pel\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NE"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Viel eher w\u00fcnschest als den Trank des Musenquelles", "tokens": ["Viel", "e\u00b7her", "w\u00fcn\u00b7schest", "als", "den", "Trank", "des", "Mu\u00b7sen\u00b7quel\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Pindars Saitenspiel;", "tokens": ["Und", "Pin\u00b7dars", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sey doch ein Musenfreund, und h\u00f6re,", "tokens": ["Sey", "doch", "ein", "Mu\u00b7sen\u00b7freund", ",", "und", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem feineren Geschmack zur Ehre,", "tokens": ["Dem", "fei\u00b7ne\u00b7ren", "Ge\u00b7schmack", "zur", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Lieder mit Gef\u00fchl.", "tokens": ["Die", "Lie\u00b7der", "mit", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bists \u2014 Getreulich unterwiesen", "tokens": ["Du", "bists", "Ge\u00b7treu\u00b7lich", "un\u00b7ter\u00b7wie\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$(", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Von Deinem Mentor ", "tokens": ["Von", "Dei\u00b7nem", "Men\u00b7tor"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Riesen", "tokens": ["Rie\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Und lachst der Zwerge schon;", "tokens": ["Und", "lachst", "der", "Zwer\u00b7ge", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sprichst von jeglichem Gedichte", "tokens": ["Und", "sprichst", "von", "jeg\u00b7li\u00b7chem", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit viel bedeutendem Gesichte", "tokens": ["Mit", "viel", "be\u00b7deu\u00b7ten\u00b7dem", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Im richterlichen Ton.", "tokens": ["Im", "rich\u00b7ter\u00b7li\u00b7chen", "Ton", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich lobe Dich, Du Freund der Lieder,", "tokens": ["Ich", "lo\u00b7be", "Dich", ",", "Du", "Freund", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch bleib auf Deiner Hut, und s\u00fcndige nicht wider", "tokens": ["Doch", "bleib", "auf", "Dei\u00b7ner", "Hut", ",", "und", "s\u00fcn\u00b7di\u00b7ge", "nicht", "wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "ADJA", "PTKNEG", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den richtigen Geschmack,", "tokens": ["Den", "rich\u00b7ti\u00b7gen", "Ge\u00b7schmack", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sonst wird Dein Eberhard ergrimmen", "tokens": ["Sonst", "wird", "Dein", "E\u00b7ber\u00b7hard", "er\u00b7grim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Auf einen ganzen Tag.", "tokens": ["Auf", "ei\u00b7nen", "gan\u00b7zen", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}