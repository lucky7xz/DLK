{"dta.poem.19266": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "113.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch keine Aufgab' hat die Baumfrucht, als zu reifen;", "tokens": ["Doch", "kei\u00b7ne", "Auf\u00b7gab'", "hat", "die", "Baum\u00b7frucht", ",", "als", "zu", "rei\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "$,", "KOUS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Lust wird sie dazu die Sommerglut ergreifen.", "tokens": ["Mit", "Lust", "wird", "sie", "da\u00b7zu", "die", "Som\u00b7mer\u00b7glut", "er\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wann die Todesglut sie nun in sich gesogen,", "tokens": ["Doch", "wann", "die", "To\u00b7des\u00b7glut", "sie", "nun", "in", "sich", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADV", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fallen soll vom Zweig, der sie solang gepflogen;", "tokens": ["Und", "fal\u00b7len", "soll", "vom", "Zweig", ",", "der", "sie", "so\u00b7lang", "ge\u00b7pflo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mit neuer Lebenslust will sie den Zweig umfangen,", "tokens": ["Mit", "neu\u00b7er", "Le\u00b7bens\u00b7lust", "will", "sie", "den", "Zweig", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Zu sp\u00e4t! was reif ist, f\u00e4llt, unreifes nur bleibt hangen.", "tokens": ["Zu", "sp\u00e4t", "!", "was", "reif", "ist", ",", "f\u00e4llt", ",", "un\u00b7rei\u00b7fes", "nur", "bleibt", "han\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$.", "PWS", "ADJD", "VAFIN", "$,", "VVFIN", "$,", "ADJA", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}