{"dta.poem.13322": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer nun ergriffe mich Thalia bey der Hand", "tokens": ["Hjer", "nun", "er\u00b7grif\u00b7fe", "mich", "Tha\u00b7lia", "bey", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PPER", "NE", "APPR", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und sprach: \u201eGetreuer Freund! jezt findst du dich im Stand", "tokens": ["Und", "sprach", ":", "\u201e", "Ge\u00b7treu\u00b7er", "Freund", "!", "jezt", "findst", "du", "dich", "im", "Stand"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edas, was du gestern schon begunntest, zu vollziehen;", "tokens": ["\u201e", "das", ",", "was", "du", "ge\u00b7stern", "schon", "be\u00b7gunn\u00b7test", ",", "zu", "voll\u00b7zie\u00b7hen", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PDS", "$,", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "460\u201dStimm deinen Sayten-Chor und sing! spahr kein Bem\u00fchen!", "tokens": ["\"", "Stimm", "dei\u00b7nen", "Say\u00b7ten\u00b7Chor", "und", "sing", "!", "spahr", "kein", "Be\u00b7m\u00fc\u00b7hen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PPOSAT", "NN", "KON", "VVFIN", "$.", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ehab nur zum Fingerzeig der Wahrheit Zuversicht,", "tokens": ["\u201e", "hab", "nur", "zum", "Fin\u00b7ger\u00b7zeig", "der", "Wahr\u00b7heit", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eund sorg dich um der Welt Lob oder Tadel nicht.", "tokens": ["\u201e", "und", "sorg", "dich", "um", "der", "Welt", "Lob", "o\u00b7der", "Ta\u00b7del", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201edie Fr\u00f6sche lassen sich im hell- und tr\u00fcben h\u00f6ren:", "tokens": ["\u201e", "die", "Fr\u00f6\u00b7sche", "las\u00b7sen", "sich", "im", "hell", "und", "tr\u00fc\u00b7ben", "h\u00f6\u00b7ren", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PRF", "APPRART", "TRUNC", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201esie werden den Gesang, den du beginnst, nicht st\u00f6ren.", "tokens": ["\u201e", "sie", "wer\u00b7den", "den", "Ge\u00b7sang", ",", "den", "du", "be\u00b7ginnst", ",", "nicht", "st\u00f6\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", 