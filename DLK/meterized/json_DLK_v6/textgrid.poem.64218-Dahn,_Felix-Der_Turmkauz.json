{"textgrid.poem.64218": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Turmkauz", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnee h\u00fcllt das Land. \u2013 Grundtief f\u00fcllt Eis den Main. \u2013", "tokens": ["Schnee", "h\u00fcllt", "das", "Land", ".", "\u2013", "Grund\u00b7tief", "f\u00fcllt", "Eis", "den", "Main", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "$(", "NN", "VVFIN", "NN", "ART", "NN", "$.", "$("], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Durch kalte Nachtluft leuchtet, \u2013 sonder Ende \u2013", "tokens": ["Durch", "kal\u00b7te", "Nacht\u00b7luft", "leuch\u00b7tet", ",", "\u2013", "son\u00b7der", "En\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "$(", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In h\u00f6h'rem Glanz, als sonst der Sterne Schein: \u2013 \u2013", "tokens": ["In", "h\u00f6h'\u00b7rem", "Glanz", ",", "als", "sonst", "der", "Ster\u00b7ne", "Schein", ":", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ADV", "ART", "NN", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist die Nacht der Jahreswende.", "tokens": ["Das", "ist", "die", "Nacht", "der", "Jah\u00b7res\u00b7wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geh', M\u00fcnsterturmwart, ruhe diese Nacht!", "tokens": ["Geh'", ",", "M\u00fcns\u00b7ter\u00b7turm\u00b7wart", ",", "ru\u00b7he", "die\u00b7se", "Nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dich l\u00f6s' ich ab in deiner luft'gen Zelle:", "tokens": ["Dich", "l\u00f6s'", "ich", "ab", "in", "dei\u00b7ner", "luft'\u00b7gen", "Zel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Selb zweit mit meiner Harfe halt' ich Wacht,", "tokens": ["Selb", "zweit", "mit", "mei\u00b7ner", "Har\u00b7fe", "halt'", "ich", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis da\u00df mich gr\u00fc\u00dft die Morgenhelle.", "tokens": ["Bis", "da\u00df", "mich", "gr\u00fc\u00dft", "die", "Mor\u00b7gen\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dorthin den Weinkrug und die Ampel: hier", "tokens": ["Dor\u00b7thin", "den", "Wein\u00b7krug", "und", "die", "Am\u00b7pel", ":", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$.", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Speer und deine lange Turmdrommete:", "tokens": ["Den", "Speer", "und", "dei\u00b7ne", "lan\u00b7ge", "Turm\u00b7drom\u00b7me\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geh nur und schlaf: ich halte Wache dir", "tokens": ["Geh", "nur", "und", "schlaf", ":", "ich", "hal\u00b7te", "Wa\u00b7che", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "KON", "VVFIN", "$.", "PPER", "VVFIN", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Sang und Sinnen und Gebete. \u2013 \u2013", "tokens": ["Mit", "Sang", "und", "Sin\u00b7nen", "und", "Ge\u00b7be\u00b7te", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Rings ruht die Stadt. \u2013 Nur auf der Burg glimmt rot", "tokens": ["Rings", "ruht", "die", "Stadt", ".", "\u2013", "Nur", "auf", "der", "Burg", "glimmt", "rot"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Gauwarts Licht. \u2013 Rings K\u00e4lte, Nacht und Schweigen, \u2013:", "tokens": ["Des", "Gau\u00b7warts", "Licht", ".", "\u2013", "Rings", "K\u00e4l\u00b7te", ",", "Nacht", "und", "Schwei\u00b7gen", ",", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "NN", "$.", "$(", "NN", "NN", "$,", "NN", "KON", "NN", "$,", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie anders einst zu Rom uns Neujahr bot", "tokens": ["Wie", "an\u00b7ders", "einst", "zu", "Rom", "uns", "Neu\u00b7jahr", "bot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "APPR", "NE", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Volk mit Tanz und Fl\u00f6tenreigen.", "tokens": ["Das", "Volk", "mit", "Tanz", "und", "Fl\u00f6\u00b7ten\u00b7rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Lau ist die Nacht dort, wie bei uns im Mai!", "tokens": ["Lau", "ist", "die", "Nacht", "dort", ",", "wie", "bei", "uns", "im", "Mai", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADV", "$,", "PWAV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wie glatt die Lispler Gru\u00df und Handschlag f\u00e4lschen:", "tokens": ["Wie", "glatt", "die", "Lisp\u00b7ler", "Gru\u00df", "und", "Hand\u00b7schlag", "f\u00e4l\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber uns die Wut der Welschen!", "tokens": ["Und", "\u00fc\u00b7ber", "uns", "die", "Wut", "der", "Wel\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das war das r\u00f6m'sche Neujahr! \u2013 Heimatland: \u2013", "tokens": ["Das", "war", "das", "r\u00f6m'\u00b7sche", "Neu\u00b7jahr", "!", "\u2013", "Hei\u00b7mat\u00b7land", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da lob' ich dich, trotz Eis und Frost! \u2013 \u2013 Was \u00e4chzet", "tokens": ["Da", "lob'", "ich", "dich", ",", "trotz", "Eis", "und", "Frost", "!", "\u2013", "\u2013", "Was", "\u00e4ch\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "APPR", "NN", "KON", "NN", "$.", "$(", "$(", "PWS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor'm Fenster dort? \u2013 Der Turmkauz! \u2013 \u00dcbler Fant!", "tokens": ["Vor'm", "Fens\u00b7ter", "dort", "?", "\u2013", "Der", "Turm\u00b7kauz", "!", "\u2013", "\u00dcb\u00b7ler", "Fant", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$.", "$(", "ART", "NN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er k\u00fcndet Unheil, wo er kr\u00e4chzet.", "tokens": ["Er", "k\u00fcn\u00b7det", "Un\u00b7heil", ",", "wo", "er", "kr\u00e4ch\u00b7zet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbwas wachst du, Mann,", "tokens": ["\u00bb", "was", "wachst", "du", ",", "Mann", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den Tag heran,", "tokens": ["Den", "Tag", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Tag vom neuen Jahre?", "tokens": ["Den", "Tag", "vom", "neu\u00b7en", "Jah\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unheil verrann,", "tokens": ["Un\u00b7heil", "ver\u00b7rann", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Unheil hebt an", "tokens": ["Un\u00b7heil", "hebt", "an"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Von Wiege bis zur Bahre.", "tokens": ["Von", "Wie\u00b7ge", "bis", "zur", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Lieb' ist Lust!", "tokens": ["Die", "Lieb'", "ist", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Treu keine Brust:", "tokens": ["Treu", "kei\u00b7ne", "Brust", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es glei\u00dft die Welt in L\u00fcgen:", "tokens": ["Es", "glei\u00dft", "die", "Welt", "in", "L\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Freund liebt sich:", "tokens": ["Der", "Freund", "liebt", "sich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Er liebt nicht dich:", "tokens": ["Er", "liebt", "nicht", "dich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "La\u00df dich den Schein nicht tr\u00fcgen.", "tokens": ["La\u00df", "dich", "den", "Schein", "nicht", "tr\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das Reich zerrinnt,", "tokens": ["Das", "Reich", "zer\u00b7rinnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und Rom gewinnt,", "tokens": ["Und", "Rom", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Kaiser beugt den Scheitel:", "tokens": ["Der", "Kai\u00b7ser", "beugt", "den", "Schei\u00b7tel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt ist schal:", "tokens": ["Die", "Welt", "ist", "schal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ja, sie ist Qual:", "tokens": ["Ja", ",", "sie", "ist", "Qual", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Reich, Lieb' und Sang sind eitel.\u00ab", "tokens": ["Reich", ",", "Lieb'", "und", "Sang", "sind", "ei\u00b7tel", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Husch, h\u00f6llisch Nachtgekr\u00e4chz, entweich' hiedann!", "tokens": ["Husch", ",", "h\u00f6l\u00b7lisch", "Nacht\u00b7ge\u00b7kr\u00e4chz", ",", "ent\u00b7weich'", "hie\u00b7dann", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sonst, Unhold, schlag' ich nach dir mit dem Speere \u2013 \u2013", "tokens": ["Sonst", ",", "Un\u00b7hold", ",", "schlag'", "ich", "nach", "dir", "mit", "dem", "Spee\u00b7re", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "$(", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ha sieh: Es tagt! Es tagt! die Nacht verrann,", "tokens": ["Ha", "sieh", ":", "Es", "tagt", "!", "Es", "tagt", "!", "die", "Nacht", "ver\u00b7rann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne steigt! Dem Herrn die Ehre!", "tokens": ["Die", "Son\u00b7ne", "steigt", "!", "Dem", "Herrn", "die", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Falsch war der Unkenruf! Es siegt das Licht:", "tokens": ["Falsch", "war", "der", "Un\u00b7ken\u00b7ruf", "!", "Es", "siegt", "das", "Licht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht eitel sind Lieb', Sang und deutsche Krone:", "tokens": ["Nicht", "ei\u00b7tel", "sind", "Lieb'", ",", "Sang", "und", "deut\u00b7sche", "Kro\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den echten Mann reut seiner Schmerzen nicht:", "tokens": ["Den", "ech\u00b7ten", "Mann", "reut", "sei\u00b7ner", "Schmer\u00b7zen", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er tr\u00e4gt tief in sich, was ihm lohne.", "tokens": ["Er", "tr\u00e4gt", "tief", "in", "sich", ",", "was", "ihm", "loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PRF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Das Fenster auf! \u2013 Komm, Wachtdrommete mein:", "tokens": ["Das", "Fens\u00b7ter", "auf", "!", "\u2013", "Komm", ",", "Wacht\u00b7drom\u00b7me\u00b7te", "mein", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["AR