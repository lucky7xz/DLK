{"textgrid.poem.60471": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Kirchhof dort bin ich gestanden,", "tokens": ["Am", "Kirch\u00b7hof", "dort", "bin", "ich", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo unten still das R\u00e4tsel modert", "tokens": ["Wo", "un\u00b7ten", "still", "das", "R\u00e4t\u00b7sel", "mo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf den Grabesrosen lodert;", "tokens": ["Und", "auf", "den", "Gra\u00b7bes\u00b7ro\u00b7sen", "lo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es bl\u00fcht die Welt in Todesbanden.", "tokens": ["Es", "bl\u00fcht", "die", "Welt", "in", "To\u00b7des\u00b7ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort l\u00e4chelt auf die Gr\u00e4ber nieder", "tokens": ["Dort", "l\u00e4\u00b7chelt", "auf", "die", "Gr\u00e4\u00b7ber", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit himmlisch duldender Geb\u00e4rde", "tokens": ["Mit", "himm\u00b7lisch", "dul\u00b7den\u00b7der", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Kreuz das h\u00f6chste Bild der Erde;", "tokens": ["Vom", "Kreuz", "das", "h\u00f6chs\u00b7te", "Bild", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vogel drauf, sang seine Lieder.", "tokens": ["Ein", "Vo\u00b7gel", "drauf", ",", "sang", "sei\u00b7ne", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch kaum da\u00df sie geklungen hatten,", "tokens": ["Doch", "kaum", "da\u00df", "sie", "ge\u00b7klun\u00b7gen", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flog scheu zum Wald zur\u00fcck der Wilde;", "tokens": ["Flog", "scheu", "zum", "Wald", "zu\u00b7r\u00fcck", "der", "Wil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sang, wie er, ein Lied dem Bilde", "tokens": ["Ich", "sang", ",", "wie", "er", ",", "ein", "Lied", "dem", "Bil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kehrte heim in meine Schatten.", "tokens": ["Und", "kehr\u00b7te", "heim", "in", "mei\u00b7ne", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Natur! will dir ans Herz mich legen!", "tokens": ["Na\u00b7tur", "!", "will", "dir", "ans", "Herz", "mich", "le\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verzeih, da\u00df ich dich konnte meiden,", "tokens": ["Ver\u00b7zeih", ",", "da\u00df", "ich", "dich", "konn\u00b7te", "mei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Heilung ich gesucht f\u00fcr Leiden,", "tokens": ["Da\u00df", "Hei\u00b7lung", "ich", "ge\u00b7sucht", "f\u00fcr", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die du mir gabst zum herben Segen.", "tokens": ["Die", "du", "mir", "gabst", "zum", "her\u00b7ben", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "In deinen Waldesfinsternissen", "tokens": ["In", "dei\u00b7nen", "Wal\u00b7des\u00b7fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab ich von mancher tiefen Ritze,", "tokens": ["Hab", "ich", "von", "man\u00b7cher", "tie\u00b7fen", "Rit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch die mir leuchten deine Blitze,", "tokens": ["Durch", "die", "mir", "leuch\u00b7ten", "dei\u00b7ne", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den tr\u00fcglichen Verband gerissen.", "tokens": ["Den", "tr\u00fcg\u00b7li\u00b7chen", "Ver\u00b7band", "ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}