{"dta.poem.12055": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst am Himmel da\u00df nichts zu bewundern bliebe", "tokens": ["Du", "sagst", "am", "Him\u00b7mel", "da\u00df", "nichts", "zu", "be\u00b7wun\u00b7dern", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOUS", "PIS", "PTKZU", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Astronomen, der erkannt sein Radgetriebe.", "tokens": ["Dem", "Ast\u00b7ro\u00b7no\u00b7men", ",", "der", "er\u00b7kannt", "sein", "Rad\u00b7ge\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich sage dir, was doch noch zu bewundern bleibt:", "tokens": ["Ich", "sa\u00b7ge", "dir", ",", "was", "doch", "noch", "zu", "be\u00b7wun\u00b7dern", "bleibt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ew'ge Grundkraft selbst, die dieses Radwerk treibt.", "tokens": ["Die", "ew'\u00b7ge", "Grund\u00b7kraft", "selbst", ",", "die", "die\u00b7ses", "Rad\u00b7werk", "treibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du hast das Leben nicht in Zahl und in Figur,", "tokens": ["Du", "hast", "das", "Le\u00b7ben", "nicht", "in", "Zahl", "und", "in", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Figur und Zahl hast du erkannt am Leben nur.", "tokens": ["Fi\u00b7gur", "und", "Zahl", "hast", "du", "er\u00b7kannt", "am", "Le\u00b7ben", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "VVPP", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}