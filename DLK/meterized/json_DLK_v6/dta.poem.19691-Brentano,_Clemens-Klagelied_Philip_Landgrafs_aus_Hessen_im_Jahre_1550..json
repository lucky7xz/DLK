{"dta.poem.19691": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Klagelied Philip Landgrafs aus Hessen  \n im Jahre 1550.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwer, langweilig ist mir mein Zeit,               ", "tokens": ["Schwer", ",", "lang\u00b7wei\u00b7lig", "ist", "mir", "mein", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Mein Herz mich treibt zu Klagen,", "tokens": ["Mein", "Herz", "mich", "treibt", "zu", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel Untreu, Mi\u00dfgunst, Ha\u00df und Neid,", "tokens": ["Viel", "Un\u00b7treu", ",", "Mi\u00df\u00b7gunst", ",", "Ha\u00df", "und", "Neid", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach ich jezund mu\u00df tragen,", "tokens": ["Ach", "ich", "je\u00b7zund", "mu\u00df", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Viel falscher List zu dieser Frist", "tokens": ["Viel", "fal\u00b7scher", "List", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird mir zu lang mit Schmerzen,", "tokens": ["Wird", "mir", "zu", "lang", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich oft klag", "tokens": ["Da\u00df", "ich", "oft", "klag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "All Nacht und Tag,", "tokens": ["All", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Doch denk ich Gotts im Herzen.", "tokens": ["Doch", "denk", "ich", "Gotts", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}