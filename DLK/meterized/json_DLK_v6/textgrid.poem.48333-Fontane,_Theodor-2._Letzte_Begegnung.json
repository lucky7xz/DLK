{"textgrid.poem.48333": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2. Letzte Begegnung", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Oskar, vom M\u00e4lar kommt er daher,", "tokens": ["K\u00f6\u00b7nig", "Os\u00b7kar", ",", "vom", "M\u00e4\u00b7lar", "kommt", "er", "da\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPRART", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "F\u00e4hrt \u00fcber den Sund, f\u00e4hrt \u00fcber das Meer,", "tokens": ["F\u00e4hrt", "\u00fc\u00b7ber", "den", "Sund", ",", "f\u00e4hrt", "\u00fc\u00b7ber", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun sieht er die K\u00fcste: Deutsches Land,", "tokens": ["Nun", "sieht", "er", "die", "K\u00fcs\u00b7te", ":", "Deut\u00b7sches", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Heide, Kiefer, m\u00e4rkischen Sand,", "tokens": ["Hei\u00b7de", ",", "Kie\u00b7fer", ",", "m\u00e4r\u00b7ki\u00b7schen", "Sand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und nun Avenuen und Schlo\u00df und Alleen \u2013", "tokens": ["Und", "nun", "A\u00b7ve\u00b7nu\u00b7en", "und", "Schlo\u00df", "und", "Al\u00b7leen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "KON", "NN", "$("], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Er kommt, um den sterbenden Kaiser zu sehn.", "tokens": ["Er", "kommt", ",", "um", "den", "ster\u00b7ben\u00b7den", "Kai\u00b7ser", "zu", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Dem melden sie's. \u00bbK\u00f6nig Oskar ist da.\u00ab", "tokens": ["Dem", "mel\u00b7den", "sie'", "s.", "\u00bb", "K\u00f6\u00b7nig", "Os\u00b7kar", "ist", "da", ".", "\u00ab"], "token_info": ["word", "word", "word", "abbreviation", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "NN", "NE", "VAFIN", "ADV", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Kaiser Friedrich wie suchend um sich sah,", "tokens": ["Kai\u00b7ser", "Fried\u00b7rich", "wie", "su\u00b7chend", "um", "sich", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KOKOM", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein leuchtendes Bildnis h\u00e4ngt an der Wand,", "tokens": ["Ein", "leuch\u00b7ten\u00b7des", "Bild\u00b7nis", "h\u00e4ngt", "an", "der", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Bildnis von Angelis Meisterhand,", "tokens": ["Sein", "Bild\u00b7nis", "von", "An\u00b7ge\u00b7lis", "Meis\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Orangeband, Orden, Helmbuschzier,", "tokens": ["O\u00b7ran\u00b7ge\u00b7band", ",", "Or\u00b7den", ",", "Helm\u00b7buschzier", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Pasewalker K\u00fcrassier,", "tokens": ["Pa\u00b7se\u00b7wal\u00b7ker", "K\u00fc\u00b7ras\u00b7sier", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Er blickt drauf hin, und den Blick sie verstehn:", "tokens": ["Er", "blickt", "drauf", "hin", ",", "und", "den", "Blick", "sie", "ver\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "$,", "KON", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.3": {"line.1": {"text": "Und sie legen ihm Koller und K\u00fcra\u00df an,", "tokens": ["Und", "sie", "le\u00b7gen", "ihm", "Kol\u00b7ler", "und", "K\u00fc\u00b7ra\u00df", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Aufrecht noch einmal der sterbende Mann,", "tokens": ["Auf\u00b7recht", "noch", "ein\u00b7mal", "der", "ster\u00b7ben\u00b7de", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Aufrecht und hager und todesfahl \u2013", "tokens": ["Auf\u00b7recht", "und", "ha\u00b7ger", "und", "to\u00b7des\u00b7fahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "K\u00f6nig Oskar tritt in den Marmorsaal,", "tokens": ["K\u00f6\u00b7nig", "Os\u00b7kar", "tritt", "in", "den", "Mar\u00b7mor\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Sprechen will er, er kann es nicht,", "tokens": ["Spre\u00b7chen", "will", "er", ",", "er", "kann", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Ein Tr\u00e4nenstrom seinem Aug' entbricht.", "tokens": ["Ein", "Tr\u00e4\u00b7nen\u00b7strom", "sei\u00b7nem", "Aug'", "ent\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da steht sein Freund in des Jammers Joch,", "tokens": ["Da", "steht", "sein", "Freund", "in", "des", "Jam\u00b7mers", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gebrochen und doch ein Kaiser noch:", "tokens": ["Ge\u00b7bro\u00b7chen", "und", "doch", "ein", "Kai\u00b7ser", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Den Pallasch zur Seite, den Helm in Hand,", "tokens": ["Den", "Pal\u00b7lasch", "zur", "Sei\u00b7te", ",", "den", "Helm", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Kaiser Friedrich vor K\u00f6nig Oskar stand.", "tokens": ["Kai\u00b7ser", "Fried\u00b7rich", "vor", "K\u00f6\u00b7nig", "Os\u00b7kar", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NN", "NE", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "\u00bbbild einst von Gr\u00f6\u00dfe, Sch\u00f6nheit, Gl\u00fcck,", "tokens": ["\u00bb", "bild", "einst", "von", "Gr\u00f6\u00b7\u00dfe", ",", "Sch\u00f6n\u00b7heit", ",", "Gl\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stumm neigt sich der K\u00f6nig, und noch einmal,", "tokens": ["Stumm", "neigt", "sich", "der", "K\u00f6\u00b7nig", ",", "und", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "ADV", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und nun zum dritten und \u2013 l\u00e4\u00dft den Saal.", "tokens": ["Und", "nun", "zum", "drit\u00b7ten", "und", "\u2013", "l\u00e4\u00dft", "den", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "KON", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}