{"textgrid.poem.51090": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Flamm ist tot, der Krater ist vergl\u00fcht,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flamm ist tot, der Krater ist vergl\u00fcht,", "tokens": ["Die", "Flamm", "ist", "tot", ",", "der", "Kra\u00b7ter", "ist", "ver\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Himmelsrose dr\u00fcber aufgebl\u00fcht;", "tokens": ["Die", "Him\u00b7mels\u00b7ro\u00b7se", "dr\u00fc\u00b7ber", "auf\u00b7ge\u00b7bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie gl\u00e4nzt auf Asche, wo die Wohnung stand,", "tokens": ["Sie", "gl\u00e4nzt", "auf", "A\u00b7sche", ",", "wo", "die", "Woh\u00b7nung", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verschwunden ist das morsche Werk der Hand.", "tokens": ["Ver\u00b7schwun\u00b7den", "ist", "das", "mor\u00b7sche", "Werk", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Woran der Mensch ruhlos die H\u00e4nde legt", "tokens": ["Wo\u00b7ran", "der", "Mensch", "ruh\u00b7los", "die", "H\u00e4n\u00b7de", "legt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und was er diebisch scheu zusammentr\u00e4gt:", "tokens": ["Und", "was", "er", "die\u00b7bisch", "scheu", "zu\u00b7sam\u00b7men\u00b7tr\u00e4gt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hin ist nun alles, was nach Richt und Ma\u00df", "tokens": ["Hin", "ist", "nun", "al\u00b7les", ",", "was", "nach", "Richt", "und", "Ma\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PIS", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gef\u00fcgt, gebunden aufeinander sa\u00df.", "tokens": ["Ge\u00b7f\u00fcgt", ",", "ge\u00b7bun\u00b7den", "auf\u00b7ein\u00b7an\u00b7der", "sa\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch ihr ergl\u00e4nzet mir unwandelbar,", "tokens": ["Doch", "ihr", "er\u00b7gl\u00e4n\u00b7zet", "mir", "un\u00b7wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Morgenlande, wonniglich und klar!", "tokens": ["Ihr", "Mor\u00b7gen\u00b7lan\u00b7de", ",", "won\u00b7nig\u00b7lich", "und", "klar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Berg' und T\u00e4ler rings im Knospendrang,", "tokens": ["Ihr", "Ber\u00b7g'", "und", "T\u00e4\u00b7ler", "rings", "im", "Knos\u00b7pen\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Voll Quellenrauschen und voll Vogelsang!", "tokens": ["Voll", "Quel\u00b7len\u00b7rau\u00b7schen", "und", "voll", "Vo\u00b7gel\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O \u00dcberf\u00fclle, die zum Lichte schwillt,", "tokens": ["O", "\u00dc\u00b7berf\u00b7\u00fcl\u00b7le", ",", "die", "zum", "Lich\u00b7te", "schwillt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Bl\u00fctenwirbel, der da \u00fcberquillt", "tokens": ["O", "Bl\u00fc\u00b7ten\u00b7wir\u00b7bel", ",", "der", "da", "\u00fc\u00b7berq\u00b7uillt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und \u00fcberwuchert, wo die tote Hand", "tokens": ["Und", "\u00fc\u00b7berw\u00b7uc\u00b7hert", ",", "wo", "die", "to\u00b7te", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit ihrer Spanne mi\u00dft das reiche Land.", "tokens": ["Mit", "ih\u00b7rer", "Span\u00b7ne", "mi\u00dft", "das", "rei\u00b7che", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das ist die Nachhut, die den R\u00fccken deckt.", "tokens": ["Das", "ist", "die", "Nach\u00b7hut", ",", "die", "den", "R\u00fc\u00b7cken", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drum auf zum Werke, Menschheit, unerschreckt!", "tokens": ["Drum", "auf", "zum", "Wer\u00b7ke", ",", "Menschheit", ",", "un\u00b7er\u00b7schreckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "APPR", "APPRART", "NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Bau auf, rei\u00df nieder und bau wieder auf:", "tokens": ["Bau", "auf", ",", "rei\u00df", "nie\u00b7der", "und", "bau", "wie\u00b7der", "auf", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "KON", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Jahr geht immer seinen Segenslauf!", "tokens": ["Das", "Jahr", "geht", "im\u00b7mer", "sei\u00b7nen", "Se\u00b7gens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}