{"textgrid.poem.59237": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Riese", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sa\u00df ein Mann gefangen", "tokens": ["Es", "sa\u00df", "ein", "Mann", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf einem hohen Turm,", "tokens": ["Auf", "ei\u00b7nem", "ho\u00b7hen", "Turm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wetterf\u00e4hnlein klangen", "tokens": ["Die", "Wet\u00b7ter\u00b7f\u00e4hn\u00b7lein", "klan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar seltsam in den Sturm.", "tokens": ["Gar", "selt\u00b7sam", "in", "den", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und drau\u00dfen h\u00f6rt' er ringen", "tokens": ["Und", "drau\u00b7\u00dfen", "h\u00f6rt'", "er", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verworrner Str\u00f6me Gang,", "tokens": ["Ver\u00b7worr\u00b7ner", "Str\u00f6\u00b7me", "Gang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dazwischen V\u00f6glein singen", "tokens": ["Da\u00b7zwi\u00b7schen", "V\u00f6\u00b7glein", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und heller Waffen Klang.", "tokens": ["Und", "hel\u00b7ler", "Waf\u00b7fen", "Klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Liedlein scholl gar lustig:", "tokens": ["Ein", "Lied\u00b7lein", "scholl", "gar", "lus\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Heisa, solang Gott will!", "tokens": ["Hei\u00b7sa", ",", "so\u00b7lang", "Gott", "will", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wilder Menge Tosen;", "tokens": ["Und", "wil\u00b7der", "Men\u00b7ge", "To\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann wieder totenstill.", "tokens": ["Dann", "wie\u00b7der", "to\u00b7tens\u00b7till", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So tausend Stimmen irren,", "tokens": ["So", "tau\u00b7send", "Stim\u00b7men", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Wind' im Meere gehn,", "tokens": ["Wie", "Wind'", "im", "Mee\u00b7re", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich teilen und verwirren,", "tokens": ["Sich", "tei\u00b7len", "und", "ver\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er konnte nichts verstehn.", "tokens": ["Er", "konn\u00b7te", "nichts", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch sp\u00fcrt' er, wer ihn gr\u00fc\u00dfe,", "tokens": ["Doch", "sp\u00fcrt'", "er", ",", "wer", "ihn", "gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Schaudern und mit Lust,", "tokens": ["Mit", "Schau\u00b7dern", "und", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es r\u00fchrt' ihm wie ein Riese", "tokens": ["Es", "r\u00fchrt'", "ihm", "wie", "ein", "Rie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Leben an die Brust.", "tokens": ["Das", "Le\u00b7ben", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}