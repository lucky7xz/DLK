{"textgrid.poem.67055": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4nnerjubel", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erhabne Tochter Gottes! Gerechtigkeit,", "tokens": ["Er\u00b7hab\u00b7ne", "Toch\u00b7ter", "Got\u00b7tes", "!", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$.", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die du den Dreimalheilgen von Anbeginn", "tokens": ["Die", "du", "den", "Drei\u00b7mal\u00b7heil\u00b7gen", "von", "An\u00b7be\u00b7ginn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umstrahltest, und umstrahlen wirst am", "tokens": ["Um\u00b7strahl\u00b7test", ",", "und", "um\u00b7strah\u00b7len", "wirst", "am"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVINF", "VAFIN", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tage der ernsten Gerichtsposaune.", "tokens": ["Ta\u00b7ge", "der", "erns\u00b7ten", "Ge\u00b7richts\u00b7po\u00b7sau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Und du, o Freiheit! heiliger \u00dcberrest", "tokens": ["Und", "du", ",", "o", "Frei\u00b7heit", "!", "hei\u00b7li\u00b7ger", "\u00dc\u00b7ber\u00b7rest"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$.", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus Edens Tagen! Perle der Redlichen!", "tokens": ["Aus", "E\u00b7dens", "Ta\u00b7gen", "!", "Per\u00b7le", "der", "Red\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In deren Halle sich der V\u00f6lker", "tokens": ["In", "de\u00b7ren", "Hal\u00b7le", "sich", "der", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kronen begr\u00fc\u00dfen, und Taten schw\u00f6ren.", "tokens": ["Kro\u00b7nen", "be\u00b7gr\u00fc\u00b7\u00dfen", ",", "und", "Ta\u00b7ten", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Und du, der Geisterkr\u00e4fte gewaltigste!", "tokens": ["Und", "du", ",", "der", "Geis\u00b7ter\u00b7kr\u00e4f\u00b7te", "ge\u00b7wal\u00b7tigs\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du l\u00f6wenstolze! Liebe des Vaterlands!", "tokens": ["Du", "l\u00f6\u00b7wen\u00b7stol\u00b7ze", "!", "Lie\u00b7be", "des", "Va\u00b7ter\u00b7lands", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die du auf Mordger\u00fcsten l\u00e4chelst,", "tokens": ["Die", "du", "auf", "Mord\u00b7ge\u00b7r\u00fcs\u00b7ten", "l\u00e4\u00b7chelst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in dem Blute gew\u00e4lzt, noch siegest.", "tokens": ["Und", "in", "dem", "Blu\u00b7te", "ge\u00b7w\u00e4lzt", ",", "noch", "sie\u00b7gest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wer wagts, zu t\u00fcrmen Riesengebirge sich,", "tokens": ["Wer", "wagts", ",", "zu", "t\u00fcr\u00b7men", "Rie\u00b7sen\u00b7ge\u00b7bir\u00b7ge", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "APPR", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu schaun den Anfang eurer Erhabenheit?", "tokens": ["Zu", "schaun", "den", "An\u00b7fang", "eu\u00b7rer", "Er\u00b7ha\u00b7ben\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wer gr\u00fcndt der Tiefen tiefste aus, nach", "tokens": ["Wer", "gr\u00fcndt", "der", "Tie\u00b7fen", "tiefs\u00b7te", "aus", ",", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "PTKVZ", "$,", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Euch sich zu beugen, vor euch, Erhabne?", "tokens": ["Euch", "sich", "zu", "beu\u00b7gen", ",", "vor", "euch", ",", "Er\u00b7hab\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PRF", "PTKZU", "VVINF", "$,", "APPR", "PPER", "$,", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Und wir \u2013 o t\u00f6net, t\u00f6net den Jubel nach,", "tokens": ["Und", "wir", "\u2013", "o", "t\u00f6\u00b7net", ",", "t\u00f6\u00b7net", "den", "Ju\u00b7bel", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "FM", "VVFIN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ihr ferne Glanzgefilde des Uranus!", "tokens": ["Ihr", "fer\u00b7ne", "Glanz\u00b7ge\u00b7fil\u00b7de", "des", "U\u00b7ra\u00b7nus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O beugt euch nieder, Orione!", "tokens": ["O", "beugt", "euch", "nie\u00b7der", ",", "O\u00b7rio\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beugt euch! wir sind der Erhabnen S\u00f6hne.", "tokens": ["Beugt", "euch", "!", "wir", "sind", "der", "Er\u00b7hab\u00b7nen", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Es glimmt in uns ein Funke der G\u00f6ttlichen;", "tokens": ["Es", "glimmt", "in", "uns", "ein", "Fun\u00b7ke", "der", "G\u00f6tt\u00b7li\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und diesen Funken soll aus der M\u00e4nnerbrust", "tokens": ["Und", "die\u00b7sen", "Fun\u00b7ken", "soll", "aus", "der", "M\u00e4n\u00b7ner\u00b7brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der H\u00f6lle Macht uns nicht entrei\u00dfen!", "tokens": ["Der", "H\u00f6l\u00b7le", "Macht", "uns", "nicht", "ent\u00b7rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt es, Despotengerichte, h\u00f6rt es!", "tokens": ["H\u00f6rt", "es", ",", "Des\u00b7po\u00b7ten\u00b7ge\u00b7rich\u00b7te", ",", "h\u00f6rt", "es", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ihn senkte, seine Welt zu verherrlichen,", "tokens": ["Ihn", "senk\u00b7te", ",", "sei\u00b7ne", "Welt", "zu", "ver\u00b7herr\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Gott der G\u00f6tter Adams Geschlecht ins Herz,", "tokens": ["Der", "Gott", "der", "G\u00f6t\u00b7ter", "A\u00b7dams", "Ge\u00b7schlecht", "ins", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des preisen wir den Gott der G\u00f6tter!", "tokens": ["Des", "prei\u00b7sen", "wir", "den", "Gott", "der", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt es, ihr Knechte des L\u00fcgners, h\u00f6rt es!", "tokens": ["H\u00f6rt", "es", ",", "ihr", "Knech\u00b7te", "des", "L\u00fcg\u00b7ners", ",", "h\u00f6rt", "es", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Was \u00fcberwiegt die Wonne, der Herrlichen,", "tokens": ["Was", "\u00fc\u00b7berw\u00b7iegt", "die", "Won\u00b7ne", ",", "der", "Herr\u00b7li\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der T\u00f6chter Gottes w\u00fcrdiger Sohn zu sein?", "tokens": ["Der", "T\u00f6ch\u00b7ter", "Got\u00b7tes", "w\u00fcr\u00b7di\u00b7ger", "Sohn", "zu", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Stolz, in ihrem Heiligtum zu", "tokens": ["Den", "Stolz", ",", "in", "ih\u00b7rem", "Hei\u00b7lig\u00b7tum", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wandeln, zu dulden um ihretwillen?", "tokens": ["Wan\u00b7deln", ",", "zu", "dul\u00b7den", "um", "ih\u00b7ret\u00b7wil\u00b7len", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKZU", "VVINF", "KOUI", "VVIZU", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Und l\u00e4rmten gleich dem hadernden Ozean", "tokens": ["Und", "l\u00e4rm\u00b7ten", "gleich", "dem", "ha\u00b7dern\u00b7den", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Despotenfl\u00fcche geifernd auf uns herab,", "tokens": ["Des\u00b7po\u00b7ten\u00b7fl\u00fc\u00b7che", "gei\u00b7fernd", "auf", "uns", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vergiftete das Schnauben ihrer", "tokens": ["Ver\u00b7gif\u00b7te\u00b7te", "das", "Schnau\u00b7ben", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rache, wie Syrias Abendl\u00fcfte \u2013", "tokens": ["Ra\u00b7che", ",", "wie", "Sy\u00b7rias", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Und dr\u00e4ute tausendarmigter P\u00f6bel, uns", "tokens": ["Und", "dr\u00e4u\u00b7te", "tau\u00b7sen\u00b7dar\u00b7mig\u00b7ter", "P\u00f6\u00b7bel", ",", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu w\u00fcrgen, tausendz\u00fcngigte Pfaffenwut", "tokens": ["Zu", "w\u00fcr\u00b7gen", ",", "tau\u00b7send\u00b7z\u00fcn\u00b7gig\u00b7te", "Pfaf\u00b7fen\u00b7wut"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Bann den Neuerern; es lachen", "tokens": ["Mit", "Bann", "den", "Neu\u00b7e\u00b7rern", ";", "es", "la\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihrer die S\u00f6hne der T\u00f6chter Gottes.", "tokens": ["Ih\u00b7rer", "die", "S\u00f6h\u00b7ne", "der", "T\u00f6ch\u00b7ter", "Got\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Und w\u00fcrden unsre Kinder vom Schwert verfolgt,", "tokens": ["Und", "w\u00fcr\u00b7den", "uns\u00b7re", "Kin\u00b7der", "vom", "Schwert", "ver\u00b7folgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu heulen \u00fcber uns in der Finsternis", "tokens": ["Zu", "heu\u00b7len", "\u00fc\u00b7ber", "uns", "in", "der", "Fins\u00b7ter\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Wolfs, und mit dem L\u00f6wen seine", "tokens": ["Des", "Wolfs", ",", "und", "mit", "dem", "L\u00f6\u00b7wen", "sei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beute zu teilen, bei Kannibalen", "tokens": ["Beu\u00b7te", "zu", "tei\u00b7len", ",", "bei", "Kan\u00b7ni\u00b7ba\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$,", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Sich V\u00e4ter, und im Sande von Afrika", "tokens": ["Sich", "V\u00e4\u00b7ter", ",", "und", "im", "San\u00b7de", "von", "Af\u00b7ri\u00b7ka"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "NN", "$,", "KON", "APPRART", "NN", "APPR", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Gastrecht aufzusuchen, sie dulden gern,", "tokens": ["Das", "Gast\u00b7recht", "auf\u00b7zu\u00b7su\u00b7chen", ",", "sie", "dul\u00b7den", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verlachen eure Blutger\u00fcste,", "tokens": ["Ver\u00b7la\u00b7chen", "eu\u00b7re", "Blut\u00b7ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Folgen den V\u00e4tern zu Schwert und Folter.", "tokens": ["Fol\u00b7gen", "den", "V\u00e4\u00b7tern", "zu", "Schwert", "und", "Fol\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Drum t\u00f6net, t\u00f6net, t\u00f6net den Jubel nach,", "tokens": ["Drum", "t\u00f6\u00b7net", ",", "t\u00f6\u00b7net", ",", "t\u00f6\u00b7net", "den", "Ju\u00b7bel", "nac