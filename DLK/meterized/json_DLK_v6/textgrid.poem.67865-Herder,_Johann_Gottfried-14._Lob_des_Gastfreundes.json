{"textgrid.poem.67865": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14. Lob des Gastfreundes", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Tugend, schwer zu erringen", "tokens": ["O", "Tu\u00b7gend", ",", "schwer", "zu", "er\u00b7rin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dem sterblichen Geschlecht,", "tokens": ["Dem", "sterb\u00b7li\u00b7chen", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Lebens sch\u00f6nste Belohnung,", "tokens": ["Des", "Le\u00b7bens", "sch\u00f6ns\u00b7te", "Be\u00b7loh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jungfrau du!", "tokens": ["Jung\u00b7frau", "du", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Um deine Sch\u00f6ne gingen", "tokens": ["Um", "dei\u00b7ne", "Sch\u00f6\u00b7ne", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Griechen freudig in Tod,", "tokens": ["Die", "Grie\u00b7chen", "freu\u00b7dig", "in", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bestanden harte Gefahren", "tokens": ["Be\u00b7stan\u00b7den", "har\u00b7te", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit eiserm Muth.", "tokens": ["Mit", "ei\u00b7serm", "Muth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du gibst dem Herzen", "tokens": ["Du", "gibst", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Unsterbliche Frucht,", "tokens": ["U\u00b7nsterb\u00b7li\u00b7che", "Frucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die s\u00fcsser als Gold und Eltern ist,", "tokens": ["Die", "s\u00fcs\u00b7ser", "als", "Gold", "und", "El\u00b7tern", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KOKOM", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und als der zarte Schlaf.", "tokens": ["Und", "als", "der", "zar\u00b7te", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Um deinetwillen hat Herkules", "tokens": ["Um", "dei\u00b7net\u00b7wil\u00b7len", "hat", "Her\u00b7ku\u00b7les"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADV", "VAFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Leda's S\u00f6hne so viel ertragen,", "tokens": ["Und", "Le\u00b7da's", "S\u00f6h\u00b7ne", "so", "viel", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zeigten in Thaten", "tokens": ["Zeig\u00b7ten", "in", "Tha\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Deine Macht.", "tokens": ["Dei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Aus Lieb' um dich ging Held Achill", "tokens": ["Aus", "Lieb'", "um", "dich", "ging", "Held", "A\u00b7chill"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Aeas ins Todtenreich,", "tokens": ["Und", "A\u00b7e\u00b7as", "ins", "Tod\u00b7ten\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um deine s\u00fcsse Gestalt hat sich Atarne's Gastfreund", "tokens": ["Um", "dei\u00b7ne", "s\u00fcs\u00b7se", "Ge\u00b7stalt", "hat", "sich", "A\u00b7tar\u00b7ne's", "Gast\u00b7freund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "NE", "NN"], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Glanz der Sonne geraubet.", "tokens": ["Den", "Glanz", "der", "Son\u00b7ne", "ge\u00b7rau\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Unsterblich singet ihn, ihn den Thatenreichen,", "tokens": ["U\u00b7nsterb\u00b7lich", "sin\u00b7get", "ihn", ",", "ihn", "den", "Tha\u00b7ten\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "O Musen, T\u00f6chter des Ruhms,", "tokens": ["O", "Mu\u00b7sen", ",", "T\u00f6ch\u00b7ter", "des", "Ruhms", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So oft ihr preiset den Gott verb\u00fcndeter Treu", "tokens": ["So", "oft", "ihr", "prei\u00b7set", "den", "Gott", "ver\u00b7b\u00fcn\u00b7de\u00b7ter", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und vester Freundschaft Lohn!", "tokens": ["Und", "ves\u00b7ter", "Freund\u00b7schaft", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}