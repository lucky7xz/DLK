{"dta.poem.11976": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Reisender ist stets vor R\u00e4ubern in Gefahr,", "tokens": ["Ein", "Rei\u00b7sen\u00b7der", "ist", "stets", "vor", "R\u00e4u\u00b7bern", "in", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leicht unter sich in Streit von Reisenden ein Paar.", "tokens": ["Leicht", "un\u00b7ter", "sich", "in", "Streit", "von", "Rei\u00b7sen\u00b7den", "ein", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PRF", "APPR", "NN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Kleeblatt Reisender hat Gl\u00fcck auf jedem Schritte;", "tokens": ["Ein", "Klee\u00b7blatt", "Rei\u00b7sen\u00b7der", "hat", "Gl\u00fcck", "auf", "je\u00b7dem", "Schrit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo uneins zweie sind, den Ausschlag gibt der dritte.", "tokens": ["Wo", "un\u00b7eins", "zwei\u00b7e", "sind", ",", "den", "Aus\u00b7schlag", "gibt", "der", "drit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-