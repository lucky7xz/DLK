{"textgrid.poem.53876": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Putzmittel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einer und er f\u00fchlt begossen,", "tokens": ["Wenn", "ei\u00b7ner", "und", "er", "f\u00fchlt", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "KON", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df er nunmehr alle Patronen verschossen,", "tokens": ["da\u00df", "er", "nun\u00b7mehr", "al\u00b7le", "Pat\u00b7ro\u00b7nen", "ver\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "und da\u00df er nicht mehr so kann wie ehedem,", "tokens": ["und", "da\u00df", "er", "nicht", "mehr", "so", "kann", "wie", "e\u00b7he\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ADV", "ADV", "VMFIN", "KOKOM", "ADV", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und da\u00df l\u00e4stige Verpflichtung, was fr\u00fcher bequem \u2013:", "tokens": ["und", "da\u00df", "l\u00e4s\u00b7ti\u00b7ge", "Ver\u00b7pflich\u00b7tung", ",", "was", "fr\u00fc\u00b7her", "be\u00b7quem", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJD", "$(", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "dann bekommt er immerhin noch etwas verschrieben,", "tokens": ["dann", "be\u00b7kommt", "er", "im\u00b7mer\u00b7hin", "noch", "et\u00b7was", "ver\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "ein s\u00fc\u00dfes Pulver ist ihm geblieben \u2013", "tokens": ["ein", "s\u00fc\u00b7\u00dfes", "Pul\u00b7ver", "ist", "ihm", "ge\u00b7blie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "in zwei Packungen \u2013 sagt die Reklame:", "tokens": ["in", "zwei", "Pa\u00b7ckun\u00b7gen", "\u2013", "sagt", "die", "Re\u00b7kla\u00b7me", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Silber f\u00fcr den Herrn und Gold f\u00fcr die Dame.", "tokens": ["Sil\u00b7ber", "f\u00fcr", "den", "Herrn", "und", "Gold", "f\u00fcr", "die", "Da\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So auch im Leben.", "tokens": ["So", "auch", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wenn zum Beispiel die Verwaltungsfritzen", "tokens": ["Wenn", "zum", "Bei\u00b7spiel", "die", "Ver\u00b7wal\u00b7tungs\u00b7frit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "es satt haben, auf ihrem B.-A. zu sitzen,", "tokens": ["es", "satt", "ha\u00b7ben", ",", "auf", "ih\u00b7rem", "B.", "A.", "zu", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation", "word", "abbreviation", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAINF", "$,", "APPR", "PPOSAT", "NN", "$(", "APPRART", "PTKZU", "VVINF", "$,"], "meter": "-++--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "dann spielen sie, statt friedlich in ihren B\u00fcros zu schlafen,", "tokens": ["dann", "spie\u00b7len", "sie", ",", "statt", "fried\u00b7lich", "in", "ih\u00b7ren", "B\u00fc\u00b7ros", "zu", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUI", "ADJD", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "L\u00e4nderkonflikt um den hamburger Hafen.", "tokens": ["L\u00e4n\u00b7der\u00b7kon\u00b7flikt", "um", "den", "ham\u00b7bur\u00b7ger", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Dienstreisen nach Altona! Keinen Fu\u00dfbreit von preu\u00dfischer Erden!", "tokens": ["Diens\u00b7trei\u00b7sen", "nach", "Al\u00b7to\u00b7na", "!", "Kei\u00b7nen", "Fu\u00df\u00b7breit", "von", "preu\u00b7\u00dfi\u00b7scher", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "$.", "PIAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Damit kann man schnellstens Oberregierungsrat werden.", "tokens": ["Da\u00b7mit", "kann", "man", "schnells\u00b7tens", "O\u00b7ber\u00b7re\u00b7gie\u00b7rungs\u00b7rat", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PIS", "ADJA", "NN", "VAINF", "$."], "meter": "--+-+-+---+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Sowas bringt Leben in die Bude. Und solche Verwaltungsdramen", "tokens": ["So\u00b7was", "bringt", "Le\u00b7ben", "in", "die", "Bu\u00b7de", ".", "Und", "sol\u00b7che", "Ver\u00b7wal\u00b7tungs\u00b7dra\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$.", "KON", "PIAT", "NN"], "meter": "-+-+-+-+--+--+-+-", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "sind Silber f\u00fcr die Herren und Gold f\u00fcr die Damen.", "tokens": ["sind", "Sil\u00b7ber", "f\u00fcr", "die", "Her\u00b7ren", "und", "Gold", "f\u00fcr", "die", "Da\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Es tun sich unter kosmischem Wimmern", "tokens": ["Es", "tun", "sich", "un\u00b7ter", "kos\u00b7mi\u00b7schem", "Wim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "viele um vieles auf der Welt bek\u00fcmmern.", "tokens": ["vie\u00b7le", "um", "vie\u00b7les", "auf", "der", "Welt", "be\u00b7k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da sieht das Auge des Vortragenden den Himmel offen \u2013", "tokens": ["Da", "sieht", "das", "Au\u00b7ge", "des", "Vor\u00b7tra\u00b7gen\u00b7den", "den", "Him\u00b7mel", "of\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+--++-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "kommen auch gern auf alle Kongresse geloffen \u2013", "tokens": ["kom\u00b7men", "auch", "gern", "auf", "al\u00b7le", "Kon\u00b7gres\u00b7se", "ge\u00b7lof\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "schwabbeln \u00fcber Klassen und \u00fcber Rassen", "tokens": ["schwab\u00b7beln", "\u00fc\u00b7ber", "Klas\u00b7sen", "und", "\u00fc\u00b7ber", "Ras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "und vergessen nicht, sich fotografieren zu lassen.", "tokens": ["und", "ver\u00b7ges\u00b7sen", "nicht", ",", "sich", "fo\u00b7to\u00b7gra\u00b7fie\u00b7ren", "zu", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRF", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Kommt nichts dabei raus. Bleibt alles am selben Fleck.", "tokens": ["Kommt", "nichts", "da\u00b7bei", "raus", ".", "Bleibt", "al\u00b7les", "am", "sel\u00b7ben", "Fleck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PAV", "PTKVZ", "$.", "VVFIN", "PIS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Aber es t\u00e4uscht so sch\u00f6n \u00fcber das eigene Manko hinweg.", "tokens": ["A\u00b7ber", "es", "t\u00e4uscht", "so", "sch\u00f6n", "\u00fc\u00b7ber", "das", "ei\u00b7ge\u00b7ne", "Man\u00b7ko", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+---+---+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "O falsche Humanit\u00e4t \u2013 Karin Michaelis ist dein Name! \u2013", "tokens": ["O", "fal\u00b7sche", "Hu\u00b7ma\u00b7ni\u00b7t\u00e4t", "\u2013", "Ka\u00b7rin", "Mic\u00b7ha\u00b7e\u00b7lis", "ist", "dein", "Na\u00b7me", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "NE", "NE", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "Schweigen ist Silber f\u00fcr den Herrn und Gold f\u00fcr die Dame.", "tokens": ["Schwei\u00b7gen", "ist", "Sil\u00b7ber", "f\u00fcr", "den", "Herrn", "und", "Gold", "f\u00fcr", "die", "Da\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}}}}}