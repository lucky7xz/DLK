{"textgrid.poem.40669": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das B\u00e4chlein l\u00e4rmt, ein spielend Kind am Pfad;", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das B\u00e4chlein l\u00e4rmt, ein spielend Kind am Pfad;", "tokens": ["Das", "B\u00e4ch\u00b7lein", "l\u00e4rmt", ",", "ein", "spie\u00b7lend", "Kind", "am", "Pfad", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Lasten zieht der m\u00e4cht'ge Strom indessen", "tokens": ["Mit", "Las\u00b7ten", "zieht", "der", "m\u00e4cht'\u00b7ge", "Strom", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Unh\u00f6rbar fast, ger\u00e4uschlos und gemessen,", "tokens": ["Un\u00b7h\u00f6r\u00b7bar", "fast", ",", "ge\u00b7r\u00e4uschlos", "und", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schweigsam dahin, ein Mann der Pflicht und That.", "tokens": ["Schweig\u00b7sam", "da\u00b7hin", ",", "ein", "Mann", "der", "Pflicht", "und", "That", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PAV", "$,", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Sein Wort: das Brausen ganz nicht zu vergessen", "tokens": ["Sein", "Wort", ":", "das", "Brau\u00b7sen", "ganz", "nicht", "zu", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mahnt ihn des Frachtschiffs Kiel, des Dampfers Rad;", "tokens": ["Mahnt", "ihn", "des", "Fracht\u00b7schiffs", "Kiel", ",", "des", "Damp\u00b7fers", "Rad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch lauter tobt der Werkflei\u00df am Gestad',", "tokens": ["Doch", "lau\u00b7ter", "tobt", "der", "Werk\u00b7flei\u00df", "am", "Ge\u00b7stad'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Marktes Ruf, Get\u00f6s von H\u00e4mmern, Essen.", "tokens": ["Des", "Mark\u00b7tes", "Ruf", ",", "Ge\u00b7t\u00f6s", "von", "H\u00e4m\u00b7mern", ",", "Es\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "APPR", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur wenn das Tagwerk ruht, lautlos die Menge,", "tokens": ["Nur", "wenn", "das", "Tag\u00b7werk", "ruht", ",", "laut\u00b7los", "die", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhebt der Strom die Stimm': ein heilig Rauschen!", "tokens": ["Er\u00b7hebt", "der", "Strom", "die", "Stimm'", ":", "ein", "hei\u00b7lig", "Rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch schweigend Dunkel zieht's wie Orgelkl\u00e4nge;", "tokens": ["Durch", "schwei\u00b7gend", "Dun\u00b7kel", "zieht's", "wie", "Or\u00b7gel\u00b7kl\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vernehmbar sei's nur f\u00fcr die reinen Sterne", "tokens": ["Ver\u00b7nehm\u00b7bar", "sei's", "nur", "f\u00fcr", "die", "rei\u00b7nen", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00fcr die ernste Nacht! \u2013 Doch ihm auch lauschen", "tokens": ["Und", "f\u00fcr", "die", "erns\u00b7te", "Nacht", "!", "\u2013", "Doch", "ihm", "auch", "lau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$.", "$(", "KON", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Stern und Nacht schlaflose Tr\u00e4umer gerne.", "tokens": ["Mit", "Stern", "und", "Nacht", "schlaf\u00b7lo\u00b7se", "Tr\u00e4u\u00b7mer", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}