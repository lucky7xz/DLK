{"dta.poem.8747": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.  \n  Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Licht, empor zum Himmel flammt es;", "tokens": ["Licht", ",", "em\u00b7por", "zum", "Him\u00b7mel", "flammt", "es", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Licht, es ist der gro\u00dfe Mittler", "tokens": ["Licht", ",", "es", "ist", "der", "gro\u00b7\u00dfe", "Mitt\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen Gott und zwischen Menschen;", "tokens": ["Zwi\u00b7schen", "Gott", "und", "zwi\u00b7schen", "Men\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als die Welt geboren wurde,", "tokens": ["Als", "die", "Welt", "ge\u00b7bo\u00b7ren", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ward das Licht vorangeboren,", "tokens": ["Ward", "das", "Licht", "vor\u00b7an\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und so ward des Sch\u00f6pfers Klarheit", "tokens": ["Und", "so", "ward", "des", "Sch\u00f6p\u00b7fers", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Mysterium der Sch\u00f6pfung;", "tokens": ["Das", "Mys\u00b7te\u00b7ri\u00b7um", "der", "Sch\u00f6p\u00b7fung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Licht verschie\u00dft die heil'gen Pfeile", "tokens": ["Licht", "ver\u00b7schie\u00dft", "die", "heil'\u00b7gen", "Pfei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weiter immer, lichter immer,", "tokens": ["Wei\u00b7ter", "im\u00b7mer", ",", "lich\u00b7ter", "im\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ahriman sogar, der dunkle,", "tokens": ["Ah\u00b7ri\u00b7man", "so\u00b7gar", ",", "der", "dunk\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wird zulezt vergehn im Lichte.", "tokens": ["Wird", "zu\u00b7lezt", "ver\u00b7gehn", "im", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVINF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}