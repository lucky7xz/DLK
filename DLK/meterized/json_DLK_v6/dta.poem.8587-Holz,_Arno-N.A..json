{"dta.poem.8587": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieben Septillionen Jahre", "tokens": ["Sie\u00b7ben", "Sep\u00b7til\u00b7li\u00b7o\u00b7nen", "Jah\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "z\u00e4hlte ich die Meilensteine am Rande der Milchstrasse.", "tokens": ["z\u00e4hl\u00b7te", "ich", "die", "Mei\u00b7len\u00b7stei\u00b7ne", "am", "Ran\u00b7de", "der", "Milch\u00b7stras\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--++-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie endeten nicht.", "tokens": ["Sie", "en\u00b7de\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Myriaden Aeonen", "tokens": ["My\u00b7ria\u00b7den", "A\u00b7e\u00b7o\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "versank ich in die Wunder eines einzigen Thautr\u00f6pfchens.", "tokens": ["ver\u00b7sank", "ich", "in", "die", "Wun\u00b7der", "ei\u00b7nes", "ein\u00b7zi\u00b7gen", "Thau\u00b7tr\u00f6pf\u00b7chens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+--", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Es erschlossen sich immer neue.", "tokens": ["Es", "er\u00b7schlos\u00b7sen", "sich", "im\u00b7mer", "neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJA", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Mein Herz erzitterte!", "tokens": ["Mein", "Herz", "er\u00b7zit\u00b7ter\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Selig ins Moos", "tokens": ["Se\u00b7lig", "ins", "Moos"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "streckte ich mich und wurde Erde.", "tokens": ["streck\u00b7te", "ich", "mich", "und", "wur\u00b7de", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "KON", "VAFIN", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Jetzt ranken Brombeeren", "tokens": ["Jetzt", "ran\u00b7ken", "Brom\u00b7bee\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "\u00fcber mir,", "tokens": ["\u00fc\u00b7ber", "mir", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "auf einem sich wiegenden Schlehdornzweig", "tokens": ["auf", "ei\u00b7nem", "sich", "wie\u00b7gen\u00b7den", "Schleh\u00b7dorn\u00b7zweig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PRF", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "zwitschert ein Rotkehlchen.", "tokens": ["zwit\u00b7schert", "ein", "Rot\u00b7kehl\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "Aus meiner Brust", "tokens": ["Aus", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "springt fr\u00f6hlich ein Quell,", "tokens": ["springt", "fr\u00f6h\u00b7lich", "ein", "Quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "aus meinem Sch\u00e4del", "tokens": ["aus", "mei\u00b7nem", "Sch\u00e4\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "wachsen Blumen.", "tokens": ["wach\u00b7sen", "Blu\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}