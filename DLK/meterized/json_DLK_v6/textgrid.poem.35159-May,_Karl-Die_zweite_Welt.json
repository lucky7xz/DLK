{"textgrid.poem.35159": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die zweite Welt", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich bin nicht frei. Ich werde fest gehalten.", "tokens": ["\u00bb", "ich", "bin", "nicht", "frei", ".", "Ich", "wer\u00b7de", "fest", "ge\u00b7hal\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$.", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich f\u00fchls, hab oft dar\u00fcber nachgedacht.", "tokens": ["Ich", "f\u00fchls", ",", "hab", "oft", "da\u00b7r\u00fc\u00b7ber", "nach\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VAFIN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will nach Gottes Willen mich gestalten,", "tokens": ["Ich", "will", "nach", "Got\u00b7tes", "Wil\u00b7len", "mich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und das wird mir so schwer, so schwer gemacht.", "tokens": ["Und", "das", "wird", "mir", "so", "schwer", ",", "so", "schwer", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "O, d\u00fcrfte meine Frage aufw\u00e4rts schweben,", "tokens": ["O", ",", "d\u00fcrf\u00b7te", "mei\u00b7ne", "Fra\u00b7ge", "auf\u00b7w\u00e4rts", "schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie ichs f\u00fcr sie ersehne, himmelan,", "tokens": ["Wie", "ichs", "f\u00fcr", "sie", "er\u00b7seh\u00b7ne", ",", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPER", "VVFIN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Empor zur Wahrheit, die mir Antwort geben,", "tokens": ["Em\u00b7por", "zur", "Wahr\u00b7heit", ",", "die", "mir", "Ant\u00b7wort", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die mich befrein, die mich erl\u00f6sen kann!\u00ab", "tokens": ["Die", "mich", "be\u00b7fr\u00b7ein", ",", "die", "mich", "er\u00b7l\u00f6\u00b7sen", "kann", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "PTKVZ", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bb\u203aKomm mit! Ich trage dich auf leichten Schwingen", "tokens": ["\u00bb", "\u203a", "Komm", "mit", "!", "Ich", "tra\u00b7ge", "dich", "auf", "leich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dieser Erde fort zur zweiten Welt.", "tokens": ["Von", "die\u00b7ser", "Er\u00b7de", "fort", "zur", "zwei\u00b7ten", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann dich nicht bis in den Himmel bringen;", "tokens": ["Ich", "kann", "dich", "nicht", "bis", "in", "den", "Him\u00b7mel", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er \u00f6ffnet keinem Sterblichen sein Zelt;", "tokens": ["Er", "\u00f6ff\u00b7net", "kei\u00b7nem", "Sterb\u00b7li\u00b7chen", "sein", "Zelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch will ich dir eins seiner Wunder zeigen,", "tokens": ["Doch", "will", "ich", "dir", "eins", "sei\u00b7ner", "Wun\u00b7der", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PIS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn du dich meiner F\u00fchrung anvertraust.", "tokens": ["Wenn", "du", "dich", "mei\u00b7ner", "F\u00fch\u00b7rung", "an\u00b7ver\u00b7traust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dein Staunen braucht nicht gegen mich zu schweigen.", "tokens": ["Dein", "Stau\u00b7nen", "braucht", "nicht", "ge\u00b7gen", "mich", "zu", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du darfst mir Alles sagen, was du schaust.\u2039\u00ab", "tokens": ["Du", "darfst", "mir", "Al\u00b7les", "sa\u00b7gen", ",", "was", "du", "schaust", ".", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbich seh der Erde finstre Schatten fallen,", "tokens": ["\u00bb", "ich", "seh", "der", "Er\u00b7de", "finst\u00b7re", "Schat\u00b7ten", "fal\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Unendlich weit, auf Ewigkeiten hin,", "tokens": ["Un\u00b7end\u00b7lich", "weit", ",", "auf", "E\u00b7wig\u00b7kei\u00b7ten", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6r aus ihnen grelle Stimmen schallen", "tokens": ["Und", "h\u00f6r", "aus", "ih\u00b7nen", "grel\u00b7le", "Stim\u00b7men", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Empor zum Glanz, in dem ich mit dir bin.", "tokens": ["Em\u00b7por", "zum", "Glanz", ",", "in", "dem", "ich", "mit", "dir", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir schweben hoch, im sanften Erdenscheine,", "tokens": ["Wir", "schwe\u00b7ben", "hoch", ",", "im", "sanf\u00b7ten", "Er\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So mild, wie ihn die stille Mondnacht liebt,", "tokens": ["So", "mild", ",", "wie", "ihn", "die", "stil\u00b7le", "Mond\u00b7nacht", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und um uns klingen \u00fcberirdisch reine", "tokens": ["Und", "um", "uns", "klin\u00b7gen", "\u00fc\u00b7be\u00b7rir\u00b7disch", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PPER", "VVFIN", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Accorde, die es nicht auf Erden giebt.\u00ab", "tokens": ["Ac\u00b7cor\u00b7de", ",", "die", "es", "nicht", "auf", "Er\u00b7den", "giebt", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bb\u203aDas ist nicht Erdenglanz und nicht ihr Schatten;", "tokens": ["\u00bb", "\u203a", "Das", "ist", "nicht", "Er\u00b7den\u00b7glanz", "und", "nicht", "ihr", "Schat\u00b7ten", ";"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PDS", "VAFIN", "PTKNEG", "NN", "KON", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist der Seelen Finsterni\u00df und Licht.", "tokens": ["Das", "ist", "der", "See\u00b7len", "Fins\u00b7ter\u00b7ni\u00df", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort fehlt das Licht, weil sie es niemals hatten,", "tokens": ["Dort", "fehlt", "das", "Licht", ",", "weil", "sie", "es", "nie\u00b7mals", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch hier wars stets, drum fehlt auch jetzt es nicht.", "tokens": ["Doch", "hier", "wars", "stets", ",", "drum", "fehlt", "auch", "jetzt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "$,", "PAV", "VVFIN", "ADV", "ADV", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo du hier bist, das bleibe dir verschwiegen,", "tokens": ["Wo", "du", "hier", "bist", ",", "das", "blei\u00b7be", "dir", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch deiner Seele ist es wohlbekannt.", "tokens": ["Doch", "dei\u00b7ner", "See\u00b7le", "ist", "es", "wohl\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Schau hin, schau hin! Siehst du es vor dir liegen,", "tokens": ["Schau", "hin", ",", "schau", "hin", "!", "Siehst", "du", "es", "vor", "dir", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "PTKVZ", "$.", "VVFIN", "PPER", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.8": {"text": "Der zweiten Welt geheimni\u00dfvolles Land?\u2039\u00ab", "tokens": ["Der", "zwei\u00b7ten", "Welt", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7les", "Land", "?", "\u2039", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbder zweiten Welt? Ist das nicht auch die Erde?", "tokens": ["\u00bb", "der", "zwei\u00b7ten", "Welt", "?", "Ist", "das", "nicht", "auch", "die", "Er\u00b7de", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "VAFIN", "PDS", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gebirge, Land und Wasser, Feld und Au,", "tokens": ["Ge\u00b7bir\u00b7ge", ",", "Land", "und", "Was\u00b7ser", ",", "Feld", "und", "Au", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gleich, so \u00e4hnlich, da\u00df fast irr ich werde,", "tokens": ["So", "gleich", ",", "so", "\u00e4hn\u00b7lich", ",", "da\u00df", "fast", "irr", "ich", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "$,", "KOUS", "ADV", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sogar der D\u00f6rfer und der St\u00e4dte Bau!", "tokens": ["So\u00b7gar", "der", "D\u00f6r\u00b7fer", "und", "der", "St\u00e4d\u00b7te", "Bau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In klarer Sch\u00f6nheit ragt empor das Reine,", "tokens": ["In", "kla\u00b7rer", "Sch\u00f6n\u00b7heit", "ragt", "em\u00b7por", "das", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als hab es sich vom Irdischen befreit;", "tokens": ["Als", "hab", "es", "sich", "vom", "Ir\u00b7di\u00b7schen", "be\u00b7freit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Nebelzwielicht sondert das Gemeine;", "tokens": ["Ein", "Ne\u00b7bel\u00b7zwie\u00b7licht", "son\u00b7dert", "das", "Ge\u00b7mei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In finstern Schluchten haust die Niedrigkeit.\u00ab", "tokens": ["In", "fins\u00b7tern", "Schluch\u00b7ten", "haust", "die", "Nied\u00b7rig\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bb\u203aDu siehst den Trieb nach oben und nach unten,", "tokens": ["\u00bb", "\u203a", "Du", "siehst", "den", "Trieb", "nach", "o\u00b7ben", "und", "nach", "un\u00b7ten", ","], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "ART", "NN", "APPR", "ADV", "KON", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Flug- und Zugkraft dieser Wunderwelt.", "tokens": ["Die", "Flug", "und", "Zug\u00b7kraft", "die\u00b7ser", "Wun\u00b7der\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Gegens\u00e4tze scheinen zwar verbunden,", "tokens": ["Die", "Ge\u00b7gen\u00b7s\u00e4t\u00b7ze", "schei\u00b7nen", "zwar", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch nur, bis Gott die letzte Frage stellt.", "tokens": ["Doch", "nur", ",", "bis", "Gott", "die", "letz\u00b7te", "Fra\u00b7ge", "stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun schau, wie ihr auf sie euch vorbereitet,", "tokens": ["Nun", "schau", ",", "wie", "ihr", "auf", "sie", "euch", "vor\u00b7be\u00b7rei\u00b7tet", ","], "token_info": ["word", "wo