{"textgrid.poem.58861": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kehr ein in deiner Seelen Grunde,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kehr ein in deiner Seelen Grunde,", "tokens": ["Kehr", "ein", "in", "dei\u00b7ner", "See\u00b7len", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da wohn und werde Gott gemein!", "tokens": ["Da", "wohn", "und", "wer\u00b7de", "Gott", "ge\u00b7mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer diesen weiten Palast funde,", "tokens": ["Wer", "die\u00b7sen", "wei\u00b7ten", "Pa\u00b7last", "fun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem mu\u00df die Welt ein Kerker sein.", "tokens": ["Dem", "mu\u00df", "die", "Welt", "ein", "Ker\u00b7ker", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}