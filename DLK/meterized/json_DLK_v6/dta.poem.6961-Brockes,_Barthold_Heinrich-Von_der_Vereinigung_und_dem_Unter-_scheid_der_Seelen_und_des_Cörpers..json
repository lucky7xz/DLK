{"dta.poem.6961": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erzeiget man dem Geist noch mehr Ver\u00e4chtlichkeit?", "tokens": ["Er\u00b7zei\u00b7get", "man", "dem", "Geist", "noch", "mehr", "Ver\u00b7\u00e4cht\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Leib, sein Werckzeug sey nicht richtig im Geschicke,", "tokens": ["Der", "Leib", ",", "sein", "Wer\u00b7ck\u00b7zeug", "sey", "nicht", "rich\u00b7tig", "im", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er sey von Dunst verwirrt, zernaget durch die Zeit;", "tokens": ["Er", "sey", "von", "Dunst", "ver\u00b7wirrt", ",", "zer\u00b7na\u00b7get", "durch", "die", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sodann ist er ein Schiff ohn Seegel, sonder Stricke,", "tokens": ["So\u00b7dann", "ist", "er", "ein", "Schiff", "ohn", "See\u00b7gel", ",", "son\u00b7der", "Stri\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des sich der Steuermann", "tokens": ["Des", "sich", "der", "Steu\u00b7er\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PRF", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht mehr gebrauchen kan.", "tokens": ["Nicht", "mehr", "ge\u00b7brau\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch duldet unser Leib die Aenderung allein,", "tokens": ["Doch", "dul\u00b7det", "un\u00b7ser", "Leib", "die", "A\u00b7en\u00b7de\u00b7rung", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der F\u00e4ulni\u00df mu\u00df der Leib nur unterwoffen seyn.", "tokens": ["Der", "F\u00e4ul\u00b7ni\u00df", "mu\u00df", "der", "Leib", "nur", "un\u00b7ter\u00b7wof\u00b7fen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}