{"textgrid.poem.38071": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Tritt zu", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann alle W\u00e4sserlein fliessen,", "tokens": ["Wann", "al\u00b7le", "W\u00e4s\u00b7ser\u00b7lein", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Soll man trinken,", "tokens": ["Soll", "man", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wann ich mein Schatz nicht rufen darf, ju ja rufen darf,", "tokens": ["Wann", "ich", "mein", "Schatz", "nicht", "ru\u00b7fen", "darf", ",", "ju", "ja", "ru\u00b7fen", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$,", "ITJ", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So thu ich ihm winken.", "tokens": ["So", "thu", "ich", "ihm", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Winken mit den Augen,", "tokens": ["Win\u00b7ken", "mit", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und treten mit dem Fu\u00df,", "tokens": ["Und", "tre\u00b7ten", "mit", "dem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "S'ist eine in der Stuben, ju ja Stuben,", "tokens": ["S'\u00b7ist", "ei\u00b7ne", "in", "der", "Stu\u00b7ben", ",", "ju", "ja", "Stu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "ART", "NN", "$,", "ITJ", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und die mir werden mu\u00df.", "tokens": ["Und", "die", "mir", "wer\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VAINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Warum soll sie mir nicht werden,", "tokens": ["Wa\u00b7rum", "soll", "sie", "mir", "nicht", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich seh sie gern,", "tokens": ["Denn", "ich", "seh", "sie", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sie hat zwei blaue Aeugelein, ju ja Aeugelein,", "tokens": ["Sie", "hat", "zwei", "blau\u00b7e", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ",", "ju", "ja", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "$,", "ITJ", "ADV", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Sie gl\u00e4nzen wie zwey Stern.", "tokens": ["Sie", "gl\u00e4n\u00b7zen", "wie", "zwey", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie hat zwey rothe B\u00e4ckelein,", "tokens": ["Sie", "hat", "zwey", "ro\u00b7the", "B\u00e4\u00b7ckel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind r\u00f6ther als der Wein,", "tokens": ["Sind", "r\u00f6\u00b7ther", "als", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein solches M\u00e4del findt man nicht, ju ja findt man nicht,", "tokens": ["Ein", "sol\u00b7ches", "M\u00e4\u00b7del", "findt", "man", "nicht", ",", "ju", "ja", "findt", "man", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PIS", "PTKNEG", "$,", "ITJ", "ADV", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wohl unter dem Sonnenschein.", "tokens": ["Wohl", "un\u00b7ter", "dem", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbach herziger Schatz, ich bitt dich drum,", "tokens": ["\u00bb", "ach", "her\u00b7zi\u00b7ger", "Schatz", ",", "ich", "bitt", "dich", "drum", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "ADJA", "NN", "$,", "PPER", "VVFIN", "PRF", "PAV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "La\u00df mich gehen!", "tokens": ["La\u00df", "mich", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Denn deine Leute schm\u00e4hen mich, ju ja schm\u00e4hen mich,", "tokens": ["Denn", "dei\u00b7ne", "Leu\u00b7te", "schm\u00e4\u00b7hen", "mich", ",", "ju", "ja", "schm\u00e4\u00b7hen", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "ITJ", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ich mu\u00df mich sch\u00e4men!\u00ab", "tokens": ["Ich", "mu\u00df", "mich", "sch\u00e4\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "\u00bbwas frag ich nach den Leuten,", "tokens": ["\u00bb", "was", "frag", "ich", "nach", "den", "Leu\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mich schm\u00e4hen;", "tokens": ["Die", "mich", "schm\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und so lieb ich noch einmal, ju ja noch einmal,", "tokens": ["Und", "so", "lieb", "ich", "noch", "ein\u00b7mal", ",", "ju", "ja", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "ADV", "ADV", "$,", "ITJ", "ADV", "ADV", "ADV", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die sch\u00f6nen M\u00e4dchen.\u00ab", "tokens": ["Die", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}