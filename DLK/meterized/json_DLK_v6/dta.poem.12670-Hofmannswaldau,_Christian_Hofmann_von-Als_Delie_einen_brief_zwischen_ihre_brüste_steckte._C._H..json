{"dta.poem.12670": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Delie einen brief zwischen  \n ihre br\u00fcste steckte.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jst hier die cantzeley?", "tokens": ["Ist", "hier", "die", "cant\u00b7ze\u00b7ley", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verwahrt man denn die briefe bey den br\u00fcsten?", "tokens": ["Ver\u00b7wahrt", "man", "denn", "die", "brie\u00b7fe", "bey", "den", "br\u00fcs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach! gieb mir auch ein amt hierbey,", "tokens": ["Ach", "!", "gieb", "mir", "auch", "ein", "amt", "hier\u00b7bey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mache mich zu deinem cancelisten.", "tokens": ["Und", "ma\u00b7che", "mich", "zu", "dei\u00b7nem", "can\u00b7ce\u00b7lis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O da\u00df ich doch nicht ein zigeuner bin,", "tokens": ["O", "da\u00df", "ich", "doch", "nicht", "ein", "zi\u00b7geu\u00b7ner", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "PTKNEG", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und meinen leib kan in di\u00df briefgen schn\u00fcren!", "tokens": ["Und", "mei\u00b7nen", "leib", "kan", "in", "di\u00df", "brief\u00b7gen", "schn\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "PDS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie gern wolt ich mich doch da hinein partieren,", "tokens": ["Wie", "gern", "wolt", "ich", "mich", "doch", "da", "hin\u00b7ein", "par\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "ADV", "ADV", "APZR", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Und um die bl\u00e4tter mich bem\u00fchn.", "tokens": ["Und", "um", "die", "bl\u00e4t\u00b7ter", "mich", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich schickte mich in diese schreiberey,", "tokens": ["Ich", "schick\u00b7te", "mich", "in", "die\u00b7se", "schrei\u00b7be\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die feder ist mir angebohren.", "tokens": ["Die", "fe\u00b7der", "ist", "mir", "an\u00b7ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Drum gieb mir Delie, bey dir doch dieses frey,", "tokens": ["Drum", "gieb", "mir", "De\u00b7lie", ",", "bey", "dir", "doch", "die\u00b7ses", "frey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NE", "$,", "APPR", "PPER", "ADV", "PDAT", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Zu dem mich selbst hat die natur erkohren.", "tokens": ["Zu", "dem", "mich", "selbst", "hat", "die", "na\u00b7tur", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}