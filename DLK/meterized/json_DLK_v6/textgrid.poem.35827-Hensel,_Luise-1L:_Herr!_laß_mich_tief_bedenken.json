{"textgrid.poem.35827": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr! la\u00df mich tief bedenken", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! la\u00df mich tief bedenken", "tokens": ["Herr", "!", "la\u00df", "mich", "tief", "be\u00b7den\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVIMP", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Lieben, Deinen Tod,", "tokens": ["Dein", "Lie\u00b7ben", ",", "Dei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sinne mein versenken", "tokens": ["Die", "Sin\u00b7ne", "mein", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Deine Wunden roth!", "tokens": ["In", "Dei\u00b7ne", "Wun\u00b7den", "roth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mag bleichen und verschwinden", "tokens": ["Mag", "blei\u00b7chen", "und", "ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Lebens Flitterzier;", "tokens": ["Des", "Le\u00b7bens", "Flit\u00b7ter\u00b7zier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie soll mein Herz nicht binden,", "tokens": ["Sie", "soll", "mein", "Herz", "nicht", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du, Jesus! stehst bei mir.", "tokens": ["Du", ",", "Je\u00b7sus", "!", "stehst", "bei", "mir", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$.", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Giebst Du mir Ehr' und Freuden,", "tokens": ["Giebst", "Du", "mir", "Ehr'", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So halte mich Dir treu;", "tokens": ["So", "hal\u00b7te", "mich", "Dir", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Giebst Du mir Schmach und Leiden,", "tokens": ["Giebst", "Du", "mir", "Schmach", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So steh' mir siegend bei.", "tokens": ["So", "steh'", "mir", "sie\u00b7gend", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr! la\u00df mich Dich nur finden,", "tokens": ["Herr", "!", "la\u00df", "mich", "Dich", "nur", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieh ganz mich hin zu Dir;", "tokens": ["Zieh", "ganz", "mich", "hin", "zu", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst Du Dich mir verbinden,", "tokens": ["Willst", "Du", "Dich", "mir", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Was gilt die Erde mir!", "tokens": ["Was", "gilt", "die", "Er\u00b7de", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "La\u00df ", "tokens": ["La\u00df"], "token_info": ["word"], "pos": ["VVIMP"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ich wei\u00df, Herr! ", "tokens": ["Ich", "wei\u00df", ",", "Herr", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Nichts soll von Dir mich treiben,", "tokens": ["Nichts", "soll", "von", "Dir", "mich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein will ich ewig sein. \u2013", "tokens": ["Dein", "will", "ich", "e\u00b7wig", "sein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O wann \u2013 wann kommt die Stunde,", "tokens": ["O", "wann", "\u2013", "wann", "kommt", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "$(", "PWAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da Dich mein Auge sieht,", "tokens": ["Da", "Dich", "mein", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da vom erbla\u00dften Munde", "tokens": ["Da", "vom", "er\u00b7bla\u00df\u00b7ten", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der letzte Hauch entflieht? \u2013", "tokens": ["Der", "letz\u00b7te", "Hauch", "ent\u00b7flieht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich kenne Deine Treue:", "tokens": ["Ich", "ken\u00b7ne", "Dei\u00b7ne", "Treu\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Einst werd' ich Dir vereint,", "tokens": ["Einst", "werd'", "ich", "Dir", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ich in Lieb' und Reue", "tokens": ["Wenn", "ich", "in", "Lieb'", "und", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier meine Schuld beweint.", "tokens": ["Hier", "mei\u00b7ne", "Schuld", "be\u00b7weint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich kenne Dein Erbarmen:", "tokens": ["Ich", "ken\u00b7ne", "Dein", "Er\u00b7bar\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es wird die Zeit erf\u00fcllt,", "tokens": ["Es", "wird", "die", "Zeit", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sich in Deinen Armen", "tokens": ["Da", "sich", "in", "Dei\u00b7nen", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein banges Sehnen stillt.", "tokens": ["Mein", "ban\u00b7ges", "Seh\u00b7nen", "stillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und qu\u00e4lt mich rastlos Sehnen,", "tokens": ["Und", "qu\u00e4lt", "mich", "rast\u00b7los", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dr\u00fcckt schwer des Lebens Joch,", "tokens": ["Dr\u00fcckt", "schwer", "des", "Le\u00b7bens", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So fragen stille Thr\u00e4nen:", "tokens": ["So", "fra\u00b7gen", "stil\u00b7le", "Thr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Herr! wie lange noch?", "tokens": ["O", "Herr", "!", "wie", "lan\u00b7ge", "noch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWAV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}