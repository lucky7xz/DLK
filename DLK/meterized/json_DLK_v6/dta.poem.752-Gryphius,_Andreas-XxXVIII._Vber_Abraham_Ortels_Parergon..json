{"dta.poem.752": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.  Vber  Abraham Ortels Parergon.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Den Schaw-platz alter Welt/ in welchem noch zu finden ", "tokens": ["Den", "Scha\u00b7wplatz", "al\u00b7ter", "Welt", "/", "in", "wel\u00b7chem", "noch", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "APPR", "PRELS", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was harter flam\u0303en grim/ vnd rawer Feinde schwerdt", "tokens": ["Was", "har\u00b7ter", "flam\u0303en", "grim", "/", "vnd", "ra\u00b7wer", "Fein\u00b7de", "schwerdt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VVINF", "VVFIN", "$(", "KON", "ADJA", "NN", "NE"], "meter": "-+------+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Was der geschwinde Plitz/ vnd lange zeit verkehrt/", "tokens": ["Was", "der", "ge\u00b7schwin\u00b7de", "Plitz", "/", "vnd", "lan\u00b7ge", "zeit", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schleust Ortels Handt hier auff. ", "tokens": ["Schleust", "Or\u00b7tels", "Handt", "hier", "auff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "schwinden", "tokens": ["schwin\u00b7den"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Bricht Pergamus schon eyn; ob die von stoltzen Winden", "tokens": ["Bricht", "Per\u00b7ga\u00b7mus", "schon", "eyn", ";", "ob", "die", "von", "stolt\u00b7zen", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "KOUS", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hoch aufgeschwellte See/ weit vber ", "tokens": ["Hoch", "auf\u00b7ge\u00b7schwell\u00b7te", "See", "/", "weit", "vber"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$(", "ADJD", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird von der Erden schlundt die Erden selbst verzehrt/ ", "tokens": ["Wird", "von", "der", "Er\u00b7den", "schlundt", "die", "Er\u00b7den", "selbst", "ver\u00b7zehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist doch ein freyer sinn durch keine macht zu binden. ", "tokens": ["Ist", "doch", "ein", "frey\u00b7er", "sinn", "durch", "kei\u00b7ne", "macht", "zu", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er reist die schrancken durch/ in den jhn fleisch vnd noht ", "tokens": ["Er", "reist", "die", "schran\u00b7cken", "durch", "/", "in", "den", "jhn", "fleisch", "vnd", "noht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "APPR", "$(", "APPR", "ART", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd sterben pochen will/ vnd pocht den blassen Todt.", "tokens": ["Vnd", "ster\u00b7ben", "po\u00b7chen", "will", "/", "vnd", "pocht", "den", "blas\u00b7sen", "Todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVINF", "VMFIN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Findt alles in sich selbst/ vnd findt sich selbst in allen.", "tokens": ["Findt", "al\u00b7les", "in", "sich", "selbst", "/", "vnd", "findt", "sich", "selbst", "in", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PRF", "ADV", "$(", "KON", "VVFIN", "PRF", "ADV", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er siht was nicht mehr ist/ vnd was noch kommen soll:", "tokens": ["Er", "siht", "was", "nicht", "mehr", "ist", "/", "vnd", "was", "noch", "kom\u00b7men", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PTKNEG", "ADV", "VAFIN", "$(", "KON", "PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhm ist im vntergang vnd weh der Erden woll.", "tokens": ["Jhm", "ist", "im", "vn\u00b7ter\u00b7gang", "vnd", "weh", "der", "Er\u00b7den", "woll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "KON", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd kan ob gleich der Leib/ sein wohnhau\u00df f\u00e4lt/ nicht fallen.", "tokens": ["Vnd", "kan", "ob", "gleich", "der", "Leib", "/", "sein", "wohn\u00b7hau\u00df", "f\u00e4lt", "/", "nicht", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KOUS", "ADV", "ART", "NN", "$(", "PPOSAT", "NN", "VVFIN", "$(", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}