{"textgrid.poem.60049": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdie ", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdie ", "tokens": ["\u00bb", "die"], "token_info": ["punct", "word"], "pos": ["$(", "ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Vermag, in deinen Reiz versenkt,", "tokens": ["Ver\u00b7mag", ",", "in", "dei\u00b7nen", "Reiz", "ver\u00b7senkt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die rohen Sinnlichkeiten", "tokens": ["Die", "ro\u00b7hen", "Sinn\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht l\u00e4nger zu bestreiten.\u00ab", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "zu", "be\u00b7strei\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ferner bittet eben derselbe Dichter in seinem trefflichen Nachtw\u00e4chter-Liede:", "tokens": ["Fer\u00b7ner", "bit\u00b7tet", "e\u00b7ben", "der\u00b7sel\u00b7be", "Dich\u00b7ter", "in", "sei\u00b7nem", "treff\u00b7li\u00b7chen", "Nacht\u00b7w\u00e4ch\u00b7ter\u00b7Lie\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-++-+--+-+--+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbdas Wasser, alter Weisen Trank,", "tokens": ["\u00bb", "das", "Was\u00b7ser", ",", "al\u00b7ter", "Wei\u00b7sen", "Trank", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb unsern jungen Weisen;", "tokens": ["Gieb", "un\u00b7sern", "jun\u00b7gen", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jage den ", "tokens": ["Und", "ja\u00b7ge", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Von freudenvollen Schm\u00e4usen.\u00ab", "tokens": ["Von", "freu\u00b7den\u00b7vol\u00b7len", "Schm\u00e4u\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ohne Zweifel kennen Sie auch die Jungfer ", "tokens": ["Oh\u00b7ne", "Zwei\u00b7fel", "ken\u00b7nen", "Sie", "auch", "die", "Jung\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbsie neigt sich artig, und steht da;", "tokens": ["\u00bb", "sie", "neigt", "sich", "ar\u00b7tig", ",", "und", "steht", "da", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PRF", "ADJD", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sagt aufs H\u00f6chste: ", "tokens": ["Und", "sagt", "aufs", "H\u00f6chs\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ach! sie ist noch ", "tokens": ["Ach", "!", "sie", "ist", "noch"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wahrhaftig das ist Schade!\u00ab", "tokens": ["Wahr\u00b7haf\u00b7tig", "das", "ist", "Scha\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "PDS", "VAFIN", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}