{"dta.poem.9571": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich mahlen lie\u00df/ und es an rother  \n farbe gebrach.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "tr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was? k\u00fcnstler/ fehlt es dir an farben zu dem munde?", "tokens": ["Was", "?", "k\u00fcnst\u00b7ler", "/", "fehlt", "es", "dir", "an", "far\u00b7ben", "zu", "dem", "mun\u00b7de", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "ADJD", "$(", "VVFIN", "PPER", "PPER", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So rieff ich: als zugleich Aurora sich erbot", "tokens": ["So", "rieff", "ich", ":", "als", "zu\u00b7gleich", "Au\u00b7ro\u00b7ra", "sich", "er\u00b7bot"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KOUS", "ADV", "NE", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit morgen-strahlen ihm zu helffen aus der noth.", "tokens": ["Mit", "mor\u00b7gen\u00b7strah\u00b7len", "ihm", "zu", "helf\u00b7fen", "aus", "der", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man sah\u2019 wie Flora sich mit rosen fertig funde:", "tokens": ["Man", "sah'", "wie", "Flo\u00b7ra", "sich", "mit", "ro\u00b7sen", "fer\u00b7tig", "fun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "NN", "PRF", "APPR", "ADJA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Thetis holete corallen aus dem grunde:", "tokens": ["Die", "The\u00b7tis", "ho\u00b7le\u00b7te", "co\u00b7ral\u00b7len", "aus", "dem", "grun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die m\u00fcde sonne gab von ihrem abend-roth:", "tokens": ["Die", "m\u00fc\u00b7de", "son\u00b7ne", "gab", "von", "ih\u00b7rem", "a\u00b7ben\u00b7droth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die purpur-schnecke gieng freywillig in den todt/", "tokens": ["Die", "pur\u00b7pur\u00b7schne\u00b7cke", "gieng", "frey\u00b7wil\u00b7lig", "in", "den", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und opfferte ihr blut noch zu derselben stunde.", "tokens": ["Und", "opf\u00b7fer\u00b7te", "ihr", "blut", "noch", "zu", "der\u00b7sel\u00b7ben", "stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PDAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die berge lieferten den schimmer von rubinen:", "tokens": ["Die", "ber\u00b7ge", "lie\u00b7fer\u00b7ten", "den", "schim\u00b7mer", "von", "ru\u00b7bi\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "APPR", "VVINF", "$."], "meter": "+--++--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Und die granate kam mit ihrem safft zu dienen/", "tokens": ["Und", "die", "gra\u00b7na\u00b7te", "kam", "mit", "ih\u00b7rem", "safft", "zu", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie stellten alle sich dem k\u00fcnstler selbst zur hand;", "tokens": ["Sie", "stell\u00b7ten", "al\u00b7le", "sich", "dem", "k\u00fcnst\u00b7ler", "selbst", "zur", "hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "ART", "ADJA", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich aber lie\u00df hierbey mein rothes hertz erblicken/", "tokens": ["Ich", "a\u00b7ber", "lie\u00df", "hier\u00b7bey", "mein", "ro\u00b7thes", "hertz", "er\u00b7bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und sprach: hier findestu glut/ flammen/ feur und brandt/", "tokens": ["Und", "sprach", ":", "hier", "fin\u00b7des\u00b7tu", "glut", "/", "flam\u00b7men", "/", "feur", "und", "brandt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "VVFIN", "NN", "$(", "VVINF", "$(", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Di\u00df wird am besten sich zu deinen farben schicken.", "tokens": ["Di\u00df", "wird", "am", "bes\u00b7ten", "sich", "zu", "dei\u00b7nen", "far\u00b7ben", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKA", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}