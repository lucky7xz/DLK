{"dta.poem.6210": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Reflexion sur l\u2019Ordre et la  \n Duree du Monde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es theilet auf der Welt sich allemal", "tokens": ["Es", "thei\u00b7let", "auf", "der", "Welt", "sich", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Jahr-und Tages-Zeit, nachdem der Sonnen-Strahl,", "tokens": ["Die", "Jahr\u00b7\u00b7und", "Ta\u00b7ges\u00b7Zeit", ",", "nach\u00b7dem", "der", "Son\u00b7nen\u00b7Strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So aus dem Wasser scheint zu eilen,", "tokens": ["So", "aus", "dem", "Was\u00b7ser", "scheint", "zu", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich uns zu n\u00e4hern scheint, und wiederum zu weichen.", "tokens": ["Sich", "uns", "zu", "n\u00e4\u00b7hern", "scheint", ",", "und", "wie\u00b7de\u00b7rum", "zu", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn sie entfernet ist, verschmachten und verbleichen", "tokens": ["Wenn", "sie", "ent\u00b7fer\u00b7net", "ist", ",", "ver\u00b7schmach\u00b7ten", "und", "ver\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "VAFIN", "$,", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Dinge, die wir sehn. Nah\u2019t man sich aber ihr,", "tokens": ["Die", "Din\u00b7ge", ",", "die", "wir", "sehn", ".", "Nah't", "man", "sich", "a\u00b7ber", "ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$.", "VVFIN", "PIS", "PRF", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So leben sie und bl\u00fchn in sch\u00f6nster Zier:", "tokens": ["So", "le\u00b7ben", "sie", "und", "bl\u00fchn", "in", "sch\u00f6ns\u00b7ter", "Zier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und in der gantzen Welt entsteht und h\u00e4ngt allein", "tokens": ["Und", "in", "der", "gant\u00b7zen", "Welt", "ent\u00b7steht", "und", "h\u00e4ngt", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Blo\u00df die Bewegung ab vom Sonnen-Schein.", "tokens": ["Blo\u00df", "die", "Be\u00b7we\u00b7gung", "ab", "vom", "Son\u00b7nen\u00b7Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}