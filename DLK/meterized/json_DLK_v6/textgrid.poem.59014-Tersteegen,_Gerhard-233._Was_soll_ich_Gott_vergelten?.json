{"textgrid.poem.59014": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "233. Was soll ich Gott vergelten?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott hat dich ewiglich erw\u00e4hlet,", "tokens": ["Gott", "hat", "dich", "e\u00b7wig\u00b7lich", "er\u00b7w\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erw\u00e4hl ihn ewig wiederum;", "tokens": ["Er\u00b7w\u00e4hl", "ihn", "e\u00b7wig", "wie\u00b7de\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs Kreuz bist du ihm teu'r verm\u00e4hlet,", "tokens": ["Durchs", "Kreuz", "bist", "du", "ihm", "teu'r", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Also werd auch sein Eigentum!", "tokens": ["Al\u00b7so", "werd", "auch", "sein", "Ei\u00b7gen\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}