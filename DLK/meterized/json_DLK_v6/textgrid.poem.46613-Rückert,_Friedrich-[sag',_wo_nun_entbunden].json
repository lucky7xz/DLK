{"textgrid.poem.46613": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sag', wo nun entbunden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag', wo nun entbunden", "tokens": ["Sag'", ",", "wo", "nun", "ent\u00b7bun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Geht dein Himmelsflug,", "tokens": ["Geht", "dein", "Him\u00b7mels\u00b7flug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob er ist verschwunden", "tokens": ["Ob", "er", "ist", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jener Schmerzenszug,", "tokens": ["Je\u00b7ner", "Schmer\u00b7zens\u00b7zug", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Den umher am Munde", "tokens": ["Den", "um\u00b7her", "am", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKVZ", "APPRART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "In der letzten Stunde", "tokens": ["In", "der", "letz\u00b7ten", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hier dein Antlitz trug?", "tokens": ["Hier", "dein", "Ant\u00b7litz", "trug", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hielt mein Aug' umwunden", "tokens": ["Hielt", "mein", "Aug'", "um\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nur ein Sinnentrug?", "tokens": ["Nur", "ein", "Sin\u00b7nen\u00b7trug", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "O wie er mir Wunden", "tokens": ["O", "wie", "er", "mir", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In die Seele schlug!", "tokens": ["In", "die", "See\u00b7le", "schlug", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ja, du hast empfunden", "tokens": ["Ja", ",", "du", "hast", "emp\u00b7fun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Kurzer Lebensstunden", "tokens": ["Kur\u00b7zer", "Le\u00b7bens\u00b7stun\u00b7den"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Langen Schmerz genug.", "tokens": ["Lan\u00b7gen", "Schmerz", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Doch nun gib mir Kunde,", "tokens": ["Doch", "nun", "gib", "mir", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Du so sch\u00f6n als klug:", "tokens": ["Du", "so", "sch\u00f6n", "als", "klug", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hast du \u00fcberwunden,", "tokens": ["Hast", "du", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Was vom bittern Krug", "tokens": ["Was", "vom", "bit\u00b7tern", "Krug"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPRART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Du, um zu gesunden,", "tokens": ["Du", ",", "um", "zu", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Trankst, und ist verschwunden", "tokens": ["Trankst", ",", "und", "ist", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VAFIN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nun der Schmerzenszug?", "tokens": ["Nun", "der", "Schmer\u00b7zens\u00b7zug", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}