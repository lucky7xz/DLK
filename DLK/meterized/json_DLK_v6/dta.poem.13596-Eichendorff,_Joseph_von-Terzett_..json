{"dta.poem.13596": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es blitzt von fern, die Heimchen St\u00e4ndchen bringen,", "tokens": ["Es", "blitzt", "von", "fern", ",", "die", "Heim\u00b7chen", "St\u00e4nd\u00b7chen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und unter Bl\u00fcthen, die im Wind sich r\u00fchren,", "tokens": ["Und", "un\u00b7ter", "Bl\u00fc\u00b7then", ",", "die", "im", "Wind", "sich", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die M\u00e4dchen plaudernd sitzen vor den Th\u00fcren;", "tokens": ["Die", "M\u00e4d\u00b7chen", "plau\u00b7dernd", "sit\u00b7zen", "vor", "den", "Th\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da la\u00df' ich meine Fl\u00f6te drein erklingen.", "tokens": ["Da", "la\u00df'", "ich", "mei\u00b7ne", "Fl\u00f6\u00b7te", "drein", "er\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ringsum durch die laue Sommernacht", "tokens": ["Da\u00df", "ring\u00b7sum", "durch", "die", "lau\u00b7e", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Fels und Brust der Wiederhall erwacht.", "tokens": ["In", "Fels", "und", "Brust", "der", "Wie\u00b7der\u00b7hall", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}