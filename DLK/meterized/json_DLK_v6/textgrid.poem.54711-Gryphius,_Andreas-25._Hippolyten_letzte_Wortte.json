{"textgrid.poem.54711": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "25. Hippolyten letzte Wortte", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als uns ", "tokens": ["Als", "uns"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Und den zwar schnellen Tod doch ohn entsetzen k\u00fcste:", "tokens": ["Und", "den", "zwar", "schnel\u00b7len", "Tod", "doch", "ohn", "ent\u00b7set\u00b7zen", "k\u00fcs\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "ADV", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sprach sie; Mich hat f\u00fcrwahr der Hintrit nicht betr\u00fcbet/", "tokens": ["Sprach", "sie", ";", "Mich", "hat", "f\u00fcr\u00b7wahr", "der", "Hin\u00b7trit", "nicht", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df kr\u00e4nckt mich/ da\u00df er die kr\u00e4nckt/ die ich h\u00f6chst gelibet.", "tokens": ["Di\u00df", "kr\u00e4nckt", "mich", "/", "da\u00df", "er", "die", "kr\u00e4nckt", "/", "die", "ich", "h\u00f6chst", "ge\u00b7li\u00b7bet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ART", "VVFIN", "$(", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}