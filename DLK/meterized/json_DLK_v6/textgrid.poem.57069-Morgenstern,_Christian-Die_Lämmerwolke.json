{"textgrid.poem.57069": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die L\u00e4mmerwolke", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es bl\u00f6kt eine L\u00e4mmerwolke", "tokens": ["Es", "bl\u00f6kt", "ei\u00b7ne", "L\u00e4m\u00b7mer\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "am blauen Firmament,", "tokens": ["am", "blau\u00b7en", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sie bl\u00f6kt nach ihrem Volke,", "tokens": ["sie", "bl\u00f6kt", "nach", "ih\u00b7rem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "das sich von ihr getrennt.", "tokens": ["das", "sich", "von", "ihr", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zu Bomst das Luftschiff \u00bbGunther\u00ab", "tokens": ["Zu", "Bomst", "das", "Luft\u00b7schiff", "\u00bb", "Gun\u00b7ther", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$(", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "vernimmt's und f\u00e4hrt empor", "tokens": ["ver\u00b7nimmt's", "und", "f\u00e4hrt", "em\u00b7por"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und bringt die Gute herunter,", "tokens": ["und", "bringt", "die", "Gu\u00b7te", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die, ach, so viel verlor.", "tokens": ["die", ",", "ach", ",", "so", "viel", "ver\u00b7lor", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ITJ", "$,", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bei Bomst wohl auf der Weide,", "tokens": ["Bei", "Bomst", "wohl", "auf", "der", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "da schwebt sie nun voll Dank,", "tokens": ["da", "schwebt", "sie", "nun", "voll", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "drei Jungfraun in wei\u00dfem Kleide,", "tokens": ["drei", "Jung\u00b7fraun", "in", "wei\u00b7\u00dfem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die bringen ihr Speis und Trank.", "tokens": ["die", "brin\u00b7gen", "ihr", "Speis", "und", "Trank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch als der Morgen gekommen,", "tokens": ["Doch", "als", "der", "Mor\u00b7gen", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "der n\u00e4chste Morgen bei Bomst,", "tokens": ["der", "n\u00e4chs\u00b7te", "Mor\u00b7gen", "bei", "Bomst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "da war sie nach Schrimm verschwommen,", "tokens": ["da", "war", "sie", "nach", "Schrimm", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NE", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "wohin du von Bomst aus kommst ...", "tokens": ["wo\u00b7hin", "du", "von", "Bomst", "aus", "kommst", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "APPR", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}