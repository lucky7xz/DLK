{"dta.poem.8174": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Kleine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Sie halberschlossen nur dem Leben,", "tokens": ["Sie", "hal\u00b7ber\u00b7schlos\u00b7sen", "nur", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich zwar nicht alt, doch aber dort,", "tokens": ["Ich", "zwar", "nicht", "alt", ",", "doch", "a\u00b7ber", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo uns verl\u00e4\u00dft die Jugend eben.", "tokens": ["Wo", "uns", "ver\u00b7l\u00e4\u00dft", "die", "Ju\u00b7gend", "e\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir wandelten hinauf, hinab", "tokens": ["Wir", "wan\u00b7del\u00b7ten", "hin\u00b7auf", ",", "hin\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im d\u00e4mmergr\u00fcnen Gang der Linden;", "tokens": ["Im", "d\u00e4m\u00b7mer\u00b7gr\u00fc\u00b7nen", "Gang", "der", "Lin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sah mich froh und leuchtend an,", "tokens": ["Sie", "sah", "mich", "froh", "und", "leuch\u00b7tend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wu\u00dfte nicht, es k\u00f6nne z\u00fcnden;", "tokens": ["Sie", "wu\u00df\u00b7te", "nicht", ",", "es", "k\u00f6n\u00b7ne", "z\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr ahnte keine M\u00f6glichkeit,", "tokens": ["Ihr", "ahn\u00b7te", "kei\u00b7ne", "M\u00f6g\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Wort von so verwegnen Dingen,", "tokens": ["Kein", "Wort", "von", "so", "ver\u00b7weg\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch, es selbst die tiefste Kluft", "tokens": ["Wo\u00b7durch", ",", "es", "selbst", "die", "tiefs\u00b7te", "Kluft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verlockend wird zu \u00fcberspringen.", "tokens": ["Ver\u00b7lo\u00b7ckend", "wird", "zu", "\u00fc\u00b7bers\u00b7prin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}