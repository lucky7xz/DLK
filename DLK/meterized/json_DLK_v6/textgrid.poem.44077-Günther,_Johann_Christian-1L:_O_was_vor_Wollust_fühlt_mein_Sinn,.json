{"textgrid.poem.44077": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: O was vor Wollust f\u00fchlt mein Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O was vor Wollust f\u00fchlt mein Sinn,", "tokens": ["O", "was", "vor", "Wol\u00b7lust", "f\u00fchlt", "mein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem ich schon so w\u00fcrdig bin,", "tokens": ["Nach\u00b7dem", "ich", "schon", "so", "w\u00fcr\u00b7dig", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Warheit wegen Schmach zu leiden;", "tokens": ["Der", "War\u00b7heit", "we\u00b7gen", "Schmach", "zu", "lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es komme, was verfolgen kan", "tokens": ["Es", "kom\u00b7me", ",", "was", "ver\u00b7fol\u00b7gen", "kan"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seze mit der Marter an,", "tokens": ["Und", "se\u00b7ze", "mit", "der", "Mar\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich troze Feind und Spott mit Freuden.", "tokens": ["Ich", "tro\u00b7ze", "Feind", "und", "Spott", "mit", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von nun an soll mein Flei\u00df nicht ruhn,", "tokens": ["Von", "nun", "an", "soll", "mein", "Flei\u00df", "nicht", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APZR", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "So viel nur m\u00f6glich recht zu thun,", "tokens": ["So", "viel", "nur", "m\u00f6g\u00b7lich", "recht", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn solche Lust daraus entspringet.", "tokens": ["Wenn", "sol\u00b7che", "Lust", "da\u00b7raus", "ent\u00b7sprin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Saz: Ich bin mir nichts bewust,", "tokens": ["Der", "Saz", ":", "Ich", "bin", "mir", "nichts", "be\u00b7wust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der ist ein Felsen meiner Brust,", "tokens": ["Der", "ist", "ein", "Fel\u00b7sen", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wovon mich keine Welle dringet.", "tokens": ["Wo\u00b7von", "mich", "kei\u00b7ne", "Wel\u00b7le", "drin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier schw\u00f6r ich, gro\u00dfer Gott, vor dir:", "tokens": ["Hier", "schw\u00f6r", "ich", ",", "gro\u00b7\u00dfer", "Gott", ",", "vor", "dir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(dein Ohr und Auge zeuge mir,)", "tokens": ["(", "dein", "Ohr", "und", "Au\u00b7ge", "zeu\u00b7ge", "mir", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab ein Herz voll Schwachheitss\u00fcnden,", "tokens": ["Ich", "hab", "ein", "Herz", "voll", "Schwach\u00b7heits\u00b7s\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wirstu solches auch voll Reu,", "tokens": ["Doch", "wirs\u00b7tu", "sol\u00b7ches", "auch", "voll", "Reu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADJD", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ohn Eigennuz und Heucheley", "tokens": ["Ohn", "Ei\u00b7gen\u00b7nuz", "und", "Heu\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gegen jeden redlich finden.", "tokens": ["Und", "ge\u00b7gen", "je\u00b7den", "red\u00b7lich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das ist, ich merck und f\u00fchl in mir", "tokens": ["Das", "ist", ",", "ich", "merck", "und", "f\u00fchl", "in", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PPER", "NN", "KON", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Neigung sehnlicher Begier,", "tokens": ["Die", "Nei\u00b7gung", "sehn\u00b7li\u00b7cher", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit allen Menschen gut zu leben;", "tokens": ["Mit", "al\u00b7len", "Men\u00b7schen", "gut", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00e4r es mir anheim gestellt,", "tokens": ["Und", "w\u00e4r", "es", "mir", "an\u00b7heim", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wollt ich jedem in der Welt", "tokens": ["So", "wollt", "ich", "je\u00b7dem", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das gl\u00fccklichste Verh\u00e4ngn\u00fc\u00df geben.", "tokens": ["Das", "gl\u00fcck\u00b7lichs\u00b7te", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gott, rechne mir zu deiner Zeit", "tokens": ["Gott", ",", "rech\u00b7ne", "mir", "zu", "dei\u00b7ner", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Vorsaz zur Gerechtigkeit", "tokens": ["Den", "Vor\u00b7saz", "zur", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00e4rck ihn durch den wahren Glauben", "tokens": ["Und", "st\u00e4rck", "ihn", "durch", "den", "wah\u00b7ren", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und las ihn meine Tr\u00f6stung seyn,", "tokens": ["Und", "las", "ihn", "mei\u00b7ne", "Tr\u00f6s\u00b7tung", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Unruh, Jammer, Angst und Pein", "tokens": ["Wenn", "Un\u00b7ruh", ",", "Jam\u00b7mer", ",", "Angst", "und", "Pein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir alle Lust auf Erden rauben.", "tokens": ["Mir", "al\u00b7le", "Lust", "auf", "Er\u00b7den", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dies, was ich oft aus Schwachheit thu,", "tokens": ["Dies", ",", "was", "ich", "oft", "aus", "Schwach\u00b7heit", "thu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das decke mit der Liebe zu,", "tokens": ["Das", "de\u00b7cke", "mit", "der", "Lie\u00b7be", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Feind und Neid erkennen m\u00fc\u00dfen.", "tokens": ["Die", "Feind", "und", "Neid", "er\u00b7ken\u00b7nen", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Leben f\u00fchr ich k\u00fcmmerlich;", "tokens": ["Mein", "Le\u00b7ben", "f\u00fchr", "ich", "k\u00fcm\u00b7mer\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wilst so, ich bescheide mich,", "tokens": ["Du", "wilst", "so", ",", "ich", "be\u00b7schei\u00b7de", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erhalt mir nur ein gut Gewi\u00dfen.", "tokens": ["Er\u00b7halt", "mir", "nur", "ein", "gut", "Ge\u00b7wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}