{"textgrid.poem.47284": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der betrogene Teufel", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Araber hatten ihr Feld bestellt,", "tokens": ["Die", "A\u00b7ra\u00b7ber", "hat\u00b7ten", "ihr", "Feld", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da kam der Teufel herbei in Eil';", "tokens": ["Da", "kam", "der", "Teu\u00b7fel", "her\u00b7bei", "in", "Eil'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er sprach: \u00bbMir geh\u00f6rt die halbe Welt,", "tokens": ["Er", "sprach", ":", "\u00bb", "Mir", "ge\u00b7h\u00f6rt", "die", "hal\u00b7be", "Welt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich will auch von euerer Ernte mein Teil.\u00ab", "tokens": ["Ich", "will", "auch", "von", "eu\u00b7e\u00b7rer", "Ern\u00b7te", "mein", "Teil", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Die Araber aber sind F\u00fcchse von Haus,", "tokens": ["Die", "A\u00b7ra\u00b7ber", "a\u00b7ber", "sind", "F\u00fcch\u00b7se", "von", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie sprachen: \u00bbDie untere H\u00e4lfte sei dein.\u00ab", "tokens": ["Sie", "spra\u00b7chen", ":", "\u00bb", "Die", "un\u00b7te\u00b7re", "H\u00e4lf\u00b7te", "sei", "dein", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Teufel will allzeit oben hinaus;", "tokens": ["Der", "Teu\u00b7fel", "will", "all\u00b7zeit", "o\u00b7ben", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbnein\u00ab, sprach er, \u00bbes soll die obere sein.\u00ab", "tokens": ["\u00bb", "nein", "\u00ab", ",", "sprach", "er", ",", "\u00bb", "es", "soll", "die", "o\u00b7be\u00b7re", "sein", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "PPER", "VMFIN", "ART", "ADJA", "VAINF", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Da bauten sie R\u00fcben in Einem Strich;", "tokens": ["Da", "bau\u00b7ten", "sie", "R\u00fc\u00b7ben", "in", "Ei\u00b7nem", "Strich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und als es nun an die Teilung ging,", "tokens": ["Und", "als", "es", "nun", "an", "die", "Tei\u00b7lung", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Araber nahmen die Wurzeln f\u00fcr sich,", "tokens": ["Die", "A\u00b7ra\u00b7ber", "nah\u00b7men", "die", "Wur\u00b7zeln", "f\u00fcr", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PRF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Teufel die gelben Bl\u00e4tter empfing.", "tokens": ["Der", "Teu\u00b7fel", "die", "gel\u00b7ben", "Bl\u00e4t\u00b7ter", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und als es wiederum ging ins Jahr,", "tokens": ["Und", "als", "es", "wie\u00b7de\u00b7rum", "ging", "ins", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da sprach der Teufel im hellen Zorn:", "tokens": ["Da", "sprach", "der", "Teu\u00b7fel", "im", "hel\u00b7len", "Zorn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbnun will ich die untere H\u00e4lfte f\u00fcrwahr.\u00ab", "tokens": ["\u00bb", "nun", "will", "ich", "die", "un\u00b7te\u00b7re", "H\u00e4lf\u00b7te", "f\u00fcr\u00b7wahr", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da bauten die Araber Weiz' und Korn.", "tokens": ["Da", "bau\u00b7ten", "die", "A\u00b7ra\u00b7ber", "Wei\u00b7z'", "und", "Korn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Und als es wieder zur Teilung kam,", "tokens": ["Und", "als", "es", "wie\u00b7der", "zur", "Tei\u00b7lung", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Araber nahmen den \u00c4hrenschnitt.", "tokens": ["Die", "A\u00b7ra\u00b7ber", "nah\u00b7men", "den", "\u00c4h\u00b7ren\u00b7schnitt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Der Teufel die leeren Stoppeln nahm", "tokens": ["Der", "Teu\u00b7fel", "die", "lee\u00b7ren", "Stop\u00b7peln", "nahm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und heizte der H\u00f6lle Ofen damit.", "tokens": ["Und", "heiz\u00b7te", "der", "H\u00f6l\u00b7le", "O\u00b7fen", "da\u00b7mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PAV", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}}}}