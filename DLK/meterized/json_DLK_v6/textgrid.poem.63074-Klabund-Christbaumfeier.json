{"textgrid.poem.63074": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Christbaumfeier", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Piano, Geige: Hupf mein M\u00e4del (forte),", "tokens": ["Pi\u00b7a\u00b7no", ",", "Gei\u00b7ge", ":", "Hupf", "mein", "M\u00e4\u00b7del", "(", "for\u00b7te", ")", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$,", "NN", "$.", "NN", "PPOSAT", "NN", "$(", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Christbaum zucken gelblich ein paar Lichter,", "tokens": ["Im", "Christ\u00b7baum", "zu\u00b7cken", "gelb\u00b7lich", "ein", "paar", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und an die Rampe tritt Kommis und Dichter", "tokens": ["Und", "an", "die", "Ram\u00b7pe", "tritt", "Kom\u00b7mis", "und", "Dich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stottert stockend tannendufte Worte.", "tokens": ["Und", "stot\u00b7tert", "sto\u00b7ckend", "tan\u00b7nen\u00b7duf\u00b7te", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Man trampelt: \u00bbBravo, Bravo\u00ab mit den F\u00fc\u00dfen", "tokens": ["Man", "tram\u00b7pelt", ":", "\u00bb", "Bra\u00b7vo", ",", "Bra\u00b7vo", "\u00ab", "mit", "den", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "$(", "ITJ", "$,", "ITJ", "$(", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und prostet mit den Kr\u00fcgen nach dem Helden,", "tokens": ["Und", "pros\u00b7tet", "mit", "den", "Kr\u00fc\u00b7gen", "nach", "dem", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Indem sich schon zwei wei\u00dfe Fr\u00e4uleins melden,", "tokens": ["In\u00b7dem", "sich", "schon", "zwei", "wei\u00b7\u00dfe", "Fr\u00e4u\u00b7leins", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit \u00bbStille Nacht\u00ab die Menge zu begr\u00fc\u00dfen.", "tokens": ["Mit", "\u00bb", "Stil\u00b7le", "Nacht", "\u00ab", "die", "Men\u00b7ge", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$(", "NN", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Man s\u00e4uft, man schreit, man giert und man verlost", "tokens": ["Man", "s\u00e4uft", ",", "man", "schreit", ",", "man", "giert", "und", "man", "ver\u00b7lost"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Lebenslust \u2013 Rosa, unwiderstehlich,", "tokens": ["Die", "Le\u00b7bens\u00b7lust", "\u2013", "Ro\u00b7sa", ",", "un\u00b7wi\u00b7der\u00b7steh\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "$,", "ADJD", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Bringt l\u00e4chelnd ihrem Buben bei (allm\u00e4hlich),", "tokens": ["Bringt", "l\u00e4\u00b7chelnd", "ih\u00b7rem", "Bu\u00b7ben", "bei", "(", "all\u00b7m\u00e4h\u00b7lich", ")", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "APPR", "$(", "ADJD", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df er mich Papa ruft. \u2013 Na danke. Prost.", "tokens": ["Da\u00df", "er", "mich", "Pa\u00b7pa", "ruft", ".", "\u2013", "Na", "dan\u00b7ke", ".", "Prost", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "VVFIN", "$.", "$(", "ITJ", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}