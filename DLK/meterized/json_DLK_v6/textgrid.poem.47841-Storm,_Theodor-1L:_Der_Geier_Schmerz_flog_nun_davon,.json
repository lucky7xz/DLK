{"textgrid.poem.47841": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Geier Schmerz flog nun davon,", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Geier Schmerz flog nun davon,", "tokens": ["Der", "Gei\u00b7er", "Schmerz", "flog", "nun", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die St\u00e4tte, wo er sa\u00df, ist leer;", "tokens": ["Die", "St\u00e4t\u00b7te", ",", "wo", "er", "sa\u00df", ",", "ist", "leer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur unten tief in meiner Brust", "tokens": ["Nur", "un\u00b7ten", "tief", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Regt sich noch etwas, dumpf und schwer.", "tokens": ["Regt", "sich", "noch", "et\u00b7was", ",", "dumpf", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PIS", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Das ist die Sehnsucht, die mit Qual", "tokens": ["Das", "ist", "die", "Sehn\u00b7sucht", ",", "die", "mit", "Qual"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um deine holde N\u00e4he wirbt,", "tokens": ["Um", "dei\u00b7ne", "hol\u00b7de", "N\u00e4\u00b7he", "wirbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, eh sie noch das Herz erreicht,", "tokens": ["Doch", ",", "eh", "sie", "noch", "das", "Herz", "er\u00b7reicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mutlos die Fl\u00fcgel senkt und stirbt.", "tokens": ["Mut\u00b7los", "die", "Fl\u00fc\u00b7gel", "senkt", "und", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}