{"dta.poem.15972": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas machst du an der Welt? sie ist bereits gemacht.\u201c", "tokens": ["\u201e", "was", "machst", "du", "an", "der", "Welt", "?", "sie", "ist", "be\u00b7reits", "ge\u00b7macht", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um deine Freiheit hat dich dieser Spruch gebracht.", "tokens": ["Um", "dei\u00b7ne", "Frei\u00b7heit", "hat", "dich", "die\u00b7ser", "Spruch", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja, fertig wenn die Welt gemacht w\u00e4r' und vollendet,", "tokens": ["Ja", ",", "fer\u00b7tig", "wenn", "die", "Welt", "ge\u00b7macht", "w\u00e4r'", "und", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "KOUS", "ART", "NN", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verloren w\u00e4r' an ihr dein Ringen und verschwendet.", "tokens": ["Ver\u00b7lo\u00b7ren", "w\u00e4r'", "an", "ihr", "dein", "Rin\u00b7gen", "und", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "APPR", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch sie ist nicht gemacht, du sollst sie helfen machen,", "tokens": ["Doch", "sie", "ist", "nicht", "ge\u00b7macht", ",", "du", "sollst", "sie", "hel\u00b7fen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,", "PPER", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dazu hat die Kraft dir Gott verliehn, dem schwachen.", "tokens": ["Und", "da\u00b7zu", "hat", "die", "Kraft", "dir", "Gott", "ver\u00b7liehn", ",", "dem", "schwa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "ART", "NN", "PPER", "NN", "VVINF", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nicht fertig ist die Welt, sie ist im ew'gen Werden,", "tokens": ["Nicht", "fer\u00b7tig", "ist", "die", "Welt", ",", "sie", "ist", "im", "ew'\u00b7gen", "Wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihre Freiheit kann die deine nicht gef\u00e4rden.", "tokens": ["Und", "ih\u00b7re", "Frei\u00b7heit", "kann", "die", "dei\u00b7ne", "nicht", "ge\u00b7f\u00e4r\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ART", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mit todtem R\u00e4derwerk greift sie in dich nicht ein;", "tokens": ["Mit", "tod\u00b7tem", "R\u00e4\u00b7der\u00b7werk", "greift", "sie", "in", "dich", "nicht", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du bist ein Lebenstrieb in ihr, gro\u00df oder klein.", "tokens": ["Du", "bist", "ein", "Le\u00b7ben\u00b7strieb", "in", "ihr", ",", "gro\u00df", "o\u00b7der", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie strebt nach ihrem Ziel mit aller Geister Ringen,", "tokens": ["Sie", "strebt", "nach", "ih\u00b7rem", "Ziel", "mit", "al\u00b7ler", "Geis\u00b7ter", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur wenn auch dein Geist ihr hilft, wird sie's erringen:", "tokens": ["Und", "nur", "wenn", "auch", "dein", "Geist", "ihr", "hilft", ",", "wird", "sie's", "er\u00b7rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie setzt dir Schwierigkeit entgegen zwar und Schranken;", "tokens": ["Sie", "setzt", "dir", "Schwie\u00b7rig\u00b7keit", "ent\u00b7ge\u00b7gen", "zwar", "und", "Schran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPO", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, r\u00e4umt dein Geist sie weg, so wird sie dir es danken.", "tokens": ["Doch", ",", "r\u00e4umt", "dein", "Geist", "sie", "weg", ",", "so", "wird", "sie", "dir", "es", "dan\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}