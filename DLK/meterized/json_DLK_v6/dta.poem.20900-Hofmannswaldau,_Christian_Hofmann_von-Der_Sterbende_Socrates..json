{"dta.poem.20900": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Geist der diesem Thun fast lange nachgegangen/", "tokens": ["Mein", "Geist", "der", "die\u00b7sem", "Thun", "fast", "lan\u00b7ge", "nach\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PDAT", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf dergleichen Bahn nichts rechtes kont erlangen/", "tokens": ["Und", "auf", "derg\u00b7lei\u00b7chen", "Bahn", "nichts", "rech\u00b7tes", "kont", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "NN", "PIS", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja durch den hellen Schein best\u00fcrtzet war gemacht/", "tokens": ["Ja", "durch", "den", "hel\u00b7len", "Schein", "be\u00b7st\u00fcrt\u00b7zet", "war", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der dachte/ wann die Sonn am allerh\u00f6chsten stehet/", "tokens": ["Der", "dach\u00b7te", "/", "wann", "die", "Sonn", "am", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "ste\u00b7het", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "PWAV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer ist doch wol geschickt zu schauen dessen Pracht/", "tokens": ["Wer", "ist", "doch", "wol", "ge\u00b7schickt", "zu", "schau\u00b7en", "des\u00b7sen", "Pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "VVPP", "PTKZU", "VVINF", "PRELAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem das Gesichte nicht in einem Nu vergebet?", "tokens": ["Dem", "das", "Ge\u00b7sich\u00b7te", "nicht", "in", "ei\u00b7nem", "Nu", "ver\u00b7ge\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PTKNEG", "APPR", "ART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird nicht der Sonnen Licht am besten f\u00fcrgestelt/", "tokens": ["Wird", "nicht", "der", "Son\u00b7nen", "Licht", "am", "bes\u00b7ten", "f\u00fcr\u00b7ge\u00b7stelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "NN", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann dessen sch\u00f6nes Bild in Flu\u00df und Wasser f\u00e4lt.", "tokens": ["Wann", "des\u00b7sen", "sch\u00f6\u00b7nes", "Bild", "in", "Flu\u00df", "und", "Was\u00b7ser", "f\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}