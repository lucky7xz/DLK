{"dta.poem.2040": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der L\u00f6we .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welche finstre Majest\u00e4t herrscht in diesem ganzen", "tokens": ["Wel\u00b7che", "finst\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t", "herrscht", "in", "die\u00b7sem", "gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "VVFIN", "APPR", "PDAT", "ADJA"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Thier!", "tokens": ["Thier", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Seiner Nerven, Muskeln, Knochen, riesenf\u00f6rmiges Ver-", "tokens": ["Sei\u00b7ner", "Ner\u00b7ven", ",", "Mus\u00b7keln", ",", "Kno\u00b7chen", ",", "rie\u00b7sen\u00b7f\u00f6r\u00b7mi\u00b7ges", "Ver"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "band", "tokens": ["band"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Zeiget, unter andern Thieren, seinen k\u00f6niglichen Stand.", "tokens": ["Zei\u00b7get", ",", "un\u00b7ter", "an\u00b7dern", "Thie\u00b7ren", ",", "sei\u00b7nen", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Stand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Eine Art von ernster Gro\u00dfmuth, nebst der St\u00e4rke, stellt", "tokens": ["Ei\u00b7ne", "Art", "von", "erns\u00b7ter", "Gro\u00df\u00b7muth", ",", "nebst", "der", "St\u00e4r\u00b7ke", ",", "stellt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "ihn mir", "tokens": ["ihn", "mir"], "token_info": ["word", "word"], "pos": ["PPER", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Recht als einen Herkules unter andern Thieren f\u00fcr.", "tokens": ["Recht", "als", "ei\u00b7nen", "Her\u00b7ku\u00b7les", "un\u00b7ter", "an\u00b7dern", "Thie\u00b7ren", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "APPR", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Sein best\u00e4ndiger Begleiter, ob er ihn gleich selbst nicht", "tokens": ["Sein", "be\u00b7st\u00e4n\u00b7di\u00b7ger", "Be\u00b7glei\u00b7ter", ",", "ob", "er", "ihn", "gleich", "selbst", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "ADV", "PTKNEG"], "meter": "+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.10": {"text": "kennet,", "tokens": ["ken\u00b7net", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Ist der Schrecken, welcher sich nie von seiner Seite trennet.", "tokens": ["Ist", "der", "Schre\u00b7cken", ",", "wel\u00b7cher", "sich", "nie", "von", "sei\u00b7ner", "Sei\u00b7te", "tren\u00b7net", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Welche starke Brust und Stirn, welche Muskeln, welche", "tokens": ["Wel\u00b7che", "star\u00b7ke", "Brust", "und", "Stirn", ",", "wel\u00b7che", "Mus\u00b7keln", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAT", "ADJA", "NN", "KON", "NN", "$,", "PWAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "M\u00e4hne,", "tokens": ["M\u00e4h\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Welche Kiefern, welche Tatzen, welche Klauen, welche", "tokens": ["Wel\u00b7che", "Kie\u00b7fern", ",", "wel\u00b7che", "Tat\u00b7zen", ",", "wel\u00b7che", "Klau\u00b7en", ",", "wel\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$,", "PWAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Z\u00e4hne!", "tokens": ["Z\u00e4h\u00b7ne", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Seiner festen Glieder Bau zeigt ein wahres Bild der St\u00e4rke", "tokens": ["Sei\u00b7ner", "fes\u00b7ten", "Glie\u00b7der", "Bau", "zeigt", "ein", "wah\u00b7res", "Bild", "der", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "In bewundrungswerther Gleichmaa\u00df; wovon ich die", "tokens": ["In", "be\u00b7wund\u00b7rungs\u00b7wert\u00b7her", "Gleich\u00b7maa\u00df", ";", "wo\u00b7von", "ich", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PWAV", "PPER", "ART"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.18": {"text": "Symmetrie", "tokens": ["Sym\u00b7me\u00b7trie"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Zu der Absicht eingerichtet und gewirkt, zu welcher sie", "tokens": ["Zu", "der", "Ab\u00b7sicht", "ein\u00b7ge\u00b7rich\u00b7tet", "und", "ge\u00b7wirkt", ",", "zu", "wel\u00b7cher", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "VVPP", "$,", "APPR", "PRELS", "PPER"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Ordentlich bestimmet ist, mit erstaunter Lust bemerke.", "tokens": ["Or\u00b7dent\u00b7lich", "be\u00b7stim\u00b7met", "ist", ",", "mit", "er\u00b7staun\u00b7ter", "Lust", "be\u00b7mer\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Die\u00df so wohlformirte Thier sehe denn doch jedermann", "tokens": ["Die\u00df", "so", "wohl\u00b7for\u00b7mir\u00b7te", "Thier", "se\u00b7he", "denn", "doch", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJA", "NN", "VVFIN", "ADV", "ADV", "PIS"], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Als ein pr\u00e4chtiges Gesch\u00f6pf eines weisen Sch\u00f6pfers an.", "tokens": ["Als", "ein", "pr\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7sch\u00f6pf", "ei\u00b7nes", "wei\u00b7sen", "Sch\u00f6p\u00b7fers", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}