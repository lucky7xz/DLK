{"textgrid.poem.60832": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Sankt Peter", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meister entwarfen dereinst zum sch\u00f6nsten Geb\u00e4ude der Welt mich,", "tokens": ["Meis\u00b7ter", "ent\u00b7war\u00b7fen", "de\u00b7reinst", "zum", "sch\u00f6ns\u00b7ten", "Ge\u00b7b\u00e4u\u00b7de", "der", "Welt", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "PPER", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "St\u00fcmpern erlag nachmals, plumpen Geschmacks, der Kolo\u00df:", "tokens": ["St\u00fcm\u00b7pern", "er\u00b7lag", "nach\u00b7mals", ",", "plum\u00b7pen", "Ge\u00b7schmacks", ",", "der", "Ko\u00b7lo\u00df", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "M\u00e4\u00dfige Tempel darum, nicht riesige bauten die Griechen,", "tokens": ["M\u00e4\u00b7\u00dfi\u00b7ge", "Tem\u00b7pel", "da\u00b7rum", ",", "nicht", "rie\u00b7si\u00b7ge", "bau\u00b7ten", "die", "Grie\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "$,", "PTKNEG", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wo Jahrhunderte dran st\u00fcckeln, wie kann es gedeihn?", "tokens": ["Wo", "Jahr\u00b7hun\u00b7der\u00b7te", "dran", "st\u00fc\u00b7ckeln", ",", "wie", "kann", "es", "ge\u00b7deihn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PAV", "VVINF", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}}}}}