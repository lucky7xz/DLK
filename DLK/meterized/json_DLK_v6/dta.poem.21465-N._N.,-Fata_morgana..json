{"dta.poem.21465": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fata morgana.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wie milde und s\u00fc\u00df", "tokens": ["Wie", "mil\u00b7de", "und", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Des Abends K\u00fchle", "tokens": ["Des", "A\u00b7bends", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und thaufrische Labung", "tokens": ["Und", "thauf\u00b7ri\u00b7sche", "La\u00b7bung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Herniedersinkt", "tokens": ["Her\u00b7nie\u00b7der\u00b7sinkt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Auf die hei\u00dfe d\u00fcrstende", "tokens": ["Auf", "die", "hei\u00b7\u00dfe", "d\u00fcrs\u00b7ten\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Glutschwangre Erde!", "tokens": ["Glutsc\u00b7hwang\u00b7re", "Er\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Matter gl\u00e4nzen die Tiefen", "tokens": ["Mat\u00b7ter", "gl\u00e4n\u00b7zen", "die", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "In duftwei\u00dfem Schimmer;", "tokens": ["In", "duft\u00b7wei\u00b7\u00dfem", "Schim\u00b7mer", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Nur in der Ferne", "tokens": ["Nur", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Wie stolzen Gebirges", "tokens": ["Wie", "stol\u00b7zen", "Ge\u00b7bir\u00b7ges"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Vielh\u00e4uptiges Steinmeer", "tokens": ["Viel\u00b7h\u00e4up\u00b7ti\u00b7ges", "Stein\u00b7meer"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Ragt auf der Wolken", "tokens": ["Ragt", "auf", "der", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Stummdr\u00e4uende Nebelwand.", "tokens": ["Stumm\u00b7dr\u00e4u\u00b7en\u00b7de", "Ne\u00b7bel\u00b7wand", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Immer dichter breitet", "tokens": ["Im\u00b7mer", "dich\u00b7ter", "brei\u00b7tet"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Um die d\u00e4mmernden Sinne", "tokens": ["Um", "die", "d\u00e4m\u00b7mern\u00b7den", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Mit Muttersorgfalt", "tokens": ["Mit", "Mut\u00b7ter\u00b7sorg\u00b7falt"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Ihren Traumesschleier", "tokens": ["Ih\u00b7ren", "Trau\u00b7mes\u00b7schlei\u00b7er"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Die Tr\u00f6sterin Nacht.", "tokens": ["Die", "Tr\u00f6s\u00b7te\u00b7rin", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Wie Geisterrauschen", "tokens": ["Wie", "Geis\u00b7ter\u00b7rau\u00b7schen"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Zieht\u2019s durch die L\u00fcfte,", "tokens": ["Zieht's", "durch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Sanft k\u00fc\u00dft es die K\u00f6pfchen", "tokens": ["Sanft", "k\u00fc\u00dft", "es", "die", "K\u00f6pf\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Zarthalmiger Gr\u00e4ser,", "tokens": ["Zar\u00b7thal\u00b7mi\u00b7ger", "Gr\u00e4\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Die stolzen Kronen", "tokens": ["Die", "stol\u00b7zen", "Kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Hochwipfliger B\u00e4ume,", "tokens": ["Hoch\u00b7wipf\u00b7li\u00b7ger", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Da\u00df sie leise erzittern", "tokens": ["Da\u00df", "sie", "lei\u00b7se", "er\u00b7zit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.27": {"text": "In wonnigem Beben", "tokens": ["In", "won\u00b7ni\u00b7gem", "Be\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Und fl\u00fcsternd sich neigen.", "tokens": ["Und", "fl\u00fcs\u00b7ternd", "sich", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Jeder Schmerz, jede Sehnsucht", "tokens": ["Je\u00b7der", "Schmerz", ",", "je\u00b7de", "Sehn\u00b7sucht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.30": {"text": "Der Seele verhaucht,", "tokens": ["Der", "See\u00b7le", "ver\u00b7haucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.31": {"text": "Mit dem Friedenslispeln", "tokens": ["Mit", "dem", "Frie\u00b7dens\u00b7li\u00b7speln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.32": {"text": "Der L\u00fcfte und Sterne.", "tokens": ["Der", "L\u00fcf\u00b7te", "und", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Himmel und Erde", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Umarmt sich alleins", "tokens": ["Um\u00b7armt", "sich", "al\u00b7leins"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "In dem Segenszauber", "tokens": ["In", "dem", "Se\u00b7gens\u00b7zau\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.36": {"text": "Der m\u00fcden Natur \u2026", "tokens": ["Der", "m\u00fc\u00b7den", "Na\u00b7tur", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Immer deutlicher gr\u00fc\u00dft", "tokens": ["Im\u00b7mer", "deut\u00b7li\u00b7cher", "gr\u00fc\u00dft"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.38": {"text": "Aus wachsenden Schatten", "tokens": ["Aus", "wach\u00b7sen\u00b7den", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Die heilige Schwelle", "tokens": ["Die", "hei\u00b7li\u00b7ge", "Schwel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.40": {"text": "Der Heimath.", "tokens": ["Der", "Hei\u00b7math", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.41": {"text": "Jetzt l\u00e4chle ich selig,", "tokens": ["Jetzt", "l\u00e4ch\u00b7le", "ich", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "Ein seliges Kind, ach,", "tokens": ["Ein", "se\u00b7li\u00b7ges", "Kind", ",", "ach", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ITJ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.43": {"text": "Im Schoo\u00dfe der Mutter.", "tokens": ["Im", "Schoo\u00b7\u00dfe", "der", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}