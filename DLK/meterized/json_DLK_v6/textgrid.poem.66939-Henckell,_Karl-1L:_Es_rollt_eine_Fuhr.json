{"textgrid.poem.66939": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es rollt eine Fuhr", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rollt eine Fuhr", "tokens": ["Es", "rollt", "ei\u00b7ne", "Fuhr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "\u00dcber eiserne Spur,", "tokens": ["\u00dc\u00b7ber", "ei\u00b7ser\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Eine spukhafte Fuhr in die Ferne,", "tokens": ["Ei\u00b7ne", "spuk\u00b7haf\u00b7te", "Fuhr", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Auf die Schiene bricht", "tokens": ["Auf", "die", "Schie\u00b7ne", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Das elektrische Licht,", "tokens": ["Das", "e\u00b7lekt\u00b7ri\u00b7sche", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Am Himmel verl\u00f6schen die Sterne.", "tokens": ["Am", "Him\u00b7mel", "ver\u00b7l\u00f6\u00b7schen", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Sein Rad vorne dreht,", "tokens": ["Sein", "Rad", "vor\u00b7ne", "dreht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Halb Tier, halb Prolet,", "tokens": ["Halb", "Tier", ",", "halb", "Pro\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und faucht in gewaltiger Frone", "tokens": ["Und", "faucht", "in", "ge\u00b7wal\u00b7ti\u00b7ger", "Fro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein h\u00e4\u00dflicher Kerl", "tokens": ["Ein", "h\u00e4\u00df\u00b7li\u00b7cher", "Kerl"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Vom H\u00f6llengeschwerl", "tokens": ["Vom", "H\u00f6l\u00b7len\u00b7ge\u00b7schwerl"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Aus der unterirdischen Zone.", "tokens": ["Aus", "der", "un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "Zo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer leitet den Zug", "tokens": ["Wer", "lei\u00b7tet", "den", "Zug"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Von Laster und Lug", "tokens": ["Von", "Las\u00b7ter", "und", "Lug"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und dr\u00fcckt auf den Knopf an der Spitze?", "tokens": ["Und", "dr\u00fcckt", "auf", "den", "Knopf", "an", "der", "Spit\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Nase, sie denkt,", "tokens": ["Die", "Na\u00b7se", ",", "sie", "denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Satan, er lenkt", "tokens": ["Der", "Sa\u00b7tan", ",", "er", "lenkt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Sippschaft vom faulen Besitze.", "tokens": ["Die", "Sipp\u00b7schaft", "vom", "fau\u00b7len", "Be\u00b7sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Die B\u00f6rse, sie lacht,", "tokens": ["Die", "B\u00f6r\u00b7se", ",", "sie", "lacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Der Bonze will Macht,", "tokens": ["Der", "Bon\u00b7ze", "will", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Doch bannt er die zuckenden Blitze?", "tokens": ["Doch", "bannt", "er", "die", "zu\u00b7cken\u00b7den", "Blit\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "\u00bbhinab mit dem Kreuz!\u00ab", "tokens": ["\u00bb", "hin\u00b7ab", "mit", "dem", "Kreuz", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Das Dirnlein gebeut's \u2013", "tokens": ["Das", "Dirn\u00b7lein", "ge\u00b7beut's", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Lumpazi macht l\u00e4sternde Witze.", "tokens": ["Lum\u00b7pa\u00b7zi", "macht", "l\u00e4s\u00b7tern\u00b7de", "Wit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbjuchheirassassa,", "tokens": ["\u00bb", "juch\u00b7hei\u00b7ras\u00b7sas\u00b7sa", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "FM.la", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und die Gaudi ist da,", "tokens": ["Und", "die", "Gau\u00b7di", "ist", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hoch lebe der heilige Schwindel!\u00ab", "tokens": ["Hoch", "le\u00b7be", "der", "hei\u00b7li\u00b7ge", "Schwin\u00b7del", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Das Sektglas am Mund,", "tokens": ["Das", "Sekt\u00b7glas", "am", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Geht nobel zugrund", "tokens": ["Geht", "no\u00b7bel", "zu\u00b7grund"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Welt mit dem Lumpengesindel.", "tokens": ["Die", "Welt", "mit", "dem", "Lum\u00b7pen\u00b7ge\u00b7sin\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Das rauft sich und tollt,", "tokens": ["Das", "rauft", "sich", "und", "tollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "KON", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Fuhre sie rollt", "tokens": ["Die", "Fuh\u00b7re", "sie", "rollt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf eiserner Spur in die Weite ...", "tokens": ["Auf", "ei\u00b7ser\u00b7ner", "Spur", "in", "die", "Wei\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das taumelt und zecht,", "tokens": ["Das", "tau\u00b7melt", "und", "zecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Wie der Herr, so der Knecht \u2013", "tokens": ["Wie", "der", "Herr", ",", "so", "der", "Knecht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ART", "NN", "$("], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Die Greise, sie schleichen zur Seite.", "tokens": ["Die", "Grei\u00b7se", ",", "sie", "schlei\u00b7chen", "zur", "Sei\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Kein Stern wird geliebt \u2013", "tokens": ["Kein", "Stern", "wird", "ge\u00b7liebt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wer wuchert und schiebt,", "tokens": ["Wer", "wu\u00b7chert", "und", "schiebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ist der wahre Gebieter der Herde.", "tokens": ["Ist", "der", "wah\u00b7re", "Ge\u00b7bie\u00b7ter", "der", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Geil grinst das Gez\u00fccht", "tokens": ["Geil", "grinst", "das", "Ge\u00b7z\u00fccht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ins grell blendende Licht \u2013", "tokens": ["Ins", "grell", "blen\u00b7den\u00b7de", "Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Steil reckt sich der Ries' aus der Erde.", "tokens": ["Steil", "reckt", "sich", "der", "Ries'", "aus", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}