{"textgrid.poem.52549": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonn ist aufgegangen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonn ist aufgegangen,", "tokens": ["Die", "Sonn", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich steh im Thau der Flur,", "tokens": ["Ich", "steh", "im", "Thau", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Glockenblumen prangen", "tokens": ["Die", "Glo\u00b7cken\u00b7blu\u00b7men", "pran\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schillern im Azur.", "tokens": ["Und", "schil\u00b7lern", "im", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die s\u00fc\u00dfen Strahlen scheuchten", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Strah\u00b7len", "scheuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die lange schwarze Nacht,", "tokens": ["Die", "lan\u00b7ge", "schwar\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Wald und Wiesen leuchten", "tokens": ["Und", "Wald", "und", "Wie\u00b7sen", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie funkelnder Smaragd.", "tokens": ["Wie", "fun\u00b7keln\u00b7der", "Sma\u00b7ragd", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Fr\u00fchlingsnelken bl\u00fchen", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7nel\u00b7ken", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie gl\u00fchender Rubin,", "tokens": ["Wie", "gl\u00fc\u00b7hen\u00b7der", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NE", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie Diamanten spr\u00fchen", "tokens": ["Wie", "Di\u00b7a\u00b7man\u00b7ten", "spr\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tropfen im Jasmin.", "tokens": ["Die", "Trop\u00b7fen", "im", "Jas\u00b7min", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Und von den Wasserf\u00e4llen", "tokens": ["Und", "von", "den", "Was\u00b7ser\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Die Perle gl\u00e4nzend rollt,", "tokens": ["Die", "Per\u00b7le", "gl\u00e4n\u00b7zend", "rollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es blitzet aus den Quellen", "tokens": ["Es", "blit\u00b7zet", "aus", "den", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Silber und wie Gold.", "tokens": ["Wie", "Sil\u00b7ber", "und", "wie", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Liebste, wie beschenk ich", "tokens": ["O", "Liebs\u00b7te", ",", "wie", "be\u00b7schenk", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit all dem ", "tokens": ["Mit", "all", "dem"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Durch dieses Liedchen denk ich", "tokens": ["Durch", "die\u00b7ses", "Lied\u00b7chen", "denk", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mach ich mein Liebchen reich!", "tokens": ["Mach", "ich", "mein", "Lieb\u00b7chen", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}