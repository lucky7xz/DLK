{"textgrid.poem.39959": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du wundervoll gesungen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du wundervoll gesungen", "tokens": ["Der", "du", "wun\u00b7der\u00b7voll", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der meerversenkten Stadt,", "tokens": ["Von", "der", "meer\u00b7ver\u00b7senk\u00b7ten", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df herauf zu uns geklungen", "tokens": ["Da\u00df", "her\u00b7auf", "zu", "uns", "ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie mit allen Glocken hat:", "tokens": ["Sie", "mit", "al\u00b7len", "Glo\u00b7cken", "hat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heute m\u00f6cht' ich dir bereiten", "tokens": ["Heu\u00b7te", "m\u00f6cht'", "ich", "dir", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lohn f\u00fcr deinen sch\u00f6nsten Sang,", "tokens": ["Lohn", "f\u00fcr", "dei\u00b7nen", "sch\u00f6ns\u00b7ten", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dir auf den Wellen gleiten", "tokens": ["Mit", "dir", "auf", "den", "Wel\u00b7len", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Sanft den Wallensee entlang.", "tokens": ["Sanft", "den", "Wal\u00b7len\u00b7see", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dir das sch\u00f6ne Wunder zeigen,", "tokens": ["Dir", "das", "sch\u00f6\u00b7ne", "Wun\u00b7der", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das dem Blick sich hier enth\u00fcllt,", "tokens": ["Das", "dem", "Blick", "sich", "hier", "ent\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun ein Bild aus deinem Reigen", "tokens": ["Nun", "ein", "Bild", "aus", "dei\u00b7nem", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich mit Lieblichkeit erf\u00fcllt. \u2013", "tokens": ["Sich", "mit", "Lieb\u00b7lich\u00b7keit", "er\u00b7f\u00fcllt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "APPR", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo die Linth als Lindwurm hauste,", "tokens": ["Wo", "die", "Linth", "als", "Lind\u00b7wurm", "haus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bald \u00fcber gr\u00fcne Saat", "tokens": ["Und", "bald", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "Saat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Mit dem Wellenleibe brauste,", "tokens": ["Mit", "dem", "Wel\u00b7len\u00b7lei\u00b7be", "braus\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald in tr\u00e4ge S\u00fcmpfe trat,", "tokens": ["Bald", "in", "tr\u00e4\u00b7ge", "S\u00fcmp\u00b7fe", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ist das Hirtenvolk gesch\u00e4ftig,", "tokens": ["Ist", "das", "Hir\u00b7ten\u00b7volk", "ge\u00b7sch\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Baut an D\u00e4mmen und Kanal,", "tokens": ["Baut", "an", "D\u00e4m\u00b7men", "und", "Ka\u00b7nal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwingt den Sehnenarm so kr\u00e4ftig,", "tokens": ["Schwingt", "den", "Seh\u00b7nen\u00b7arm", "so", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis die Schlange wird zum Aal.", "tokens": ["Bis", "die", "Schlan\u00b7ge", "wird", "zum", "Aal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und so schl\u00fcpft der Flu\u00df bezwungen", "tokens": ["Und", "so", "schl\u00fcpft", "der", "Flu\u00df", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dem Becken willig fort,", "tokens": ["Aus", "dem", "Be\u00b7cken", "wil\u00b7lig", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch der See, ihm nachgedrungen,", "tokens": ["Auch", "der", "See", ",", "ihm", "nach\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Senkt sich an der M\u00fcndung dort.", "tokens": ["Senkt", "sich", "an", "der", "M\u00fcn\u00b7dung", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch, wie sich die Fluten legen,", "tokens": ["Doch", ",", "wie", "sich", "die", "Flu\u00b7ten", "le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was enth\u00fcllt die Tiefe nur?", "tokens": ["Was", "ent\u00b7h\u00fcllt", "die", "Tie\u00b7fe", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mauern steigen uns entgegen", "tokens": ["Mau\u00b7ern", "stei\u00b7gen", "uns", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ergrauter Th\u00fcrme Spur.", "tokens": ["Und", "er\u00b7grau\u00b7ter", "Th\u00fcr\u00b7me", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Zack'ge Zinnen, Gothenfenster", "tokens": ["Zack'\u00b7ge", "Zin\u00b7nen", ",", "Go\u00b7then\u00b7fens\u00b7ter"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein spitzgew\u00f6lbtes Thor \u2013", "tokens": ["Und", "ein", "spitz\u00b7ge\u00b7w\u00f6lb\u00b7tes", "Thor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Springen Nixen und Gespenster", "tokens": ["Sprin\u00b7gen", "Ni\u00b7xen", "und", "Ge\u00b7spens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht im Harnisch bald hervor?", "tokens": ["Nicht", "im", "Har\u00b7nisch", "bald", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Eine Burg ist's, die zerfallen", "tokens": ["Ei\u00b7ne", "Burg", "ist's", ",", "die", "zer\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier Jahrhunderte vertr\u00e4umt,", "tokens": ["Hier", "Jahr\u00b7hun\u00b7der\u00b7te", "ver\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die der alte, gr\u00fcne Wallen", "tokens": ["Die", "der", "al\u00b7te", ",", "gr\u00fc\u00b7ne", "Wal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Wellen \u00fcbersch\u00e4umt.", "tokens": ["Mit", "den", "Wel\u00b7len", "\u00fc\u00b7bersc\u00b7h\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aus erlogener Vernichtung", "tokens": ["Aus", "er\u00b7lo\u00b7ge\u00b7ner", "Ver\u00b7nich\u00b7tung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Steht die auferstandne hier:", "tokens": ["Steht", "die", "auf\u00b7er\u00b7stand\u00b7ne", "hier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dichter, freu dich deiner Dichtung,", "tokens": ["Dich\u00b7ter", ",", "freu", "dich", "dei\u00b7ner", "Dich\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese Burg geh\u00f6rt ja dir!", "tokens": ["Die\u00b7se", "Burg", "ge\u00b7h\u00f6rt", "ja", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie sie schmuck und heiter l\u00e4chelt,", "tokens": ["Wie", "sie", "schmuck", "und", "hei\u00b7ter", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von den Wellen reingesp\u00fclt!", "tokens": ["Von", "den", "Wel\u00b7len", "rein\u00b7ge\u00b7sp\u00fclt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Seewind sie umf\u00e4chelt,", "tokens": ["Wie", "der", "See\u00b7wind", "sie", "um\u00b7f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die besonnten Mauern k\u00fchlt!", "tokens": ["Die", "be\u00b7sonn\u00b7ten", "Mau\u00b7ern", "k\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Diese Burg hast du ersungen,", "tokens": ["Die\u00b7se", "Burg", "hast", "du", "er\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimm sie an aus S\u00e4ngerhand,", "tokens": ["Nimm", "sie", "an", "aus", "S\u00e4n\u00b7ger\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df uns einziehn, armumschlungen,", "tokens": ["La\u00df", "uns", "ein\u00b7ziehn", ",", "ar\u00b7mum\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df uns singen liedentbrannt.", "tokens": ["La\u00df", "uns", "sin\u00b7gen", "lie\u00b7dent\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "La\u00df uns eins zusammen bechern,", "tokens": ["La\u00df", "uns", "eins", "zu\u00b7sam\u00b7men", "be\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Rittersaal geschwind!", "tokens": ["In", "dem", "Rit\u00b7ter\u00b7saal", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis uns einf\u00e4llt, trotz'gen Zechern,", "tokens": ["Bis", "uns", "ein\u00b7f\u00e4llt", ",", "trotz'\u00b7gen", "Ze\u00b7chern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df wir ew'ge Feinde sind.", "tokens": ["Da\u00df", "wir", "ew'\u00b7ge", "Fein\u00b7de", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}