{"textgrid.poem.55908": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist dieses Bette nicht wie eine K\u00fcste,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist dieses Bette nicht wie eine K\u00fcste,", "tokens": ["Ist", "die\u00b7ses", "Bet\u00b7te", "nicht", "wie", "ei\u00b7ne", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PTKNEG", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ein K\u00fcstenstreifen nur, darauf wir liegen?", "tokens": ["ein", "K\u00fcs\u00b7ten\u00b7strei\u00b7fen", "nur", ",", "da\u00b7rauf", "wir", "lie\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nichts ist gewi\u00df als deine hohen Br\u00fcste,", "tokens": ["Nichts", "ist", "ge\u00b7wi\u00df", "als", "dei\u00b7ne", "ho\u00b7hen", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "die mein Gef\u00fchl in Schwindeln \u00fcberstiegen.", "tokens": ["die", "mein", "Ge\u00b7f\u00fchl", "in", "Schwin\u00b7deln", "\u00fc\u00b7bers\u00b7tie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn diese Nacht, in der so vieles schrie,", "tokens": ["Denn", "die\u00b7se", "Nacht", ",", "in", "der", "so", "vie\u00b7les", "schrie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "APPR", "ART", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in der sich Tiere rufen und zerrei\u00dfen,", "tokens": ["in", "der", "sich", "Tie\u00b7re", "ru\u00b7fen", "und", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADJA", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ist sie uns nicht entsetzlich fremd? Und wie:", "tokens": ["ist", "sie", "uns", "nicht", "ent\u00b7setz\u00b7lich", "fremd", "?", "Und", "wie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "ADJD", "$.", "KON", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "was drau\u00dfen langsam anhebt, Tag gehei\u00dfen,", "tokens": ["was", "drau\u00b7\u00dfen", "lang\u00b7sam", "an\u00b7hebt", ",", "Tag", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVFIN", "$,", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "ist das uns denn verst\u00e4ndlicher als sie?", "tokens": ["ist", "das", "uns", "denn", "ver\u00b7st\u00e4nd\u00b7li\u00b7cher", "als", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Man m\u00fc\u00dfte so sich ineinanderlegen", "tokens": ["Man", "m\u00fc\u00df\u00b7te", "so", "sich", "in\u00b7ein\u00b7an\u00b7der\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie Bl\u00fctenbl\u00e4tter um die Staubgef\u00e4\u00dfe:", "tokens": ["wie", "Bl\u00fc\u00b7ten\u00b7bl\u00e4t\u00b7ter", "um", "die", "Staub\u00b7ge\u00b7f\u00e4\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so sehr ist \u00fcberall das Ungem\u00e4\u00dfe", "tokens": ["so", "sehr", "ist", "\u00fc\u00b7be\u00b7rall", "das", "Un\u00b7ge\u00b7m\u00e4\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und h\u00e4uft sich an und st\u00fcrzt sich uns entgegen.", "tokens": ["und", "h\u00e4uft", "sich", "an", "und", "st\u00fcrzt", "sich", "uns", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch w\u00e4hrend wir uns aneinander dr\u00fccken,", "tokens": ["Doch", "w\u00e4h\u00b7rend", "wir", "uns", "an\u00b7ein\u00b7an\u00b7der", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "um nicht zu sehen, wie es ringsum naht,", "tokens": ["um", "nicht", "zu", "se\u00b7hen", ",", "wie", "es", "ring\u00b7sum", "naht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKNEG", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "kann es aus dir, kann es aus mir sich z\u00fccken:", "tokens": ["kann", "es", "aus", "dir", ",", "kann", "es", "aus", "mir", "sich", "z\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "$,", "VMFIN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "denn unsre Seelen leben von Verrat.", "tokens": ["denn", "uns\u00b7re", "See\u00b7len", "le\u00b7ben", "von", "Ver\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}