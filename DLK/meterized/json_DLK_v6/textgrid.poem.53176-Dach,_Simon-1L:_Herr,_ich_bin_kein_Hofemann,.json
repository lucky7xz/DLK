{"textgrid.poem.53176": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, ich bin kein Hofemann,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, ich bin kein Hofemann,", "tokens": ["Herr", ",", "ich", "bin", "kein", "Ho\u00b7fe\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher vnversch\u00e4mt seyn kan,", "tokens": ["Wel\u00b7cher", "vn\u00b7ver\u00b7sch\u00e4mt", "seyn", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonst w\u00fcrd ich wol selber kommen,", "tokens": ["Sonst", "w\u00fcrd", "ich", "wol", "sel\u00b7ber", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dieser Reim, seht, sch\u00e4mt sich nicht,", "tokens": ["Die\u00b7ser", "Reim", ",", "seht", ",", "sch\u00e4mt", "sich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "PRF", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6nnt ihm ewer Angesicht,", "tokens": ["G\u00f6nnt", "ihm", "e\u00b7wer", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lasset ihn sein angenommen.", "tokens": ["Las\u00b7set", "ihn", "sein", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Habet seiner kein Beschwer,", "tokens": ["Ha\u00b7bet", "sei\u00b7ner", "kein", "Be\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber wa\u00df ist sein begehr?", "tokens": ["A\u00b7ber", "wa\u00df", "ist", "sein", "be\u00b7gehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd wa\u00df wirdt ihm anbefohlen?", "tokens": ["Vnd", "wa\u00df", "wirdt", "ihm", "an\u00b7be\u00b7foh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt er etwa zu euch ein", "tokens": ["Bringt", "er", "et\u00b7wa", "zu", "euch", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einig hundert Thaler? nein.", "tokens": ["Ei\u00b7nig", "hun\u00b7dert", "Tha\u00b7ler", "?", "nein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$.", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herr, verzeiht, er wil sie holen.", "tokens": ["Herr", ",", "ver\u00b7zeiht", ",", "er", "wil", "sie", "ho\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ostern sind schon l\u00e4ngst vorbey,", "tokens": ["Os\u00b7tern", "sind", "schon", "l\u00e4ngst", "vor\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wisst, da\u00df ich gantz krafftlo\u00df sey", "tokens": ["Wisst", ",", "da\u00df", "ich", "gantz", "kraff\u00b7tlo\u00df", "sey"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem Hunger nach dem Gelde,", "tokens": ["Vor", "dem", "Hun\u00b7ger", "nach", "dem", "Gel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut, HochEdler Herr, mir kunt,", "tokens": ["Thut", ",", "Hoch", "Ed\u00b7ler", "Herr", ",", "mir", "kunt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind sie da vor meinen Mund", "tokens": ["Sind", "sie", "da", "vor", "mei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder noch im weiten Felde?", "tokens": ["O\u00b7der", "noch", "im", "wei\u00b7ten", "Fel\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O ein Wildpret, da\u00df erst schmeckt,", "tokens": ["O", "ein", "Wild\u00b7pret", ",", "da\u00df", "erst", "schmeckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hett es sich gleich auch versteckt", "tokens": ["Hett", "es", "sich", "gleich", "auch", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tieff in wildverwach\u00dfne Hecken,", "tokens": ["Tieff", "in", "wild\u00b7ver\u00b7wach\u00df\u00b7ne", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich verfolgt es, wie ich kan,", "tokens": ["Ich", "ver\u00b7folgt", "es", ",", "wie", "ich", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Krieg ich es, wie werd ich dan", "tokens": ["Krieg", "ich", "es", ",", "wie", "werd", "ich", "dan"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "$,", "PWAV", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Finger darnach lecken.", "tokens": ["Mei\u00b7ne", "Fin\u00b7ger", "dar\u00b7nach", "le\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Herr, seyd mein Befordrer hier,", "tokens": ["Herr", ",", "seyd", "mein", "Be\u00b7ford\u00b7rer", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Helfft mir jagen dieses Thier,", "tokens": ["Helfft", "mir", "ja\u00b7gen", "die\u00b7ses", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Springt mir bey mit schnellen Winden,", "tokens": ["Springt", "mir", "bey", "mit", "schnel\u00b7len", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil kein Raht ist also bald,", "tokens": ["Weil", "kein", "Raht", "ist", "al\u00b7so", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es verl\u00e4ufft sich in den Wald,", "tokens": ["Es", "ver\u00b7l\u00e4ufft", "sich", "in", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo vnd wenn werd ich es finden?", "tokens": ["Wo", "vnd", "wenn", "werd", "ich", "es", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "KOUS", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}}}}