{"dta.poem.16142": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "196.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, die Tr\u00e4gheit ward vom Unverstand gefreit,", "tokens": ["Man", "sagt", ",", "die", "Tr\u00e4g\u00b7heit", "ward", "vom", "Un\u00b7ver\u00b7stand", "ge\u00b7freit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihrer Eh entspro\u00df Armuth und D\u00fcrftigkeit.", "tokens": ["Und", "ih\u00b7rer", "Eh", "ent\u00b7spro\u00df", "Ar\u00b7muth", "und", "D\u00fcrf\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Eltern legten nur die H\u00e4nde in den Schoo\u00df,", "tokens": ["Die", "El\u00b7tern", "leg\u00b7ten", "nur", "die", "H\u00e4n\u00b7de", "in", "den", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ohne Unterhalt wurden die Kinder gro\u00df.", "tokens": ["Doch", "oh\u00b7ne", "Un\u00b7ter\u00b7halt", "wur\u00b7den", "die", "Kin\u00b7der", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Die waren undankbar, und trieben aus dem Haus", "tokens": ["Die", "wa\u00b7ren", "un\u00b7dank\u00b7bar", ",", "und", "trie\u00b7ben", "aus", "dem", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Eltern, und das Paar zog in die Welt hinaus.", "tokens": ["Die", "El\u00b7tern", ",", "und", "das", "Paar", "zog", "in", "die", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da war es wunderbar, sie lie\u00dfen doch die Kinder", "tokens": ["Da", "war", "es", "wun\u00b7der\u00b7bar", ",", "sie", "lie\u00b7\u00dfen", "doch", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Haus, und fanden nun sie da und dort nicht minder.", "tokens": ["Zu", "Haus", ",", "und", "fan\u00b7den", "nun", "sie", "da", "und", "dort", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "ADV", "PPER", "ADV", "KON", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Voll Schrecken flohen sie und wollten sich verstecken,", "tokens": ["Voll", "Schre\u00b7cken", "flo\u00b7hen", "sie", "und", "woll\u00b7ten", "sich", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch stets bedrohen sie die Kinder aus den Ecken.", "tokens": ["Doch", "stets", "be\u00b7dro\u00b7hen", "sie", "die", "Kin\u00b7der", "aus", "den", "E\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}