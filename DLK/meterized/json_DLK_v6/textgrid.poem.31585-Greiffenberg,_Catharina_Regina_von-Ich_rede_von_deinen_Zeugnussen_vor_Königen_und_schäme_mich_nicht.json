{"textgrid.poem.31585": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "Ich rede von deinen Zeugnussen vor K\u00f6nigen /und sch\u00e4me mich nicht", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solt man der H\u00f6chsten Ehr' und Wei\u00dfheit sich auch sch\u00e4men /", "tokens": ["Solt", "man", "der", "H\u00f6chs\u00b7ten", "Ehr'", "und", "Wei\u00df\u00b7heit", "sich", "auch", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "ADJA", "NN", "KON", "NN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "welch ist des Herrn Wort? Ach nein! man soll sie frey", "tokens": ["welch", "ist", "des", "Herrn", "Wort", "?", "Ach", "nein", "!", "man", "soll", "sie", "frey"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAT", "VAFIN", "ART", "NN", "NN", "$.", "NN", "PTKANT", "$.", "PIS", "VMFIN", "PPER", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "bekennen wie da\u00df sie des Lebens Leben sey:", "tokens": ["be\u00b7ken\u00b7nen", "wie", "da\u00df", "sie", "des", "Le\u00b7bens", "Le\u00b7ben", "sey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "KOUS", "PPER", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf da\u00df die andern sich zu ihrem Dienst bequ\u00e4men.", "tokens": ["auf", "da\u00df", "die", "an\u00b7dern", "sich", "zu", "ih\u00b7rem", "Dienst", "be\u00b7qu\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Will man hier\u00fcber auch uns Leib und Leben nehmen /", "tokens": ["Will", "man", "hier\u00b7\u00fc\u00b7ber", "auch", "uns", "Leib", "und", "Le\u00b7ben", "neh\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PAV", "ADV", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "schadt nicht: die Warheit steht uns todt und lebend bey.", "tokens": ["schadt", "nicht", ":", "die", "War\u00b7heit", "steht", "uns", "todt", "und", "le\u00b7bend", "bey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "sie hat der Freudenblick so viel und mancherley /", "tokens": ["sie", "hat", "der", "Freu\u00b7den\u00b7blick", "so", "viel", "und", "man\u00b7cher\u00b7ley", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "KON", "PIS", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df man mit jauchzen stirbt / stat da\u00df man sich solt gr\u00e4men.", "tokens": ["da\u00df", "man", "mit", "jauch\u00b7zen", "stirbt", "/", "stat", "da\u00df", "man", "sich", "solt", "gr\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "VVINF", "VVFIN", "$(", "APPR", "KOUS", "PIS", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Warheit Klarheit scheint durch d\u00fcstre Irthums-Nacht:", "tokens": ["Der", "War\u00b7heit", "Klar\u00b7heit", "scheint", "durch", "d\u00fcst\u00b7re", "Ir\u00b7thums\u00b7Nacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Verf\u00fchrungs-Schatten mu\u00df von ihren Strahlen weichen:", "tokens": ["Ver\u00b7f\u00fch\u00b7rungs\u00b7Schat\u00b7ten", "mu\u00df", "von", "ih\u00b7ren", "Strah\u00b7len", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "wer sie verderben will / wie jener Thor es macht /", "tokens": ["wer", "sie", "ver\u00b7der\u00b7ben", "will", "/", "wie", "je\u00b7ner", "Thor", "es", "macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VMFIN", "$(", "KOKOM", "PDAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der mit ein wenig Erd die Sonne wolt verstreichen.", "tokens": ["der", "mit", "ein", "we\u00b7nig", "Erd", "die", "Son\u00b7ne", "wolt", "ver\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "PIAT", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr Thoren! seht ihr nicht / das Sonn' und Warheit ist", "tokens": ["Ihr", "Tho\u00b7ren", "!", "seht", "ihr", "nicht", "/", "das", "Sonn'", "und", "War\u00b7heit", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$(", "ART", "NN", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "viel gr\u00f6sser / als die Erd' und aller Menschen List?", "tokens": ["viel", "gr\u00f6s\u00b7ser", "/", "als", "die", "Erd'", "und", "al\u00b7ler", "Men\u00b7schen", "List", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "KOUS", "ART", "NN", "KON", "PIAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}