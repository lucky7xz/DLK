{"textgrid.poem.41681": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleich stummen Herden sich im Sande lagernd, wenden", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich stummen Herden sich im Sande lagernd, wenden", "tokens": ["Gleich", "stum\u00b7men", "Her\u00b7den", "sich", "im", "San\u00b7de", "la\u00b7gernd", ",", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "PRF", "APPRART", "NN", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ihre Augen nach dem Horizont der See.", "tokens": ["Sie", "ih\u00b7re", "Au\u00b7gen", "nach", "dem", "Ho\u00b7ri\u00b7zont", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In ihren F\u00fc\u00dfen, die sich suchen, ihren H\u00e4nden,", "tokens": ["In", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", ",", "die", "sich", "su\u00b7chen", ",", "ih\u00b7ren", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "VVINF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bebt sehnsuchtsbanger Wunsch und fr\u00f6stelnd-herbes Weh.", "tokens": ["Bebt", "sehn\u00b7suchts\u00b7ban\u00b7ger", "Wunsch", "und", "fr\u00f6s\u00b7teln\u00b7dher\u00b7bes", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die einen, trunken von gehauchten Traulichkeiten,", "tokens": ["Die", "ei\u00b7nen", ",", "trun\u00b7ken", "von", "ge\u00b7hauch\u00b7ten", "Trau\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehn an den B\u00e4chen hin, die lallen durch den Hain,", "tokens": ["Gehn", "an", "den", "B\u00e4\u00b7chen", "hin", ",", "die", "lal\u00b7len", "durch", "den", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und stammeln bang die Glut der scheuen Kinderzeiten", "tokens": ["Und", "stam\u00b7meln", "bang", "die", "Glut", "der", "scheu\u00b7en", "Kin\u00b7der\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ritzen Namen in die jungen B\u00e4ume ein.", "tokens": ["Und", "rit\u00b7zen", "Na\u00b7men", "in", "die", "jun\u00b7gen", "B\u00e4u\u00b7me", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und andre, Schwestern gleich, durchwandern ernst und schweigend", "tokens": ["Und", "and\u00b7re", ",", "Schwes\u00b7tern", "gleich", ",", "durch\u00b7wan\u00b7dern", "ernst", "und", "schwei\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "NN", "ADV", "$,", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Felsenk\u00fcste, die Gesichte l\u00e4\u00dft erstehn.", "tokens": ["Die", "Fel\u00b7sen\u00b7k\u00fcs\u00b7te", ",", "die", "Ge\u00b7sich\u00b7te", "l\u00e4\u00dft", "er\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Sankt Antonius, wie Lavafluten steigend,", "tokens": ["Wo", "Sankt", "An\u00b7to\u00b7ni\u00b7us", ",", "wie", "La\u00b7va\u00b7flu\u00b7ten", "stei\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NE", "$,", "PWAV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die nackten Br\u00fcste der Versuchung einst gesehn.", "tokens": ["Die", "nack\u00b7ten", "Br\u00fcs\u00b7te", "der", "Ver\u00b7su\u00b7chung", "einst", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und andre, die im Schein verglommner Fackeln weilen,", "tokens": ["Und", "and\u00b7re", ",", "die", "im", "Schein", "ver\u00b7glomm\u00b7ner", "Fa\u00b7ckeln", "wei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In heidnischen Gew\u00f6lbs verschwiegner Dunkelheit,", "tokens": ["In", "heid\u00b7ni\u00b7schen", "Ge\u00b7w\u00f6l\u00b7bs", "ver\u00b7schwieg\u00b7ner", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Flehn deine Hilfe an, ihr Fieberweh zu heilen,", "tokens": ["Flehn", "dei\u00b7ne", "Hil\u00b7fe", "an", ",", "ihr", "Fie\u00b7ber\u00b7weh", "zu", "hei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.4": {"text": "O Bacchus, der der Qual Vergessenheit verleiht.", "tokens": ["O", "Bac\u00b7chus", ",", "der", "der", "Qual", "Ver\u00b7ges\u00b7sen\u00b7heit", "ver\u00b7leiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Noch andre, deren Brust bedeckt vom Skapuliere,", "tokens": ["Noch", "and\u00b7re", ",", "de\u00b7ren", "Brust", "be\u00b7deckt", "vom", "Ska\u00b7pu\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELAT", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die eine Gei\u00dfel in dem faltgen Kleid versteckt,", "tokens": ["Die", "ei\u00b7ne", "Gei\u00b7\u00dfel", "in", "dem", "falt\u00b7gen", "Kleid", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vereinen in der Nacht der \u00f6den Waldreviere", "tokens": ["Ver\u00b7ei\u00b7nen", "in", "der", "Nacht", "der", "\u00f6\u00b7den", "Wald\u00b7re\u00b7vie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Rausch der wilden Lust der Pein, die Tr\u00e4nen weckt.", "tokens": ["Den", "Rausch", "der", "wil\u00b7den", "Lust", "der", "Pein", ",", "die", "Tr\u00e4\u00b7nen", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "D\u00e4monen, Jungfraun ihr, Untiere, Dulderinnen,", "tokens": ["D\u00e4\u00b7mo\u00b7nen", ",", "Jung\u00b7fraun", "ihr", ",", "Un\u00b7tie\u00b7re", ",", "Dul\u00b7de\u00b7rin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhabne Geister, die die Wirklichkeit verschm\u00e4hn,", "tokens": ["Er\u00b7hab\u00b7ne", "Geis\u00b7ter", ",", "die", "die", "Wirk\u00b7lich\u00b7keit", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die \u2013 l\u00fcstern oder fromm \u2013 auf Unbegrenztes sinnen,", "tokens": ["Die", "\u2013", "l\u00fcs\u00b7tern", "o\u00b7der", "fromm", "\u2013", "auf", "Un\u00b7be\u00b7grenz\u00b7tes", "sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADJD", "KON", "ADJD", "$(", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die bald verzweifelt schrein, in Tr\u00e4nen bald zergehn,", "tokens": ["Die", "bald", "ver\u00b7zwei\u00b7felt", "schrein", ",", "in", "Tr\u00e4\u00b7nen", "bald", "zer\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAINF", "$,", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ihr, denen ich ins Graun der H\u00f6lle nachgegangen,", "tokens": ["Ihr", ",", "de\u00b7nen", "ich", "ins", "Graun", "der", "H\u00f6l\u00b7le", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich liebe, Schwestern, euch und klage euer Los,", "tokens": ["Ich", "lie\u00b7be", ",", "Schwes\u00b7tern", ",", "euch", "und", "kla\u00b7ge", "eu\u00b7er", "Los", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um euer finster Leid und ungestillt Verlangen,", "tokens": ["Um", "eu\u00b7er", "fins\u00b7ter", "Leid", "und", "un\u00b7ge\u00b7stillt", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um das Ged\u00e4chtnis an die Glut, so tief und gro\u00df.", "tokens": ["Um", "das", "Ge\u00b7d\u00e4cht\u00b7nis", "an", "die", "Glut", ",", "so", "tief", "und", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}