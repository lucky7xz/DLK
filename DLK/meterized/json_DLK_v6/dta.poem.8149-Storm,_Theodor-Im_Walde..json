{"dta.poem.8149": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Verstummet ganz der Wind;", "tokens": ["Ver\u00b7stum\u00b7met", "ganz", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Zweige h\u00e4ngen nieder,", "tokens": ["Die", "Zwei\u00b7ge", "h\u00e4n\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darunter sitzt das Kind.", "tokens": ["Da\u00b7run\u00b7ter", "sitzt", "das", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sitzt in Thymiane,", "tokens": ["Sie", "sitzt", "in", "Thy\u00b7mi\u00b7a\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie sitzt in lauter Duft;", "tokens": ["Sie", "sitzt", "in", "lau\u00b7ter", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die blauen Fliegen summen", "tokens": ["Die", "blau\u00b7en", "Flie\u00b7gen", "sum\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und blitzen durch die Luft.", "tokens": ["Und", "blit\u00b7zen", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es steht der Wald so schweigend,", "tokens": ["Es", "steht", "der", "Wald", "so", "schwei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie schaut so klug darein;", "tokens": ["Sie", "schaut", "so", "klug", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um ihre braunen Locken", "tokens": ["Um", "ih\u00b7re", "brau\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinflie\u00dft der Sonnenschein.", "tokens": ["Hin\u00b7flie\u00dft", "der", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Der Kuckuck lacht von ferne,", "tokens": ["Der", "Ku\u00b7ckuck", "lacht", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es geht mir durch den Sinn:", "tokens": ["Es", "geht", "mir", "durch", "den", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat die goldnen Augen", "tokens": ["Sie", "hat", "die", "gold\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Waldesk\u00f6nigin.", "tokens": ["Der", "Wal\u00b7des\u00b7k\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}