{"dta.poem.12953": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von seiner mit ehr-furcht ver-  \n kn\u00fcpfften liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Garamanten brunn hat diese wunder-krafft,", "tokens": ["Der", "Ga\u00b7ra\u00b7man\u00b7ten", "brunn", "hat", "die\u00b7se", "wun\u00b7der\u00b7krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er bey nachte zu gefrieren,", "tokens": ["Da\u00df", "er", "bey", "nach\u00b7te", "zu", "ge\u00b7frie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des tages aber recht zu brennen pflegt.", "tokens": ["Des", "ta\u00b7ges", "a\u00b7ber", "recht", "zu", "bren\u00b7nen", "pflegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In mir l\u00e4st sich was ungemeines sp\u00fcren,", "tokens": ["In", "mir", "l\u00e4st", "sich", "was", "un\u00b7ge\u00b7mei\u00b7nes", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "PWS", "ADJA", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Weil meine brust zugleich so hitz als k\u00e4lte tr\u00e4gt;", "tokens": ["Weil", "mei\u00b7ne", "brust", "zu\u00b7gleich", "so", "hitz", "als", "k\u00e4l\u00b7te", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "KOKOM", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn ich w\u00fcste keine stunden,", "tokens": ["Denn", "ich", "w\u00fcs\u00b7te", "kei\u00b7ne", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seit dem ich Sylvien erblickt,", "tokens": ["Seit", "dem", "ich", "Syl\u00b7vi\u00b7en", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ihre majest\u00e4t und sch\u00f6nheit mich entz\u00fcckt,", "tokens": ["Und", "ih\u00b7re", "ma\u00b7jes\u00b7t\u00e4t", "und", "sch\u00f6n\u00b7heit", "mich", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df ich nicht heisse lieb und kalte furcht empfunden.", "tokens": ["Da\u00df", "ich", "nicht", "heis\u00b7se", "lieb", "und", "kal\u00b7te", "furcht", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADJD", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}