{"dta.poem.11995": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer Seele mehr nicht hat, als braucht zum eignen Leben", "tokens": ["Wer", "See\u00b7le", "mehr", "nicht", "hat", ",", "als", "braucht", "zum", "eig\u00b7nen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "ADV", "PTKNEG", "VAFIN", "$,", "KOUS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Leib, der wird davon nach au\u00dfen keine geben.", "tokens": ["Sein", "Leib", ",", "der", "wird", "da\u00b7von", "nach", "au\u00b7\u00dfen", "kei\u00b7ne", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "PAV", "APPR", "ADV", "PIAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer aber Ueberflu\u00df und mehr hat als er braucht,", "tokens": ["Wer", "a\u00b7ber", "Ue\u00b7berf\u00b7lu\u00df", "und", "mehr", "hat", "als", "er", "braucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "ADV", "VAFIN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ists, der Seele wie die Rose Duft verhaucht.", "tokens": ["Der", "ists", ",", "der", "See\u00b7le", "wie", "die", "Ro\u00b7se", "Duft", "ver\u00b7haucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ART", "NN", "KOKOM", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum seelespendender als starke sind die schwachen", "tokens": ["Drum", "see\u00b7les\u00b7pen\u00b7den\u00b7der", "als", "star\u00b7ke", "sind", "die", "schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "KOKOM", "VVFIN", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An Leib, die Seligen, die frei vom Leib sich machen.", "tokens": ["An", "Leib", ",", "die", "Se\u00b7li\u00b7gen", ",", "die", "frei", "vom", "Leib", "sich", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}