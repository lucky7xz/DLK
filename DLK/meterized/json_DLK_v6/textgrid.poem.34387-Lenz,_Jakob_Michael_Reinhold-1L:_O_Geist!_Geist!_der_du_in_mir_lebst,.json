{"textgrid.poem.34387": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Geist! Geist! der du in mir lebst,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Geist! Geist! der du in mir lebst,", "tokens": ["O", "Geist", "!", "Geist", "!", "der", "du", "in", "mir", "lebst", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "$.", "ART", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Woher kamst du, da\u00df du so eilst?", "tokens": ["Wo\u00b7her", "kamst", "du", ",", "da\u00df", "du", "so", "eilst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O verzeuch noch, himmlischer Geist;", "tokens": ["O", "ver\u00b7zeuch", "noch", ",", "himm\u00b7li\u00b7scher", "Geist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Deine H\u00fclle vermags nicht \u2013", "tokens": ["Dei\u00b7ne", "H\u00fcl\u00b7le", "ver\u00b7mags", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKNEG", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "All ihre Bande zittern.", "tokens": ["All", "ih\u00b7re", "Ban\u00b7de", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Komm nicht weiter empor!", "tokens": ["Komm", "nicht", "wei\u00b7ter", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Sei nur getrost, bald bist du frei,", "tokens": ["Sei", "nur", "ge\u00b7trost", ",", "bald", "bist", "du", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bald wird dir's gelungen sein, Grausamer,", "tokens": ["Bald", "wird", "dir's", "ge\u00b7lun\u00b7gen", "sein", ",", "Grau\u00b7sa\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "VVPP", "VAINF", "$,", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Bald hast du dein steinern, nordisch", "tokens": ["Bald", "hast", "du", "dein", "stei\u00b7nern", ",", "nor\u00b7disch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "VVINF", "$,", "ADJD"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.10": {"text": "Treues Haus \u00fcber den Kopf dir zertr\u00fcmmert.", "tokens": ["Treu\u00b7es", "Haus", "\u00fc\u00b7ber", "den", "Kopf", "dir", "zer\u00b7tr\u00fcm\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-++--+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Ach! da stehst du, wie ", "tokens": ["Ach", "!", "da", "stehst", "du", ",", "wie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "$,", "PWAV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Wirfst \u2013 strebst \u2013 wirfst's \u00fcber'n Haufen! \u2013", "tokens": ["Wirfst", "\u2013", "strebst", "\u2013", "wirfst's", "\u00fc\u00b7ber'n", "Hau\u00b7fen", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "$(", "VVFIN", "$(", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Weh uns Allen! schone noch, schone!", "tokens": ["Weh", "uns", "Al\u00b7len", "!", "scho\u00b7ne", "noch", ",", "scho\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "NE", "$.", "VVFIN", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Dieser treuen H\u00fctte Tr\u00fcmmer", "tokens": ["Die\u00b7ser", "treu\u00b7en", "H\u00fct\u00b7te", "Tr\u00fcm\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "M\u00f6chten dich sonst unter sich begraben.", "tokens": ["M\u00f6ch\u00b7ten", "dich", "sonst", "un\u00b7ter", "sich", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh! noch h\u00e4lt sie mit schmeichelnden Banden", "tokens": ["Sieh", "!", "noch", "h\u00e4lt", "sie", "mit", "schmei\u00b7cheln\u00b7den", "Ban\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dich zur\u00fcck; verspricht dir reine,", "tokens": ["Dich", "zu\u00b7r\u00fcck", ";", "ver\u00b7spricht", "dir", "rei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausend reine Lebensfreuden,", "tokens": ["Tau\u00b7send", "rei\u00b7ne", "Le\u00b7bens\u00b7freu\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zur Belohnung f\u00fcr deine M\u00fch.", "tokens": ["Zur", "Be\u00b7loh\u00b7nung", "f\u00fcr", "dei\u00b7ne", "M\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Schone noch, Grausamer, Undankbarer,", "tokens": ["Scho\u00b7ne", "noch", ",", "Grau\u00b7sa\u00b7mer", ",", "Un\u00b7dank\u00b7ba\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Kehre zur\u00fcck! heft' ihre Gelenke", "tokens": ["Keh\u00b7re", "zu\u00b7r\u00fcck", "!", "heft'", "ih\u00b7re", "Ge\u00b7len\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+----", "measure": "iambic.tri.invert"}, "line.7": {"text": "Wieder mit zarter Selbstlieb' zusammen,", "tokens": ["Wie\u00b7der", "mit", "zar\u00b7ter", "Selbst\u00b7lieb'", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Denn Gott selber baute sie dir,", "tokens": ["Denn", "Gott", "sel\u00b7ber", "bau\u00b7te", "sie", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Klein und gebrechlich, wie sie da ist.", "tokens": ["Klein", "und", "ge\u00b7brech\u00b7lich", ",", "wie", "sie", "da", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn sie ausgedauert, dann breche sie;", "tokens": ["Wenn", "sie", "aus\u00b7ge\u00b7dau\u00b7ert", ",", "dann", "bre\u00b7che", "sie", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Erst wenn der Baum gesaftet, gebl\u00fcht,", "tokens": ["Erst", "wenn", "der", "Baum", "ge\u00b7saf\u00b7tet", ",", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Fr\u00fcchte mehrj\u00e4hrig getragen, verdorret,", "tokens": ["Fr\u00fcch\u00b7te", "mehr\u00b7j\u00e4h\u00b7rig", "ge\u00b7tra\u00b7gen", ",", "ver\u00b7dor\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Gehe sein Keim ins ewige Leben.", "tokens": ["Ge\u00b7he", "sein", "Keim", "ins", "e\u00b7wi\u00b7ge", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aber jetzt, heilige, himmlische Flamme,", "tokens": ["A\u00b7ber", "jetzt", ",", "hei\u00b7li\u00b7ge", ",", "himm\u00b7li\u00b7sche", "Flam\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Jetzt \u2013 Erbarmen \u2013 verzehr ihn noch nicht!", "tokens": ["Jetzt", "\u2013", "Er\u00b7bar\u00b7men", "\u2013", "ver\u00b7zehr", "ihn", "noch", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "$(", "VVIMP", "PPER", "ADV", "PTKNEG", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}