{"dta.poem.12932": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sein sehen bringt ihm den tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schau ich dich nicht, mein Leben!", "tokens": ["Schau", "ich", "dich", "nicht", ",", "mein", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "PTKNEG", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "So ist mein ende da:", "tokens": ["So", "ist", "mein", "en\u00b7de", "da", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schau ich dich, du kalte Sylvia!", "tokens": ["Und", "schau", "ich", "dich", ",", "du", "kal\u00b7te", "Syl\u00b7via", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NE", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "So mu\u00df ich doch den geist aufgeben,", "tokens": ["So", "mu\u00df", "ich", "doch", "den", "geist", "auf\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.5": {"text": "Denn deine grausamkeit erbarmt sich keiner noth.", "tokens": ["Denn", "dei\u00b7ne", "grau\u00b7sam\u00b7keit", "er\u00b7barmt", "sich", "kei\u00b7ner", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Elender lohn, den lieb und treu verspricht!", "tokens": ["E\u00b7len\u00b7der", "lohn", ",", "den", "lieb", "und", "treu", "ver\u00b7spricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Ich schau mein Leben oder nicht;", "tokens": ["Ich", "schau", "mein", "Le\u00b7ben", "o\u00b7der", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So k\u00fcsset mich der kalte tod.", "tokens": ["So", "k\u00fcs\u00b7set", "mich", "der", "kal\u00b7te", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}