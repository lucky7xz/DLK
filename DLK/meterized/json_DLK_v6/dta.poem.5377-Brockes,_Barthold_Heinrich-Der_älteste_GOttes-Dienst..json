{"dta.poem.5377": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der \u00e4lteste GOttes-Dienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Adam ohne S\u00fcnd' in Eden blieben w\u00e4re,", "tokens": ["Wenn", "A\u00b7dam", "oh\u00b7ne", "S\u00fcnd'", "in", "E\u00b7den", "blie\u00b7ben", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sammt der, mit welcher ihn des Sch\u00f6pfers Huld", "tokens": ["Sammt", "der", ",", "mit", "wel\u00b7cher", "ihn", "des", "Sch\u00f6p\u00b7fers", "Huld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "$,", "APPR", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "gepar't;", "tokens": ["ge\u00b7pa\u00b7r't", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "So k\u00f6nnen wir von ihrer Lebens-Art", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "von", "ih\u00b7rer", "Le\u00b7bens\u00b7Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nichts fassen, als da\u00df sie, zu ihres Sch\u00f6pfers Ehren,", "tokens": ["Nichts", "fas\u00b7sen", ",", "als", "da\u00df", "sie", ",", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "KOKOM", "KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zum Preise seiner Lieb\u2019 und seiner weisen Macht,", "tokens": ["Zum", "Prei\u00b7se", "sei\u00b7ner", "Lieb'", "und", "sei\u00b7ner", "wei\u00b7sen", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An aller Creatur Vollkommenheit und Pracht,", "tokens": ["An", "al\u00b7ler", "Crea\u00b7tur", "Voll\u00b7kom\u00b7men\u00b7heit", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "An der Vortreflichkeit und den verlieh\u2019nen Gaben", "tokens": ["An", "der", "Vor\u00b7tre\u00b7flich\u00b7keit", "und", "den", "ver\u00b7lieh'\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Sich w\u00fcrden ungest\u00f6hrt ergetzet haben.", "tokens": ["Sich", "w\u00fcr\u00b7den", "un\u00b7ge\u00b7st\u00f6hrt", "er\u00b7get\u00b7zet", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VAFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Kein andrer GOttes-Dienst, als der allein,", "tokens": ["Kein", "an\u00b7drer", "Got\u00b7tes\u00b7Dienst", ",", "als", "der", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOUS", "ART", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und gar kein\u2019 andre Weise", "tokens": ["Und", "gar", "kein'", "and\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Vom GOttes-Dienst im Paradeise,", "tokens": ["Vom", "Got\u00b7tes\u00b7Dienst", "im", "Pa\u00b7ra\u00b7dei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kann uns begreiflich seyn.", "tokens": ["Kann", "uns", "be\u00b7greif\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Hieraus nun ist ja Sonnen-klar", "tokens": ["Hier\u00b7aus", "nun", "ist", "ja", "Son\u00b7nen\u00b7klar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und \u00fcberzeuglich zu bemercken,", "tokens": ["Und", "\u00fc\u00b7berz\u00b7eug\u00b7lich", "zu", "be\u00b7mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wie GOtt gef\u00e4llig, n\u00f6thig, wahr", "tokens": ["Wie", "Gott", "ge\u00b7f\u00e4l\u00b7lig", ",", "n\u00f6\u00b7thig", ",", "wahr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die Lehre von dem Dienst des Sch\u00f6pfers in den Wercken.", "tokens": ["Die", "Leh\u00b7re", "von", "dem", "Dienst", "des", "Sch\u00f6p\u00b7fers", "in", "den", "Wer\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auf denn, geliebter Mensch, la\u00df deiner Seelen Kr\u00e4fte", "tokens": ["Auf", "denn", ",", "ge\u00b7lieb\u00b7ter", "Mensch", ",", "la\u00df", "dei\u00b7ner", "See\u00b7len", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "$,", "ADJA", "NN", "$,", "VVIMP", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zu dem so n\u00f6th-als n\u00fctz-und fr\u00f6lichen Gesch\u00e4fte", "tokens": ["Zu", "dem", "so", "n\u00f6\u00b7th\u00b7als", "n\u00fctz\u00b7\u00b7und", "fr\u00f6\u00b7li\u00b7chen", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "----+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.20": {"text": "In Andacht rege seyn, bestrebe dich dahin,", "tokens": ["In", "An\u00b7dacht", "re\u00b7ge", "seyn", ",", "be\u00b7stre\u00b7be", "dich", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "VAINF", "$,", "VVFIN", "PRF", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Blos durch Erk\u00e4nntlichkeit getrieben,", "tokens": ["Blos", "durch", "Er\u00b7k\u00e4nnt\u00b7lich\u00b7keit", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Als Sch\u00f6pfer, Geber, HErrn und Vater ihn zu lieben!", "tokens": ["Als", "Sch\u00f6p\u00b7fer", ",", "Ge\u00b7ber", ",", "Herrn", "und", "Va\u00b7ter", "ihn", "zu", "lie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "La\u00df GOtt durch einen jeden Sinn,", "tokens": ["La\u00df", "Gott", "durch", "ei\u00b7nen", "je\u00b7den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Den er dir ja zu diesem Zweck verliehen,", "tokens": ["Den", "er", "dir", "ja", "zu", "die\u00b7sem", "Zweck", "ver\u00b7lie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Zur Ehr\u2019, in deiner Lust, ein Andacht-Opffer gl\u00fchen!", "tokens": ["Zur", "Ehr'", ",", "in", "dei\u00b7ner", "Lust", ",", "ein", "An\u00b7dacht\u00b7Opf\u00b7fer", "gl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Denn nichts so sehr, als die\u00df, kann unsern Geist bereiten", "tokens": ["Denn", "nichts", "so", "sehr", ",", "als", "die\u00df", ",", "kann", "un\u00b7sern", "Geist", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "ADV", "$,", "KOUS", "PDS", "$,", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und nichts so sehr, als die\u00df, kann unsre Seele leiten", "tokens": ["Und", "nichts", "so", "sehr", ",", "als", "die\u00df", ",", "kann", "uns\u00b7re", "See\u00b7le", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "ADV", "$,", "KOUS", "PDS", "$,", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zu den verlohrnen Herrlichkeiten.", "tokens": ["Zu", "den", "ver\u00b7lohr\u00b7nen", "Herr\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}