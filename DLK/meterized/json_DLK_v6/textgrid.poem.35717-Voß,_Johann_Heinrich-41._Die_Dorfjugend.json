{"textgrid.poem.35717": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "41. Die Dorfjugend", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, der K\u00fcster beiert,", "tokens": ["Horch", ",", "der", "K\u00fcs\u00b7ter", "bei\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "M\u00e4dchen, wei\u00df und zart:", "tokens": ["M\u00e4d\u00b7chen", ",", "wei\u00df", "und", "zart", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Morgen wird gefeiert,", "tokens": ["Mor\u00b7gen", "wird", "ge\u00b7fei\u00b7ert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Denk' ich, Himmelfahrt.", "tokens": ["Denk'", "ich", ",", "Him\u00b7mel\u00b7fahrt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dann ist keine Schule,", "tokens": ["Dann", "ist", "kei\u00b7ne", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dann wird Rad und Spule", "tokens": ["Dann", "wird", "Rad", "und", "Spu\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Samt dem Zeichentuch verwahrt.", "tokens": ["Samt", "dem", "Zei\u00b7chen\u00b7tuch", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Glatt im Sonntagsj\u00e4ckchen", "tokens": ["Glatt", "im", "Sonn\u00b7tags\u00b7j\u00e4ck\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mu\u00dft du morgen sein,", "tokens": ["Mu\u00dft", "du", "mor\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VAINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Buntgewirkt das R\u00f6ckchen,", "tokens": ["Bunt\u00b7ge\u00b7wirkt", "das", "R\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Tuch und Sch\u00fcrze fein;", "tokens": ["Tuch", "und", "Sch\u00fcr\u00b7ze", "fein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und die blanke M\u00fctze", "tokens": ["Und", "die", "blan\u00b7ke", "M\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Samt den Schnallen blitze,", "tokens": ["Samt", "den", "Schnal\u00b7len", "blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie du gehst, im Sonnenschein.", "tokens": ["Wie", "du", "gehst", ",", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00e4ngs dem Kirchengange", "tokens": ["L\u00e4ngs", "dem", "Kir\u00b7chen\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gafft dich alles an:", "tokens": ["Gafft", "dich", "al\u00b7les", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seht die schmucke Lange!", "tokens": ["Seht", "die", "schmu\u00b7cke", "Lan\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Seht, sie w\u00e4chst heran!", "tokens": ["Seht", ",", "sie", "w\u00e4chst", "he\u00b7ran", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Selbst der Pfarrer b\u00fccket", "tokens": ["Selbst", "der", "Pfar\u00b7rer", "b\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Fromm das Haupt, und blicket,", "tokens": ["Fromm", "das", "Haupt", ",", "und", "bli\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Was sein Auge blicken kann.", "tokens": ["Was", "sein", "Au\u00b7ge", "bli\u00b7cken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ich, dein Lieber,", "tokens": ["A\u00b7ber", "ich", ",", "dein", "Lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ist das Wetter sch\u00f6n,", "tokens": ["Ist", "das", "Wet\u00b7ter", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Werde gegen\u00fcber", "tokens": ["Wer\u00b7de", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word"], "pos": ["VAFIN", "APPR"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Auch im Schmucke stehn,", "tokens": ["Auch", "im", "Schmu\u00b7cke", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und bei Saitenklange,", "tokens": ["Und", "bei", "Sai\u00b7ten\u00b7klan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Predigt und Gesange,", "tokens": ["Pre\u00b7digt", "und", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dich nur h\u00f6ren, dich nur sehn.", "tokens": ["Dich", "nur", "h\u00f6\u00b7ren", ",", "dich", "nur", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nachmittags dann holen,", "tokens": ["Nach\u00b7mit\u00b7tags", "dann", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Liebchen, du und ich,", "tokens": ["Lieb\u00b7chen", ",", "du", "und", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "KON", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Str\u00e4u\u00dfer von Violen,", "tokens": ["Str\u00e4u\u00b7\u00dfer", "von", "Vi\u00b7o\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kr\u00e4nz' aus M\u00f6serich;", "tokens": ["Kr\u00e4nz'", "aus", "M\u00f6\u00b7se\u00b7rich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und wo gr\u00fcn von Zweigen", "tokens": ["Und", "wo", "gr\u00fcn", "von", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Junge Mai'n sich neigen,", "tokens": ["Jun\u00b7ge", "Mai'n", "sich", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Lagert man am H\u00fcgel sich.", "tokens": ["La\u00b7gert", "man", "am", "H\u00fc\u00b7gel", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n in Strau\u00df und Kranze,", "tokens": ["Sch\u00f6n", "in", "Strau\u00df", "und", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sch\u00f6n wie eine Braut,", "tokens": ["Sch\u00f6n", "wie", "ei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Folgst du mir zum Tanze", "tokens": ["Folgst", "du", "mir", "zum", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sittsam und vertraut:", "tokens": ["Sitt\u00b7sam", "und", "ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da wird frisch gesungen", "tokens": ["Da", "wird", "frisch", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "VVPP"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Und herumgesprungen,", "tokens": ["Und", "her\u00b7um\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Nach des blinden Fiedlers Laut.", "tokens": ["Nach", "des", "blin\u00b7den", "Fied\u00b7lers", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit Gekreisch und Juchen", "tokens": ["Mit", "Ge\u00b7kreisch", "und", "Ju\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schw\u00e4rmt des Dorfs Gew\u00fchl", "tokens": ["Schw\u00e4rmt", "des", "Dorfs", "Ge\u00b7w\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dann um N\u00fc\u00df' und Kuchen", "tokens": ["Dann", "um", "N\u00fc\u00df'", "und", "Ku\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "KON", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und ein Pf\u00e4nderspiel.", "tokens": ["Und", "ein", "Pf\u00e4n\u00b7der\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Aber, kleine Dirne,", "tokens": ["A\u00b7ber", ",", "klei\u00b7ne", "Dir\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gieb mir acht, ich z\u00fcrne,", "tokens": ["Gieb", "mir", "acht", ",", "ich", "z\u00fcr\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "CARD", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "K\u00fcssest du mir allzu viel!", "tokens": ["K\u00fcs\u00b7sest", "du", "mir", "all\u00b7zu", "viel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKA", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}