{"textgrid.poem.56602": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: De Hansohm wahnt ok redi wunnersch\u00f6n!", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Hansohm wahnt ok redi wunnersch\u00f6n!", "tokens": ["De", "Han\u00b7sohm", "wahnt", "ok", "re\u00b7di", "wun\u00b7ner\u00b7sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "De Wisch an Garn un an de Wisch dat Holt,", "tokens": ["De", "Wisch", "an", "Garn", "un", "an", "de", "Wisch", "dat", "Holt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "FM", "APPR", "NE", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Un rechts un links de M\u00f6ller un de Prester:", "tokens": ["Un", "rechts", "un", "links", "de", "M\u00f6l\u00b7ler", "un", "de", "Pres\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "De hett genog vun Eer un ok vun Himmel,", "tokens": ["De", "hett", "ge\u00b7nog", "vun", "E\u00b7er", "un", "ok", "vun", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "De d\u00f6ss ni wannern na Amerika \u2013", "tokens": ["De", "d\u00f6ss", "ni", "wan\u00b7nern", "na", "A\u00b7me\u00b7ri\u00b7ka", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vun v\u0153rn de warme S\u00fcnn, na'n achtern Schatten,", "tokens": ["Vun", "v\u0153rn", "de", "war\u00b7me", "S\u00fcnn", ",", "na'n", "ach\u00b7tern", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Un rund herum de gr\u00f6ne Ogenweid ...", "tokens": ["Un", "rund", "he\u00b7rum", "de", "gr\u00f6\u00b7ne", "O\u00b7gen\u00b7weid", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "He's ok al rut un lett de Ogen lopen,", "tokens": ["He's", "ok", "al", "rut", "un", "lett", "de", "O\u00b7gen", "lo\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "De S\u00fcnn un Wollgefalln op beide Backen.", "tokens": ["De", "S\u00fcnn", "un", "Woll\u00b7ge\u00b7falln", "op", "bei\u00b7de", "Ba\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "He kikt mal \u0153wern Dik un na de Swan,", "tokens": ["He", "kikt", "mal", "\u0153wern", "Dik", "un", "na", "de", "Swan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "NN", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Un h\u00f6rt v\u0153r D\u0153r dat Water un de R\u0153der:", "tokens": ["Un", "h\u00f6rt", "v\u0153r", "D\u0153r", "dat", "Wa\u00b7ter", "un", "de", "R\u0153\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "ART", "NN", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dats nett vun Feerns, ok wenn man \u00f6ller ward", "tokens": ["Dats", "nett", "vun", "Fe\u00b7erns", ",", "ok", "wenn", "man", "\u00f6l\u00b7ler", "ward"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADV", "KOUS", "PIS", "ADJD", "VAFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Un hett al graue Haar, as Hansohm kr\u0119gn hett.", "tokens": ["Un", "hett", "al", "grau\u00b7e", "Haar", ",", "as", "Han\u00b7sohm", "kr\u0119gn", "hett", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Dat ward Een n\u00f6di as dat d\u00e4gli Brot,", "tokens": ["Dat", "ward", "E\u00b7en", "n\u00f6\u00b7di", "as", "dat", "d\u00e4g\u00b7li", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Een smeckt doch nix, wenn dat en daglang feilt,", "tokens": ["E\u00b7en", "smeckt", "doch", "nix", ",", "wenn", "dat", "en", "da\u00b7glang", "feilt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NE", "$,", "KOUS", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Un de Gewohnheit makt dat j\u00fcmmer s\u00f6ter;", "tokens": ["Un", "de", "Ge\u00b7wohn\u00b7heit", "makt", "dat", "j\u00fcm\u00b7mer", "s\u00f6\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Un Hansohm, de ni Kind noch K\u00fcken hett,", "tokens": ["Un", "Han\u00b7sohm", ",", "de", "ni", "Kind", "noch", "K\u00fc\u00b7ken", "hett", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "NN", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "\u2013 Uns Heinri is eerst dar, so lang he grot is \u2013", "tokens": ["\u2013", "Uns", "Hein\u00b7ri", "is", "e\u00b7erst", "dar", ",", "so", "lang", "he", "grot", "is", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "FM", "FM", "ADV", "PTKVZ", "$,", "ADV", "ADJD", "FM", "FM", "FM", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Steit nu binah as fr\u00f6her Obbe d\u0119,", "tokens": ["Steit", "nu", "bi\u00b7nah", "as", "fr\u00f6\u00b7her", "Ob\u00b7be", "d\u0119", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Un kikt na B\u00f6m un B\u00fcsch, as weern't sin Kinner.", "tokens": ["Un", "kikt", "na", "B\u00f6m", "un", "B\u00fcsch", ",", "as", "weern't", "sin", "Kin\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "NN", "$,", "FM", "FM", "FM", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Uns Heinri sluntert lewer langs den Hof", "tokens": ["Uns", "Hein\u00b7ri", "slun\u00b7tert", "le\u00b7wer", "langs", "den", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Un s\u00fcht na Risen un Levkojenplanten,", "tokens": ["Un", "s\u00fcht", "na", "Ri\u00b7sen", "un", "Lev\u00b7ko\u00b7jen\u00b7plan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Un wa de dicken K\u00f6pp vun Bohn un Arfen", "tokens": ["Un", "wa", "de", "di\u00b7cken", "K\u00f6pp", "vun", "Bohn", "un", "Ar\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "In lange Reegn de swarten Betten kl\u00f6vt.", "tokens": ["In", "lan\u00b7ge", "Re\u00b7egn", "de", "swar\u00b7ten", "Bet\u00b7ten", "kl\u00f6vt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Dat is en egen Bengel, still un schu.", "tokens": ["Dat", "is", "en", "e\u00b7gen", "Ben\u00b7gel", ",", "still", "un", "schu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJA", "NN", "$,", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man schull wul meenn, de kunn en Eek utriten:", "tokens": ["Man", "schull", "wul", "me\u00b7enn", ",", "de", "kunn", "en", "E\u00b7ek", "ut\u00b7ri\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dat deit he ok \u2013 son l\u00fcttje as en Finger,", "tokens": ["Dat", "deit", "he", "ok", "\u2013", "son", "l\u00fctt\u00b7je", "as", "en", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "XY", "FM", "FM", "FM", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn he een finnt, wo Minsch un Veh herumpett,", "tokens": ["Wenn", "he", "e\u00b7en", "finnt", ",", "wo", "Minsch", "un", "Veh", "her\u00b7um\u00b7pett", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "VVFIN", "$,", "PWAV", "NN", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Un driggt se sorgsam, as en Vagelnest,", "tokens": ["Un", "driggt", "se", "sorg\u00b7sam", ",", "as", "en", "Va\u00b7gel\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit beide Hann', un plant se still in Garn.", "tokens": ["Mit", "bei\u00b7de", "Hann'", ",", "un", "plant", "se", "still", "in", "Garn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "FM", "FM", "FM", "FM", "APPR", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vunmorgens geit he lingelangs den Hof", "tokens": ["Vun\u00b7mor\u00b7gens", "geit", "he", "lin\u00b7ge\u00b7langs", "den", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Un stellt sik anne Heck, de na de Wisch geit.", "tokens": ["Un", "stellt", "sik", "an\u00b7ne", "Heck", ",", "de", "na", "de", "Wisch", "geit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVFIN", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "He l\u0153hnt sik mit de Ellbagn op de Port,", "tokens": ["He", "l\u0153hnt", "sik", "mit", "de", "Ell\u00b7bagn", "op", "de", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Un s\u00fcht ged\u00fclli na de K\u00f6h int Grasen.", "tokens": ["Un", "s\u00fcht", "ge\u00b7d\u00fcl\u00b7li", "na", "de", "K\u00f6h", "int", "Gra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wa r\u00fckt dat s\u00f6t un kr\u00fcdri na Kaneelbl\u00f6m!", "tokens": ["Wa", "r\u00fckt", "dat", "s\u00f6t", "un", "kr\u00fcd\u00b7ri", "na", "Ka\u00b7neel\u00b7bl\u00f6m", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-++-+---", "measure": "unknown.measure.penta"}, "line.6": {"text": "Un stormt der gr\u00f6n un safti rut ut Gras!", "tokens": ["Un", "stormt", "der", "gr\u00f6n", "un", "saf\u00b7ti", "rut", "ut", "Gras", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Un langs de stille Wisch mit stille K\u00f6h", "tokens": ["Un", "langs", "de", "stil\u00b7le", "Wisch", "mit", "stil\u00b7le", "K\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "NE", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dar drus't un dr\u00f6mt de M\u0153lenb\u0119k hintlank", "tokens": ["Dar", "drus't", "un", "dr\u00f6mt", "de", "M\u0153len\u00b7b\u0119k", "hint\u00b7lank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Un blinkert as en Spegel mit en Goldrahm", "tokens": ["Un", "blin\u00b7kert", "as", "en", "Spe\u00b7gel", "mit", "en", "Gold\u00b7rahm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "FM", "FM", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Vun Botterbl\u00f6m, so sach un doch so ili,", "tokens": ["Vun", "Bot\u00b7ter\u00b7bl\u00f6m", ",", "so", "sach", "un", "doch", "so", "i\u00b7li", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "ADJD", "FM", "ADV", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "As gev dat nerrn in Schatten wunner Wichtigs,", "tokens": ["As", "gev", "dat", "nerrn", "in", "Schat\u00b7ten", "wun\u00b7ner", "Wich\u00b7tigs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Wo he sik mank de Ellernb\u00fcsch verstickt! \u2013 \u2013", "tokens": ["Wo", "he", "sik", "mank", "de", "El\u00b7lern\u00b7b\u00fcsch", "ver\u00b7stickt", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "FM", "FM", "FM", "FM", "NE", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wat knarrt un klirrt dar rechter Hand in Tun,", "tokens": ["Wat", "knarrt", "un", "klirrt", "dar", "rech\u00b7ter", "Hand", "in", "Tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "FM", "VVFIN", "PTKVZ", "ADJA", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "De as en gr\u00f6ne M\u00fcr de Wisch hentlanktreckt?", "tokens": ["De", "as", "en", "gr\u00f6\u00b7ne", "M\u00fcr", "de", "Wisch", "hent\u00b7lank\u00b7treckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Un baben sitt he vull vun witte Bl\u00f6m;", "tokens": ["Un", "ba\u00b7ben", "sitt", "he", "vull", "vun", "wit\u00b7te", "Bl\u00