{"textgrid.poem.38248": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es wollt ein M\u00e4gdlein Wasser holen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein M\u00e4gdlein Wasser holen,", "tokens": ["Es", "wollt", "ein", "M\u00e4gd\u00b7lein", "Was\u00b7ser", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei einem k\u00fchlen Brunnen;", "tokens": ["Bei", "ei\u00b7nem", "k\u00fch\u00b7len", "Brun\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein schneewei\u00df Hemdlein hat sie an,", "tokens": ["Ein", "schnee\u00b7wei\u00df", "Hemd\u00b7lein", "hat", "sie", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dadurch scheint ihr die Sonne.", "tokens": ["Da\u00b7durch", "scheint", "ihr", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Sie sah sich um, sie sah sich her,", "tokens": ["Sie", "sah", "sich", "um", ",", "sie", "sah", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie meint, sie w\u00e4r alleine;", "tokens": ["Sie", "meint", ",", "sie", "w\u00e4r", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kam ein Reuter daher geritten,", "tokens": ["Da", "kam", "ein", "Reu\u00b7ter", "da\u00b7her", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "PAV", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er gr\u00fc\u00dft die Jungfrau reine.", "tokens": ["Er", "gr\u00fc\u00dft", "die", "Jung\u00b7frau", "rei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gott gr\u00fc\u00df euch zartes Jungfr\u00e4ulein,", "tokens": ["Gott", "gr\u00fc\u00df", "euch", "zar\u00b7tes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie stehet ihr hier allein;", "tokens": ["Wie", "ste\u00b7het", "ihr", "hier", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wollt ihr dies Jahr mein Schlafbuhl seyn?", "tokens": ["Wollt", "ihr", "dies", "Jahr", "mein", "Schlaf\u00b7buhl", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ziehet mit mir heime.", "tokens": ["So", "zie\u00b7het", "mit", "mir", "hei\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und euer Schlafbuhl bin ich nicht,", "tokens": ["Und", "eu\u00b7er", "Schlaf\u00b7buhl", "bin", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr bringt mir dann drei Rosen,", "tokens": ["Ihr", "bringt", "mir", "dann", "drei", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die in der Zeit gewachsen seyn,", "tokens": ["Die", "in", "der", "Zeit", "ge\u00b7wach\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl zwischen Weihnacht und Ostern.", "tokens": ["Wohl", "zwi\u00b7schen", "Weih\u00b7nacht", "und", "Os\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Er reit \u00fcber Berg und tiefe Thal,", "tokens": ["Er", "reit", "\u00fc\u00b7ber", "Berg", "und", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er konnt ihrer keine finden;", "tokens": ["Er", "konnt", "ih\u00b7rer", "kei\u00b7ne", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "PIAT", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er reit wohl vor der Mahlerin Th\u00fcr:", "tokens": ["Er", "reit", "wohl", "vor", "der", "Mah\u00b7le\u00b7rin", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Frau Mahlerin seyd ihr darinnen?", "tokens": ["Frau", "Mah\u00b7le\u00b7rin", "seyd", "ihr", "da\u00b7rin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Seyd ihr darin, so kommt herf\u00fcr,", "tokens": ["Seyd", "ihr", "da\u00b7rin", ",", "so", "kommt", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PAV", "$,", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mahlet mir drei Rosen,", "tokens": ["Und", "mah\u00b7let", "mir", "drei", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die dieses Jahr gewachsen seyn,", "tokens": ["Die", "die\u00b7ses", "Jahr", "ge\u00b7wach\u00b7sen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl zwischen Weihnachten und Ostern.", "tokens": ["Wohl", "zwi\u00b7schen", "Weih\u00b7nach\u00b7ten", "und", "Os\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Und da die Rosen gemahlet waren,", "tokens": ["Und", "da", "die", "Ro\u00b7sen", "ge\u00b7mah\u00b7let", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da hub er an zu singen:", "tokens": ["Da", "hub", "er", "an", "zu", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bberfreu dich M\u00e4gdlein, wo du bist,", "tokens": ["\u00bb", "er\u00b7freu", "dich", "M\u00e4gd\u00b7lein", ",", "wo", "du", "bist", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "NN", "$,", "PWAV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drei Rosen thu ich dir bringen.\u00ab", "tokens": ["Drei", "Ro\u00b7sen", "thu", "ich", "dir", "brin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Das M\u00e4gdlein an dem Laden stund,", "tokens": ["Das", "M\u00e4gd\u00b7lein", "an", "dem", "La\u00b7den", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar bitterlich th\u00e4t sie weinen;", "tokens": ["Gar", "bit\u00b7ter\u00b7lich", "th\u00e4t", "sie", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie sprach: Ich habs im Scherz geredt,", "tokens": ["Sie", "sprach", ":", "Ich", "habs", "im", "Scherz", "ge\u00b7redt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "NE", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich meint ihr findet keine!", "tokens": ["Ich", "meint", "ihr", "fin\u00b7det", "kei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hast du es nur im Scherz geredt,", "tokens": ["Hast", "du", "es", "nur", "im", "Scherz", "ge\u00b7redt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar scherzlich woll'n wirs wagen;", "tokens": ["Gar", "scherz\u00b7lich", "woll'n", "wirs", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bin ich dein Scherz, bist du mein Scherz,", "tokens": ["Bin", "ich", "dein", "Scherz", ",", "bist", "du", "mein", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So scherzen wir beid zusammen.", "tokens": ["So", "scher\u00b7zen", "wir", "beid", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}