{"dta.poem.16783": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "142.  Du must es selber seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Frag nicht was G\u00f6ttlich sey: Denn so du es nicht bist/", "tokens": ["Frag", "nicht", "was", "G\u00f6tt\u00b7lich", "sey", ":", "Denn", "so", "du", "es", "nicht", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PWS", "ADJD", "VAFIN", "$.", "KON", "ADV", "PPER", "PPER", "PTKNEG", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So weistu es doch nicht/ ob dus\u2019 gleich h\u00f6rst mein Christ.", "tokens": ["So", "weis\u00b7tu", "es", "doch", "nicht", "/", "ob", "dus'", "gleich", "h\u00f6rst", "mein", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$(", "KOUS", "NE", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}