{"textgrid.poem.35542": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr?", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie bin ich nur so j\u00e4h hierher verschlagen", "tokens": ["Wie", "bin", "ich", "nur", "so", "j\u00e4h", "hier\u00b7her", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dein entfremdet Reich, Waldeinsamkeit?", "tokens": ["In", "dein", "ent\u00b7frem\u00b7det", "Reich", ",", "Wal\u00b7de\u00b7in\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "NN", "$,", "NN", "$."], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Zu Gast war ich in schicksalsk\u00fchnen Tagen", "tokens": ["Zu", "Gast", "war", "ich", "in", "schick\u00b7sals\u00b7k\u00fch\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des S\u00fcdens formgewalt'ger Heiterkeit!", "tokens": ["Des", "S\u00fc\u00b7dens", "form\u00b7ge\u00b7walt'\u00b7ger", "Hei\u00b7ter\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wieder nun des Nordlands Thymiand\u00fcfte", "tokens": ["Und", "wie\u00b7der", "nun", "des", "Nord\u00b7lands", "Thy\u00b7mi\u00b7an\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und seiner Erlenw\u00e4lder Herbstmusik?", "tokens": ["Und", "sei\u00b7ner", "Er\u00b7len\u00b7w\u00e4l\u00b7der", "Herbst\u00b7mu\u00b7sik", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein m\u00fcder, summender Wind ... und tr\u00e4umende Wolkenbilder ...", "tokens": ["Ein", "m\u00fc\u00b7der", ",", "sum\u00b7men\u00b7der", "Wind", "...", "und", "tr\u00e4u\u00b7men\u00b7de", "Wol\u00b7ken\u00b7bil\u00b7der", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nach Mitternacht des Mondes Nebelblick ...", "tokens": ["Nach", "Mit\u00b7ter\u00b7nacht", "des", "Mon\u00b7des", "Ne\u00b7bel\u00b7blick", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und meiner Heimat l\u00e4ngstvergessene Sprache ...", "tokens": ["Und", "mei\u00b7ner", "Hei\u00b7mat", "l\u00e4ngst\u00b7ver\u00b7ges\u00b7se\u00b7ne", "Spra\u00b7che", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und l\u00e4ngst vergessener Menschen Angesicht \u2013", "tokens": ["Und", "l\u00e4ngst", "ver\u00b7ges\u00b7se\u00b7ner", "Men\u00b7schen", "An\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie alles sich einschmeicheln will! \u2013 Ich starre", "tokens": ["Wie", "al\u00b7les", "sich", "ein\u00b7schmei\u00b7cheln", "will", "!", "\u2013", "Ich", "star\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWAV", "PIS", "PRF", "VVINF", "VMFIN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In meines Morgenrots erloschenes Licht ...", "tokens": ["In", "mei\u00b7nes", "Mor\u00b7gen\u00b7rots", "er\u00b7lo\u00b7sche\u00b7nes", "Licht", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Habt ihr mich wieder? \u2013 Bin ich fremd geworden? \u2013", "tokens": ["Habt", "ihr", "mich", "wie\u00b7der", "?", "\u2013", "Bin", "ich", "fremd", "ge\u00b7wor\u00b7den", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "$.", "$(", "VAFIN", "PPER", "ADJD", "VAPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Braunrot quillt auf des Abends Dunstgeflecht ...", "tokens": ["Braun\u00b7rot", "quillt", "auf", "des", "A\u00b7bends", "Dunst\u00b7ge\u00b7flecht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weit ... weit das Land ... die wei\u00dfen Nebel leuchten \u2013", "tokens": ["Weit", "...", "weit", "das", "Land", "...", "die", "wei\u00b7\u00dfen", "Ne\u00b7bel", "leuch\u00b7ten", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu mir tritt meiner Sehnsucht Lichtgeschlecht \u2013", "tokens": ["Zu", "mir", "tritt", "mei\u00b7ner", "Sehn\u00b7sucht", "Licht\u00b7ge\u00b7schlecht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort, wo das Leben reinere Glieder rundet,", "tokens": ["Dort", ",", "wo", "das", "Le\u00b7ben", "rei\u00b7ne\u00b7re", "Glie\u00b7der", "run\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zu gr\u00f6\u00dferer F\u00fclle seine Kr\u00e4fte stimmt,", "tokens": ["Zu", "gr\u00f6\u00b7\u00dfe\u00b7rer", "F\u00fcl\u00b7le", "sei\u00b7ne", "Kr\u00e4f\u00b7te", "stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "M\u00f6cht' ich mit dir, Geliebte, sonnumstundet", "tokens": ["M\u00f6cht'", "ich", "mit", "dir", ",", "Ge\u00b7lieb\u00b7te", ",", "son\u00b7numst\u00b7un\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Sein vertr\u00e4umen, bis es sanft verschwimmt ...", "tokens": ["Mein", "Sein", "ver\u00b7tr\u00e4u\u00b7men", ",", "bis", "es", "sanft", "ver\u00b7schwimmt", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wir lugen weit ... weit \u00fcbers Meer, das blaue \u2013", "tokens": ["Wir", "lu\u00b7gen", "weit", "...", "weit", "\u00fc\u00b7bers", "Meer", ",", "das", "blau\u00b7e", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "ADJD", "APPRART", "NN", "$,", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um stillere Inseln noch wirbt unser Blick ...", "tokens": ["Um", "stil\u00b7le\u00b7re", "In\u00b7seln", "noch", "wirbt", "un\u00b7ser", "Blick", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wenn ich dann in deine Augen schaue,", "tokens": ["Und", "wenn", "ich", "dann", "in", "dei\u00b7ne", "Au\u00b7gen", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Find' ich erschweigend mein intimstes Gl\u00fcck ...", "tokens": ["Find'", "ich", "er\u00b7schwei\u00b7gend", "mein", "in\u00b7tims\u00b7tes", "Gl\u00fcck", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zu Zeiten, die gewesen ... ungewesen ...", "tokens": ["Zu", "Zei\u00b7ten", ",", "die", "ge\u00b7we\u00b7sen", "...", "un\u00b7ge\u00b7we\u00b7sen", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "VAPP", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beruhigt unsere Gegenwart verflie\u00dft ...", "tokens": ["Be\u00b7ru\u00b7higt", "un\u00b7se\u00b7re", "Ge\u00b7gen\u00b7wart", "ver\u00b7flie\u00dft", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und von der D\u00e4mmerung Schattenspiel genesen,", "tokens": ["Und", "von", "der", "D\u00e4m\u00b7me\u00b7rung", "Schat\u00b7ten\u00b7spiel", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ward uns der Geist, der lichterf\u00fcllt genie\u00dft \u2013", "tokens": ["Ward", "uns", "der", "Geist", ",", "der", "lich\u00b7ter\u00b7f\u00fcllt", "ge\u00b7nie\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Bis er, am Horizont ein Wolkenst\u00e4ubchen,", "tokens": ["Bis", "er", ",", "am", "Ho\u00b7ri\u00b7zont", "ein", "Wol\u00b7ken\u00b7st\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darauf die Sonne lag mit mildem Gl\u00fchn,", "tokens": ["Da\u00b7rauf", "die", "Son\u00b7ne", "lag", "mit", "mil\u00b7dem", "Gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich sanft entkr\u00e4uselt ... Weiter rollen Stunden ...", "tokens": ["Sich", "sanft", "ent\u00b7kr\u00e4u\u00b7selt", "...", "Wei\u00b7ter", "rol\u00b7len", "Stun\u00b7den", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$(", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Jahre, Menschen, Sterne weiterziehn ...", "tokens": ["Und", "Jah\u00b7re", ",", "Men\u00b7schen", ",", "Ster\u00b7ne", "wei\u00b7ter\u00b7ziehn", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Geliebte Heimat, den nun deine Krume", "tokens": ["Ge\u00b7lieb\u00b7te", "Hei\u00b7mat", ",", "den", "nun", "dei\u00b7ne", "Kru\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum letztenmal mit ihrem Herbst gen\u00e4hrt \u2013", "tokens": ["Zum", "letz\u00b7ten\u00b7mal", "mit", "ih\u00b7rem", "Herbst", "ge\u00b7n\u00e4hrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verzeihe ihm! Gib ihm zum Abschied deine letzte Blume \u2013", "tokens": ["Ver\u00b7zei\u00b7he", "ihm", "!", "Gib", "ihm", "zum", "Ab\u00b7schied", "dei\u00b7ne", "letz\u00b7te", "Blu\u00b7me", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVIMP", "PPER", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und la\u00df ihn ziehn, der deiner nicht mehr wert ...", "tokens": ["Und", "la\u00df", "ihn", "ziehn", ",", "der", "dei\u00b7ner", "nicht", "mehr", "wert", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "VVINF", "$,", "PRELS", "PPOSAT", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}