{"textgrid.poem.56413": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zehntausend steigen von den Bergen nieder,", "tokens": ["Zehn\u00b7tau\u00b7send", "stei\u00b7gen", "von", "den", "Ber\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bl\u00fcte Hellas', sich dem Tod zu weihen.", "tokens": ["Die", "Bl\u00fc\u00b7te", "Hel\u00b7las'", ",", "sich", "dem", "Tod", "zu", "wei\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Morgend\u00e4mmrung ziehen ihre Reihen.", "tokens": ["Durch", "Mor\u00b7gen\u00b7d\u00e4mm\u00b7rung", "zie\u00b7hen", "ih\u00b7re", "Rei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Wall von Erz ziehn hin des Heeres Glieder.", "tokens": ["Ein", "Wall", "von", "Erz", "ziehn", "hin", "des", "Hee\u00b7res", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Lerchen singen ihre Morgenlieder,", "tokens": ["Die", "Ler\u00b7chen", "sin\u00b7gen", "ih\u00b7re", "Mor\u00b7gen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie schwingen sich zum Himmel ohne Zahl.", "tokens": ["Sie", "schwin\u00b7gen", "sich", "zum", "Him\u00b7mel", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr helles Singen f\u00fcllt das ganze Tal,", "tokens": ["Ihr", "hel\u00b7les", "Sin\u00b7gen", "f\u00fcllt", "das", "gan\u00b7ze", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie steigen in dem Blauen auf und nieder.", "tokens": ["Sie", "stei\u00b7gen", "in", "dem", "Blau\u00b7en", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch sind die Morgenwinde nicht erwacht.", "tokens": ["Noch", "sind", "die", "Mor\u00b7gen\u00b7win\u00b7de", "nicht", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In s\u00fc\u00dfem Schlummer liegt noch weit die Welt,", "tokens": ["In", "s\u00fc\u00b7\u00dfem", "Schlum\u00b7mer", "liegt", "noch", "weit", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Morgenstern steht noch in keuscher Pracht.", "tokens": ["Der", "Mor\u00b7gens\u00b7tern", "steht", "noch", "in", "keu\u00b7scher", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Eub\u00f6a nur ist weithin schon erhellt.", "tokens": ["Eu\u00b7b\u00f6a", "nur", "ist", "weit\u00b7hin", "schon", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Da rauscht die Sonne aus des Meeres Schacht", "tokens": ["Da", "rauscht", "die", "Son\u00b7ne", "aus", "des", "Mee\u00b7res", "Schacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und vor dem Heere liegen Zelt bei Zelt.", "tokens": ["Und", "vor", "dem", "Hee\u00b7re", "lie\u00b7gen", "Zelt", "bei", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}