{"textgrid.poem.38801": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[dein harrend, sinnend, ganz von Liebe-Denken]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein harrend, sinnend, ganz von Liebe-Denken", "tokens": ["Dein", "har\u00b7rend", ",", "sin\u00b7nend", ",", "ganz", "von", "Lie\u00b7be\u00b7Den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "$,", "VVPP", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umringt, von Schmerzen, die mich hold umspielen,", "tokens": ["Um\u00b7ringt", ",", "von", "Schmer\u00b7zen", ",", "die", "mich", "hold", "um\u00b7spie\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00df ich dein Fernsein mir recht nahe f\u00fchlen,", "tokens": ["Mu\u00df", "ich", "dein", "Fern\u00b7sein", "mir", "recht", "na\u00b7he", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Baum und Bl\u00fcthe steigen sie und senken", "tokens": ["Aus", "Baum", "und", "Bl\u00fc\u00b7the", "stei\u00b7gen", "sie", "und", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "PPER", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sich in mein Herz, mit Thr\u00e4nen l\u00e4chelnd schenken", "tokens": ["Sich", "in", "mein", "Herz", ",", "mit", "Thr\u00e4\u00b7nen", "l\u00e4\u00b7chelnd", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie Kinderblicke, alle Strahlen zielen", "tokens": ["Sie", "Kin\u00b7der\u00b7bli\u00b7cke", ",", "al\u00b7le", "Strah\u00b7len", "zie\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach meinen Herzens-Augen; lieblich k\u00fchlen", "tokens": ["Nach", "mei\u00b7nen", "Her\u00b7zens\u00b7Au\u00b7gen", ";", "lieb\u00b7lich", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Thr\u00e4nen m\u00f6chte sich dies Angedenken.", "tokens": ["In", "Thr\u00e4\u00b7nen", "m\u00f6ch\u00b7te", "sich", "dies", "An\u00b7ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PRF", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So bin ich doch, Alma, an deiner Seite,", "tokens": ["So", "bin", "ich", "doch", ",", "Al\u00b7ma", ",", "an", "dei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir lacht Gr\u00fcn, Himmelblau, des Sees Gl\u00e4nzen,", "tokens": ["Dir", "lacht", "Gr\u00fcn", ",", "Him\u00b7mel\u00b7blau", ",", "des", "Sees", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Aus duftger Luft spricht Lieb', aus Waldgefieder;", "tokens": ["Aus", "duft\u00b7ger", "Luft", "spricht", "Lieb'", ",", "aus", "Wald\u00b7ge\u00b7fie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vernimmt dein Ohr die s\u00fc\u00dfen Liebeslieder,", "tokens": ["Ver\u00b7nimmt", "dein", "Ohr", "die", "s\u00fc\u00b7\u00dfen", "Lie\u00b7bes\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Siehst du winken nach mir in Blumenkr\u00e4nzen", "tokens": ["Siehst", "du", "win\u00b7ken", "nach", "mir", "in", "Blu\u00b7men\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPER", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die Liebe, wird ein Nahsein jede Weite.", "tokens": ["Die", "Lie\u00b7be", ",", "wird", "ein", "Na\u00b7hsein", "je\u00b7de", "Wei\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}