{"textgrid.poem.35559": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus braunen Mauern tritt ein Dorf, ein Feld.", "tokens": ["Aus", "brau\u00b7nen", "Mau\u00b7ern", "tritt", "ein", "Dorf", ",", "ein", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Hirt verwest auf einem alten Stein.", "tokens": ["Ein", "Hirt", "ver\u00b7west", "auf", "ei\u00b7nem", "al\u00b7ten", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Saum des Walds schlie\u00dft blaue Tiere ein,", "tokens": ["Der", "Saum", "des", "Walds", "schlie\u00dft", "blau\u00b7e", "Tie\u00b7re", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das sanfte Laub, das in die Stille f\u00e4llt.", "tokens": ["Das", "sanf\u00b7te", "Laub", ",", "das", "in", "die", "Stil\u00b7le", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Bauern braune Stirnen. Lange t\u00f6nt", "tokens": ["Der", "Bau\u00b7ern", "brau\u00b7ne", "Stir\u00b7nen", ".", "Lan\u00b7ge", "t\u00f6nt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Abendglocke; sch\u00f6n ist frommer Brauch,", "tokens": ["Die", "A\u00b7bend\u00b7glo\u00b7cke", ";", "sch\u00f6n", "ist", "from\u00b7mer", "Brauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Heilands schwarzes Haupt im Dornenstrauch,", "tokens": ["Des", "Hei\u00b7lands", "schwar\u00b7zes", "Haupt", "im", "Dor\u00b7nen\u00b7strauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die k\u00fchle Stube, die der Tod vers\u00f6hnt.", "tokens": ["Die", "k\u00fch\u00b7le", "Stu\u00b7be", ",", "die", "der", "Tod", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie bleich die M\u00fctter sind. Die Bl\u00e4ue sinkt", "tokens": ["Wie", "bleich", "die", "M\u00fct\u00b7ter", "sind", ".", "Die", "Bl\u00e4u\u00b7e", "sinkt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VAFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Glas und Truh, die stolz ihr Sinn bewahrt;", "tokens": ["Auf", "Glas", "und", "Truh", ",", "die", "stolz", "ihr", "Sinn", "be\u00b7wahrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch neigt ein wei\u00dfes Haupt sich hochbejahrt", "tokens": ["Auch", "neigt", "ein", "wei\u00b7\u00dfes", "Haupt", "sich", "hoch\u00b7be\u00b7jahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aufs Enkelkind, das Milch und Sterne trinkt.", "tokens": ["Aufs", "En\u00b7kel\u00b7kind", ",", "das", "Milch", "und", "Ster\u00b7ne", "trinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}