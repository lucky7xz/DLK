{"textgrid.poem.53267": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[mein letztes Hoffen wird erf\u00fcllt]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein letztes Hoffen wird erf\u00fcllt;", "tokens": ["Mein", "letz\u00b7tes", "Hof\u00b7fen", "wird", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich scheide: stillt, ihr Freunde, stillt", "tokens": ["Ich", "schei\u00b7de", ":", "stillt", ",", "ihr", "Freun\u00b7de", ",", "stillt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Klagen, die jhr f\u00fchret;", "tokens": ["Die", "Kla\u00b7gen", ",", "die", "jhr", "f\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rt endlich auff, es ist genug,", "tokens": ["H\u00f6rt", "end\u00b7lich", "auff", ",", "es", "ist", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mi\u00dfg\u00f6nt mir nicht den edlen Schmuck", "tokens": ["Mi\u00df\u00b7g\u00f6nt", "mir", "nicht", "den", "ed\u00b7len", "Schmuck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Kronen, die mich zieret.", "tokens": ["Der", "Kro\u00b7nen", ",", "die", "mich", "zie\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gott selber reisst mich von euch hin,", "tokens": ["Gott", "sel\u00b7ber", "reisst", "mich", "von", "euch", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bey dem ich gleichwol lieber bin,", "tokens": ["Bey", "dem", "ich", "gleich\u00b7wol", "lie\u00b7ber", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ob mich nach euch verlanget;", "tokens": ["Ob", "mich", "nach", "euch", "ver\u00b7lan\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ihr liebet mich: Gott noch viel mehr,", "tokens": ["Ihr", "lie\u00b7bet", "mich", ":", "Gott", "noch", "viel", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Nach dessen Rath in andrer Ehr'", "tokens": ["Nach", "des\u00b7sen", "Rath", "in", "an\u00b7drer", "Ehr'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Itzt meine Seele pranget.", "tokens": ["Itzt", "mei\u00b7ne", "See\u00b7le", "pran\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was aller Frommen h\u00f6chstes Gut", "tokens": ["Was", "al\u00b7ler", "From\u00b7men", "h\u00f6chs\u00b7tes", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd Hoffnung ist, durch Gottes Blut", "tokens": ["Vnd", "Hoff\u00b7nung", "ist", ",", "durch", "Got\u00b7tes", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So thewer vor erworben,", "tokens": ["So", "the\u00b7wer", "vor", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Besitz' ich schon. Welt, gute Nacht!", "tokens": ["Be\u00b7sitz'", "ich", "schon", ".", "Welt", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Anmuth deiner gantzen Pracht", "tokens": ["Der", "An\u00b7muth", "dei\u00b7ner", "gant\u00b7zen", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist bey mir nun erstorben.", "tokens": ["Ist", "bey", "mir", "nun", "ers\u00b7tor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nicht aller Reichthumb, alle Lust,", "tokens": ["Nicht", "al\u00b7ler", "Reicht\u00b7humb", ",", "al\u00b7le", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd was dir hohes ist bewust,", "tokens": ["Vnd", "was", "dir", "ho\u00b7hes", "ist", "be\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJA", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kan mich herwieder bringen:", "tokens": ["Kan", "mich", "her\u00b7wie\u00b7der", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die s\u00fcsse Ruh, der Engel Chor,", "tokens": ["Die", "s\u00fcs\u00b7se", "Ruh", ",", "der", "En\u00b7gel", "Chor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die Seelen, die hieher zuvor", "tokens": ["Die", "See\u00b7len", ",", "die", "hie\u00b7her", "zu\u00b7vor"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "ADV"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Sind kommen mich bezwingen.", "tokens": ["Sind", "kom\u00b7men", "mich", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier seh' ich, was der Zeiten List", "tokens": ["Hier", "seh'", "ich", ",", "was", "der", "Zei\u00b7ten", "List"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seele zu ber\u00fccken ist,", "tokens": ["Die", "See\u00b7le", "zu", "be\u00b7r\u00fc\u00b7cken", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was Frewd' vnd Wollust k\u00f6nnen;", "tokens": ["Was", "Fre\u00b7wd'", "vnd", "Wol\u00b7lust", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VMFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hier lach' ich aller Menschen M\u00fch'", "tokens": ["Hier", "lach'", "ich", "al\u00b7ler", "Men\u00b7schen", "M\u00fch'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd Sorgen, die sie spat vnd fr\u00fch", "tokens": ["Vnd", "Sor\u00b7gen", ",", "die", "sie", "spat", "vnd", "fr\u00fch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In jhrer Flucht beginnen.", "tokens": ["In", "jhrer", "Flucht", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie wann ein Schiff durch strengen Nord", "tokens": ["Wie", "wann", "ein", "Schiff", "durch", "stren\u00b7gen", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In seine Sicherheit vnd Port", "tokens": ["In", "sei\u00b7ne", "Si\u00b7cher\u00b7heit", "vnd", "Port"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Itzt gl\u00fccklich ist getrieben:", "tokens": ["Itzt", "gl\u00fcck\u00b7lich", "ist", "ge\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "So bin ich auff der w\u00fcsten See", "tokens": ["So", "bin", "ich", "auff", "der", "w\u00fcs\u00b7ten", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Welt entgangen allem Weh'", "tokens": ["Der", "Welt", "ent\u00b7gan\u00b7gen", "al\u00b7lem", "Weh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Vnd ruhe nach belieben.", "tokens": ["Vnd", "ru\u00b7he", "nach", "be\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr, deine hand mich sicher helt.", "tokens": ["Herr", ",", "dei\u00b7ne", "hand", "mich", "si\u00b7cher", "helt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mich forthin kein Vngl\u00fcck fellt,", "tokens": ["Da\u00df", "mich", "for\u00b7thin", "kein", "Vn\u00b7gl\u00fcck", "fellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Das andre noch verwirret;", "tokens": ["Das", "and\u00b7re", "noch", "ver\u00b7wir\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du hast mich selber angethan", "tokens": ["Du", "hast", "mich", "sel\u00b7ber", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit deiner Krafft, da\u00df ich der Bahn", "tokens": ["Mit", "dei\u00b7ner", "Krafft", ",", "da\u00df", "ich", "der", "Bahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Lebens nicht geirret.", "tokens": ["Des", "Le\u00b7bens", "nicht", "ge\u00b7ir\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich warte, wenn das feste Band,", "tokens": ["Ich", "war\u00b7te", ",", "wenn", "das", "fes\u00b7te", "Band", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das jetzt der grimme Todt getrannt,", "tokens": ["Das", "jetzt", "der", "grim\u00b7me", "Todt", "ge\u00b7trannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sol wieder einig werden:", "tokens": ["Sol", "wie\u00b7der", "ei\u00b7nig", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da werd' ich erst f\u00fcr deine Trew", "tokens": ["Da", "werd'", "ich", "erst", "f\u00fcr", "dei\u00b7ne", "Trew"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dich loben, mir bezeiget frey", "tokens": ["Dich", "lo\u00b7ben", ",", "mir", "be\u00b7zei\u00b7get", "frey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Im Himmel vnd auff Erden.", "tokens": ["Im", "Him\u00b7mel", "vnd", "auff", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}