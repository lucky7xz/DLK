{"textgrid.poem.59357": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gute Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00f6hn und W\u00e4lder schon steigen", "tokens": ["Die", "H\u00f6hn", "und", "W\u00e4l\u00b7der", "schon", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Immer tiefer ins Abendgold,", "tokens": ["Im\u00b7mer", "tie\u00b7fer", "ins", "A\u00b7bend\u00b7gold", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ein V\u00f6glein fr\u00e4gt in den Zweigen:", "tokens": ["Ein", "V\u00f6\u00b7glein", "fr\u00e4gt", "in", "den", "Zwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob es Liebchen gr\u00fc\u00dfen sollt?", "tokens": ["Ob", "es", "Lieb\u00b7chen", "gr\u00fc\u00b7\u00dfen", "sollt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O V\u00f6glein, du hast dich betrogen,", "tokens": ["O", "V\u00f6\u00b7glein", ",", "du", "hast", "dich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wohnet nicht mehr im Tal,", "tokens": ["Sie", "woh\u00b7net", "nicht", "mehr", "im", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schwing auf dich zum Himmelsbogen,", "tokens": ["Schwing", "auf", "dich", "zum", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00df sie droben zum letztenmal!", "tokens": ["Gr\u00fc\u00df", "sie", "dro\u00b7ben", "zum", "letz\u00b7ten\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}