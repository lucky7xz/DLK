{"textgrid.poem.50219": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "51. An einen eifrigen Schulmeister", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Sch\u00fcler, weil du lehrst, ", "tokens": ["Dein", "Sch\u00fc\u00b7ler", ",", "weil", "du", "lehrst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du lehrst das ", "tokens": ["Du", "lehrst", "das"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch findet man, dass ihr ein ander wenig lehret,", "tokens": ["Doch", "fin\u00b7det", "man", ",", "dass", "ihr", "ein", "an\u00b7der", "we\u00b7nig", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil Du so wenig ihn, als dich der Sch\u00fcler ", "tokens": ["Weil", "Du", "so", "we\u00b7nig", "ihn", ",", "als", "dich", "der", "Sch\u00fc\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "PPER", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum ", "tokens": ["Drum"], "token_info": ["word"], "pos": ["PAV"], "meter": "-", "measure": "single.down"}}}}}