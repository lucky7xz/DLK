{"textgrid.poem.48306": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Schwerin", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun aber soll erschallen", "tokens": ["Nun", "a\u00b7ber", "soll", "er\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir Preis und Ruhm, ", "tokens": ["Dir", "Preis", "und", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der du vor Prag gefallen", "tokens": ["Der", "du", "vor", "Prag", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NE", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Sturme der Batt'rien;", "tokens": ["Beim", "Stur\u00b7me", "der", "Batt'\u00b7ri\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Es lebt in eins verschlungen", "tokens": ["Es", "lebt", "in", "eins", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbschwerin\u00ab und \u00bbSchlacht bei Prag\u00ab,", "tokens": ["\u00bb", "schwe\u00b7rin", "\u00ab", "und", "\u00bb", "Schlacht", "bei", "Prag", "\u00ab", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$(", "KON", "$(", "NN", "APPR", "NE", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Drum sei dein Lob gesungen", "tokens": ["Drum", "sei", "dein", "Lob", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch deinen Ehrentag. \u2013", "tokens": ["Durch", "dei\u00b7nen", "Eh\u00b7ren\u00b7tag", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des sechsten Maies Morgen", "tokens": ["Des", "sechs\u00b7ten", "Mai\u00b7es", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwebt \u00fcber Berg und Au,", "tokens": ["Schwebt", "\u00fc\u00b7ber", "Berg", "und", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Feind ist wohlgeborgen", "tokens": ["Der", "Feind", "ist", "wohl\u00b7ge\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch Gr\u00e4ben und Verhau;", "tokens": ["Durch", "Gr\u00e4\u00b7ben", "und", "Ver\u00b7hau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es halten seine Fl\u00fcgel", "tokens": ["Es", "hal\u00b7ten", "sei\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die H\u00f6hen rings besetzt,", "tokens": ["Die", "H\u00f6\u00b7hen", "rings", "be\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein feuerspei'nder H\u00fcgel", "tokens": ["Ein", "feu\u00b7er\u00b7spei'n\u00b7der", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist jede Kuppe jetzt.", "tokens": ["Ist", "je\u00b7de", "Kup\u00b7pe", "jetzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier wird die Schlacht geschlagen!", "tokens": ["Hier", "wird", "die", "Schlacht", "ge\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steil ist die Bergesbahn,", "tokens": ["Steil", "ist", "die", "Ber\u00b7ges\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch siegen und nicht wagen,", "tokens": ["Doch", "sie\u00b7gen", "und", "nicht", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das hei\u00dft nur halb getan;", "tokens": ["Das", "hei\u00dft", "nur", "halb", "ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Grenadiere st\u00fcrmen,", "tokens": ["Die", "Gre\u00b7na\u00b7die\u00b7re", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kart\u00e4tschen prasseln drauf,", "tokens": ["Kar\u00b7t\u00e4t\u00b7schen", "pras\u00b7seln", "drauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Sich Leichenh\u00fcgel auf.", "tokens": ["Sich", "Lei\u00b7chen\u00b7h\u00fc\u00b7gel", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Am Boden liegt vernichtet", "tokens": ["Am", "Bo\u00b7den", "liegt", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwerins Leibbataillon;", "tokens": ["Schwe\u00b7rins", "Leib\u00b7ba\u00b7tail\u00b7lon", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ein Eichwald, tief gelichtet,", "tokens": ["Ein", "Eich\u00b7wald", ",", "tief", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So steht ein zweites schon;", "tokens": ["So", "steht", "ein", "zwei\u00b7tes", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Getroffen sinkt danieder", "tokens": ["Ge\u00b7trof\u00b7fen", "sinkt", "da\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gen'ral ", "tokens": ["Gen'\u00b7ral"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und die zerscho\u00dfnen Glieder", "tokens": ["Und", "die", "zer\u00b7scho\u00df\u00b7nen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nichts mehr im Feuer h\u00e4lt.", "tokens": ["Nichts", "mehr", "im", "Feu\u00b7er", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie fliehn. Die alte Erde", "tokens": ["Sie", "fliehn", ".", "Die", "al\u00b7te", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bebt selbst, als ob ihr's graut,", "tokens": ["Bebt", "selbst", ",", "als", "ob", "ih\u00b7r's", "graut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da steigt ", "tokens": ["Da", "steigt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "\u00bbmir nach!\u00ab so ruft er laut;", "tokens": ["\u00bb", "mir", "nach", "!", "\u00ab", "so", "ruft", "er", "laut", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er fa\u00dft die alte Fahne,", "tokens": ["Er", "fa\u00dft", "die", "al\u00b7te", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Noch nie zur Flucht gewandt,", "tokens": ["Noch", "nie", "zur", "Flucht", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er den Sieg erbahne", "tokens": ["Da\u00df", "er", "den", "Sieg", "er\u00b7bah\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit seiner Greisenhand. \u2013", "tokens": ["Mit", "sei\u00b7ner", "Grei\u00b7sen\u00b7hand", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die H\u00fcgel sind erstiegen,", "tokens": ["Die", "H\u00fc\u00b7gel", "sind", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Kaiserlichen fliehn,", "tokens": ["Die", "Kai\u00b7ser\u00b7li\u00b7chen", "fliehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch trauervolles Siegen,", "tokens": ["Doch", "trau\u00b7er\u00b7vol\u00b7les", "Sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Sterben liegt \u2013 ", "tokens": ["Im", "Ster\u00b7ben", "liegt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vier Kugeln, erzgegossen,", "tokens": ["Vier", "Ku\u00b7geln", ",", "erz\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["CARD", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie haben ihn zerfetzt,", "tokens": ["Sie", "ha\u00b7ben", "ihn", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Fahne, die zerschossen,", "tokens": ["Die", "Fah\u00b7ne", ",", "die", "zer\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Bahrtuch ist sie jetzt.", "tokens": ["Sein", "Bahr\u00b7tuch", "ist", "sie", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Truppen ziehn vor\u00fcber", "tokens": ["Die", "Trup\u00b7pen", "ziehn", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit dumpfem Trommelschlag,", "tokens": ["Mit", "dum\u00b7pfem", "Trom\u00b7mel\u00b7schlag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Solch Tag des Gl\u00fccks ist tr\u00fcber", "tokens": ["Solch", "Tag", "des", "Gl\u00fccks", "ist", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als mancher Ungl\u00fcckstag;", "tokens": ["Als", "man\u00b7cher", "Un\u00b7gl\u00fccks\u00b7tag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie Wetterwolkenschwere", "tokens": ["Wie", "Wet\u00b7ter\u00b7wol\u00b7ken\u00b7schwe\u00b7re"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sieht man's am Himmel ziehn,", "tokens": ["Sieht", "man's", "am", "Him\u00b7mel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie ziehen vorauf dem Heere,", "tokens": ["Sie", "zie\u00b7hen", "vor\u00b7auf", "dem", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Sich lagernd \u00fcber \u2013 ", "tokens": ["Sich", "la\u00b7gernd", "\u00fc\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}