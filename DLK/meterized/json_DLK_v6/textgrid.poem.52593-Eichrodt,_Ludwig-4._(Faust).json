{"textgrid.poem.52593": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4. (Faust)", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist der sanften Triebe", "tokens": ["Es", "ist", "der", "sanf\u00b7ten", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Friedfertige Gewalt,", "tokens": ["Fried\u00b7fer\u00b7ti\u00b7ge", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die br\u00fcderliche Liebe", "tokens": ["Die", "br\u00fc\u00b7der\u00b7li\u00b7che", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Entschlafen o so bald!", "tokens": ["Ent\u00b7schla\u00b7fen", "o", "so", "bald", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Geist und deine Sch\u00f6nheit", "tokens": ["Dein", "Geist", "und", "dei\u00b7ne", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Trafen mein Herz so j\u00e4h,", "tokens": ["Tra\u00b7fen", "mein", "Herz", "so", "j\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Mit ihrem k\u00fchnen Strahle,", "tokens": ["Mit", "ih\u00b7rem", "k\u00fch\u00b7nen", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df ich in Flammen steh.", "tokens": ["Da\u00df", "ich", "in", "Flam\u00b7men", "steh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nun ist auch der Gedanken", "tokens": ["Nun", "ist", "auch", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unb\u00e4ndiger Sturmwind los \u2013", "tokens": ["Un\u00b7b\u00e4n\u00b7di\u00b7ger", "Sturm\u00b7wind", "los", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich mu\u00df ins Meer versinken,", "tokens": ["Ich", "mu\u00df", "ins", "Meer", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Meer der Wonne sinken,", "tokens": ["Ins", "Meer", "der", "Won\u00b7ne", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O K\u00f6nigin der Seele", "tokens": ["O", "K\u00f6\u00b7ni\u00b7gin", "der", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich sinke in deinen Schoos!", "tokens": ["Ich", "sin\u00b7ke", "in", "dei\u00b7nen", "Schoos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}