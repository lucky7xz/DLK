{"dta.poem.14121": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtvogel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Liegt der Tag rings auf der Lauer,", "tokens": ["Liegt", "der", "Tag", "rings", "auf", "der", "Lau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt so schlau auf Lust und Trauer:", "tokens": ["Blickt", "so", "schlau", "auf", "Lust", "und", "Trau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann ich kaum mich selbst verstehen.", "tokens": ["Kann", "ich", "kaum", "mich", "selbst", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df die Lauscher schlafen gehen!", "tokens": ["La\u00df", "die", "Lau\u00b7scher", "schla\u00b7fen", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur ein St\u00fcndchen unbewacht", "tokens": ["Nur", "ein", "St\u00fcnd\u00b7chen", "un\u00b7be\u00b7wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df' in der verschwiegenen Nacht", "tokens": ["La\u00df'", "in", "der", "ver\u00b7schwie\u00b7ge\u00b7nen", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Mich in Deine Augen sehen", "tokens": ["Mich", "in", "Dei\u00b7ne", "Au\u00b7gen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie in stillen Mondenschein.", "tokens": ["Wie", "in", "stil\u00b7len", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In dem Park an der Rotunde,", "tokens": ["In", "dem", "Park", "an", "der", "Ro\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn es dunkelt, harr' ich Dein.", "tokens": ["Wenn", "es", "dun\u00b7kelt", ",", "harr'", "ich", "Dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Still und fromm ja will ich sein.", "tokens": ["Still", "und", "fromm", "ja", "will", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ADV", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Liebste, ach nur Eine Stunde! \u2014", "tokens": ["Liebs\u00b7te", ",", "ach", "nur", "Ei\u00b7ne", "Stun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ITJ", "ADV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Sieh' mir nicht so b\u00f6se drein!", "tokens": ["Sieh'", "mir", "nicht", "so", "b\u00f6\u00b7se", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Willst Du nie Dein Schweigen brechen,", "tokens": ["Willst", "Du", "nie", "Dein", "Schwei\u00b7gen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ewig stumm, wie Blumen, sein:", "tokens": ["E\u00b7wig", "stumm", ",", "wie", "Blu\u00b7men", ",", "sein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NN", "$,", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "O so la\u00df mich das Versprechen", "tokens": ["O", "so", "la\u00df", "mich", "das", "Ver\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVIMP", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Pfl\u00fccken Dir vom stillen Munde:", "tokens": ["Pfl\u00fc\u00b7cken", "Dir", "vom", "stil\u00b7len", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Liebste, ach nur Eine Stunde!", "tokens": ["Liebs\u00b7te", ",", "ach", "nur", "Ei\u00b7ne", "Stun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "In dem Park, an der Rotunde,", "tokens": ["In", "dem", "Park", ",", "an", "der", "Ro\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wenn es dunkelt, harr' ich Dein.", "tokens": ["Wenn", "es", "dun\u00b7kelt", ",", "harr'", "ich", "Dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}