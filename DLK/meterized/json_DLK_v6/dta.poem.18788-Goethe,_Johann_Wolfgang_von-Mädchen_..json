{"dta.poem.18788": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Singst du schon Suleika wieder!", "tokens": ["Singst", "du", "schon", "Sul\u00b7ei\u00b7ka", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NE", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese k\u00f6nnen wir nicht leiden,", "tokens": ["Die\u00b7se", "k\u00f6n\u00b7nen", "wir", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht um dich \u2014 um deine Lieder", "tokens": ["Nicht", "um", "dich", "um", "dei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PPER", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollen, m\u00fcssen wir sie neiden.", "tokens": ["Wol\u00b7len", ",", "m\u00fcs\u00b7sen", "wir", "sie", "nei\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn wenn sie auch garstig w\u00e4re", "tokens": ["Denn", "wenn", "sie", "auch", "gars\u00b7tig", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADJD", "VAFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Machtst du sie zum sch\u00f6nsten Wesen,", "tokens": ["Machtst", "du", "sie", "zum", "sch\u00f6ns\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so haben wir von Dschemil", "tokens": ["Und", "so", "ha\u00b7ben", "wir", "von", "Dsche\u00b7mil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Boteinah viel gelesen.", "tokens": ["Und", "Bo\u00b7tei\u00b7nah", "viel", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber eben weil wir h\u00fcbsch sind", "tokens": ["A\u00b7ber", "e\u00b7ben", "weil", "wir", "h\u00fcbsch", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6chten wir auch gern gemalt seyn,", "tokens": ["M\u00f6ch\u00b7ten", "wir", "auch", "gern", "ge\u00b7malt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, wenn du es billig machest,", "tokens": ["Und", ",", "wenn", "du", "es", "bil\u00b7lig", "ma\u00b7chest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollst du auch recht h\u00fcbsch bezahlt seyn.", "tokens": ["Sollst", "du", "auch", "recht", "h\u00fcbsch", "be\u00b7zahlt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}