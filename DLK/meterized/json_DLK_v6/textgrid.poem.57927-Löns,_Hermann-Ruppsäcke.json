{"textgrid.poem.57927": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Rupps\u00e4cke", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist denn mit der Eilenriede passiert?", "tokens": ["Was", "ist", "denn", "mit", "der", "Ei\u00b7len\u00b7rie\u00b7de", "pas\u00b7siert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sie sieht ja so aus als wie ganz ruiniert,", "tokens": ["Sie", "sieht", "ja", "so", "aus", "als", "wie", "ganz", "ru\u00b7i\u00b7niert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "KOUS", "KOKOM", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch gestern war sie ganz mit Blumen geschm\u00fcckt,", "tokens": ["Noch", "ge\u00b7stern", "war", "sie", "ganz", "mit", "Blu\u00b7men", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und heute ist alles zerruppt und zerdr\u00fcckt.", "tokens": ["Und", "heu\u00b7te", "ist", "al\u00b7les", "zer\u00b7ruppt", "und", "zer\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "War das eine Kuh oder war es ein Pferd,", "tokens": ["War", "das", "ei\u00b7ne", "Kuh", "o\u00b7der", "war", "es", "ein", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "KON", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das dort hat die lieblichen Blumen verzehrt,", "tokens": ["Das", "dort", "hat", "die", "lieb\u00b7li\u00b7chen", "Blu\u00b7men", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein Esel vielleicht oder ein wildes Schwein?", "tokens": ["Ein", "E\u00b7sel", "viel\u00b7leicht", "o\u00b7der", "ein", "wil\u00b7des", "Schwein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn so etwas \u00e4hnliches mu\u00df es wohl sein.", "tokens": ["Denn", "so", "et\u00b7was", "\u00e4hn\u00b7li\u00b7ches", "mu\u00df", "es", "wohl", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es war keine Kuh und kein Pferd war es nicht,", "tokens": ["Es", "war", "kei\u00b7ne", "Kuh", "und", "kein", "Pferd", "war", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es trug ein menschliches Angesicht,", "tokens": ["Es", "trug", "ein", "menschli\u00b7ches", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was dort hat geruppt so roh und gemein,", "tokens": ["Was", "dort", "hat", "ge\u00b7ruppt", "so", "roh", "und", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "VVPP", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein ruppiger Mensch wird's gewesen sein.", "tokens": ["Ein", "rup\u00b7pi\u00b7ger", "Mensch", "wird's", "ge\u00b7we\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}