{"dta.poem.12025": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch wunderbare Art den L\u00e4ugner zu bekehren,", "tokens": ["Welch", "wun\u00b7der\u00b7ba\u00b7re", "Art", "den", "L\u00e4ug\u00b7ner", "zu", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihn zu behandeln als unf\u00e4hig deiner Lehren!", "tokens": ["Ihn", "zu", "be\u00b7han\u00b7deln", "als", "un\u00b7f\u00e4\u00b7hig", "dei\u00b7ner", "Leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "KOKOM", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kannst du verlangen da\u00df dich fassen soll der Mann,", "tokens": ["Kannst", "du", "ver\u00b7lan\u00b7gen", "da\u00df", "dich", "fas\u00b7sen", "soll", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du behauptest da\u00df er dich nicht fassen kann?", "tokens": ["Wenn", "du", "be\u00b7haup\u00b7test", "da\u00df", "er", "dich", "nicht", "fas\u00b7sen", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Beweisest ihm zuerst, da\u00df er verstehn nicht kann;", "tokens": ["Be\u00b7wei\u00b7sest", "ihm", "zu\u00b7erst", ",", "da\u00df", "er", "ver\u00b7stehn", "nicht", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "PTKNEG", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er verstehn nicht will, verargest du ihm dann.", "tokens": ["Da\u00df", "er", "ver\u00b7stehn", "nicht", "will", ",", "ver\u00b7ar\u00b7gest", "du", "ihm", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKNEG", "VMFIN", "$,", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zuerst mach' es ihm klar, wie er dich fassen solle,", "tokens": ["Zu\u00b7erst", "mach'", "es", "ihm", "klar", ",", "wie", "er", "dich", "fas\u00b7sen", "sol\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dann \u00fcberlass es ihm, ob er dich fassen wolle.", "tokens": ["Dann", "\u00fc\u00b7ber\u00b7lass", "es", "ihm", ",", "ob", "er", "dich", "fas\u00b7sen", "wol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}