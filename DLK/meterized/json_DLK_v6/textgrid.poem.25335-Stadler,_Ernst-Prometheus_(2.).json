{"textgrid.poem.25335": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "Prometheus (2.)", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und in der Nacht, da er am Felsen hing,", "tokens": ["Und", "in", "der", "Nacht", ",", "da", "er", "am", "Fel\u00b7sen", "hing", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Unter Adlerf\u00e4ngen sein Leib sich b\u00e4umte,", "tokens": ["Un\u00b7ter", "Ad\u00b7ler\u00b7f\u00e4n\u00b7gen", "sein", "Leib", "sich", "b\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Blutigen Schaum die Flut aufleckend ihm ins Antlitz spie,", "tokens": ["Blu\u00b7ti\u00b7gen", "Schaum", "die", "Flut", "au\u00b7fle\u00b7ckend", "ihm", "ins", "Ant\u00b7litz", "spie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "VVPP", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Trat vor ihn aus den Schatten der Versucher.", "tokens": ["Trat", "vor", "ihn", "aus", "den", "Schat\u00b7ten", "der", "Ver\u00b7su\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und rauher peitschte, h\u00f6hnender das Meer", "tokens": ["Und", "rau\u00b7her", "peitschte", ",", "h\u00f6h\u00b7nen\u00b7der", "das", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Um seine Lenden. Geller fuhr", "tokens": ["Um", "sei\u00b7ne", "Len\u00b7den", ".", "Gel\u00b7ler", "fuhr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sein Br\u00fcllen in der Elemente Sturm.", "tokens": ["Sein", "Br\u00fcl\u00b7len", "in", "der", "E\u00b7le\u00b7men\u00b7te", "Sturm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wild durch sein Blut sprang des Versuchers Lied.", "tokens": ["Wild", "durch", "sein", "Blut", "sprang", "des", "Ver\u00b7su\u00b7chers", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Sirenenlocken warf in s\u00fc\u00dfem Rausch", "tokens": ["Si\u00b7re\u00b7nen\u00b7lo\u00b7cken", "warf", "in", "s\u00fc\u00b7\u00dfem", "Rausch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie Bl\u00fctennektar weich sich \u00fcber ihn.", "tokens": ["Wie", "Bl\u00fc\u00b7ten\u00b7nek\u00b7tar", "weich", "sich", "\u00fc\u00b7ber", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Dann wieder", "tokens": ["Dann", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "War's wie ein Tanz,", "tokens": ["Wa\u00b7r's", "wie", "ein", "Tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Der \u00fcber Welten raste, da aus Wolken noch", "tokens": ["Der", "\u00fc\u00b7ber", "Wel\u00b7ten", "ras\u00b7te", ",", "da", "aus", "Wol\u00b7ken", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,", "KOUS", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "G\u00f6tter sich neigten, wilde Lippen", "tokens": ["G\u00f6t\u00b7ter", "sich", "neig\u00b7ten", ",", "wil\u00b7de", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PRF", "VVFIN", "$,", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Auf weichen Wangen gl\u00fchten,", "tokens": ["Auf", "wei\u00b7chen", "Wan\u00b7gen", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und durch den Glanz der schw\u00fclen Sommernacht", "tokens": ["Und", "durch", "den", "Glanz", "der", "schw\u00fc\u00b7len", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Des Blutes ehern Lied aus schrillen Saiten scholl.", "tokens": ["Des", "Blu\u00b7tes", "e\u00b7hern", "Lied", "aus", "schril\u00b7len", "Sai\u00b7ten", "scholl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sein Atem keuchte,", "tokens": ["Sein", "A\u00b7tem", "keuch\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Seine Adern schwollen \u2013", "tokens": ["Sei\u00b7ne", "A\u00b7dern", "schwol\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "\u00bbnicht weiter, Zeus! ... Ich will \u2013 \u2013\u00ab", "tokens": ["\u00bb", "nicht", "wei\u00b7ter", ",", "Zeus", "!", "...", "Ich", "will", "\u2013", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKNEG", "PTKVZ", "$,", "NE", "$.", "$(", "PPER", "VMFIN", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Da stieg die Sonne leuchtend \u00fcbers Meer.", "tokens": ["Da", "stieg", "die", "Son\u00b7ne", "leuch\u00b7tend", "\u00fc\u00b7bers", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Ein Flimmern, Rauschen. Kreischend flieht", "tokens": ["Ein", "Flim\u00b7mern", ",", "Rau\u00b7schen", ".", "Krei\u00b7schend", "flieht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Adler. Geduckt, in wilder Gier", "tokens": ["Der", "Ad\u00b7ler", ".", "Ge\u00b7duckt", ",", "in", "wil\u00b7der", "Gier"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Lauert der Fremde", "tokens": ["Lau\u00b7ert", "der", "Frem\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "Auf jenes Wort, das Sonnen schmettern soll", "tokens": ["Auf", "je\u00b7nes", "Wort", ",", "das", "Son\u00b7nen", "schmet\u00b7tern", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Aus ihren Bahnen und die Welt in Nacht.", "tokens": ["Aus", "ih\u00b7ren", "Bah\u00b7nen", "und", "die", "Welt", "in", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In stummem Tr\u00e4umen stand Prometheus.", "tokens": ["In", "stum\u00b7mem", "Tr\u00e4u\u00b7men", "stand", "Pro\u00b7me\u00b7theus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Nur tiefer hob und senkte sich die Brust", "tokens": ["Nur", "tie\u00b7fer", "hob", "und", "senk\u00b7te", "sich", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und trank in hei\u00dfen Z\u00fcgen Morgenlicht.", "tokens": ["Und", "trank", "in", "hei\u00b7\u00dfen", "Z\u00fc\u00b7gen", "Mor\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um seine Lippen flo\u00df ein roter Quell", "tokens": ["Um", "sei\u00b7ne", "Lip\u00b7pen", "flo\u00df", "ein", "ro\u00b7ter", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von Morgensonne \u2013 Siegerseligkeit.", "tokens": ["Von", "Mor\u00b7gen\u00b7son\u00b7ne", "\u2013", "Sie\u00b7ger\u00b7se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein Wort ...", "tokens": ["Kein", "Wort", "..."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Auf roten Wogen flie\u00dft der junge Tag.", "tokens": ["Auf", "ro\u00b7ten", "Wo\u00b7gen", "flie\u00dft", "der", "jun\u00b7ge", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aus L\u00fcften bricht's wie Dank aus tausend Kehlen.", "tokens": ["Aus", "L\u00fcf\u00b7ten", "bricht's", "wie", "Dank", "aus", "tau\u00b7send", "Keh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KOKOM", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vom Meere leuchtend steigt die Sonne auf.", "tokens": ["Vom", "Mee\u00b7re", "leuch\u00b7tend", "steigt", "die", "Son\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}