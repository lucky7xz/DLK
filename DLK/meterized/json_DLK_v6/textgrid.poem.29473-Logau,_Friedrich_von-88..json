{"textgrid.poem.29473": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Venus war gef\u00e4hrlich kranck, schickte hin den kleinen Sch\u00fctzen,", "tokens": ["Ve\u00b7nus", "war", "ge\u00b7f\u00e4hr\u00b7lich", "kranck", ",", "schick\u00b7te", "hin", "den", "klei\u00b7nen", "Sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "ADJD", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df er solle Jungfern-Fleisch mit dem g\u00f6ldnen Pfeile ritzen,", "tokens": ["Da\u00df", "er", "sol\u00b7le", "Jung\u00b7fern\u00b7Fleisch", "mit", "dem", "g\u00f6ld\u00b7nen", "Pfei\u00b7le", "rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Weil sie Jungfern-Blut bedurffte; zwar der Knabe scho\u00df gewi\u00df,", "tokens": ["Weil", "sie", "Jung\u00b7fern\u00b7Blut", "be\u00b7durff\u00b7te", ";", "zwar", "der", "Kna\u00b7be", "scho\u00df", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$.", "ADV", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Gleichwol merckt er, wo er traffe, da\u00df kein Blut sich sehen lie\u00df;", "tokens": ["Gleich\u00b7wol", "merckt", "er", ",", "wo", "er", "traf\u00b7fe", ",", "da\u00df", "kein", "Blut", "sich", "se\u00b7hen", "lie\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "PRF", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Flog betr\u00fcbt zur Mutter zu, wolte dr\u00fcber sich beschweren,", "tokens": ["Flog", "be\u00b7tr\u00fcbt", "zur", "Mut\u00b7ter", "zu", ",", "wol\u00b7te", "dr\u00fc\u00b7ber", "sich", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,", "VMFIN", "PAV", "PRF", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Bi\u00df er h\u00f6rte, da\u00df durch Krieg auch die Jungfern feste weren.", "tokens": ["Bi\u00df", "er", "h\u00f6r\u00b7te", ",", "da\u00df", "durch", "Krieg", "auch", "die", "Jung\u00b7fern", "fes\u00b7te", "we\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "APPR", "NN", "ADV", "ART", "NN", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}