{"dta.poem.11404": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leander an Sylvien, als er ihr das ver-  \n sprochne h\u00fcndgen schicken  \n solte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Sylvia! st\u00fcnd es in meinen h\u00e4nden,", "tokens": ["Ge\u00b7lieb\u00b7te", "Syl\u00b7via", "!", "st\u00fcnd", "es", "in", "mei\u00b7nen", "h\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So k\u00fc\u00dfte dir itzund dein h\u00fcndgen fu\u00df und hand;", "tokens": ["So", "k\u00fc\u00df\u00b7te", "dir", "it\u00b7zund", "dein", "h\u00fcnd\u00b7gen", "fu\u00df", "und", "hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So aber kan ich nichts, als leere verse, senden,", "tokens": ["So", "a\u00b7ber", "kan", "ich", "nichts", ",", "als", "lee\u00b7re", "ver\u00b7se", ",", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PIS", "$,", "KOUS", "ADJA", "NN", "$,", "VAFIN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die dein gerechter zorn vielleicht zur gluth verbannt.", "tokens": ["Die", "dein", "ge\u00b7rech\u00b7ter", "zorn", "viel\u00b7leicht", "zur", "gluth", "ver\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin zwar ausser schuld, so fern das schn\u00f6de gl\u00fccke", "tokens": ["Ich", "bin", "zwar", "aus\u00b7ser", "schuld", ",", "so", "fern", "das", "schn\u00f6\u00b7de", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mich nicht gew\u00e4hren l\u00e4st, was ich dir zugesagt;", "tokens": ["Mich", "nicht", "ge\u00b7w\u00e4h\u00b7ren", "l\u00e4st", ",", "was", "ich", "dir", "zu\u00b7ge\u00b7sagt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "VVFIN", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Doch warum hielt ich nicht das k\u00fchne wort zur\u00fccke?", "tokens": ["Doch", "wa\u00b7rum", "hielt", "ich", "nicht", "das", "k\u00fch\u00b7ne", "wort", "zu\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wohl diesem! der ja nichts auf gut gel\u00fccke wagt.", "tokens": ["Wohl", "die\u00b7sem", "!", "der", "ja", "nichts", "auf", "gut", "ge\u00b7l\u00fc\u00b7cke", "wagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$.", "ART", "ADV", "PIS", "APPR", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du hast dann, sch\u00f6nes kind! das sch\u00f6nste recht zu z\u00fcrnen,", "tokens": ["Du", "hast", "dann", ",", "sch\u00f6\u00b7nes", "kind", "!", "das", "sch\u00f6ns\u00b7te", "recht", "zu", "z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bi\u00df ich die missethat vollkommen abgeb\u00fc\u00dft:", "tokens": ["Bi\u00df", "ich", "die", "mis\u00b7se\u00b7that", "voll\u00b7kom\u00b7men", "ab\u00b7ge\u00b7b\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wiewohl, du gleichst vielmehr den g\u00fctigen gestirnen,", "tokens": ["Wie\u00b7wohl", ",", "du", "gleichst", "viel\u00b7mehr", "den", "g\u00fc\u00b7ti\u00b7gen", "ge\u00b7stir\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PPER", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Aus denen lauter gold geneigter strahlen flie\u00dft.", "tokens": ["Aus", "de\u00b7nen", "lau\u00b7ter", "gold", "ge\u00b7neig\u00b7ter", "strah\u00b7len", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df, hoff\u2019 ich. Sylvia! la\u00df meine hoffnung gelten,", "tokens": ["Di\u00df", ",", "hoff'", "ich", ".", "Syl\u00b7via", "!", "la\u00df", "mei\u00b7ne", "hoff\u00b7nung", "gel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVFIN", "PPER", "$.", "NE", "$.", "VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Weil die vergebung dir nicht wenig ehre bringt!", "tokens": ["Weil", "die", "ver\u00b7ge\u00b7bung", "dir", "nicht", "we\u00b7nig", "eh\u00b7re", "bringt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die cantzel selber pflegt, auf zorn und grimm zu schelten:", "tokens": ["Die", "cant\u00b7zel", "sel\u00b7ber", "pflegt", ",", "auf", "zorn", "und", "grimm", "zu", "schel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "APPR", "NN", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du weist, wie Seeligmann auf lieb und sanfftmuth dringt.", "tokens": ["Du", "weist", ",", "wie", "See\u00b7lig\u00b7mann", "auf", "lieb", "und", "sanfft\u00b7muth", "dringt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man mu\u00df die predigten der priester nicht verwerffen,", "tokens": ["Man", "mu\u00df", "die", "pre\u00b7dig\u00b7ten", "der", "pries\u00b7ter", "nicht", "ver\u00b7werf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn ihre wachsamkeit bewahrt des HErren haus:", "tokens": ["Denn", "ih\u00b7re", "wach\u00b7sam\u00b7keit", "be\u00b7wahrt", "des", "Her\u00b7ren", "haus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sie wissen bald das schwerd des cherubims zu sch\u00e4rffen,", "tokens": ["Sie", "wis\u00b7sen", "bald", "das", "schwerd", "des", "che\u00b7ru\u00b7bims", "zu", "sch\u00e4rf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.20": {"text": "Und jagen uns damit von Edens garten aus.", "tokens": ["Und", "ja\u00b7gen", "uns", "da\u00b7mit", "von", "E\u00b7dens", "gar\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "APPR", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach! k\u00f6nnt\u2019 ich meine lieb itzt in ein h\u00fcndgen kehren!", "tokens": ["Ach", "!", "k\u00f6nnt'", "ich", "mei\u00b7ne", "lieb", "itzt", "in", "ein", "h\u00fcnd\u00b7gen", "keh\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PPOSAT", "ADJD", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So w\u00e4r ich meiner furcht am allerbesten los:", "tokens": ["So", "w\u00e4r", "ich", "mei\u00b7ner", "furcht", "am", "al\u00b7ler\u00b7bes\u00b7ten", "los", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich wei\u00df, du w\u00fcrdest es mit eigner hand ern\u00e4hren:", "tokens": ["Ich", "wei\u00df", ",", "du", "w\u00fcr\u00b7dest", "es", "mit", "eig\u00b7ner", "hand", "er\u00b7n\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es ruhte manche stund auf deiner sanfften schos.", "tokens": ["Es", "ruh\u00b7te", "man\u00b7che", "stund", "auf", "dei\u00b7ner", "sanff\u00b7ten", "schos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "VVFIN", "APPR", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du w\u00fcrdest ihm auch selbst das bette nicht verschliessen,", "tokens": ["Du", "w\u00fcr\u00b7dest", "ihm", "auch", "selbst", "das", "bet\u00b7te", "nicht", "ver\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "PDS", "VVFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In welches Sylvia die zarten glieder streckt:", "tokens": ["In", "wel\u00b7ches", "Syl\u00b7via", "die", "zar\u00b7ten", "glie\u00b7der", "streckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NE", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Es d\u00fcrffte dir getrost die reinen lippen k\u00fcssen,", "tokens": ["Es", "d\u00fcrff\u00b7te", "dir", "ge\u00b7trost", "die", "rei\u00b7nen", "lip\u00b7pen", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wo krafft und lieblichkeit in warmen rosen steckt.", "tokens": ["Wo", "krafft", "und", "lieb\u00b7lich\u00b7keit", "in", "war\u00b7men", "ro\u00b7sen", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Sonst kostet es viel m\u00fch, ein h\u00fcndgen abzurichten;", "tokens": ["Sonst", "kos\u00b7tet", "es", "viel", "m\u00fch", ",", "ein", "h\u00fcnd\u00b7gen", "ab\u00b7zu\u00b7rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ART", "ADJA", "VVIZU", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Hier aber h\u00e4ttest du dergleichen sorge nicht:", "tokens": ["Hier", "a\u00b7ber", "h\u00e4t\u00b7test", "du", "derg\u00b7lei\u00b7chen", "sor\u00b7ge", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIS", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es w\u00fcrde dir zu lieb auf lauter k\u00fcnste tichten!", "tokens": ["Es", "w\u00fcr\u00b7de", "dir", "zu", "lieb", "auf", "lau\u00b7ter", "k\u00fcns\u00b7te", "tich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKA", "ADJD", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dein auge w\u00e4r\u2019 sein stern, dein augen-winck sein licht.", "tokens": ["Dein", "au\u00b7ge", "w\u00e4r'", "sein", "stern", ",", "dein", "au\u00b7gen\u00b7winck", "sein", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "ADJA", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es w\u00fcrd\u2019 unn\u00f6thig seyn, ihm eine magd zu halten,", "tokens": ["Es", "w\u00fcrd'", "un\u00b7n\u00f6\u00b7thig", "seyn", ",", "ihm", "ei\u00b7ne", "magd", "zu", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VAINF", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich wei\u00df, es f\u00fchrte sich stets rein und sauber auf;", "tokens": ["Ich", "wei\u00df", ",", "es", "f\u00fchr\u00b7te", "sich", "stets", "rein", "und", "sau\u00b7ber", "auf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es w\u00fcrde lieber selbst des dieners statt verwalten:", "tokens": ["Es", "w\u00fcr\u00b7de", "lie\u00b7ber", "selbst", "des", "die\u00b7ners", "statt", "ver\u00b7wal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Dir ewig treu zu seyn, das w\u00e4r sein lebens-lauff.", "tokens": ["Dir", "e\u00b7wig", "treu", "zu", "seyn", ",", "das", "w\u00e4r", "sein", "le\u00b7bens\u00b7lauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "PTKZU", "VAINF", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.37": {"text": "Versuchte man es gleich, mit mandeln zu bestechen:", "tokens": ["Ver\u00b7such\u00b7te", "man", "es", "gleich", ",", "mit", "man\u00b7deln", "zu", "be\u00b7ste\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "$,", "APPR", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Gieng ihm die schmeicheley gleich niedlich um den mund;", "tokens": ["Gieng", "ihm", "die", "schmei\u00b7che\u00b7ley", "gleich", "nied\u00b7lich", "um", "den", "mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So macht\u2019 es, wolte man ihm hals und beine brechen,", "tokens": ["So", "macht'", "es", ",", "wol\u00b7te", "man", "ihm", "hals", "und", "bei\u00b7ne", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VMFIN", "PIS", "PPER", "ADV", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Doch nicht das bitterst