{"textgrid.poem.50287": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wahr ist's! ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahr ist's! ", "tokens": ["Wahr", "ist's", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Doch scheint's, als wenn es drum allein geschehen w\u00e4r,", "tokens": ["Doch", "scheint's", ",", "als", "wenn", "es", "drum", "al\u00b7lein", "ge\u00b7sche\u00b7hen", "w\u00e4r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "PAV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dass dein ", "tokens": ["Dass", "dein"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Denn pflegt man erst die Kunst des ", "tokens": ["Denn", "pflegt", "man", "erst", "die", "Kunst", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn er in einem ", "tokens": ["Wenn", "er", "in", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Nun aber herrscht die Ruh' auf deinem weiten Grund,", "tokens": ["Nun", "a\u00b7ber", "herrscht", "die", "Ruh'", "auf", "dei\u00b7nem", "wei\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und deine ", "tokens": ["Und", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Weil sich dein m\u00e4chtig Kriegsheer theilet,", "tokens": ["Weil", "sich", "dein", "m\u00e4ch\u00b7tig", "Kriegs\u00b7heer", "thei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und denen, ", "tokens": ["Und", "de\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PDS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Und deiner ", "tokens": ["Und", "dei\u00b7ner"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}}}}