{"textgrid.poem.25984": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Unsterblich sind nicht nur die Sorgen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Barbaratag im d\u00fcrrsten Hag,", "tokens": ["Am", "Bar\u00b7ba\u00b7ra\u00b7tag", "im", "d\u00fcrrs\u00b7ten", "Hag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Langsam der Saft wieder steigen mag,", "tokens": ["Lang\u00b7sam", "der", "Saft", "wie\u00b7der", "stei\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Gl\u00fcck schwebt schon drau\u00dfen in kahlen Zweigen,", "tokens": ["Gl\u00fcck", "schwebt", "schon", "drau\u00b7\u00dfen", "in", "kah\u00b7len", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hebt leise an Leises zu geigen:", "tokens": ["Hebt", "lei\u00b7se", "an", "Lei\u00b7ses", "zu", "gei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt tut dir noch jede Schneeflocke weh,", "tokens": ["Jetzt", "tut", "dir", "noch", "je\u00b7de", "Schnee\u00b7flo\u00b7cke", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch die Hand, die heut, ach, am Sorgengarn spinnt,", "tokens": ["Doch", "die", "Hand", ",", "die", "heut", ",", "ach", ",", "am", "Sor\u00b7gen\u00b7garn", "spinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "$,", "ITJ", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Wohl morgen schon Glocken zu l\u00e4uten beginnt,", "tokens": ["Wohl", "mor\u00b7gen", "schon", "Glo\u00b7cken", "zu", "l\u00e4u\u00b7ten", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und der Sinn wird ein hurtiges Reh.", "tokens": ["Und", "der", "Sinn", "wird", "ein", "hur\u00b7ti\u00b7ges", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Solange du lebst, auch das Leben dich mag,", "tokens": ["So\u00b7lan\u00b7ge", "du", "lebst", ",", "auch", "das", "Le\u00b7ben", "dich", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "$,", "ADV", "ART", "NN", "PPER", "VMFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Saft steigt in das D\u00fcrrholz am Barbaratag,", "tokens": ["Saft", "steigt", "in", "das", "D\u00fcrr\u00b7holz", "am", "Bar\u00b7ba\u00b7ra\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Saft steigt auch ins Gl\u00fcck wohl schon morgen,", "tokens": ["Saft", "steigt", "auch", "ins", "Gl\u00fcck", "wohl", "schon", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Unsterblich sind nicht nur die Sorgen.", "tokens": ["U\u00b7nsterb\u00b7lich", "sind", "nicht", "nur", "die", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}