{"dta.poem.10710": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Echo oder Widerschall.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Komm/ Echo/ komm/ die niemand nicht kan finden/", "tokens": ["Komm", "/", "E\u00b7cho", "/", "komm", "/", "die", "nie\u00b7mand", "nicht", "kan", "fin\u00b7den", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "$(", "VVFIN", "$(", "ART", "PIS", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd bist doch nicht/ wann man dir rufft/ dahinden/", "tokens": ["Vnd", "bist", "doch", "nicht", "/", "wann", "man", "dir", "rufft", "/", "da\u00b7hin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "$(", "PWAV", "PIS", "PPER", "VVFIN", "$(", "PAV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Antworte mir auffmeine Frage/ frage.", "tokens": ["Ant\u00b7wor\u00b7te", "mir", "auf\u00b7fmei\u00b7ne", "Fra\u00b7ge", "/", "fra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was thue ich in des Tages hitze? Sitze.", "tokens": ["Was", "thue", "ich", "in", "des", "Ta\u00b7ges", "hit\u00b7ze", "?", "Sit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich mich mit dir vnderrede? rede.", "tokens": ["Da\u00df", "ich", "mich", "mit", "dir", "vn\u00b7der\u00b7re\u00b7de", "?", "re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ists/ da\u00df mich so thut au\u00dfsaugen? Augen.", "tokens": ["Was", "ists", "/", "da\u00df", "mich", "so", "thut", "au\u00df\u00b7sau\u00b7gen", "?", "Au\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "KOUS", "PPER", "ADV", "VVFIN", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So kan mein Lieb die falsche st\u00fccke? T\u00fccke.", "tokens": ["So", "kan", "mein", "Lieb", "die", "fal\u00b7sche", "st\u00fc\u00b7cke", "?", "T\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Vnd krieg ich das vor meine trewe? Rewe.", "tokens": ["Vnd", "krieg", "ich", "das", "vor", "mei\u00b7ne", "tre\u00b7we", "?", "Re\u00b7we", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "PPER", "PDS", "APPR", "PPOSAT", "ADJA", "$.", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Thut sich doch nichts so hart erwei\u00dfen? Eisen.", "tokens": ["Thut", "sich", "doch", "nichts", "so", "hart", "er\u00b7wei\u00b7\u00dfen", "?", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PIS", "ADV", "ADJD", "VVINF", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie mach ichs dan\u0303/ da\u00df ichs erleide? leide.", "tokens": ["Wie", "mach", "ichs", "da\u00f1", "/", "da\u00df", "ichs", "er\u00b7lei\u00b7de", "?", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PAV", "$(", "KOUS", "PIS", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie thue ich/ da\u00df ich sie erbitte? bitte.", "tokens": ["Wie", "thue", "ich", "/", "da\u00df", "ich", "sie", "er\u00b7bit\u00b7te", "?", "bit\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PPER", "VVFIN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So soll ich mich jhr vndergeben? geben.", "tokens": ["So", "soll", "ich", "mich", "jhr", "vn\u00b7der\u00b7ge\u00b7ben", "?", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PPOSAT", "NN", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Was macht mich dann rechtschaffen lieben? vben.", "tokens": ["Was", "macht", "mich", "dann", "recht\u00b7schaf\u00b7fen", "lie\u00b7ben", "?", "vben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVINF", "VVINF", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Mu\u00df ich die lieb auch andren schweigen? eigen.", "tokens": ["Mu\u00df", "ich", "die", "lieb", "auch", "an\u00b7dren", "schwei\u00b7gen", "?", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJD", "ADV", "PIS", "VVINF", "$.", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "So wird sich ja da\u00df blat noch wenden? enden.", "tokens": ["So", "wird", "sich", "ja", "da\u00df", "blat", "noch", "wen\u00b7den", "?", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "KOUS", "NE", "ADV", "VVINF", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wie mach ich es/ da\u00df ichs erwarte? warte.", "tokens": ["Wie", "mach", "ich", "es", "/", "da\u00df", "ichs", "er\u00b7war\u00b7te", "?", "war\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$(", "KOUS", "PIS", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Was werd ich dann zuletzt erhalten? halten.", "tokens": ["Was", "werd", "ich", "dann", "zu\u00b7letzt", "er\u00b7hal\u00b7ten", "?", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Es ist genug/ hab ich die Gnade. Ade.", "tokens": ["Es", "ist", "ge\u00b7nug", "/", "hab", "ich", "die", "Gna\u00b7de", ".", "A\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "VAFIN", "PPER", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}