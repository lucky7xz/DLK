{"textgrid.poem.67028": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid gegr\u00fc\u00dft, ihr zufluchtsvolle Schatten,", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "zu\u00b7fluchts\u00b7vol\u00b7le", "Schat\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ihr Fluren, die ihr einsam um mich ruht;", "tokens": ["Ihr", "Flu\u00b7ren", ",", "die", "ihr", "ein\u00b7sam", "um", "mich", "ruht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du stiller Mond, du h\u00f6rst, nicht wie Verleumder lauren,", "tokens": ["Du", "stil\u00b7ler", "Mond", ",", "du", "h\u00f6rst", ",", "nicht", "wie", "Ver\u00b7leum\u00b7der", "lau\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PTKNEG", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Herz, entz\u00fcckt von deinem Perlenglanz.", "tokens": ["Mein", "Herz", ",", "ent\u00b7z\u00fcckt", "von", "dei\u00b7nem", "Per\u00b7len\u00b7glanz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus der Welt, wo tolle Toren spotten,", "tokens": ["Aus", "der", "Welt", ",", "wo", "tol\u00b7le", "To\u00b7ren", "spot\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um leere Schattenbilder sich bem\u00fchn,", "tokens": ["Um", "lee\u00b7re", "Schat\u00b7ten\u00b7bil\u00b7der", "sich", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Flieht der zu euch, der nicht das schimmernde Get\u00fcmmel", "tokens": ["Flieht", "der", "zu", "euch", ",", "der", "nicht", "das", "schim\u00b7mern\u00b7de", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "APPR", "PPER", "$,", "PRELS", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der eitlen Welt, nein! nur die Tugend liebt.", "tokens": ["Der", "eit\u00b7len", "Welt", ",", "nein", "!", "nur", "die", "Tu\u00b7gend", "liebt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKANT", "$.", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur bei dir empfindt auch hier die Seele,", "tokens": ["Nur", "bei", "dir", "emp\u00b7findt", "auch", "hier", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie g\u00f6ttlich sie dereinst wird sein,", "tokens": ["Wie", "g\u00f6tt\u00b7lich", "sie", "de\u00b7reinst", "wird", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freude, deren falschem Schein so viel Alt\u00e4re,", "tokens": ["Die", "Freu\u00b7de", ",", "de\u00b7ren", "fal\u00b7schem", "Schein", "so", "viel", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So viele Opfer hier gewidmet sind.", "tokens": ["So", "vie\u00b7le", "Op\u00b7fer", "hier", "ge\u00b7wid\u00b7met", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weit hinauf, weit \u00fcber euch, ihr Sterne,", "tokens": ["Weit", "hin\u00b7auf", ",", "weit", "\u00fc\u00b7ber", "euch", ",", "ihr", "Ster\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Geht sie entz\u00fcckt mit heilgem Seraphsflug;", "tokens": ["Geht", "sie", "ent\u00b7z\u00fcckt", "mit", "heil\u00b7gem", "Se\u00b7raphs\u00b7flug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sieht \u00fcber euch herab mit g\u00f6ttlich heilgem Blicke,", "tokens": ["Sieht", "\u00fc\u00b7ber", "euch", "her\u00b7ab", "mit", "g\u00f6tt\u00b7lich", "heil\u00b7gem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf ihre Erd, da wo sie schlummernd ruht....", "tokens": ["Auf", "ih\u00b7re", "Erd", ",", "da", "wo", "sie", "schlum\u00b7mernd", "ruht", "...."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Goldner Schlaf, nur dessen Herz zufrieden", "tokens": ["Gold\u00b7ner", "Schlaf", ",", "nur", "des\u00b7sen", "Herz", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ART", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wohlt\u00e4tger Tugend wahre Freude kennt,", "tokens": ["Wohl\u00b7t\u00e4t\u00b7ger", "Tu\u00b7gend", "wah\u00b7re", "Freu\u00b7de", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur der f\u00fchlt dich. \u2013 Hier stellst du d\u00fcrftig schwache Arme,", "tokens": ["Nur", "der", "f\u00fchlt", "dich", ".", "\u2013", "Hier", "stellst", "du", "d\u00fcrf\u00b7tig", "schwa\u00b7che", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PPER", "$.", "$(", "ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die seine H\u00fclfe suchen, vor ihn hin.", "tokens": ["Die", "sei\u00b7ne", "H\u00fcl\u00b7fe", "su\u00b7chen", ",", "vor", "ihn", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Schnell f\u00fchlt er des armen Bruders Leiden;", "tokens": ["Schnell", "f\u00fchlt", "er", "des", "ar\u00b7men", "Bru\u00b7ders", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der arme weint, er weinet auch mit ihm;", "tokens": ["Der", "ar\u00b7me", "weint", ",", "er", "wei\u00b7net", "auch", "mit", "ihm", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon Trost genug! Doch spricht er, gab Gott seine Gaben", "tokens": ["Schon", "Trost", "ge\u00b7nug", "!", "Doch", "spricht", "er", ",", "gab", "Gott", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "ADV", "$.", "KON", "VVFIN", "PPER", "$,", "VVFIN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur mir? nein, auch f\u00fcr andre lebe ich. \u2013", "tokens": ["Nur", "mir", "?", "nein", ",", "auch", "f\u00fcr", "and\u00b7re", "le\u00b7be", "ich", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "$.", "PTKANT", "$,", "ADV", "APPR", "PIS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nicht von Stolz, noch Eitelkeit getrieben,", "tokens": ["Nicht", "von", "Stolz", ",", "noch", "Ei\u00b7tel\u00b7keit", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ADV", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kleidt er den Nackten dann, und s\u00e4ttigt den,", "tokens": ["Kleidt", "er", "den", "Nack\u00b7ten", "dann", ",", "und", "s\u00e4t\u00b7tigt", "den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADV", "$,", "KON", "VVFIN", "ART", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dem blasse Hungersnot sein schwach Gerippe z\u00e4hlet;", "tokens": ["Dem", "blas\u00b7se", "Hun\u00b7gers\u00b7not", "sein", "schwach", "Ge\u00b7rip\u00b7pe", "z\u00e4h\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und himmlisch wird sein f\u00fchlend Herz entz\u00fcckt.", "tokens": ["Und", "himm\u00b7lisch", "wird", "sein", "f\u00fch\u00b7lend", "Herz", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So ruht er, allein des Lasters Sklaven", "tokens": ["So", "ruht", "er", ",", "al\u00b7lein", "des", "Las\u00b7ters", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Qu\u00e4lt des Gewissens bange Donnerstimm,", "tokens": ["Qu\u00e4lt", "des", "Ge\u00b7wis\u00b7sens", "ban\u00b7ge", "Don\u00b7ner\u00b7stimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "NE", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und Todesangst w\u00e4lzt sie auf ihren weichen Lagern,", "tokens": ["Und", "To\u00b7de\u00b7sangst", "w\u00e4lzt", "sie", "auf", "ih\u00b7ren", "wei\u00b7chen", "La\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Wollust selber sich die Rute h\u00e4lt.", "tokens": ["Wo", "Wol\u00b7lust", "sel\u00b7ber", "sich", "die", "Ru\u00b7te", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}