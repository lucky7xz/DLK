{"textgrid.poem.56082": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Mohammeds Berufung", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da aber als in sein Versteck der Hohe,", "tokens": ["Da", "a\u00b7ber", "als", "in", "sein", "Ver\u00b7steck", "der", "Ho\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sofort Erkennbare: der Engel, trat,", "tokens": ["so\u00b7fort", "Er\u00b7kenn\u00b7ba\u00b7re", ":", "der", "En\u00b7gel", ",", "trat", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$.", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "aufrecht, der lautere und lichterlohe:", "tokens": ["auf\u00b7recht", ",", "der", "lau\u00b7te\u00b7re", "und", "lich\u00b7ter\u00b7lo\u00b7he", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da tat er allen Anspruch ab und bat", "tokens": ["da", "tat", "er", "al\u00b7len", "An\u00b7spruch", "ab", "und", "bat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "bleiben zu d\u00fcrfen der von seinen Reisen", "tokens": ["blei\u00b7ben", "zu", "d\u00fcr\u00b7fen", "der", "von", "sei\u00b7nen", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVINF", "PTKZU", "VMINF", "ART", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "innen verwirrte Kaufmann, der er war;", "tokens": ["in\u00b7nen", "ver\u00b7wirr\u00b7te", "Kauf\u00b7mann", ",", "der", "er", "war", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "er hatte nie gelesen \u2013 und nun gar", "tokens": ["er", "hat\u00b7te", "nie", "ge\u00b7le\u00b7sen", "\u2013", "und", "nun", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$(", "KON", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ein ", "tokens": ["ein"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Der Engel aber, herrisch, wies und wies", "tokens": ["Der", "En\u00b7gel", "a\u00b7ber", ",", "her\u00b7risch", ",", "wies", "und", "wies"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ihm, was geschrieben stand auf seinem Blatte,", "tokens": ["ihm", ",", "was", "ge\u00b7schrie\u00b7ben", "stand", "auf", "sei\u00b7nem", "Blat\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und gab nicht nach und wollte wieder: ", "tokens": ["und", "gab", "nicht", "nach", "und", "woll\u00b7te", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "KON", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da las er: so, da\u00df sich der Engel bog.", "tokens": ["Da", "las", "er", ":", "so", ",", "da\u00df", "sich", "der", "En\u00b7gel", "bog", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADV", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und war schon einer, der gelesen ", "tokens": ["Und", "war", "schon", "ei\u00b7ner", ",", "der", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "PIS", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und konnte und gehorchte und vollzog.", "tokens": ["und", "konn\u00b7te", "und", "ge\u00b7horch\u00b7te", "und", "voll\u00b7zog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}