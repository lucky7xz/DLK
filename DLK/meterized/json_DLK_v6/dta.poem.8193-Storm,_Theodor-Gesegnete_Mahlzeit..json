{"dta.poem.8193": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gesegnete Mahlzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Warm und behaglich rollt ihr Blut,", "tokens": ["Warm", "und", "be\u00b7hag\u00b7lich", "rollt", "ihr", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Menschenliebe ist ihr Herz,", "tokens": ["Voll", "Men\u00b7schen\u00b7lie\u00b7be", "ist", "ihr", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sind der ganzen Welt so gut!", "tokens": ["Sie", "sind", "der", "gan\u00b7zen", "Welt", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sch\u00fctteln z\u00e4rtlich sich die Hand,", "tokens": ["Sie", "sch\u00fct\u00b7teln", "z\u00e4rt\u00b7lich", "sich", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umwandelnd den geleerten Tisch,", "tokens": ["Um\u00b7wan\u00b7delnd", "den", "ge\u00b7leer\u00b7ten", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00fcnschen, da\u00df gesegnet sei", "tokens": ["Und", "w\u00fcn\u00b7schen", ",", "da\u00df", "ge\u00b7seg\u00b7net", "sei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wein, der Braten und der Fisch.", "tokens": ["Der", "Wein", ",", "der", "Bra\u00b7ten", "und", "der", "Fisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Geistlichkeit, die Weltlichkeit,", "tokens": ["Die", "Geist\u00b7lich\u00b7keit", ",", "die", "Welt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie so ganz verstehen sich!", "tokens": ["Wie", "sie", "so", "ganz", "ver\u00b7ste\u00b7hen", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich glaube, Gott verzeihe mir,", "tokens": ["Ich", "glau\u00b7be", ",", "Gott", "ver\u00b7zei\u00b7he", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lieben sich herzinniglich.", "tokens": ["Sie", "lie\u00b7ben", "sich", "her\u00b7zin\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}