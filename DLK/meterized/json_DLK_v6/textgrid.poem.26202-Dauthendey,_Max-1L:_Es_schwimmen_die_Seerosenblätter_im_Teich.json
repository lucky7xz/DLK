{"textgrid.poem.26202": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es schwimmen die Seerosenbl\u00e4tter im Teich", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schwimmen die Seerosenbl\u00e4tter im Teich", "tokens": ["Es", "schwim\u00b7men", "die", "See\u00b7ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "im", "Teich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie kleine Inseln und wie flache K\u00e4hne.", "tokens": ["Wie", "klei\u00b7ne", "In\u00b7seln", "und", "wie", "fla\u00b7che", "K\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es heben sich Geisterrosen aus dem Wasserreich", "tokens": ["Es", "he\u00b7ben", "sich", "Geis\u00b7ter\u00b7ro\u00b7sen", "aus", "dem", "Was\u00b7ser\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u00dcber den Wasserrahmen ger\u00e4uschlos und bleich,", "tokens": ["\u00dc\u00b7ber", "den", "Was\u00b7ser\u00b7rah\u00b7men", "ge\u00b7r\u00e4uschlos", "und", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und um ihre Bilder gleiten die Schw\u00e4ne.", "tokens": ["Und", "um", "ih\u00b7re", "Bil\u00b7der", "glei\u00b7ten", "die", "Schw\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Als riefe einer sie schlafwandelnd stumm herauf,", "tokens": ["Als", "rie\u00b7fe", "ei\u00b7ner", "sie", "schlaf\u00b7wan\u00b7delnd", "stumm", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "PPER", "ADJD", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als \u00f6ffnet sich der Sehnsucht selbst die Wassertiefe,", "tokens": ["Als", "\u00f6ff\u00b7net", "sich", "der", "Sehn\u00b7sucht", "selbst", "die", "Was\u00b7ser\u00b7tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PRF", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Biegen sich \u00fcber die gro\u00dfen Bl\u00e4tter, die regungslosen,", "tokens": ["Bie\u00b7gen", "sich", "\u00fc\u00b7ber", "die", "gro\u00b7\u00dfen", "Bl\u00e4t\u00b7ter", ",", "die", "re\u00b7gungs\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Weitaufgeschlagen in Tagen und N\u00e4chten, die R\u00e4tselrosen.", "tokens": ["Weit\u00b7auf\u00b7ge\u00b7schla\u00b7gen", "in", "Ta\u00b7gen", "und", "N\u00e4ch\u00b7ten", ",", "die", "R\u00e4t\u00b7sel\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}