{"dta.poem.4405": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wasser-Linsen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann die Welt so sch\u00f6n, so pr\u00e4chtig,", "tokens": ["Wann", "die", "Welt", "so", "sch\u00f6n", ",", "so", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch durch Schatten lustig ist;", "tokens": ["Auch", "durch", "Schat\u00b7ten", "lus\u00b7tig", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeigt sie, da\u00df Du GOtt allm\u00e4chtig", "tokens": ["Zeigt", "sie", ",", "da\u00df", "Du", "Gott", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und allgegenw\u00e4rtig bist.", "tokens": ["Und", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "bist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wann ich G\u00e4rten, Wiesen, W\u00e4lder,", "tokens": ["Wann", "ich", "G\u00e4r\u00b7ten", ",", "Wie\u00b7sen", ",", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wasser, H\u00fcgel, Berg und Felder,", "tokens": ["Was\u00b7ser", ",", "H\u00fc\u00b7gel", ",", "Berg", "und", "Fel\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "B\u00fcsche, Bluhmen, Gras und Klee", "tokens": ["B\u00fc\u00b7sche", ",", "Bluh\u00b7men", ",", "Gras", "und", "Klee"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit vergn\u00fcg\u2019ten Augen seh;", "tokens": ["Mit", "ver\u00b7gn\u00fcg'\u00b7ten", "Au\u00b7gen", "seh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Deucht mich, kann ich in der H\u00f6h,", "tokens": ["Deucht", "mich", ",", "kann", "ich", "in", "der", "H\u00f6h", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "So wie in den tiefsten Gr\u00fcnden,", "tokens": ["So", "wie", "in", "den", "tiefs\u00b7ten", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Ein unendlich Wesen finden.", "tokens": ["Ein", "un\u00b7end\u00b7lich", "We\u00b7sen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}