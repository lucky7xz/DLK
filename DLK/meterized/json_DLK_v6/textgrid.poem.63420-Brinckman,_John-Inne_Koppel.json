{"textgrid.poem.63420": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Inne Koppel", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gistern \u2013 hente teihn so, Nahwer,", "tokens": ["Gis\u00b7tern", "\u2013", "hen\u00b7te", "teihn", "so", ",", "Nah\u00b7wer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "FM", "FM", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bi Klock elbn so r\u00fcm, v\u00f6r Disch \u2013", "tokens": ["bi", "Klock", "elbn", "so", "r\u00fcm", ",", "v\u00f6r", "Disch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "wull ick ran eens an min'n Hawer", "tokens": ["wull", "ick", "ran", "e\u00b7ens", "an", "min'n", "Ha\u00b7wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "un dat Mankkurn bi de Wisch.", "tokens": ["un", "dat", "Mank\u00b7kurn", "bi", "de", "Wisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "K\u00fcmmt min Jung dor sacht ansl\u00e4ken,", "tokens": ["K\u00fcmmt", "min", "Jung", "dor", "sacht", "ans\u00b7l\u00e4\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "un he w\u00fcrr mi nich gewohr", "tokens": ["un", "he", "w\u00fcrr", "mi", "nich", "ge\u00b7wohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "PTKNEG", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ick hadd achter'n Bom mi st\u00e4ken \u2013", "tokens": ["ick", "hadd", "ach\u00b7ter'n", "Bom", "mi", "st\u00e4\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ih, denk ick, wat deit he dor?", "tokens": ["ih", ",", "denk", "ick", ",", "wat", "deit", "he", "dor", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "PPER", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sacht slek he sick an dat Fahlen", "tokens": ["Sacht", "slek", "he", "sick", "an", "dat", "Fah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "FM", "FM", "FM", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "in de Koppel, an dat ble\u00dft,", "tokens": ["in", "de", "Kop\u00b7pel", ",", "an", "dat", "ble\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "un \u2013 mi schall de Schinner halen! \u2013", "tokens": ["un", "\u2013", "mi", "schall", "de", "Schin\u00b7ner", "ha\u00b7len", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "$(", "FM", "FM", "FM", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "een-twee-dree \u2013 is rup he west.", "tokens": ["e\u00b7en\u00b7\u00b7twee\u00b7dree", "\u2013", "is", "rup", "he", "west", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "FM", "FM", "FM", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Na, de Vo\u00df \u2013 tweej\u00e4hrig w\u00fcrr he", "tokens": ["Na", ",", "de", "Vo\u00df", "\u2013", "twee\u00b7j\u00e4h\u00b7rig", "w\u00fcrr", "he"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "NE", "$(", "ADJD", "VAFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "un is'n heel kaptales Pird \u2013", "tokens": ["un", "is'n", "heel", "kap\u00b7ta\u00b7les", "Pird", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "irst dor st\u00fcnn bomstill un sch\u00fcdd he", "tokens": ["irst", "dor", "st\u00fcnn", "bom\u00b7still", "un", "sch\u00fcdd", "he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sick, as hadd he sick verfihrt.", "tokens": ["sick", ",", "as", "hadd", "he", "sick", "ver\u00b7fihrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "FM", "FM", "FM", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Na, nahst steidelt \u0153wer Enn sick", "tokens": ["Na", ",", "nahst", "stei\u00b7delt", "\u0153wer", "Enn", "sick"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "boltengrad he, sl\u00f6g f\u00f6r dull,", "tokens": ["bol\u00b7ten\u00b7grad", "he", ",", "sl\u00f6g", "f\u00f6r", "dull", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "man de Jung h\u00f6ll mit de H\u00e4nn' sick", "tokens": ["man", "de", "Jung", "h\u00f6ll", "mit", "de", "H\u00e4nn'", "sick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NE", "NE", "NE", "APPR", "NE", "XY", "XY"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "as 'ne Klatt an Hamelwull.", "tokens": ["as", "'ne", "Klatt", "an", "Ha\u00b7mel\u00b7wull", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nahsten sn\u00f6w de Vo\u00df un buckt he,", "tokens": ["Nahs\u00b7ten", "sn\u00f6w", "de", "Vo\u00df", "un", "buckt", "he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "as hadd he v\u00f6rhen man spa\u00dft,", "tokens": ["as", "hadd", "he", "v\u00f6r\u00b7hen", "man", "spa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "PIS", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "man de Jung, nich ruckt un muckt he,", "tokens": ["man", "de", "Jung", ",", "nich", "ruckt", "un", "muckt", "he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NE", "NE", "$,", "PTKNEG", "VVFIN", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "as sonn Tw\u00e4l so h\u00f6ll he fast.", "tokens": ["as", "sonn", "Tw\u00e4l", "so", "h\u00f6ll", "he", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Na, toletz dunn r\u00f6nnt dat Fahlen", "tokens": ["Na", ",", "to\u00b7letz", "dunn", "r\u00f6nnt", "dat", "Fah\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "ADJA", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "as sonn Vullblot in de Bahn,", "tokens": ["as", "sonn", "Vull\u00b7blot", "in", "de", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "APPR", "NE", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "rein as hadd een gleundig Kahlen", "tokens": ["rein", "as", "hadd", "e\u00b7en", "gleun\u00b7dig", "Kah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "FM", "FM", "FM", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "achter \u00fcnner'n Swanz em dan.", "tokens": ["ach\u00b7ter", "\u00fcn\u00b7ner'n", "Swanz", "em", "dan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "R\u0153wer g\u00fcng dat \u0153wer'n Graben,", "tokens": ["R\u0153wer", "g\u00fcng", "dat", "\u0153wer'n", "Gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "\u0153wer't Rick un d\u00f6rche B\u00e4k \u2013", "tokens": ["\u0153wer't", "Rick", "un", "d\u00f6r\u00b7che", "B\u00e4k", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "FM", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "un de Jung, een m\u00f6t dat laben,", "tokens": ["un", "de", "Jung", ",", "e\u00b7en", "m\u00f6t", "dat", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "NN", "$,", "FM.nl", "FM.nl", "FM.nl", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "de set fast grad as sonn T\u00e4k.", "tokens": ["de", "set", "fast", "grad", "as", "sonn", "T\u00e4k", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Oewerst grad in't dullste R\u00f6nnen", "tokens": ["O\u00b7e\u00b7werst", "grad", "in't", "dulls\u00b7te", "R\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "baff! dor st\u00fcnn min Vo\u00df, un baff!", "tokens": ["baff", "!", "dor", "st\u00fcnn", "min", "Vo\u00df", ",", "un", "baff", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "FM", "FM", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "los let dunn de Jung sin H\u00e4nn'n", "tokens": ["los", "let", "dunn", "de", "Jung", "sin", "H\u00e4nn'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "un koppheister sch\u00f6t he raf.", "tokens": ["un", "kop\u00b7pheis\u00b7ter", "sch\u00f6t", "he", "raf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ick dunn ran: \u00bbHest ok wat braken \u2013", "tokens": ["Ick", "dunn", "ran", ":", "\u00bb", "Hest", "ok", "wat", "bra\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "FM", "FM", "FM", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "felst di ok toschannen wat?\u00ab", "tokens": ["felst", "di", "ok", "to\u00b7schan\u00b7nen", "wat", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbnee\u00ab, lacht he dunn, \u00bbPogg un Snaken!", "tokens": ["\u00bb", "nee", "\u00ab", ",", "lacht", "he", "dunn", ",", "\u00bb", "Pogg", "un", "Sna\u00b7ken", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "$,", "VVFIN", "NE", "NE", "$,", "$(", "FM", "FM", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vader, nee, wu sch\u00f6n g\u00fcng dat!\u00ab", "tokens": ["Va\u00b7der", ",", "nee", ",", "wu", "sch\u00f6n", "g\u00fcng", "dat", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PWAV", "ADJD", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}