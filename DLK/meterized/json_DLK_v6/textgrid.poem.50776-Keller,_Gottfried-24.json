{"textgrid.poem.50776": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "24", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahret wohl, ihr sch\u00f6nen Gr\u00e4ber,", "tokens": ["Fah\u00b7ret", "wohl", ",", "ihr", "sch\u00f6\u00b7nen", "Gr\u00e4\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klirre zu, du morsches Gitter!", "tokens": ["Klir\u00b7re", "zu", ",", "du", "mor\u00b7sches", "Git\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lachend kehr ich euch den R\u00fccken,", "tokens": ["La\u00b7chend", "kehr", "ich", "euch", "den", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lilienstolz und Rosenflitter!", "tokens": ["Li\u00b7li\u00b7en\u00b7stolz", "und", "Ro\u00b7sen\u00b7flit\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Abgetan ist nun die Liebe \u2013", "tokens": ["Ab\u00b7ge\u00b7tan", "ist", "nun", "die", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hei! wie bin ich nun so munter!", "tokens": ["Hei", "!", "wie", "bin", "ich", "nun", "so", "mun\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in dem befreiten Herzen", "tokens": ["Und", "in", "dem", "be\u00b7frei\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Geht es lustig drauf und drunter!", "tokens": ["Geht", "es", "lus\u00b7tig", "drauf", "und", "drun\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "PAV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gegen Morgen, gegen Morgen", "tokens": ["Ge\u00b7gen", "Mor\u00b7gen", ",", "ge\u00b7gen", "Mor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau ich trotzig in die Sonne;", "tokens": ["Schau", "ich", "trot\u00b7zig", "in", "die", "Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie scheint sie so wild und feurig,", "tokens": ["Wie", "scheint", "sie", "so", "wild", "und", "feu\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "L\u00e4chelnd in Gewitterwonne!", "tokens": ["L\u00e4\u00b7chelnd", "in", "Ge\u00b7wit\u00b7ter\u00b7won\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich gewappnet um die Heldin", "tokens": ["Sich", "ge\u00b7wapp\u00b7net", "um", "die", "Hel\u00b7din"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fchne Wetterwolken scharen,", "tokens": ["K\u00fch\u00b7ne", "Wet\u00b7ter\u00b7wol\u00b7ken", "scha\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie auf stolzem Ozeane", "tokens": ["Wie", "auf", "stol\u00b7zem", "O\u00b7zea\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Drohende Armaden fahren.", "tokens": ["Dro\u00b7hen\u00b7de", "Ar\u00b7ma\u00b7den", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Vor mir liegt das reiche Leben,", "tokens": ["Vor", "mir", "liegt", "das", "rei\u00b7che", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4gt die Zeit die hohen Wogen,", "tokens": ["Schl\u00e4gt", "die", "Zeit", "die", "ho\u00b7hen", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kreist die Welt mit ihren Sternen:", "tokens": ["Kreist", "die", "Welt", "mit", "ih\u00b7ren", "Ster\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00f6hlich bin ich ausgezogen,", "tokens": ["Fr\u00f6h\u00b7lich", "bin", "ich", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Biete Stirn und Herz den St\u00fcrmen,", "tokens": ["Bie\u00b7te", "Stirn", "und", "Herz", "den", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lasse meine Wimpel wehen,", "tokens": ["Las\u00b7se", "mei\u00b7ne", "Wim\u00b7pel", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und beim wilden Kreuzen denk ich", "tokens": ["Und", "beim", "wil\u00b7den", "Kreu\u00b7zen", "denk", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kaum noch an ein Wiedersehen!", "tokens": ["Kaum", "noch", "an", "ein", "Wie\u00b7der\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}