{"textgrid.poem.36242": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als die Sonne nun versunken]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die Sonne nun versunken,", "tokens": ["Als", "die", "Son\u00b7ne", "nun", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchet noch der Abend rot.", "tokens": ["Bl\u00fc\u00b7het", "noch", "der", "A\u00b7bend", "rot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lange schienen weit die Flammen,", "tokens": ["Lan\u00b7ge", "schie\u00b7nen", "weit", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen\u00fcber stand der Mond;", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "stand", "der", "Mond", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie zwei Welten gegen\u00fcber,", "tokens": ["Wie", "zwei", "Wel\u00b7ten", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese bleich und jene rot,", "tokens": ["Die\u00b7se", "bleich", "und", "je\u00b7ne", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJD", "KON", "PDS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mitten inne kleine Sterne,", "tokens": ["Mit\u00b7ten", "in\u00b7ne", "klei\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An des Himmels G\u00fcrtel hoch;", "tokens": ["An", "des", "Him\u00b7mels", "G\u00fcr\u00b7tel", "hoch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Unten dann die gro\u00dfe Erde,", "tokens": ["Un\u00b7ten", "dann", "die", "gro\u00b7\u00dfe", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo im tiefen Dunkel schon", "tokens": ["Wo", "im", "tie\u00b7fen", "Dun\u00b7kel", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Blumen duften, B\u00e4ume rauschen", "tokens": ["Blu\u00b7men", "duf\u00b7ten", ",", "B\u00e4u\u00b7me", "rau\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bei der Nachtigallen Ton.", "tokens": ["Bei", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Bla\u00df wird jede sch\u00f6ne Glut", "tokens": ["Bla\u00df", "wird", "je\u00b7de", "sch\u00f6\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und die Freude sinkt vom Thron;", "tokens": ["Und", "die", "Freu\u00b7de", "sinkt", "vom", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Fern ist ganz des Tages Mutter,", "tokens": ["Fern", "ist", "ganz", "des", "Ta\u00b7ges", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Lichter scheint der bleiche Sohn.", "tokens": ["Lich\u00b7ter", "scheint", "der", "blei\u00b7che", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "An dem Schimmer freut der Mensch sich", "tokens": ["An", "dem", "Schim\u00b7mer", "freut", "der", "Mensch", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Und ist auch im Dunkel froh.", "tokens": ["Und", "ist", "auch", "im", "Dun\u00b7kel", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}