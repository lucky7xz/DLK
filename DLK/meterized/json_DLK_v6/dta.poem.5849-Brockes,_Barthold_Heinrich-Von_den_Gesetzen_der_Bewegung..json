{"dta.poem.5849": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seit dem die Welt hervor gebracht,", "tokens": ["Seit", "dem", "die", "Welt", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleibt alles, wie es war, die Sonne, Tag und Nacht,", "tokens": ["Bleibt", "al\u00b7les", ",", "wie", "es", "war", ",", "die", "Son\u00b7ne", ",", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWAV", "PPER", "VAFIN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lufft, Himmel, Erd und Fluht; nichts h\u00e4lt die Ordnung auf:", "tokens": ["Lufft", ",", "Him\u00b7mel", ",", "Erd", "und", "Fluht", ";", "nichts", "h\u00e4lt", "die", "Ord\u00b7nung", "auf", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$.", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alles h\u00e4lt denselben Lauff.", "tokens": ["Und", "al\u00b7les", "h\u00e4lt", "den\u00b7sel\u00b7ben", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}