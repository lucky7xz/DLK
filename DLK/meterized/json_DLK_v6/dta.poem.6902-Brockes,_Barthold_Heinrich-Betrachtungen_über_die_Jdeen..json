{"dta.poem.6902": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie f\u00fcgt sich von sich selbst zu dem vollkommnen Geist,", "tokens": ["Sie", "f\u00fcgt", "sich", "von", "sich", "selbst", "zu", "dem", "voll\u00b7komm\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der alles, was bekannt und unbekannt,", "tokens": ["Der", "al\u00b7les", ",", "was", "be\u00b7kannt", "und", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erschaffen und hervorgebracht.", "tokens": ["Er\u00b7schaf\u00b7fen", "und", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo trifft man V\u00f6lcker an, die einer GOTTHEJT Macht,", "tokens": ["Wo", "trifft", "man", "V\u00f6l\u00b7cker", "an", ",", "die", "ei\u00b7ner", "GoTTHEJT", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "NN", "PTKVZ", "$,", "PRELS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ob sie des Jrrthums Dunst und Nebel gleich beth\u00f6ren,", "tokens": ["Ob", "sie", "des", "Jrr\u00b7thums", "Dunst", "und", "Ne\u00b7bel", "gleich", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht f\u00fchlen und nicht ehren?", "tokens": ["Nicht", "f\u00fch\u00b7len", "und", "nicht", "eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In ihrem Gottesdienst, er mag so n\u00e4rrisch seyn,", "tokens": ["In", "ih\u00b7rem", "Got\u00b7tes\u00b7dienst", ",", "er", "mag", "so", "n\u00e4r\u00b7risch", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So voll von wunderlich-und l\u00e4cherlichen Dingen;", "tokens": ["So", "voll", "von", "wun\u00b7der\u00b7lich\u00b7\u00b7und", "l\u00e4\u00b7cher\u00b7li\u00b7chen", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So schlagen sie dennoch den Weg zur Gottheit ein,", "tokens": ["So", "schla\u00b7gen", "sie", "den\u00b7noch", "den", "Weg", "zur", "Got\u00b7theit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und suchen Ehr und Furcht zum Opffer Jhr zu bringen.", "tokens": ["Und", "su\u00b7chen", "Ehr", "und", "Furcht", "zum", "Opf\u00b7fer", "Ihr", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "APPRART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein schwaches Schimmer-Licht in dieser duncklen Nacht,", "tokens": ["Ein", "schwa\u00b7ches", "Schim\u00b7mer\u00b7Licht", "in", "die\u00b7ser", "dunck\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat tausend G\u00f6tter gar aus einem GOTT gemacht.", "tokens": ["Hat", "tau\u00b7send", "G\u00f6t\u00b7ter", "gar", "aus", "ei\u00b7nem", "GoTT", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Indem sie Seine Gr\u00f6ss\u2019 in unterschiednen Bildern,", "tokens": ["In\u00b7dem", "sie", "Sei\u00b7ne", "Gr\u00f6ss'", "in", "un\u00b7ter\u00b7schied\u00b7nen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In ihrer Phantasey sich schildern;", "tokens": ["In", "ih\u00b7rer", "Phan\u00b7ta\u00b7sey", "sich", "schil\u00b7dern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Erdichten sie, ans jeder Eigenschafft,", "tokens": ["Er\u00b7dich\u00b7ten", "sie", ",", "ans", "je\u00b7der", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sich einen eignen Gott von einer eignen Krafft.", "tokens": ["Sich", "ei\u00b7nen", "eig\u00b7nen", "Gott", "von", "ei\u00b7ner", "eig\u00b7nen", "Krafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}