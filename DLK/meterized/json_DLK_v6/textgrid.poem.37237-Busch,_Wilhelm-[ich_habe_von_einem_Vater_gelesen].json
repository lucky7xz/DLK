{"textgrid.poem.37237": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich habe von einem Vater gelesen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe von einem Vater gelesen:", "tokens": ["Ich", "ha\u00b7be", "von", "ei\u00b7nem", "Va\u00b7ter", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Tochter ist beim Theater gewesen.", "tokens": ["Die", "Toch\u00b7ter", "ist", "beim", "The\u00b7a\u00b7ter", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VAPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Schurke hat ihm das M\u00e4dchen verdorben,", "tokens": ["Ein", "Schur\u00b7ke", "hat", "ihm", "das", "M\u00e4d\u00b7chen", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So da\u00df es im Wochenbette gestorben.", "tokens": ["So", "da\u00df", "es", "im", "Wo\u00b7chen\u00b7bet\u00b7te", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das nahm der Vater sich tief zu Gem\u00fcte.", "tokens": ["Das", "nahm", "der", "Va\u00b7ter", "sich", "tief", "zu", "Ge\u00b7m\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und als er den Schurken zu fassen kriegte,", "tokens": ["Und", "als", "er", "den", "Schur\u00b7ken", "zu", "fas\u00b7sen", "krieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verzieh er ihm nobel die ganze Geschichte.", "tokens": ["Ver\u00b7zieh", "er", "ihm", "no\u00b7bel", "die", "gan\u00b7ze", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich weine ob solcher G\u00fcte.", "tokens": ["Ich", "wei\u00b7ne", "ob", "sol\u00b7cher", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}