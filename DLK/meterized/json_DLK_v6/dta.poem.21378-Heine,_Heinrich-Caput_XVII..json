{"dta.poem.21378": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stell\u2019 wieder her das Halsgericht,", "tokens": ["Stell'", "wie\u00b7der", "her", "das", "Hals\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das peinliche Carls des f\u00fcnften,", "tokens": ["Das", "pein\u00b7li\u00b7che", "Carls", "des", "f\u00fcnf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und theile wieder ein das Volk", "tokens": ["Und", "thei\u00b7le", "wie\u00b7der", "ein", "das", "Volk"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach St\u00e4nden, Gilden und Z\u00fcnften.", "tokens": ["Nach", "St\u00e4n\u00b7den", ",", "Gil\u00b7den", "und", "Z\u00fcnf\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das alte heilige r\u00f6mische Reich,", "tokens": ["Das", "al\u00b7te", "hei\u00b7li\u00b7ge", "r\u00f6\u00b7mi\u00b7sche", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Stell\u2019s wieder her, das ganze,", "tokens": ["Stell's", "wie\u00b7der", "her", ",", "das", "gan\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gieb uns den modrigsten Plunder zur\u00fcck", "tokens": ["Gieb", "uns", "den", "mod\u00b7rigs\u00b7ten", "Plun\u00b7der", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit allem Firlifanze.", "tokens": ["Mit", "al\u00b7lem", "Fir\u00b7li\u00b7fan\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Mittelalter, immerhin,", "tokens": ["Das", "Mit\u00b7tel\u00b7al\u00b7ter", ",", "im\u00b7mer\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wahre, wie es gewesen,", "tokens": ["Das", "wah\u00b7re", ",", "wie", "es", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "PPER", "VAPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich will es ertragen \u2013 erl\u00f6se uns nur", "tokens": ["Ich", "will", "es", "er\u00b7tra\u00b7gen", "\u2013", "er\u00b7l\u00f6\u00b7se", "uns", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$(", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Von jenem Zwitterwesen,", "tokens": ["Von", "je\u00b7nem", "Zwit\u00b7ter\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von jenem Kamaschenritterthum,", "tokens": ["Von", "je\u00b7nem", "Ka\u00b7ma\u00b7schen\u00b7rit\u00b7ter\u00b7thum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das ekelhaft ein Gemisch ist", "tokens": ["Das", "e\u00b7kel\u00b7haft", "ein", "Ge\u00b7misch", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von gothischem Wahn und modernem Lug,", "tokens": ["Von", "go\u00b7thi\u00b7schem", "Wahn", "und", "mo\u00b7der\u00b7nem", "Lug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Das weder Fleisch noch Fisch ist.", "tokens": ["Das", "we\u00b7der", "Fleisch", "noch", "Fisch", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}