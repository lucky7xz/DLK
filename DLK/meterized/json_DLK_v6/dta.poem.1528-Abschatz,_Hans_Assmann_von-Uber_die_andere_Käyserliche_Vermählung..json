{"dta.poem.1528": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die andere K\u00e4yserliche Verm\u00e4hlung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wann GL\u00fcCK und LEUEN-MUTT in Liebe sich ver-", "tokens": ["Wann", "Gl\u00fcCK", "und", "LeU\u00b7EN\u00b7MUTT", "in", "Lie\u00b7be", "sich", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NE", "APPR", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "m\u00e4hlen/", "tokens": ["m\u00e4h\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "So kan es nicht an Lust/ an Sieg und Helden fehlen.", "tokens": ["So", "kan", "es", "nicht", "an", "Lust", "/", "an", "Sieg", "und", "Hel\u00b7den", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "$(", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gl\u00fcckseligs Oesterreich/ du hast mit deinem LEUEN", "tokens": ["Gl\u00fcck\u00b7se\u00b7ligs", "O\u00b7es\u00b7ter\u00b7reich", "/", "du", "hast", "mit", "dei\u00b7nem", "LeU\u00b7EN"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$(", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Dich \u00fcber Sanfftmutt/ Gl\u00fcck und Hoffnung zu erfreuen!", "tokens": ["Dich", "\u00fc\u00b7ber", "Sanfft\u00b7mutt", "/", "Gl\u00fcck", "und", "Hoff\u00b7nung", "zu", "er\u00b7freu\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "$(", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}