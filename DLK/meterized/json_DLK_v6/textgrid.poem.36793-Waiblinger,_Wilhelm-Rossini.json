{"textgrid.poem.36793": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Rossini", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrlich, es ist zuweilen, als h\u00e4tte der Sch\u00f6pfer vor Unmuth", "tokens": ["Wahr\u00b7lich", ",", "es", "ist", "zu\u00b7wei\u00b7len", ",", "als", "h\u00e4t\u00b7te", "der", "Sch\u00f6p\u00b7fer", "vor", "Un\u00b7muth"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "ADV", "$,", "KOKOM", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "+--+-+--+--+-+++", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Ueber das Menschengeschlecht und seine Frechheit gesagt:", "tokens": ["Ue\u00b7ber", "das", "Men\u00b7schen\u00b7ge\u00b7schlecht", "und", "sei\u00b7ne", "Frech\u00b7heit", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nun denn, weil so vern\u00fcnftig ihr sein wollt, geb' ich dem Hohlkopf", "tokens": ["Nun", "denn", ",", "weil", "so", "ver\u00b7n\u00fcnf\u00b7tig", "ihr", "sein", "wollt", ",", "geb'", "ich", "dem", "Hohl\u00b7kopf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "ADJD", "PPER", "VAINF", "VMFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-++", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Gro\u00dfes Talent, la\u00df uns sehen, was er f\u00fcr Dinge draus macht.", "tokens": ["Gro\u00b7\u00dfes", "Ta\u00b7lent", ",", "la\u00df", "uns", "se\u00b7hen", ",", "was", "er", "f\u00f