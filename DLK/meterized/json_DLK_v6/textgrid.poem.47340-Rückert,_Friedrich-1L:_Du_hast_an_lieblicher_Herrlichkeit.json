{"textgrid.poem.47340": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast an lieblicher Herrlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast an lieblicher Herrlichkeit", "tokens": ["Du", "hast", "an", "lieb\u00b7li\u00b7cher", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erstiegen die h\u00f6chste Stufe;", "tokens": ["Er\u00b7stie\u00b7gen", "die", "h\u00f6chs\u00b7te", "Stu\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die ganze Seele sei dir geweiht", "tokens": ["Die", "gan\u00b7ze", "See\u00b7le", "sei", "dir", "ge\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu einem Huldigungsrufe.", "tokens": ["Zu", "ei\u00b7nem", "Hul\u00b7di\u00b7gungs\u00b7ru\u00b7fe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "O liebentglommener Rosenstrauch,", "tokens": ["O", "lie\u00b7bent\u00b7glom\u00b7me\u00b7ner", "Ro\u00b7sen\u00b7strauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Himmels Tau dich erquicke!", "tokens": ["Des", "Him\u00b7mels", "Tau", "dich", "er\u00b7qui\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Beschirme dich Gott vor giftigem Hauch", "tokens": ["Be\u00b7schir\u00b7me", "dich", "Gott", "vor", "gif\u00b7ti\u00b7gem", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Welt und sch\u00e4dlichem Blicke!", "tokens": ["Der", "Welt", "und", "sch\u00e4d\u00b7li\u00b7chem", "Bli\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}