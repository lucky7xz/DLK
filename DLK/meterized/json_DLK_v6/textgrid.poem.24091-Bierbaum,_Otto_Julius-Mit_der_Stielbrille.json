{"textgrid.poem.24091": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Mit der Stielbrille", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch immer sprichts in mir: Es kann nicht sein,", "tokens": ["Noch", "im\u00b7mer", "sprichts", "in", "mir", ":", "Es", "kann", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es ist ein w\u00fcster, widerlicher Traum;", "tokens": ["Es", "ist", "ein", "w\u00fcs\u00b7ter", ",", "wi\u00b7der\u00b7li\u00b7cher", "Traum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich mu\u00df mich sch\u00e4men, da\u00df ich so getr\u00e4umt.", "tokens": ["Ich", "mu\u00df", "mich", "sch\u00e4\u00b7men", ",", "da\u00df", "ich", "so", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und hin zu ihr und ihr die Hand gek\u00fc\u00dft:", "tokens": ["Und", "hin", "zu", "ihr", "und", "ihr", "die", "Hand", "ge\u00b7k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "KON", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vergieb mir, du, ich sah im Traum dich schlecht,", "tokens": ["Ver\u00b7gieb", "mir", ",", "du", ",", "ich", "sah", "im", "Traum", "dich", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "$,", "PPER", "VVFIN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Traum hat dich beleidigt; ich bin krank,", "tokens": ["Mein", "Traum", "hat", "dich", "be\u00b7lei\u00b7digt", ";", "ich", "bin", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich so frevelhaften Wahnsinn spann.", "tokens": ["Da\u00df", "ich", "so", "fre\u00b7vel\u00b7haf\u00b7ten", "Wahn\u00b7sinn", "spann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und dann erwach ich in die Wirklichkeit", "tokens": ["Und", "dann", "er\u00b7wach", "ich", "in", "die", "Wirk\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und seh mich um und sehe mich allein", "tokens": ["Und", "seh", "mich", "um", "und", "se\u00b7he", "mich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und wei\u00df und sag es laut zur leeren Wand:", "tokens": ["Und", "wei\u00df", "und", "sag", "es", "laut", "zur", "lee\u00b7ren", "Wand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Bonjour madame, ich habe mich geirrt,", "tokens": ["Bon\u00b7jour", "ma\u00b7da\u00b7me", ",", "ich", "ha\u00b7be", "mich", "ge\u00b7irrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Es war kein Traum, jedoch ein Tr\u00e4umer ich.", "tokens": ["Es", "war", "kein", "Traum", ",", "je\u00b7doch", "ein", "Tr\u00e4u\u00b7mer", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Bonjour madame et bon plaisir, die Welt", "tokens": ["Bon\u00b7jour", "ma\u00b7da\u00b7me", "et", "bon", "plai\u00b7sir", ",", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Ist h\u00f6chst verwunderlich und ein Roman.", "tokens": ["Ist", "h\u00f6chst", "ver\u00b7wun\u00b7der\u00b7lich", "und", "ein", "Ro\u00b7man", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Man mu\u00df sie nehmen, wie sie ist, Madam.", "tokens": ["Man", "mu\u00df", "sie", "neh\u00b7men", ",", "wie", "sie", "ist", ",", "Ma\u00b7dam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PPER", "VAFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Man mu\u00df nicht meinen, sie sei so und so,", "tokens": ["Man", "mu\u00df", "nicht", "mei\u00b7nen", ",", "sie", "sei", "so", "und", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und mu\u00df aus Tr\u00e4umen sich kein Goldgespinst", "tokens": ["Und", "mu\u00df", "aus", "Tr\u00e4u\u00b7men", "sich", "kein", "Gold\u00b7ge\u00b7spinst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Um Stirn und Auge legen, nein, Madam,", "tokens": ["Um", "Stirn", "und", "Au\u00b7ge", "le\u00b7gen", ",", "nein", ",", "Ma\u00b7dam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "VVINF", "$,", "PTKANT", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Mit klarem Auge, l\u00e4cheln und moquant,", "tokens": ["Mit", "kla\u00b7rem", "Au\u00b7ge", ",", "l\u00e4\u00b7cheln", "und", "mo\u00b7quant", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Mu\u00df man sie ansehn wie ein Kavalier", "tokens": ["Mu\u00df", "man", "sie", "an\u00b7sehn", "wie", "ein", "Ka\u00b7va\u00b7lier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PPER", "VVINF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Zur sehr gescheiten Zeit des Rokoko,", "tokens": ["Zur", "sehr", "ge\u00b7schei\u00b7ten", "Zeit", "des", "Ro\u00b7ko\u00b7ko", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "ADJA", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Das Stielglas vor der Nase und, ah\u00fc,", "tokens": ["Das", "Stiel\u00b7glas", "vor", "der", "Na\u00b7se", "und", ",", "a\u00b7h\u00fc", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "$,", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ein wenig h\u00fcsteln, wenn man etwa merkt,", "tokens": ["Ein", "we\u00b7nig", "h\u00fcs\u00b7teln", ",", "wenn", "man", "et\u00b7wa", "merkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Sie sei nur eine Dirne, keine Fee.", "tokens": ["Sie", "sei", "nur", "ei\u00b7ne", "Dir\u00b7ne", ",", "kei\u00b7ne", "Fee", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}