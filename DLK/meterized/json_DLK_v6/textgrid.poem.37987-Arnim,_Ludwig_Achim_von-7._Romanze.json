{"textgrid.poem.37987": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "7. Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Hochzeit je mit solcher Pracht,", "tokens": ["Kein", "Hoch\u00b7zeit", "je", "mit", "sol\u00b7cher", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gehalten ward bis tief in die Nacht,", "tokens": ["Ge\u00b7hal\u00b7ten", "ward", "bis", "tief", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Viel Lieder und viel Saitenspiel,", "tokens": ["Viel", "Lie\u00b7der", "und", "viel", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man h\u00f6rte in dem Schlosse viel,", "tokens": ["Man", "h\u00f6r\u00b7te", "in", "dem", "Schlos\u00b7se", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alles bey dem Tische sa\u00df,", "tokens": ["Und", "al\u00b7les", "bey", "dem", "Ti\u00b7sche", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man war da fr\u00f6hlich ohne Maa\u00df.", "tokens": ["Man", "war", "da", "fr\u00f6h\u00b7lich", "oh\u00b7ne", "Maa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sa\u00dfen da im gro\u00dfen Saal,", "tokens": ["Sie", "sa\u00b7\u00dfen", "da", "im", "gro\u00b7\u00dfen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alsbald da sah man \u00fcberall,", "tokens": ["Als\u00b7bald", "da", "sah", "man", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die M\u00e4nner sahens und die Frauen,", "tokens": ["Die", "M\u00e4n\u00b7ner", "sa\u00b7hens", "und", "die", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie konnten beyde es anschauen,", "tokens": ["Sie", "konn\u00b7ten", "bey\u00b7de", "es", "an\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie etwas durch die B\u00fchne stie\u00df,", "tokens": ["Wie", "et\u00b7was", "durch", "die", "B\u00fch\u00b7ne", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Menschen-Fu\u00df sich sehen lie\u00df.", "tokens": ["Ein", "Men\u00b7schen\u00b7Fu\u00df", "sich", "se\u00b7hen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Blos zeigt er sich bis an die Knie,", "tokens": ["Blos", "zeigt", "er", "sich", "bis", "an", "die", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein sch\u00f6nern Fu\u00df sie sahen nie,", "tokens": ["Kein", "sch\u00f6\u00b7nern", "Fu\u00df", "sie", "sa\u00b7hen", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Fu\u00df wohl \u00fcberm Saal erscheint,", "tokens": ["Der", "Fu\u00df", "wohl", "\u00fc\u00b7berm", "Saal", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6n und wei\u00df wie Elfenbein,", "tokens": ["So", "sch\u00f6n", "und", "wei\u00df", "wie", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Ritter still sa\u00df bey der Braut,", "tokens": ["Der", "Rit\u00b7ter", "still", "sa\u00df", "bey", "der", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die schrie auf und schrie laut.", "tokens": ["Die", "schrie", "auf", "und", "schrie", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--++", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Ritter, als er den Fu\u00df ersah,", "tokens": ["Der", "Rit\u00b7ter", ",", "als", "er", "den", "Fu\u00df", "er\u00b7sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erschrack er und ganz traurig sprach:", "tokens": ["Er\u00b7schrack", "er", "und", "ganz", "trau\u00b7rig", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbo Weh, o Weh, mir armem Mann!\u00ab", "tokens": ["\u00bb", "o", "Weh", ",", "o", "Weh", ",", "mir", "ar\u00b7mem", "Mann", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$,", "FM", "NN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wurde bleich von Stunde an.", "tokens": ["Und", "wur\u00b7de", "bleich", "von", "Stun\u00b7de", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man bracht ihm sein kristallnes Glas,", "tokens": ["Man", "bracht", "ihm", "sein", "kris\u00b7tall\u00b7nes", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sah es an und wurde bla\u00df.", "tokens": ["Er", "sah", "es", "an", "und", "wur\u00b7de", "bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er sah in dem kristallnen Pokale,", "tokens": ["Er", "sah", "in", "dem", "kris\u00b7tall\u00b7nen", "Po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Kind das schlief beym lauten Mahle,", "tokens": ["Ein", "Kind", "das", "schlief", "beym", "lau\u00b7ten", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es schlief vom Weine \u00fcberdeckt,", "tokens": ["Es", "schlief", "vom", "Wei\u00b7ne", "\u00fc\u00b7berd\u00b7eckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein F\u00fc\u00dfchen hat es vorgestreckt,", "tokens": ["Ein", "F\u00fc\u00df\u00b7chen", "hat", "es", "vor\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wie der Wein getrunken aus,", "tokens": ["Doch", "wie", "der", "Wein", "ge\u00b7trun\u00b7ken", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So schwand das Kindlein auch hinaus.", "tokens": ["So", "schwand", "das", "Kin\u00b7dlein", "auch", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Ritter sprach: \u00bbDer gro\u00dfen Noth,", "tokens": ["Der", "Rit\u00b7ter", "sprach", ":", "\u00bb", "Der", "gro\u00b7\u00dfen", "Noth", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dreyen Tagen da bin ich todt.\u00ab", "tokens": ["In", "drey\u00b7en", "Ta\u00b7gen", "da", "bin", "ich", "todt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "NN", "ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Fu\u00df, der war verschwunden da,", "tokens": ["Der", "Fu\u00df", ",", "der", "war", "ver\u00b7schwun\u00b7den", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jeder trat der B\u00fchne nah,", "tokens": ["Ein", "je\u00b7der", "trat", "der", "B\u00fch\u00b7ne", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo doch der Fu\u00df w\u00e4r kommen hin,", "tokens": ["Wo", "doch", "der", "Fu\u00df", "w\u00e4r", "kom\u00b7men", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Loch sah man da in der B\u00fchn.", "tokens": ["Kein", "Loch", "sah", "man", "da", "in", "der", "B\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "All Freud und Kurzweil war zerst\u00f6rt,", "tokens": ["All", "Freud", "und", "Kurz\u00b7weil", "war", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Instrument wurd nimmer geh\u00f6rt,", "tokens": ["Kein", "Inst\u00b7ru\u00b7ment", "wurd", "nim\u00b7mer", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Aus war das Tanzen und das Singen,", "tokens": ["Aus", "war", "das", "Tan\u00b7zen", "und", "das", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Turnieren, K\u00e4mpfen, Fechten, Ringen,", "tokens": ["Tur\u00b7nie\u00b7ren", ",", "K\u00e4mp\u00b7fen", ",", "Fech\u00b7ten", ",", "Rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Das alles still darnieder leit,", "tokens": ["Das", "al\u00b7les", "still", "dar\u00b7nie\u00b7der", "leit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADJD", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die G\u00e4ste fliehn in die Felder weit.", "tokens": ["Die", "G\u00e4s\u00b7te", "fliehn", "in", "die", "Fel\u00b7der", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Braut nur bleibt bey ihrem Mann,", "tokens": ["Die", "Braut", "nur", "bleibt", "bey", "ih\u00b7rem", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ritter sieht sie traurig an;", "tokens": ["Der", "Rit\u00b7ter", "sieht", "sie", "trau\u00b7rig", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbgesegne dich du edle Braut,", "tokens": ["\u00bb", "ge\u00b7seg\u00b7ne", "dich", "du", "ed\u00b7le", "Braut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bleibest bey mir, hast mir vertraut.\u00ab", "tokens": ["Du", "blei\u00b7best", "bey", "mir", ",", "hast", "mir", "ver\u00b7traut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "\u00bbdurch mich verliert ihr euer Leben,", "tokens": ["\u00bb", "durch", "mich", "ver\u00b7liert", "ihr", "eu\u00b7er", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In geistlichem Stand will ich nun leben.", "tokens": ["In", "geist\u00b7li\u00b7chem", "Stand", "will", "ich", "nun", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Das heilge Oel empfing er dann,", "tokens": ["Das", "heil\u00b7ge", "O\u00b7el", "emp\u00b7fing", "er", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Nach dreyen Tagen rief der Mann:", "tokens": ["Nach", "drey\u00b7en", "Ta\u00b7gen", "rief", "der", "Mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmein Herr und Gott in deine H\u00e4nd,", "tokens": ["\u00bb", "mein", "Herr", "und", "Gott", "in", "dei\u00b7ne", "H\u00e4nd", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich meine arme Seele send,", "tokens": ["Ich", "mei\u00b7ne", "ar\u00b7me", "See\u00b7le", "send", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Seel thu ich