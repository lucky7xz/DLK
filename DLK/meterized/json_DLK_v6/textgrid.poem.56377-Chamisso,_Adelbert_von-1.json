{"textgrid.poem.56377": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu sangest sonst von Frauen-Lieb und Leben,", "tokens": ["\u00bb", "du", "san\u00b7gest", "sonst", "von", "Frau\u00b7en\u00b7Lieb", "und", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein trauter Freund, mir sch\u00f6ne Lieder vor;", "tokens": ["Mein", "trau\u00b7ter", "Freund", ",", "mir", "sch\u00f6\u00b7ne", "Lie\u00b7der", "vor", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An deinen lieben Lippen hing mein Ohr,", "tokens": ["An", "dei\u00b7nen", "lie\u00b7ben", "Lip\u00b7pen", "hing", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich f\u00fchlte mich in Lieb und Lust erbeben.", "tokens": ["Ich", "f\u00fchl\u00b7te", "mich", "in", "Lieb", "und", "Lust", "er\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du singst nicht mehr; \u2013 um deine Lyra weben", "tokens": ["Du", "singst", "nicht", "mehr", ";", "\u2013", "um", "dei\u00b7ne", "Ly\u00b7ra", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$.", "$(", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Spinnen, d\u00fcnkt mich, einen Trauerflor;", "tokens": ["Die", "Spin\u00b7nen", ",", "d\u00fcnkt", "mich", ",", "ei\u00b7nen", "Trau\u00b7er\u00b7flor", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sprich, wirst du nie die Lust, die ich verlor,", "tokens": ["Sprich", ",", "wirst", "du", "nie", "die", "Lust", ",", "die", "ich", "ver\u00b7lor", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du s\u00fc\u00dfer Liedermund, mir wiedergeben?\u00ab", "tokens": ["Du", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der\u00b7mund", ",", "mir", "wie\u00b7der\u00b7ge\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich trage selbst \u2013 still, still! mein gutes Kind \u2013", "tokens": ["Ich", "tra\u00b7ge", "selbst", "\u2013", "still", ",", "still", "!", "mein", "gu\u00b7tes", "Kind", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PTKVZ", "$,", "ADJD", "$.", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geduldig und entbehre sonder Klage;", "tokens": ["Ge\u00b7dul\u00b7dig", "und", "ent\u00b7beh\u00b7re", "son\u00b7der", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bin m\u00fcde jetzt, verklungen ist mein Singen.", "tokens": ["Bin", "m\u00fc\u00b7de", "jetzt", ",", "ver\u00b7klun\u00b7gen", "ist", "mein", "Sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein S\u00e4nger war ich, wie die V\u00f6gel sind,", "tokens": ["Ein", "S\u00e4n\u00b7ger", "war", "ich", ",", "wie", "die", "V\u00f6\u00b7gel", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die kleinen, die nur zwitschern ihre Tage. \u2013", "tokens": ["Die", "klei\u00b7nen", ",", "die", "nur", "zwit\u00b7schern", "ih\u00b7re", "Ta\u00b7ge", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Schwan nur... \u2013 Reden wir von andern Dingen!", "tokens": ["Der", "Schwan", "nur", "...", "\u2013", "Re\u00b7den", "wir", "von", "an\u00b7dern", "Din\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "$(", "NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}