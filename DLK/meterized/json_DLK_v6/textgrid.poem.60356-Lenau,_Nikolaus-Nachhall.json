{"textgrid.poem.60356": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Nachhall", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wandrer l\u00e4\u00dft sein helles Lied erklingen:", "tokens": ["Ein", "Wand\u00b7rer", "l\u00e4\u00dft", "sein", "hel\u00b7les", "Lied", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun schweigt er still und schwindet in den F\u00f6hren;", "tokens": ["Nun", "schweigt", "er", "still", "und", "schwin\u00b7det", "in", "den", "F\u00f6h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6chte l\u00e4nger noch ihn singen h\u00f6ren,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "l\u00e4n\u00b7ger", "noch", "ihn", "sin\u00b7gen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch tr\u00f6st ich mich: er kann nicht ewig singen.", "tokens": ["Doch", "tr\u00f6st", "ich", "mich", ":", "er", "kann", "nicht", "e\u00b7wig", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "$.", "PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Wandrer schweigt, doch jene Felsen bringen", "tokens": ["Der", "Wand\u00b7rer", "schweigt", ",", "doch", "je\u00b7ne", "Fel\u00b7sen", "brin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir seinen Widerhall in dunklen Ch\u00f6ren,", "tokens": ["Mir", "sei\u00b7nen", "Wi\u00b7der\u00b7hall", "in", "dunk\u00b7len", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als wollten sie sein Lied zur\u00fcckbeschw\u00f6ren,", "tokens": ["Als", "woll\u00b7ten", "sie", "sein", "Lied", "zu\u00b7r\u00fcck\u00b7be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun ist es still \u2013 den Quell nur h\u00f6r ich springen.", "tokens": ["Nun", "ist", "es", "still", "\u2013", "den", "Quell", "nur", "h\u00f6r", "ich", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$(", "ART", "NN", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Wandrer schwieg und schied; ich sprach gelassen:", "tokens": ["Der", "Wand\u00b7rer", "schwieg", "und", "schied", ";", "ich", "sprach", "ge\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fahr wohl! Warum denn f\u00fchl ich jetzt ein Trauern,", "tokens": ["Fahr", "wohl", "!", "Wa\u00b7rum", "denn", "f\u00fchl", "ich", "jetzt", "ein", "Trau\u00b7ern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PWAV", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df l\u00e4nger nicht sein Nachhall mochte dauern?", "tokens": ["Da\u00df", "l\u00e4n\u00b7ger", "nicht", "sein", "Nach\u00b7hall", "moch\u00b7te", "dau\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PTKNEG", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mehr als des Menschen Tod will michs erfassen,", "tokens": ["Mehr", "als", "des", "Men\u00b7schen", "Tod", "will", "michs", "er\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ihn bereits nach wenig Tagesneigen", "tokens": ["Wenn", "ihn", "be\u00b7reits", "nach", "we\u00b7nig", "Ta\u00b7ge\u00b7snei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier, dort noch einer nennt \u2013 bis alle schweigen.", "tokens": ["Hier", ",", "dort", "noch", "ei\u00b7ner", "nennt", "\u2013", "bis", "al\u00b7le", "schwei\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "PIS", "VVFIN", "$(", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}