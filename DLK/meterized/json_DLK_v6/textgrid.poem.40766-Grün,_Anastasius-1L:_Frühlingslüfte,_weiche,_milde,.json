{"textgrid.poem.40766": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fr\u00fchlingsl\u00fcfte, weiche, milde,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchlingsl\u00fcfte, weiche, milde,", "tokens": ["Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ",", "wei\u00b7che", ",", "mil\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streichen um Egyptens Lande,", "tokens": ["Strei\u00b7chen", "um", "E\u00b7gyp\u00b7tens", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hauchen in das Saatgefilde,", "tokens": ["Hau\u00b7chen", "in", "das", "Saat\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4cheln \u00fcber starrem Sande;", "tokens": ["F\u00e4\u00b7cheln", "\u00fc\u00b7ber", "star\u00b7rem", "San\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was da wallt, soll frischer wallen,", "tokens": ["Was", "da", "wallt", ",", "soll", "fri\u00b7scher", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was da lebt, soll doppelt leben,", "tokens": ["Was", "da", "lebt", ",", "soll", "dop\u00b7pelt", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch was todt ist, soll zerfallen,", "tokens": ["Doch", "was", "todt", "ist", ",", "soll", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich verj\u00fcngt einst zu erheben.", "tokens": ["Sich", "ver\u00b7j\u00fcngt", "einst", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchlingsl\u00fcfte wollen haschen", "tokens": ["Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "wol\u00b7len", "ha\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fl\u00fccht'ge Keime halberstorben,", "tokens": ["Fl\u00fccht'\u00b7ge", "Kei\u00b7me", "hal\u00b7ber\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst des Grabs zerstreute Aschen", "tokens": ["Selbst", "des", "Grabs", "zer\u00b7streu\u00b7te", "A\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unverloren, unverdorben;", "tokens": ["Un\u00b7ver\u00b7lo\u00b7ren", ",", "un\u00b7ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes finde seine St\u00e4tte", "tokens": ["Je\u00b7des", "fin\u00b7de", "sei\u00b7ne", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Nilthals reichen Schollen,", "tokens": ["In", "des", "Nilt\u00b7hals", "rei\u00b7chen", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo Gestad' und Strom zur Wette", "tokens": ["Wo", "Ge\u00b7stad'", "und", "Strom", "zur", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Volle Segenswogen rollen.", "tokens": ["Vol\u00b7le", "Se\u00b7gens\u00b7wo\u00b7gen", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sie wehn unaufgehalten", "tokens": ["Und", "sie", "wehn", "un\u00b7auf\u00b7ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die alten Nekropolen,", "tokens": ["Um", "die", "al\u00b7ten", "Nek\u00b7ro\u00b7po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch der Pyramide Spalten", "tokens": ["Durch", "der", "Py\u00b7ra\u00b7mi\u00b7de", "Spal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00fcpfen sie hinein verstohlen,", "tokens": ["Schl\u00fcp\u00b7fen", "sie", "hin\u00b7ein", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch der G\u00e4nge Schlangengleise", "tokens": ["Durch", "der", "G\u00e4n\u00b7ge", "Schlan\u00b7gen\u00b7glei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis zum Zellengrab zu schleichen,", "tokens": ["Bis", "zum", "Zel\u00b7len\u00b7grab", "zu", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "R\u00fctteln an den S\u00e4rgen leise,", "tokens": ["R\u00fct\u00b7teln", "an", "den", "S\u00e4r\u00b7gen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fl\u00fcstern in das Ohr der Leichen.", "tokens": ["Fl\u00fcs\u00b7tern", "in", "das", "Ohr", "der", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und die K\u00f6nigsmumie drinnen,", "tokens": ["Und", "die", "K\u00f6\u00b7nigs\u00b7mu\u00b7mie", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Prunkversteint und unverwittert,", "tokens": ["Prunk\u00b7ver\u00b7steint", "und", "un\u00b7ver\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlt den Hauch zum Herzen rinnen,", "tokens": ["F\u00fchlt", "den", "Hauch", "zum", "Her\u00b7zen", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ein Zucken sie durchzittert;", "tokens": ["Da\u00df", "ein", "Zu\u00b7cken", "sie", "durch\u00b7zit\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6cht' entraffen sich den Gr\u00fcften,", "tokens": ["M\u00f6cht'", "en\u00b7traf\u00b7fen", "sich", "den", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht zu leben, nicht zu wallen,", "tokens": ["Nicht", "zu", "le\u00b7ben", ",", "nicht", "zu", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nein, hinaus nur, an den L\u00fcften", "tokens": ["Nein", ",", "hin\u00b7aus", "nur", ",", "an", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ADV", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zu verwehn und zu zerfallen:", "tokens": ["Zu", "ver\u00b7wehn", "und", "zu", "zer\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbfr\u00fchling, Fr\u00fchling! Auch den Todten", "tokens": ["\u00bb", "fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", "!", "Auch", "den", "Tod\u00b7ten"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stillersehnt und s\u00fc\u00dfwillkommen!", "tokens": ["Stil\u00b7ler\u00b7sehnt", "und", "s\u00fc\u00df\u00b7will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sendest uns auch deine Boten", "tokens": ["Sen\u00b7dest", "uns", "auch", "dei\u00b7ne", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Haft, die uns beklommen;", "tokens": ["In", "die", "Haft", ",", "die", "uns", "be\u00b7klom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, schon f\u00fchl' ich deine Quellen", "tokens": ["Ja", ",", "schon", "f\u00fchl'", "ich", "dei\u00b7ne", "Quel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leis in meinen Adern rinnen,", "tokens": ["Leis", "in", "mei\u00b7nen", "A\u00b7dern", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein Verlebtes fortzuschwellen,", "tokens": ["Mein", "Ver\u00b7leb\u00b7tes", "fort\u00b7zu\u00b7schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir ein neu Gewand zu spinnen.", "tokens": ["Mir", "ein", "neu", "Ge\u00b7wand", "zu", "spin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weh, vergiftet meine S\u00e4fte,", "tokens": ["Weh", ",", "ver\u00b7gif\u00b7tet", "mei\u00b7ne", "S\u00e4f\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df daran der Fr\u00fchling machtlos;", "tokens": ["Da\u00df", "da\u00b7ran", "der", "Fr\u00fch\u00b7ling", "macht\u00b7los", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und bet\u00e4ubt die tiefsten Kr\u00e4fte,", "tokens": ["Und", "be\u00b7t\u00e4ubt", "die", "tiefs\u00b7ten", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst des Auferstehens achtlos!", "tokens": ["Selbst", "des", "Auf\u00b7er\u00b7ste\u00b7hens", "acht\u00b7los", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mit den Harzen und Balsamen", "tokens": ["Mit", "den", "Har\u00b7zen", "und", "Bal\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "Eingetr\u00e4uft in meine Adern,", "tokens": ["Ein\u00b7ge\u00b7tr\u00e4uft", "in", "mei\u00b7ne", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Starb des Lebens letzter Samen,", "tokens": ["Starb", "des", "Le\u00b7bens", "letz\u00b7ter", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ward ich stumpf wie diese Quadern!", "tokens": ["Ward", "ich", "stumpf", "wie", "die\u00b7se", "Qua\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sklaven, die mit feigem Bangen", "tokens": ["Skla\u00b7ven", ",", "die", "mit", "fei\u00b7gem", "Ban\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinem Augenwink gezittert,", "tokens": ["Mei\u00b7nem", "Au\u00b7gen\u00b7wink", "ge\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Halten mich im Schlaf gefangen,", "tokens": ["Hal\u00b7ten", "mich", "im", "Schlaf", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Angefesselt und umgittert;", "tokens": ["An\u00b7ge\u00b7fes\u00b7selt", "und", "um\u00b7git\u00b7tert", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An die eherne Erstarrung", "tokens": ["An", "die", "e\u00b7her\u00b7ne", "Er\u00b7star\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Haben sie mich festgekettet,", "tokens": ["Ha\u00b7ben", "sie", "mich", "fest\u00b7ge\u00b7ket\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu lebendiger Verstarrung", "tokens": ["Zu", "le\u00b7ben\u00b7di\u00b7ger", "Ver\u00b7star\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "In den Cedernschrein gebettet!", "tokens": ["In", "den", "Ce\u00b7dern\u00b7schrein", "ge\u00b7bet\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Der mich zu verg\u00f6ttern glaubte,", "tokens": ["Der", "mich", "zu", "ver\u00b7g\u00f6t\u00b7tern", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Knechtsinn, hat mich hingerichtet,", "tokens": ["Knech\u00b7tsinn", ",", "hat", "mich", "hin\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir, da er mein Welken raubte,", "