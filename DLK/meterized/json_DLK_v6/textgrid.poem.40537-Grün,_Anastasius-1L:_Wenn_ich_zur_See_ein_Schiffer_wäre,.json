{"textgrid.poem.40537": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich zur See ein Schiffer w\u00e4re,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich zur See ein Schiffer w\u00e4re,", "tokens": ["Wenn", "ich", "zur", "See", "ein", "Schif\u00b7fer", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vorbei die\u00df Ufer k\u00f6nnt' ich nie;", "tokens": ["Vor\u00b7bei", "die\u00df", "U\u00b7fer", "k\u00f6nnt'", "ich", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Je hell're Luft, je still're Meere,", "tokens": ["Je", "hell'\u00b7re", "Luft", ",", "je", "still'\u00b7re", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sich'rer litt ich Schiffbruch hie!", "tokens": ["So", "sich'\u00b7rer", "litt", "ich", "Schiff\u00b7bruch", "hie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Willst du, o Herr, nicht, da\u00df ich strande,", "tokens": ["Willst", "du", ",", "o", "Herr", ",", "nicht", ",", "da\u00df", "ich", "stran\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Th\u00fcrm' auf im Sturm den Wogenschwall,", "tokens": ["Th\u00fcrm'", "auf", "im", "Sturm", "den", "Wo\u00b7gen\u00b7schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verh\u00fcll' in Nebel diese Lande,", "tokens": ["Ver\u00b7h\u00fcll'", "in", "Ne\u00b7bel", "die\u00b7se", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "G\u00fcrt' ums Gestad' der Brandung Wall!", "tokens": ["G\u00fcrt'", "ums", "Ge\u00b7stad'", "der", "Bran\u00b7dung", "Wall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Denn dieser Sturm von Sonnenl\u00fcften,", "tokens": ["Denn", "die\u00b7ser", "Sturm", "von", "Son\u00b7nen\u00b7l\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Bl\u00fcthengluth und Lorbeernacht,", "tokens": ["Von", "Bl\u00fc\u00b7then\u00b7gluth", "und", "Lor\u00b7beer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Schmeichelwinden, Fr\u00fchlingsd\u00fcften", "tokens": ["Von", "Schmei\u00b7chel\u00b7win\u00b7den", ",", "Fr\u00fch\u00b7lings\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist's, der mich hier noch scheitern macht!", "tokens": ["Ist's", ",", "der", "mich", "hier", "noch", "schei\u00b7tern", "macht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Viel tausend Blumenfesseln schwingt es", "tokens": ["Viel", "tau\u00b7send", "Blu\u00b7men\u00b7fes\u00b7seln", "schwingt", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von jenen Bergen her nach mir,", "tokens": ["Von", "je\u00b7nen", "Ber\u00b7gen", "her", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APZR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In L\u00fcften rauscht's, aus B\u00fcschen singt es:", "tokens": ["In", "L\u00fcf\u00b7ten", "rauscht's", ",", "aus", "B\u00fc\u00b7schen", "singt", "es", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O bleibe hier, o bleibe hier!", "tokens": ["O", "blei\u00b7be", "hier", ",", "o", "blei\u00b7be", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "FM", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Maid vom Gebirge, deine Augen,", "tokens": ["Maid", "vom", "Ge\u00b7bir\u00b7ge", ",", "dei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Leitsterne, dran mein Blick gebannt,", "tokens": ["Leits\u00b7ter\u00b7ne", ",", "dran", "mein", "Blick", "ge\u00b7bannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sie mochten die\u00dfmal eben taugen,", "tokens": ["Sie", "moch\u00b7ten", "die\u00df\u00b7mal", "e\u00b7ben", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Schiff zu locken auf den Strand!", "tokens": ["Mein", "Schiff", "zu", "lo\u00b7cken", "auf", "den", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Weh, von den gl\u00fchenden Granaten", "tokens": ["Weh", ",", "von", "den", "gl\u00fc\u00b7hen\u00b7den", "Gra\u00b7na\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Geschossen wird es in den Grund!", "tokens": ["Ge\u00b7schos\u00b7sen", "wird", "es", "in", "den", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geentert wird es von Piraten,", "tokens": ["Ge\u00b7en\u00b7tert", "wird", "es", "von", "Pi\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Bl\u00fcthenranken, kriegrisch bunt.", "tokens": ["Den", "Bl\u00fc\u00b7then\u00b7ran\u00b7ken", ",", "krie\u00b7grisch", "bunt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie springen an des Bord's Altane", "tokens": ["Sie", "sprin\u00b7gen", "an", "des", "Bord's", "Al\u00b7ta\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und klettern rings empor in Hast,", "tokens": ["Und", "klet\u00b7tern", "rings", "em\u00b7por", "in", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Rose, deine Flaggenfahne,", "tokens": ["Die", "Ro\u00b7se", ",", "dei\u00b7ne", "Flag\u00b7gen\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu pflanzen auf Kastell und Mast.", "tokens": ["Zu", "pflan\u00b7zen", "auf", "Kas\u00b7tell", "und", "Mast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NE", "KON", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "O la\u00df mich ruhn vor deiner Schwelle,", "tokens": ["O", "la\u00df", "mich", "ruhn", "vor", "dei\u00b7ner", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schaun aufs weite Meergebiet", "tokens": ["Und", "schaun", "aufs", "wei\u00b7te", "Meer\u00b7ge\u00b7biet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in dein Aug', das liebe, helle,", "tokens": ["Und", "in", "dein", "Aug'", ",", "das", "lie\u00b7be", ",", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und singen laut mein Schifferlied,", "tokens": ["Und", "sin\u00b7gen", "laut", "mein", "Schif\u00b7fer\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df deine Berg' empor es brandet,", "tokens": ["Da\u00df", "dei\u00b7ne", "Ber\u00b7g'", "em\u00b7por", "es", "bran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als schl\u00fcge dr\u00fcber Wogenklang!", "tokens": ["Als", "schl\u00fc\u00b7ge", "dr\u00fc\u00b7ber", "Wo\u00b7gen\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl hat noch Keiner, der gestrandet,", "tokens": ["Wohl", "hat", "noch", "Kei\u00b7ner", ",", "der", "ge\u00b7stran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gestimmt so fr\u00f6hlichen Gesang.", "tokens": ["Ge\u00b7stimmt", "so", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}