{"dta.poem.19552": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der Kamerad.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Mit dem Tode schlo\u00df ich Kameradschaft.", "tokens": ["Mit", "dem", "To\u00b7de", "schlo\u00df", "ich", "Ka\u00b7me\u00b7rad\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ueber einem vollen Humpen sa\u00dfen", "tokens": ["Ue\u00b7ber", "ei\u00b7nem", "vol\u00b7len", "Hum\u00b7pen", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Oft wir n\u00e4chtens und philosophirten.", "tokens": ["Oft", "wir", "n\u00e4ch\u00b7tens", "und", "phi\u00b7lo\u00b7so\u00b7phir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "KON", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Auch zusammen gingen wir spaziren,", "tokens": ["Auch", "zu\u00b7sam\u00b7men", "gin\u00b7gen", "wir", "spa\u00b7zi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lauschten mit elegischen Gef\u00fchlen", "tokens": ["Lauschten", "mit", "e\u00b7le\u00b7gi\u00b7schen", "Ge\u00b7f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Nach dem Pilgerruf der Abendglocke.", "tokens": ["Nach", "dem", "Pil\u00b7ger\u00b7ruf", "der", "A\u00b7bend\u00b7glo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Aber m\u00e4nnlich auch an meiner Seite", "tokens": ["A\u00b7ber", "m\u00e4nn\u00b7lich", "auch", "an", "mei\u00b7ner", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Stand der Kamerad und secundirte,", "tokens": ["Stand", "der", "Ka\u00b7me\u00b7rad", "und", "se\u00b7cun\u00b7dir\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Oder wann ich im Gebirg verirrt war,", "tokens": ["O\u00b7der", "wann", "ich", "im", "Ge\u00b7birg", "ver\u00b7irrt", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Hangend \u00fcber schwindelnd tiefem Abgrund,", "tokens": ["Han\u00b7gend", "\u00fc\u00b7ber", "schwin\u00b7delnd", "tie\u00b7fem", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Sprach er: Blick mir in das Auge ruhig", "tokens": ["Sprach", "er", ":", "Blick", "mir", "in", "das", "Au\u00b7ge", "ru\u00b7hig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "NN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Und ich that es und ich war gerettet \u2014", "tokens": ["Und", "ich", "that", "es", "und", "ich", "war", "ge\u00b7ret\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Lange standen wir auf gutem Fu\u00dfe,", "tokens": ["Lan\u00b7ge", "stan\u00b7den", "wir", "auf", "gu\u00b7tem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Bis mich volles Leben \u00fcberstr\u00f6mte", "tokens": ["Bis", "mich", "vol\u00b7les", "Le\u00b7ben", "\u00fc\u00b7ber\u00b7str\u00f6m\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Gl\u00fchend warm mit unbekannter F\u00fclle,", "tokens": ["Gl\u00fc\u00b7hend", "warm", "mit", "un\u00b7be\u00b7kann\u00b7ter", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Und mir schauderte vor meinem Freunde ...", "tokens": ["Und", "mir", "schau\u00b7der\u00b7te", "vor", "mei\u00b7nem", "Freun\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Als das Liebchen heute mir am Hals hing,", "tokens": ["Als", "das", "Lieb\u00b7chen", "heu\u00b7te", "mir", "am", "Hals", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Ueber seine Schulter weg erblickt' ich", "tokens": ["Ue\u00b7ber", "sei\u00b7ne", "Schul\u00b7ter", "weg", "er\u00b7blickt'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Meines Kameraden leichten Umri\u00df", "tokens": ["Mei\u00b7nes", "Ka\u00b7me\u00b7ra\u00b7den", "leich\u00b7ten", "Um\u00b7ri\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "Auf dem Abendhimmel und er grollte:", "tokens": ["Auf", "dem", "A\u00b7bend\u00b7him\u00b7mel", "und", "er", "groll\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "\u201ebin ich dir verleidet? Deine feigen", "tokens": ["\u201e", "bin", "ich", "dir", "ver\u00b7lei\u00b7det", "?", "Dei\u00b7ne", "fei\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VAFIN", "PPER", "PPER", "VVPP", "$.", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Lippen meiden meinen schlichten Namen?", "tokens": ["Lip\u00b7pen", "mei\u00b7den", "mei\u00b7nen", "schlich\u00b7ten", "Na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Ist das h\u00fcbsch von einem Kameraden?\u201c", "tokens": ["Ist", "das", "h\u00fcbsch", "von", "ei\u00b7nem", "Ka\u00b7me\u00b7ra\u00b7den", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "In demselben Augenblick umarmte", "tokens": ["In", "dem\u00b7sel\u00b7ben", "Au\u00b7gen\u00b7blick", "um\u00b7arm\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Liebchen mich und rief: \u201eSo m\u00f6cht' ich sterben!", "tokens": ["Lieb\u00b7chen", "mich", "und", "rief", ":", "\u201e", "So", "m\u00f6cht'", "ich", "ster\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "$.", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Komme, Tod, und raub' mich, Tod, im Kusse!\u201c", "tokens": ["Kom\u00b7me", ",", "Tod", ",", "und", "raub'", "mich", ",", "Tod", ",", "im", "Kus\u00b7se", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Und der Tod, von schwellend jungen Lippen", "tokens": ["Und", "der", "Tod", ",", "von", "schwel\u00b7lend", "jun\u00b7gen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Hei\u00df und leidenschaftlich angerufen,", "tokens": ["Hei\u00df", "und", "lei\u00b7den\u00b7schaft\u00b7lich", "an\u00b7ge\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "H\u00f6rte seinen Namen mit Vergn\u00fcgen.", "tokens": ["H\u00f6r\u00b7te", "sei\u00b7nen", "Na\u00b7men", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Ueber sein geheimni\u00dfvolles Antlitz", "tokens": ["Ue\u00b7ber", "sein", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7les", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Glitt ein Leuchten und er schied in Minne.", "tokens": ["Glitt", "ein", "Leuch\u00b7ten", "und", "er", "schied", "in", "Min\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}