{"textgrid.poem.35781": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hienieden ward dem Lenze", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hienieden ward dem Lenze", "tokens": ["Hien\u00b7ie\u00b7den", "ward", "dem", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein kurzes Sein verlieh'n:", "tokens": ["Ein", "kur\u00b7zes", "Sein", "ver\u00b7lieh'n", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum wanden wir uns Kr\u00e4nze,", "tokens": ["Kaum", "wan\u00b7den", "wir", "uns", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist er schon dahin.", "tokens": ["So", "ist", "er", "schon", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sommer w\u00e4hrt nicht lange", "tokens": ["Der", "Som\u00b7mer", "w\u00e4hrt", "nicht", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit seiner Sicheln Schall:", "tokens": ["Mit", "sei\u00b7ner", "Si\u00b7cheln", "Schall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum r\u00f6thet unsre Wange", "tokens": ["Kaum", "r\u00f6\u00b7thet", "uns\u00b7re", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der w\u00e4rm're Sonnenstrahl.", "tokens": ["Der", "w\u00e4r\u00b7m'\u00b7re", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Bald wird der Himmel tr\u00fcber,", "tokens": ["Bald", "wird", "der", "Him\u00b7mel", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Frucht entf\u00e4llt dem Baum \u2013", "tokens": ["Die", "Frucht", "ent\u00b7f\u00e4llt", "dem", "Baum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon ist der Herbst vor\u00fcber,", "tokens": ["Schon", "ist", "der", "Herbst", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir freuten sein uns kaum.", "tokens": ["Wir", "freu\u00b7ten", "sein", "uns", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun steigt der Winter nieder", "tokens": ["Nun", "steigt", "der", "Win\u00b7ter", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schlie\u00dft des Jahres Reih'n!", "tokens": ["Und", "schlie\u00dft", "des", "Jah\u00b7res", "Reih'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schweigen alle Lieder.", "tokens": ["Es", "schwei\u00b7gen", "al\u00b7le", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er gr\u00e4bt die Blumen ein.", "tokens": ["Er", "gr\u00e4bt", "die", "Blu\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So eilen unsre Freuden,", "tokens": ["So", "ei\u00b7len", "uns\u00b7re", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So endet alle Lust,", "tokens": ["So", "en\u00b7det", "al\u00b7le", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So schwinden auch die Leiden,", "tokens": ["So", "schwin\u00b7den", "auch", "die", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kaum sind wir's uns bewu\u00dft.", "tokens": ["Kaum", "sind", "wir's", "uns", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nur was nach oben ziehet,", "tokens": ["Nur", "was", "nach", "o\u00b7ben", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das kann nicht untergehn;", "tokens": ["Das", "kann", "nicht", "un\u00b7ter\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was heilig in uns gl\u00fchet,", "tokens": ["Was", "hei\u00b7lig", "in", "uns", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das wird kein Nord verwehn.", "tokens": ["Das", "wird", "kein", "Nord", "ver\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und dort bl\u00fchn andre Lenze,", "tokens": ["Und", "dort", "bl\u00fchn", "and\u00b7re", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die nimmermehr entfliehn;", "tokens": ["Die", "nim\u00b7mer\u00b7mehr", "ent\u00b7fliehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dort werden ew'ge Kr\u00e4nze", "tokens": ["Dort", "wer\u00b7den", "ew'\u00b7ge", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um unsre Scheitel bl\u00fchn.", "tokens": ["Um", "uns\u00b7re", "Schei\u00b7tel", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O, la\u00dft dahin uns streben", "tokens": ["O", ",", "la\u00dft", "da\u00b7hin", "uns", "stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PAV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon hier im Schattenland.", "tokens": ["Schon", "hier", "im", "Schat\u00b7ten\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "All unser Thun und Leben", "tokens": ["All", "un\u00b7ser", "Thun", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sei nur auf Gott gewandt.", "tokens": ["Sei", "nur", "auf", "Gott", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}