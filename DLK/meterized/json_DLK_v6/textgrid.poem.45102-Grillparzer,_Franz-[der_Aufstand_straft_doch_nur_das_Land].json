{"textgrid.poem.45102": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[der Aufstand straft doch nur das Land]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Aufstand straft doch nur das Land", "tokens": ["Der", "Auf\u00b7stand", "straft", "doch", "nur", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kommt nicht in Betrachtung.", "tokens": ["Und", "kommt", "nicht", "in", "Be\u00b7trach\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist ein Mittel noch zur Hand,", "tokens": ["Doch", "ist", "ein", "Mit\u00b7tel", "noch", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wirkungsvollste Widerstand:", "tokens": ["Der", "wir\u00b7kungs\u00b7volls\u00b7te", "Wi\u00b7der\u00b7stand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gehorsam und Verachtung.", "tokens": ["Ge\u00b7hor\u00b7sam", "und", "Ver\u00b7ach\u00b7tung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}