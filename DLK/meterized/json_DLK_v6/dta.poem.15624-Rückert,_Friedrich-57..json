{"dta.poem.15624": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df einen Augenblick, es ziemt dem Menschenwitze,", "tokens": ["La\u00df", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", ",", "es", "ziemt", "dem", "Men\u00b7schen\u00b7wit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in die H\u00f6he wir gebaut vom Grund zur Spitze,", "tokens": ["Was", "in", "die", "H\u00f6\u00b7he", "wir", "ge\u00b7baut", "vom", "Grund", "zur", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVPP", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Sch\u00f6pfung Pyramid', auf deren Gipfel steht", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Py\u00b7ra\u00b7mid'", ",", "auf", "de\u00b7ren", "Gip\u00b7fel", "steht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "APPR", "PRELAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mensch, aus dem zur\u00fcck Gott in sich selber geht;", "tokens": ["Der", "Mensch", ",", "aus", "dem", "zu\u00b7r\u00fcck", "Gott", "in", "sich", "sel\u00b7ber", "geht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PTKVZ", "NN", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Umbauend la\u00df uns dis zu einem Kreise runden,", "tokens": ["Um\u00b7bau\u00b7end", "la\u00df", "uns", "dis", "zu", "ei\u00b7nem", "Krei\u00b7se", "run\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVIMP", "PPER", "PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gleich ist anderer Zusammenhang gefunden.", "tokens": ["Und", "gleich", "ist", "an\u00b7de\u00b7rer", "Zu\u00b7sam\u00b7men\u00b7hang", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Setz' Elemente hier, Luft, Feuer, Wasser, Erde,", "tokens": ["Setz'", "E\u00b7le\u00b7men\u00b7te", "hier", ",", "Luft", ",", "Feu\u00b7er", ",", "Was\u00b7ser", ",", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+--+++-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dann sage da\u00df aus ihr das Mineralreich werde.", "tokens": ["Dann", "sa\u00b7ge", "da\u00df", "aus", "ihr", "das", "Mi\u00b7ne\u00b7ral\u00b7reich", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOUS", "APPR", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus diesem aber la\u00df der Pflanzen Formen sprie\u00dfen,", "tokens": ["Aus", "die\u00b7sem", "a\u00b7ber", "la\u00df", "der", "Pflan\u00b7zen", "For\u00b7men", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "KON", "VVIMP", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an dieselben sich der Thierwelt Glieder schlie\u00dfen.", "tokens": ["Und", "an", "die\u00b7sel\u00b7ben", "sich", "der", "Thier\u00b7welt", "Glie\u00b7der", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "PRF", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und lie\u00dfest du daraus den Menschengeist entfalten,", "tokens": ["Und", "lie\u00b7\u00dfest", "du", "da\u00b7raus", "den", "Men\u00b7schen\u00b7geist", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df nun Geister auch elementarisch walten.", "tokens": ["So", "la\u00df", "nun", "Geis\u00b7ter", "auch", "e\u00b7lem\u00b7en\u00b7ta\u00b7risch", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und also kommt, damit im Kreis sei keine L\u00fccke,", "tokens": ["Und", "al\u00b7so", "kommt", ",", "da\u00b7mit", "im", "Kreis", "sei", "kei\u00b7ne", "L\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "APPRART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Elementarisches zum Element zur\u00fccke.", "tokens": ["E\u00b7lem\u00b7en\u00b7ta\u00b7ri\u00b7sches", "zum", "E\u00b7le\u00b7ment", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.8": {"line.1": {"text": "Wenn nun dem Kreise noch ein Mittelpunkt gebricht,", "tokens": ["Wenn", "nun", "dem", "Krei\u00b7se", "noch", "ein", "Mit\u00b7tel\u00b7punkt", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Setz' als der Sch\u00f6pfung Aus- und Einstralpunkt das Licht.", "tokens": ["Setz'", "als", "der", "Sch\u00f6p\u00b7fung", "Aus", "und", "Ein\u00b7stral\u00b7punkt", "das", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "TRUNC", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Den Kreis magst du beschaun, bis dich erfa\u00dft mit Graun", "tokens": ["Den", "Kreis", "magst", "du", "be\u00b7schaun", ",", "bis", "dich", "er\u00b7fa\u00dft", "mit", "Graun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ew'ge Wirbel, dann la\u00df uns was andres baun.", "tokens": ["Der", "ew'\u00b7ge", "Wir\u00b7bel", ",", "dann", "la\u00df", "uns", "was", "and\u00b7res", "baun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVIMP", "PPER", "PIS", "PIS", "VVINF", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}