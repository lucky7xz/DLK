{"textgrid.poem.42763": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Am Barren", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deutsche Frau, dich ruft der Barrn,", "tokens": ["Deut\u00b7sche", "Frau", ",", "dich", "ruft", "der", "Barrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn dies trauliche Gel\u00e4nder", "tokens": ["Denn", "dies", "trau\u00b7li\u00b7che", "Ge\u00b7l\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00f6rdert nicht nur Hirn und Harn,", "tokens": ["F\u00f6r\u00b7dert", "nicht", "nur", "Hirn", "und", "Harn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern auch die Muskelb\u00e4nder,", "tokens": ["Son\u00b7dern", "auch", "die", "Mus\u00b7kel\u00b7b\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unterleib und Oberlippe.", "tokens": ["Un\u00b7ter\u00b7leib", "und", "O\u00b7berl\u00b7ip\u00b7pe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Sollst, das H\u00fcftgelenk zu st\u00e4hlen,", "tokens": ["Sollst", ",", "das", "H\u00fcft\u00b7ge\u00b7lenk", "zu", "st\u00e4h\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich im Knickst\u00fctz ihm verm\u00e4hlen.", "tokens": ["Dich", "im", "Knicks\u00b7t\u00fctz", "ihm", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deutsches Weib, komm: Kippe, Kippe!", "tokens": ["Deut\u00b7sches", "Weib", ",", "komm", ":", "Kip\u00b7pe", ",", "Kip\u00b7pe", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$.", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deutsche Frau, nun la\u00df dich wieder", "tokens": ["Deut\u00b7sche", "Frau", ",", "nun", "la\u00df", "dich", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "VVIMP", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ellengriffs im Schwimmhang nieder.", "tokens": ["El\u00b7len\u00b7griffs", "im", "Schwimm\u00b7hang", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So, nun Hackenschlu\u00df! Und schwinge!", "tokens": ["So", ",", "nun", "Ha\u00b7cken\u00b7schlu\u00df", "!", "Und", "schwin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "$.", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwinge! Hurtig rum den Leib!", "tokens": ["Schwin\u00b7ge", "!", "Hur\u00b7tig", "rum", "den", "Leib", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O, es gibt noch wundervolle", "tokens": ["O", ",", "es", "gibt", "noch", "wun\u00b7der\u00b7vol\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dinge. Rolle vorw\u00e4rts! Rolle!", "tokens": ["Din\u00b7ge", ".", "Rol\u00b7le", "vor\u00b7w\u00e4rts", "!", "Rol\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "PTKVZ", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Rolle r\u00fcckw\u00e4rts, deutsches Weib.", "tokens": ["Rol\u00b7le", "r\u00fcck\u00b7w\u00e4rts", ",", "deut\u00b7sches", "Weib", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deutsche Jungfrau, weg das Armband!", "tokens": ["Deut\u00b7sche", "Jung\u00b7frau", ",", "weg", "das", "Arm\u00b7band", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Hose! Aus dem Rocke!", "tokens": ["In", "die", "Ho\u00b7se", "!", "Aus", "dem", "Ro\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Streckst\u00fctz in den Armstand,", "tokens": ["Aus", "dem", "Streck\u00b7st\u00fctz", "in", "den", "Arm\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun die Flanke. Sehr gut! Danke!", "tokens": ["Nun", "die", "Flan\u00b7ke", ".", "Sehr", "gut", "!", "Dan\u00b7ke", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "ADJD", "$.", "PTKANT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deutsches M\u00e4dchen, Hocke, Hocke!", "tokens": ["Deut\u00b7sches", "M\u00e4d\u00b7chen", ",", "Ho\u00b7cke", ",", "Ho\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mu\u00dft dich keck emanzipieren", "tokens": ["Mu\u00dft", "dich", "keck", "e\u00b7man\u00b7zi\u00b7pie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mit kindlichem \u00bb\u00c4tsch-\u00c4tsche\u00ab", "tokens": ["Und", "mit", "kind\u00b7li\u00b7chem", "\u00bb", "\u00c4tsch\u00c4t\u00b7sche", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "$(", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "\u00dcber M\u00e4nner triumphieren,", "tokens": ["\u00dc\u00b7ber", "M\u00e4n\u00b7ner", "tri\u00b7um\u00b7phie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00dft wie Bombe und Kart\u00e4tsche", "tokens": ["Mu\u00dft", "wie", "Bom\u00b7be", "und", "Kar\u00b7t\u00e4t\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Kr\u00e4fte demonstrieren.", "tokens": ["Dei\u00b7ne", "Kr\u00e4f\u00b7te", "de\u00b7monst\u00b7rie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutsches M\u00e4dchen \u2013 Gr\u00e4tsche! Gr\u00e4tsche!", "tokens": ["Deut\u00b7sches", "M\u00e4d\u00b7chen", "\u2013", "Gr\u00e4t\u00b7sche", "!", "Gr\u00e4t\u00b7sche", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}