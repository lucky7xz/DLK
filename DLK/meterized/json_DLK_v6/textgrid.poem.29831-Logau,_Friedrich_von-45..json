{"textgrid.poem.29831": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Duplex, der mit Pfeiffen handelt, f\u00fchret meistens schlimme Pfeiffen,", "tokens": ["Du\u00b7plex", ",", "der", "mit", "Pfeif\u00b7fen", "han\u00b7delt", ",", "f\u00fch\u00b7ret", "meis\u00b7tens", "schlim\u00b7me", "Pfeif\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Die ihm aber wol gehn abe; denn er kan sie selbsten greiffen.", "tokens": ["Die", "ihm", "a\u00b7ber", "wol", "gehn", "a\u00b7be", ";", "denn", "er", "kan", "sie", "selbs\u00b7ten", "greif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "PTKVZ", "$.", "KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Simplex handelt auch mit Pfeiffen, derer kein ihm abe gehet,", "tokens": ["Sim\u00b7plex", "han\u00b7delt", "auch", "mit", "Pfeif\u00b7fen", ",", "de\u00b7rer", "kein", "ihm", "a\u00b7be", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "$,", "PRELS", "PIAT", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ob sie gleich sind wol gebrochen, weil er pfeiffen nicht verstehet.", "tokens": ["Ob", "sie", "gleich", "sind", "wol", "ge\u00b7bro\u00b7chen", ",", "weil", "er", "pfeif\u00b7fen", "nicht", "ver\u00b7ste\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Gleiches ist doch nimmer eines; gleiche handeln, gleiche seyn,", "tokens": ["Glei\u00b7ches", "ist", "doch", "nim\u00b7mer", "ei\u00b7nes", ";", "glei\u00b7che", "han\u00b7deln", ",", "glei\u00b7che", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VAFIN", "ADV", "ADV", "PIS", "$.", "ADJA", "VVINF", "$,", "ADJA", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Gleiche seyn und gleiche handeln, trifft doch nimmer \u00fcber-ein.", "tokens": ["Glei\u00b7che", "seyn", "und", "glei\u00b7che", "han\u00b7deln", ",", "trifft", "doch", "nim\u00b7mer", "\u00fc\u00b7ber\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "KON", "ADJA", "VVINF", "$,", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}