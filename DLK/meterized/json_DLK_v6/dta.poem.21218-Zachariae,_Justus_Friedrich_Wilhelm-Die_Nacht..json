{"dta.poem.21218": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Nacht! und du, o feyerliche Stille!", "tokens": ["O", "Nacht", "!", "und", "du", ",", "o", "fey\u00b7er\u00b7li\u00b7che", "Stil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "KON", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Indem ich mich in eure Schatten h\u00fclle,", "tokens": ["In\u00b7dem", "ich", "mich", "in", "eu\u00b7re", "Schat\u00b7ten", "h\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fall ich hin in den Staub vor dem, der", "tokens": ["Fall", "ich", "hin", "in", "den", "Staub", "vor", "dem", ",", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "ART", "$,", "PRELS"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "mich gemacht.\nVon dieser Unterwelt Get\u00fcmmel\nHebt unser Herz nichts mehr zum Hunmel\nAls deine Majest\u00e4t, o Nacht!", "tokens": ["mich", "ge\u00b7macht", ".", "Von", "die\u00b7ser", "Un\u00b7ter\u00b7welt", "Ge\u00b7t\u00fcm\u00b7mel", "Hebt", "un\u00b7ser", "Herz", "nichts", "mehr", "zum", "Hun\u00b7mel", "Als", "dei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", ",", "o", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$.", "APPR", "PDAT", "NN", "NN", "VVFIN", "PPOSAT", "NN", "PIS", "ADV", "APPRART", "NN", "KOUS", "PPOSAT", "NN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-+-+----+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie k\u00f6mmt! Jhr helles Sternenkleid", "tokens": ["Sie", "k\u00f6mmt", "!", "Ihr", "hel\u00b7les", "Ster\u00b7nen\u00b7kleid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flie\u00dft \u00fcber ihren pr\u00e4chtgen Wagen,", "tokens": ["Flie\u00dft", "\u00fc\u00b7ber", "ih\u00b7ren", "pr\u00e4cht\u00b7gen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begeistert von der Macht der dunkeln Einsamkeit,", "tokens": ["Be\u00b7geis\u00b7tert", "von", "der", "Macht", "der", "dun\u00b7keln", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Steht ietzt der Christ, durch sie geweiht,", "tokens": ["Steht", "ietzt", "der", "Christ", ",", "durch", "sie", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und denket seine Sterblichkeit.", "tokens": ["Und", "den\u00b7ket", "sei\u00b7ne", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er h\u00f6rt die Todtenglocke schlagen,", "tokens": ["Er", "h\u00f6rt", "die", "Tod\u00b7ten\u00b7glo\u00b7cke", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Indem er unter Gr\u00e4bern irrt,", "tokens": ["In\u00b7dem", "er", "un\u00b7ter", "Gr\u00e4\u00b7bern", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und auf den Staub hinweint, der er auch werden", "tokens": ["Und", "auf", "den", "Staub", "hin\u00b7weint", ",", "der", "er", "auch", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADV", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch welcher Trost stralt in die bange Seele?", "tokens": ["Doch", "wel\u00b7cher", "Trost", "stralt", "in", "die", "ban\u00b7ge", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Umsonst schreckt ihn des Grabes dunkle H\u00f6hle;", "tokens": ["Um\u00b7sonst", "schreckt", "ihn", "des", "Gra\u00b7bes", "dunk\u00b7le", "H\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Von jedem Stern ruft ihm ein Engel zu,", "tokens": ["Von", "je\u00b7dem", "Stern", "ruft", "ihm", "ein", "En\u00b7gel", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df er unsterblich ist. Er schmeckt des Trostes", "tokens": ["Da\u00df", "er", "uns\u00b7terb\u00b7lich", "ist", ".", "Er", "schmeckt", "des", "Tros\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Weit hinter jener Nacht sieht er den Vorhang", "tokens": ["Weit", "hin\u00b7ter", "je\u00b7ner", "Nacht", "sieht", "er", "den", "Vor\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PDAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und Palmen, ihm bestimmt, und Seraphim ihm", "tokens": ["Und", "Pal\u00b7men", ",", "ihm", "be\u00b7stimmt", ",", "und", "Se\u00b7ra\u00b7phim", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PPER", "VVPP", "$,", "KON", "NE", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie wird des Grabes Nacht entweichen,", "tokens": ["Wie", "wird", "des", "Gra\u00b7bes", "Nacht", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn \u00fcber Schrecken, Graus und Leichen,", "tokens": ["Wenn", "\u00fc\u00b7ber", "Schre\u00b7cken", ",", "Graus", "und", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Christen ewger Morgen gl\u00e4nzt!", "tokens": ["Des", "Chris\u00b7ten", "ew\u00b7ger", "Mor\u00b7gen", "gl\u00e4nzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Auge wird den Finsternissen,", "tokens": ["Sein", "Au\u00b7ge", "wird", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Geist der Sterblichkeit entrissen;", "tokens": ["Sein", "Geist", "der", "Sterb\u00b7lich\u00b7keit", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Seligkeit ist rein, und unbegr\u00e4nzt.", "tokens": ["Und", "sei\u00b7ne", "Se\u00b7lig\u00b7keit", "ist", "rein", ",", "und", "un\u00b7be\u00b7gr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}