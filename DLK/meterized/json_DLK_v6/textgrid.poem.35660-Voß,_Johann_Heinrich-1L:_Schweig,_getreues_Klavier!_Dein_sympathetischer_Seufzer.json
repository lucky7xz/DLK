word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.84": {"text": "Und der W\u00e4chter der Stadt k\u00fcndet den nahenden Tag.", "tokens": ["Und", "der", "W\u00e4ch\u00b7ter", "der", "Stadt", "k\u00fcn\u00b7det", "den", "na\u00b7hen\u00b7den", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.85": {"text": "F\u00fchrt mich, o Morgentr\u00e4ume, zu Edens bl\u00fchenden Lauben,", "tokens": ["F\u00fchrt", "mich", ",", "o", "Mor\u00b7gen\u00b7tr\u00e4u\u00b7me", ",", "zu", "E\u00b7dens", "bl\u00fc\u00b7hen\u00b7den", "Lau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.86": {"text": "Wo die Ewigkeit uns fester und fester vereint!", "tokens": ["Wo", "die", "E\u00b7wig\u00b7keit", "uns", "fes\u00b7ter", "und", "fes\u00b7ter", "ver\u00b7eint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}