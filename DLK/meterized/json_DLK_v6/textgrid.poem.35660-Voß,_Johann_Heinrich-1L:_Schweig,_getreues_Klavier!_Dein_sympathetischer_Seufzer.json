{"textgrid.poem.35660": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schweig, getreues Klavier! Dein sympathetischer Seufzer", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweig, getreues Klavier! Dein sympathetischer Seufzer", "tokens": ["Schweig", ",", "ge\u00b7treu\u00b7es", "Kla\u00b7vier", "!", "Dein", "sym\u00b7pa\u00b7the\u00b7ti\u00b7scher", "Seuf\u00b7zer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Weckt den starren Gram, der mir die Seele zerrei\u00dft;", "tokens": ["Weckt", "den", "star\u00b7ren", "Gram", ",", "der", "mir", "die", "See\u00b7le", "zer\u00b7rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Wie der irrende d\u00fcstre Mond, der weinende Himmel,", "tokens": ["Wie", "der", "ir\u00b7ren\u00b7de", "d\u00fcst\u00b7re", "Mond", ",", "der", "wei\u00b7nen\u00b7de", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "VVFIN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und der Espe Ger\u00e4usch \u00fcber dem Grabe der Braut.", "tokens": ["Und", "der", "Es\u00b7pe", "Ge\u00b7r\u00e4usch", "\u00fc\u00b7ber", "dem", "Gra\u00b7be", "der", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "---+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Selbst am Busen des Freundes ist jetzt kein Trost! Mich entrei\u00dfen", "tokens": ["Selbst", "am", "Bu\u00b7sen", "des", "Freun\u00b7des", "ist", "jetzt", "kein", "Trost", "!", "Mich", "ent\u00b7rei\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VAFIN", "ADV", "PIAT", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Mu\u00dft' ich! Auch du, mein Hahn, lie\u00dfest mich trostlos entfliehn?", "tokens": ["Mu\u00dft'", "ich", "!", "Auch", "du", ",", "mein", "Hahn", ",", "lie\u00b7\u00dfest", "mich", "trost\u00b7los", "ent\u00b7fliehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$.", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Traurige, traurige Nacht! du schwarze Botin des Schicksals!", "tokens": ["Trau\u00b7ri\u00b7ge", ",", "trau\u00b7ri\u00b7ge", "Nacht", "!", "du", "schwar\u00b7ze", "Bo\u00b7tin", "des", "Schick\u00b7sals", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$.", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Deines Gerichts Last dr\u00fcckt, schwer wie ein Fels, mich herab!", "tokens": ["Dei\u00b7nes", "Ge\u00b7richts", "Last", "dr\u00fcckt", ",", "schwer", "wie", "ein", "Fels", ",", "mich", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,", "PPER", "ADV", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Drei auf einmal raubte dein Wink dem seligsten Bunde:", "tokens": ["Drei", "auf", "ein\u00b7mal", "raub\u00b7te", "dein", "Wink", "dem", "se\u00b7ligs\u00b7ten", "Bun\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.10": {"text": "Meine Stolberg', euch, z\u00e4rtlichster Clauswitz, und dich!", "tokens": ["Mei\u00b7ne", "Stol\u00b7ber\u00b7g'", ",", "euch", ",", "z\u00e4rt\u00b7lichs\u00b7ter", "Claus\u00b7witz", ",", "und", "dich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "$,", "ADJA", "NE", "$,", "KON", "PPER", "$."], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Ach! dem seligsten festesten Bund'! und drei auf einmal!", "tokens": ["Ach", "!", "dem", "se\u00b7ligs\u00b7ten", "fes\u00b7tes\u00b7ten", "Bund'", "!", "und", "drei", "auf", "ein\u00b7mal", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "ADJA", "NN", "$.", "KON", "CARD", "APPR", "ADV", "$."], "meter": "+--+-+--+-+--+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Und so schleunig! so fern! ach! und auf immer so fern!", "tokens": ["Und", "so", "schleu\u00b7nig", "!", "so", "fern", "!", "ach", "!", "und", "auf", "im\u00b7mer", "so", "fern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$.", "ADV", "ADJD", "$.", "XY", "$.", "KON", "APPR", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.13": {"text": "Und doch l\u00e4cheltest du, als dort mit dem Schimmer des Mondes", "tokens": ["Und", "doch", "l\u00e4\u00b7chel\u00b7test", "du", ",", "als", "dort", "mit", "dem", "Schim\u00b7mer", "des", "Mon\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Uns in der Eichen Graun heiliger Schauer ergriff,", "tokens": ["Uns", "in", "der", "Ei\u00b7chen", "Graun", "hei\u00b7li\u00b7ger", "Schau\u00b7er", "er\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Da\u00df wir dem Vaterlande, der Tugend und Freundschaft schwuren!", "tokens": ["Da\u00df", "wir", "dem", "Va\u00b7ter\u00b7lan\u00b7de", ",", "der", "Tu\u00b7gend", "und", "Freund\u00b7schaft", "schwu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Wahrlich! ein edler Schwur, nicht ungesegnet von Gott!", "tokens": ["Wahr\u00b7lich", "!", "ein", "ed\u00b7ler", "Schwur", ",", "nicht", "un\u00b7ge\u00b7seg\u00b7net", "von", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "ADJA", "NN", "$,", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Tretet heran, und zeugt mir's, ihr Tage des goldenen Alters,", "tokens": ["Tre\u00b7tet", "he\u00b7ran", ",", "und", "zeugt", "mir's", ",", "ihr", "Ta\u00b7ge", "des", "gol\u00b7de\u00b7nen", "Al\u00b7ters", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "NE", "$,", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Oft bei Liedern und Milch unter dem bl\u00fchenden Baum,", "tokens": ["Oft", "bei", "Lie\u00b7dern", "und", "Milch", "un\u00b7ter", "dem", "bl\u00fc\u00b7hen\u00b7den", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Oft in den Lauben des Sommers bei blinkenden Kirschen und Erdbeern,", "tokens": ["Oft", "in", "den", "Lau\u00b7ben", "des", "Som\u00b7mers", "bei", "blin\u00b7ken\u00b7den", "Kir\u00b7schen", "und", "Erd\u00b7beern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Und dem rheinischen geistfl\u00fcgelnden Nektar, gefei'rt!", "tokens": ["Und", "dem", "rhei\u00b7ni\u00b7schen", "ge\u00b7ist\u00b7fl\u00fc\u00b7geln\u00b7den", "Nek\u00b7tar", ",", "ge\u00b7fei'rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Zeug' es, o stille Nacht, die dem m\u00fchlenrauschenden Garten,", "tokens": ["Zeug'", "es", ",", "o", "stil\u00b7le", "Nacht", ",", "die", "dem", "m\u00fch\u00b7len\u00b7rau\u00b7schen\u00b7den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "ADJA", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Nach den Gluten des Tags, Schimmer und K\u00fchlungen troff,", "tokens": ["Nach", "den", "Glu\u00b7ten", "des", "Tags", ",", "Schim\u00b7mer", "und", "K\u00fch\u00b7lun\u00b7gen", "troff", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "Als durch Balsamviolen und rote Johannsbeerhecken,", "tokens": ["Als", "durch", "Bal\u00b7sam\u00b7vi\u00b7o\u00b7len", "und", "ro\u00b7te", "Jo\u00b7hanns\u00b7be\u00b7er\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Mit verschlungenem Arm, Weisheit und lachender Scherz,", "tokens": ["Mit", "ver\u00b7schlun\u00b7ge\u00b7nem", "Arm", ",", "Weis\u00b7heit", "und", "la\u00b7chen\u00b7der", "Scherz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "S\u00fc\u00df wie M\u00e4dchengefl\u00fcster in Nachtigallhainen, uns f\u00fchrte,", "tokens": ["S\u00fc\u00df", "wie", "M\u00e4d\u00b7chen\u00b7ge\u00b7fl\u00fcs\u00b7ter", "in", "Nach\u00b7ti\u00b7gall\u00b7hai\u00b7nen", ",", "uns", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPR", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Und ich leiser empfand, da\u00df ich dich, Selma, nie sah!", "tokens": ["Und", "ich", "lei\u00b7ser", "emp\u00b7fand", ",", "da\u00df", "ich", "dich", ",", "Sel\u00b7ma", ",", "nie", "sah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "PRF", "$,", "NE", "$,", "ADV", "VVFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Tritt heran, im lyrischen Tanz, mit der Palme Sionas", "tokens": ["Tritt", "he\u00b7ran", ",", "im", "ly\u00b7ri\u00b7schen", "Tanz", ",", "mit", "der", "Pal\u00b7me", "Si\u00b7o\u00b7nas"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NE"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Und Amaranthen umrauscht, Tag, der den S\u00e4nger gebar,", "tokens": ["Und", "A\u00b7ma\u00b7ran\u00b7then", "um\u00b7rauscht", ",", "Tag", ",", "der", "den", "S\u00e4n\u00b7ger", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Gottes und seines Messias und Deutschlands heiligen S\u00e4nger!", "tokens": ["Got\u00b7tes", "und", "sei\u00b7nes", "Mes\u00b7si\u00b7as", "und", "Deutschlands", "hei\u00b7li\u00b7gen", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "KON", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+-++--+-", "measure": "iambic.septa.invert"}, "line.30": {"text": "Br\u00fcder, und unsern Freund! kr\u00e4nzt euch mit Rosen das Haupt!", "tokens": ["Br\u00fc\u00b7der", ",", "und", "un\u00b7sern", "Freund", "!", "kr\u00e4nzt", "euch", "mit", "Ro\u00b7sen", "das", "Haupt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}, "line.31": {"text": "Kr\u00e4nzt der Freiheit und Tugend Altar! und Flammen und Dolche,", "tokens": ["Kr\u00e4nzt", "der", "Frei\u00b7heit", "und", "Tu\u00b7gend", "Al\u00b7tar", "!", "und", "Flam\u00b7men", "und", "Dol\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "NN", "$.", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Gegen den Frevler gez\u00fcckt, halle der Opfergesang!", "tokens": ["Ge\u00b7gen", "den", "Frev\u00b7ler", "ge\u00b7z\u00fcckt", ",", "hal\u00b7le", "der", "Op\u00b7fer\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.33": {"text": "Und \u2013 doch entflieht, Gestalten der seligen Abende! Nimmer", "tokens": ["Und", "\u2013", "doch", "ent\u00b7flieht", ",", "Ge\u00b7stal\u00b7ten", "der", "se\u00b7li\u00b7gen", "A\u00b7ben\u00b7de", "!", "Nim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "$(", "ADV", "VVFIN", "$,", "NN", "ART", "ADJA", "NN", "$.", "NE"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.34": {"text": "Schaut ihr die Frohen hinfort um den vertrauten Kamin!", "tokens": ["Schaut", "ihr", "die", "Fro\u00b7hen", "hin\u00b7fort", "um", "den", "ver\u00b7trau\u00b7ten", "Ka\u00b7min", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Still und schwermutsvoll traur't k\u00fcnftig die \u00f6de Versammlung;", "tokens": ["Still", "und", "schwer\u00b7muts\u00b7voll", "traur't", "k\u00fcnf\u00b7tig", "die", "\u00f6\u00b7de", "Ver\u00b7samm\u00b7lung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Furchtsam jeder, woher dieses Verstummen, zu sp\u00e4hn.", "tokens": ["Furcht\u00b7sam", "je\u00b7der", ",", "wo\u00b7her", "die\u00b7ses", "Ver\u00b7stum\u00b7men", ",", "zu", "sp\u00e4hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$,", "PWAV", "PDAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Und entsprie\u00dfen uns seltne Freuden, so sind es nur Blumen", "tokens": ["Und", "ent\u00b7sprie\u00b7\u00dfen", "uns", "selt\u00b7ne", "Freu\u00b7den", ",", "so", "sind", "es", "nur", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "NN"], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.38": {"text": "Von dem Grabe der Braut, an der Gespielinnen Brust.", "tokens": ["Von", "dem", "Gra\u00b7be", "der", "Braut", ",", "an", "der", "Ge\u00b7spie\u00b7lin\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.39": {"text": "O! wie trog uns die Hoffnung, du Stifterin unsers Bundes,", "tokens": ["O", "!", "wie", "trog", "uns", "die", "Hoff\u00b7nung", ",", "du", "Stif\u00b7te\u00b7rin", "un\u00b7sers", "Bun\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.40": {"text": "Zw\u00f6lfte Septembernacht, feierlich dich zu empfahn!", "tokens": ["Zw\u00f6lf\u00b7te", "Sep\u00b7tem\u00b7ber\u00b7nacht", ",", "fei\u00b7er\u00b7lich", "dich", "zu", "em\u00b7pfahn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.41": {"text": "Dich, die Scheitel umrauscht von der Eiche duftender Jugend,", "tokens": ["Dich", ",", "die", "Schei\u00b7tel", "um\u00b7rauscht", "von", "der", "Ei\u00b7che", "duf\u00b7ten\u00b7der", "Ju\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVPP", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Mit des stolzen Triumphs lautem Get\u00f6n zu empfahn!", "tokens": ["Mit", "des", "stol\u00b7zen", "Tri\u00b7umphs", "lau\u00b7tem", "Ge\u00b7t\u00f6n", "zu", "em\u00b7pfahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Feierlich bist du empfangen! Dir t\u00f6nte Millers Triumphlied:", "tokens": ["Fei\u00b7er\u00b7lich", "bist", "du", "emp\u00b7fan\u00b7gen", "!", "Dir", "t\u00f6n\u00b7te", "Mil\u00b7lers", "Tri\u00b7ump\u00b7hlied", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "NE", "NN", "$."], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.44": {"text": "\u00bb\u00fcber den Sternen vereint fester die Ewigkeit uns!\u00ab", "tokens": ["\u00bb", "\u00fc\u00b7ber", "den", "Ster\u00b7nen", "ver\u00b7eint", "fes\u00b7ter", "die", "E\u00b7wig\u00b7keit", "uns", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "PPER", "$.", "$("], "meter": "+--+--++--+-++", "measure": "dactylic.di.plus"}, "line.45": {"text": "Auch rauscht' Eichenger\u00e4usch, die Umarmung war hei\u00df, und Thr\u00e4nen", "tokens": ["Auch", "rauscht'", "Ei\u00b7chen\u00b7ge\u00b7r\u00e4usch", ",", "die", "Um\u00b7ar\u00b7mung", "war", "hei\u00df", ",", "und", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "NN"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.46": {"text": "Mischten zu Thr\u00e4nen sich! Thr\u00e4nen, der Freude gest\u00fcrzt!...", "tokens": ["Mischten", "zu", "Thr\u00e4\u00b7nen", "sich", "!", "Thr\u00e4\u00b7nen", ",", "der", "Freu\u00b7de", "ge\u00b7st\u00fcrzt", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "$.", "NN", "$,", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.47": {"text": "Miller! du bist mein Freund; doch du hast \u00fcbel gehandelt,", "tokens": ["Mil\u00b7ler", "!", "du", "bist", "mein", "Freund", ";", "doch", "du", "hast", "\u00fc\u00b7bel", "ge\u00b7han\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "KON", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.48": {"text": "Da\u00df du dein Taumellied wieder von neuem begannst!", "tokens": ["Da\u00df", "du", "dein", "Tau\u00b7mel\u00b7lied", "wie\u00b7der", "von", "neu\u00b7em", "be\u00b7gannst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.49": {"text": "Sahest du nicht, wie schnell mein Leopold mir um den Hals fiel?", "tokens": ["Sa\u00b7hest", "du", "nicht", ",", "wie", "schnell", "mein", "Leo\u00b7pold", "mir", "um", "den", "Hals", "fiel", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "ADJD", "PPOSAT", "NE", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Wie der leise Ton zittert' und stockt', und schwieg?", "tokens": ["Wie", "der", "lei\u00b7se", "Ton", "zit\u00b7tert'", "und", "stockt'", ",", "und", "schwieg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.51": {"text": "Dich, dich klaget es an, das schreckliche lange Verstummen!", "tokens": ["Dich", ",", "dich", "kla\u00b7get", "es", "an", ",", "das", "schreck\u00b7li\u00b7che", "lan\u00b7ge", "Ver\u00b7stum\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.52": {"text": "Dich der gebrochne Laut: Lieben, nun ist es Zeit!", "tokens": ["Dich", "der", "ge\u00b7broch\u00b7ne", "Laut", ":", "Lie\u00b7ben", ",", "nun", "ist", "es", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$.", "ADJA", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.53": {"text": "Dich der lautere Jammer, die Eile, das Streben, das Ringen,", "tokens": ["Dich", "der", "lau\u00b7te\u00b7re", "Jam\u00b7mer", ",", "die", "Ei\u00b7le", ",", "das", "Stre\u00b7ben", ",", "das", "Rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.54": {"text": "Und die mit schwimmendem Blick flehende, rei\u00dfende Flucht!", "tokens": ["Und", "die", "mit", "schwim\u00b7men\u00b7dem", "Blick", "fle\u00b7hen\u00b7de", ",", "rei\u00b7\u00dfen\u00b7de", "Flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.55": {"text": "Alle h\u00e4tt' ich noch einmal, wie Clauswitz, umarmt, und feurig,", "tokens": ["Al\u00b7le", "h\u00e4tt'", "ich", "noch", "ein\u00b7mal", ",", "wie", "Claus\u00b7witz", ",", "um\u00b7armt", ",", "und", "feu\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADV", "ADV", "$,", "PWAV", "NE", "$,", "VVPP", "$,", "KON", "ADJD", "$,"], "meter": "--+--+--+--+-+-", "measure": "anapaest.tetra.plus"}, "line.56": {"text": "Schnell sie gek\u00fc\u00dft, und fest, fest an das Herz mir gedr\u00fcckt!", "tokens": ["Schnell", "sie", "ge\u00b7k\u00fc\u00dft", ",", "und", "fest", ",", "fest", "an", "das", "Herz", "mir", "ge\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,", "KON", "PTKVZ", "$,", "ADJD", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.57": {"text": "Aber sie flohn! Bald stehn sie bet\u00e4ubt an Daniens Grenzen.", "tokens": ["A\u00b7ber", "sie", "flohn", "!", "Bald", "stehn", "sie", "be\u00b7t\u00e4ubt", "an", "Da\u00b7ni\u00b7ens", "Gren\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "VVPP", "APPR", "NE", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.58": {"text": "Schaun noch lange zur\u00fcck, weinen und fliegen hinweg!", "tokens": ["Schaun", "noch", "lan\u00b7ge", "zu\u00b7r\u00fcck", ",", "wei\u00b7nen", "und", "flie\u00b7gen", "hin\u00b7weg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "$,", "VVINF", "KON", "VVFIN", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.59": {"text": "Und so entfliegen sie alle, vom schicksalschwangeren Wetter", "tokens": ["Und", "so", "ent\u00b7flie\u00b7gen", "sie", "al\u00b7le", ",", "vom", "schick\u00b7sal\u00b7schwan\u00b7ge\u00b7ren", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.60": {"text": "Hiehin und dorthin, wie Spreu, unter die Himmel gest\u00fcrmt!", "tokens": ["Hie\u00b7hin", "und", "dor\u00b7thin", ",", "wie", "Spreu", ",", "un\u00b7ter", "die", "Him\u00b7mel", "ge\u00b7st\u00fcrmt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,", "PWAV", "ADJD", "$,", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.61": {"text": "Wende den thr\u00e4nenden Blick, mein Esmarch! er thr\u00e4nt nicht um Stolberg!", "tokens": ["Wen\u00b7de", "den", "thr\u00e4\u00b7nen\u00b7den", "Blick", ",", "mein", "Es\u00b7march", "!", "er", "thr\u00e4nt", "nicht", "um", "Stol\u00b7berg", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "NE", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.62": {"text": "Nur sechs Abende noch dr\u00fcckst du mir sprachlos die Hand!", "tokens": ["Nur", "sechs", "A\u00b7ben\u00b7de", "noch", "dr\u00fcckst", "du", "mir", "sprach\u00b7los", "die", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADV", "VVFIN", "PPER", "PPER", "ADJD", "ART", "NN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.63": {"text": "H\u00f6lty, du z\u00f6gerst hier, des Liebenden \u00e4ngstliches Z\u00f6gern!", "tokens": ["H\u00f6l\u00b7ty", ",", "du", "z\u00f6\u00b7gerst", "hier", ",", "des", "Lie\u00b7ben\u00b7den", "\u00e4ngst\u00b7li\u00b7ches", "Z\u00f6\u00b7gern", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.64": {"text": "Ach! du lauschest nicht mehr Nachtigallt\u00f6nen mit uns,", "tokens": ["Ach", "!", "du", "lau\u00b7schest", "nicht", "mehr", "Nach\u00b7ti\u00b7gall\u00b7t\u00f6\u00b7nen", "mit", "uns", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PTKNEG", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.65": {"text": "Angeblinkt vom gr\u00fcnlichen Schimmer der purpurnen Sonne", "tokens": ["An\u00b7ge\u00b7blinkt", "vom", "gr\u00fcn\u00b7li\u00b7chen", "Schim\u00b7mer", "der", "pur\u00b7pur\u00b7nen", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.66": {"text": "Hinter den Saaten! Der Lenz raubt dich und Cramer und Hahn!", "tokens": ["Hin\u00b7ter", "den", "Saa\u00b7ten", "!", "Der", "Lenz", "raubt", "dich", "und", "Cra\u00b7mer", "und", "Hahn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.67": {"text": "Dann noch ein banger Sommer voll Sehnsuchtsthr\u00e4nen; und alle,", "tokens": ["Dann", "noch", "ein", "ban\u00b7ger", "Som\u00b7mer", "voll", "Sehn\u00b7suchtst\u00b7hr\u00e4\u00b7nen", ";", "und", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "ADJD", "NN", "$.", "KON", "PIS", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.68": {"text": "Alle sind ferne von mir, tr\u00f6sten durch Briefe den Freund.", "tokens": ["Al\u00b7le", "sind", "fer\u00b7ne", "von", "mir", ",", "tr\u00f6s\u00b7ten", "durch", "Brie\u00b7fe", "den", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "APPR", "PPER", "$,", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.69": {"text": "Eile, mein Boie, von Albions Flur! Was entweihest du Klopstocks", "tokens": ["Ei\u00b7le", ",", "mein", "Boi\u00b7e", ",", "von", "Al\u00b7bions", "Flur", "!", "Was", "ent\u00b7wei\u00b7hest", "du", "Klops\u00b7tocks"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "NE", "NN", "$.", "PWS", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.70": {"text": "Psalter vor jenen, die noch Ossians Harfe verschm\u00e4hn?", "tokens": ["Psal\u00b7ter", "vor", "je\u00b7nen", ",", "die", "noch", "Os\u00b7si\u00b7ans", "Har\u00b7fe", "ver\u00b7schm\u00e4hn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "$,", "PRELS", "ADV", "NE", "NN", "VVINF", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.71": {"text": "B\u00fcrger, ich komme nicht mehr von lachenden Freunden begleitet,", "tokens": ["B\u00fcr\u00b7ger", ",", "ich", "kom\u00b7me", "nicht", "mehr", "von", "la\u00b7chen\u00b7den", "Freun\u00b7den", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.72": {"text": "Einsam komm' ich und still unter dein l\u00e4ndliches Dach.", "tokens": ["Ein\u00b7sam", "komm'", "ich", "und", "still", "un\u00b7ter", "dein", "l\u00e4nd\u00b7li\u00b7ches", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.73": {"text": "Sprich mir, du liebst sie ja auch, mein B\u00fcrger! von unsern Geliebten!", "tokens": ["Sprich", "mir", ",", "du", "liebst", "sie", "ja", "auch", ",", "mein", "B\u00fcr\u00b7ger", "!", "von", "un\u00b7sern", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.74": {"text": "Oder ich halt' es nicht aus, eile zu Br\u00fcckner zur\u00fcck!", "tokens": ["O\u00b7der", "ich", "halt'", "es", "nicht", "aus", ",", "ei\u00b7le", "zu", "Br\u00fcck\u00b7ner", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.75": {"text": "Trostlos wank' ich Verla\u00dfner um jed' einst selige St\u00e4tte,", "tokens": ["Trost\u00b7los", "wank'", "ich", "Ver\u00b7la\u00df\u00b7ner", "um", "jed'", "einst", "se\u00b7li\u00b7ge", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "APPR", "PIS", "ADV", "ADJA", "NN", "$,"], "meter": "--++-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.76": {"text": "Setze mich thr\u00e4nenvoll, wo mein Geliebter einst sa\u00df,", "tokens": ["Set\u00b7ze", "mich", "thr\u00e4\u00b7nen\u00b7voll", ",", "wo", "mein", "Ge\u00b7lieb\u00b7ter", "einst", "sa\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.77": {"text": "Klag' in wehender Nacht an der herbstlichen Eiche des Bundes:", "tokens": ["Klag'", "in", "we\u00b7hen\u00b7der", "Nacht", "an", "der", "herbst\u00b7li\u00b7chen", "Ei\u00b7che", "des", "Bun\u00b7des", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.78": {"text": "Ach! dort stand der Mond! Dort entbrannte der Stern!", "tokens": ["Ach", "!", "dort", "stand", "der", "Mond", "!", "Dort", "ent\u00b7brann\u00b7te", "der", "Stern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.79": {"text": "Hier umarmten wir uns und jauchzten! 