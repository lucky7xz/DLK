{"textgrid.poem.26753": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Majest\u00e4t der Menschennatur! Dich soll ich beim Haufen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Majest\u00e4t der Menschennatur! Dich soll ich beim Haufen", "tokens": ["Ma\u00b7jes\u00b7t\u00e4t", "der", "Men\u00b7schen\u00b7na\u00b7tur", "!", "Dich", "soll", "ich", "beim", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VMFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Suchen? Bei wenigen nur hast du von jeher gewohnt.", "tokens": ["Su\u00b7chen", "?", "Bei", "we\u00b7ni\u00b7gen", "nur", "hast", "du", "von", "je\u00b7her", "ge\u00b7wohnt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PIAT", "ADV", "VAFIN", "PPER", "APPR", "ADV", "VVPP", "$."], "meter": "+----+-+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Einzelne wenige z\u00e4hlen, die \u00fcbrigen alle sind blinde", "tokens": ["Ein\u00b7zel\u00b7ne", "we\u00b7ni\u00b7ge", "z\u00e4h\u00b7len", ",", "die", "\u00fcb\u00b7ri\u00b7gen", "al\u00b7le", "sind", "blin\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "PIS", "VVINF", "$,", "ART", "ADJA", "PIS", "VAFIN", "ADJA"], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nieten, ihr leeres Gew\u00fchl h\u00fcllet die Treffer nur ein.", "tokens": ["Nie\u00b7ten", ",", "ihr", "lee\u00b7res", "Ge\u00b7w\u00fchl", "h\u00fcl\u00b7let", "die", "Tref\u00b7fer", "nur", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "