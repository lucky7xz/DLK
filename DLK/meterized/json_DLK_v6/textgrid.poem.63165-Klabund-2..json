{"textgrid.poem.63165": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6tet diesen gottverdammten Schwafler Kant", "tokens": ["T\u00f6\u00b7tet", "die\u00b7sen", "gott\u00b7ver\u00b7damm\u00b7ten", "Schwaf\u00b7ler", "Kant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und Nietzschke und Trietschke,", "tokens": ["Und", "Nietz\u00b7schke", "und", "Trietschke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Weissgardisten,", "tokens": ["Die", "Weiss\u00b7gar\u00b7dis\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die Sch .... gardisten,", "tokens": ["Die", "Sch", "....", "gar\u00b7dis\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Dr\u00fcckeberger", "tokens": ["Die", "Dr\u00fc\u00b7cke\u00b7ber\u00b7ger"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Vom Traum der Tat.", "tokens": ["Vom", "Traum", "der", "Tat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wir r\u00fccken an", "tokens": ["Wir", "r\u00fc\u00b7cken", "an"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wir ewiger Wanderer durch die L\u00e4nder der tausend Seen.", "tokens": ["Wir", "e\u00b7wi\u00b7ger", "Wan\u00b7de\u00b7rer", "durch", "die", "L\u00e4n\u00b7der", "der", "tau\u00b7send", "Seen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "NN", "ART", "CARD", "NN", "$."], "meter": "-+--+--+-+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Unsre Augen sind feucht noch vom Tau des Himalaja,", "tokens": ["Uns\u00b7re", "Au\u00b7gen", "sind", "feucht", "noch", "vom", "Tau", "des", "Hi\u00b7ma\u00b7la\u00b7ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+-+--+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Unter unsern Fingern\u00e4geln", "tokens": ["Un\u00b7ter", "un\u00b7sern", "Fin\u00b7ger\u00b7n\u00e4\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Brennt noch die schmutzige Erde Afrikas.", "tokens": ["Brennt", "noch", "die", "schmut\u00b7zi\u00b7ge", "Er\u00b7de", "Af\u00b7ri\u00b7kas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Unser Herz trommelt an die Rippen", "tokens": ["Un\u00b7ser", "Herz", "trom\u00b7melt", "an", "die", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Die Melodie der Negertrommeln.", "tokens": ["Die", "Me\u00b7lo\u00b7die", "der", "Ne\u00b7ger\u00b7trom\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wir wissen aus und ein", "tokens": ["Wir", "wis\u00b7sen", "aus", "und", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und ins Dickicht des Urwalds.", "tokens": ["Und", "ins", "Di\u00b7ckicht", "des", "Ur\u00b7walds", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}