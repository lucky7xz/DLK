{"dta.poem.16757": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "116.  Die beste Gesellfchafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gesellschafft acht\u2019 ich nicht: Es sey dann da\u00df das Kind/", "tokens": ["Ge\u00b7sell\u00b7schafft", "acht'", "ich", "nicht", ":", "Es", "sey", "dann", "da\u00df", "das", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "ADV", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Jungfrau/ und die Daub\u2019/ und\u2019 s Lam\u0304 beysam\u0304en sind.", "tokens": ["Die", "Jung\u00b7frau", "/", "und", "die", "Daub'", "/", "und'", "s", "Lam\u0304", "bey\u00b7sam\u0304en", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "ART", "NN", "$(", "KON", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}