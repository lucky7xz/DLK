{"textgrid.poem.42216": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Der Tantenm\u00f6rder", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab meine Tante geschlachtet,", "tokens": ["Ich", "hab", "mei\u00b7ne", "Tan\u00b7te", "ge\u00b7schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Meine Tante war alt und schwach;", "tokens": ["Mei\u00b7ne", "Tan\u00b7te", "war", "alt", "und", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ich hatte bei ihr \u00fcbernachtet", "tokens": ["Ich", "hat\u00b7te", "bei", "ihr", "\u00fc\u00b7bern\u00b7ach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und grub in den Kisten-Kasten nach.", "tokens": ["Und", "grub", "in", "den", "Kis\u00b7ten\u00b7Kas\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da fand ich goldene Haufen,", "tokens": ["Da", "fand", "ich", "gol\u00b7de\u00b7ne", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fand auch an Papieren gar viel", "tokens": ["Fand", "auch", "an", "Pa\u00b7pie\u00b7ren", "gar", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ADV", "PIAT"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und h\u00f6rte die alte Tante schnaufen", "tokens": ["Und", "h\u00f6r\u00b7te", "die", "al\u00b7te", "Tan\u00b7te", "schnau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ohn Mitleid und Zartgef\u00fchl.", "tokens": ["Ohn", "Mit\u00b7leid", "und", "Zart\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Was nutzt es, da\u00df sie sich noch h\u00e4rme \u2013", "tokens": ["Was", "nutzt", "es", ",", "da\u00df", "sie", "sich", "noch", "h\u00e4r\u00b7me", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nacht war es rings um mich her \u2013", "tokens": ["Nacht", "war", "es", "rings", "um", "mich", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPR", "PRF", "APZR", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ich stie\u00df ihr den Dolch in die D\u00e4rme,", "tokens": ["Ich", "stie\u00df", "ihr", "den", "Dolch", "in", "die", "D\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Tante schnaufte nicht mehr.", "tokens": ["Die", "Tan\u00b7te", "schnauf\u00b7te", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Das Geld war schwer zu tragen,", "tokens": ["Das", "Geld", "war", "schwer", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel schwerer die Tante noch.", "tokens": ["Viel", "schwe\u00b7rer", "die", "Tan\u00b7te", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich fa\u00dfte sie bebend am Kragen", "tokens": ["Ich", "fa\u00df\u00b7te", "sie", "be\u00b7bend", "am", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und stie\u00df sie ins tiefe Kellerloch. \u2013", "tokens": ["Und", "stie\u00df", "sie", "ins", "tie\u00b7fe", "Kel\u00b7ler\u00b7loch", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich hab meine Tante geschlachtet,", "tokens": ["Ich", "hab", "mei\u00b7ne", "Tan\u00b7te", "ge\u00b7schlach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Meine Tante war alt und schwach;", "tokens": ["Mei\u00b7ne", "Tan\u00b7te", "war", "alt", "und", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ihr aber, o Richter, ihr trachtet", "tokens": ["Ihr", "a\u00b7ber", ",", "o", "Rich\u00b7ter", ",", "ihr", "trach\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "PPER", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Meiner bl\u00fchenden Jugend-Jugend nach.", "tokens": ["Mei\u00b7ner", "bl\u00fc\u00b7hen\u00b7den", "Ju\u00b7gend\u00b7Ju\u00b7gend", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}