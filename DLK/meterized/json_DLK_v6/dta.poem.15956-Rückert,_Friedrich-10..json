{"dta.poem.15956": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edir scheinet heute dis, und jenes scheint dir morgen;", "tokens": ["\u201e", "dir", "schei\u00b7net", "heu\u00b7te", "dis", ",", "und", "je\u00b7nes", "scheint", "dir", "mor\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PDS", "$,", "KON", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Wahre, wie es scheint, bleibt immer dir verborgen.\u201c", "tokens": ["Das", "Wah\u00b7re", ",", "wie", "es", "scheint", ",", "bleibt", "im\u00b7mer", "dir", "ver\u00b7bor\u00b7gen", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O nein, bald seh' ich den, bald seh' ich jenen Glanz;", "tokens": ["O", "nein", ",", "bald", "seh'", "ich", "den", ",", "bald", "seh'", "ich", "je\u00b7nen", "Glanz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "ART", "$,", "ADV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das vielgetheilte Licht wird nur im Wechsel ganz.", "tokens": ["Das", "viel\u00b7ge\u00b7theil\u00b7te", "Licht", "wird", "nur", "im", "Wech\u00b7sel", "ganz",