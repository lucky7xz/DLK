{"textgrid.poem.31394": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du mit Wei\u00dfheits Safft die Sternen kanst befeuchten /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mit Wei\u00dfheits Safft die Sternen kanst befeuchten /", "tokens": ["Der", "du", "mit", "Wei\u00df\u00b7heits", "Safft", "die", "Ster\u00b7nen", "kanst", "be\u00b7feuch\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "daraus das Schicksel wird; zu zeiten ohn ihr Werk", "tokens": ["da\u00b7raus", "das", "Schick\u00b7sel", "wird", ";", "zu", "zei\u00b7ten", "ohn", "ihr", "Werk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VAFIN", "$.", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein Kunst begebnu\u00df spielst / zu zeigen deine St\u00e4rk /", "tokens": ["ein", "Kunst", "be\u00b7geb\u00b7nu\u00df", "spielst", "/", "zu", "zei\u00b7gen", "dei\u00b7ne", "St\u00e4rk", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "die aller Himmel Kr\u00e4fft in h\u00f6chster Demut scheuchten!", "tokens": ["die", "al\u00b7ler", "Him\u00b7mel", "Kr\u00e4fft", "in", "h\u00f6chs\u00b7ter", "De\u00b7mut", "scheuch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es pflegt dein herrschungs-Stab von Recht und g\u00fct zuleuchten.", "tokens": ["Es", "pflegt", "dein", "herr\u00b7schungs\u00b7Stab", "von", "Recht", "und", "g\u00fct", "zu\u00b7leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit wunder einverm\u00e4ngt die vorsicht ich vermerck /", "tokens": ["Mit", "wun\u00b7der", "ein\u00b7ver\u00b7m\u00e4ngt", "die", "vor\u00b7sicht", "ich", "ver\u00b7merck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "vom h\u00f6chsten Welt-gesch\u00f6pf bi\u00df auf die ringe sp\u00f6rk.", "tokens": ["vom", "h\u00f6chs\u00b7ten", "Welt\u00b7ge\u00b7sch\u00f6pf", "bi\u00df", "auf", "die", "rin\u00b7ge", "sp\u00f6rk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der Engel feur-verst\u00e4nd die lieb-sorg nicht erreichten.", "tokens": ["der", "En\u00b7gel", "feur\u00b7ver\u00b7st\u00e4nd", "die", "lie\u00b7bsorg", "nicht", "er\u00b7reich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du spinnst ein Gl\u00fckks-Geweb mit tausend F\u00e4den an:", "tokens": ["Du", "spinnst", "ein", "Gl\u00fc\u00b7kks\u00b7Ge\u00b7web", "mit", "tau\u00b7send", "F\u00e4\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "durch alle Sternen Krei\u00df / durch alle Ort der Erden", "tokens": ["durch", "al\u00b7le", "Ster\u00b7nen", "Krei\u00df", "/", "durch", "al\u00b7le", "Ort", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "$(", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "mu\u00df Werkzeug zu dem thun / da\u00df du beginnst / bracht werden.", "tokens": ["mu\u00df", "Werk\u00b7zeug", "zu", "dem", "thun", "/", "da\u00df", "du", "be\u00b7ginnst", "/", "bracht", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "ART", "VVINF", "$(", "KOUS", "PPER", "VVFIN", "$(", "VVFIN", "VAINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Dein' Allverschaffungs Krafft macht \u00fcberall die Bahn.", "tokens": ["Dein'", "All\u00b7ver\u00b7schaf\u00b7fungs", "Krafft", "macht", "\u00fc\u00b7be\u00b7rall", "die", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "ziehst du nur diese Schnur / dran alle Herzen hangen /", "tokens": ["ziehst", "du", "nur", "die\u00b7se", "Schnur", "/", "dran", "al\u00b7le", "Her\u00b7zen", "han\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PDAT", "NN", "$(", "PAV", "PIAT", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.14": {"text": "so ist der Sinn-Entwurf schon in das That-seyn gangen.", "tokens": ["so", "ist", "der", "Sinn\u00b7Ent\u00b7wurf", "schon", "in", "das", "That\u00b7seyn", "gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}