{"textgrid.poem.55789": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war im Palmenhaus; die feuchte Luft,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war im Palmenhaus; die feuchte Luft,", "tokens": ["Es", "war", "im", "Pal\u00b7men\u00b7haus", ";", "die", "feuch\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Blumend\u00fcnsten schwer, umspielte laulich", "tokens": ["Von", "Blu\u00b7men\u00b7d\u00fcns\u00b7ten", "schwer", ",", "um\u00b7spiel\u00b7te", "lau\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADJD", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In weichen Wellen unser beider Haupt.", "tokens": ["In", "wei\u00b7chen", "Wel\u00b7len", "un\u00b7ser", "bei\u00b7der", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In eine tiefgebauchte, k\u00fchle Gartenbank", "tokens": ["In", "ei\u00b7ne", "tief\u00b7ge\u00b7bauch\u00b7te", ",", "k\u00fch\u00b7le", "Gar\u00b7ten\u00b7bank"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zur\u00fcckgelehnt, so sa\u00dfen wir, ganz still.", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7lehnt", ",", "so", "sa\u00b7\u00dfen", "wir", ",", "ganz", "still", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Verklungen l\u00e4ngst war Wort und Gegenwort,", "tokens": ["Ver\u00b7klun\u00b7gen", "l\u00e4ngst", "war", "Wort", "und", "Ge\u00b7gen\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wir waren beide m\u00fcd, und reglos starrten", "tokens": ["Wir", "wa\u00b7ren", "bei\u00b7de", "m\u00fcd", ",", "und", "reg\u00b7los", "starr\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wir durch der W\u00e4nde spiegelklare Scheiben", "tokens": ["Wir", "durch", "der", "W\u00e4n\u00b7de", "spie\u00b7gel\u00b7kla\u00b7re", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Tief in des Himmels safrangelben Glanz.", "tokens": ["Tief", "in", "des", "Him\u00b7mels", "saf\u00b7ran\u00b7gel\u00b7ben", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Von Zeit zu Zeit, wenn abendk\u00fchl ein Windhauch", "tokens": ["Von", "Zeit", "zu", "Zeit", ",", "wenn", "a\u00b7bend\u00b7k\u00fchl", "ein", "Wind\u00b7hauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Um unsere hei\u00dfen Schl\u00e4fen strich, erklang", "tokens": ["Um", "un\u00b7se\u00b7re", "hei\u00b7\u00dfen", "Schl\u00e4\u00b7fen", "strich", ",", "er\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADJD", "$,", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ged\u00e4mpft und mild durch weitgespreizte Fenster", "tokens": ["Ge\u00b7d\u00e4mpft", "und", "mild", "durch", "weit\u00b7ge\u00b7spreiz\u00b7te", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Das Schluchzen der Fontainen aus dem Garten,", "tokens": ["Das", "Schluch\u00b7zen", "der", "Fon\u00b7tai\u00b7nen", "aus", "dem", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und leise rauschten dann die F\u00e4cherpalmen,", "tokens": ["Und", "lei\u00b7se", "rauschten", "dann", "die", "F\u00e4\u00b7cher\u00b7pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und Asiens wunderliche Riesenblumen,", "tokens": ["Und", "A\u00b7siens", "wun\u00b7der\u00b7li\u00b7che", "Rie\u00b7sen\u00b7blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Von dunkelgr\u00fcnem, sattem Laub umspielt,", "tokens": ["Von", "dun\u00b7kel\u00b7gr\u00fc\u00b7nem", ",", "sat\u00b7tem", "Laub", "um\u00b7spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sie nickten langsam, wie Pagodenh\u00e4upter,", "tokens": ["Sie", "nick\u00b7ten", "lang\u00b7sam", ",", "wie", "Pa\u00b7go\u00b7den\u00b7h\u00e4up\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und schwergew\u00fcrzte Glutarome rannen", "tokens": ["Und", "schwer\u00b7ge\u00b7w\u00fcrz\u00b7te", "Glu\u00b7ta\u00b7ro\u00b7me", "ran\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "In die europam\u00fcden Schw\u00e4rmerseelen ...", "tokens": ["In", "die", "eu\u00b7ro\u00b7pa\u00b7m\u00fc\u00b7den", "Schw\u00e4r\u00b7mer\u00b7see\u00b7len", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Das Haupt an's Haupt gelehnt und Hand in Hand,", "tokens": ["Das", "Haupt", "an's", "Haupt", "ge\u00b7lehnt", "und", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Mit heimwehkranker Seele tr\u00e4umten wir", "tokens": ["Mit", "heim\u00b7weh\u00b7kran\u00b7ker", "See\u00b7le", "tr\u00e4um\u00b7ten", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Von einer fernen S\u00fcdseeinsel Strand,", "tokens": ["Von", "ei\u00b7ner", "fer\u00b7nen", "S\u00fcd\u00b7see\u00b7in\u00b7sel", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Wo reicher die Natur und farbenhei\u00dfer,", "tokens": ["Wo", "rei\u00b7cher", "die", "Na\u00b7tur", "und", "far\u00b7ben\u00b7hei\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Wo lilasilbern Meereswogen leuchten", "tokens": ["Wo", "li\u00b7la\u00b7sil\u00b7bern", "Mee\u00b7res\u00b7wo\u00b7gen", "leuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "In winddurchkoster, schw\u00fcler Tropennacht,", "tokens": ["In", "wind\u00b7durch\u00b7kos\u00b7ter", ",", "schw\u00fc\u00b7ler", "Tro\u00b7pen\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Wo still und tr\u00e4umerisch und sinnlich-mild,", "tokens": ["Wo", "still", "und", "tr\u00e4u\u00b7me\u00b7risch", "und", "sinn\u00b7lich\u00b7mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Das Leben weiterflie\u00dft, wo keine Schranken", "tokens": ["Das", "Le\u00b7ben", "wei\u00b7ter\u00b7flie\u00dft", ",", "wo", "kei\u00b7ne", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Des Herzens tr\u00e4umerisch-bizarre W\u00fcnsche", "tokens": ["Des", "Her\u00b7zens", "tr\u00e4u\u00b7me\u00b7rischbi\u00b7zar\u00b7re", "W\u00fcn\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Stumpfsinnig-k\u00fchl verneinen und zerst\u00f6ren.", "tokens": ["Stumpf\u00b7sin\u00b7nig\u00b7k\u00fchl", "ver\u00b7nei\u00b7nen", "und", "zer\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.31": {"text": "Wo bist du, meine ferne S\u00fcdseeinsel?", "tokens": ["Wo", "bist", "du", ",", "mei\u00b7ne", "fer\u00b7ne", "S\u00fcd\u00b7see\u00b7in\u00b7sel", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}