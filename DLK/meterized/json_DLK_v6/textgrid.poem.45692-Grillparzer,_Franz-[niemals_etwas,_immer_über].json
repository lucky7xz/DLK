{"textgrid.poem.45692": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[niemals etwas, immer \u00fcber]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemals etwas, immer \u00fcber,", "tokens": ["Nie\u00b7mals", "et\u00b7was", ",", "im\u00b7mer", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "ADV", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber etwas schreib, mein Lieber!", "tokens": ["\u00dc\u00b7ber", "et\u00b7was", "schreib", ",", "mein", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So kommt Eignes zur Entfaltung,", "tokens": ["So", "kommt", "Eig\u00b7nes", "zur", "Ent\u00b7fal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Fremde gibt die Haltung.", "tokens": ["Und", "das", "Frem\u00b7de", "gibt", "die", "Hal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}