{"textgrid.poem.57357": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst verj\u00fcngte mein Alter, durchrann, wie der tr\u00e4nkende Bach rint", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst verj\u00fcngte mein Alter, durchrann, wie der tr\u00e4nkende Bach rint", "tokens": ["Einst", "ver\u00b7j\u00fcng\u00b7te", "mein", "Al\u00b7ter", ",", "durch\u00b7rann", ",", "wie", "der", "tr\u00e4n\u00b7ken\u00b7de", "Bach", "rint"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "VMFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+--+--++-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Durch die Wiese, mein Herz, machte den Heiteren froh,", "tokens": ["Durch", "die", "Wie\u00b7se", ",", "mein", "Herz", ",", "mach\u00b7te", "den", "Hei\u00b7te\u00b7ren", "froh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "War mir Wonne, zauberte mich in Segensgefilde,", "tokens": ["War", "mir", "Won\u00b7ne", ",", "zau\u00b7ber\u00b7te", "mich", "in", "Se\u00b7gens\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$,", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wo die Pflugschaar nur blinkte, kein furchendes Schwert;", "tokens": ["Wo", "die", "Pflug\u00b7schaar", "nur", "blink\u00b7te", ",", "kein", "fur\u00b7chen\u00b7des", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Wo der Wolke Donner nur scholl, dem labendes Tr\u00e4ufeln", "tokens": ["Wo", "der", "Wol\u00b7ke", "Don\u00b7ner", "nur", "scholl", ",", "dem", "la\u00b7ben\u00b7des", "Tr\u00e4u\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Folgte, des Eisens nicht scholl, welchem t\u00f6dtliches folgt.", "tokens": ["Folg\u00b7te", ",", "des", "Ei\u00b7sens", "nicht", "scholl", ",", "wel\u00b7chem", "t\u00f6dt\u00b7li\u00b7ches", "folgt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PTKNEG", "ADJD", "$,", "PWAT", "ADJA", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "Aber das Eine verj\u00fcngt mich nicht mehr, ich empfinde das Alter,", "tokens": ["A\u00b7ber", "das", "Ei\u00b7ne", "ver\u00b7j\u00fcngt", "mich", "nicht", "mehr", ",", "ich", "emp\u00b7fin\u00b7de", "das", "Al\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "All mein Frohes, ach meine Wonn' ist dahin!", "tokens": ["All", "mein", "Fro\u00b7hes", ",", "ach", "mei\u00b7ne", "Wonn'", "ist", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "$,", "XY", "PPOSAT", "NN", "VAFIN", "PAV", "$."], "meter": "+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.9": {"text": "Denn die Freyheit ist in den Himmel wiedergekehret!", "tokens": ["Denn", "die", "Frey\u00b7heit", "ist", "in", "den", "Him\u00b7mel", "wie\u00b7der\u00b7ge\u00b7keh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Oder s\u00e4umet vielleicht in dem Gew\u00f6lke sie noch?", "tokens": ["O\u00b7der", "s\u00e4u\u00b7met", "viel\u00b7leicht", "in", "dem", "Ge\u00b7w\u00f6l\u00b7ke", "sie", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "PPER", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Sehet ihr sie noch? Mir ist die G\u00f6ttin verschwunden!", "tokens": ["Se\u00b7het", "ihr", "sie", "noch", "?", "Mir", "ist", "die", "G\u00f6t\u00b7tin", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Aber verschwunden ist mir ihre Verfolgerin nicht!", "tokens": ["A\u00b7ber", "ver\u00b7schwun\u00b7den", "ist", "mir", "ih\u00b7re", "Ver\u00b7fol\u00b7ge\u00b7rin", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Ha die Alekto (Ungesetz ist ihr schrecklicher Name)", "tokens": ["Ha", "die", "A\u00b7lek\u00b7to", "(", "Un\u00b7ge\u00b7setz", "ist", "ihr", "schreck\u00b7li\u00b7cher", "Na\u00b7me", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "$(", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.14": {"text": "Wird nun heimisch bey euch, zischt mit den Schlangen umher!", "tokens": ["Wird", "nun", "hei\u00b7misch", "bey", "euch", ",", "zischt", "mit", "den", "Schlan\u00b7gen", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Sch\u00fcttelt die Todesfackel! Sie nimt oft Menschengestalt an,", "tokens": ["Sch\u00fct\u00b7telt", "die", "To\u00b7des\u00b7fa\u00b7ckel", "!", "Sie", "nimt", "oft", "Men\u00b7schen\u00b7ge\u00b7stalt", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Sitzt im Senat; doch gelingt ihre Verwandlung ihr nicht.", "tokens": ["Sitzt", "im", "Se\u00b7nat", ";", "doch", "ge\u00b7lingt", "ih\u00b7re", "Ver\u00b7wand\u00b7lung", "ihr", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.17": {"text": "Denn sie t\u00e4uschet nicht; weiss es, bleibt! Doch Andrer Verwandlung", "tokens": ["Denn", "sie", "t\u00e4u\u00b7schet", "nicht", ";", "weiss", "es", ",", "bleibt", "!", "Doch", "A\u00b7ndrer", "Ver\u00b7wand\u00b7lung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$.", "KOUS", "PPER", "$,", "VVFIN", "$.", "KON", "ADJA", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Gl\u00fcckte ihr einst: todtdroh'nd schuf sie zu Stein den Senat!", "tokens": ["Gl\u00fcck\u00b7te", "ihr", "einst", ":", "todt\u00b7droh'nd", "schuf", "sie", "zu", "Stein", "den", "Se\u00b7nat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "ADJD", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Hast du mich, theurer Schatten, geh\u00f6rt; so rede. Denn jetzo", "tokens": ["Hast", "du", "mich", ",", "theu\u00b7rer", "Schat\u00b7ten", ",", "ge\u00b7h\u00f6rt", ";", "so", "re\u00b7de", ".", "Denn", "jet\u00b7zo"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,", "VVFIN", "$.", "ADV", "VVFIN", "$.", "KON", "ADV"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Siehst du die Zukunft. Ach schweiget dereinst das Gezisch", "tokens": ["Siehst", "du", "die", "Zu\u00b7kunft", ".", "Ach", "schwei\u00b7get", "de\u00b7reinst", "das", "Ge\u00b7zisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ITJ", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.21": {"text": "Um der Alekto Haupt? muss je sie die Todesfackel", "tokens": ["Um", "der", "A\u00b7lek\u00b7to", "Haupt", "?", "muss", "je", "sie", "die", "To\u00b7des\u00b7fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NE", "NN", "$.", "VMFIN", "ADV", "PPER", "ART", "NN"], "meter": "---+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Von sich werfen, entfliehn? Wird er entsteint der Senat?", "tokens": ["Von", "sich", "wer\u00b7fen", ",", "ent\u00b7fliehn", "?", "Wird", "er", "ent\u00b7steint", "der", "Se\u00b7nat", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$,", "VVINF", "$.", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Kehrt die G\u00f6ttin zur\u00fcck, die gen Himmel wieder emporstieg?", "tokens": ["Kehrt", "die", "G\u00f6t\u00b7tin", "zu\u00b7r\u00fcck", ",", "die", "gen", "Him\u00b7mel", "wie\u00b7der", "em\u00b7por\u00b7stieg", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Oder vers\u00f6hnen sie die, welche sie l\u00e4sterten, nie?", "tokens": ["O\u00b7der", "ver\u00b7s\u00f6h\u00b7nen", "sie", "die", ",", "wel\u00b7che", "sie", "l\u00e4s\u00b7ter\u00b7ten", ",", "nie", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.25": {"text": "Edler Todter, ich sehe dich nicht: doch ahnd' ich dich nahe;", "tokens": ["Ed\u00b7ler", "Tod\u00b7ter", ",", "ich", "se\u00b7he", "dich", "nicht", ":", "doch", "ahnd'", "ich", "dich", "na\u00b7he", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Denn in der D\u00e4mmerung dort seh' ich ein blutig Gewand.", "tokens": ["Denn", "in", "der", "D\u00e4m\u00b7me\u00b7rung", "dort", "seh'", "ich", "ein", "blu\u00b7tig", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Ach nun schwebest du, schwebst! hast meine Wehmuth vernommen,", "tokens": ["Ach", "nun", "schwe\u00b7best", "du", ",", "schwebst", "!", "hast", "mei\u00b7ne", "Weh\u00b7muth", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "$.", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Hast die Frage des Grams, die ich dir weinte, geh\u00f6rt.", "tokens": ["Hast", "die", "Fra\u00b7ge", "des", "Grams", ",", "die", "ich", "dir", "wein\u00b7te", ",", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Aber du schweigst. So starbest du denn vergebens, du Guter,", "tokens": ["A\u00b7ber", "du", "schweigst", ".", "So", "star\u00b7best", "du", "denn", "ver\u00b7ge\u00b7bens", ",", "du", "Gu\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "F\u00fcr dein Vaterland! waltet auf immer die Wuth", "tokens": ["F\u00fcr", "dein", "Va\u00b7ter\u00b7land", "!", "wal\u00b7tet", "auf", "im\u00b7mer", "die", "Wuth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "VVFIN", "APPR", "ADV", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Jener Emp\u00f6rer! tritt ihr Fuss auf immer die grosse", "tokens": ["Je\u00b7ner", "Em\u00b7p\u00f6\u00b7rer", "!", "tritt", "ihr", "Fuss", "auf", "im\u00b7mer", "die", "gros\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$.", "VVFIN", "PPOSAT", "NN", "APPR", "ADV", "ART", "ADJA"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.32": {"text": "Nazion, mit des Hohns bitterer Lach', in den Staub!", "tokens": ["Na\u00b7zi\u00b7on", ",", "mit", "des", "Hohns", "bit\u00b7te\u00b7rer", "Lach'", ",", "in", "den", "Staub", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "---+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Duldet auf immer, dass sie geh\u00f6hnt da liege die grosse", "tokens": ["Dul\u00b7det", "auf", "im\u00b7mer", ",", "dass", "sie", "ge\u00b7h\u00f6hnt", "da", "lie\u00b7ge", "die", "gros\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADV", "$,", "KOUS", "PPER", "VVPP", "ADV", "VVFIN", "ART", "ADJA"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Nazion in dem Staub', unter der W\u00fcthenden Fuss?", "tokens": ["Na\u00b7zi\u00b7on", "in", "dem", "Staub'", ",", "un\u00b7ter", "der", "W\u00fct\u00b7hen\u00b7den", "Fuss", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Kehret sie nie zur\u00fcck, die gen Himmel wieder emporstieg,", "tokens": ["Keh\u00b7ret", "sie", "nie", "zu\u00b7r\u00fcck", ",", "die", "gen", "Him\u00b7mel", "wie\u00b7der", "em\u00b7por\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "PRELS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Und vers\u00f6hnen sie die, welche sie l\u00e4sterten, nie!", "tokens": ["Und", "ver\u00b7s\u00f6h\u00b7nen", "sie", "die", ",", "wel\u00b7che", "sie", "l\u00e4s\u00b7ter\u00b7ten", ",", "nie", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}