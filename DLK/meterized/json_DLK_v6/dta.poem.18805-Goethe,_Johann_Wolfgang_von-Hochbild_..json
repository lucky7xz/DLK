{"dta.poem.18805": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hochbild .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne, Helios der Griechen,", "tokens": ["Die", "Son\u00b7ne", ",", "He\u00b7li\u00b7os", "der", "Grie\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00e4hrt pr\u00e4chtig auf der Himmelsbahn,", "tokens": ["F\u00e4hrt", "pr\u00e4ch\u00b7tig", "auf", "der", "Him\u00b7mels\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewiss das Weltall zu besiegen", "tokens": ["Ge\u00b7wiss", "das", "Wel\u00b7tall", "zu", "be\u00b7sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blickt er umher, hinab, hinan.", "tokens": ["Blickt", "er", "um\u00b7her", ",", "hin\u00b7ab", ",", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PTKVZ", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er sieht die sch\u00f6nste G\u00f6ttinn weinen,", "tokens": ["Er", "sieht", "die", "sch\u00f6ns\u00b7te", "G\u00f6t\u00b7tinn", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wolkentochter, Himmelskind,", "tokens": ["Die", "Wol\u00b7ken\u00b7toch\u00b7ter", ",", "Him\u00b7mel\u00b7skind", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr scheint er nur allein zu scheinen,", "tokens": ["Ihr", "scheint", "er", "nur", "al\u00b7lein", "zu", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr alle heitre R\u00e4ume blind", "tokens": ["F\u00fcr", "al\u00b7le", "heit\u00b7re", "R\u00e4u\u00b7me", "blind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Versenkt er sich in Schmerz und Schauer", "tokens": ["Ver\u00b7senkt", "er", "sich", "in", "Schmerz", "und", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4ufiger quillt ihr Thr\u00e4nenguss;", "tokens": ["Und", "h\u00e4u\u00b7fi\u00b7ger", "quillt", "ihr", "Thr\u00e4\u00b7nen\u00b7guss", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er sendet Lust in ihre Trauer", "tokens": ["Er", "sen\u00b7det", "Lust", "in", "ih\u00b7re", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und jeder Perle Kuss auf Kuss.", "tokens": ["Und", "je\u00b7der", "Per\u00b7le", "Kuss", "auf", "Kuss", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun f\u00fchlt sie tief des Blicks Gewalten,", "tokens": ["Nun", "f\u00fchlt", "sie", "tief", "des", "Blicks", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und unverwandt schaut sie hinauf,", "tokens": ["Und", "un\u00b7ver\u00b7wandt", "schaut", "sie", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Perlen wollen sich gestalten:", "tokens": ["Die", "Per\u00b7len", "wol\u00b7len", "sich", "ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn jede nahm sein Bildniss auf.", "tokens": ["Denn", "je\u00b7de", "nahm", "sein", "Bild\u00b7niss", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so, umkr\u00e4nzt von Farb\u2019 und Bogen,", "tokens": ["Und", "so", ",", "um\u00b7kr\u00e4nzt", "von", "Fa\u00b7rb'", "und", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erheitert leuchtet ihr Gesicht,", "tokens": ["Er\u00b7hei\u00b7tert", "leuch\u00b7tet", "ihr", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entgegen kommt er ihr gezogen,", "tokens": ["Ent\u00b7ge\u00b7gen", "kommt", "er", "ihr", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch er! doch ach! erreicht sie nicht.", "tokens": ["Doch", "er", "!", "doch", "ach", "!", "er\u00b7reicht", "sie", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$.", "ADV", "ADV", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So, nach des Schicksals hartem Loose,", "tokens": ["So", ",", "nach", "des", "Schick\u00b7sals", "har\u00b7tem", "Loo\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weichst du mir Lieblichste davon,", "tokens": ["Weichst", "du", "mir", "Lieb\u00b7lichs\u00b7te", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4r\u2019 ich Helios der grosse", "tokens": ["Und", "w\u00e4r'", "ich", "He\u00b7li\u00b7os", "der", "gros\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was n\u00fctzte mir der Wagenthron?", "tokens": ["Was", "n\u00fctz\u00b7te", "mir", "der", "Wa\u00b7gen\u00b7thron", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}