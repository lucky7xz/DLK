{"dta.poem.7086": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O h\u00f6chst begl\u00fcckt-o dreymahl seel\u2019ge Seele!", "tokens": ["O", "h\u00f6chst", "be\u00b7gl\u00fcck\u00b7t\u00b7o", "drey\u00b7mahl", "seel'\u00b7ge", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was wirst du sehn? was wirst du finden?", "tokens": ["Was", "wirst", "du", "sehn", "?", "was", "wirst", "du", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$.", "PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie unbegreifflich wird der Unterscheid,", "tokens": ["Wie", "un\u00b7be\u00b7greif\u00b7flich", "wird", "der", "Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von Pracht, von Anmuth, Herrlichkeit,", "tokens": ["Von", "Pracht", ",", "von", "An\u00b7muth", ",", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Wundern, von Gestalten, von Figuren", "tokens": ["Von", "Wun\u00b7dern", ",", "von", "Ge\u00b7stal\u00b7ten", ",", "von", "Fi\u00b7gu\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der ungez\u00e4hlten Creaturen,", "tokens": ["Der", "un\u00b7ge\u00b7z\u00e4hl\u00b7ten", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Was vor ein Reichthum doch von Farben, Licht und Schein,", "tokens": ["Was", "vor", "ein", "Reicht\u00b7hum", "doch", "von", "Far\u00b7ben", ",", "Licht", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja wie wird deine Krafft so gro\u00df im Dencken seyn!", "tokens": ["Ja", "wie", "wird", "dei\u00b7ne", "Krafft", "so", "gro\u00df", "im", "Den\u00b7cken", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOKOM", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So gro\u00df die F\u00e4higkeit, Vergn\u00fcgen zu empfinden!", "tokens": ["So", "gro\u00df", "die", "F\u00e4\u00b7hig\u00b7keit", ",", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}