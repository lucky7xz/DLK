{"textgrid.poem.55840": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Aus einer Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Dunkeln war wie Reichtum in dem Raume,", "tokens": ["Das", "Dun\u00b7keln", "war", "wie", "Reich\u00b7tum", "in", "dem", "Rau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "KOKOM", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "darin der Knabe, sehr verheimlicht, sa\u00df.", "tokens": ["da\u00b7rin", "der", "Kna\u00b7be", ",", "sehr", "ver\u00b7heim\u00b7licht", ",", "sa\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ADV", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und als die Mutter eintrat wie im Traume,", "tokens": ["Und", "als", "die", "Mut\u00b7ter", "ein\u00b7trat", "wie", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erzitterte im stillen Schrank ein Glas.", "tokens": ["er\u00b7zit\u00b7ter\u00b7te", "im", "stil\u00b7len", "Schrank", "ein", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie f\u00fchlte, wie das Zimmer sie verriet,", "tokens": ["Sie", "f\u00fchl\u00b7te", ",", "wie", "das", "Zim\u00b7mer", "sie", "ver\u00b7riet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und k\u00fc\u00dfte ihren Knaben: Bist du hier?...", "tokens": ["und", "k\u00fc\u00df\u00b7te", "ih\u00b7ren", "Kna\u00b7ben", ":", "Bist", "du", "hier", "?", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann schauten beide bang nach dem Klavier,", "tokens": ["Dann", "schau\u00b7ten", "bei\u00b7de", "bang", "nach", "dem", "Kla\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+++", "measure": "zehnsilber"}, "line.8": {"text": "denn manchen Abend hatte sie ein Lied,", "tokens": ["denn", "man\u00b7chen", "A\u00b7bend", "hat\u00b7te", "sie", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "darin das Kind sich seltsam tief verfing.", "tokens": ["da\u00b7rin", "das", "Kind", "sich", "selt\u00b7sam", "tief", "ver\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es sa\u00df sehr still. Sein gro\u00dfes Schauen hing", "tokens": ["Es", "sa\u00df", "sehr", "still", ".", "Sein", "gro\u00b7\u00dfes", "Schau\u00b7en", "hing"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$.", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "an ihrer Hand, die ganz gebeugt vom Ringe,", "tokens": ["an", "ih\u00b7rer", "Hand", ",", "die", "ganz", "ge\u00b7beugt", "vom", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als ob sie schwer in Schneewehn ginge,", "tokens": ["als", "ob", "sie", "schwer", "in", "Schnee\u00b7wehn", "gin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00fcber die wei\u00dfen Tasten ging.", "tokens": ["\u00fc\u00b7ber", "die", "wei\u00b7\u00dfen", "Tas\u00b7ten", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}