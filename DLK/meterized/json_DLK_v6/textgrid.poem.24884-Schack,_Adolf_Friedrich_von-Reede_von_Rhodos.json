{"textgrid.poem.24884": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Reede von Rhodos", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Langsam vom Wind dahingetrieben, gleitet", "tokens": ["Lang\u00b7sam", "vom", "Wind", "da\u00b7hin\u00b7ge\u00b7trie\u00b7ben", ",", "glei\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "APPRART", "NN", "PAV", "$,", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Das Schiff durch wei\u00dfbesch\u00e4umte Flut;", "tokens": ["Das", "Schiff", "durch", "wei\u00df\u00b7be\u00b7sch\u00e4um\u00b7te", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Schlaf und Traum sind alle rings vertieft;", "tokens": ["In", "Schlaf", "und", "Traum", "sind", "al\u00b7le", "rings", "ver\u00b7tieft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Mondlicht trieft", "tokens": ["Das", "Mond\u00b7licht", "trieft"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durch Nebelflor herab, der hingebreitet", "tokens": ["Durch", "Ne\u00b7bel\u00b7flor", "her\u00b7ab", ",", "der", "hin\u00b7ge\u00b7brei\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADV", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf Inselstrand und Wellen ruht.", "tokens": ["Auf", "In\u00b7sel\u00b7strand", "und", "Wel\u00b7len", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nein; nicht von dem Mond ist das Gefunkel,", "tokens": ["Doch", "nein", ";", "nicht", "von", "dem", "Mond", "ist", "das", "Ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$.", "PTKNEG", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das zitternd auf den Wogen wallt;", "tokens": ["Das", "zit\u00b7ternd", "auf", "den", "Wo\u00b7gen", "wallt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nah flammt's, und n\u00e4her nun, als w\u00e4r's der Strahl,", "tokens": ["Nah", "flammt's", ",", "und", "n\u00e4\u00b7her", "nun", ",", "als", "w\u00e4r's", "der", "Strahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "ADJD", "ADV", "$,", "KOKOM", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den ein Fanal", "tokens": ["Den", "ein", "Fa\u00b7nal"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Vom Felsen wirft, und d\u00e4mmernd aus dem Dunkel", "tokens": ["Vom", "Fel\u00b7sen", "wirft", ",", "und", "d\u00e4m\u00b7mernd", "aus", "dem", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Steigt eine riesige Gestalt.", "tokens": ["Steigt", "ei\u00b7ne", "rie\u00b7si\u00b7ge", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vom Nebel l\u00f6sen sich die Glieder;", "tokens": ["Vom", "Ne\u00b7bel", "l\u00f6\u00b7sen", "sich", "die", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Arm, gigantisch ausgestreckt,", "tokens": ["Ein", "Arm", ",", "gi\u00b7gan\u00b7tisch", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Taucht aus der Finsternis, in seiner Hand", "tokens": ["Taucht", "aus", "der", "Fins\u00b7ter\u00b7nis", ",", "in", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ein Fackelbrand,", "tokens": ["Ein", "Fa\u00b7ckel\u00b7brand", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Von dem die Glut im Windhauch auf und nieder", "tokens": ["Von", "dem", "die", "Glut", "im", "Wind\u00b7hauch", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit roter Flammenzunge leckt.", "tokens": ["Mit", "ro\u00b7ter", "Flam\u00b7men\u00b7zun\u00b7ge", "leckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weitleuchtend strahlt die Stirn des Sonnenriesen", "tokens": ["Weit\u00b7leuch\u00b7tend", "strahlt", "die", "Stirn", "des", "Son\u00b7nen\u00b7rie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Meer hinaus \u2013 der Nebel f\u00e4llt \u2013", "tokens": ["Aufs", "Meer", "hin\u00b7aus", "\u2013", "der", "Ne\u00b7bel", "f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da steht er ganz, der m\u00e4chtige Kolo\u00df", "tokens": ["Da", "steht", "er", "ganz", ",", "der", "m\u00e4ch\u00b7ti\u00b7ge", "Ko\u00b7lo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Helios,", "tokens": ["Des", "He\u00b7li\u00b7os", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Glorreich, so wie der Dichter ihn gepriesen,", "tokens": ["Glor\u00b7reich", ",", "so", "wie", "der", "Dich\u00b7ter", "ihn", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "KOKOM", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Ruhm von Rhodos und der Welt.", "tokens": ["Der", "Ruhm", "von", "Rho\u00b7dos", "und", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf Felsen, zu des Hafens beiden Seiten", "tokens": ["Auf", "Fel\u00b7sen", ",", "zu", "des", "Ha\u00b7fens", "bei\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die eh'rnen F\u00fc\u00dfe hingestemmt,", "tokens": ["Die", "eh'r\u00b7nen", "F\u00fc\u00b7\u00dfe", "hin\u00b7ge\u00b7stemmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ragt er empor; von Segeln ringsumher", "tokens": ["Ragt", "er", "em\u00b7por", ";", "von", "Se\u00b7geln", "rings\u00b7um\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ergl\u00e4nzt das Meer,", "tokens": ["Er\u00b7gl\u00e4nzt", "das", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und unter seinen ries'gen Gliedern gleiten", "tokens": ["Und", "un\u00b7ter", "sei\u00b7nen", "ries'\u00b7gen", "Glie\u00b7dern", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie in den Hafen ungehemmt.", "tokens": ["Sie", "in", "den", "Ha\u00b7fen", "un\u00b7ge\u00b7hemmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich selbst mit ihnen. Welch ein Wald von Masten!", "tokens": ["Ich", "selbst", "mit", "ih\u00b7nen", ".", "Welch", "ein", "Wald", "von", "Mas\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$.", "PIAT", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier Griechenschiffe, Kiel an Kiel,", "tokens": ["Hier", "Grie\u00b7chen\u00b7schif\u00b7fe", ",", "Kiel", "an", "Kiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf jedem vorn das Dioskurenpaar,", "tokens": ["Auf", "je\u00b7dem", "vorn", "das", "Dios\u00b7ku\u00b7ren\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das in Gefahr", "tokens": ["Das", "in", "Ge\u00b7fahr"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Schiffer sch\u00fctzt; dort, schwer von Warenlasten,", "tokens": ["Die", "Schif\u00b7fer", "sch\u00fctzt", ";", "dort", ",", "schwer", "von", "Wa\u00b7ren\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Barken von Tyrus und vom Nil.", "tokens": ["Bar\u00b7ken", "von", "Ty\u00b7rus", "und", "vom", "Nil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "KON", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Am Ufer buntes Volksgedr\u00e4ng und L\u00e4rmen.", "tokens": ["Am", "U\u00b7fer", "bun\u00b7tes", "Volks\u00b7ge\u00b7dr\u00e4ng", "und", "L\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Marmor leuchtend und von Erz,", "tokens": ["Von", "Mar\u00b7mor", "leuch\u00b7tend", "und", "von", "Erz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "T\u00fcrmt mit Theater, Halle, Hippodrom,", "tokens": ["T\u00fcrmt", "mit", "The\u00b7a\u00b7ter", ",", "Hal\u00b7le", ",", "Hip\u00b7po\u00b7drom", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Menschenstrom", "tokens": ["Vom", "Men\u00b7schen\u00b7strom"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Durchwogt, voll Tempeln, Statuen und Hermen", "tokens": ["Durch\u00b7wogt", ",", "voll", "Tem\u00b7peln", ",", "Sta\u00b7tu\u00b7en", "und", "Her\u00b7men"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "NN", "$,", "NN", "KON", "NN"], "meter": "++-+---+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Vor mir die Stadt sich himmelw\u00e4rts.", "tokens": ["Vor", "mir", "die", "Stadt", "sich", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch horch! es rollt der Anker; ich erwache \u2013", "tokens": ["Doch", "horch", "!", "es", "rollt", "der", "An\u00b7ker", ";", "ich", "er\u00b7wa\u00b7che", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin, wohin mein Traum verweht?", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "mein", "Traum", "ver\u00b7weht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Armsel'ge H\u00fctten stehn vor mir von Lehm,", "tokens": ["Arm\u00b7sel'\u00b7ge", "H\u00fct\u00b7ten", "stehn", "vor", "mir", "von", "Lehm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wo ehedem", "tokens": ["Wo", "e\u00b7he\u00b7dem"], "token_info": ["word", "word"], "pos": ["PWAV", "ADV"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Rhodos geprangt hat; vom Moscheendache", "tokens": ["Rho\u00b7dos", "ge\u00b7prangt", "hat", ";", "vom", "Mo\u00b7scheen\u00b7da\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVPP", "VAFIN", "$.", "APPRART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Ruft der Muezzin zum Gebet.", "tokens": ["Ruft", "der", "Muez\u00b7zin", "zum", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}}}}