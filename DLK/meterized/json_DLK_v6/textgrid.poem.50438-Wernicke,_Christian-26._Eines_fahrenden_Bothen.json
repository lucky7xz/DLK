{"textgrid.poem.50438": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "26. Eines fahrenden Bothen", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der halb in ", "tokens": ["Der", "halb", "in"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und Leute, Pack, und Brief' in ein ", "tokens": ["Und", "Leu\u00b7te", ",", "Pack", ",", "und", "Brie\u00b7f'", "in", "ein"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "KON", "NN", "APPR", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der seinen Nutzen macht' aus einem", "tokens": ["Der", "sei\u00b7nen", "Nut\u00b7zen", "macht'", "aus", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und offt an einen Orth, doch nirgends lange blieb:", "tokens": ["Und", "offt", "an", "ei\u00b7nen", "Orth", ",", "doch", "nir\u00b7gends", "lan\u00b7ge", "blieb", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der l\u00e4g' auch nicht ", "tokens": ["Der", "l\u00e4g'", "auch", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}}}}}