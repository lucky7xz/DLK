{"textgrid.poem.52496": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "16. W\u00e4hrend einer Predigt", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hei\u00dft gepredigt, meiner Treu!", "tokens": ["Das", "hei\u00dft", "ge\u00b7pre\u00b7digt", ",", "mei\u00b7ner", "Treu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Mensch bleibt unger\u00fchrt dabei;", "tokens": ["Kein", "Mensch", "bleibt", "un\u00b7ge\u00b7r\u00fchrt", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jedes Auge schwimmt in Thr\u00e4nen", "tokens": ["Und", "je\u00b7des", "Au\u00b7ge", "schwimmt", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom \u2013 \u2013 \u2013 G\u00e4hnen.", "tokens": ["Vom", "\u2013", "\u2013", "\u2013", "G\u00e4h\u00b7nen", "."], "token_info": ["word", "punct", "punct", "punct", "word", "punct"], "pos": ["APPRART", "$(", "$(", "$(", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}