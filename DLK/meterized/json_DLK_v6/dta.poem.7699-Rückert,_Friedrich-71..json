{"dta.poem.7699": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Eigennutz entspringt die Dankbarkeit der Meisten,", "tokens": ["Aus", "Ei\u00b7gen\u00b7nutz", "ent\u00b7springt", "die", "Dank\u00b7bar\u00b7keit", "der", "Meis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr einen Dienst, den wir geleistet oder leisten.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Dienst", ",", "den", "wir", "ge\u00b7leis\u00b7tet", "o\u00b7der", "leis\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch ist die Dankbarkeit auch so der sch\u00f6nste Lohn,", "tokens": ["Doch", "ist", "die", "Dank\u00b7bar\u00b7keit", "auch", "so", "der", "sch\u00f6ns\u00b7te", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den selbst man soll mit Dank annehmen, nicht mit Hohn.", "tokens": ["Den", "selbst", "man", "soll", "mit", "Dank", "an\u00b7neh\u00b7men", ",", "nicht", "mit", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VMFIN", "APPR", "NN", "VVINF", "$,", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sei dankbar, da\u00df den Dank der Eigennutz dir bringt,", "tokens": ["Sei", "dank\u00b7bar", ",", "da\u00df", "den", "Dank", "der", "Ei\u00b7gen\u00b7nutz", "dir", "bringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df aus so schlechtem Grund so edler Trieb entspringt.", "tokens": ["Da\u00df", "aus", "so", "schlech\u00b7tem", "Grund", "so", "ed\u00b7ler", "Trieb", "ent\u00b7springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}