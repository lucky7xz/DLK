{"textgrid.poem.55264": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Jahre", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jahre sind allerliebste Leut:", "tokens": ["Die", "Jah\u00b7re", "sind", "al\u00b7ler\u00b7liebs\u00b7te", "Leut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie brachten gestern, sie bringen heut,", "tokens": ["Sie", "brach\u00b7ten", "ge\u00b7stern", ",", "sie", "brin\u00b7gen", "heut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und so verbringen wir J\u00fcngern eben", "tokens": ["Und", "so", "ver\u00b7brin\u00b7gen", "wir", "J\u00fcn\u00b7gern", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "ADV"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das allerliebste Schlaraffenleben.", "tokens": ["Das", "al\u00b7ler\u00b7liebs\u00b7te", "Schla\u00b7raf\u00b7fen\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und dann f\u00e4llt's den Jahren auf einmal ein,", "tokens": ["Und", "dann", "f\u00e4llt's", "den", "Jah\u00b7ren", "auf", "ein\u00b7mal", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nicht mehr wie sonst bequem zu sein,", "tokens": ["Nicht", "mehr", "wie", "sonst", "be\u00b7quem", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wollen nicht mehr schenken, wollen nicht mehr borgen", "tokens": ["Wol\u00b7len", "nicht", "mehr", "schen\u00b7ken", ",", "wol\u00b7len", "nicht", "mehr", "bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "VVINF", "$,", "VMFIN", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Sie nehmen heute, sie nehmen morgen.", "tokens": ["Sie", "neh\u00b7men", "heu\u00b7te", ",", "sie", "neh\u00b7men", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}