{"textgrid.poem.51608": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Seele wirckt, davon der C\u00f6rper nichts versteht.", "tokens": ["Die", "See\u00b7le", "wirckt", ",", "da\u00b7von", "der", "C\u00f6r\u00b7per", "nichts", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PAV", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daher sein Wesen nicht aus ihrem ist noch geht.", "tokens": ["Da\u00b7her", "sein", "We\u00b7sen", "nicht", "aus", "ih\u00b7rem", "ist", "noch", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}