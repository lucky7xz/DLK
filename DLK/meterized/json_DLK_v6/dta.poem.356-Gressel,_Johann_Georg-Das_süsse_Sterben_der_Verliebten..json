{"dta.poem.356": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das s\u00fcsse Sterben der Verliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Stirb nur/ Amando stirb/ die Lust steht dir zur Seiten/", "tokens": ["Stirb", "nur", "/", "A\u00b7man\u00b7do", "stirb", "/", "die", "Lust", "steht", "dir", "zur", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$(", "NE", "VVFIN", "$(", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die sch\u00f6ne Marmor-Brust will dir das Grab bereiten:", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mar\u00b7mor\u00b7Brust", "will", "dir", "das", "Grab", "be\u00b7rei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O wundersch\u00f6ner Tod! gl\u00fccksehlig wer so stirbt/", "tokens": ["O", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Tod", "!", "gl\u00fcck\u00b7seh\u00b7lig", "wer", "so", "stirbt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADJD", "PWS", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in der hohlen Schoo\u00df ein s\u00fcsses Grab erwirbt", "tokens": ["Und", "in", "der", "hoh\u00b7len", "Schoo\u00df", "ein", "s\u00fcs\u00b7ses", "Grab", "er\u00b7wirbt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man stirbt indem man lebt/ man geht in Wollust unter/", "tokens": ["Man", "stirbt", "in\u00b7dem", "man", "lebt", "/", "man", "geht", "in", "Wol\u00b7lust", "un\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "PIS", "VVFIN", "$(", "PIS", "VVFIN", "APPR", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wird nach solchen Tod zu neuen Freuden munter.", "tokens": ["Und", "wird", "nach", "sol\u00b7chen", "Tod", "zu", "neu\u00b7en", "Freu\u00b7den", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Besteige nur das Grab/ erf\u00fcll die sch\u00f6ne Grufft/", "tokens": ["Be\u00b7stei\u00b7ge", "nur", "das", "Grab", "/", "er\u00b7f\u00fcll", "die", "sch\u00f6\u00b7ne", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn der ist recht vergn\u00fcgt/ der so im Grabe dufft.", "tokens": ["Denn", "der", "ist", "recht", "ver\u00b7gn\u00fcgt", "/", "der", "so", "im", "Gra\u00b7be", "dufft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VAFIN", "ADJD", "VVPP", "$(", "ART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}