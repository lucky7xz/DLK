{"textgrid.poem.48991": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fahr' durch das Klippental,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr' durch das Klippental,", "tokens": ["Fahr'", "durch", "das", "Klip\u00b7pen\u00b7tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fahr' durch die Wipfel:", "tokens": ["Fahr'", "durch", "die", "Wip\u00b7fel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "Der Tannen hohes Lied!", "tokens": ["Der", "Tan\u00b7nen", "ho\u00b7hes", "Lied", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Schwing' dich auf zu jenem Felsk\u00f6nig,", "tokens": ["Schwing'", "dich", "auf", "zu", "je\u00b7nem", "Fels\u00b7k\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Der f\u00fcrchterlich durch Blitze blickt,", "tokens": ["Der", "f\u00fcrch\u00b7ter\u00b7lich", "durch", "Blit\u00b7ze", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn licht gefl\u00fcgelt seine Schulter", "tokens": ["Wenn", "licht", "ge\u00b7fl\u00fc\u00b7gelt", "sei\u00b7ne", "Schul\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Donners Wagen dr\u00fcckt,", "tokens": ["Des", "Don\u00b7ners", "Wa\u00b7gen", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Damit es sich erhelle! \u2013 Ruf zur\u00fcck", "tokens": ["Da\u00b7mit", "es", "sich", "er\u00b7hel\u00b7le", "!", "\u2013", "Ruf", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$.", "$(", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den Tag, da er das erste Mal gleich einem Riesen ausgestreckt", "tokens": ["Den", "Tag", ",", "da", "er", "das", "ers\u00b7te", "Mal", "gleich", "ei\u00b7nem", "Rie\u00b7sen", "aus\u00b7ge\u00b7streckt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Der Sonne gegen\u00fcberstand, zehn T\u00e4ler und zehn W\u00e4lder", "tokens": ["Der", "Son\u00b7ne", "ge\u00b7gen\u00b7\u00fc\u00b7bers\u00b7tand", ",", "zehn", "T\u00e4\u00b7ler", "und", "zehn", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "CARD", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Mit seinem Schatten \u00fcberdeckte,", "tokens": ["Mit", "sei\u00b7nem", "Schat\u00b7ten", "\u00fc\u00b7berd\u00b7eck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und wie vom s\u00fc\u00dfen Strahle trunken,", "tokens": ["Und", "wie", "vom", "s\u00fc\u00b7\u00dfen", "Strah\u00b7le", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Gleich einem frohen S\u00e4ugling l\u00e4chelte \u2013 voll Lust", "tokens": ["Gleich", "ei\u00b7nem", "fro\u00b7hen", "S\u00e4ug\u00b7ling", "l\u00e4\u00b7chel\u00b7te", "\u2013", "voll", "Lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$(", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die aufgeschwollne, gr\u00fcne Schulter b\u00e4umte, hoch erhob", "tokens": ["Die", "auf\u00b7ge\u00b7schwoll\u00b7ne", ",", "gr\u00fc\u00b7ne", "Schul\u00b7ter", "b\u00e4um\u00b7te", ",", "hoch", "er\u00b7hob"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Die rauhe Felsenbrust.", "tokens": ["Die", "rau\u00b7he", "Fel\u00b7sen\u00b7brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Und wie auf seinem Haupt nun Blumen aufgeschlossen,", "tokens": ["Und", "wie", "auf", "sei\u00b7nem", "Haupt", "nun", "Blu\u00b7men", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er schmachtend in das Abendrot geblickt,", "tokens": ["Er", "schmach\u00b7tend", "in", "das", "A\u00b7ben\u00b7drot", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Gleich einem J\u00fcngling, dem mit erstem Siegeskranz", "tokens": ["Gleich", "ei\u00b7nem", "J\u00fcng\u00b7ling", ",", "dem", "mit", "ers\u00b7tem", "Sie\u00b7ges\u00b7kranz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das M\u00e4dchen seine Stirn umschm\u00fcckt.", "tokens": ["Das", "M\u00e4d\u00b7chen", "sei\u00b7ne", "Stirn", "um\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}