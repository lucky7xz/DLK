{"textgrid.poem.46481": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun Du!", "genre": "verse", "period": "N.A.", "pub_year": 1917, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun Du!", "tokens": ["Nun", "Du", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PPER", "$."], "meter": "++", "measure": "spondeus"}, "line.2": {"text": "Du neuer Blick und Atem gegen\u00fcber, \u2013", "tokens": ["Du", "neu\u00b7er", "Blick", "und", "A\u00b7tem", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dir zwing ich meine Lippen, weil ich mu\u00df", "tokens": ["Dir", "zwing", "ich", "mei\u00b7ne", "Lip\u00b7pen", ",", "weil", "ich", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VMFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sage:", "tokens": ["Und", "sa\u00b7ge", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Sieh mich an!", "tokens": ["Sieh", "mich", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Gesicht la\u00df ruhen in Gesicht,", "tokens": ["Ge\u00b7sicht", "la\u00df", "ru\u00b7hen", "in", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es geht nicht anders mehr.", "tokens": ["Es", "geht", "nicht", "an\u00b7ders", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo ist denn Schuld,", "tokens": ["Wo", "ist", "denn", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Da\u00df Stirne nun an Stirne st\u00f6\u00dft,", "tokens": ["Da\u00df", "Stir\u00b7ne", "nun", "an", "Stir\u00b7ne", "st\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Herz, das sonst in Einsamkeit hinfror,", "tokens": ["Das", "Herz", ",", "das", "sonst", "in", "Ein\u00b7sam\u00b7keit", "hinf\u00b7ror", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In taubem Kummer sich verlor,", "tokens": ["In", "tau\u00b7bem", "Kum\u00b7mer", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Drang die schwere Zunge l\u00f6st,", "tokens": ["Im", "Drang", "die", "schwe\u00b7re", "Zun\u00b7ge", "l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ergriffen st\u00fcrzt:", "tokens": ["Er\u00b7grif\u00b7fen", "st\u00fcrzt", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Du Weib!", "tokens": ["Du", "Weib", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Schlie\u00df auf, schlie\u00df auf", "tokens": ["Schlie\u00df", "auf", ",", "schlie\u00df", "auf"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Den engen Ring, der meine Brust umpre\u00dft!", "tokens": ["Den", "en\u00b7gen", "Ring", ",", "der", "mei\u00b7ne", "Brust", "um\u00b7pre\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der mich nicht atmen l\u00e4\u00dft,", "tokens": ["Der", "mich", "nicht", "at\u00b7men", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der mich zum qualgepfl\u00fcgten Boden niederwarf,", "tokens": ["Der", "mich", "zum", "qual\u00b7ge\u00b7pfl\u00fcg\u00b7ten", "Bo\u00b7den", "nie\u00b7der\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So oft ich meine Stirn erhob \u2013", "tokens": ["So", "oft", "ich", "mei\u00b7ne", "Stirn", "er\u00b7hob", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du hast die Macht.", "tokens": ["Du", "hast", "die", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du brauchst nur mit deinem Finger", "tokens": ["Du", "brauchst", "nur", "mit", "dei\u00b7nem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An mein Herz zu r\u00fchren,", "tokens": ["An", "mein", "Herz", "zu", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Damit es wieder sehend w\u00fcrde:", "tokens": ["Da\u00b7mit", "es", "wie\u00b7der", "se\u00b7hend", "w\u00fcr\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle T\u00fcren, Horizonte, alle Himmel", "tokens": ["Und", "al\u00b7le", "T\u00fc\u00b7ren", ",", "Ho\u00b7ri\u00b7zon\u00b7te", ",", "al\u00b7le", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "VVFIN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Spr\u00e4ngen offen mir entgegen:", "tokens": ["Spr\u00e4n\u00b7gen", "of\u00b7fen", "mir", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich schritte m\u00e4chtig aus auf brausenden Wegen,", "tokens": ["Ich", "schrit\u00b7te", "m\u00e4ch\u00b7tig", "aus", "auf", "brau\u00b7sen\u00b7den", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Best\u00fcrmt und durchschwellt,", "tokens": ["Be\u00b7st\u00fcrmt", "und", "durch\u00b7schwellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Zu neuem Lebensland!", "tokens": ["Zu", "neu\u00b7em", "Le\u00b7bens\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zu deinem Herzen in der Welt!", "tokens": ["Zu", "dei\u00b7nem", "Her\u00b7zen", "in", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Verbirgst du deine Hand?", "tokens": ["Ver\u00b7birgst", "du", "dei\u00b7ne", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}