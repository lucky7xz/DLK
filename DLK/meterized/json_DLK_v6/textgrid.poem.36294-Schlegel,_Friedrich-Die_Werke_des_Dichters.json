{"textgrid.poem.36294": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Werke des Dichters", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Faust und Tasso und Meister sind silbergediegene St\u00fccke,", "tokens": ["Faust", "und", "Tas\u00b7so", "und", "Meis\u00b7ter", "sind", "sil\u00b7ber\u00b7ge\u00b7die\u00b7ge\u00b7ne", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sinnreich gebildet mit Flei\u00df, oder erhaben gedacht.", "tokens": ["Sinn\u00b7reich", "ge\u00b7bil\u00b7det", "mit", "Flei\u00df", ",", "o\u00b7der", "er\u00b7ha\u00b7ben", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "NN", "$,", "KON", "ADJD", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "R\u00fchmliches Streben erzeugt' Iphigenien, bildete Egmont,", "tokens": ["R\u00fchm\u00b7li\u00b7ches", "Stre\u00b7ben", "er\u00b7zeugt'", "Ip\u00b7hi\u00b7ge\u00b7ni\u00b7en", ",", "bil\u00b7de\u00b7te", "Eg\u00b7mont", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NE", "$,", "VVFIN", "NE", "$,"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ja auch der Jugend Kraft dr\u00e4ngt' in der F\u00fclle nach Kunst.", "tokens": ["Ja", "auch", "der", "Ju\u00b7gend", "Kraft", "dr\u00e4ngt'", "in", "der", "F\u00fcl\u00b7le", "nach", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Liebliche Kinder des heitersten Genius bl\u00fcht ihr, Claudine!", "tokens": ["Lieb\u00b7li\u00b7che", "Kin\u00b7der", "des", "hei\u00b7ters\u00b7ten", "Ge\u00b7nius", "bl\u00fcht", "ihr", ",", "Clau\u00b7di\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "PPER", "$,", "NE", "$."], "meter": "+--+--+---+-+-+-", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Du der Scherze Triumph, Aristophanischer Witz.", "tokens": ["Du", "der", "Scher\u00b7ze", "Tri\u00b7umph", ",", "A\u00b7ris\u00b7to\u00b7pha\u00b7ni\u00b7scher", "Witz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Tief bewegt uns das kunstlose Lied aus sehnendem Herzen,", "tokens": ["Tief", "be\u00b7wegt", "uns", "das", "kunst\u00b7lo\u00b7se", "Lied", "aus", "seh\u00b7nen\u00b7dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "M\u00e4nnlich klar ist der Blick, jugendlich warm das Gef\u00fchl.", "tokens": ["M\u00e4nn\u00b7lich", "klar", "ist", "der", "Blick", ",", "ju\u00b7gend\u00b7lich", "warm", "das", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "S\u00fc\u00dfer noch t\u00f6nt Elegie und Idyll, und im Rhythmus der Alten", "tokens": ["S\u00fc\u00b7\u00dfer", "noch", "t\u00f6nt", "E\u00b7le\u00b7gie", "und", "I\u00b7dyll", ",", "und", "im", "Rhyth\u00b7mus", "der", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "NN", "KON", "NN", "$,", "KON", "APPRART", "NN", "ART", "NN"], "meter": "+--+--+-+---+--+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "L\u00e4chelt milde der Geist, freut sich der s\u00fcdlichen Luft.", "tokens": ["L\u00e4\u00b7chelt", "mil\u00b7de", "der", "Geist", ",", "freut", "sich", "der", "s\u00fcd\u00b7li\u00b7chen", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Keime nur sind es zu gro\u00dfem Entwurf, wie der l\u00f6bliche Hermann;", "tokens": ["Kei\u00b7me", "nur", "sind", "es", "zu", "gro\u00b7\u00dfem", "Ent\u00b7wurf", ",", "wie", "der", "l\u00f6b\u00b7li\u00b7che", "Her\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Gibt die Parze denn Heil, wachsen sie herrlich empor!", "tokens": ["Gibt", "die", "Par\u00b7ze", "denn", "Heil", ",", "wach\u00b7sen", "sie", "herr\u00b7lich", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}