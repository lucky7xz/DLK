{"dta.poem.2112": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Gott, la\u00df meine Seele doch, durch deiner Krea-", "tokens": ["Mein", "Gott", ",", "la\u00df", "mei\u00b7ne", "See\u00b7le", "doch", ",", "durch", "dei\u00b7ner", "Krea"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVIMP", "PPOSAT", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "turen Zier,", "tokens": ["tu\u00b7ren", "Zier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wovon so Erd\u2019 als Himmel voll, in dir gef\u00e4lligem Ver-", "tokens": ["Wo\u00b7von", "so", "Erd'", "als", "Him\u00b7mel", "voll", ",", "in", "dir", "ge\u00b7f\u00e4l\u00b7li\u00b7gem", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NN", "KOUS", "NN", "ADJD", "$,", "APPR", "PPER", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "gn\u00fcgen,", "tokens": ["gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Voll heiliger Betrachtung jetzt und voll Bewundrung,", "tokens": ["Voll", "hei\u00b7li\u00b7ger", "Be\u00b7trach\u00b7tung", "jetzt", "und", "voll", "Be\u00b7wund\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ADV", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "sich zu dir,", "tokens": ["sich", "zu", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Mit Loben und mit Danken, f\u00fcgen!", "tokens": ["Mit", "Lo\u00b7ben", "und", "mit", "Dan\u00b7ken", ",", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich seh den hellen Himmel an. Erquickt durch sein ent-", "tokens": ["Ich", "seh", "den", "hel\u00b7len", "Him\u00b7mel", "an", ".", "Er\u00b7quickt", "durch", "sein", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "VVPP", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "w\u00f6lket Licht,", "tokens": ["w\u00f6l\u00b7ket", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Das, wie aus einem hellen Born, aus der beflammten", "tokens": ["Das", ",", "wie", "aus", "ei\u00b7nem", "hel\u00b7len", "Born", ",", "aus", "der", "be\u00b7flamm\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PWAV", "APPR", "ART", "ADJA", "NE", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sonne quillet,", "tokens": ["Son\u00b7ne", "quil\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Und durch der Stralen feurig Meer des Firmamentes", "tokens": ["Und", "durch", "der", "Stra\u00b7len", "feu\u00b7rig", "Meer", "des", "Fir\u00b7ma\u00b7men\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Tiefen f\u00fcllet,", "tokens": ["Tie\u00b7fen", "f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Bewundr\u2019 ich nicht nur dieses Wunder; auch mein von", "tokens": ["Be\u00b7wundr'", "ich", "nicht", "nur", "die\u00b7ses", "Wun\u00b7der", ";", "auch", "mein", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "ADV", "PDAT", "NN", "$.", "ADV", "PPOSAT", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gott geschenkt Gesicht,", "tokens": ["Gott", "ge\u00b7schenkt", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Das sich am Licht, auch an den Sch\u00e4tzen, die es uns zeigt,", "tokens": ["Das", "sich", "am", "Licht", ",", "auch", "an", "den", "Sch\u00e4t\u00b7zen", ",", "die", "es", "uns", "zeigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.17": {"text": "ergetzt und n\u00e4hret,", "tokens": ["er\u00b7getzt", "und", "n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und billig den, der jenes schuf und die\u00df mir schenket,", "tokens": ["Und", "bil\u00b7lig", "den", ",", "der", "je\u00b7nes", "schuf", "und", "die\u00df", "mir", "schen\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "$,", "PRELS", "PDS", "VVFIN", "KON", "PDS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "preist und ehret.", "tokens": ["preist", "und", "eh\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Drauf senk ich Blick und Geist herab, um, an der Erden", "tokens": ["Drauf", "senk", "ich", "Blick", "und", "Geist", "her\u00b7ab", ",", "um", ",", "an", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "NN", "KON", "NN", "ADV", "$,", "KOUI", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "bunten Sch\u00e4tzen,", "tokens": ["bun\u00b7ten", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Vom Licht gezeuget und gezeigt, mich, durch die Augen,", "tokens": ["Vom", "Licht", "ge\u00b7zeu\u00b7get", "und", "ge\u00b7zeigt", ",", "mich", ",", "durch", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "KON", "VVPP", "$,", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "zu ergetzen.", "tokens": ["zu", "er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Bevor ich nun herab gelange, durchdringt mein Blick", "tokens": ["Be\u00b7vor", "ich", "nun", "her\u00b7ab", "ge\u00b7lan\u00b7ge", ",", "durch\u00b7dringt", "mein", "Blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "das Reich der Luft,", "tokens": ["das", "Reich", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "In welchem mir ein neu Vergn\u00fcgen, mich etwas aufzu-", "tokens": ["In", "wel\u00b7chem", "mir", "ein", "neu", "Ver\u00b7gn\u00fc\u00b7gen", ",", "mich", "et\u00b7was", "auf\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ART", "ADJD", "NN", "$,", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "halten, ruft,", "tokens": ["hal\u00b7ten", ",", "ruft", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVINF", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}}}}}