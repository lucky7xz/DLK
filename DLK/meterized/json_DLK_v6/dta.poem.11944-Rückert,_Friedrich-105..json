{"dta.poem.11944": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "105.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In langem Umgang kann vermeiden ganz kein Mann,", "tokens": ["In", "lan\u00b7gem", "Um\u00b7gang", "kann", "ver\u00b7mei\u00b7den", "ganz", "kein", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "VVINF", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu kr\u00e4nken und gekr\u00e4nkt zu werden dann und wann.", "tokens": ["Zu", "kr\u00e4n\u00b7ken", "und", "ge\u00b7kr\u00e4nkt", "zu", "wer\u00b7den", "dann", "und", "wann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVPP", "PTKZU", "VAINF", "ADV", "KON", "PWAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer aber weis' ist, sucht des Freunds Entschuldigung", "tokens": ["Wer", "a\u00b7ber", "weis'", "ist", ",", "sucht", "des", "Freunds", "Ent\u00b7schul\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In sich, und wer da sucht, der findet bald genung,", "tokens": ["In", "sich", ",", "und", "wer", "da", "sucht", ",", "der", "fin\u00b7det", "bald", "ge\u00b7nung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "$,", "KON", "PWS", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sieht, ob er kann verzeihn mit Ehren und Gewissen,", "tokens": ["Sieht", ",", "ob", "er", "kann", "ver\u00b7zeihn", "mit", "Eh\u00b7ren", "und", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und will um Eitelkeit ein Menschenherz nicht missen.", "tokens": ["Und", "will", "um", "Ei\u00b7tel\u00b7keit", "ein", "Men\u00b7schen\u00b7herz", "nicht", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}