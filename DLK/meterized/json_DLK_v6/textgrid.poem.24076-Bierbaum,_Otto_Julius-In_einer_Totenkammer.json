{"textgrid.poem.24076": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "In einer Totenkammer", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum bin ich von den gr\u00fcnen Wiesen gegangen", "tokens": ["Wa\u00b7rum", "bin", "ich", "von", "den", "gr\u00fc\u00b7nen", "Wie\u00b7sen", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ging aus der lieben W\u00e4rme meiner zwei braunen Sonnen?", "tokens": ["Und", "ging", "aus", "der", "lie\u00b7ben", "W\u00e4r\u00b7me", "mei\u00b7ner", "zwei", "brau\u00b7nen", "Son\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "CARD", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Da war des Lebens schenkende G\u00fcte,", "tokens": ["Da", "war", "des", "Le\u00b7bens", "schen\u00b7ken\u00b7de", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und alle Blumen bl\u00fchten da f\u00fcr mich,", "tokens": ["Und", "al\u00b7le", "Blu\u00b7men", "bl\u00fch\u00b7ten", "da", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn auch Qual in meinem Herzen war,", "tokens": ["Und", "wenn", "auch", "Qual", "in", "mei\u00b7nem", "Her\u00b7zen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vor lauter Liebe Qual:", "tokens": ["Vor", "lau\u00b7ter", "Lie\u00b7be", "Qual", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich war doch gl\u00fccklich unter hellen Himmeln,", "tokens": ["Ich", "war", "doch", "gl\u00fcck\u00b7lich", "un\u00b7ter", "hel\u00b7len", "Him\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wenn ich tief in meine Seele lauschte,", "tokens": ["Und", "wenn", "ich", "tief", "in", "mei\u00b7ne", "See\u00b7le", "lauschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vernahm ich leise Geigen und Kinderstimmen,", "tokens": ["Ver\u00b7nahm", "ich", "lei\u00b7se", "Gei\u00b7gen", "und", "Kin\u00b7der\u00b7stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Fr\u00fchlingslieder wenn auch der Herbst", "tokens": ["Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "wenn", "auch", "der", "Herbst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOUS", "ADV", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Mit hohler Stimme sein hartes Lied,", "tokens": ["Mit", "hoh\u00b7ler", "Stim\u00b7me", "sein", "har\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Sein Herrscherlied im Totentanze", "tokens": ["Sein", "Herr\u00b7scher\u00b7lied", "im", "To\u00b7ten\u00b7tan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der d\u00fcrren Bl\u00e4tter heulte: Hussa,", "tokens": ["Der", "d\u00fcr\u00b7ren", "Bl\u00e4t\u00b7ter", "heul\u00b7te", ":", "Hus\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "NE", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.12": {"text": "Der Heiland Tod, Hymen, Hymen\u00e4us!", "tokens": ["Der", "Hei\u00b7land", "Tod", ",", "Hy\u00b7men", ",", "Hy\u00b7me\u00b7n\u00e4us", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "$,", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Fr\u00fchlingslieder aus dem Rosengarten des Herzens,", "tokens": ["Fr\u00fch\u00b7lings\u00b7lie\u00b7der", "aus", "dem", "Ro\u00b7sen\u00b7gar\u00b7ten", "des", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "In dem die Engel des lachenden Lebens sangen:", "tokens": ["In", "dem", "die", "En\u00b7gel", "des", "la\u00b7chen\u00b7den", "Le\u00b7bens", "san\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Deine Liebe sangen und meine ...", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", "san\u00b7gen", "und", "mei\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "KON", "PPOSAT", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach, wie so sanft war der Sang unsrer Liebe,", "tokens": ["Ach", ",", "wie", "so", "sanft", "war", "der", "Sang", "uns\u00b7rer", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "ADJD", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sanft wie deine Blicke, mein M\u00e4dchen.", "tokens": ["Sanft", "wie", "dei\u00b7ne", "Bli\u00b7cke", ",", "mein", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Wirbelwind warf mich von gr\u00fcnen Wiesen", "tokens": ["Ein", "Wir\u00b7bel\u00b7wind", "warf", "mich", "von", "gr\u00fc\u00b7nen", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In starre, steinerne Stra\u00dfen.", "tokens": ["In", "star\u00b7re", ",", "stei\u00b7ner\u00b7ne", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Sonnen versanken, die Blumen verbl\u00fchten,", "tokens": ["Die", "Son\u00b7nen", "ver\u00b7san\u00b7ken", ",", "die", "Blu\u00b7men", "ver\u00b7bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In meinem Herzen stiert das Schweigen.", "tokens": ["In", "mei\u00b7nem", "Her\u00b7zen", "stiert", "das", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herberge bot mir der Tod. Ich liege", "tokens": ["Her\u00b7ber\u00b7ge", "bot", "mir", "der", "Tod", ".", "Ich", "lie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "In dunkler Kammer, ein blasses Weib", "tokens": ["In", "dunk\u00b7ler", "Kam\u00b7mer", ",", "ein", "blas\u00b7ses", "Weib"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ruht neben mir: tot, denn es hat keine Liebe.", "tokens": ["Ruht", "ne\u00b7ben", "mir", ":", "tot", ",", "denn", "es", "hat", "kei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$.", "ADJD", "$,", "KON", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Tot, tot, um Gott, mein Herz auch du?", "tokens": ["Tot", ",", "tot", ",", "um", "Gott", ",", "mein", "Herz", "auch", "du", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "KOUI", "NN", "$,", "PPOSAT", "NN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Kerze flackt, ihre Flamme stirbt,", "tokens": ["Die", "Ker\u00b7ze", "flackt", ",", "ih\u00b7re", "Flam\u00b7me", "stirbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schwirrt eine gro\u00dfe, schwarze Fliege matt", "tokens": ["Es", "schwirrt", "ei\u00b7ne", "gro\u00b7\u00dfe", ",", "schwar\u00b7ze", "Flie\u00b7ge", "matt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im eisig stillen Raume.", "tokens": ["Im", "ei\u00b7sig", "stil\u00b7len", "Rau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Das blasse Weib mit dem wirren Haar", "tokens": ["Das", "blas\u00b7se", "Weib", "mit", "dem", "wir\u00b7ren", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und den gr\u00fcnen Schatten unter den verbuhlten Augen, \u2013", "tokens": ["Und", "den", "gr\u00fc\u00b7nen", "Schat\u00b7ten", "un\u00b7ter", "den", "ver\u00b7buhl\u00b7ten", "Au\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Horch, wie sein Atem sich hebt.", "tokens": ["Horch", ",", "wie", "sein", "A\u00b7tem", "sich", "hebt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Oh Leben, wie weltenferne bist du mir:", "tokens": ["Oh", "Le\u00b7ben", ",", "wie", "wel\u00b7ten\u00b7fer\u00b7ne", "bist", "du", "mir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "NN", "$,", "PWAV", "ADV", "VAFIN", "PPER", "PPER", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Es liegt der Tod an meiner Seite.", "tokens": ["Es", "liegt", "der", "Tod", "an", "mei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "L\u00f6sch aus du letztes Licht in meinem Leben:", "tokens": ["L\u00f6sch", "aus", "du", "letz\u00b7tes", "Licht", "in", "mei\u00b7nem", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Heilige Erinnerung.", "tokens": ["Hei\u00b7li\u00b7ge", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.11": {"line.1": {"text": "Ueber gr\u00fcne Wiesen ein letzter Blick ...", "tokens": ["Ue\u00b7ber", "gr\u00fc\u00b7ne", "Wie\u00b7sen", "ein", "letz\u00b7ter", "Blick", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sonnen! Sonnen! Sie l\u00f6schen aus ...", "tokens": ["Son\u00b7nen", "!", "Son\u00b7nen", "!", "Sie", "l\u00f6\u00b7schen", "aus", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Da thut der Tod an meiner Seite die gr\u00fcnen Augen auf.", "tokens": ["Da", "thut", "der", "Tod", "an", "mei\u00b7ner", "Sei\u00b7te", "die", "gr\u00fc\u00b7nen", "Au\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Zwei weiche Arme pressen mich wild,", "tokens": ["Zwei", "wei\u00b7che", "Ar\u00b7me", "pres\u00b7sen", "mich", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Zwei giftige Lichter stechen in mein Herz.", "tokens": ["Zwei", "gif\u00b7ti\u00b7ge", "Lich\u00b7ter", "ste\u00b7chen", "in", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der H\u00f6lle Br\u00fcnste fressen mich. Hussa!", "tokens": ["Der", "H\u00f6l\u00b7le", "Br\u00fcns\u00b7te", "fres\u00b7sen", "mich", ".", "Hus\u00b7sa", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$.", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Heiland Tod!", "tokens": ["Der", "Hei\u00b7land", "Tod", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Es rauscht aus weiter Ferne wie ein Lied", "tokens": ["Es", "rauscht", "aus", "wei\u00b7ter", "Fer\u00b7ne", "wie", "ein", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Hunderttausenden, die gl\u00fccklich sind ...", "tokens": ["Von", "Hun\u00b7dert\u00b7tau\u00b7sen\u00b7den", ",", "die", "gl\u00fcck\u00b7lich", "sind", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}