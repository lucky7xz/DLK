{"textgrid.poem.55759": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist meine Herrin geworden,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist meine Herrin geworden,", "tokens": ["Du", "bist", "mei\u00b7ne", "Her\u00b7rin", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VAPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bacchantisch berauschendes Weib,", "tokens": ["Bac\u00b7chan\u00b7tisch", "be\u00b7rau\u00b7schen\u00b7des", "Weib", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Trink' aus, trink' aus meine Seele,", "tokens": ["Trink'", "aus", ",", "trink'", "aus", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zerst\u00f6r' den vergifteten Leib.", "tokens": ["Zer\u00b7st\u00f6r'", "den", "ver\u00b7gif\u00b7te\u00b7ten", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ich kann nicht mehr hei\u00dfer empfinden,", "tokens": ["Ich", "kann", "nicht", "mehr", "hei\u00b7\u00dfer", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich reiche zu Dir nicht hinan,", "tokens": ["Ich", "rei\u00b7che", "zu", "Dir", "nicht", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist der D\u00e4mon der Liebe", "tokens": ["Du", "bist", "der", "D\u00e4\u00b7mon", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und ich \u2013 ein sterblicher Mann.", "tokens": ["Und", "ich", "\u2013", "ein", "sterb\u00b7li\u00b7cher", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}