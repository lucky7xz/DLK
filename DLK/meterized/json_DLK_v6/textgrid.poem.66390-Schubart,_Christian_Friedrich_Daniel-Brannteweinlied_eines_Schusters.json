{"textgrid.poem.66390": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Brannteweinlied eines Schusters", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Fl\u00e4scherl, h\u00fcbsch und fein,", "tokens": ["O", "Fl\u00e4\u00b7scherl", ",", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gef\u00fcllt mit Branntewein!", "tokens": ["Ge\u00b7f\u00fcllt", "mit", "Brann\u00b7te\u00b7wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist des Wurstels Freude,", "tokens": ["Du", "bist", "des", "Wurs\u00b7tels", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bist seine Schnabelweide,", "tokens": ["Bist", "sei\u00b7ne", "Schna\u00b7bel\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gluk gluk, gluk gluk, gluk", "tokens": ["Gluk", "gluk", ",", "gluk", "gluk", ",", "gluk"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "NE", "$,", "NE", "NE", "$,", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gluk, gluk \u2013 \u2013 \u2013", "tokens": ["Gluk", ",", "gluk", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NE", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "O goldner Branntewein,", "tokens": ["O", "gold\u00b7ner", "Brann\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie s\u00fc\u00df schl\u00fcpfst du hinein!", "tokens": ["Wie", "s\u00fc\u00df", "schl\u00fcpfst", "du", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Fl\u00e4scherl, st\u00e4rke mich,", "tokens": ["O", "Fl\u00e4\u00b7scherl", ",", "st\u00e4r\u00b7ke", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Komm her, ich k\u00fcsse dich.", "tokens": ["Komm", "her", ",", "ich", "k\u00fcs\u00b7se", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sei g'scheid, mein liebes Weiberl,", "tokens": ["Sei", "g'\u00b7scheid", ",", "mein", "lie\u00b7bes", "Wei\u00b7berl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich bring' dir's zu, mein T\u00e4uberl.", "tokens": ["Ich", "bring'", "dir's", "zu", ",", "mein", "T\u00e4u\u00b7berl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gluk \u2013 \u2013 \u2013 \u2013", "tokens": ["Gluk", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NN", "$(", "$(", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Gluk \u2013 \u2013 \u2013 \u2013", "tokens": ["Gluk", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NN", "$(", "$(", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "O goldner Branntewein,", "tokens": ["O", "gold\u00b7ner", "Brann\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie s\u00fc\u00df schl\u00fcpfst du hinein!", "tokens": ["Wie", "s\u00fc\u00df", "schl\u00fcpfst", "du", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ich des Morgens trink',", "tokens": ["Wenn", "ich", "des", "Mor\u00b7gens", "trink'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich zur Arbeit flink;", "tokens": ["Bin", "ich", "zur", "Ar\u00b7beit", "flink", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kann sohlen, steppen, n\u00e4hen,", "tokens": ["Kann", "soh\u00b7len", ",", "step\u00b7pen", ",", "n\u00e4\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Draht im Takte drehen.", "tokens": ["Den", "Draht", "im", "Tak\u00b7te", "dre\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schluk, schluk \u2013 \u2013 \u2013", "tokens": ["Schluk", ",", "schluk", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Schluk \u2013 \u2013 \u2013 \u2013", "tokens": ["Schluk", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NE", "$(", "$(", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "O goldner Branntewein,", "tokens": ["O", "gold\u00b7ner", "Brann\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie s\u00fc\u00df schl\u00fcpfst du hinein.", "tokens": ["Wie", "s\u00fc\u00df", "schl\u00fcpfst", "du", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Macht mich der Gerber toll,", "tokens": ["Macht", "mich", "der", "Ger\u00b7ber", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wenn ich ihn zahlen soll;", "tokens": ["Wenn", "ich", "ihn", "zah\u00b7len", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denk' ich: hol' der Sperber", "tokens": ["So", "denk'", "ich", ":", "hol'", "der", "Sper\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Ledrer sammt dem Gerber.", "tokens": ["Den", "Led\u00b7rer", "sammt", "dem", "Ger\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gluk, gluk \u2013 \u2013 \u2013", "tokens": ["Gluk", ",", "gluk", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NE", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Gluk \u2013 \u2013 \u2013 \u2013", "tokens": ["Gluk", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NN", "$(", "$(", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Beim Gl\u00e4schen Branntewein", "tokens": ["Beim", "Gl\u00e4s\u00b7chen", "Brann\u00b7te\u00b7wein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "F\u00e4llt keine Schuld mir ein.", "tokens": ["F\u00e4llt", "kei\u00b7ne", "Schuld", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Trink' ich ein Gl\u00e4schen Spitz,", "tokens": ["Trink'", "ich", "ein", "Gl\u00e4s\u00b7chen", "Spitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Krieg' ich Verstand und Witz.", "tokens": ["Krieg'", "ich", "Ver\u00b7stand", "und", "Witz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann tanz' ich nach der Fiedel,", "tokens": ["Dann", "tanz'", "ich", "nach", "der", "Fie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sing' h\u00fcbsche deutsche Liedel.", "tokens": ["Sing'", "h\u00fcb\u00b7sche", "deut\u00b7sche", "Lie\u00b7del", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gluk, gluk \u2013 \u2013 \u2013", "tokens": ["Gluk", ",", "gluk", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "NE", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Gluk \u2013 \u2013 \u2013 \u2013", "tokens": ["Gluk", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NN", "$(", "$(", "$(", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "O goldner Branntewein,", "tokens": ["O", "gold\u00b7ner", "Brann\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wie s\u00fc\u00df schl\u00fcpfst du hinein!", "tokens": ["Wie", "s\u00fc\u00df", "schl\u00fcpfst", "du", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Fl\u00e4scherl, h\u00fcbsch und fein,", "tokens": ["O", "Fl\u00e4\u00b7scherl", ",", "h\u00fcbsch", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Sch\u00e4zerl sollst du sein,", "tokens": ["Mein", "Sch\u00e4\u00b7zerl", "sollst", "du", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will dich mit nassen Blicken", "tokens": ["Will", "dich", "mit", "nas\u00b7sen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Oft an mein Goscherl dr\u00fccken.", "tokens": ["Oft", "an", "mein", "Go\u00b7scherl", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gluk, gluk, gluk, gluk, gluk,", "tokens": ["Gluk", ",", "gluk", ",", "gluk", ",", "gluk", ",", "gluk", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Gluk, gluk, gluk, gluk, gluk.", "tokens": ["Gluk", ",", "gluk", ",", "gluk", ",", "gluk", ",", "gluk", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Es lebe h\u00fcbsch und fein", "tokens": ["Es", "le\u00b7be", "h\u00fcbsch", "und", "fein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mein Fl\u00e4scherl Branntewein!", "tokens": ["Mein", "Fl\u00e4\u00b7scherl", "Brann\u00b7te\u00b7wein", "!"], "token_info": ["word", 