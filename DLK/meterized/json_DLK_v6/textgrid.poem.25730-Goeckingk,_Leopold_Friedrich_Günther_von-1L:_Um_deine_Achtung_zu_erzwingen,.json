{"textgrid.poem.25730": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Um deine Achtung zu erzwingen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um deine Achtung zu erzwingen,", "tokens": ["Um", "dei\u00b7ne", "Ach\u00b7tung", "zu", "er\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt zwar ein Thor so oft, im Lauf'", "tokens": ["Stellt", "zwar", "ein", "Thor", "so", "oft", ",", "im", "Lauf'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ADV", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Lebens, deinen F\u00fc\u00dfen tausend Schlingen,", "tokens": ["Des", "Le\u00b7bens", ",", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "tau\u00b7send", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Allein vergeblich auf.", "tokens": ["Al\u00b7lein", "ver\u00b7geb\u00b7lich", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Narr ist darum dir nicht minder", "tokens": ["Ein", "Narr", "ist", "da\u00b7rum", "dir", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PAV", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Narr, wenn er auf Silber dich", "tokens": ["Ein", "Narr", ",", "wenn", "er", "auf", "Sil\u00b7ber", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Kaviar bewirthen will, denn blinder", "tokens": ["Mit", "Ka\u00b7vi\u00b7ar", "be\u00b7wirt\u00b7hen", "will", ",", "denn", "blin\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Macht er damit nur sich.", "tokens": ["Macht", "er", "da\u00b7mit", "nur", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "ADV", "PRF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Was k\u00fcmmert dich sein Dutzend G\u00e4ste,", "tokens": ["Was", "k\u00fcm\u00b7mert", "dich", "sein", "Dut\u00b7zend", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nachsinnend, wie es ihm den Wanst", "tokens": ["Nach\u00b7sin\u00b7nend", ",", "wie", "es", "ihm", "den", "Wanst"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit Schmeicheleien und mit L\u00fcgen m\u00e4ste?", "tokens": ["Mit", "Schmei\u00b7che\u00b7lei\u00b7en", "und", "mit", "L\u00fc\u00b7gen", "m\u00e4s\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da du nicht heucheln kannst?", "tokens": ["Da", "du", "nicht", "heu\u00b7cheln", "kannst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Inde\u00df schleicht um den Tisch des Thoren", "tokens": ["In\u00b7de\u00df", "schleicht", "um", "den", "Tisch", "des", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Der Tod sich ungesehn herum,", "tokens": ["Der", "Tod", "sich", "un\u00b7ge\u00b7sehn", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und schl\u00e4gt ihm mit der Fackel um die Ohren,", "tokens": ["Und", "schl\u00e4gt", "ihm", "mit", "der", "Fa\u00b7ckel", "um", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und wirft den Lehnstuhl um.", "tokens": ["Und", "wirft", "den", "Lehn\u00b7stuhl", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und sieh! da liegt der G\u00f6tze r\u00f6chelnd,", "tokens": ["Und", "sieh", "!", "da", "liegt", "der", "G\u00f6t\u00b7ze", "r\u00f6\u00b7chelnd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und f\u00e4hrt, wer wei\u00df wohin? und du", "tokens": ["Und", "f\u00e4hrt", ",", "wer", "wei\u00df", "wo\u00b7hin", "?", "und", "du"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWS", "VVFIN", "PWAV", "$.", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Siehst dem geborgten Leichenzuge, l\u00e4chelnd,", "tokens": ["Siehst", "dem", "ge\u00b7borg\u00b7ten", "Lei\u00b7chen\u00b7zu\u00b7ge", ",", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "An deinem Fenster zu.", "tokens": ["An", "dei\u00b7nem", "Fens\u00b7ter", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Auch du stirbst einst; dann aber bl\u00e4hen Hundert", "tokens": ["Auch", "du", "stirbst", "einst", ";", "dann", "a\u00b7ber", "bl\u00e4\u00b7hen", "Hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$.", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Der Thoren auf, ihr stolzes Unterkinn,", "tokens": ["Der", "Tho\u00b7ren", "auf", ",", "ihr", "stol\u00b7zes", "Un\u00b7ter\u00b7kinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und sehen neidisch nach, wenn du bewundert,", "tokens": ["Und", "se\u00b7hen", "nei\u00b7disch", "nach", ",", "wenn", "du", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Fortgehst \u2013 man wei\u00df, wohin.", "tokens": ["Fort\u00b7gehst", "\u2013", "man", "wei\u00df", ",", "wo\u00b7hin", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "PIS", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}