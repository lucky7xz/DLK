{"textgrid.poem.25678": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Buttler's Grabmal", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der als ein Bettler starb, liegt hier als F\u00fcrst begraben.", "tokens": ["Der", "als", "ein", "Bett\u00b7ler", "starb", ",", "liegt", "hier", "als", "F\u00fcrst", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sp\u00e4t ward sein Verdienst erkannt.", "tokens": ["Zu", "sp\u00e4t", "ward", "sein", "Ver\u00b7dienst", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl! Kein solch Monument hat doch mein Vaterland!", "tokens": ["Wohl", "!", "Kein", "solch", "Mo\u00b7nu\u00b7ment", "hat", "doch", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PIAT", "PIAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie klug wir sind! und k\u00f6nnten zwanzig haben.", "tokens": ["Wie", "klug", "wir", "sind", "!", "und", "k\u00f6nn\u00b7ten", "zwan\u00b7zig", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$.", "KON", "VMFIN", "CARD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}