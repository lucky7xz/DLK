{"textgrid.poem.47842": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Waisenkind", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin eine Rose, pfl\u00fcck mich geschwind!", "tokens": ["Ich", "bin", "ei\u00b7ne", "Ro\u00b7se", ",", "pfl\u00fcck", "mich", "ge\u00b7schwind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VVIMP", "PPER", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Blo\u00df liegen die W\u00fcrzlein dem Regen und Wind.", "tokens": ["Blo\u00df", "lie\u00b7gen", "die", "W\u00fcrz\u00b7lein", "dem", "Re\u00b7gen", "und", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Nein, geh nur vor\u00fcber und la\u00df du mich los!", "tokens": ["Nein", ",", "geh", "nur", "vor\u00b7\u00fc\u00b7ber", "und", "la\u00df", "du", "mich", "los", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "PTKVZ", "KON", "VVIMP", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich bin keine Blume, ich bin keine Ros'.", "tokens": ["Ich", "bin", "kei\u00b7ne", "Blu\u00b7me", ",", "ich", "bin", "kei\u00b7ne", "Ros'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wohl wehet mein R\u00f6cklein, wohl fa\u00dft mich der Wind;", "tokens": ["Wohl", "we\u00b7het", "mein", "R\u00f6ck\u00b7lein", ",", "wohl", "fa\u00dft", "mich", "der", "Wind", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich bin nur ein vater- und mutterlos Kind.", "tokens": ["Ich", "bin", "nur", "ein", "va\u00b7ter", "und", "mut\u00b7ter\u00b7los", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "TRUNC", "KON", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}