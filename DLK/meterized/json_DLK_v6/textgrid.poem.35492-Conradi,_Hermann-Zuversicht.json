{"textgrid.poem.35492": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Zuversicht", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat sich wilder Tatendrang", "tokens": ["Es", "hat", "sich", "wil\u00b7der", "Ta\u00b7ten\u00b7drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meiner Brust emporgereckt! ...", "tokens": ["In", "mei\u00b7ner", "Brust", "em\u00b7por\u00b7ge\u00b7reckt", "!", "..."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll der zahme, lahme Klang,", "tokens": ["Was", "soll", "der", "zah\u00b7me", ",", "lah\u00b7me", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der nimmermehr die Feigen schreckt?", "tokens": ["Der", "nim\u00b7mer\u00b7mehr", "die", "Fei\u00b7gen", "schreckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der nimmermehr die M\u00fcden rei\u00dft", "tokens": ["Der", "nim\u00b7mer\u00b7mehr", "die", "M\u00fc\u00b7den", "rei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihren Pf\u00fchlen jach empor?", "tokens": ["Von", "ih\u00b7ren", "Pf\u00fch\u00b7len", "jach", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ersteh, gewalt'ger Sch\u00f6pfergeist!", "tokens": ["Er\u00b7steh", ",", "ge\u00b7walt'\u00b7ger", "Sch\u00f6p\u00b7fer\u00b7geist", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zerrei\u00dfe, brauner Wolkenflor!", "tokens": ["Zer\u00b7rei\u00b7\u00dfe", ",", "brau\u00b7ner", "Wol\u00b7ken\u00b7flor", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Sehnsuchtsschauern lagen wir", "tokens": ["In", "Sehn\u00b7suchts\u00b7schau\u00b7ern", "la\u00b7gen", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und haben Nacht geharrt und Tag,", "tokens": ["Und", "ha\u00b7ben", "Nacht", "ge\u00b7harrt", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob sich der Zukunft Sturmpanier", "tokens": ["Ob", "sich", "der", "Zu\u00b7kunft", "Sturm\u00b7pa\u00b7nier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus falben Nebeln heben mag?", "tokens": ["Aus", "fal\u00b7ben", "Ne\u00b7beln", "he\u00b7ben", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch aber liegt's wie schweres Erz", "tokens": ["Noch", "a\u00b7ber", "liegt's", "wie", "schwe\u00b7res", "Erz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Land und Volk, wie Winterfron \u2013", "tokens": ["Auf", "Land", "und", "Volk", ",", "wie", "Win\u00b7ter\u00b7fron", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWAV", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und doch verzweifle nicht, mein Herz,", "tokens": ["Und", "doch", "ver\u00b7zweif\u00b7le", "nicht", ",", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fr\u00fchlings", "tokens": ["Die", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}