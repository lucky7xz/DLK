{"textgrid.poem.42491": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im einzelstehenden Arbeiterhaus", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im einzelstehenden Arbeiterhaus", "tokens": ["Im", "ein\u00b7zel\u00b7ste\u00b7hen\u00b7den", "Ar\u00b7bei\u00b7ter\u00b7haus"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "M\u00fcssen die Mieter schleunig hinaus:", "tokens": ["M\u00fcs\u00b7sen", "die", "Mie\u00b7ter", "schleu\u00b7nig", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es zeigen sich pl\u00f6tzlich Risse und Spalten,", "tokens": ["Es", "zei\u00b7gen", "sich", "pl\u00f6tz\u00b7lich", "Ris\u00b7se", "und", "Spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "M\u00f6rtel und Kalk wollen nicht mehr halten,", "tokens": ["M\u00f6r\u00b7tel", "und", "Kalk", "wol\u00b7len", "nicht", "mehr", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Ein leises Knistern geht unheimlich los,", "tokens": ["Ein", "lei\u00b7ses", "Knis\u00b7tern", "geht", "un\u00b7heim\u00b7lich", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Einsturzgefahr wird riesengro\u00df.", "tokens": ["Die", "Ein\u00b7sturz\u00b7ge\u00b7fahr", "wird", "rie\u00b7sen\u00b7gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Die Bewohner k\u00f6nnen nichts mehr retten,", "tokens": ["Die", "Be\u00b7woh\u00b7ner", "k\u00f6n\u00b7nen", "nichts", "mehr", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Alles bleibt drinnen, M\u00f6bel und Betten;", "tokens": ["Al\u00b7les", "bleibt", "drin\u00b7nen", ",", "M\u00f6\u00b7bel", "und", "Bet\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Kaum raffen sie noch ihr bi\u00dfchen Geld,", "tokens": ["Kaum", "raf\u00b7fen", "sie", "noch", "ihr", "bi\u00df\u00b7chen", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Eh das Geb\u00e4ude zersplittert, zerschellt.", "tokens": ["Eh", "das", "Ge\u00b7b\u00e4u\u00b7de", "zer\u00b7split\u00b7tert", ",", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Was f\u00e4llt denn der alten N\u00e4herin ein?", "tokens": ["Was", "f\u00e4llt", "denn", "der", "al\u00b7ten", "N\u00e4\u00b7he\u00b7rin", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie l\u00e4uft noch einmal ins Haus hinein,", "tokens": ["Sie", "l\u00e4uft", "noch", "ein\u00b7mal", "ins", "Haus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Um ihren Kanarienvogel zu holen.", "tokens": ["Um", "ih\u00b7ren", "Ka\u00b7na\u00b7ri\u00b7en\u00b7vo\u00b7gel", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zur\u00fcck! Schon poltern Geb\u00e4lk und Bohlen,", "tokens": ["Zu\u00b7r\u00fcck", "!", "Schon", "pol\u00b7tern", "Ge\u00b7b\u00e4lk", "und", "Boh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es l\u00f6sen sich Fugen, Klammern und Schlu\u00df,", "tokens": ["Es", "l\u00f6\u00b7sen", "sich", "Fu\u00b7gen", ",", "Klam\u00b7mern", "und", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df der Bau krachend zerst\u00e4uben mu\u00df.", "tokens": ["Da\u00df", "der", "Bau", "kra\u00b7chend", "zer\u00b7st\u00e4u\u00b7ben", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Stehn geblieben ist nur eine Wand,", "tokens": ["Stehn", "ge\u00b7blie\u00b7ben", "ist", "nur", "ei\u00b7ne", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Von unten bis oben; die widerstand.", "tokens": ["Von", "un\u00b7ten", "bis", "o\u00b7ben", ";", "die", "wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "$.", "ART", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Im vierten Stock h\u00e4ngt an der Mauer", "tokens": ["Im", "vier\u00b7ten", "Stock", "h\u00e4ngt", "an", "der", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Kanarienvogel in seinem Bauer", "tokens": ["Ein", "Ka\u00b7na\u00b7ri\u00b7en\u00b7vo\u00b7gel", "in", "sei\u00b7nem", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Und jubelt und schmettert und trillert und singt,", "tokens": ["Und", "ju\u00b7belt", "und", "schmet\u00b7tert", "und", "tril\u00b7lert", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Da\u00df es frohlockend zum Himmel klingt.", "tokens": ["Da\u00df", "es", "froh\u00b7lo\u00b7ckend", "zum", "Him\u00b7mel", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Staub und Schuttwolke sind verflogen,", "tokens": ["Staub", "und", "Schutt\u00b7wol\u00b7ke", "sind", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Die Frau ist aus den Tr\u00fcmmern gezogen,", "tokens": ["Die", "Frau", "ist", "aus", "den", "Tr\u00fcm\u00b7mern", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die treue Frau. Doch wie ein gefeiter", "tokens": ["Die", "treu\u00b7e", "Frau", ".", "Doch", "wie", "ein", "ge\u00b7fei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PWAV", "ART", "ADJA"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Singt oben und jubelt und tiriliert weiter", "tokens": ["Singt", "o\u00b7ben", "und", "ju\u00b7belt", "und", "ti\u00b7ri\u00b7liert", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Der kleine Kanarienvogel.", "tokens": ["Der", "klei\u00b7ne", "Ka\u00b7na\u00b7ri\u00b7en\u00b7vo\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}