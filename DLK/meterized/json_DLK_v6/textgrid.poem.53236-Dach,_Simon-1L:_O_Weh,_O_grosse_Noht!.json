{"textgrid.poem.53236": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Weh, O grosse Noht!", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Weh, O grosse Noht!", "tokens": ["O", "Weh", ",", "O", "gros\u00b7se", "Noht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der V\u00f6lcker Trost ist tod.", "tokens": ["Der", "V\u00f6l\u00b7cker", "Trost", "ist", "tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ChurErb, unsre Frewde,", "tokens": ["Der", "Chur", "Erb", ",", "uns\u00b7re", "Frew\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehrt schon der Sternen Reich", "tokens": ["Mehrt", "schon", "der", "Ster\u00b7nen", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und l\u00e4sst durch seine Leich'", "tokens": ["Und", "l\u00e4sst", "durch", "sei\u00b7ne", "Leich'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Uns in zu tieffem Leide!", "tokens": ["Uns", "in", "zu", "tief\u00b7fem", "Lei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Eh' dieser Stral uns schien',", "tokens": ["Eh'", "die\u00b7ser", "Stral", "uns", "schien'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ach pflagen wir umb Ihn", "tokens": ["Ach", "pfla\u00b7gen", "wir", "umb", "Ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht Tag und Nacht zu flehen?", "tokens": ["Nicht", "Tag", "und", "Nacht", "zu", "fle\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er kam, der wehrte Gast,", "tokens": ["Er", "kam", ",", "der", "wehr\u00b7te", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sungen: Gott, du hast", "tokens": ["Wir", "sun\u00b7gen", ":", "Gott", ",", "du", "hast"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Uns gn\u00e4digst angesehen!", "tokens": ["Uns", "gn\u00e4\u00b7digst", "an\u00b7ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auff solche gutte Zeit", "tokens": ["Auff", "sol\u00b7che", "gut\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat uns die Sicherheit", "tokens": ["Hat", "uns", "die", "Si\u00b7cher\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu hefftig eingenommen:", "tokens": ["Zu", "heff\u00b7tig", "ein\u00b7ge\u00b7nom\u00b7men", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drumb sind wir auch so bald", "tokens": ["Drumb", "sind", "wir", "auch", "so", "bald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Umb unsern Auffenthalt,", "tokens": ["Umb", "un\u00b7sern", "Auf\u00b7fent\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "O Hertzeleid! gekommen.", "tokens": ["O", "Hert\u00b7ze\u00b7leid", "!", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$.", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der grosse ChurF\u00fcrst weint,", "tokens": ["Der", "gros\u00b7se", "Chur", "F\u00fcrst", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die thewre Mutter scheint", "tokens": ["Die", "thew\u00b7re", "Mut\u00b7ter", "scheint"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch schier nicht Trost zu fassen;", "tokens": ["Auch", "schier", "nicht", "Trost", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Gro\u00df-Fraw-Mutter Hertz", "tokens": ["Der", "Gro\u00df\u00b7Fra\u00b7wMut\u00b7ter", "Hertz"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Birgt in sich diesen Schmertz", "tokens": ["Birgt", "in", "sich", "die\u00b7sen", "Schmertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PRF", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und seufftzet nur ohn massen.", "tokens": ["Und", "seufft\u00b7zet", "nur", "ohn", "mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das Brandenburger-Hau\u00df", "tokens": ["Das", "Bran\u00b7den\u00b7bur\u00b7ger\u00b7Hau\u00df"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sieht \u00e4ngst- und kl\u00e4glich au\u00df,", "tokens": ["Sieht", "\u00e4ngst", "und", "kl\u00e4g\u00b7lich", "au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Rein mu\u00df schwerer fliessen,", "tokens": ["Der", "Rein", "mu\u00df", "schwe\u00b7rer", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Elb, Oder, Ucker, Spree", "tokens": ["Elb", ",", "O\u00b7der", ",", "Uc\u00b7ker", ",", "Spree"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An stat des Wassers Weh'", "tokens": ["An", "stat", "des", "Was\u00b7sers", "Weh'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und heisse Thr\u00e4nen giessen.", "tokens": ["Und", "heis\u00b7se", "Thr\u00e4\u00b7nen", "gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und Preussen wolt' allein", "tokens": ["Und", "Preus\u00b7sen", "wolt'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ohn Schmertz' und Kummer seyn?", "tokens": ["Ohn", "Schmertz'", "und", "Kum\u00b7mer", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O klag f\u00fcr allen Dingen,", "tokens": ["O", "klag", "f\u00fcr", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch diesen Trawer-Stand", "tokens": ["Durch", "die\u00b7sen", "Tra\u00b7wer\u00b7Stand"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Must du auch, armes Land,", "tokens": ["Must", "du", "auch", ",", "ar\u00b7mes", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit deinem Tode ringen.", "tokens": ["Mit", "dei\u00b7nem", "To\u00b7de", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du bist vorhin nicht starck,", "tokens": ["Du", "bist", "vor\u00b7hin", "nicht", "starck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Betracht dich, schaw, dein Marck", "tokens": ["Be\u00b7tracht", "dich", ",", "schaw", ",", "dein", "Marck"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist gantz in dir verschwunden:", "tokens": ["Ist", "gantz", "in", "dir", "ver\u00b7schwun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erkenn doch die\u00df Gericht,", "tokens": ["Er\u00b7kenn", "doch", "die\u00df", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im fall du vormals nicht", "tokens": ["Im", "fall", "du", "vor\u00b7mals", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gef\u00fchlt hast deine Wunden.", "tokens": ["Ge\u00b7f\u00fchlt", "hast", "dei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Klagt beydes Weib und Mann,", "tokens": ["Klagt", "bey\u00b7des", "Weib", "und", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zieht grobe Kittel an,", "tokens": ["Zieht", "gro\u00b7be", "Kit\u00b7tel", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bestrewet euch mit Aschen,", "tokens": ["Be\u00b7stre\u00b7wet", "euch", "mit", "A\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Seyd aller Schmertzen voll,", "tokens": ["Seyd", "al\u00b7ler", "Schmert\u00b7zen", "voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Busse Seiffe soll", "tokens": ["Der", "Bus\u00b7se", "Seif\u00b7fe", "soll"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Thr\u00e4nen-Laug' euch waschen.", "tokens": ["Mit", "Thr\u00e4\u00b7nen\u00b7Laug'", "euch", "wa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wer jetzt sich fr\u00f6lich stellt,", "tokens": ["Wer", "jetzt", "sich", "fr\u00f6\u00b7lich", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "S\u00e4ufft, Gastereyen h\u00e4lt", "tokens": ["S\u00e4ufft", ",", "Gas\u00b7te\u00b7re\u00b7yen", "h\u00e4lt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Lust ihm sucht zu schaffen,", "tokens": ["Und", "Lust", "ihm", "sucht", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er sey auch wer er sey,", "tokens": ["Er", "sey", "auch", "wer", "er", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist, allem Recht nach, frey", "tokens": ["Ist", ",", "al\u00b7lem", "Recht", "nach", ",", "frey"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "PIS", "NN", "PTKVZ", "$,", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Thurn und Bann zu straffen.", "tokens": ["Mit", "Thurn", "und", "Bann", "zu", "straf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Jung, Alt und Arm und Reich", "tokens": ["Jung", ",", "Alt", "und", "Arm", "und", "Reich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "KON", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weint ewren Priestern gleich,", "tokens": ["Weint", "ew\u00b7ren", "Pries\u00b7tern", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seht \u00e4ngstig von Geberden:", "tokens": ["Seht", "\u00e4ngs\u00b7tig", "von", "Ge\u00b7ber\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-----", "measure": "dactylic.init"}, "line.4": {"text": "Schont ewrer Glieder nicht,", "tokens": ["Schont", "ew\u00b7rer", "Glie\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Liegt auff dem Angesicht,", "tokens": ["Liegt", "auff", "dem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Beschmutzt von schwartzer Erden.", "tokens": ["Be\u00b7schmutzt", "von", "schwart\u00b7zer", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sprecht: Gott, du bist gerecht,", "tokens": ["Sprecht", ":", "Gott", ",", "du", "bist", "ge\u00b7recht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir sind die b\u00f6sen Knecht'", "tokens": ["Wir", "sind", "die", "b\u00f6\u00b7sen", "Knecht'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und reiff zu allen Plagen,", "tokens": ["Und", "reiff", "zu", "al\u00b7len", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon aber deiner Schar", "tokens": ["Schon", "a\u00b7ber", "dei\u00b7ner", "Schar"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und la\u00df nicht also gar", "tokens": ["Und", "la\u00df", "nicht", "al\u00b7so", "gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ohn Gnad' uns arme schlagen.", "tokens": ["Ohn", "Gnad'", "uns", "ar\u00b7me", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wir sind Israels Hau\u00df,", "tokens": ["Wir", "sind", "Is\u00b7raels", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Sch\u00fctt deinen Eiffer au\u00df", "tokens": ["Sch\u00fctt", "dei\u00b7nen", "Eif\u00b7fer", "au\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auff die, so dich nicht kennen,", "tokens": ["Auff", "die", ",", "so", "dich", "nicht", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du siehest ja, da\u00df wir,", "tokens": ["Du", "sie\u00b7hest", "ja", ",", "da\u00df", "wir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Erbtheil, uns nach dir", "tokens": ["Dein", "E\u00b7rbtheil", ",", "uns", "nach", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und deinem Nahmen nennen.", "tokens": ["Und", "dei\u00b7nem", "Nah\u00b7men", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}}, "stanza.13": {"line.1": {"text": "Ersetz, wie dir bewust,", "tokens": ["Er\u00b7setz", ",", "wie", "dir", "be\u00b7wust", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Schaden und Verlust,", "tokens": ["Den", "Scha\u00b7den", "und", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der, so uns zwingt zu heulen:", "tokens": ["Der", ",", "so", "uns", "zwingt", "zu", "heu\u00b7len", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Richt dieses Hau\u00df empor,", "tokens": ["Richt", "die\u00b7ses", "Hau\u00df", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vermehr' es wie zuvor", "tokens": ["Ver\u00b7mehr'", "es", "wie", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "KOKOM", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und gieb ihm newe Seulen.", "tokens": ["Und", "gieb", "ihm", "ne\u00b7we", "Seu\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.14": {"line.1": {"text": "Der ChurF\u00fcrst, unsre Zier", "tokens": ["Der", "Chur", "F\u00fcrst", ",", "uns\u00b7re", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und h\u00f6chste Rhu nechst dir,", "tokens": ["Und", "h\u00f6chs\u00b7te", "Rhu", "nechst", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Empfinde newes Leben,", "tokens": ["Emp\u00b7fin\u00b7de", "ne\u00b7wes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Mutter werd' erfrewt,", "tokens": ["Die", "Mut\u00b7ter", "werd'", "er\u00b7frewt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df sie auff dieses Leid", "tokens": ["La\u00df", "sie", "auff", "die\u00b7ses", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Best\u00e4ndig' Hoffnung heben.", "tokens": ["Be\u00b7st\u00e4n\u00b7dig'", "Hoff\u00b7nung", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Des hohen Hauses Pracht", "tokens": ["Des", "ho\u00b7hen", "Hau\u00b7ses", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "M\u00fcss' auff die Tr\u00fcbni\u00df-Nacht", "tokens": ["M\u00fcss'", "auff", "die", "Tr\u00fcb\u00b7ni\u00df\u00b7Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Frewden-Licht empfinden:", "tokens": ["Ein", "Fre\u00b7wden\u00b7Licht", "emp\u00b7fin\u00b7den", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaff allen Raht und Rast,", "tokens": ["Schaff", "al\u00b7len", "Raht", "und", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was du verwundet hast,", "tokens": ["Was", "du", "ver\u00b7wun\u00b7det", "hast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Such wieder zu verbinden.", "tokens": ["Such", "wie\u00b7der", "zu", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Uns aber gieb ein Hertz,", "tokens": ["Uns", "a\u00b7ber", "gieb", "ein", "Hertz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df wir der S\u00fcnden-Schmertz,", "tokens": ["Da\u00df", "wir", "der", "S\u00fcn\u00b7den\u00b7Schmertz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ewig Weh bringt, meiden", "tokens": ["Der", "e\u00b7wig", "Weh", "bringt", ",", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tragen seine Zucht,", "tokens": ["Und", "tra\u00b7gen", "sei\u00b7ne", "Zucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die nie was b\u00f6ses sucht,", "tokens": ["Die", "nie", "was", "b\u00f6\u00b7ses", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PWS", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gedultig und bescheiden.", "tokens": ["Ge\u00b7dul\u00b7tig", "und", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Wer wei\u00df, dich kan der Noht,", "tokens": ["Wer", "wei\u00df", ",", "dich", "kan", "der", "Noht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Untergang und Tod", "tokens": ["Die", "Un\u00b7ter\u00b7gang", "und", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns ansagt, noch gerewen:", "tokens": ["Uns", "an\u00b7sagt", ",", "noch", "ge\u00b7re\u00b7wen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Auff diesen rawen Ost", "tokens": ["Auff", "die\u00b7sen", "ra\u00b7wen", "Ost"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kan deiner Sonnen Trost", "tokens": ["Kan", "dei\u00b7ner", "Son\u00b7nen", "Trost"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Segen uns erfrewen.", "tokens": ["Mit", "Se\u00b7gen", "uns", "er\u00b7fre\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "O thu' es, und vermehr", "tokens": ["O", "thu'", "es", ",", "und", "ver\u00b7mehr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So deines Nahmens Ehr'", "tokens": ["So", "dei\u00b7nes", "Nah\u00b7mens", "Ehr'"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hie und auff aller Erden;", "tokens": ["Hie", "und", "auff", "al\u00b7ler", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "PIAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Denn du erkennst, was wir", "tokens": ["Denn", "du", "er\u00b7kennst", ",", "was", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr solche Gutthat dir", "tokens": ["F\u00fcr", "sol\u00b7che", "Gut\u00b7that", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Noch schuldig bleiben werden!", "tokens": ["Noch", "schul\u00b7dig", "blei\u00b7ben", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}