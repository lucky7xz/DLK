{"textgrid.poem.48174": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Wanderrast", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier la\u00df uns ruhn; der Tag ist schw\u00fcl", "tokens": ["Hier", "la\u00df", "uns", "ruhn", ";", "der", "Tag", "ist", "schw\u00fcl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$.", "ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und weit der Weg, mein Kind.", "tokens": ["und", "weit", "der", "Weg", ",", "mein", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hier winkt ein Zeltdach schattig k\u00fchl,", "tokens": ["Hier", "winkt", "ein", "Zelt\u00b7dach", "schat\u00b7tig", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein Sammetpf\u00fchl \u2013", "tokens": ["ein", "Sam\u00b7met\u00b7pf\u00fchl", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "und leise singt der Wind.", "tokens": ["und", "lei\u00b7se", "singt", "der", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier la\u00df mich ruhn, der ruhelos", "tokens": ["Hier", "la\u00df", "mich", "ruhn", ",", "der", "ru\u00b7he\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "VVINF", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die Welt durchzog in Hast;", "tokens": ["die", "Welt", "durch\u00b7zog", "in", "Hast", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "am Silberquell im Waldesscho\u00df", "tokens": ["am", "Sil\u00b7ber\u00b7quell", "im", "Wal\u00b7des\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "auf Laub und Moos", "tokens": ["auf", "Laub", "und", "Moos"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "bl\u00fcht mir die Wanderrast.", "tokens": ["bl\u00fcht", "mir", "die", "Wan\u00b7der\u00b7rast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Nun birg dein Haupt, blondlockig Kind,", "tokens": ["Nun", "birg", "dein", "Haupt", ",", "blon\u00b7dlo\u00b7ckig", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "an meiner Brust zur Ruh;", "tokens": ["an", "mei\u00b7ner", "Brust", "zur", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der Vogel ruft, es wirft der Wind", "tokens": ["der", "Vo\u00b7gel", "ruft", ",", "es", "wirft", "der", "Wind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "uns weich und lind", "tokens": ["uns", "weich", "und", "lind"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "taufeuchte Bl\u00fcten zu.", "tokens": ["tau\u00b7feuch\u00b7te", "Bl\u00fc\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es singt der Quell ein M\u00e4rlein alt", "tokens": ["Es", "singt", "der", "Quell", "ein", "M\u00e4r\u00b7lein", "alt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vom Knaben, welcher k\u00fchn", "tokens": ["vom", "Kna\u00b7ben", ",", "wel\u00b7cher", "k\u00fchn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die Wunderblume brach und bald", "tokens": ["die", "Wun\u00b7der\u00b7blu\u00b7me", "brach", "und", "bald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "im Felsenspalt", "tokens": ["im", "Fel\u00b7sen\u00b7spalt"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "viel Sch\u00e4tze sah ergl\u00fchn. \u2013", "tokens": ["viel", "Sch\u00e4t\u00b7ze", "sah", "er\u00b7gl\u00fchn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein Haar ist Gold, es will dein Mund", "tokens": ["Dein", "Haar", "ist", "Gold", ",", "es", "will", "dein", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "rubinenrot ergl\u00fchn \u2013", "tokens": ["ru\u00b7bi\u00b7nen\u00b7rot", "er\u00b7gl\u00fchn", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mich lockte der Schatz: in Waldesgrund", "tokens": ["mich", "lock\u00b7te", "der", "Schatz", ":", "in", "Wal\u00b7des\u00b7grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "seh ich zur Stund", "tokens": ["seh", "ich", "zur", "Stund"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "die Wunderblume bl\u00fchn.", "tokens": ["die", "Wun\u00b7der\u00b7blu\u00b7me", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}