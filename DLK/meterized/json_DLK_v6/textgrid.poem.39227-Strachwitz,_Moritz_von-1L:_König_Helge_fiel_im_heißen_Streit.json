{"textgrid.poem.39227": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nig Helge fiel im hei\u00dfen Streit", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Helge fiel im hei\u00dfen Streit", "tokens": ["K\u00f6\u00b7nig", "Hel\u00b7ge", "fiel", "im", "hei\u00b7\u00dfen", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und mit ihm fiel die geliebte Maid,", "tokens": ["Und", "mit", "ihm", "fiel", "die", "ge\u00b7lieb\u00b7te", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie fiel, was mochte sie leben?", "tokens": ["Sie", "fiel", ",", "was", "moch\u00b7te", "sie", "le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "K\u00f6nig Helge, der Held, und die Maid Sigrun,", "tokens": ["K\u00f6\u00b7nig", "Hel\u00b7ge", ",", "der", "Held", ",", "und", "die", "Maid", "Sig\u00b7run", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$,", "KON", "ART", "NN", "NE", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sie mu\u00dften zu zwei im H\u00fcgel ruhn,", "tokens": ["Sie", "mu\u00df\u00b7ten", "zu", "zwei", "im", "H\u00fc\u00b7gel", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "CARD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sein Hengst, der ruhte daneben.", "tokens": ["Sein", "Hengst", ",", "der", "ruh\u00b7te", "da\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Allvater sa\u00df auf Idas Feld:", "tokens": ["All\u00b7va\u00b7ter", "sa\u00df", "auf", "I\u00b7das", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbes kommt f\u00fcrwahr ein gewaltiger Held", "tokens": ["\u00bb", "es", "kommt", "f\u00fcr\u00b7wahr", "ein", "ge\u00b7wal\u00b7ti\u00b7ger", "Held"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Noch heut von der Erde her\u00fcber,", "tokens": ["Noch", "heut", "von", "der", "Er\u00b7de", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es heult mein Wolf und fri\u00dft nicht mehr,", "tokens": ["Es", "heult", "mein", "Wolf", "und", "fri\u00dft", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NE", "KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Gjallars Br\u00fccke donnert sehr,", "tokens": ["Und", "Gjal\u00b7lars", "Br\u00fc\u00b7cke", "don\u00b7nert", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als ritt' ich selber dar\u00fcber.\u00ab", "tokens": ["Als", "ritt'", "ich", "sel\u00b7ber", "da\u00b7r\u00fc\u00b7ber", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "PAV", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "K\u00f6nig Helge trat in Odin's Palast", "tokens": ["K\u00f6\u00b7nig", "Hel\u00b7ge", "trat", "in", "O\u00b7din's", "Pa\u00b7last"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "APPR", "NE", "NE"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In schwarzem Stahl, ein finsterer Gast,", "tokens": ["In", "schwar\u00b7zem", "Stahl", ",", "ein", "fins\u00b7te\u00b7rer", "Gast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Durch die Helden schritt er stumm.", "tokens": ["Durch", "die", "Hel\u00b7den", "schritt", "er", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er schritt hindurch ohne Gru\u00df und Dank", "tokens": ["Er", "schritt", "hin\u00b7durch", "oh\u00b7ne", "Gru\u00df", "und", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und setzte sich auf die letzte Bank", "tokens": ["Und", "setz\u00b7te", "sich", "auf", "die", "letz\u00b7te", "Bank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und sah sich gar nicht um.", "tokens": ["Und", "sah", "sich", "gar", "nicht", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aufsprangen die Helden zu Spiel und Kampf,", "tokens": ["Auf\u00b7spran\u00b7gen", "die", "Hel\u00b7den", "zu", "Spiel", "und", "Kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ha Schildeskrachen und Hufgestampf,", "tokens": ["Ha", "Schil\u00b7des\u00b7kra\u00b7chen", "und", "Huf\u00b7ge\u00b7stampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie wogt es st\u00e4hlern und dicht!", "tokens": ["Wie", "wogt", "es", "st\u00e4h\u00b7lern", "und", "dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVINF", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "K\u00f6nig Helge sa\u00df, ihm scholl kein Horn,", "tokens": ["K\u00f6\u00b7nig", "Hel\u00b7ge", "sa\u00df", ",", "ihm", "scholl", "kein", "Horn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$,", "PPER", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ihm sauste kein Speer, ihm klirrte kein Sporn,", "tokens": ["Ihm", "saus\u00b7te", "kein", "Speer", ",", "ihm", "klirr\u00b7te", "kein", "Sporn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "K\u00f6nig Helge, der focht nicht.", "tokens": ["K\u00f6\u00b7nig", "Hel\u00b7ge", ",", "der", "focht", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbwohl ist er hehr, Allvater's Saal,", "tokens": ["\u00bb", "wohl", "ist", "er", "hehr", ",", "All\u00b7va\u00b7ter's", "Saal", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Boden von Gold, das Dach von Stahl,", "tokens": ["Der", "Bo\u00b7den", "von", "Gold", ",", "das", "Dach", "von", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und silbern flie\u00dft die Luft;", "tokens": ["Und", "sil\u00b7bern", "flie\u00dft", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch w\u00e4re der Himmel noch einmal so licht,", "tokens": ["Doch", "w\u00e4\u00b7re", "der", "Him\u00b7mel", "noch", "ein\u00b7mal", "so", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Den ganzen Himmel m\u00f6cht' ich nicht", "tokens": ["Den", "gan\u00b7zen", "Him\u00b7mel", "m\u00f6cht'", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Sigruns enge Gruft!\u00ab", "tokens": ["F\u00fcr", "Sig\u00b7runs", "en\u00b7ge", "Gruft", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Her trat mit Augen veilchenblau", "tokens": ["Her", "trat", "mit", "Au\u00b7gen", "veil\u00b7chen\u00b7blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die schwanenbusigste Schildjungfrau,", "tokens": ["Die", "schwa\u00b7nen\u00b7bu\u00b7sigs\u00b7te", "Schild\u00b7jung\u00b7frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie leuchtet ihr Gesicht!", "tokens": ["Wie", "leuch\u00b7tet", "ihr", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie hielt das Horn, sie trank ihm zu:", "tokens": ["Sie", "hielt", "das", "Horn", ",", "sie", "trank", "ihm", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbmein schlanker Held, nun trinke Du!\u00ab", "tokens": ["\u00bb", "mein", "schlan\u00b7ker", "Held", ",", "nun", "trin\u00b7ke", "Du", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "K\u00f6nig Helge, der trank nicht.", "tokens": ["K\u00f6\u00b7nig", "Hel\u00b7ge", ",", "der", "trank", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbund liebten mich hundert Jungfrau'n hei\u00df,", "tokens": ["\u00bb", "und", "lieb\u00b7ten", "mich", "hun\u00b7dert", "Jung\u00b7frau'n", "hei\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "CARD", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie die Hirschkuh schlank, wie das Schneehuhn wei\u00df,", "tokens": ["Wie", "die", "Hirschkuh", "schlank", ",", "wie", "das", "Schnee\u00b7huhn", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich h\u00f6be mein Auge kaum.", "tokens": ["Ich", "h\u00f6\u00b7be", "mein", "Au\u00b7ge", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du nimm Dein Horn und la\u00df mich mir,", "tokens": ["Du", "nimm", "Dein", "Horn", "und", "la\u00df", "mich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVIMP", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bist nicht halb so sch\u00f6n als Sigrunur,", "tokens": ["Bist", "nicht", "halb", "so", "sch\u00f6n", "als", "Sig\u00b7ru\u00b7nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Bei Sigrun ist mein Traum!\u00ab", "tokens": ["Bei", "Sig\u00b7run", "ist", "mein", "Traum", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So sitzt er da und trotzt und schweigt,", "tokens": ["So", "sitzt", "er", "da", "und", "trotzt", "und", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis die Mitternacht niederblickt schwarzge\u00e4ugt,", "tokens": ["Bis", "die", "Mit\u00b7ter\u00b7nacht", "nie\u00b7der\u00b7blickt", "schwarz\u00b7ge\u00b7\u00e4ugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dann ist frei der Geister Tun.", "tokens": ["Dann", "ist", "frei", "der", "Geis\u00b7ter", "Tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dann flammt sein Aug' und rauscht sein Schwert,", "tokens": ["Dann", "flammt", "sein", "Aug'", "und", "rauscht", "sein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann g\u00fcrtet er sein goldrot' Pferd;", "tokens": ["Dann", "g\u00fcr\u00b7tet", "er", "sein", "gold\u00b7rot'", "Pferd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann geht es zu Sigrun.", "tokens": ["Dann", "geht", "es", "zu", "Sig\u00b7run", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wie wild der Reiter, wie wild der Ritt,", "tokens": ["Wie", "wild", "der", "Rei\u00b7ter", ",", "wie", "wild", "der", "Ritt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PWAV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie klangvoll h\u00e4mmert des Hengstes Tritt,", "tokens": ["Wie", "klang\u00b7voll", "h\u00e4m\u00b7mert", "des", "Hengs\u00b7tes", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es geht ja zu Sigrun!", "tokens": ["Es", "geht", "ja", "zu", "Sig\u00b7run", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die Luft zerrinnt, und die Erde birst,", "tokens": ["Die", "Luft", "zer\u00b