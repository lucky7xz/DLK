{"textgrid.poem.43088": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kom\u00f6dianten ziehen vor\u00fcber,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kom\u00f6dianten ziehen vor\u00fcber,", "tokens": ["Ko\u00b7m\u00f6\u00b7di\u00b7an\u00b7ten", "zie\u00b7hen", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "W\u00fcst verwitterte Gestalten", "tokens": ["W\u00fcst", "ver\u00b7wit\u00b7ter\u00b7te", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit verblichenen Gew\u00e4ndern,", "tokens": ["Mit", "ver\u00b7bli\u00b7che\u00b7nen", "Ge\u00b7w\u00e4n\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00fcgnerisch verschminkten Falten.", "tokens": ["L\u00fcg\u00b7ne\u00b7risch", "ver\u00b7schmink\u00b7ten", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dieses \u00fcbert\u00fcnchte Elend", "tokens": ["Die\u00b7ses", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7te", "E\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese rohe Prahlerei", "tokens": ["Die\u00b7se", "ro\u00b7he", "Prah\u00b7le\u00b7rei"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist doch einmal etwas Neues", "tokens": ["Ist", "doch", "ein\u00b7mal", "et\u00b7was", "Neu\u00b7es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem eklen Einerlei.", "tokens": ["In", "dem", "ek\u00b7len", "Ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nehmt mich mit! Ich will das spielen,", "tokens": ["Nehmt", "mich", "mit", "!", "Ich", "will", "das", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VMFIN", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mich Welt und Liebe lehrte,", "tokens": ["Was", "mich", "Welt", "und", "Lie\u00b7be", "lehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr sollt euch wundern, Leute,", "tokens": ["Und", "ihr", "sollt", "euch", "wun\u00b7dern", ",", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie mein Elend ich verwerthe!", "tokens": ["Wie", "mein", "E\u00b7lend", "ich", "ver\u00b7wert\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}