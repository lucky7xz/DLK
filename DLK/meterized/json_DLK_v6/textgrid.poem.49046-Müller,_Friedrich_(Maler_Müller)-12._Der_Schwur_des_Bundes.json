{"textgrid.poem.49046": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "12. Der Schwur des Bundes", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tempel winkt zur heil'gen Bundesfeier", "tokens": ["Der", "Tem\u00b7pel", "winkt", "zur", "heil'\u00b7gen", "Bun\u00b7des\u00b7fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf dem Altare flammtgeweihtes Licht.", "tokens": ["Auf", "dem", "Al\u00b7ta\u00b7re", "flammt\u00b7ge\u00b7weih\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es webt die Nacht geheimni\u00dfvoll den Schleier,", "tokens": ["Es", "webt", "die", "Nacht", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "den", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwartung gl\u00fch't auf jedem Angesicht;", "tokens": ["Er\u00b7war\u00b7tung", "gl\u00fch't", "auf", "je\u00b7dem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es t\u00f6nen ernst die Saiten goldner Leier,", "tokens": ["Es", "t\u00f6\u00b7nen", "ernst", "die", "Sai\u00b7ten", "gold\u00b7ner", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ahndungsvoll zu dem Gef\u00fchle spricht:", "tokens": ["Die", "ahn\u00b7dungs\u00b7voll", "zu", "dem", "Ge\u00b7f\u00fch\u00b7le", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbund was der Geist im Herzen kann ergr\u00fcnden", "tokens": ["\u00bb", "und", "was", "der", "Geist", "im", "Her\u00b7zen", "kann", "er\u00b7gr\u00fcn\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PWS", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Soll jetzt der Mund dem Bunde laut verk\u00fcnden.\u00ab", "tokens": ["Soll", "jetzt", "der", "Mund", "dem", "Bun\u00b7de", "laut", "ver\u00b7k\u00fcn\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hoch strebt der Wunsch; im rauschenden Gew\u00fchle", "tokens": ["Hoch", "strebt", "der", "Wunsch", ";", "im", "rau\u00b7schen\u00b7den", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens hat nie echtes Gl\u00fcck gebl\u00fcht,", "tokens": ["Des", "Le\u00b7bens", "hat", "nie", "ech\u00b7tes", "Gl\u00fcck", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Stumm flieht die Zeit nach ihrem ew'gen Ziele,", "tokens": ["Stumm", "flieht", "die", "Zeit", "nach", "ih\u00b7rem", "ew'\u00b7gen", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schnell ist der Jugend goldner Traum vergl\u00fch't;", "tokens": ["Schnell", "ist", "der", "Ju\u00b7gend", "gold\u00b7ner", "Traum", "ver\u00b7gl\u00fch't", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dem aus dem Herzen nicht das Leben spr\u00fcht;", "tokens": ["Dem", "aus", "dem", "Her\u00b7zen", "nicht", "das", "Le\u00b7ben", "spr\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch ein Gesetz lebt f\u00fcr des Busen Triebe.", "tokens": ["Doch", "ein", "Ge\u00b7setz", "lebt", "f\u00fcr", "des", "Bu\u00b7sen", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf! Huldigt ihm, schw\u00f6r't ", "tokens": ["Auf", "!", "Hul\u00b7digt", "ihm", ",", "schw\u00f6r't"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "$.", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "In ewiger junger Fr\u00fchlingspracht verk\u00fcndet", "tokens": ["In", "e\u00b7wi\u00b7ger", "jun\u00b7ger", "Fr\u00fch\u00b7lings\u00b7pracht", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sich laut und rein des Welt-Alls Harmonie,", "tokens": ["Sich", "laut", "und", "rein", "des", "Welt\u00b7Alls", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Sternkreis, wie das Menschenherz, umwindet", "tokens": ["Den", "Stern\u00b7kreis", ",", "wie", "das", "Men\u00b7schen\u00b7herz", ",", "um\u00b7win\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Liebe mit dem Band der Sympathie,", "tokens": ["Die", "Lie\u00b7be", "mit", "dem", "Band", "der", "Sym\u00b7pa\u00b7thie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was lebt und bl\u00fcht, im Zeitenstrom verschwindet,", "tokens": ["Was", "lebt", "und", "bl\u00fcht", ",", "im", "Zei\u00b7ten\u00b7strom", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur was die Liebe heiligt, altet nie;", "tokens": ["Nur", "was", "die", "Lie\u00b7be", "hei\u00b7ligt", ",", "al\u00b7tet", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur sie strahlt Licht des Lebens Labyrinthen,", "tokens": ["Nur", "sie", "strahlt", "Licht", "des", "Le\u00b7bens", "La\u00b7by\u00b7rin\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Drum lerne fr\u00fch dein Herz im fremden finden.", "tokens": ["Drum", "ler\u00b7ne", "fr\u00fch", "dein", "Herz", "im", "frem\u00b7den", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "PPOSAT", "NN", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was hellet uns der Zukunft d\u00fcstre Tr\u00e4ume,", "tokens": ["Was", "hel\u00b7let", "uns", "der", "Zu\u00b7kunft", "d\u00fcst\u00b7re", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was hat den Muth der K\u00e4mpfenden gest\u00e4hlt?", "tokens": ["Was", "hat", "den", "Muth", "der", "K\u00e4mp\u00b7fen\u00b7den", "ge\u00b7st\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fest blickt ", "tokens": ["Fest", "blickt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Das Herz hat sich Unsterblichkeit gew\u00e4hlt,", "tokens": ["Das", "Herz", "hat", "sich", "U\u00b7nsterb\u00b7lich\u00b7keit", "ge\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Dem Guten wird das ", "tokens": ["Dem", "Gu\u00b7ten", "wird", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und was die Liebe zauberisch verbunden,", "tokens": ["Und", "was", "die", "Lie\u00b7be", "zau\u00b7be\u00b7risch", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird treu und wahr durch Ewigkeit erfunden.", "tokens": ["Wird", "treu", "und", "wahr", "durch", "E\u00b7wig\u00b7keit", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem sch\u00f6nsten Gl\u00fcck schl\u00e4gt schnell die Trenungsstunde,", "tokens": ["Dem", "sch\u00f6ns\u00b7ten", "Gl\u00fcck", "schl\u00e4gt", "schnell", "die", "Tre\u00b7nungs\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Nacht verlischt der Freude Zauberschein;", "tokens": ["In", "Nacht", "ver\u00b7lischt", "der", "Freu\u00b7de", "Zau\u00b7ber\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was gibt dem Herzen aus der Ferne Kunde", "tokens": ["Was", "gibt", "dem", "Her\u00b7zen", "aus", "der", "Fer\u00b7ne", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Siegel f\u00fcr den heiligen Verein?", "tokens": ["Zum", "Sie\u00b7gel", "f\u00fcr", "den", "hei\u00b7li\u00b7gen", "Ver\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Hoffnung fl\u00f6\u00dft mit ihrem Rosenmunde", "tokens": ["Die", "Hoff\u00b7nung", "fl\u00f6\u00dft", "mit", "ih\u00b7rem", "Ro\u00b7sen\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Sehnenden den s\u00fc\u00dfen Nectar ein:", "tokens": ["Dem", "Seh\u00b7nen\u00b7den", "den", "s\u00fc\u00b7\u00dfen", "Nec\u00b7tar", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie reicht dem Sinkenden die Strahlen-Rechte,", "tokens": ["Sie", "reicht", "dem", "Sin\u00b7ken\u00b7den", "die", "Strah\u00b7len\u00b7Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und flammet Morgenroth durch Grabes-N\u00e4chte.", "tokens": ["Und", "flam\u00b7met", "Mor\u00b7gen\u00b7roth", "durch", "Gra\u00b7bes\u00b7N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}