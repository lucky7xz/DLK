{"dta.poem.21685": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Schutzengel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Drei Geister nahten diese Nacht,", "tokens": ["Drei", "Geis\u00b7ter", "nah\u00b7ten", "die\u00b7se", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die tr\u00fcbe sinnend ich durchwacht.", "tokens": ["Die", "tr\u00fc\u00b7be", "sin\u00b7nend", "ich", "durch\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein innres Aug\u2019 sah Visionen.", "tokens": ["Mein", "inn\u00b7res", "Aug'", "sah", "Vi\u00b7si\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie thronen nicht im Sternenzelt:", "tokens": ["Sie", "thro\u00b7nen", "nicht", "im", "Ster\u00b7nen\u00b7zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sind Gesch\u00f6pfe dieser Welt,", "tokens": ["Sie", "sind", "Ge\u00b7sch\u00f6p\u00b7fe", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die als Schutzengel sie bewohnen.", "tokens": ["Die", "als", "Schut\u00b7zen\u00b7gel", "sie", "be\u00b7woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der erste Geist am Lager stand,", "tokens": ["Der", "ers\u00b7te", "Geist", "am", "La\u00b7ger", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwarz war sein Haar und sein Gewand,", "tokens": ["Schwarz", "war", "sein", "Haar", "und", "sein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach mit monotonem Laute:", "tokens": ["Und", "sprach", "mit", "mo\u00b7no\u00b7to\u00b7nem", "Lau\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich bin der Trost f\u00fcr jeden Schmerz,", "tokens": ["\u201e", "ich", "bin", "der", "Trost", "f\u00fcr", "je\u00b7den", "Schmerz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz ich weise himmelw\u00e4rts,", "tokens": ["Das", "Herz", "ich", "wei\u00b7se", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Das auf der Falschheit Schw\u00fcre baute.", "tokens": ["Das", "auf", "der", "Falschheit", "Schw\u00fc\u00b7re", "bau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Bin Vampyr, der aus wunder Brust", "tokens": ["Bin", "Vam\u00b7pyr", ",", "der", "aus", "wun\u00b7der", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar saugt das Blut der Lebenslust,", "tokens": ["Zwar", "saugt", "das", "Blut", "der", "Le\u00b7bens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch auch fortf\u00e4chelt alle Leiden;", "tokens": ["Doch", "auch", "fort\u00b7f\u00e4\u00b7chelt", "al\u00b7le", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bin Stab, der zur Erkenntni\u00df f\u00fchrt,", "tokens": ["Bin", "Stab", ",", "der", "zur", "Er\u00b7kennt\u00b7ni\u00df", "f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch welken macht, was er ber\u00fchrt \u2014", "tokens": ["Doch", "wel\u00b7ken", "macht", ",", "was", "er", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin die Mutter dieser Beiden.\u201c", "tokens": ["Ich", "bin", "die", "Mut\u00b7ter", "die\u00b7ser", "Bei\u00b7den", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie wies auf jene andern Zwei.", "tokens": ["Sie", "wies", "auf", "je\u00b7ne", "an\u00b7dern", "Zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vortrat die zweite stolz und frei", "tokens": ["Vor\u00b7trat", "die", "zwei\u00b7te", "stolz", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mit festem herrschaft-sicherm Tritte.", "tokens": ["Mit", "fes\u00b7tem", "herr\u00b7schaft\u00b7si\u00b7cherm", "Trit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie prahlte nicht mit Prunkgeschmaid,", "tokens": ["Sie", "prahl\u00b7te", "nicht", "mit", "Prunk\u00b7ge\u00b7schmaid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur einen Spiegel hielt die Maid", "tokens": ["Nur", "ei\u00b7nen", "Spie\u00b7gel", "hielt", "die", "Maid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und neigte sich in stummer Bitte.", "tokens": ["Und", "neig\u00b7te", "sich", "in", "stum\u00b7mer", "Bit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}