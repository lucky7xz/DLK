{"textgrid.poem.48412": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was soll man ferner tun? Sie ist nunmehr vorbei,", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was soll man ferner tun? Sie ist nunmehr vorbei,", "tokens": ["Was", "soll", "man", "fer\u00b7ner", "tun", "?", "Sie", "ist", "nun\u00b7mehr", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADV", "VVINF", "$.", "PPER", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das liebe sch\u00f6ne Kind. Die Augen sind entzwei;", "tokens": ["das", "lie\u00b7be", "sch\u00f6\u00b7ne", "Kind", ".", "Die", "Au\u00b7gen", "sind", "ent\u00b7zwei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "di\u00df ist der letzte Hauch, in dem die fromme Seele", "tokens": ["di\u00df", "ist", "der", "letz\u00b7te", "Hauch", ",", "in", "dem", "die", "from\u00b7me", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "aus ihrem Miethause, des keuschen Leibes H\u00f6hle,", "tokens": ["aus", "ih\u00b7rem", "Miet\u00b7hau\u00b7se", ",", "des", "keu\u00b7schen", "Lei\u00b7bes", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "in ihr recht' Vaterland, den hohen Himmel, reist.", "tokens": ["in", "ihr", "recht'", "Va\u00b7ter\u00b7land", ",", "den", "ho\u00b7hen", "Him\u00b7mel", ",", "reist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df, was hier hinterbleibt und auf die Erde weist,", "tokens": ["Di\u00df", ",", "was", "hier", "hin\u00b7ter\u00b7bleibt", "und", "auf", "die", "Er\u00b7de", "weist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "VVFIN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ihr wolgeschm\u00fcckter Leib, will hin, woher er kommen,", "tokens": ["ihr", "wol\u00b7ge\u00b7schm\u00fcck\u00b7ter", "Leib", ",", "will", "hin", ",", "wo\u00b7her", "er", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VMFIN", "ADV", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "in seiner Mutter Scho\u00df. Es hat zu sich genommen", "tokens": ["in", "sei\u00b7ner", "Mut\u00b7ter", "Scho\u00df", ".", "Es", "hat", "zu", "sich", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "PPER", "VAFIN", "APPR", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "ein iedes seinen Teil. Ihr bleichen Eltern ihr,", "tokens": ["ein", "ie\u00b7des", "sei\u00b7nen", "Teil", ".", "Ihr", "blei\u00b7chen", "El\u00b7tern", "ihr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ihr klagt nun gar zu spat! Vor war sie noch allhier,", "tokens": ["ihr", "klagt", "nun", "gar", "zu", "spat", "!", "Vor", "war", "sie", "noch", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKA", "ADJD", "$.", "APPR", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "vor war man noch in Furcht, sie w\u00fcrde nicht genesen;", "tokens": ["vor", "war", "man", "noch", "in", "Furcht", ",", "sie", "w\u00fcr\u00b7de", "nicht", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "PIS", "ADV", "APPR", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "itzt steht sie nicht mehr auf. Er ist nun da gewesen,", "tokens": ["itzt", "steht", "sie", "nicht", "mehr", "auf", ".", "Er", "ist", "nun", "da", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der Leibes Gast, der Geist. Itzt hilft kein Weinen nicht,", "tokens": ["der", "Lei\u00b7bes", "Gast", ",", "der", "Geist", ".", "Itzt", "hilft", "kein", "Wei\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$.", "ADV", "VVFIN", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "kein Bitten, keine Bu\u00df', und was man sonst verspricht", "tokens": ["kein", "Bit\u00b7ten", ",", "kei\u00b7ne", "Bu\u00df'", ",", "und", "was", "man", "sonst", "ver\u00b7spricht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "KON", "PWS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "in einer solchen Angst. Sie hat den Wundsch erf\u00fcllet,", "tokens": ["in", "ei\u00b7ner", "sol\u00b7chen", "Angst", ".", "Sie", "hat", "den", "Wund\u00b7sch", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "der doch auch eure war. Ihr Leid ist ganz gestillet", "tokens": ["der", "doch", "auch", "eu\u00b7re", "war", ".", "Ihr", "Leid", "ist", "ganz", "ge\u00b7stil\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPOSAT", "VAFIN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "und eures hebt sich an. Stillt aber eures auch,", "tokens": ["und", "eu\u00b7res", "hebt", "sich", "an", ".", "Stillt", "a\u00b7ber", "eu\u00b7res", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PRF", "PTKVZ", "$.", "VVFIN", "ADV", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "da\u00df sie recht ruhen mag! Beweist der Christen Brauch,", "tokens": ["da\u00df", "sie", "recht", "ru\u00b7hen", "mag", "!", "Be\u00b7weist", "der", "Chris\u00b7ten", "Brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$.", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "der zwar den fr\u00fchen Tod der Seinen hei\u00dft betauren,", "tokens": ["der", "zwar", "den", "fr\u00fc\u00b7hen", "Tod", "der", "Sei\u00b7nen", "hei\u00dft", "be\u00b7tau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ART", "PPOSS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "nicht aber trostlos l\u00e4\u00dft auch mitten in dem Trauren!", "tokens": ["nicht", "a\u00b7ber", "trost\u00b7los", "l\u00e4\u00dft", "auch", "mit\u00b7ten", "in", "dem", "Trau\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie unterscheiden wol, was ihr und Gottes ist,", "tokens": ["Sie", "un\u00b7ter\u00b7schei\u00b7den", "wol", ",", "was", "ihr", "und", "Got\u00b7tes", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "der mehr als Seines nichts hinwieder ihm erkiest,", "tokens": ["der", "mehr", "als", "Sei\u00b7nes", "nichts", "hin\u00b7wie\u00b7der", "ihm", "er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "PPOSAT", "PIS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "zur Unzeit und zur Zeit. Was er zuvor verborget,", "tokens": ["zur", "Un\u00b7zeit", "und", "zur", "Zeit", ".", "Was", "er", "zu\u00b7vor", "ver\u00b7bor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$.", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "das fodert er mit Recht'. Ein heidnisch Herze sorget,", "tokens": ["das", "fo\u00b7dert", "er", "mit", "Recht'", ".", "Ein", "heid\u00b7nisch", "Her\u00b7ze", "sor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "$.", "ART", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "spricht: Einem, der jung stirbt, dem ist der Himmel Feind.", "tokens": ["spricht", ":", "Ei\u00b7nem", ",", "der", "jung", "stirbt", ",", "dem", "ist", "der", "Him\u00b7mel", "Feind", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PIS", "$,", "PRELS", "ADJD", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Nicht so! Wer zeitlich f\u00e4lt, mit dem ist Gott mehr Freund.", "tokens": ["Nicht", "so", "!", "Wer", "zeit\u00b7lich", "f\u00e4lt", ",", "mit", "dem", "ist", "Gott", "mehr", "Freund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "PWS", "ADJD", "VVFIN", "$,", "APPR", "ART", "VAFIN", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Liebe ha\u00dft Verzug: ie b\u00e4lder Einer stirbet,", "tokens": ["Die", "Lie\u00b7be", "ha\u00dft", "Ver\u00b7zug", ":", "ie", "b\u00e4l\u00b7der", "Ei\u00b7ner", "stir\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$.", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "ie lieber ist er Gott. Was aber hier verdirbet,", "tokens": ["ie", "lie\u00b7ber", "ist", "er", "Gott", ".", "Was", "a\u00b7ber", "hier", "ver\u00b7dir\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "NN", "$.", "PWS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "der Leib, die Zier, die Kunst und was man sonsten liebt", "tokens": ["der", "Leib", ",", "die", "Zier", ",", "die", "Kunst", "und", "was", "man", "sons\u00b7ten", "liebt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "PWS", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "(darinnen euer Kind euch billich mehr betr\u00fcbt,", "tokens": ["(", "da\u00b7rin\u00b7nen", "eu\u00b7er", "Kind", "euch", "bil\u00b7lich", "mehr", "be\u00b7tr\u00fcbt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "dieweil sie fertig war), das folgt der Flut der Zeiten.", "tokens": ["die\u00b7weil", "sie", "fer\u00b7tig", "war", ")", ",", "das", "folgt", "der", "Flut", "der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$(", "$,", "PDS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Gott aber wird den Leib hinwieder zubereiten,", "tokens": ["Gott", "a\u00b7ber", "wird", "den", "Leib", "hin\u00b7wie\u00b7der", "zu\u00b7be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "da\u00df er soll ewig sein, da denn die Kunst und Zier,", "tokens": ["da\u00df", "er", "soll", "e\u00b7wig", "sein", ",", "da", "denn", "die", "Kunst", "und", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "VAINF", "$,", "KOUS", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "die nicht kan untergehn, wenn wir sind nicht mehr wir,", "tokens": ["die", "nicht", "kan", "un\u00b7ter\u00b7gehn", ",", "wenn", "wir", "sind", "nicht", "mehr", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VMFIN", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "PTKNEG", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "in den verkl\u00e4rten Leib wird wieder eingegossen,", "tokens": ["in", "den", "ver\u00b7kl\u00e4r\u00b7ten", "Leib", "wird", "wie\u00b7der", "ein\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "da\u00df sie gleich ewig sein. Inde\u00df habt ihr genossen", "tokens": ["da\u00df", "sie", "gleich", "e\u00b7wig", "sein", ".", "In\u00b7de\u00df", "habt", "ihr", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAINF", "$.", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "der zwar wol kurzen Zeit, da eure Tochter euch", "tokens": ["der", "zwar", "wol", "kur\u00b7zen", "Zeit", ",", "da", "eu\u00b7re", "Toch\u00b7ter", "euch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "$,", "KOUS", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von Herzen hat erfreut. Sie war an Sch\u00f6nheit reich,", "tokens": ["von", "Her\u00b7zen", "hat", "er\u00b7freut", ".", "Sie", "war", "an", "Sch\u00f6n\u00b7heit", "reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "$.", "PPER", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "an vielen Gaben hold, der Rehen zu vergleichen,", "tokens": ["an", "vie\u00b7len", "Ga\u00b7ben", "hold", ",", "der", "Re\u00b7hen", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der weisen K\u00fcnstlerin, ein ausgestecktes Zeichen", "tokens": ["der", "wei\u00b7sen", "K\u00fcnst\u00b7le\u00b7rin", ",", "ein", "aus\u00b7ge\u00b7steck\u00b7tes", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der angewandten Zucht. Vollkommen war sie schon,", "tokens": ["der", "an\u00b7ge\u00b7wand\u00b7ten", "Zucht", ".", "Voll\u00b7kom\u00b7men", "war", "sie", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "ob sie gleich war ein Kind. Drum mu\u00df sie jung davon.", "tokens": ["ob", "sie", "gleich", "war", "ein", "Kind", ".", "Drum", "mu\u00df", "sie", "jung", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ART", "NN", "$.", "PAV", "VMFIN", "PPER", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Obst, das balde reift, wird zeitlich abgenommen.", "tokens": ["Ein", "Obst", ",", "das", "bal\u00b7de", "reift", ",", "wird", "zeit\u00b7lich", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir sind von wilder Art. G\u00f6nnt ihr, zu was sie kommen,", "tokens": ["Wir", "sind", "von", "wil\u00b7der", "Art", ".", "G\u00f6nnt", "ihr", ",", "zu", "was", "sie", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$.", "VVFIN", "PPER", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und wisset, da\u00df die Zeit, die sie, als wie man sch\u00e4tzt,", "tokens": ["und", "wis\u00b7set", ",", "da\u00df", "die", "Zeit", ",", "die", "sie", ",", "als", "wie", "man", "sch\u00e4tzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "$,", "KOUS", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "allhier zu kurz gelebt, die Ewigkeit ersetzt!", "tokens": ["all\u00b7hier", "zu", "kurz", "ge\u00b7lebt", ",", "die", "E\u00b7wig\u00b7keit", "er\u00b7setzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}