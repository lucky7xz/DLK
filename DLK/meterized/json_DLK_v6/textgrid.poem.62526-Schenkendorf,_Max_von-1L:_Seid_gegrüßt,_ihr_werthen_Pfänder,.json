{"textgrid.poem.62526": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seid gegr\u00fc\u00dft, ihr werthen Pf\u00e4nder,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid gegr\u00fc\u00dft, ihr werthen Pf\u00e4nder,", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "wert\u00b7hen", "Pf\u00e4n\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Helden, die auf rauhen Wegen", "tokens": ["Hel\u00b7den", ",", "die", "auf", "rau\u00b7hen", "We\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Meere, Berge, L\u00e4nder", "tokens": ["Durch", "die", "Mee\u00b7re", ",", "Ber\u00b7ge", ",", "L\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zogen ihrem Heil entgegen!", "tokens": ["Zo\u00b7gen", "ih\u00b7rem", "Heil", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn ein Wort, ein Himmelszeichen", "tokens": ["Denn", "ein", "Wort", ",", "ein", "Him\u00b7mels\u00b7zei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in eurer Brust ein Hoffen", "tokens": ["Und", "in", "eu\u00b7rer", "Brust", "ein", "Hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df nicht wanken euch, noch weichen,", "tokens": ["Lie\u00df", "nicht", "wan\u00b7ken", "euch", ",", "noch", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Welt lag vor euch offen.", "tokens": ["Und", "die", "Welt", "lag", "vor", "euch", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier, in Deutschland angekommen,", "tokens": ["Hier", ",", "in", "Deutschland", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "D\u00fcrft ihr endlich ruhn und bleiben,", "tokens": ["D\u00fcrft", "ihr", "end\u00b7lich", "ruhn", "und", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pilger, da\u00df an euch die Frommen", "tokens": ["Pil\u00b7ger", ",", "da\u00df", "an", "euch", "die", "From\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "St\u00e4rken sich zu gutem Treiben.", "tokens": ["St\u00e4r\u00b7ken", "sich", "zu", "gu\u00b7tem", "Trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn die Zeichen wiederkehren,", "tokens": ["Wenn", "die", "Zei\u00b7chen", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kr\u00e4ft' und Zeiten sich verj\u00fcngen,", "tokens": ["Kr\u00e4ft'", "und", "Zei\u00b7ten", "sich", "ver\u00b7j\u00fcn\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns auf die Stimmen h\u00f6ren,", "tokens": ["La\u00dft", "uns", "auf", "die", "Stim\u00b7men", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die so gute Botschaft bringen.", "tokens": ["Die", "so", "gu\u00b7te", "Bot\u00b7schaft", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6rt man schon die St\u00fcrme wehen?", "tokens": ["H\u00f6rt", "man", "schon", "die", "St\u00fcr\u00b7me", "we\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle K\u00e4mpfer sind geladen \u2013", "tokens": ["Al\u00b7le", "K\u00e4mp\u00b7fer", "sind", "ge\u00b7la\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns dann entgegengehen", "tokens": ["La\u00dft", "uns", "dann", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unserm Licht auf allen Pfaden!", "tokens": ["Un\u00b7serm", "Licht", "auf", "al\u00b7len", "Pfa\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fern der Heimat, fern den Lieben,", "tokens": ["Fern", "der", "Hei\u00b7mat", ",", "fern", "den", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll Er auf dem Weg' uns finden,", "tokens": ["Soll", "Er", "auf", "dem", "Weg'", "uns", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem wir uns zum Dienst verschrieben,", "tokens": ["Dem", "wir", "uns", "zum", "Dienst", "ver\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Ankunft zu verk\u00fcnden.", "tokens": ["Sei\u00b7ne", "An\u00b7kunft", "zu", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nimmer haben noch die Geister", "tokens": ["Nim\u00b7mer", "ha\u00b7ben", "noch", "die", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also schweren Kampf bestanden;", "tokens": ["Al\u00b7so", "schwe\u00b7ren", "Kampf", "be\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zieht voran, ihr treuen Meister,", "tokens": ["Zieht", "vo\u00b7ran", ",", "ihr", "treu\u00b7en", "Meis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weise aus den Morgenlanden!", "tokens": ["Wei\u00b7se", "aus", "den", "Mor\u00b7gen\u00b7lan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df wir Alle freudig setzen", "tokens": ["Da\u00df", "wir", "Al\u00b7le", "freu\u00b7dig", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Glauben Gut und Leben,", "tokens": ["An", "den", "Glau\u00b7ben", "Gut", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ringen all' nach h\u00f6hern Sch\u00e4tzen,", "tokens": ["Rin\u00b7gen", "all'", "nach", "h\u00f6\u00b7hern", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt ihr vor dem Zuge schweben.", "tokens": ["Sollt", "ihr", "vor", "dem", "Zu\u00b7ge", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Da\u00df die Sterne niemals tr\u00fcgen,", "tokens": ["Da\u00df", "die", "Ster\u00b7ne", "nie\u00b7mals", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aarons Stab auch hier kann gr\u00fcnen,", "tokens": ["Aa\u00b7rons", "Stab", "auch", "hier", "kann", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes Wort in uns nicht l\u00fcgen,", "tokens": ["Got\u00b7tes", "Wort", "in", "uns", "nicht", "l\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollt ihr uns zur B\u00fcrgschaft dienen.", "tokens": ["Sollt", "ihr", "uns", "zur", "B\u00fcrg\u00b7schaft", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ewig hat zu Schutzpatronen", "tokens": ["E\u00b7wig", "hat", "zu", "Schutz\u00b7pat\u00b7ro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch das deutsche Volk erlesen,", "tokens": ["Euch", "das", "deut\u00b7sche", "Volk", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewig sollt ihr bei uns wohnen \u2013", "tokens": ["E\u00b7wig", "sollt", "ihr", "bei", "uns", "woh\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segnet unser Thun und Wesen!", "tokens": ["Seg\u00b7net", "un\u00b7ser", "Thun", "und", "We\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}