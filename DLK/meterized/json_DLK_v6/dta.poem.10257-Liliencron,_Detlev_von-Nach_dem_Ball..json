{"dta.poem.10257": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Ball.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Setz' in des Wagens Finsterni\u00df             ", "tokens": ["Setz'", "in", "des", "Wa\u00b7gens", "Fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getrost den Atlasschuh.", "tokens": ["Ge\u00b7trost", "den", "At\u00b7las\u00b7schuh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die F\u00fcchse sch\u00e4umen ins Gebi\u00df,", "tokens": ["Die", "F\u00fcch\u00b7se", "sch\u00e4u\u00b7men", "ins", "Ge\u00b7bi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nun, Johann, fahr\u2019 zu.", "tokens": ["Und", "nun", ",", "Jo\u00b7hann", ",", "fahr'", "zu", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "NE", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Nacht versinkt in Sumpf und Moor,", "tokens": ["Die", "Nacht", "ver\u00b7sinkt", "in", "Sumpf", "und", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein erster roter Streif.", "tokens": ["Ein", "ers\u00b7ter", "ro\u00b7ter", "Streif", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kiebitz sch\u00fcttelt sich im Rohr", "tokens": ["Der", "Kie\u00b7bitz", "sch\u00fct\u00b7telt", "sich", "im", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Schopf und Pelz den Reif.", "tokens": ["Aus", "Schopf", "und", "Pelz", "den", "Reif", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Sichel klingt vom Wiesengrund,", "tokens": ["Die", "Si\u00b7chel", "klingt", "vom", "Wie\u00b7sen\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tauber gurrt und lacht,", "tokens": ["Der", "Tau\u00b7ber", "gurrt", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Rade kl\u00e4fft der Bauernhund,", "tokens": ["Am", "Ra\u00b7de", "kl\u00e4fft", "der", "Bau\u00b7ern\u00b7hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All\u2019 Leben ist erwacht.", "tokens": ["All'", "Le\u00b7ben", "ist", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}