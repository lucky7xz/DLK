{"textgrid.poem.54221": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Beliebter Zeitvertreib, o sch\u00f6ne Dichter-Kunst!", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Beliebter Zeitvertreib, o sch\u00f6ne Dichter-Kunst!", "tokens": ["Be\u00b7lieb\u00b7ter", "Zeit\u00b7ver\u00b7treib", ",", "o", "sch\u00f6\u00b7ne", "Dich\u00b7ter\u00b7Kunst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du hast mein Hertz geraubt, und wirst auch meine Gunst,", "tokens": ["Du", "hast", "mein", "Hertz", "ge\u00b7raubt", ",", "und", "wirst", "auch", "mei\u00b7ne", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KON", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lange, bis mein Blut in Adern wird erkalten,", "tokens": ["So", "lan\u00b7ge", ",", "bis", "mein", "Blut", "in", "A\u00b7dern", "wird", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor aller andern Lust, ich schwehr es dir, behalten.", "tokens": ["Vor", "al\u00b7ler", "an\u00b7dern", "Lust", ",", "ich", "schwehr", "es", "dir", ",", "be\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann ich alleine bin, und sich mein Leib verschlie\u00dft,", "tokens": ["Wann", "ich", "al\u00b7lei\u00b7ne", "bin", ",", "und", "sich", "mein", "Leib", "ver\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VAFIN", "$,", "KON", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So wird mir doch durch dich die Einsamkeit vers\u00fc\u00dft,", "tokens": ["So", "wird", "mir", "doch", "durch", "dich", "die", "Ein\u00b7sam\u00b7keit", "ver\u00b7s\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dieweil dein holder Winck durch tausend artge Sachen,", "tokens": ["Die\u00b7weil", "dein", "hol\u00b7der", "Win\u00b7ck", "durch", "tau\u00b7send", "art\u00b7ge", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Mir lange Tage kan zu kurtzen Stunden machen.", "tokens": ["Mir", "lan\u00b7ge", "Ta\u00b7ge", "kan", "zu", "kurt\u00b7zen", "Stun\u00b7den", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nimmt Unmuth und Verdru\u00df den Kopf mir manchmahl ein,", "tokens": ["Nimmt", "Un\u00b7muth", "und", "Ver\u00b7dru\u00df", "den", "Kopf", "mir", "manch\u00b7mahl", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ART", "NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil man nicht allemahl kan froh und muthig seyn,", "tokens": ["Weil", "man", "nicht", "al\u00b7le\u00b7mahl", "kan", "froh", "und", "mut\u00b7hig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADV", "VMFIN", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So wei\u00df Calliope mir tr\u00f6stlich zuzusprechen,", "tokens": ["So", "wei\u00df", "Cal\u00b7li\u00b7o\u00b7pe", "mir", "tr\u00f6st\u00b7lich", "zu\u00b7zu\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die meiner Grillen Dunst und Nebel sucht zu brechen.", "tokens": ["Die", "mei\u00b7ner", "Gril\u00b7len", "Dunst", "und", "Ne\u00b7bel", "sucht", "zu", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Flennt Momus seinen Zahn und treibt mit mir sein Spiel,", "tokens": ["Flennt", "Mo\u00b7mus", "sei\u00b7nen", "Zahn", "und", "treibt", "mit", "mir", "sein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So lach ich selbgen aus, ergreiffe meinen Kiel,", "tokens": ["So", "lach", "ich", "selb\u00b7gen", "aus", ",", "er\u00b7greif\u00b7fe", "mei\u00b7nen", "Kiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und zeige, da\u00df man mir nur Splittergen will weisen,", "tokens": ["Und", "zei\u00b7ge", ",", "da\u00df", "man", "mir", "nur", "Split\u00b7ter\u00b7gen", "will", "wei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIS", "PPER", "ADV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie gro\u00df die Balcken offt in seinen Augen heissen.", "tokens": ["Wie", "gro\u00df", "die", "Bal\u00b7cken", "offt", "in", "sei\u00b7nen", "Au\u00b7gen", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Winckt mir denn Cypripor, der, wenn man es nicht denckt,", "tokens": ["Winckt", "mir", "denn", "Cyp\u00b7ri\u00b7por", ",", "der", ",", "wenn", "man", "es", "nicht", "denckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NE", "$,", "PRELS", "$,", "KOUS", "PIS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sich, nach der Fliegen Art, in unsre Zimmer dr\u00e4ngt,", "tokens": ["Sich", ",", "nach", "der", "Flie\u00b7gen", "Art", ",", "in", "uns\u00b7re", "Zim\u00b7mer", "dr\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "ART", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So such ich diesen Gast bey h\u00f6hnischen Gebehrden", "tokens": ["So", "such", "ich", "die\u00b7sen", "Gast", "bey", "h\u00f6h\u00b7ni\u00b7schen", "Ge\u00b7behr\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Durch Einwurff und Bewei\u00df im Schreiben lo\u00df zu werden.", "tokens": ["Durch", "Ein\u00b7wurff", "und", "Be\u00b7wei\u00df", "im", "Schrei\u00b7ben", "lo\u00df", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "PTKVZ", "PTKZU", "VAINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Mein Ph\u00f6bus wird mir doch, es giebts der Augenschein,", "tokens": ["Mein", "Ph\u00f6\u00b7bus", "wird", "mir", "doch", ",", "es", "giebts", "der", "Au\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weit angenehmer, als der kleine Knabe seyn,", "tokens": ["Weit", "an\u00b7ge\u00b7neh\u00b7mer", ",", "als", "der", "klei\u00b7ne", "Kna\u00b7be", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer bey den Musen will bem\u00fcht und embsig sitzen,", "tokens": ["Wer", "bey", "den", "Mu\u00b7sen", "will", "be\u00b7m\u00fcht", "und", "emb\u00b7sig", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "VVPP", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der l\u00e4st nach M\u00e4nnern nicht vom Buch das Auge blitzen.", "tokens": ["Der", "l\u00e4st", "nach", "M\u00e4n\u00b7nern", "nicht", "vom", "Buch", "das", "Au\u00b7ge", "blit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PTKNEG", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und st\u00f6\u00dft mir Kranckheit zu, bist du der Medicus,", "tokens": ["Und", "st\u00f6\u00dft", "mir", "Kran\u00b7ck\u00b7heit", "zu", ",", "bist", "du", "der", "Me\u00b7di\u00b7cus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "$,", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Der die Recepte schreibt, und mich curiren mu\u00df.", "tokens": ["Der", "die", "Re\u00b7cep\u00b7te", "schreibt", ",", "und", "mich", "cu\u00b7ri\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wer wolte nicht daher mit mir zugleich bekennen,", "tokens": ["Wer", "wol\u00b7te", "nicht", "da\u00b7her", "mit", "mir", "zu\u00b7gleich", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "PAV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Du w\u00e4rst was reitzendes, beliebte Kunst, zu nennen.", "tokens": ["Du", "w\u00e4rst", "was", "reit\u00b7zen\u00b7des", ",", "be\u00b7lieb\u00b7te", "Kunst", ",", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJA", "$,", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}