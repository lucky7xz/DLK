{"textgrid.poem.35593": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Geburt", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebirge: Schw\u00e4rze, Schweigen und Schnee.", "tokens": ["Ge\u00b7bir\u00b7ge", ":", "Schw\u00e4r\u00b7ze", ",", "Schwei\u00b7gen", "und", "Schnee", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Rot vom Wald niedersteigt die Jagd;", "tokens": ["Rot", "vom", "Wald", "nie\u00b7ders\u00b7teigt", "die", "Jagd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "O, die moosigen Blicke des Wilds.", "tokens": ["O", ",", "die", "moo\u00b7si\u00b7gen", "Bli\u00b7cke", "des", "Wilds", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Stille der Mutter; unter schwarzen Tannen", "tokens": ["Stil\u00b7le", "der", "Mut\u00b7ter", ";", "un\u00b7ter", "schwar\u00b7zen", "Tan\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u00d6ffnen sich die schlafenden H\u00e4nde,", "tokens": ["\u00d6ff\u00b7nen", "sich", "die", "schla\u00b7fen\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Wenn verfallen der kalte Mond erscheint.", "tokens": ["Wenn", "ver\u00b7fal\u00b7len", "der", "kal\u00b7te", "Mond", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "O, die Geburt des Menschen. N\u00e4chtlich rauscht", "tokens": ["O", ",", "die", "Ge\u00b7burt", "des", "Men\u00b7schen", ".", "N\u00e4cht\u00b7lich", "rauscht"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Blaues Wasser im Felsengrund;", "tokens": ["Blau\u00b7es", "Was\u00b7ser", "im", "Fel\u00b7sen\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Seufzend erblickt sein Bild der gefallene Engel,", "tokens": ["Seuf\u00b7zend", "er\u00b7blickt", "sein", "Bild", "der", "ge\u00b7fal\u00b7le\u00b7ne", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Erwacht ein Bleiches in dumpfer Stube.", "tokens": ["Er\u00b7wacht", "ein", "Blei\u00b7ches", "in", "dum\u00b7pfer", "Stu\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zwei Monde", "tokens": ["Zwei", "Mon\u00b7de"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ergl\u00e4nzen die Augen der steinernen Greisin.", "tokens": ["Er\u00b7gl\u00e4n\u00b7zen", "die", "Au\u00b7gen", "der", "stei\u00b7ner\u00b7nen", "Grei\u00b7sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Weh, der Geb\u00e4renden Schrei. Mit schwarzem Fl\u00fcgel", "tokens": ["Weh", ",", "der", "Ge\u00b7b\u00e4\u00b7ren\u00b7den", "Schrei", ".", "Mit", "schwar\u00b7zem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "R\u00fchrt die Knabenschl\u00e4fe die Nacht,", "tokens": ["R\u00fchrt", "die", "Kna\u00b7ben\u00b7schl\u00e4\u00b7fe", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Schnee, der leise aus purpurner Wolke sinkt.", "tokens": ["Schnee", ",", "der", "lei\u00b7se", "aus", "pur\u00b7pur\u00b7ner", "Wol\u00b7ke", "sinkt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}