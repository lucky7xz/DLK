{"dta.poem.4311": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An meinem Gebuhrts-Tage  \n  1739 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da, seitdem ich in der Welt, abermahl ein Jahr ver-", "tokens": ["Da", ",", "seit\u00b7dem", "ich", "in", "der", "Welt", ",", "a\u00b7ber\u00b7mahl", "ein", "Jahr", "ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "schwunden,", "tokens": ["schwun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und nunmehr des sechszigsten erster Tag sich eingefunden;", "tokens": ["Und", "nun\u00b7mehr", "des", "sechs\u00b7zigs\u00b7ten", "ers\u00b7ter", "Tag", "sich", "ein\u00b7ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Zieh ich billig die Gedanken ganz von andern Dingen ab,", "tokens": ["Zieh", "ich", "bil\u00b7lig", "die", "Ge\u00b7dan\u00b7ken", "ganz", "von", "an\u00b7dern", "Din\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Um, in Andacht, Dem zu danken, welcher mir mein Wesen", "tokens": ["Um", ",", "in", "An\u00b7dacht", ",", "Dem", "zu", "dan\u00b7ken", ",", "wel\u00b7cher", "mir", "mein", "We\u00b7sen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "$,", "APPR", "NN", "$,", "PDS", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "gab,", "tokens": ["gab", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Welcher mich bisher erhalten, mich beschirmet, mich ern\u00e4hrt,", "tokens": ["Wel\u00b7cher", "mich", "bis\u00b7her", "er\u00b7hal\u00b7ten", ",", "mich", "be\u00b7schir\u00b7met", ",", "mich", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Welcher mir Gesundheit, Frieden, und so manche Lust", "tokens": ["Wel\u00b7cher", "mir", "Ge\u00b7sund\u00b7heit", ",", "Frie\u00b7den", ",", "und", "so", "man\u00b7che", "Lust"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAT", "PPER", "NN", "$,", "NN", "$,", "KON", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "beschehrt!", "tokens": ["be\u00b7schehrt", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Aber, welch ein Tr\u00e4gheits-Nebel suchet mein Gem\u00fcht", "tokens": ["A\u00b7ber", ",", "welch", "ein", "Tr\u00e4g\u00b7heits\u00b7Ne\u00b7bel", "su\u00b7chet", "mein", "Ge\u00b7m\u00fcht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAT", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "zu f\u00fcllen,", "tokens": ["zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und das helle Licht der Freuden mit Gewalt fast zu ver-", "tokens": ["Und", "das", "hel\u00b7le", "Licht", "der", "Freu\u00b7den", "mit", "Ge\u00b7walt", "fast", "zu", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "ADV", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "h\u00fcllen!", "tokens": ["h\u00fcl\u00b7len", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Die, bey diesem frohen Tag, sonst versp\u00fchrte Munterkeit,", "tokens": ["Die", ",", "bey", "die\u00b7sem", "fro\u00b7hen", "Tag", ",", "sonst", "ver\u00b7sp\u00fchr\u00b7te", "Mun\u00b7ter\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "PDAT", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Das empfindliche Vergn\u00fcgen, bey der oft erlebten Zeit,", "tokens": ["Das", "emp\u00b7find\u00b7li\u00b7che", "Ver\u00b7gn\u00fc\u00b7gen", ",", "bey", "der", "oft", "er\u00b7leb\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Ist so feurig nicht, wie sonst, nicht so f\u00fchlbar, nicht so rege,", "tokens": ["Ist", "so", "feu\u00b7rig", "nicht", ",", "wie", "sonst", ",", "nicht", "so", "f\u00fchl\u00b7bar", ",", "nicht", "so", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PTKNEG", "$,", "PWAV", "ADV", "$,", "PTKNEG", "ADV", "ADJD", "$,", "PTKNEG", "ADV", "ADJA", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Die sonst fertigen Gedanken sind mehr, als sie pflegten, tr\u00e4ge.", "tokens": ["Die", "sonst", "fer\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7ken", "sind", "mehr", ",", "als", "sie", "pfleg\u00b7ten", ",", "tr\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Ach, mir ist nicht ihre Quell\u2019, noch die Ursach\u2019 unbekannt!", "tokens": ["Ach", ",", "mir", "ist", "nicht", "ih\u00b7re", "Quell'", ",", "noch", "die", "Ur\u00b7sach'", "un\u00b7be\u00b7kannt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Ich empfinde, wie so leicht hier auf Erden unser Stand", "tokens": ["Ich", "emp\u00b7fin\u00b7de", ",", "wie", "so", "leicht", "hier", "auf", "Er\u00b7den", "un\u00b7ser", "Stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ADJD", "ADV", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Der Ver\u00e4ndrung unterworfen, wie so leicht des Unfalls", "tokens": ["Der", "Ver\u00b7\u00e4n\u00b7drung", "un\u00b7ter\u00b7wor\u00b7fen", ",", "wie", "so", "leicht", "des", "Un\u00b7falls"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Wellen,", "tokens": ["Wel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Die sich \u00fcberall erheben, auf den Ocean der Welt,", "tokens": ["Die", "sich", "\u00fc\u00b7be\u00b7rall", "er\u00b7he\u00b7ben", ",", "auf", "den", "O\u00b7cean", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "VVINF", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.14": {"text": "Auch sich gegen einen jeden mit besch\u00e4umtem Brausen", "tokens": ["Auch", "sich", "ge\u00b7gen", "ei\u00b7nen", "je\u00b7den", "mit", "be\u00b7sch\u00e4um\u00b7tem", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "APPR", "ART", "PIAT", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "schwellen,", "tokens": ["schwel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Und zu st\u00fcrzen drohen k\u00f6nnen, w\u00e4r er noch so fest gestellt.", "tokens": ["Und", "zu", "st\u00fcr\u00b7zen", "dro\u00b7hen", "k\u00f6n\u00b7nen", ",", "w\u00e4r", "er", "noch", "so", "fest", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "VVINF", "VMINF", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Recht empfindlich traffen mich in dem abgewichnen Jahr", "tokens": ["Recht", "emp\u00b7find\u00b7lich", "traf\u00b7fen", "mich", "in", "dem", "ab\u00b7ge\u00b7wich\u00b7nen", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Unversehne Ungl\u00fccks-F\u00e4lle, wovon einer gnugsam war,", "tokens": ["Un\u00b7ver\u00b7seh\u00b7ne", "Un\u00b7gl\u00fccks\u00b7F\u00e4l\u00b7le", ",", "wo\u00b7von", "ei\u00b7ner", "gnug\u00b7sam", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "ART", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Mir viel gegenw\u00e4rtigs Guts, ja auch k\u00fcnftigs zu verg\u00e4llen.", "tokens": ["Mir", "viel", "ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "Guts", ",", "ja", "auch", "k\u00fcnf\u00b7tigs", "zu", "ver\u00b7g\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Es ward mancher Tag in Sorgen, \u00fcberdem noch manche", "tokens": ["Es", "ward", "man\u00b7cher", "Tag", "in", "Sor\u00b7gen", ",", "\u00fc\u00b7ber\u00b7dem", "noch", "man\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "NN", "$,", "ADV", "ADV", "PIAT"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Nacht,", "tokens": ["Nacht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Eben durch die schwehren Sorgen Ruh- und Schlaf-los", "tokens": ["E\u00b7ben", "durch", "die", "schweh\u00b7ren", "Sor\u00b7gen", "Ruh", "und", "Schlaf\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "zugebracht.", "tokens": ["zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Bis ich endlich mich entschlo\u00df alles GOtt anheim zu stellen,", "tokens": ["Bis", "ich", "end\u00b7lich", "mich", "ent\u00b7schlo\u00df", "al\u00b7les", "Gott", "an\u00b7heim", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "PIAT", "NN", "APPRART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Gott, Der alle Welt regieret,", "tokens": ["Gott", ",", "Der", "al\u00b7le", "Welt", "re\u00b7gie\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gott, Der auch das Aller\u00e4rgste allemahl zum Guten f\u00fchret,", "tokens": ["Gott", ",", "Der", "auch", "das", "Al\u00b7ler\u00b7\u00e4rgs\u00b7te", "al\u00b7le\u00b7mahl", "zum", "Gu\u00b7ten", "f\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADV", "ART", "ADJA", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Gott, ohn\u2019 welchen nichts von allen, was geschicht, gesche-", "tokens": ["Gott", ",", "ohn'", "wel\u00b7chen", "nichts", "von", "al\u00b7len", ",", "was", "ge\u00b7schicht", ",", "ge\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "APPR", "PWAT", "PIS", "APPR", "PIAT", "$,", "PRELS", "VVPP", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "hen kann.", "tokens": ["hen", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Wodurch ich in meinem Creuz einen neuen Muht gewann;", "tokens": ["Wo\u00b7durch", "ich", "in", "mei\u00b7nem", "Creuz", "ei\u00b7nen", "neu\u00b7en", "Muht", "ge\u00b7wann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Und ich finde mich beruhigt, da ich Dem mich ganz ergebe,", "tokens": ["Und", "ich", "fin\u00b7de", "mich", "be\u00b7ru\u00b7higt", ",", "da", "ich", "Dem", "mich", "ganz", "er\u00b7ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$,", "KOUS", "PPER", "ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Durch De\u00df unvermeidlich Wollen, und durch Dessen Gnad\u2019", "tokens": ["Durch", "De\u00df", "un\u00b7ver\u00b7meid\u00b7lich", "Wol\u00b7len", ",", "und", "durch", "Des\u00b7sen", "Gnad'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN", "$,", "KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "ich lebe.", "tokens": ["ich", "le\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Da mir von des H\u00f6chsten Hand so viel Gutes zugeflossen,", "tokens": ["Da", "mir", "von", "des", "H\u00f6chs\u00b7ten", "Hand", "so", "viel", "Gu\u00b7tes", "zu\u00b7ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ich, ohn\u2019 es zu verdienen, nicht zu z\u00e4hlend Guts genossen,", "tokens": ["Da", "ich", ",", "ohn'", "es", "zu", "ver\u00b7die\u00b7nen", ",", "nicht", "zu", "z\u00e4h\u00b7lend", "Guts", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,", "PTKNEG", "PTKA", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da ich noch viel Gutes hab; hab ich denn auch, fiel mir ein,", "tokens": ["Da", "ich", "noch", "viel", "Gu\u00b7tes", "hab", ";", "hab", "ich", "denn", "auch", ",", "fiel", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VAFIN", "$.", "VAFIN", "PPER", "ADV", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Recht und Fug mich zu beschwehren? Will und mu\u00df denn", "tokens": ["Recht", "und", "Fug", "mich", "zu", "be\u00b7sch