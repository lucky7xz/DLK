{"textgrid.poem.36133": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hast du wol eh' die Sterne gesehn? \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du wol eh' die Sterne gesehn? \u2013", "tokens": ["Hast", "du", "wol", "eh'", "die", "Ster\u00b7ne", "ge\u00b7sehn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOUS", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ihr'r ist so wunder viel.", "tokens": ["Ih\u00b7r'r", "ist", "so", "wun\u00b7der", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr Gesicht ist herrlich anzuschau'n,", "tokens": ["Ihr", "Ge\u00b7sicht", "ist", "herr\u00b7lich", "an\u00b7zu\u00b7schau'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auch gar zu herrlich und hehr!", "tokens": ["Auch", "gar", "zu", "herr\u00b7lich", "und", "hehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ich bin manche liebe liebe Nacht", "tokens": ["Ich", "bin", "man\u00b7che", "lie\u00b7be", "lie\u00b7be", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "ADJA", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Hinaus gegangen, und hab'", "tokens": ["Hin\u00b7aus", "ge\u00b7gan\u00b7gen", ",", "und", "hab'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "KON", "VAFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die gro\u00dfe Gotteskraft beschau't", "tokens": ["Die", "gro\u00b7\u00dfe", "Got\u00b7tes\u00b7kraft", "be\u00b7schau't"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In seinem Sterngeb\u00e4u.", "tokens": ["In", "sei\u00b7nem", "Stern\u00b7ge\u00b7b\u00e4u", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jetzt eben \u2013 es ist nach Mitternacht \u2013", "tokens": ["Jetzt", "e\u00b7ben", "\u2013", "es", "ist", "nach", "Mit\u00b7ter\u00b7nacht", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VAFIN", "APPR", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin hinaus gewest,", "tokens": ["Ich", "bin", "hin\u00b7aus", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APZR", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu denken an mein M\u00e4dchen daheim \u2013", "tokens": ["Zu", "den\u00b7ken", "an", "mein", "M\u00e4d\u00b7chen", "da\u00b7heim", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich konnt' nicht denken dran.", "tokens": ["Ich", "konnt'", "nicht", "den\u00b7ken", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich mu\u00dfte denken an Gott, den Herrn,", "tokens": ["Ich", "mu\u00df\u00b7te", "den\u00b7ken", "an", "Gott", ",", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Sternbaumeister so gro\u00df.", "tokens": ["Den", "Stern\u00b7bau\u00b7meis\u00b7ter", "so", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Ich mu\u00dfte knien, mu\u00dfte hei\u00df", "tokens": ["Ich", "mu\u00df\u00b7te", "kni\u00b7en", ",", "mu\u00df\u00b7te", "hei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "VVFIN", "$,", "VMFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Anbeten den ", "tokens": ["An\u00b7be\u00b7ten", "den"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "-+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Du Gro\u00dfer \u2013 Gro\u00dfer \u2013 \u2013 Wie ist Dein Nam'?", "tokens": ["Du", "Gro\u00b7\u00dfer", "\u2013", "Gro\u00b7\u00dfer", "\u2013", "\u2013", "Wie", "ist", "Dein", "Nam'", "?"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "NN", "$(", "$(", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er ist die ", "tokens": ["Er", "ist", "die"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dein freundlicher J\u00fcnger ", "tokens": ["Dein", "freund\u00b7li\u00b7cher", "J\u00fcn\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den J\u00fcnger hab' ich lieb.)", "tokens": ["Den", "J\u00fcn\u00b7ger", "hab'", "ich", "lieb", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bist die Liebe \u2013 Du Liebesgott,", "tokens": ["Du", "bist", "die", "Lie\u00b7be", "\u2013", "Du", "Lie\u00b7bes\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "PPER", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie sing' ich Dir dann. Wie fleu\u00dft", "tokens": ["Wie", "sing'", "ich", "Dir", "dann", ".", "Wie", "fleu\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "$.", "PWAV", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Mein Herz Dir aus, das voll ist, voll", "tokens": ["Mein", "Herz", "Dir", "aus", ",", "das", "voll", "ist", ",", "voll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Von Deinem Sterngeb\u00e4u?", "tokens": ["Von", "Dei\u00b7nem", "Stern\u00b7ge\u00b7b\u00e4u", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Nacht, die ist so klar. Es ist", "tokens": ["Die", "Nacht", ",", "die", "ist", "so", "klar", ".", "Es", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel so blau. Der Mond", "tokens": ["Der", "Him\u00b7mel", "so", "blau", ".", "Der", "Mond"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "$.", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Steht nicht daran. Das Blaue hindurch", "tokens": ["Steht", "nicht", "da\u00b7ran", ".", "Das", "Blau\u00b7e", "hin\u00b7durch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PAV", "$.", "ART", "ADJA", "PAV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Blinkt mehr denn Mondenschein.", "tokens": ["Blinkt", "mehr", "denn", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Viel tausend tausend Sterne sind's,", "tokens": ["Viel", "tau\u00b7send", "tau\u00b7send", "Ster\u00b7ne", "sin\u00b7d's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "CARD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die blitzen klein und gro\u00df", "tokens": ["Die", "blit\u00b7zen", "klein", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das sch\u00f6ne Dunkelblau hindurch \u2013", "tokens": ["Das", "sch\u00f6\u00b7ne", "Dun\u00b7kel\u00b7blau", "hin\u00b7durch", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das machte Gott, der Herr!!", "tokens": ["Das", "mach\u00b7te", "Gott", ",", "der", "Herr", "!!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich steh' und schau' empor, und schau'", "tokens": ["Ich", "steh'", "und", "schau'", "em\u00b7por", ",", "und", "schau'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sternlein all'. Sie steh'n", "tokens": ["Die", "Stern\u00b7lein", "all'", ".", "Sie", "steh'n"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PIAT", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da oben so die Kreuz und Quer,", "tokens": ["Da", "o\u00b7ben", "so", "die", "Kreuz", "und", "Quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wild und doch so wahr", "tokens": ["So", "wild", "und", "doch", "so", "wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sch\u00f6n geordnet. \u2013 F\u00fcrwahr! Das mu\u00df", "tokens": ["Und", "sch\u00f6n", "ge\u00b7ord\u00b7net", ".", "\u2013", "F\u00fcr\u00b7wahr", "!", "Das", "mu\u00df"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "$.", "$(", "NN", "$.", "PDS", "VMFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein gro\u00dfer Meister seyn,", "tokens": ["Ein", "gro\u00b7\u00dfer", "Meis\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein gro\u00dfer weiser Baumeister \u2013 Er hat's", "tokens": ["Ein", "gro\u00b7\u00dfer", "wei\u00b7ser", "Bau\u00b7meis\u00b7ter", "\u2013", "Er", "hat's"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "PPER", "VAFIN"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.8": {"text": "Auch gar zu herrlich gebau't. \u2013 \u2013", "tokens": ["Auch", "gar", "zu", "herr\u00b7lich", "ge\u00b7bau'", "t.", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "abbreviation", "punct", "punct"], "pos": ["ADV", "ADV", "PTKA", "ADJD", "VVFIN", "NE", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Dort steht ein Stern, ist gro\u00df und hell,", "tokens": ["Dort", "steht", "ein", "Stern", ",", "ist", "gro\u00df", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und brennt und leuchtet sehr \u2013", "tokens": ["Und", "brennt", "und", "leuch\u00b7tet", "sehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast du wol eh' den Stern geseh'n?", "tokens": ["Hast", "du", "wol", "eh'", "den", "Stern", "ge\u00b7seh'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er ist so werth des Aug's.", "tokens": ["Er", "ist", "so", "werth", "des", "Aug'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dort, denk' ich, mag's wol seyn, wo nun,", "tokens": ["Dort", ",", "denk'", "ich", ",", "mag's", "wol", "seyn", ",", "wo", "nun", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ADV", "VAINF", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie mir Freund ", "tokens": ["Wie", "mir", "Freund"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Mit seinen J\u00fcngern all'.", "tokens": ["Mit", "sei\u00b7nen", "J\u00fcn\u00b7gern", "all'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Du den klaren Stern", "tokens": ["Der", "Du", "den", "kla\u00b7ren", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dort hoch bewandelst, blick' auf mich", "tokens": ["Dort", "hoch", "be\u00b7wan\u00b7delst", ",", "blick'", "auf", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Deinen J\u00fcngern all'.", "tokens": ["Mit", "Dei\u00b7nen", "J\u00fcn\u00b7gern", "all'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Blick' her, wie hier im bereiften Gras", "tokens": ["Blick'", "her", ",", "wie", "hier", "im", "be\u00b7reif\u00b7ten", "Gras"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich niederknie vor Dir,", "tokens": ["Ich", "nie\u00b7der\u00b7knie", "vor", "Dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und bete: Du seyst, Wundermann,", "tokens": ["Und", "be\u00b7te", ":", "Du", "seyst", ",", "Wun\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VAFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Sternbaumeister so lieb! \u2013", "tokens": ["Dem", "Stern\u00b7bau\u00b7meis\u00b7ter", "so", "lieb", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Auch steht vom hellen Stern nicht weit", "tokens": ["Auch", "steht", "vom", "hel\u00b7len", "Stern", "nicht", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein andrer Stern. Er sieht", "tokens": ["Ein", "an\u00b7drer", "Stern", ".", "Er", "sieht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So tr\u00fcb', so tr\u00fcb'. Sein Antlitz ist", "tokens": ["So", "tr\u00fcb'", ",", "so", "tr\u00fcb'", ".", "Sein", "Ant\u00b7litz", "i