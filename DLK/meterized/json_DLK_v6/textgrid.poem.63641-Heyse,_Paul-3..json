{"textgrid.poem.63641": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und doch, ein Christfest war auch uns beschieden;", "tokens": ["Und", "doch", ",", "ein", "Christ\u00b7fest", "war", "auch", "uns", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein nordisch lust'ger Tannenbaum, statt dessen", "tokens": ["Kein", "nor\u00b7disch", "lust'\u00b7ger", "Tan\u00b7nen\u00b7baum", ",", "statt", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJD", "ADJA", "NN", "$,", "KOUI", "PDS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein ganzer Hain hochragender Zypressen", "tokens": ["Ein", "gan\u00b7zer", "Hain", "hoch\u00b7ra\u00b7gen\u00b7der", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Fu\u00df der stillsten aller Pyramiden", "tokens": ["Am", "Fu\u00df", "der", "stills\u00b7ten", "al\u00b7ler", "Py\u00b7ra\u00b7mi\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir gingen langsam durch den Todesfrieden", "tokens": ["Wir", "gin\u00b7gen", "lang\u00b7sam", "durch", "den", "To\u00b7des\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lasen alte Namen, meist vergessen,", "tokens": ["Und", "la\u00b7sen", "al\u00b7te", "Na\u00b7men", ",", "meist", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von K\u00e4mpfern, die schon lang die Bahn durchmessen", "tokens": ["Von", "K\u00e4mp\u00b7fern", ",", "die", "schon", "lang", "die", "Bahn", "durch\u00b7mes\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und narbenvoll aus dem Get\u00fcmmel schieden.", "tokens": ["Und", "nar\u00b7ben\u00b7voll", "aus", "dem", "Ge\u00b7t\u00fcm\u00b7mel", "schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Her\u00fcber sah von fern durch grauen Duft", "tokens": ["Her\u00b7\u00fc\u00b7ber", "sah", "von", "fern", "durch", "grau\u00b7en", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Kapitol, ein Riesenhaupt, ergraut,", "tokens": ["Das", "Ka\u00b7pi\u00b7tol", ",", "ein", "Rie\u00b7sen\u00b7haupt", ",", "er\u00b7graut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil es Geburt und Tod mu\u00df \u00fcberdauern.", "tokens": ["Weil", "es", "Ge\u00b7burt", "und", "Tod", "mu\u00df", "\u00fc\u00b7berd\u00b7au\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zwei Veilchen pfl\u00fccktest du von einer Gruft", "tokens": ["Zwei", "Veil\u00b7chen", "pfl\u00fcck\u00b7test", "du", "von", "ei\u00b7ner", "Gruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und brachst in Tr\u00e4nen aus, als pl\u00f6tzlich laut", "tokens": ["Und", "brachst", "in", "Tr\u00e4\u00b7nen", "aus", ",", "als", "pl\u00f6tz\u00b7lich", "laut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "KOUS", "ADJD", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die V\u00f6gel sangen auf den Gartenmauern.", "tokens": ["Die", "V\u00f6\u00b7gel", "san\u00b7gen", "auf", "den", "Gar\u00b7ten\u00b7mau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}