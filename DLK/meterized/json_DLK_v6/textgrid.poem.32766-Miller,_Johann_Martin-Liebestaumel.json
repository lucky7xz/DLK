{"textgrid.poem.32766": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Liebestaumel", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was gehn mich Erd' und Himmel an,", "tokens": ["Was", "gehn", "mich", "Erd'", "und", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich die Holde sehen kann?", "tokens": ["Wenn", "ich", "die", "Hol\u00b7de", "se\u00b7hen", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herab zu mir, herabgebracht", "tokens": ["Her\u00b7ab", "zu", "mir", ",", "her\u00b7ab\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Paradies durch Liebes Macht!", "tokens": ["Ist", "Pa\u00b7ra\u00b7dies", "durch", "Lie\u00b7bes", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Lach mir, du blaues Auge, du!", "tokens": ["Lach", "mir", ",", "du", "blau\u00b7es", "Au\u00b7ge", ",", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Raub meinem Herzen alle Ruh'!", "tokens": ["Raub", "mei\u00b7nem", "Her\u00b7zen", "al\u00b7le", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schwimm' im Liebesmeer dahin,", "tokens": ["Ich", "schwimm'", "im", "Lie\u00b7bes\u00b7meer", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch ist mir so wohl im Sinn!", "tokens": ["Und", "doch", "ist", "mir", "so", "wohl", "im", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df k\u00fcssen, la\u00df umarmen dich!", "tokens": ["La\u00df", "k\u00fcs\u00b7sen", ",", "la\u00df", "um\u00b7ar\u00b7men", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Paradieses Wonn' um mich!", "tokens": ["O", "Pa\u00b7ra\u00b7die\u00b7ses", "Wonn'", "um", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df leben ewig mich bei dir!", "tokens": ["La\u00df", "le\u00b7ben", "e\u00b7wig", "mich", "bei", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ADJD", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst gieb den Tod, du Holde, mir!", "tokens": ["Sonst", "gieb", "den", "Tod", ",", "du", "Hol\u00b7de", ",", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "$,", "PPER", "NN", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}