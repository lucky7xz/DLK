{"dta.poem.7510": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sekten alle sind im Glauben einverstanden,", "tokens": ["Die", "Sek\u00b7ten", "al\u00b7le", "sind", "im", "Glau\u00b7ben", "ein\u00b7ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es sei ein h\u00f6chstes Gut zu suchen und vorhanden.", "tokens": ["Es", "sei", "ein", "h\u00f6chs\u00b7tes", "Gut", "zu", "su\u00b7chen", "und", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo es zu finden sei, das ist die erste Spaltung,", "tokens": ["Wo", "es", "zu", "fin\u00b7den", "sei", ",", "das", "ist", "die", "ers\u00b7te", "Spal\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKZU", "VVINF", "VAFIN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wie zu suchen? das des weitern Streits Entfaltung.", "tokens": ["Und", "wie", "zu", "su\u00b7chen", "?", "das", "des", "wei\u00b7tern", "Streits", "Ent\u00b7fal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PTKZU", "VVINF", "$.", "ART", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der eine steckte hoch das Ziel, der andre tiefer,", "tokens": ["Der", "ei\u00b7ne", "steck\u00b7te", "hoch", "das", "Ziel", ",", "der", "and\u00b7re", "tie\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "ART", "NN", "$,", "PRELS", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Danach nach seiner Kraft dann kroch er oder lief er.", "tokens": ["Da\u00b7nach", "nach", "sei\u00b7ner", "Kraft", "dann", "kroch", "er", "o\u00b7der", "lief", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Niedrigste wird auch nach etwas H\u00f6chstem geizen,", "tokens": ["Der", "Nied\u00b7rigs\u00b7te", "wird", "auch", "nach", "et\u00b7was", "H\u00f6chs\u00b7tem", "gei\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das h\u00f6chste H\u00f6chste kann den h\u00f6chsten Sinn nur reizen.", "tokens": ["Das", "h\u00f6chs\u00b7te", "H\u00f6chs\u00b7te", "kann", "den", "h\u00f6chs\u00b7ten", "Sinn", "nur", "rei\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein H\u00f6chstes ist Genu\u00df, ein H\u00f6h'res sel'ge Ruh;", "tokens": ["Ein", "H\u00f6chs\u00b7tes", "ist", "Ge\u00b7nu\u00df", ",", "ein", "H\u00f6h'\u00b7res", "sel'\u00b7ge", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dir das H\u00f6chste gilt, Erkenntnis suche du.", "tokens": ["Was", "dir", "das", "H\u00f6chs\u00b7te", "gilt", ",", "Er\u00b7kennt\u00b7nis", "su\u00b7che", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "VVFIN", "$,", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "In der Erkenntnis ist Genu\u00df das Suchen schon,", "tokens": ["In", "der", "Er\u00b7kennt\u00b7nis", "ist", "Ge\u00b7nu\u00df", "das", "Su\u00b7chen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einst wird sel'ge Ruh seyn der gefundne Lohn;", "tokens": ["Und", "einst", "wird", "sel'\u00b7ge", "Ruh", "seyn", "der", "ge\u00b7fund\u00b7ne", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJA", "NN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn alles du als gut im h\u00f6chsten Gut erkennst,", "tokens": ["Wenn", "al\u00b7les", "du", "als", "gut", "im", "h\u00f6chs\u00b7ten", "Gut", "er\u00b7kennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "KOUS", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen b\u00f6sen Schein allein das B\u00f6se nennst.", "tokens": ["Und", "ei\u00b7nen", "b\u00f6\u00b7sen", "Schein", "al\u00b7lein", "das", "B\u00f6\u00b7se", "nennst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Inzwischen mu\u00dft du Gut und B\u00f6ses unterscheiden,", "tokens": ["I\u00b7nzwi\u00b7schen", "mu\u00dft", "du", "Gut", "und", "B\u00f6\u00b7ses", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "NN", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und f\u00fcr das Gute selbst den Schein des B\u00f6sen meiden.", "tokens": ["Und", "f\u00fcr", "das", "Gu\u00b7te", "selbst", "den", "Schein", "des", "B\u00f6\u00b7sen", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Erkenntnis, Ruh, Genu\u00df, ist nie bei b\u00f6sem Muth;", "tokens": ["Er\u00b7kennt\u00b7nis", ",", "Ruh", ",", "Ge\u00b7nu\u00df", ",", "ist", "nie", "bei", "b\u00f6\u00b7sem", "Muth", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur auf des Guten Pfad kommst du zum h\u00f6chsten Gut.", "tokens": ["Nur", "auf", "des", "Gu\u00b7ten", "Pfad", "kommst", "du", "zum", "h\u00f6chs\u00b7ten", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}