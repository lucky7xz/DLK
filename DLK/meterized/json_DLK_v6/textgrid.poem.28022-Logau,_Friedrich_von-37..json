{"textgrid.poem.28022": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein gr\u00f6sser Unrecht wird Juristen angethan,", "tokens": ["Kein", "gr\u00f6s\u00b7ser", "Un\u00b7recht", "wird", "Ju\u00b7ris\u00b7ten", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wann ein ieder Recht erweiset iederman,", "tokens": ["Als", "wann", "ein", "ie\u00b7der", "Recht", "er\u00b7wei\u00b7set", "ie\u00b7der\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "PIAT", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ihnen Unrecht recht. Wann Unrecht wo nicht w\u00e4r,", "tokens": ["Weil", "ih\u00b7nen", "Un\u00b7recht", "recht", ".", "Wann", "Un\u00b7recht", "wo", "nicht", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "$.", "PWAV", "NN", "PWAV", "PTKNEG", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00e4r zwar ihr Buch voll Recht, ihr Beutel aber leer.", "tokens": ["W\u00e4r", "zwar", "ihr", "Buch", "voll", "Recht", ",", "ihr", "Beu\u00b7tel", "a\u00b7ber", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "NN", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}