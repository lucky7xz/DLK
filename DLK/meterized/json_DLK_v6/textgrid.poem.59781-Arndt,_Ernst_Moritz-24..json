{"textgrid.poem.59781": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Eisen sinkt im Meer,", "tokens": ["Das", "Ei\u00b7sen", "sinkt", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch wei\u00dft du's auszuweiten,", "tokens": ["Doch", "wei\u00dft", "du's", "aus\u00b7zu\u00b7wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So kann's auf Wogen reiten", "tokens": ["So", "kann's", "auf", "Wo\u00b7gen", "rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als leichtes Schiff einher.", "tokens": ["Als", "leich\u00b7tes", "Schiff", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ist, o Mensch, dein Mut \u2013", "tokens": ["So", "ist", ",", "o", "Mensch", ",", "dein", "Mut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df er nicht schwer verd\u00e4mmre,", "tokens": ["Da\u00df", "er", "nicht", "schwer", "ver\u00b7d\u00e4mm\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schlag rastlos drauf und h\u00e4mmre,", "tokens": ["Schlag", "rast\u00b7los", "drauf", "und", "h\u00e4mm\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Halt frisch der Schmiede Glut!", "tokens": ["Halt", "frisch", "der", "Schmie\u00b7de", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}