{"textgrid.poem.65057": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stern am Himmelsbogen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stern am Himmelsbogen", "tokens": ["Stern", "am", "Him\u00b7mels\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schimmernd silberblank,", "tokens": ["Schim\u00b7mernd", "sil\u00b7ber\u00b7blank", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn in's Grab der Wogen", "tokens": ["Wenn", "in's", "Grab", "der", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "L\u00e4ngst die Sonne sank!", "tokens": ["L\u00e4ngst", "die", "Son\u00b7ne", "sank", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ton der Aeolsharfe,", "tokens": ["Ton", "der", "A\u00b7e\u00b7ols\u00b7har\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ach, nur dann erlauscht,", "tokens": ["Ach", ",", "nur", "dann", "er\u00b7lauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn der Nord, der scharfe,", "tokens": ["Wenn", "der", "Nord", ",", "der", "schar\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wild dar\u00fcber rauscht.", "tokens": ["Wild", "da\u00b7r\u00fc\u00b7ber", "rauscht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PAV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Duft im Blumenkelche", "tokens": ["Duft", "im", "Blu\u00b7men\u00b7kel\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schlummernd lind und leis,", "tokens": ["Schlum\u00b7mernd", "lind", "und", "leis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tiefe Lieb', um welche", "tokens": ["Tie\u00b7fe", "Lieb'", ",", "um", "wel\u00b7che"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PRELS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gott allein nur wei\u00df.", "tokens": ["Gott", "al\u00b7lein", "nur", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Still und ungefeiert", "tokens": ["Still", "und", "un\u00b7ge\u00b7fei\u00b7ert"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Durch die Reihen dicht,", "tokens": ["Durch", "die", "Rei\u00b7hen", "dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wall'st du, ein verschleiert", "tokens": ["Wall'st", "du", ",", "ein", "ver\u00b7schlei\u00b7ert"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "ART", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Engelsangesicht! \u2013", "tokens": ["En\u00b7gel\u00b7san\u00b7ge\u00b7sicht", "!", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}