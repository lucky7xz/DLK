{"textgrid.poem.52948": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, M\u00e4dchen, mich Dein Herz dem\u00fctig k\u00fcssen,", "tokens": ["La\u00df", ",", "M\u00e4d\u00b7chen", ",", "mich", "Dein", "Herz", "de\u00b7m\u00fc\u00b7tig", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wiege Du mit reinen Liebes-Armen", "tokens": ["Und", "wie\u00b7ge", "Du", "mit", "rei\u00b7nen", "Lie\u00b7bes\u00b7Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Haupt in Deinem jungfr\u00e4ulichen Scho\u00dfe!", "tokens": ["Mein", "Haupt", "in", "Dei\u00b7nem", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Scho\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor Dir m\u00f6cht ich mein ganzes Unrecht b\u00fc\u00dfen,", "tokens": ["Vor", "Dir", "m\u00f6cht", "ich", "mein", "gan\u00b7zes", "Un\u00b7recht", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du w\u00fcrdest meiner Schuld Dich mild erbarmen", "tokens": ["Du", "w\u00fcr\u00b7dest", "mei\u00b7ner", "Schuld", "Dich", "mild", "er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und mich vers\u00f6hnen mit dem Dichter-Lose,", "tokens": ["Und", "mich", "ver\u00b7s\u00f6h\u00b7nen", "mit", "dem", "Dich\u00b7ter\u00b7Lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In Dornen eine Rose.", "tokens": ["In", "Dor\u00b7nen", "ei\u00b7ne", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich zweifelte an Weiber-Lieb und Treue,", "tokens": ["Ich", "zwei\u00b7fel\u00b7te", "an", "Wei\u00b7ber\u00b7Lieb", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "An Freund und Feind, an Gott und meines Gleichen;", "tokens": ["An", "Freund", "und", "Feind", ",", "an", "Gott", "und", "mei\u00b7nes", "Glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nun f\u00fchl ich wieder Sehnsucht, Schmerz und Reue", "tokens": ["Nun", "f\u00fchl", "ich", "wie\u00b7der", "Sehn\u00b7sucht", ",", "Schmerz", "und", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wie Fr\u00fchlingsatem schmeichelnd mich beschleichen,", "tokens": ["Wie", "Fr\u00fch\u00b7lin\u00b7gsa\u00b7tem", "schmei\u00b7chelnd", "mich", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und die mir Lieb' in Jahren schlug, die Wunden,", "tokens": ["Und", "die", "mir", "Lieb'", "in", "Jah\u00b7ren", "schlug", ",", "die", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "NN", "APPR", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die Liebe heilt sie ach! in wenig Stunden.", "tokens": ["Die", "Lie\u00b7be", "heilt", "sie", "ach", "!", "in", "we\u00b7nig", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}