{"textgrid.poem.45845": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[o lieber Gott, so mild und lind]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O lieber Gott, so mild und lind,", "tokens": ["O", "lie\u00b7ber", "Gott", ",", "so", "mild", "und", "lind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du schlie\u00dfest mit Erbarmen,", "tokens": ["Du", "schlie\u00b7\u00dfest", "mit", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kinder all, die Waisen sind,", "tokens": ["Die", "Kin\u00b7der", "all", ",", "die", "Wai\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deine Vaterarmen.", "tokens": ["In", "dei\u00b7ne", "Va\u00b7te\u00b7rar\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Siehst nieder in der stillen Nacht", "tokens": ["Siehst", "nie\u00b7der", "in", "der", "stil\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit tausend kleinen Sternen,", "tokens": ["Mit", "tau\u00b7send", "klei\u00b7nen", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wo dein freundlich Auge wacht,", "tokens": ["Und", "wo", "dein", "freund\u00b7lich", "Au\u00b7ge", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df sich der Feind entfernen.", "tokens": ["Mu\u00df", "sich", "der", "Feind", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum fasse Mut, du Menschenkind,", "tokens": ["Drum", "fas\u00b7se", "Mut", ",", "du", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verlier' dich nicht im Dunkeln,", "tokens": ["Ver\u00b7lier'", "dich", "nicht", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lichter ja am Himmel sind", "tokens": ["Die", "Lich\u00b7ter", "ja", "am", "Him\u00b7mel", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um tr\u00f6stlich dir zu funkeln.", "tokens": ["Um", "tr\u00f6st\u00b7lich", "dir", "zu", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}