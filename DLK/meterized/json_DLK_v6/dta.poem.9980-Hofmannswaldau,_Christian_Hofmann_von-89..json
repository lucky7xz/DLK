{"dta.poem.9980": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.57", "nl:0.14", "sv:0.14", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tragen kan man uns gar wol/ ohne da\u00df man es vermercke;", "tokens": ["Tra\u00b7gen", "kan", "man", "uns", "gar", "wol", "/", "oh\u00b7ne", "da\u00df", "man", "es", "ver\u00b7mer\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PPER", "ADV", "ADV", "$(", "APPR", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Aber sicht bar machen nicht unsre gr\u00f6\u00df und unsre st\u00e4rcke.", "tokens": ["A\u00b7ber", "sicht", "bar", "ma\u00b7chen", "nicht", "uns\u00b7re", "gr\u00f6\u00df", "und", "uns\u00b7re", "st\u00e4r\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "PTKNEG", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Dieser kan uns leichtlich geben/ der uns nie gehabt/ noch hat/", "tokens": ["Die\u00b7ser", "kan", "uns", "leicht\u00b7lich", "ge\u00b7ben", "/", "der", "uns", "nie", "ge\u00b7habt", "/", "noch", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$(", "PRELS", "PPER", "ADV", "VAPP", "$(", "ADV", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "In gedancken man uns siehet/ niemahls recht und in der that.", "tokens": ["In", "ge\u00b7dan\u00b7cken", "man", "uns", "sie\u00b7het", "/", "nie\u00b7mahls", "recht", "und", "in", "der", "that", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIS", "PPER", "VVFIN", "$(", "ADV", "ADJD", "KON", "APPR", "ART", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}