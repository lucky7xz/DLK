{"textgrid.poem.35981": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "5. Wehmut", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch in der Linde dr\u00fcben", "tokens": ["Hoch", "in", "der", "Lin\u00b7de", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NE", "ADV"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein V\u00f6glein wohnte lang;", "tokens": ["Ein", "V\u00f6\u00b7glein", "wohn\u00b7te", "lang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich tat es herzlich lieben,", "tokens": ["Ich", "tat", "es", "herz\u00b7lich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gern lauschen seinem Sang.", "tokens": ["Gern", "lau\u00b7schen", "sei\u00b7nem", "Sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo ist mein S\u00e4nger blieben?", "tokens": ["Wo", "ist", "mein", "S\u00e4n\u00b7ger", "blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach, schweigt ja schon so lang.", "tokens": ["Ach", ",", "schweigt", "ja", "schon", "so", "lang", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er sang von Lenz und Lieben \u2013", "tokens": ["Er", "sang", "von", "Lenz", "und", "Lie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dr\u00fcben", "tokens": ["Dr\u00fc\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Ist alles stumm und bang.", "tokens": ["Ist", "al\u00b7les", "stumm", "und", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die Linde seh' ich stehen", "tokens": ["Die", "Lin\u00b7de", "seh'", "ich", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So traurig dunkelgr\u00fcn,", "tokens": ["So", "trau\u00b7rig", "dun\u00b7kel\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die Blumen all vergehen", "tokens": ["Die", "Blu\u00b7men", "all", "ver\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und wollen nicht mehr bl\u00fchn.", "tokens": ["Und", "wol\u00b7len", "nicht", "mehr", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ich hab' ihn oft gesehen,", "tokens": ["Ich", "hab'", "ihn", "oft", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Nun ist er hin, ist hin \u2013", "tokens": ["Nun", "ist", "er", "hin", ",", "ist", "hin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Die Winde schaurig wehen,", "tokens": ["Die", "Win\u00b7de", "schau\u00b7rig", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Gehen", "tokens": ["Ge\u00b7hen"], "token_info": ["word"], "pos": ["NN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.18": {"text": "Und seufzen all um ihn.", "tokens": ["Und", "seuf\u00b7zen", "all", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}