{"textgrid.poem.43782": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht, wie das Schneefeld dr\u00fcben uns winkt!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht, wie das Schneefeld dr\u00fcben uns winkt!", "tokens": ["Seht", ",", "wie", "das", "Schnee\u00b7feld", "dr\u00fc\u00b7ben", "uns", "winkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seht, wie es flimmert! Seht, wie es blinkt!", "tokens": ["Seht", ",", "wie", "es", "flim\u00b7mert", "!", "Seht", ",", "wie", "es", "blinkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nicht l\u00e4nger bedacht!", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Fort, fort in die Schlacht!", "tokens": ["Fort", ",", "fort", "in", "die", "Schlacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ballet den Schnee geschwind wie der Wind!", "tokens": ["Bal\u00b7let", "den", "Schnee", "ge\u00b7schwind", "wie", "der", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fort auf den Plan, wo's K\u00e4mpfen beginnt!", "tokens": ["Fort", "auf", "den", "Plan", ",", "wo's", "K\u00e4mp\u00b7fen", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schnee ist das Gewehr,", "tokens": ["Schnee", "ist", "das", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schnee Degen und Speer.", "tokens": ["Schnee", "De\u00b7gen", "und", "Speer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "N\u00e4her dem Feinde, n\u00e4her ger\u00fcckt!", "tokens": ["N\u00e4\u00b7her", "dem", "Fein\u00b7de", ",", "n\u00e4\u00b7her", "ge\u00b7r\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Flink sich gedreht und flinker geb\u00fcckt!", "tokens": ["Flink", "sich", "ge\u00b7dreht", "und", "flin\u00b7ker", "ge\u00b7b\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "List leite das Spiel!", "tokens": ["List", "lei\u00b7te", "das", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Muth f\u00fchret zum Ziel.", "tokens": ["Muth", "f\u00fch\u00b7ret", "zum", "Ziel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Seht, wie das Schneefeld dr\u00fcben uns winkt!", "tokens": ["Seht", ",", "wie", "das", "Schnee\u00b7feld", "dr\u00fc\u00b7ben", "uns", "winkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Seht, wie es flimmert! Seht, wie es blinkt!", "tokens": ["Seht", ",", "wie", "es", "flim\u00b7mert", "!", "Seht", ",", "wie", "es", "blinkt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$.", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Nicht l\u00e4nger bedacht!", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Fort, fort in die Schlacht!", "tokens": ["Fort", ",", "fort", "in", "die", "Schlacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}