{"dta.poem.23694": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Tausch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wilst du tauschen? sprach ein Reicher,", "tokens": ["Wilst", "du", "tau\u00b7schen", "?", "sprach", "ein", "Rei\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er wies mir seine Sch\u00e4tze,", "tokens": ["Und", "er", "wies", "mir", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich solt ihm f\u00fcr die Sch\u00e4tze,", "tokens": ["Und", "ich", "solt", "ihm", "f\u00fcr", "die", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine braune Doris geben.", "tokens": ["Mei\u00b7ne", "brau\u00b7ne", "Do\u00b7ris", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber wi\u00dft ihr, was ich sagte?", "tokens": ["A\u00b7ber", "wi\u00dft", "ihr", ",", "was", "ich", "sag\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reicher, sprach ich, du bist n\u00e4rrisch!", "tokens": ["Rei\u00b7cher", ",", "sprach", "ich", ",", "du", "bist", "n\u00e4r\u00b7risch", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Macht dein Gold vergn\u00fcgte Minen?", "tokens": ["Macht", "dein", "Gold", "ver\u00b7gn\u00fcg\u00b7te", "Mi\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist dein Gold getreu und artig?", "tokens": ["Ist", "dein", "Gold", "ge\u00b7treu", "und", "ar\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist es witzig, ist es z\u00e4rtlich?", "tokens": ["Ist", "es", "wit\u00b7zig", ",", "ist", "es", "z\u00e4rt\u00b7lich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist es tugendhaft, wie Doris?", "tokens": ["Ist", "es", "tu\u00b7gend\u00b7haft", ",", "wie", "Do\u00b7ris", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "PWAV", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Kann es dem Besitzer schmeicheln?", "tokens": ["Kann", "es", "dem", "Be\u00b7sit\u00b7zer", "schmei\u00b7cheln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Kann es scherzen, kann es k\u00fcssen?", "tokens": ["Kann", "es", "scher\u00b7zen", ",", "kann", "es", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Kann es t\u00e4ndeln, wie mein M\u00e4dchen?", "tokens": ["Kann", "es", "t\u00e4n\u00b7deln", ",", "wie", "mein", "M\u00e4d\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Reicher, k\u00f6nnen dis die Sch\u00e4tze,", "tokens": ["Rei\u00b7cher", ",", "k\u00f6n\u00b7nen", "dis", "die", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "VMFIN", "PDS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "So vertausch ich dir mein M\u00e4dchen.", "tokens": ["So", "ver\u00b7tausch", "ich", "dir", "mein", "M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}