{"dta.poem.3370": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Orpheus Hymne  \n  an die Natur .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Natur, allwirkende G\u00f6ttin', alls&#228;ugende", "tokens": ["O", "Na\u00b7tur", ",", "all\u00b7wir\u00b7ken\u00b7de", "G\u00f6t\u00b7tin'", ",", "alls", "&#228;", "u\u00b7gen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "XML_entity", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,", "KOUS", "$(", "ADJA"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Mutter,", "tokens": ["Mut\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Erste, \u00c4lteste, Letzte, Ehrw\u00fcrdigste, Oberste", "tokens": ["Ers\u00b7te", ",", "\u00c4l\u00b7tes\u00b7te", ",", "Letz\u00b7te", ",", "Ehr\u00b7w\u00fcr\u00b7digs\u00b7te", ",", "O\u00b7bers\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "$,", "NN", "$,", "ADJA", "$,", "NN", "$,", "ADJA"], "meter": "+-+--+-+-+-+--", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Aller,", "tokens": ["Al\u00b7ler", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Herrscherin, K\u00f6nigin, Weltbezwingerin, Nimmer-", "tokens": ["Herr\u00b7sche\u00b7rin", ",", "K\u00f6\u00b7ni\u00b7gin", ",", "Welt\u00b7be\u00b7zwin\u00b7ge\u00b7rin", ",", "Nim\u00b7mer"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "bezwungne,", "tokens": ["be\u00b7zwung\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nimmerbezwingbare, Nimmerzerst\u00f6rbare, Preisliche,", "tokens": ["Nim\u00b7mer\u00b7be\u00b7zwing\u00b7ba\u00b7re", ",", "Nim\u00b7mer\u00b7zer\u00b7st\u00f6r\u00b7ba\u00b7re", ",", "Preis\u00b7li\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.8": {"text": "Hohe,", "tokens": ["Ho\u00b7he", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Heilige, Heimliche, N\u00e4chtliche, Freudige, Strah-", "tokens": ["Hei\u00b7li\u00b7ge", ",", "Heim\u00b7li\u00b7che", ",", "N\u00e4cht\u00b7li\u00b7che", ",", "Freu\u00b7di\u00b7ge", ",", "Strah"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "$,", "ADJA", "$,", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.10": {"text": "lende, Hehre,", "tokens": ["len\u00b7de", ",", "Heh\u00b7re", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.11": {"text": "Die du wandelst leisen Schrittes verborgene Pfade,", "tokens": ["Die", "du", "wan\u00b7delst", "lei\u00b7sen", "Schrit\u00b7tes", "ver\u00b7bor\u00b7ge\u00b7ne", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Die du zeichnest den leisesten Schritt mit Thaten", "tokens": ["Die", "du", "zeich\u00b7nest", "den", "lei\u00b7ses\u00b7ten", "Schritt", "mit", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.13": {"text": "und Wundern,", "tokens": ["und", "Wun\u00b7dern", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Keusche G\u00f6tterdienerin, nimmerendendes Ende,", "tokens": ["Keu\u00b7sche", "G\u00f6t\u00b7ter\u00b7die\u00b7ne\u00b7rin", ",", "nim\u00b7me\u00b7ren\u00b7den\u00b7des", "En\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+---+-", "measure": "unknown.measure.hexa"}, "line.15": {"text": "Die du jeglichem Freude gew\u00e4hrst, und begehrst", "tokens": ["Die", "du", "jeg\u00b7li\u00b7chem", "Freu\u00b7de", "ge\u00b7w\u00e4hrst", ",", "und", "be\u00b7gehrst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "PIAT", "NN", "VVPP", "$,", "KON", "VVFIN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "sie von keinem,", "tokens": ["sie", "von", "kei\u00b7nem", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIS", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Vater- und Mutterlose, doch Aller Vater und Mutter,", "tokens": ["Va\u00b7ter", "und", "Mut\u00b7ter\u00b7lo\u00b7se", ",", "doch", "Al\u00b7ler", "Va\u00b7ter", "und", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Starke, Gewaltige, K\u00fchne, Erhabenste, H\u00f6chste", "tokens": ["Star\u00b7ke", ",", "Ge\u00b7wal\u00b7ti\u00b7ge", ",", "K\u00fch\u00b7ne", ",", "Er\u00b7ha\u00b7bens\u00b7te", ",", "H\u00f6chs\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "$,", "ADJA"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.19": {"text": "der Kr\u00e4fte,", "tokens": ["der", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Duftige, Liebliche, Labende, Freundliche, K\u00fcnst-", "tokens": ["Duf\u00b7ti\u00b7ge", ",", "Lieb\u00b7li\u00b7che", ",", "La\u00b7ben\u00b7de", ",", "Freund\u00b7li\u00b7che", ",", "K\u00fcnst"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.21": {"text": "liche, Weise,", "tokens": ["li\u00b7che", ",", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJA", "$,", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.22": {"text": "Zeugerin, Hegerin, Pfleg'rin, Erzieherin, W\u00e4r-", "tokens": ["Zeu\u00b7ge\u00b7rin", ",", "He\u00b7ge\u00b7rin", ",", "Pfleg'\u00b7rin", ",", "Er\u00b7zie\u00b7he\u00b7rin", ",", "W\u00e4r"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "+--+--+--+---", "measure": "dactylic.tetra.plus"}, "line.23": {"text": "terin, Amme,", "tokens": ["te\u00b7rin", ",", "Am\u00b7me", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.24": {"text": "Heldin, Huldin, Dichterin, Rednerin, Meisterin,", "tokens": ["Hel\u00b7din", ",", "Hul\u00b7din", ",", "Dich\u00b7te\u00b7rin", ",", "Red\u00b7ne\u00b7rin", ",", "Meis\u00b7te\u00b7rin", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "F\u00fcrstin,", "tokens": ["F\u00fcrs\u00b7tin", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "Die du waltest im Himmel, und schaffst auf der", "tokens": ["Die", "du", "wal\u00b7test", "im", "Him\u00b7mel", ",", "und", "schaffst", "auf", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$,", "KON", "VVFIN", "APPR", "ART"], "meter": "--+--+-+++-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Erd' und im Meere,", "tokens": ["Erd'", "und", "im", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPRART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.28": {"text": "Bitter dem B\u00f6sen, und s\u00fcss dem Guten, und gn\u00e4-", "tokens": ["Bit\u00b7ter", "dem", "B\u00f6\u00b7sen", ",", "und", "s\u00fcss", "dem", "Gu\u00b7ten", ",", "und", "gn\u00e4"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,", "KON", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.29": {"text": "dig dem Frommen,", "tokens": ["dig", "dem", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.30": {"text": "Die du Alle belehrst, und Alle bedenkst und er-", "tokens": ["Die", "du", "Al\u00b7le", "be\u00b7lehrst", ",", "und", "Al\u00b7le", "be\u00b7denkst", "und", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "KON", "TRUNC"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "n\u00e4hrest,", "tokens": ["n\u00e4h\u00b7rest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Die du alles besamest, und alles gestaltest und", "tokens": ["Die", "du", "al\u00b7les", "be\u00b7sa\u00b7mest", ",", "und", "al\u00b7les", "ge\u00b7stal\u00b7test", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$,", "KON", "PIS", "VVPP", "KON"], "meter": "+-+---+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "bildest,", "tokens": ["bil\u00b7dest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.34": {"text": "Alles entwickelst und alles vollendest, und alles", "tokens": ["Al\u00b7les", "ent\u00b7wi\u00b7ckelst", "und", "al\u00b7les", "voll\u00b7en\u00b7dest", ",", "und", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,", "KON", "PIS"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.35": {"text": "zerst\u00f6rest,", "tokens": ["zer\u00b7st\u00f6\u00b7rest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Alle Keime reifest, und die gereiften verstreuest,", "tokens": ["Al\u00b7le", "Kei\u00b7me", "rei\u00b7fest", ",", "und", "die", "ge\u00b7reif\u00b7ten", "ver\u00b7streu\u00b7est", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "VVFIN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Mutterbusen schwellst, und kreissende Sch\u00f6ss' ent-", "tokens": ["Mut\u00b7ter\u00b7bu\u00b7sen", "schwellst", ",", "und", "kreis\u00b7sen\u00b7de", "Sch\u00f6ss'", "ent"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "bindest,", "tokens": ["bin\u00b7dest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.39": {"text": "Ewig rastlos kreisest in ewig wirbelnden Strudeln,", "tokens": ["E\u00b7wig", "rast\u00b7los", "krei\u00b7sest", "in", "e\u00b7wig", "wir\u00b7beln\u00b7den", "Stru\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.40": {"text": "Allgef\u00e4llig dich fugst in tausendfache Gebilde,", "tokens": ["All\u00b7ge\u00b7f\u00e4l\u00b7lig", "dich", "fugst", "in", "tau\u00b7send\u00b7fa\u00b7che", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.41": {"text": "Hoch auf Thronen sitzest, und Recht und Gerech-", "tokens": ["Hoch", "auf", "Thro\u00b7nen", "sit\u00b7zest", ",", "und", "Recht", "und", "Ge\u00b7rech"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$,", "KON", "NN", "KON", "TRUNC"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "tigkeit aussprichst,", "tokens": ["tig\u00b7keit", "aus\u00b7sprichst", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.43": {"text": "\u00dcber Gebieter gebeutst, und \u00fcber die M\u00e4chtigen", "tokens": ["\u00dc\u00b7ber", "Ge\u00b7bie\u00b7ter", "ge\u00b7beutst", ",", "und", "\u00fc\u00b7ber", "die", "M\u00e4ch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "APPR", "ART", "NN"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.44": {"text": "Macht \u00fcbst,", "tokens": ["Macht", "\u00fcbst", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Alles b\u00e4ndigst und Keinem bebst \u2014 Allwaltendes", "tokens": ["Al\u00b7les", "b\u00e4n\u00b7digst", "und", "Kei\u00b7nem", "bebst", "All\u00b7wal\u00b7ten\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "ADJD", "KON", "PIS", "VVFIN", "$(", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.46": {"text": "Schicksal,", "tokens": ["Schick\u00b7sal", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.47": {"text": "Nieversiegendes Leben, und nimmerschlummernde", "tokens": ["Nie\u00b7ver\u00b7sie\u00b7gen\u00b7des", "Le\u00b7ben", ",", "und", "nim\u00b7mer\u00b7schlum\u00b7mern\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.48": {"text": "Vorsicht \u2014", "tokens": ["Vor\u00b7sicht"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.49": {"text": "Alles ist dein, dieweil du alles gebarst und er-", "tokens": ["Al\u00b7les", "ist", "dein", ",", "die\u00b7weil", "du", "al\u00b7les", "ge\u00b7barst", "und", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPOSAT", "$,", "KOUS", "PPER", "PIS", "VVPP", "KON", "TRUNC"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.50": {"text": "zogest!", "tokens": ["zo\u00b7gest", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.51": {"text": "O, so fleh' ich dich, G\u00f6ttin, du wollest mir selige", "tokens": ["O", ",", "so", "fleh'", "ich", "dich", ",", "G\u00f6t\u00b7tin", ",", "du", "wol\u00b7lest", "mir", "se\u00b7li\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,", "PPER", "VMFIN", "PPER", "ADJA"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.52": {"text": "Stunden,", "tokens": ["Stun\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.53": {"text": "Frieden gew\u00e4hren und Ruh, und guten gedeihlichen", "tokens": ["Frie\u00b7den", "ge\u00b7w\u00e4h\u00b7ren", "und", "Ruh", ",", "und", "gu\u00b7ten", "ge\u00b7deih\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.54": {"text": "Wachsthum.", "tokens": ["Wach\u00b7sthum", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}