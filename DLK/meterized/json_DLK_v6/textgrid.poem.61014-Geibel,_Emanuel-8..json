{"textgrid.poem.61014": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe gleicht dem April:", "tokens": ["Die", "Lie\u00b7be", "gleicht", "dem", "Ap\u00b7ril", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Bald Frost, bald fr\u00f6hliche Strahlen,", "tokens": ["Bald", "Frost", ",", "bald", "fr\u00f6h\u00b7li\u00b7che", "Strah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald Bl\u00fcten in Herzen und Talen,", "tokens": ["Bald", "Bl\u00fc\u00b7ten", "in", "Her\u00b7zen", "und", "Ta\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bald st\u00fcrmisch und bald still,", "tokens": ["Bald", "st\u00fcr\u00b7misch", "und", "bald", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald heimliches Ringen und Dehnen,", "tokens": ["Bald", "heim\u00b7li\u00b7ches", "Rin\u00b7gen", "und", "Deh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Bald Wolken, Regen und Tr\u00e4nen -", "tokens": ["Bald", "Wol\u00b7ken", ",", "Re\u00b7gen", "und", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Im ewigen Schwanken und Sehnen", "tokens": ["Im", "e\u00b7wi\u00b7gen", "Schwan\u00b7ken", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wer wei\u00df, was werden will!", "tokens": ["Wer", "wei\u00df", ",", "was", "wer\u00b7den", "will", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "VAINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}