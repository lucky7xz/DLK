{"textgrid.poem.63639": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Baum mit Lichtern, keine Weihnachtsgaben.", "tokens": ["Kein", "Baum", "mit", "Lich\u00b7tern", ",", "kei\u00b7ne", "Weih\u00b7nachts\u00b7ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sitzen uns gen\u00fcber bang und stumm,", "tokens": ["Wir", "sit\u00b7zen", "uns", "ge\u00b7n\u00fc\u00b7ber", "bang", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und jedes wei\u00df, und keines sagt, warum:", "tokens": ["Und", "je\u00b7des", "wei\u00df", ",", "und", "kei\u00b7nes", "sagt", ",", "wa\u00b7rum", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,", "PWAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drei Kinder in der Ferne, drei begraben.", "tokens": ["Drei", "Kin\u00b7der", "in", "der", "Fer\u00b7ne", ",", "drei", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,", "CARD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir werden stille Feiertage haben,", "tokens": ["Wir", "wer\u00b7den", "stil\u00b7le", "Fei\u00b7er\u00b7ta\u00b7ge", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trotz Glockenl\u00e4uten, frohem Festgesumm.", "tokens": ["Trotz", "Glo\u00b7cken\u00b7l\u00e4u\u00b7ten", ",", "fro\u00b7hem", "Fest\u00b7ge\u00b7summ", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn immer geistet bleich um uns herum", "tokens": ["Denn", "im\u00b7mer", "geis\u00b7tet", "bleich", "um", "uns", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "APPR", "PRF", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Schmerzensantlitz unsres lieben Knaben.", "tokens": ["Das", "Schmer\u00b7zen\u00b7sant\u00b7litz", "uns\u00b7res", "lie\u00b7ben", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun wohl! So werd' auch dies noch ausgestanden,", "tokens": ["Nun", "wohl", "!", "So", "werd'", "auch", "dies", "noch", "aus\u00b7ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "VAFIN", "ADV", "PDS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschl\u00fcrft im Jammerkelch der herbste Tropfen!", "tokens": ["Ge\u00b7schl\u00fcrft", "im", "Jam\u00b7mer\u00b7kelch", "der", "herbs\u00b7te", "Trop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch Bittreres ist schwerlich mehr vorhanden.", "tokens": ["Noch", "Bit\u00b7tre\u00b7res", "ist", "schwer\u00b7lich", "mehr", "vor\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es w\u00e4re denn der Blutsquell nicht zu stopfen,", "tokens": ["Es", "w\u00e4\u00b7re", "denn", "der", "Bluts\u00b7quell", "nicht", "zu", "stop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von zwei Herzen, fest in Liebesbanden,", "tokens": ["Und", "von", "zwei", "Her\u00b7zen", ",", "fest", "in", "Lie\u00b7bes\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6rte das eine vorschnell auf zu klopfen.", "tokens": ["H\u00f6r\u00b7te", "das", "ei\u00b7ne", "vor\u00b7schnell", "auf", "zu", "klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ADJD", "APPR", "PTKZU", "VVINF", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}}}}}