{"dta.poem.891": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "AbENDMUSE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Ans Blumenfenster wieder kehrt des Kirchturms Schatten", "tokens": ["Ans", "Blu\u00b7men\u00b7fens\u00b7ter", "wie\u00b7der", "kehrt", "des", "Kirch\u00b7turms", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Goldnes. Die hei\u00dfe Stirn vergl\u00fcht in Ruh und Schweigen.", "tokens": ["Und", "Gold\u00b7nes", ".", "Die", "hei\u00b7\u00dfe", "Stirn", "ver\u00b7gl\u00fcht", "in", "Ruh", "und", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein Brunnen f\u00e4llt im Dunkel von Kastanienzweigen \u2014", "tokens": ["Ein", "Brun\u00b7nen", "f\u00e4llt", "im", "Dun\u00b7kel", "von", "Kas\u00b7ta\u00b7ni\u00b7en\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da f\u00fchlst du: es ist gut! in schmerzlichem Ermatten.", "tokens": ["Da", "f\u00fchlst", "du", ":", "es", "ist", "gut", "!", "in", "schmerz\u00b7li\u00b7chem", "Er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$.", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Markt ist leer von Sommerfr\u00fcchten und Gewinden.", "tokens": ["Der", "Markt", "ist", "leer", "von", "Som\u00b7mer\u00b7fr\u00fcch\u00b7ten", "und", "Ge\u00b7win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eintr\u00e4chtig stimmt der Tore schw\u00e4rzliches Gepr\u00e4nge.", "tokens": ["Ein\u00b7tr\u00e4ch\u00b7tig", "stimmt", "der", "To\u00b7re", "schw\u00e4rz\u00b7li\u00b7ches", "Ge\u00b7pr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In einem Garten t\u00f6nen sanften Spieles Kl\u00e4nge,", "tokens": ["In", "ei\u00b7nem", "Gar\u00b7ten", "t\u00f6\u00b7nen", "sanf\u00b7ten", "Spie\u00b7les", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo Freunde nach dem Mahle sich zusammenfinden.", "tokens": ["Wo", "Freun\u00b7de", "nach", "dem", "Mah\u00b7le", "sich", "zu\u00b7sam\u00b7men\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Des wei\u00dfen Magiers M\u00e4rchen lauscht die Seele gerne.", "tokens": ["Des", "wei\u00b7\u00dfen", "Ma\u00b7giers", "M\u00e4r\u00b7chen", "lauscht", "die", "See\u00b7le", "ger\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rund saust das Korn, das M\u00e4her nachmittags geschnitten.", "tokens": ["Rund", "saust", "das", "Korn", ",", "das", "M\u00e4\u00b7her", "nach\u00b7mit\u00b7tags", "ge\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geduldig schweigt das harte Leben in den H\u00fctten;", "tokens": ["Ge\u00b7dul\u00b7dig", "schweigt", "das", "har\u00b7te", "Le\u00b7ben", "in", "den", "H\u00fct\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der K\u00fche linden Schlaf bescheint die Stallaterne.", "tokens": ["Der", "K\u00fc\u00b7he", "lin\u00b7den", "Schlaf", "be\u00b7scheint", "die", "Stal\u00b7la\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von L\u00fcften trunken sinken balde ein die Lider", "tokens": ["Von", "L\u00fcf\u00b7ten", "trun\u00b7ken", "sin\u00b7ken", "bal\u00b7de", "ein", "die", "Li\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "ADV", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00f6ffnen leise sich zu fremden Sternenzeichen.", "tokens": ["Und", "\u00f6ff\u00b7nen", "lei\u00b7se", "sich", "zu", "frem\u00b7den", "Ster\u00b7nen\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Endymion taucht aus dem Dunkel alter Eichen", "tokens": ["En\u00b7dy\u00b7mi\u00b7on", "taucht", "aus", "dem", "Dun\u00b7kel", "al\u00b7ter", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und beugt sich \u00fcber trauervolle Wasser nieder.", "tokens": ["Und", "beugt", "sich", "\u00fc\u00b7ber", "trau\u00b7er\u00b7vol\u00b7le", "Was\u00b7ser", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}