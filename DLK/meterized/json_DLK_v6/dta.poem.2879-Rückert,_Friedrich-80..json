{"dta.poem.2879": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verzage nicht, o Herz! die Lust entspringt aus Trauer;", "tokens": ["Ver\u00b7za\u00b7ge", "nicht", ",", "o", "Herz", "!", "die", "Lust", "ent\u00b7springt", "aus", "Trau\u00b7er", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "FM", "NN", "$.", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Sonnenaufgang geht voraus ein Morgenschauer.", "tokens": ["Dem", "Son\u00b7nen\u00b7auf\u00b7gang", "geht", "vo\u00b7raus", "ein", "Mor\u00b7gen\u00b7schau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In diesem Schauer wird, was gestern bl\u00fchte, sterben:", "tokens": ["In", "die\u00b7sem", "Schau\u00b7er", "wird", ",", "was", "ge\u00b7stern", "bl\u00fch\u00b7te", ",", "ster\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was heute soll erbl\u00fchn, wird davon Kraft erwerben.", "tokens": ["Was", "heu\u00b7te", "soll", "er\u00b7bl\u00fchn", ",", "wird", "da\u00b7von", "Kraft", "er\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "VVINF", "$,", "VAFIN", "PAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verzage nicht, wenn ab die welke Hoffnung fiel;", "tokens": ["Ver\u00b7za\u00b7ge", "nicht", ",", "wenn", "ab", "die", "wel\u00b7ke", "Hoff\u00b7nung", "fiel", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die neue schon erhebt sich jung auf frischem Stiel.", "tokens": ["Die", "neu\u00b7e", "schon", "er\u00b7hebt", "sich", "jung", "auf", "fri\u00b7schem", "Stiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}