{"textgrid.poem.60192": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du! den mir mein Freund empfahl,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du! den mir mein Freund empfahl,", "tokens": ["O", "du", "!", "den", "mir", "mein", "Freund", "emp\u00b7fahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gold ist nicht meiner Neigung G\u00f6tze.", "tokens": ["Gold", "ist", "nicht", "mei\u00b7ner", "Nei\u00b7gung", "G\u00f6t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich rechne meiner Freunde Zahl,", "tokens": ["Ich", "rech\u00b7ne", "mei\u00b7ner", "Freun\u00b7de", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So z\u00e4hl ich gr\u00f6\u00dfre Sch\u00e4tze.", "tokens": ["So", "z\u00e4hl", "ich", "gr\u00f6\u00df\u00b7re", "Sch\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mir unumtauschbar ist ein Freund!", "tokens": ["Mir", "un\u00b7um\u00b7tauschbar", "ist", "ein", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Mir darf kein Prinz den Flei\u00df belohnen:", "tokens": ["Mir", "darf", "kein", "Prinz", "den", "Flei\u00df", "be\u00b7loh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Sulzer werde nie mein Feind;", "tokens": ["Nur", "Sul\u00b7zer", "wer\u00b7de", "nie", "mein", "Feind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn g\u00e4b ich nicht um Cronen.", "tokens": ["Ihn", "g\u00e4b", "ich", "nicht", "um", "Cro\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und jenen, der mir aus Berlin", "tokens": ["Und", "je\u00b7nen", ",", "der", "mir", "aus", "Ber\u00b7lin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mehr als Ges\u00e4nge noch entf\u00fchrte,", "tokens": ["Mehr", "als", "Ge\u00b7s\u00e4n\u00b7ge", "noch", "ent\u00b7f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "ADV", "VVFIN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fc\u00dft ich dem Zepter vorzuziehn,", "tokens": ["W\u00fc\u00dft", "ich", "dem", "Zep\u00b7ter", "vor\u00b7zu\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der eine Welt regierte.", "tokens": ["Der", "ei\u00b7ne", "Welt", "re\u00b7gier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und diesen, dem Apollo gab", "tokens": ["Und", "die\u00b7sen", ",", "dem", "A\u00b7pol\u00b7lo", "gab"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "ART", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des C\u00e4sars Dichter nachzuahmen,", "tokens": ["Des", "C\u00e4\u00b7sars", "Dich\u00b7ter", "nach\u00b7zu\u00b7ah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den tauscht mir keine F\u00fcrstin ab", "tokens": ["Den", "tauscht", "mir", "kei\u00b7ne", "F\u00fcrs\u00b7tin", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem Durchlauchten Nahmen.", "tokens": ["Mit", "dem", "Durch\u00b7lauch\u00b7ten", "Nah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Auf keinen meiner Freunde l\u00e4\u00dft", "tokens": ["Auf", "kei\u00b7nen", "mei\u00b7ner", "Freun\u00b7de", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz den grossen Anspruch fahren.", "tokens": ["Mein", "Herz", "den", "gros\u00b7sen", "An\u00b7spruch", "fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie machen meines Lebens Rest", "tokens": ["Sie", "ma\u00b7chen", "mei\u00b7nes", "Le\u00b7bens", "Rest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu lauter Jubel-Jahren.", "tokens": ["Zu", "lau\u00b7ter", "Ju\u00b7bel\u00b7Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und du! so ganz f\u00fcr meine Wahl", "tokens": ["Und", "du", "!", "so", "ganz", "f\u00fcr", "mei\u00b7ne", "Wahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$.", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geschaffner Freund! verg\u00f6nne", "tokens": ["Ge\u00b7schaff\u00b7ner", "Freund", "!", "ver\u00b7g\u00f6n\u00b7ne"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich bey Buchholz und bey Stahl,", "tokens": ["Da\u00df", "ich", "bey", "Buch\u00b7holz", "und", "bey", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dich als ein Kleinod nenne.", "tokens": ["Dich", "als", "ein", "Klei\u00b7nod", "nen\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+--++--", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Sechs Freunde! Welch ein K\u00f6nigreich", "tokens": ["Sechs", "Freun\u00b7de", "!", "Welch", "ein", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$.", "PIAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt seinem Herrscher solch Vergn\u00fcgen?", "tokens": ["Giebt", "sei\u00b7nem", "Herr\u00b7scher", "solch", "Ver\u00b7gn\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Elisabeth ist mir nicht gleich", "tokens": ["E\u00b7lis\u00b7a\u00b7beth", "ist", "mir", "nicht", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Russen vor ihr liegen!", "tokens": ["Wenn", "Rus\u00b7sen", "vor", "ihr", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie wird gef\u00fcrchtet, nicht geliebt;", "tokens": ["Sie", "wird", "ge\u00b7f\u00fcrch\u00b7tet", ",", "nicht", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geehrt, doch nicht um ihrer willen.", "tokens": ["Ge\u00b7ehrt", ",", "doch", "nicht", "um", "ih\u00b7rer", "wil\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein! um den Glanz der Sie umgiebt", "tokens": ["Nein", "!", "um", "den", "Glanz", "der", "Sie", "um\u00b7giebt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "ART", "PPER", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und um die Purpur-H\u00fcllen!", "tokens": ["Und", "um", "die", "Pur\u00b7pur\u00b7H\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mir bleiben meine Freunde hold", "tokens": ["Mir", "blei\u00b7ben", "mei\u00b7ne", "Freun\u00b7de", "hold"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Leyer wegen, die ich spiele;", "tokens": ["Der", "Le\u00b7yer", "we\u00b7gen", ",", "die", "ich", "spie\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und weil ich minder f\u00fcr das Gold,", "tokens": ["Und", "weil", "ich", "min\u00b7der", "f\u00fcr", "das", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als f\u00fcr die Freundschaft f\u00fchle.", "tokens": ["Als", "f\u00fcr", "die", "Freund\u00b7schaft", "f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}