{"textgrid.poem.40145": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun bl\u00fcht auf deinem Antlitz wieder Leben!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun bl\u00fcht auf deinem Antlitz wieder Leben!", "tokens": ["Nun", "bl\u00fcht", "auf", "dei\u00b7nem", "Ant\u00b7litz", "wie\u00b7der", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Zittern deiner Lippen steht's geschrieben,", "tokens": ["Im", "Zit\u00b7tern", "dei\u00b7ner", "Lip\u00b7pen", "steht's", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich les' es tief aus deiner Augen Grunde,", "tokens": ["ich", "les'", "es", "tief", "aus", "dei\u00b7ner", "Au\u00b7gen", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ich lausch' es mir von deinem warmen Munde,", "tokens": ["ich", "lausch'", "es", "mir", "von", "dei\u00b7nem", "war\u00b7men", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "wie's in dir jubelt: Ihm geblieben!", "tokens": ["wie's", "in", "dir", "ju\u00b7belt", ":", "Ihm", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "$.", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun quillt auch mir im Herzen wieder Leben!", "tokens": ["Nun", "quillt", "auch", "mir", "im", "Her\u00b7zen", "wie\u00b7der", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als r\u00e4ng' ich mit dem Tod um ", "tokens": ["Als", "r\u00e4ng'", "ich", "mit", "dem", "Tod", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "hab' ich dein irres Auge angefleht,", "tokens": ["hab'", "ich", "dein", "ir\u00b7res", "Au\u00b7ge", "an\u00b7ge\u00b7fleht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "mein ganz Gef\u00fchl ein einziges Gebet, \u2013", "tokens": ["mein", "ganz", "Ge\u00b7f\u00fchl", "ein", "ein\u00b7zi\u00b7ges", "Ge\u00b7bet", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADV", "NN", "ART", "ADJA", "NN", "$,", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "ein einz'ger Dank jetzt: mir geblieben!", "tokens": ["ein", "einz'\u00b7ger", "Dank", "jetzt", ":", "mir", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$.", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun jubelt Alles, Alles wieder Leben!", "tokens": ["Nun", "ju\u00b7belt", "Al\u00b7les", ",", "Al\u00b7les", "wie\u00b7der", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PIS", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinaus zum Fr\u00fchling hat es mich getrieben,", "tokens": ["Hin\u00b7aus", "zum", "Fr\u00fch\u00b7ling", "hat", "es", "mich", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "als m\u00fc\u00dfte jede Knospe davon springen,", "tokens": ["als", "m\u00fc\u00df\u00b7te", "je\u00b7de", "Knos\u00b7pe", "da\u00b7von", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PIAT", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "als m\u00fc\u00dft's im Liedchen jedes Vogels klingen,", "tokens": ["als", "m\u00fc\u00dft's", "im", "Lied\u00b7chen", "je\u00b7des", "Vo\u00b7gels", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPRART", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "da\u00df Alles, Alles uns geblieben!", "tokens": ["da\u00df", "Al\u00b7les", ",", "Al\u00b7les", "uns", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}