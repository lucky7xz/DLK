{"dta.poem.21081": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Letztes Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bin ich schon der Erde Qual entbunden?               ", "tokens": ["Bin", "ich", "schon", "der", "Er\u00b7de", "Qual", "ent\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schl\u00e4gt das kranke Herz gehaltnern Schlag?", "tokens": ["Schl\u00e4gt", "das", "kran\u00b7ke", "Herz", "ge\u00b7halt\u00b7nern", "Schlag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sch\u00f6ne Seele, du hast \u00fcberwunden,", "tokens": ["Sch\u00f6\u00b7ne", "See\u00b7le", ",", "du", "hast", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und err\u00f6thend ringt dein Freund dir nach.", "tokens": ["Und", "er\u00b7r\u00f6\u00b7thend", "ringt", "dein", "Freund", "dir", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Opfern will ich dir mit Kampf und Thr\u00e4nen \u2014", "tokens": ["Op\u00b7fern", "will", "ich", "dir", "mit", "Kampf", "und", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nicht die Flamme, welche mich durchflammt \u2014", "tokens": ["Nicht", "die", "Flam\u00b7me", ",", "wel\u00b7che", "mich", "durch\u00b7flammt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber jedes ungest\u00fcme Sehnen,", "tokens": ["A\u00b7ber", "je\u00b7des", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Was von niedrer Erde stammt.", "tokens": ["Was", "von", "nie\u00b7drer", "Er\u00b7de", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebend, Lieblingin, an dir zu hangen,", "tokens": ["Lie\u00b7bend", ",", "Lieb\u00b7lin\u00b7gin", ",", "an", "dir", "zu", "han\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Edlen Zutrauns voll mich dir zu nahn,", "tokens": ["Ed\u00b7len", "Zu\u00b7trauns", "voll", "mich", "dir", "zu", "nahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PPER", "PPER", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit Bewunderung dich zu umfangen,", "tokens": ["Mit", "Be\u00b7wun\u00b7de\u00b7rung", "dich", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit Begeisterung dich zu umfahn,", "tokens": ["Mit", "Be\u00b7geis\u00b7te\u00b7rung", "dich", "zu", "um\u00b7fahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mit der Inbrunst trunkenem Entz\u00fccken", "tokens": ["Mit", "der", "In\u00b7brunst", "trun\u00b7ke\u00b7nem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zu der Wangen bleichem Morgenroth,", "tokens": ["Zu", "der", "Wan\u00b7gen", "blei\u00b7chem", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Zu der Augen Himmel aufzublicken \u2014", "tokens": ["Zu", "der", "Au\u00b7gen", "Him\u00b7mel", "auf\u00b7zu\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wehret keiner Pflicht Verbot.", "tokens": ["Weh\u00b7ret", "kei\u00b7ner", "Pflicht", "Ver\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sollte je in mir der Zug erschlaffen,", "tokens": ["Soll\u00b7te", "je", "in", "mir", "der", "Zug", "er\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der mich hinzieht zur Vortrefflichkeit?", "tokens": ["Der", "mich", "hin\u00b7zieht", "zur", "Vor\u00b7treff\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sollte mich ein Gott mit Blindheit strafen", "tokens": ["Soll\u00b7te", "mich", "ein", "Gott", "mit", "Blind\u00b7heit", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fcr die Sch\u00f6nheit und Vollkommenheit?", "tokens": ["F\u00fcr", "die", "Sch\u00f6n\u00b7heit", "und", "Voll\u00b7kom\u00b7men\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Sollt' in mir die hohe Gluth verlodern,", "tokens": ["Sollt'", "in", "mir", "die", "ho\u00b7he", "Gluth", "ver\u00b7lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ohne die ich feig' im Staube schlich?", "tokens": ["Oh\u00b7ne", "die", "ich", "feig'", "im", "Stau\u00b7be", "schlich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Sollt' in mir das G\u00f6ttliche vermodern? \u2014", "tokens": ["Sollt'", "in", "mir", "das", "G\u00f6tt\u00b7li\u00b7che", "ver\u00b7mo\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ewig, Edle, lieb' ich dich.", "tokens": ["E\u00b7wig", ",", "Ed\u00b7le", ",", "lieb'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber sorgsam k\u00fchlen, emsig d\u00e4mpfen", "tokens": ["A\u00b7ber", "sorg\u00b7sam", "k\u00fch\u00b7len", ",", "em\u00b7sig", "d\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Will ich jede st\u00fcrmische Begier.", "tokens": ["Will", "ich", "je\u00b7de", "st\u00fcr\u00b7mi\u00b7sche", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jeden leisen Wunsch will ich bek\u00e4mpfen,", "tokens": ["Je\u00b7den", "lei\u00b7sen", "Wunsch", "will", "ich", "be\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede Selbstsucht bannen fern von mir.", "tokens": ["Je\u00b7de", "Selbst\u00b7sucht", "ban\u00b7nen", "fern", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ritterlich will ich den Trieb bekriegen,", "tokens": ["Rit\u00b7ter\u00b7lich", "will", "ich", "den", "Trieb", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seinen St\u00fcrmen tapfer widerstehn.", "tokens": ["Sei\u00b7nen", "St\u00fcr\u00b7men", "tap\u00b7fer", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Ich gelob' es: Glorreich will ich siegen,", "tokens": ["Ich", "ge\u00b7lob'", "es", ":", "Glor\u00b7reich", "will", "ich", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Oder glorreich untergehn!", "tokens": ["O\u00b7der", "glor\u00b7reich", "un\u00b7ter\u00b7gehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "L\u00e4chle nur, o Seele meiner Seele,", "tokens": ["L\u00e4ch\u00b7le", "nur", ",", "o", "See\u00b7le", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00e4chle mir bisweilen tr\u00f6stend zu.", "tokens": ["L\u00e4ch\u00b7le", "mir", "bis\u00b7wei\u00b7len", "tr\u00f6s\u00b7tend", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Blicke Kraft mir in die m\u00fcde Seele", "tokens": ["Bli\u00b7cke", "Kraft", "mir", "in", "die", "m\u00fc\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus dem Auge voll erhabner Ruh.", "tokens": ["Aus", "dem", "Au\u00b7ge", "voll", "er\u00b7hab\u00b7ner", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn die Schw\u00fcle mir den Athem hemmet,", "tokens": ["Wenn", "die", "Schw\u00fc\u00b7le", "mir", "den", "A\u00b7them", "hem\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Reiche huldreich mir die theure Hand.", "tokens": ["Rei\u00b7che", "huld\u00b7reich", "mir", "die", "theu\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wenn das dumpfe Daseyn mich beklemmet,", "tokens": ["Wenn", "das", "dump\u00b7fe", "Da\u00b7seyn", "mich", "be\u00b7klem\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Zeige mir das helle Land,", "tokens": ["Zei\u00b7ge", "mir", "das", "hel\u00b7le", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo der Leidenschaft Tumulte schweigen,", "tokens": ["Wo", "der", "Lei\u00b7den\u00b7schaft", "Tu\u00b7mul\u00b7te", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo kein Sinnenrausch den Geist verwirrt;", "tokens": ["Wo", "kein", "Sin\u00b7nen\u00b7rausch", "den", "Geist", "ver\u00b7wirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo Entz\u00fccken in den Bl\u00fcthenzweigen,", "tokens": ["Wo", "Ent\u00b7z\u00fc\u00b7cken", "in", "den", "Bl\u00fc\u00b7then\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In den Blumenkelchen Liebe girrt;", "tokens": ["In", "den", "Blu\u00b7men\u00b7kel\u00b7chen", "Lie\u00b7be", "girrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo berauscht vom Duft der Ambramatten,", "tokens": ["Wo", "be\u00b7rauscht", "vom", "Duft", "der", "A\u00b7mbra\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Aufgel\u00f6st vom Zauber des Gesangs,", "tokens": ["Auf\u00b7ge\u00b7l\u00f6st", "vom", "Zau\u00b7ber", "des", "Ge\u00b7sangs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Seelen selig sich zu Seelen gatten,", "tokens": ["See\u00b7len", "se\u00b7lig", "sich", "zu", "See\u00b7len", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ledig jedes Fesselzwangs.", "tokens": ["Le\u00b7dig", "je\u00b7des", "Fes\u00b7sel\u00b7zwangs", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wo am Busen seiner ", "tokens": ["Wo", "am", "Bu\u00b7sen", "sei\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "PPOSAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Coucy seines Herzens Sehnsucht letzt;               ", "tokens": ["Cou\u00b7cy", "sei\u00b7nes", "Her\u00b7zens", "Sehn\u00b7sucht", "letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Sich an ", "tokens": ["Sich", "an"], "token_info": ["word", "word"], "pos": ["PRF", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wo von ihrem ", "tokens": ["Wo", "von", "ih\u00b7rem"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Heloise jeden Wunsch verneint;               ", "tokens": ["He\u00b7loi\u00b7se", "je\u00b7den", "Wunsch", "ver\u00b7neint", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und von ", "tokens": ["Und", "von"], "token_info": ["word", "word"], "pos": [