{"textgrid.poem.66503": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Stra\u00dfen f\u00fcllen sich auf's ", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stra\u00dfen f\u00fcllen sich auf's ", "tokens": ["Die", "Stra\u00b7\u00dfen", "f\u00fcl\u00b7len", "sich", "auf's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Das Wandern hebt sich durch die Welt,", "tokens": ["Das", "Wan\u00b7dern", "hebt", "sich", "durch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hoch von des Himmels klarer Bl\u00e4ue", "tokens": ["Hoch", "von", "des", "Him\u00b7mels", "kla\u00b7rer", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weht Fr\u00fchlingsluft durch Wald und Feld.", "tokens": ["Weht", "Fr\u00fch\u00b7lings\u00b7luft", "durch", "Wald", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So r\u00fcst' auch ich die Schritte wieder,", "tokens": ["So", "r\u00fcst'", "auch", "ich", "die", "Schrit\u00b7te", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohin es geht, mir einerlei!", "tokens": ["Wo\u00b7hin", "es", "geht", ",", "mir", "ei\u00b7ner\u00b7lei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn nur f\u00fcr frohgestimmte Lieder", "tokens": ["Wenn", "nur", "f\u00fcr", "froh\u00b7ge\u00b7stimm\u00b7te", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Herz mir und die Stra\u00dfe frei.", "tokens": ["Das", "Herz", "mir", "und", "die", "Stra\u00b7\u00dfe", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wer sich zu mir eingefunden,", "tokens": ["Und", "wer", "sich", "zu", "mir", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu theilen meinen Wanderschritt,", "tokens": ["Zu", "thei\u00b7len", "mei\u00b7nen", "Wan\u00b7der\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sei mir f\u00fcr den Tag verbunden,", "tokens": ["Der", "sei", "mir", "f\u00fcr", "den", "Tag", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt er die rechten Kl\u00e4nge mit.", "tokens": ["Bringt", "er", "die", "rech\u00b7ten", "Kl\u00e4n\u00b7ge", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Bl\u00fcthenzeit in ihrer Sch\u00f6ne", "tokens": ["Die", "Bl\u00fc\u00b7then\u00b7zeit", "in", "ih\u00b7rer", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vereint zu schau'n, sei doppelt Lust!", "tokens": ["Ver\u00b7eint", "zu", "schau'n", ",", "sei", "dop\u00b7pelt", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VVINF", "$,", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verbunden klingen erst die T\u00f6ne", "tokens": ["Ver\u00b7bun\u00b7den", "klin\u00b7gen", "erst", "die", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit vollsten Kl\u00e4ngen aus der Brust.", "tokens": ["Mit", "volls\u00b7ten", "Kl\u00e4n\u00b7gen", "aus", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus deiner Schale la\u00df mich trinken,", "tokens": ["Aus", "dei\u00b7ner", "Scha\u00b7le", "la\u00df", "mich", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Fr\u00fchlingstag, mit durstgem Zug", "tokens": ["Du", "Fr\u00fch\u00b7lings\u00b7tag", ",", "mit", "durst\u00b7gem", "Zug"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr alle Freuden, die mir winken,", "tokens": ["F\u00fcr", "al\u00b7le", "Freu\u00b7den", ",", "die", "mir", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu st\u00e4rken Brust und Liedesflug!", "tokens": ["Zu", "st\u00e4r\u00b7ken", "Brust", "und", "Lie\u00b7des\u00b7flug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wo es sei in allen Fernen,", "tokens": ["Und", "wo", "es", "sei", "in", "al\u00b7len", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dahin mich Weg und Tag gebracht,", "tokens": ["Da\u00b7hin", "mich", "Weg", "und", "Tag", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein letztes Lied den guten Sternen,", "tokens": ["Ein", "letz\u00b7tes", "Lied", "den", "gu\u00b7ten", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den stillen Wandrern durch die Nacht!", "tokens": ["Den", "stil\u00b7len", "Wand\u00b7rern", "durch", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}