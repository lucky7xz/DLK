{"textgrid.poem.62976": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Priester, der im alten Griechenland", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Priester, der im alten Griechenland", "tokens": ["Ein", "Pries\u00b7ter", ",", "der", "im", "al\u00b7ten", "Grie\u00b7chen\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Ph\u00f6bus opfern wollte,", "tokens": ["Dem", "Ph\u00f6\u00b7bus", "op\u00b7fern", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hielt sich die Nase zu, da seine fromme Hand", "tokens": ["Hielt", "sich", "die", "Na\u00b7se", "zu", ",", "da", "sei\u00b7ne", "from\u00b7me", "Hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Rauchfa\u00df schwingen sollte.", "tokens": ["Das", "Rauch\u00b7fa\u00df", "schwin\u00b7gen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein stolzer Augur siehts und staunt und fragt:", "tokens": ["Ein", "stol\u00b7zer", "Au\u00b7gur", "siehts", "und", "staunt", "und", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Scheust du die Weihrauchd\u00fcfte?", "tokens": ["Scheust", "du", "die", "Weih\u00b7rauch\u00b7d\u00fcf\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbder Duft, erwiedert er, der einem Gott behagt,", "tokens": ["\u00bb", "der", "Duft", ",", "er\u00b7wie\u00b7dert", "er", ",", "der", "ei\u00b7nem", "Gott", "be\u00b7hagt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird Sterblichen zu Gifte.\u00ab", "tokens": ["Wird", "Sterb\u00b7li\u00b7chen", "zu", "Gif\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}