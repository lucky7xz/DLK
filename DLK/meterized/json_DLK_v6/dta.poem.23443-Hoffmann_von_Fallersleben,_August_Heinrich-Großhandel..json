{"dta.poem.23443": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gro\u00dfhandel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sklavenhandel! weh, ich zittre", "tokens": ["Skla\u00b7ven\u00b7han\u00b7del", "!", "weh", ",", "ich", "zitt\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "PTKVZ", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem Worte schon;", "tokens": ["Bei", "dem", "Wor\u00b7te", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alles Grauenvoll' und Bittre", "tokens": ["Al\u00b7les", "Grau\u00b7en\u00b7voll'", "und", "Bitt\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt in diesem Ton.", "tokens": ["Liegt", "in", "die\u00b7sem", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nun, den Frevel hat gerochen", "tokens": ["Nun", ",", "den", "Fre\u00b7vel", "hat", "ge\u00b7ro\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich unsre Zeit,", "tokens": ["End\u00b7lich", "uns\u00b7re", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Endlich ward der Stab gebrochen", "tokens": ["End\u00b7lich", "ward", "der", "Stab", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Grausamkeit.", "tokens": ["Die\u00b7ser", "Grau\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber ach! es schwand im Kleinen", "tokens": ["A\u00b7ber", "ach", "!", "es", "schwand", "im", "Klei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur der Menschenkauf,", "tokens": ["Nur", "der", "Men\u00b7schen\u00b7kauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn im Gro\u00dfen, will es scheinen,", "tokens": ["Denn", "im", "Gro\u00b7\u00dfen", ",", "will", "es", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "$,", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt er niemals auf.", "tokens": ["H\u00f6rt", "er", "nie\u00b7mals", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hat man doch auf den Congressen", "tokens": ["Hat", "man", "doch", "auf", "den", "Con\u00b7gres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seelen gnug verkauft,", "tokens": ["See\u00b7len", "gnug", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hat zur Wohlthat die Finessen", "tokens": ["Hat", "zur", "Wohlt\u00b7hat", "die", "Fi\u00b7nes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gn\u00e4digst umgetauft.", "tokens": ["Gn\u00e4\u00b7digst", "um\u00b7ge\u00b7tauft", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und man wird noch wiederholen", "tokens": ["Und", "man", "wird", "noch", "wie\u00b7der\u00b7ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Wohlthat oft,", "tokens": ["Die\u00b7se", "Wohlt\u00b7hat", "oft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn es giebt noch manches Polen,", "tokens": ["Denn", "es", "giebt", "noch", "man\u00b7ches", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man Theilung hofft.", "tokens": ["Wo", "man", "Thei\u00b7lung", "hofft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}