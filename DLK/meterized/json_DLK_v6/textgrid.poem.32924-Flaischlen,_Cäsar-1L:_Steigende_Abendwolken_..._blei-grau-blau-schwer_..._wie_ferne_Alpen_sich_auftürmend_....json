{"textgrid.poem.32924": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steigende Abendwolken ... blei-grau-blau-schwer ... wie ferne Alpen sich auft\u00fcrmend ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steigende Abendwolken ... blei-grau-blau-schwer ... wie ferne Alpen sich auft\u00fcrmend ...", "tokens": ["Stei\u00b7gen\u00b7de", "Ab\u00b7end\u00b7wol\u00b7ken", "...", "blei\u00b7grau\u00b7blau\u00b7schwer", "...", "wie", "fer\u00b7ne", "Al\u00b7pen", "sich", "auf\u00b7t\u00fcr\u00b7mend", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJD", "$(", "PWAV", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "+--+-+-+--+-+-+-+-+-", "measure": "iambic.octa.plus.invert"}}, "stanza.2": {"line.1": {"text": "die sinkende Sonne dahinter, die R\u00e4nder mit blendendem Gold umkantend ...", "tokens": ["die", "sin\u00b7ken\u00b7de", "Son\u00b7ne", "da\u00b7hin\u00b7ter", ",", "die", "R\u00e4n\u00b7der", "mit", "blen\u00b7den\u00b7dem", "Gold", "um\u00b7kan\u00b7tend", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+--+--+-+-", "measure": "amphibrach.penta.plus"}}, "stanza.3": {"line.1": {"text": "Auf der H\u00fcgelh\u00f6he mitten im gl\u00fchenden Feuer des Abendrots eine M\u00fchle,", "tokens": ["Auf", "der", "H\u00fc\u00b7gel\u00b7h\u00f6\u00b7he", "mit\u00b7ten", "im", "gl\u00fc\u00b7hen\u00b7den", "Feu\u00b7er", "des", "A\u00b7ben\u00b7drots", "ei\u00b7ne", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+--+--+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "langsam die Fl\u00fcgel drehend,", "tokens": ["lang\u00b7sam", "die", "Fl\u00fc\u00b7gel", "dre\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "als schaufle sie der Sonne rinnend Gold in ihre Tenne.", "tokens": ["als", "schauf\u00b7le", "sie", "der", "Son\u00b7ne", "rin\u00b7nend", "Gold", "in", "ih\u00b7re", "Ten\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "VVPP", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}