{"textgrid.poem.34440": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "[hellgr\u00fcne Knospen an den schwarzen Zweigen]", "genre": "verse", "period": "N.A.", "pub_year": 1886, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hellgr\u00fcne Knospen an den schwarzen Zweigen,", "tokens": ["Hell\u00b7gr\u00fc\u00b7ne", "Knos\u00b7pen", "an", "den", "schwar\u00b7zen", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auflebt das Winterholz.", "tokens": ["auf\u00b7lebt", "das", "Win\u00b7ter\u00b7holz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In meiner Brust, der lebensleeren, feigen,", "tokens": ["In", "mei\u00b7ner", "Brust", ",", "der", "le\u00b7bens\u00b7lee\u00b7ren", ",", "fei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "hinsiecht der Stolz.", "tokens": ["hin\u00b7siecht", "der", "Stolz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Ich wollte dich durch alle St\u00fcrme tragen,", "tokens": ["Ich", "woll\u00b7te", "dich", "durch", "al\u00b7le", "St\u00fcr\u00b7me", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "k\u00fchn trotzen dem Geschick,", "tokens": ["k\u00fchn", "trot\u00b7zen", "dem", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "das dich mit grausam kalter Hand geschlagen:", "tokens": ["das", "dich", "mit", "grau\u00b7sam", "kal\u00b7ter", "Hand", "ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "mich traf dein Blick.", "tokens": ["mich", "traf", "dein", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Dein Blick, der sprach: ich hab in dir gefunden,", "tokens": ["Dein", "Blick", ",", "der", "sprach", ":", "ich", "hab", "in", "dir", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "was mich erh\u00e4lt und hebt,", "tokens": ["was", "mich", "er\u00b7h\u00e4lt", "und", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dein stummer Blick, stumm wie das Blut der Wunden,", "tokens": ["dein", "stum\u00b7mer", "Blick", ",", "stumm", "wie", "das", "Blut", "der", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "doch qualdurchbebt.", "tokens": ["doch", "qual\u00b7durch\u00b7bebt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "O dass du mich, o dass du mich erkoren \u2013", "tokens": ["O", "dass", "du", "mich", ",", "o", "dass", "du", "mich", "er\u00b7ko\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PRF", "$,", "FM", "KOUS", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "unwerth und schwach bin ich.", "tokens": ["un\u00b7werth", "und", "schwach", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab die Kraft, ich hab das Gl\u00fcck verloren,", "tokens": ["Ich", "hab", "die", "Kraft", ",", "ich", "hab", "das", "Gl\u00fcck", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "verlor auch dich.", "tokens": ["ver\u00b7lor", "auch", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}