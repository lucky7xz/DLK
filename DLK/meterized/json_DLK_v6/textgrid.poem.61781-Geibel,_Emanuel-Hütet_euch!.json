{"textgrid.poem.61781": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "H\u00fctet euch!", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo am Herd ein Brautpaar siedelt,", "tokens": ["Wo", "am", "Herd", "ein", "Braut\u00b7paar", "sie\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid auf eurer Hut, ihr Knaben,", "tokens": ["Seid", "auf", "eu\u00b7rer", "Hut", ",", "ihr", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahrt, ihr M\u00e4dchen, euer Herz!", "tokens": ["Wahrt", ",", "ihr", "M\u00e4d\u00b7chen", ",", "eu\u00b7er", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn am Morgen, denn am Mittag", "tokens": ["Denn", "am", "Mor\u00b7gen", ",", "denn", "am", "Mit\u00b7tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "$,", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Duft von wilden Rosen", "tokens": ["Wie", "ein", "Duft", "von", "wil\u00b7den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebt die Glut verstohlner K\u00fcsse", "tokens": ["Schwebt", "die", "Glut", "ver\u00b7stohl\u00b7ner", "K\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort bezaubernd in den L\u00fcften.", "tokens": ["Dort", "be\u00b7zau\u00b7bernd", "in", "den", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, und wenn der Abend dunkelt,", "tokens": ["Ach", ",", "und", "wenn", "der", "A\u00b7bend", "dun\u00b7kelt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unverh\u00fcllt durch die Gem\u00e4cher", "tokens": ["Un\u00b7ver\u00b7h\u00fcllt", "durch", "die", "Ge\u00b7m\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wandelt mit geschwungner Fackel", "tokens": ["Wan\u00b7delt", "mit", "ge\u00b7schwung\u00b7ner", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eros dann, und unabl\u00e4ssig", "tokens": ["E\u00b7ros", "dann", ",", "und", "un\u00b7ab\u00b7l\u00e4s\u00b7sig"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "$,", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Spr\u00fchn der Sehnsucht irre Funken", "tokens": ["Spr\u00fchn", "der", "Sehn\u00b7sucht", "ir\u00b7re", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Weiterz\u00fcndend um ihn her.", "tokens": ["Wei\u00b7ter\u00b7z\u00fcn\u00b7dend", "um", "ihn", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo am Herd ein Brautpaar siedelt,", "tokens": ["Wo", "am", "Herd", "ein", "Braut\u00b7paar", "sie\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seid auf eurer Hut, ihr Knaben,", "tokens": ["Seid", "auf", "eu\u00b7rer", "Hut", ",", "ihr", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahrt, ihr M\u00e4dchen, euer Herz!", "tokens": ["Wahrt", ",", "ihr", "M\u00e4d\u00b7chen", ",", "eu\u00b7er", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}