{"textgrid.poem.41422": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Ameise und die Grille", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sang die heischre Grille", "tokens": ["Es", "sang", "die", "heischre", "Gril\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die ganze Sommerzeit,", "tokens": ["Die", "gan\u00b7ze", "Som\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sich in aller Stille", "tokens": ["Da", "sich", "in", "al\u00b7ler", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Ameis' auch erfreut.", "tokens": ["Die", "A\u00b7meis'", "auch", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie h\u00e4uft der Zellen F\u00fclle", "tokens": ["Sie", "h\u00e4uft", "der", "Zel\u00b7len", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit kluger Emsigkeit.", "tokens": ["Mit", "klu\u00b7ger", "Em\u00b7sig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Grille singt voll Freude", "tokens": ["Die", "Gril\u00b7le", "singt", "voll", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um Feld und Busch und Hain,", "tokens": ["Um", "Feld", "und", "Busch", "und", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sammelt kein Getreide", "tokens": ["Und", "sam\u00b7melt", "kein", "Ge\u00b7trei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum n\u00e4chsten Winter ein.", "tokens": ["Zum", "n\u00e4chs\u00b7ten", "Win\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als endlich sich die Sonne", "tokens": ["Als", "end\u00b7lich", "sich", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Umw\u00f6lkt dem Sch\u00fctzen naht,", "tokens": ["Um\u00b7w\u00f6lkt", "dem", "Sch\u00fct\u00b7zen", "naht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Erde keine Wonne,", "tokens": ["Die", "Er\u00b7de", "kei\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Alles Mangel hat;", "tokens": ["Und", "Al\u00b7les", "Man\u00b7gel", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Da f\u00fchlt sie das Geschicke", "tokens": ["Da", "f\u00fchlt", "sie", "das", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Der darbenden Natur,", "tokens": ["Der", "dar\u00b7ben\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und hoffet Trost und Gl\u00fccke", "tokens": ["Und", "hof\u00b7fet", "Trost", "und", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Von ihrer Freundin nur.", "tokens": ["Von", "ih\u00b7rer", "Freun\u00b7din", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Sie sagt: O leiht mir Weizen,", "tokens": ["Sie", "sagt", ":", "O", "leiht", "mir", "Wei\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Geliebte Nachbarin.", "tokens": ["Ge\u00b7lieb\u00b7te", "Nach\u00b7ba\u00b7rin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Ihr werdet ja nicht geizen,", "tokens": ["Ihr", "wer\u00b7det", "ja", "nicht", "gei\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Ihr wi\u00dft, wie arm ich bin.", "tokens": ["Ihr", "wi\u00dft", ",", "wie", "arm", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die fragt: Zur Zeit der Rosen,", "tokens": ["Die", "fragt", ":", "Zur", "Zeit", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was hast du da gemacht?", "tokens": ["Was", "hast", "du", "da", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hat den Virtuosen", "tokens": ["Die", "hat", "den", "Vir\u00b7tu\u00b7o\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht nichts eingebracht.", "tokens": ["Viel\u00b7leicht", "nichts", "ein\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich sang, zwar ungedungen;", "tokens": ["Ich", "sang", ",", "zwar", "un\u00b7ge\u00b7dun\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Allein, was sollt' ich thun?", "tokens": ["Al\u00b7lein", ",", "was", "sollt'", "ich", "thun", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du hast damals gesungen:", "tokens": ["Du", "hast", "da\u00b7mals", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohlan, so tanze nun!", "tokens": ["Wo\u00b7hlan", ",", "so", "tan\u00b7ze", "nun", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}