{"dta.poem.12076": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00e4ngst an Wurzeln, die du von Natur gewannst,", "tokens": ["Du", "h\u00e4ngst", "an", "Wur\u00b7zeln", ",", "die", "du", "von", "Na\u00b7tur", "ge\u00b7wannst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen du dich los nicht rei\u00dfen sollst noch kannst.", "tokens": ["Von", "de\u00b7nen", "du", "dich", "los", "nicht", "rei\u00b7\u00dfen", "sollst", "noch", "kannst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADJD", "PTKNEG", "VVINF", "VMFIN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Wurzeln, deine Volks- und deine Glaubensart,", "tokens": ["Die", "Wur\u00b7zeln", ",", "dei\u00b7ne", "Volks", "und", "dei\u00b7ne", "Glau\u00b7ben\u00b7sart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "TRUNC", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind jede stark f\u00fcr sich, und doppelt stark gepaart.", "tokens": ["Sind", "je\u00b7de", "stark", "f\u00fcr", "sich", ",", "und", "dop\u00b7pelt", "stark", "ge\u00b7paart", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJD", "APPR", "PRF", "$,", "KON", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aus ihnen Nahrung hast du unbewu\u00dft gesogen;", "tokens": ["Aus", "ih\u00b7nen", "Nah\u00b7rung", "hast", "du", "un\u00b7be\u00b7wu\u00dft", "ge\u00b7so\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie halten dich, wo du dich ihnen glaubst entzogen.", "tokens": ["Sie", "hal\u00b7ten", "dich", ",", "wo", "du", "dich", "ih\u00b7nen", "glaubst", "ent\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "PPER", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dich halten sollen sie, doch nicht da\u00df du nicht strebest,", "tokens": ["Dich", "hal\u00b7ten", "sol\u00b7len", "sie", ",", "doch", "nicht", "da\u00df", "du", "nicht", "stre\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$,", "ADV", "PTKNEG", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcber sie hinaus ins Menschliche dich hebest.", "tokens": ["Und", "\u00fc\u00b7ber", "sie", "hin\u00b7aus", "ins", "Menschli\u00b7che", "dich", "he\u00b7best", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APZR", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}}, "stanza.5": {"line.1": {"text": "Des Menschen Kron' ist, da\u00df sich Menschheit offenbart", "tokens": ["Des", "Men\u00b7schen", "Kron'", "ist", ",", "da\u00df", "sich", "Menschheit", "of\u00b7fen\u00b7bart"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "$,", "KOUS", "PRF", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In ihm, trotz seiner Volks-, trotz seiner Glaubensart.", "tokens": ["In", "ihm", ",", "trotz", "sei\u00b7ner", "Volks", ",", "trotz", "sei\u00b7ner", "Glau\u00b7ben\u00b7sart", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "TRUNC", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df an der Menschheit dich, nicht sie an dir du messest,", "tokens": ["Da\u00df", "an", "der", "Menschheit", "dich", ",", "nicht", "sie", "an", "dir", "du", "mes\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "$,", "PTKNEG", "PPER", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht ihre Formenf\u00fcll' in deine Model pressest;", "tokens": ["Nicht", "ih\u00b7re", "For\u00b7men\u00b7f\u00fcll'", "in", "dei\u00b7ne", "Mo\u00b7del", "pres\u00b7sest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht Fremdes deutest um, verf\u00e4lschend seinen Sinn,", "tokens": ["Nicht", "Frem\u00b7des", "deu\u00b7test", "um", ",", "ver\u00b7f\u00e4l\u00b7schend", "sei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PTKVZ", "$,", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil eigens\u00fcchtig du den eignen suchst darinn;", "tokens": ["Weil", "ei\u00b7gen\u00b7s\u00fcch\u00b7tig", "du", "den", "eig\u00b7nen", "suchst", "da\u00b7rinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "ADJA", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nicht dich in deiner Art verstockest und versteifest,", "tokens": ["Nicht", "dich", "in", "dei\u00b7ner", "Art", "ver\u00b7sto\u00b7ckest", "und", "ver\u00b7stei\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebendig nur als Glied im Ganzen dich begreifest;", "tokens": ["Le\u00b7ben\u00b7dig", "nur", "als", "Glied", "im", "Gan\u00b7zen", "dich", "be\u00b7grei\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KOUS", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.9": {"line.1": {"text": "Nicht w\u00e4hnend, da\u00df um dich als Mittelpunkt sich drehn", "tokens": ["Nicht", "w\u00e4h\u00b7nend", ",", "da\u00df", "um", "dich", "als", "Mit\u00b7tel\u00b7punkt", "sich", "drehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "APPR", "PPER", "KOUS", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Welt Entwicklungen, die immer weiter gehn.", "tokens": ["Der", "Welt", "Ent\u00b7wick\u00b7lun\u00b7gen", ",", "die", "im\u00b7mer", "wei\u00b7ter", "gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}}}}