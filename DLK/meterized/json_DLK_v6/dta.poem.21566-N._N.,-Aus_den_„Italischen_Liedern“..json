{"dta.poem.21566": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Aus den \u201eItalischen Liedern\u201c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft Du noch, Du mein toskanisch Kind,", "tokens": ["Wei\u00dft", "Du", "noch", ",", "Du", "mein", "tos\u00b7ka\u00b7nisch", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie wir im fl\u00fcsternden Abendwind", "tokens": ["Wie", "wir", "im", "fl\u00fcs\u00b7tern\u00b7den", "Ab\u00b7end\u00b7wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ueber das Meer gefahren,", "tokens": ["Ue\u00b7ber", "das", "Meer", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Bis in der sternespiegelnden See", "tokens": ["Bis", "in", "der", "ster\u00b7ne\u00b7spie\u00b7geln\u00b7den", "See"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Fels und Stadt Portovenere", "tokens": ["Fels", "und", "Stadt", "Por\u00b7to\u00b7ve\u00b7ne\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "NE"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Leise versunken waren?", "tokens": ["Lei\u00b7se", "ver\u00b7sun\u00b7ken", "wa\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wei\u00dft Du, Luisella, wie liebeberauscht,", "tokens": ["Wei\u00dft", "Du", ",", "Lu\u00b7i\u00b7sel\u00b7la", ",", "wie", "lie\u00b7be\u00b7be\u00b7rauscht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "PWAV", "ADJD", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nur von der Fluth und dem Himmel belauscht,", "tokens": ["Nur", "von", "der", "Fluth", "und", "dem", "Him\u00b7mel", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Unsere Seelen sich fanden,", "tokens": ["Un\u00b7se\u00b7re", "See\u00b7len", "sich", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Lippe auf Lippe lodernd gebrannt", "tokens": ["Lip\u00b7pe", "auf", "Lip\u00b7pe", "lo\u00b7dernd", "ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "ADJD", "VVPP"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und Du dem Sohn aus dem nordischen Land", "tokens": ["Und", "Du", "dem", "Sohn", "aus", "dem", "nor\u00b7di\u00b7schen", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Seligst Geheimni\u00df gestanden?", "tokens": ["Se\u00b7ligst", "Ge\u00b7heim\u00b7ni\u00df", "ge\u00b7stan\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Wei\u00dft Du auch, da\u00df ich besser nie", "tokens": ["Wei\u00dft", "Du", "auch", ",", "da\u00df", "ich", "bes\u00b7ser", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich h\u00e4tte geh\u00f6rt in Lerici", "tokens": ["Dich", "h\u00e4t\u00b7te", "ge\u00b7h\u00f6rt", "in", "Le\u00b7ri\u00b7ci"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVFIN", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "S\u00fc\u00df zur Guitarre singen?", "tokens": ["S\u00fc\u00df", "zur", "Gui\u00b7tar\u00b7re", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nach der Heimath ruft mich neidisch Geschick:", "tokens": ["Nach", "der", "Hei\u00b7math", "ruft", "mich", "nei\u00b7disch", "Ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Fahr wohl, fahr wohl, du Traum von Gl\u00fcck \u2014", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", ",", "du", "Traum", "von", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$,", "PPER", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Weh will das Herz mir zerspringen.", "tokens": ["In", "Weh", "will", "das", "Herz", "mir", "zer\u00b7sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}