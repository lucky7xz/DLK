{"textgrid.poem.33251": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der T\u00e4ler gr\u00fcnstem Tale", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der T\u00e4ler gr\u00fcnstem Tale", "tokens": ["In", "der", "T\u00e4\u00b7ler", "gr\u00fcns\u00b7tem", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat, von Engeln einst bewohnt,", "tokens": ["Hat", ",", "von", "En\u00b7geln", "einst", "be\u00b7wohnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich des Himmels Kathedrale", "tokens": ["Gleich", "des", "Him\u00b7mels", "Ka\u00b7thed\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Golddurchstrahlt ein Schlo\u00df gethront.", "tokens": ["Gold\u00b7durch\u00b7strahlt", "ein", "Schlo\u00df", "ge\u00b7thront", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rings auf Erden diesem Schlosse", "tokens": ["Rings", "auf", "Er\u00b7den", "die\u00b7sem", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keines glich;", "tokens": ["Kei\u00b7nes", "glich", ";"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Herrschte dort mit reichem Trosse", "tokens": ["Herrschte", "dort", "mit", "rei\u00b7chem", "Tros\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gelber Fahnen Faltenschlagen", "tokens": ["Gel\u00b7ber", "Fah\u00b7nen", "Fal\u00b7ten\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flo\u00df wie Sonnengold im Wind \u2013", "tokens": ["Flo\u00df", "wie", "Son\u00b7nen\u00b7gold", "im", "Wind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, es war in alten Tagen,", "tokens": ["Ach", ",", "es", "war", "in", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die nun l\u00e4ngst vergangen sind! \u2013", "tokens": ["Die", "nun", "l\u00e4ngst", "ver\u00b7gan\u00b7gen", "sind", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADV", "VVPP", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Damals kosten s\u00fc\u00dfe L\u00fcfte", "tokens": ["Da\u00b7mals", "kos\u00b7ten", "s\u00fc\u00b7\u00dfe", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lind den Ort,", "tokens": ["Lind", "den", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Zogen als beschwingte D\u00fcfte", "tokens": ["Zo\u00b7gen", "als", "be\u00b7schwing\u00b7te", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von des Schlosses W\u00e4llen fort.", "tokens": ["Von", "des", "Schlos\u00b7ses", "W\u00e4l\u00b7len", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wandrer in dem Tale schauten", "tokens": ["Wand\u00b7rer", "in", "dem", "Ta\u00b7le", "schau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Fenster lichten Glanz", "tokens": ["Durch", "der", "Fens\u00b7ter", "lich\u00b7ten", "Glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Genien, die zum Sang der Lauten", "tokens": ["Ge\u00b7ni\u00b7en", ",", "die", "zum", "Sang", "der", "Lau\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schritten in geme\u00dfnem Tanz", "tokens": ["Schrit\u00b7ten", "in", "ge\u00b7me\u00df\u00b7nem", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um den Thron, auf dem erhaben,", "tokens": ["Um", "den", "Thron", ",", "auf", "dem", "er\u00b7ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "APPR", "ART", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Marmorsch\u00f6n,", "tokens": ["Mar\u00b7mor\u00b7sch\u00f6n", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "W\u00fcrdig solcher Weihegaben,", "tokens": ["W\u00fcr\u00b7dig", "sol\u00b7cher", "Wei\u00b7he\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War des Reiches Herr zu sehn.", "tokens": ["War", "des", "Rei\u00b7ches", "Herr", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Perlen- und rubinenglutend", "tokens": ["Per\u00b7len", "und", "ru\u00b7bi\u00b7nen\u00b7glu\u00b7tend"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War des stolzen Schlosses Tor,", "tokens": ["War", "des", "stol\u00b7zen", "Schlos\u00b7ses", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm entschwebten flutend, flutend", "tokens": ["Ihm", "ent\u00b7schweb\u00b7ten", "flu\u00b7tend", ",", "flu\u00b7tend"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe Echos, die im Chor,", "tokens": ["S\u00fc\u00b7\u00dfe", "E\u00b7chos", ",", "die", "im", "Chor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weithinklingend, froh besangen", "tokens": ["Weit\u00b7hin\u00b7klin\u00b7gend", ",", "froh", "be\u00b7san\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u2013 S\u00fc\u00dfe Pflicht! \u2013", "tokens": ["\u2013", "S\u00fc\u00b7\u00dfe", "Pflicht", "!", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ihres K\u00f6nigs hehres Prangen", "tokens": ["Ih\u00b7res", "K\u00f6\u00b7nigs", "heh\u00b7res", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Weisheit Himmelslicht.", "tokens": ["In", "der", "Weis\u00b7heit", "Him\u00b7mels\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch D\u00e4monen, schwarze Sorgen,", "tokens": ["Doch", "D\u00e4\u00b7mo\u00b7nen", ",", "schwar\u00b7ze", "Sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcrzten roh des K\u00f6nigs Thron. \u2013", "tokens": ["St\u00fcrz\u00b7ten", "roh", "des", "K\u00f6\u00b7nigs", "Thron", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trauert, Freunde, denn kein Morgen", "tokens": ["Trau\u00b7ert", ",", "Freun\u00b7de", ",", "denn", "kein", "Mor\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird ein Schlo\u00df wie dies umlohn!", "tokens": ["Wird", "ein", "Schlo\u00df", "wie", "dies", "um\u00b7lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KOKOM", "PDS", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was da bl\u00fchte, was da gl\u00fchte", "tokens": ["Was", "da", "bl\u00fch\u00b7te", ",", "was", "da", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u2013 Herrlichkeit! \u2013", "tokens": ["\u2013", "Herr\u00b7lich\u00b7keit", "!", "\u2013"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Eine welke M\u00e4rchenbl\u00fcte", "tokens": ["Ei\u00b7ne", "wel\u00b7ke", "M\u00e4r\u00b7chen\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist's aus l\u00e4ngst begrabner Zeit.", "tokens": ["Ist's", "aus", "l\u00e4ngst", "be\u00b7grab\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und durch glutenrote Fenster", "tokens": ["Und", "durch", "glu\u00b7ten\u00b7ro\u00b7te", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Werden heute Wandrer sehn", "tokens": ["Wer\u00b7den", "heu\u00b7te", "Wand\u00b7rer", "sehn"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ungeheure Wahngespenster", "tokens": ["Un\u00b7ge\u00b7heu\u00b7re", "Wahn\u00b7ge\u00b7spens\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grauenhaft im Tanz sich drehn;", "tokens": ["Grau\u00b7en\u00b7haft", "im", "Tanz", "sich", "drehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Tor in wildem Wellen,", "tokens": ["Aus", "dem", "Tor", "in", "wil\u00b7dem", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie ein Meer,", "tokens": ["Wie", "ein", "Meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Weh! sie ", "tokens": ["Weh", "!", "sie"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "PPER"], "meter": "+-", "measure": "trochaic.single"}}}}}