{"textgrid.poem.33907": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Dicker Wald", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid ihr's wieder, finstre W\u00e4lder,", "tokens": ["Seid", "ih\u00b7r's", "wie\u00b7der", ",", "finst\u00b7re", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "PIS", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll von Mord und Tod und Gift,", "tokens": ["Voll", "von", "Mord", "und", "Tod", "und", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo man keine Gr\u00e4nzen-W\u00e4chter,", "tokens": ["Wo", "man", "kei\u00b7ne", "Gr\u00e4n\u00b7zen\u00b7W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch zuweilen R\u00e4uber trifft?", "tokens": ["Doch", "zu\u00b7wei\u00b7len", "R\u00e4u\u00b7ber", "trifft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Belladonna bietet gastlich", "tokens": ["Bel\u00b7la\u00b7don\u00b7na", "bie\u00b7tet", "gast\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre Kirschen, roth und rund,", "tokens": ["Ih\u00b7re", "Kir\u00b7schen", ",", "roth", "und", "rund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Schlange gr\u00fcnes Auge", "tokens": ["Und", "der", "Schlan\u00b7ge", "gr\u00fc\u00b7nes", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blinzt mich an vom schwarzen Grund.", "tokens": ["Blinzt", "mich", "an", "vom", "schwar\u00b7zen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eine Natter als Geschmeide", "tokens": ["Ei\u00b7ne", "Nat\u00b7ter", "als", "Ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Hals, in dumpfem Sinn,", "tokens": ["Um", "den", "Hals", ",", "in", "dum\u00b7pfem", "Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Kauert dort ein gelbes M\u00e4dchen,", "tokens": ["Kau\u00b7ert", "dort", "ein", "gel\u00b7bes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie ist Schlangen-K\u00f6nigin.", "tokens": ["Sie", "ist", "Schlan\u00b7gen\u00b7K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hei, wie f\u00fchlt man hier sein Leben,", "tokens": ["Hei", ",", "wie", "f\u00fchlt", "man", "hier", "sein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PIS", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie h\u00e4ngt man sich daran,", "tokens": ["Und", "wie", "h\u00e4ngt", "man", "sich", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PIS", "PRF", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo aus n\u00e4chstem Busch des R\u00e4ubers", "tokens": ["Wo", "aus", "n\u00e4chs\u00b7tem", "Busch", "des", "R\u00e4u\u00b7bers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Erster Schu\u00df es nehmen kann!", "tokens": ["Ers\u00b7ter", "Schu\u00df", "es", "neh\u00b7men", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zwar ist Nichts bei mir zu holen,", "tokens": ["Zwar", "ist", "Nichts", "bei", "mir", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch so wird die Hand ge\u00fcbt,", "tokens": ["Doch", "so", "wird", "die", "Hand", "ge\u00b7\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich selbst bin ja der Priester,", "tokens": ["Und", "ich", "selbst", "bin", "ja", "der", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ihm im Voraus vergiebt.", "tokens": ["Der", "ihm", "im", "Vo\u00b7raus", "ver\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}