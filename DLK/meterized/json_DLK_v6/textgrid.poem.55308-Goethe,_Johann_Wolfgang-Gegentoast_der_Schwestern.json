{"textgrid.poem.55308": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Gegentoast der Schwestern", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unser Dank, und wenn auch trutzig,", "tokens": ["Un\u00b7ser", "Dank", ",", "und", "wenn", "auch", "trut\u00b7zig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "KOUS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dfend alle lieben G\u00e4ste,", "tokens": ["Gr\u00fc\u00b7\u00dfend", "al\u00b7le", "lie\u00b7ben", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mache keinen Frohen stutzig:", "tokens": ["Ma\u00b7che", "kei\u00b7nen", "Fro\u00b7hen", "stut\u00b7zig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn wir feiern eure Feste.", "tokens": ["Denn", "wir", "fei\u00b7ern", "eu\u00b7re", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sollten aber wir, die Frauen,", "tokens": ["Soll\u00b7ten", "a\u00b7ber", "wir", ",", "die", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dankbar solche Br\u00fcder preisen,", "tokens": ["Dank\u00b7bar", "sol\u00b7che", "Br\u00fc\u00b7der", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, ins Innere zu schauen,", "tokens": ["Die", ",", "ins", "In\u00b7ne\u00b7re", "zu", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer uns zur Seite weisen?", "tokens": ["Im\u00b7mer", "uns", "zur", "Sei\u00b7te", "wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch Amalien, der hehren,", "tokens": ["Doch", "A\u00b7ma\u00b7li\u00b7en", ",", "der", "heh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die auch euch verkl\u00e4rt erscheinet,", "tokens": ["Die", "auch", "euch", "ver\u00b7kl\u00e4rt", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprechend, singend ihr zu Ehren", "tokens": ["Spre\u00b7chend", ",", "sin\u00b7gend", "ihr", "zu", "Eh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind wir doch mit euch vereinet.", "tokens": ["Sind", "wir", "doch", "mit", "euch", "ver\u00b7ei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und indem wir eure Lieder", "tokens": ["Und", "in\u00b7dem", "wir", "eu\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denken keineswegs zu st\u00f6ren,", "tokens": ["Den\u00b7ken", "kei\u00b7nes\u00b7wegs", "zu", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fragen alle sich die Br\u00fcder,", "tokens": ["Fra\u00b7gen", "al\u00b7le", "sich", "die", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sie ohne Schwestern w\u00e4ren.", "tokens": ["Was", "sie", "oh\u00b7ne", "Schwes\u00b7tern", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}