{"dta.poem.11535": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Fussreise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Am frischgeschnitt'nen Wanderstab,", "tokens": ["Am", "frischge\u00b7schnitt'\u00b7nen", "Wan\u00b7der\u00b7stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich in der Fr\u00fche", "tokens": ["Wenn", "ich", "in", "der", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "So durch W\u00e4lder ziehe,", "tokens": ["So", "durch", "W\u00e4l\u00b7der", "zie\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00fcgel auf und ab:", "tokens": ["H\u00fc\u00b7gel", "auf", "und", "ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dann wie's V\u00f6gelein im Laube", "tokens": ["Dann", "wie's", "V\u00f6\u00b7ge\u00b7lein", "im", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Singet und sich r\u00fchrt,", "tokens": ["Sin\u00b7get", "und", "sich", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PRF", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Oder wie die goldne Traube", "tokens": ["O\u00b7der", "wie", "die", "gold\u00b7ne", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wonnegeister sp\u00fcrt", "tokens": ["Won\u00b7ne\u00b7geis\u00b7ter", "sp\u00fcrt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "In der ersten Morgensonne:", "tokens": ["In", "der", "ers\u00b7ten", "Mor\u00b7gen\u00b7son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "So f\u00fchlt auch mein alter, lieber", "tokens": ["So", "f\u00fchlt", "auch", "mein", "al\u00b7ter", ",", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "$,", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Adam Herbst- und Fr\u00fchlingsfieber,", "tokens": ["A\u00b7dam", "Herbs\u00b7t", "und", "Fr\u00fch\u00b7lings\u00b7fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "KON", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Gottbeherzte,", "tokens": ["Gott\u00b7be\u00b7herz\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Nie verscherzte", "tokens": ["Nie", "ver\u00b7scherz\u00b7te"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Erstlings-Paradieseswonne.", "tokens": ["Erst\u00b7lings\u00b7Pa\u00b7ra\u00b7die\u00b7ses\u00b7won\u00b7ne", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also bist du nicht so schlimm, o alter", "tokens": ["Al\u00b7so", "bist", "du", "nicht", "so", "schlimm", ",", "o", "al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "FM", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Adam, wie die strengen Lehrer sagen:", "tokens": ["A\u00b7dam", ",", "wie", "die", "stren\u00b7gen", "Leh\u00b7rer", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Liebst und lobst du immer doch,", "tokens": ["Liebst", "und", "lobst", "du", "im\u00b7mer", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Singst und preisest immer noch,", "tokens": ["Singst", "und", "prei\u00b7sest", "im\u00b7mer", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie an ewig neuen Sch\u00f6pfungstagen", "tokens": ["Wie", "an", "e\u00b7wig", "neu\u00b7en", "Sch\u00f6p\u00b7fungs\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Deinen lieben Sch\u00f6pfer und Erhalter.", "tokens": ["Dei\u00b7nen", "lie\u00b7ben", "Sch\u00f6p\u00b7fer", "und", "Er\u00b7hal\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "M\u00f6cht' es Dieser geben,", "tokens": ["M\u00f6cht'", "es", "Die\u00b7ser", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und mein ganzes Leben", "tokens": ["Und", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4r' im leichten Wanderschwei\u00dfe", "tokens": ["W\u00e4r'", "im", "leich\u00b7ten", "Wan\u00b7der\u00b7schwei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine solche Morgenreise!", "tokens": ["Ei\u00b7ne", "sol\u00b7che", "Mor\u00b7gen\u00b7rei\u00b7se", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}