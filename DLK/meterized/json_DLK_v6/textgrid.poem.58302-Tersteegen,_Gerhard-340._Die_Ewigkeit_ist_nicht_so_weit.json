{"textgrid.poem.58302": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "340. Die Ewigkeit ist nicht so weit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die stille Ewigkeit ist unserm Geiste nah,", "tokens": ["Die", "stil\u00b7le", "E\u00b7wig\u00b7keit", "ist", "un\u00b7serm", "Geis\u00b7te", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er durch Gott von Zeit und Kreatur entbunden;", "tokens": ["Wenn", "er", "durch", "Gott", "von", "Zeit", "und", "Kre\u00b7a\u00b7tur", "ent\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kehr aus der Vielheit ein, Gott ist nur Eines ja,", "tokens": ["Kehr", "aus", "der", "Viel\u00b7heit", "ein", ",", "Gott", "ist", "nur", "Ei\u00b7nes", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "NN", "VAFIN", "ADV", "PIS", "ADV", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "O selig, wer dies Gut in seinem Grund gefunden!", "tokens": ["O", "se\u00b7lig", ",", "wer", "dies", "Gut", "in", "sei\u00b7nem", "Grund", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "PDS", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}