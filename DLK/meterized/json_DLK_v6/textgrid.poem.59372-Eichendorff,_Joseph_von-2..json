{"textgrid.poem.59372": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von k\u00fchnen Wunderbildern", "tokens": ["Von", "k\u00fch\u00b7nen", "Wun\u00b7der\u00b7bil\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein gro\u00dfer Tr\u00fcmmerhauf,", "tokens": ["Ein", "gro\u00b7\u00dfer", "Tr\u00fcm\u00b7mer\u00b7hauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In reizendem Verwildern", "tokens": ["In", "rei\u00b7zen\u00b7dem", "Ver\u00b7wil\u00b7dern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein bl\u00fchnder Garten drauf;", "tokens": ["Ein", "bl\u00fchn\u00b7der", "Gar\u00b7ten", "drauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Versunknes Reich zu F\u00fc\u00dfen,", "tokens": ["Ver\u00b7sunk\u00b7nes", "Reich", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Himmel fern und nah,", "tokens": ["Vom", "Him\u00b7mel", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus anderm Reich ein Gr\u00fc\u00dfen \u2013", "tokens": ["Aus", "an\u00b7derm", "Reich", "ein", "Gr\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist Italia!", "tokens": ["Das", "ist", "I\u00b7ta\u00b7lia", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NE", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Wenn Fr\u00fchlingsl\u00fcfte wehen", "tokens": ["Wenn", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", "we\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hold \u00fcbern gr\u00fcnen Plan,", "tokens": ["Hold", "\u00fc\u00b7bern", "gr\u00fc\u00b7nen", "Plan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein leises Auferstehen", "tokens": ["Ein", "lei\u00b7ses", "Auf\u00b7er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hebt in den T\u00e4lern an.", "tokens": ["Hebt", "in", "den", "T\u00e4\u00b7lern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Da will sich's unten r\u00fchren", "tokens": ["Da", "will", "sich's", "un\u00b7ten", "r\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im stillen G\u00f6ttergrab,", "tokens": ["Im", "stil\u00b7len", "G\u00f6t\u00b7ter\u00b7grab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch kann's schauernd sp\u00fcren", "tokens": ["Der", "Mensch", "kann's", "schau\u00b7ernd", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief in die Brust hinab.", "tokens": ["Tief", "in", "die", "Brust", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Verwirrend in den B\u00e4umen", "tokens": ["Ver\u00b7wir\u00b7rend", "in", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gehn Stimmen hin und her,", "tokens": ["Gehn", "Stim\u00b7men", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein sehnsuchtsvolles Tr\u00e4umen", "tokens": ["Ein", "sehn\u00b7suchts\u00b7vol\u00b7les", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weht \u00fcbers blaue Meer.", "tokens": ["Weht", "\u00fc\u00b7bers", "blau\u00b7e", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und unterm duft'gen Schleier", "tokens": ["Und", "un\u00b7term", "duft'\u00b7gen", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sooft der Lenz erwacht,", "tokens": ["Sooft", "der", "Lenz", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Webt in geheimer Feier", "tokens": ["Webt", "in", "ge\u00b7hei\u00b7mer", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die alte Zaubermacht.", "tokens": ["Die", "al\u00b7te", "Zau\u00b7ber\u00b7macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Frau Venus h\u00f6rt das Locken,", "tokens": ["Frau", "Ve\u00b7nus", "h\u00f6rt", "das", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der V\u00f6gel heitern Chor,", "tokens": ["Der", "V\u00f6\u00b7gel", "hei\u00b7tern", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und richtet froh erschrocken", "tokens": ["Und", "rich\u00b7tet", "froh", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Blumen sich empor.", "tokens": ["Aus", "Blu\u00b7men", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie sucht die alten Stellen,", "tokens": ["Sie", "sucht", "die", "al\u00b7ten", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das luft'ge S\u00e4ulenhaus,", "tokens": ["Das", "luft'\u00b7ge", "S\u00e4u\u00b7len\u00b7haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut l\u00e4chelnd in die Wellen", "tokens": ["Schaut", "l\u00e4\u00b7chelnd", "in", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fr\u00fchlingsluft hinaus.", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7luft", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch \u00f6d sind nun die Stellen,", "tokens": ["Doch", "\u00f6d", "sind", "nun", "die", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stumm liegt ihr S\u00e4ulenhaus,", "tokens": ["Stumm", "liegt", "ihr", "S\u00e4u\u00b7len\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gras w\u00e4chst da auf den Schwellen,", "tokens": ["Gras", "w\u00e4chst", "da", "auf", "den", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Wind zieht ein und aus.", "tokens": ["Der", "Wind", "zieht", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wo sind nun die Gespielen?", "tokens": ["Wo", "sind", "nun", "die", "Ge\u00b7spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Diana schl\u00e4ft im Wald,", "tokens": ["Di\u00b7a\u00b7na", "schl\u00e4ft", "im", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Neptunus ruht im k\u00fchlen", "tokens": ["Nep\u00b7tu\u00b7nus", "ruht", "im", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meerschlo\u00df, das einsam hallt.", "tokens": ["Meer\u00b7schlo\u00df", ",", "das", "ein\u00b7sam", "hallt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Zuweilen nur Sirenen", "tokens": ["Zu\u00b7wei\u00b7len", "nur", "Si\u00b7re\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch tauchen aus dem Grund,", "tokens": ["Noch", "tau\u00b7chen", "aus", "dem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tun in irren T\u00f6nen", "tokens": ["Und", "tun", "in", "ir\u00b7ren", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die tiefe Wehmut kund. \u2013", "tokens": ["Die", "tie\u00b7fe", "Weh\u00b7mut", "kund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie selbst mu\u00df sinnend stehen", "tokens": ["Sie", "selbst", "mu\u00df", "sin\u00b7nend", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleich im Fr\u00fchlingsschein,", "tokens": ["So", "bleich", "im", "Fr\u00fch\u00b7lings\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen untergehen,", "tokens": ["Die", "Au\u00b7gen", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sch\u00f6ne Leib wird Stein. \u2013", "tokens": ["Der", "sch\u00f6\u00b7ne", "Leib", "wird", "Stein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Denn \u00fcber Land und Wogen", "tokens": ["Denn", "\u00fc\u00b7ber", "Land", "und", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erscheint, so still und mild,", "tokens": ["Er\u00b7scheint", ",", "so", "still", "und", "mild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch auf dem Regenbogen", "tokens": ["Hoch", "auf", "dem", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein andres Frauenbild.", "tokens": ["Ein", "and\u00b7res", "Frau\u00b7en\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ein Kindlein in den Armen", "tokens": ["Ein", "Kin\u00b7dlein", "in", "den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Wunderbare h\u00e4lt,", "tokens": ["Die", "Wun\u00b7der\u00b7ba\u00b7re", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und himmlisches Erbarmen", "tokens": ["Und", "himm\u00b7li\u00b7sches", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durchdringt die ganze Welt.", "tokens": ["Durch\u00b7dringt", "die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Da in den lichten R\u00e4umen", "tokens": ["Da", "in", "den", "lich\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erwacht das Menschenkind,", "tokens": ["Er\u00b7wacht", "das", "Men\u00b7schen\u00b7kind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00fcttelt b\u00f6ses Tr\u00e4umen", "tokens": ["Und", "sch\u00fct\u00b7telt", "b\u00f6\u00b7ses", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von seinem Haupt geschwind.", "tokens": ["Von", "sei\u00b7nem", "Haupt", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Und, wie die Lerche singend,", "tokens": ["Und", ",", "wie", "die", "Ler\u00b7che", "sin\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus schw\u00fclen Zaubers Kluft", "tokens": ["Aus", "schw\u00fc\u00b7len", "Zau\u00b7bers", "Kluft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erhebt die Seele ringend", "tokens": ["Er\u00b7hebt", "die", "See\u00b7le", "rin\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich in die Morgenluft.", "tokens": ["Sich", "in", "die", "Mor\u00b7gen\u00b7luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}