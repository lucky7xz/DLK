["Als", "sie", "mit", "ih\u00b7rem", "Mann", "dem", "Thra\u00b7sea", "ver\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Der Reigersbergin Ruhm bi\u00df an die Sterne steiget/", "tokens": ["Der", "Rei\u00b7gers\u00b7ber\u00b7gin", "Ruhm", "bi\u00df", "an", "die", "Ster\u00b7ne", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durch welche Grotius/ der Ph\u00f6nix unsrer Zeit/", "tokens": ["Durch", "wel\u00b7che", "Gro\u00b7ti\u00b7us", "/", "der", "Ph\u00f6\u00b7nix", "uns\u00b7rer", "Zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NE", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.39": {"text": "Mit neu-erdachter List die scharffe Wacht beschweiget/", "tokens": ["Mit", "neu\u00b7er\u00b7dach\u00b7ter", "List", "die", "scharf\u00b7fe", "Wacht", "be\u00b7schwei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "L\u00e4st sie zur Geissel da/ und macht sich anderweit.", "tokens": ["L\u00e4st", "sie", "zur", "Geis\u00b7sel", "da", "/", "und", "macht", "sich", "an\u00b7der\u00b7weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$(", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+