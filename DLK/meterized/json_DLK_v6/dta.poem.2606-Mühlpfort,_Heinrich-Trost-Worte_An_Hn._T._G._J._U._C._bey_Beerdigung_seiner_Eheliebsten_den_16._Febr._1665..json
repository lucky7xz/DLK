{"dta.poem.2606": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Worte/  \n  An Hn. T. G.   J. U. C.   bey Beerdigung seiner  \n Eheliebsten/ den 16. Febr. 1665.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ihm das gr\u00f6ste Theil des Hertzens sey ger\u00fchret/", "tokens": ["Da\u00df", "ihm", "das", "gr\u00f6s\u00b7te", "Theil", "des", "Hert\u00b7zens", "sey", "ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dieser Fall/ mein Freund/ bi\u00df an die Seele geht/", "tokens": ["Da\u00df", "die\u00b7ser", "Fall", "/", "mein", "Freund", "/", "bi\u00df", "an", "die", "See\u00b7le", "geht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "$(", "PPOSAT", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darff keiner Wunderung. Wo Liebe wird gesp\u00fcret/", "tokens": ["Darff", "kei\u00b7ner", "Wun\u00b7de\u00b7rung", ".", "Wo", "Lie\u00b7be", "wird", "ge\u00b7sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "$.", "PWAV", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wird zu gleich das Leid/ so bald was k\u00f6m\u0303t/ erh\u00f6ht.", "tokens": ["Da", "wird", "zu", "gleich", "das", "Leid", "/", "so", "bald", "was", "k\u00f6m\u0303t", "/", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "ART", "NN", "$(", "ADV", "ADV", "PWS", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Augen so zuvor den Weg zur Huld gezeiget/", "tokens": ["Die", "Au\u00b7gen", "so", "zu\u00b7vor", "den", "Weg", "zur", "Huld", "ge\u00b7zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In denen Lieb und Gunst als wie zusammen lieff/", "tokens": ["In", "de\u00b7nen", "Lieb", "und", "Gunst", "als", "wie", "zu\u00b7sam\u00b7men", "lieff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "KON", "NN", "KOUS", "KOKOM", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sind weiter nur ein Quell au\u00df dem die Thr\u00e4ne steiget/", "tokens": ["Sind", "wei\u00b7ter", "nur", "ein", "Quell", "au\u00df", "dem", "die", "Thr\u00e4\u00b7ne", "stei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sagt auf welcher Fluth schwebt der Gedancken Schiff.", "tokens": ["Und", "sagt", "auf", "wel\u00b7cher", "Fluth", "schwebt", "der", "Ge\u00b7dan\u00b7cken", "Schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PWAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein eintzig Jahr gewehrt ihm Lust vermengt mit Leiden/", "tokens": ["Ein", "eint\u00b7zig", "Jahr", "ge\u00b7wehrt", "ihm", "Lust", "ver\u00b7mengt", "mit", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVPP", "PPER", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Rosen-Liebe so zu erst die Eh fing an;", "tokens": ["Die", "Ro\u00b7sen\u00b7Lie\u00b7be", "so", "zu", "erst", "die", "Eh", "fing", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mu\u00df sich jetzt mit Napell/ mit Bux und Rauten kleiden/", "tokens": ["Mu\u00df", "sich", "jetzt", "mit", "Na\u00b7pell", "/", "mit", "Bux", "und", "Rau\u00b7ten", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "APPR", "NE", "$(", "APPR", "NE", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer dencket/ was alsdenn nicht so ein Wechsel kan?", "tokens": ["Wer", "den\u00b7cket", "/", "was", "als\u00b7denn", "nicht", "so", "ein", "Wech\u00b7sel", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "ADV", "PTKNEG", "ADV", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Hier ist kein Eigenthum/ es wird nur dargeliehen/", "tokens": ["Hier", "ist", "kein", "Ei\u00b7gen\u00b7thum", "/", "es", "wird", "nur", "dar\u00b7ge\u00b7lie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$(", "PPER", "VAFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wie Kleider zu dem Spiel. Nach dem das Ende schleust/", "tokens": ["Wie", "Klei\u00b7der", "zu", "dem", "Spiel", ".", "Nach", "dem", "das", "En\u00b7de", "schleust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$.", "APPR", "ART", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mu\u00df der vor K\u00f6nig war/ den Purpur-Rock ausziehen/", "tokens": ["Mu\u00df", "der", "vor", "K\u00f6\u00b7nig", "war", "/", "den", "Pur\u00b7pur\u00b7Rock", "aus\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "APPR", "NN", "VAFIN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und jeder lernt da\u00df man in frembden Kleidern gleist.", "tokens": ["Und", "je\u00b7der", "lernt", "da\u00df", "man", "in", "fremb\u00b7den", "Klei\u00b7dern", "gleist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KOUS", "PIS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Leben selbsten ist leibeigen nicht gegeben/", "tokens": ["Das", "Le\u00b7ben", "selbs\u00b7ten", "ist", "leib\u00b7ei\u00b7gen", "nicht", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Brauch der Jahre hat schon sein gesetztes Ziel.", "tokens": ["Der", "Brauch", "der", "Jah\u00b7re", "hat", "schon", "sein", "ge\u00b7setz\u00b7tes", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Dem eintzig-weisen ", "tokens": ["Dem", "eint\u00b7zig\u00b7wei\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Der mu\u00df gezwungen fort der nicht gutwillig will.", "tokens": ["Der", "mu\u00df", "ge\u00b7zwun\u00b7gen", "fort", "der", "nicht", "gut\u00b7wil\u00b7lig", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVPP", "PTKVZ", "ART", "PTKNEG", "ADJD", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Zwar di\u00df verwerff\u2019 ich nicht die Todten zu beweinen/", "tokens": ["Zwar", "di\u00df", "ver\u00b7werff'", "ich", "nicht", "die", "Tod\u00b7ten", "zu", "be\u00b7wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wir aber sind vielmehr/ denn jene/ Thr\u00e4nen werth/", "tokens": ["Wir", "a\u00b7ber", "sind", "viel\u00b7mehr", "/", "denn", "je\u00b7ne", "/", "Thr\u00e4\u00b7nen", "werth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "$(", "KON", "PDAT", "$(", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Himmel kan uns nicht stets hell und g\u00f6lden scheinen/", "tokens": ["Der", "Him\u00b7mel", "kan", "uns", "nicht", "stets", "hell", "und", "g\u00f6l\u00b7den", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "KON", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wer wol gestorben ist/ der wird nicht mehr beschwert.", "tokens": ["Wer", "wol", "ge\u00b7stor\u00b7ben", "ist", "/", "der", "wird", "nicht", "mehr", "be\u00b7schwert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$(", "ART", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es sey da\u00df ihm/ mein Freund/ die Liebste fr\u00fch verblichen/", "tokens": ["Es", "sey", "da\u00df", "ihm", "/", "mein", "Freund", "/", "die", "Liebs\u00b7te", "fr\u00fch", "ver\u00b7bli\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "PPER", "$(", "PPOSAT", "NN", "$(", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Man glaub\u2019ts/ der Eltern Angst vergr\u00f6ssert seinen Schmertz.", "tokens": ["Man", "glaub'ts", "/", "der", "El\u00b7tern", "Angst", "ver\u00b7gr\u00f6s\u00b7sert", "sei\u00b7nen", "Schmertz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df sie bey strenger Noth von dieser Welt gewichen/", "tokens": ["Da\u00df", "sie", "bey", "stren\u00b7ger", "Noth", "von", "die\u00b7ser", "Welt", "ge\u00b7wi\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Zugleich ein Pfand verl\u00e4st/ drumb blutet jetzt das Hertz.", "tokens": ["Zu\u00b7gleich", "ein", "Pfand", "ver\u00b7l\u00e4st", "/", "drumb", "blu\u00b7tet", "jetzt", "das", "Hertz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$(", "PAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Je dennoch wenn er will des H\u00f6chsten Schlu\u00df erwegen/", "tokens": ["Je", "den\u00b7noch", "wenn", "er", "will", "des", "H\u00f6chs\u00b7ten", "Schlu\u00df", "er\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wie die Liebst\u2019 ihn treu bi\u00df in den Tod geliebt/", "tokens": ["Und", "wie", "die", "Liebst'", "ihn", "treu", "bi\u00df", "in", "den", "Tod", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADJD", "APPR", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So wird der wache Schmertz sich endlich schlaffen iegen/", "tokens": ["So", "wird", "der", "wa\u00b7che", "Schmertz", "sich", "end\