{"dta.poem.19054": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.  An des Geheimen  Conferenz-Mini-  \n stri  Frey-Herrn von Gersdorf 45.  \n Geburts-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "O Auge! dem des Abgrunds tiefe Schl\u00fcnde", "tokens": ["O", "Au\u00b7ge", "!", "dem", "des", "Ab\u00b7grunds", "tie\u00b7fe", "Schl\u00fcn\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So nahe sind, als die gestirnte Bahn,", "tokens": ["So", "na\u00b7he", "sind", ",", "als", "die", "ge\u00b7stirn\u00b7te", "Bahn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es weidet sich dein Blick im Thal der Gr\u00fcnde,", "tokens": ["Es", "wei\u00b7det", "sich", "dein", "Blick", "im", "Thal", "der", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kaum streiffet er die stoltzen Cedern an,", "tokens": ["Kaum", "streif\u00b7fet", "er", "die", "stolt\u00b7zen", "Ce\u00b7dern", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir wollen dich bey dieser Abend-Zeit", "tokens": ["Wir", "wol\u00b7len", "dich", "bey", "die\u00b7ser", "A\u00b7ben\u00b7dZeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Allm\u00e4chtiger! auf unsre Art erh\u00f6hn,", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "!", "auf", "uns\u00b7re", "Art", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und w\u00e4ren gern von deiner H\u00f6h gesehn,", "tokens": ["Und", "w\u00e4\u00b7ren", "gern", "von", "dei\u00b7ner", "H\u00f6h", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Drum b\u00fccken wir uns tief mit Niedrigkeit.", "tokens": ["Drum", "b\u00fc\u00b7cken", "wir", "uns", "tief", "mit", "Nied\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Bi\u00df diesen Tag, mit gleicher Kraft geregt,", "tokens": ["Bi\u00df", "die\u00b7sen", "Tag", ",", "mit", "glei\u00b7cher", "Kraft", "ge\u00b7regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der nicht bedarf, da\u00df man ihm unterzwinge,", "tokens": ["Der", "nicht", "be\u00b7darf", ",", "da\u00df", "man", "ihm", "un\u00b7ter\u00b7zwin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sich aus Trotz vor seine St\u00e4rcke legt,", "tokens": ["Was", "sich", "aus", "Trotz", "vor", "sei\u00b7ne", "St\u00e4r\u00b7cke", "legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von dessen Schlag der Erd-Krei\u00df bebt und kracht;", "tokens": ["Von", "des\u00b7sen", "Schlag", "der", "Erd\u00b7Krei\u00df", "bebt", "und", "kracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du hebst und tr\u00e4gst der Deinen kleine Zahl,", "tokens": ["Du", "hebst", "und", "tr\u00e4gst", "der", "Dei\u00b7nen", "klei\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du f\u00fchrest sie so sanft durchs Jammerthal,", "tokens": ["Du", "f\u00fch\u00b7rest", "sie", "so", "sanft", "durchs", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Man \u00fcbergiebt sich willig deiner Macht.", "tokens": ["Man", "\u00fc\u00b7berg\u00b7iebt", "sich", "wil\u00b7lig", "dei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O ", "tokens": ["O"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Dieweil du selbst ein GOtt der Ordnung bist;", "tokens": ["Die\u00b7weil", "du", "selbst", "ein", "Gott", "der", "Ord\u00b7nung", "bist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So h\u00e4ltest du auch alles in den Schrancken.", "tokens": ["So", "h\u00e4l\u00b7test", "du", "auch", "al\u00b7les", "in", "den", "Schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum wissen wir, da\u00df dirs gef\u00e4llig ist,", "tokens": ["Drum", "wis\u00b7sen", "wir", ",", "da\u00df", "dirs", "ge\u00b7f\u00e4l\u00b7lig", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn man sich gern vor denen neigen mag,", "tokens": ["Wenn", "man", "sich", "gern", "vor", "de\u00b7nen", "nei\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "APPR", "PDS", "VVFIN", "VMFIN", "$,"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Die deine Macht auf H\u00f6hen treten l\u00e4st.", "tokens": ["Die", "dei\u00b7ne", "Macht", "auf", "H\u00f6\u00b7hen", "tre\u00b7ten", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drum heisset uns das heut\u2019ge Jahres-Fest", "tokens": ["Drum", "heis\u00b7set", "uns", "das", "heut'\u00b7ge", "Jah\u00b7res\u00b7Fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir ehren den, den heute deine Rechte,", "tokens": ["Wir", "eh\u00b7ren", "den", ",", "den", "heu\u00b7te", "dei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du Lebens-GOtt zum ersten vorgezeigt:", "tokens": ["Du", "Le\u00b7bens\u00b7GOtt", "zum", "ers\u00b7ten", "vor\u00b7ge\u00b7zeigt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer liebet nicht sein eigenes Geschlechte?", "tokens": ["Wer", "lie\u00b7bet", "nicht", "sein", "ei\u00b7ge\u00b7nes", "Ge\u00b7schlech\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Drum sind wir ihm mit Liebe zugeneigt.", "tokens": ["Drum", "sind", "wir", "ihm", "mit", "Lie\u00b7be", "zu\u00b7gen\u00b7eigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was sollen wir ihm nur zu gute thun?", "tokens": ["Was", "sol\u00b7len", "wir", "ihm", "nur", "zu", "gu\u00b7te", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Was dich erblickt, das sehnet sich nach dir,", "tokens": ["Was", "dich", "er\u00b7blickt", ",", "das", "seh\u00b7net", "sich", "nach", "dir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "PDS", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer so wie du sein Wohlthun kan verneuen,", "tokens": ["Wer", "so", "wie", "du", "sein", "Wohl\u00b7thun", "kan", "ver\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KOKOM", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verneuet leicht der Seinen Liebs-Begier;", "tokens": ["Ver\u00b7neu\u00b7et", "leicht", "der", "Sei\u00b7nen", "Liebs\u00b7Be\u00b7gier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verneue dich dem ", "tokens": ["Ver\u00b7neu\u00b7e", "dich", "dem"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Erneure ihm den sch\u00f6nen Liebes-Rath!", "tokens": ["Er\u00b7neu\u00b7re", "ihm", "den", "sch\u00f6\u00b7nen", "Lie\u00b7bes\u00b7Rath", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der hat genug, der dich alleine hat;", "tokens": ["Der", "hat", "ge\u00b7nug", ",", "der", "dich", "al\u00b7lei\u00b7ne", "hat", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Noch bindest du gar an ein sanftes Joch.", "tokens": ["Noch", "bin\u00b7dest", "du", "gar", "an", "ein", "sanf\u00b7tes", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was k\u00f6nnen wir ihm zur Verehrung reichen?", "tokens": ["Was", "k\u00f6n\u00b7nen", "wir", "ihm", "zur", "Ver\u00b7eh\u00b7rung", "rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Geld noch Gut; das hast du schon versehn,", "tokens": ["Nicht", "Geld", "noch", "Gut", ";", "das", "hast", "du", "schon", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "ADJD", "$.", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Ehr und Gl\u00fcck, er tr\u00e4gt davon die Zeichen:", "tokens": ["Nicht", "Ehr", "und", "Gl\u00fcck", ",", "er", "tr\u00e4gt", "da\u00b7von", "die", "Zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum bleibt der Wunsch bey ", "tokens": ["Drum", "bleibt", "der", "Wunsch", "bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die g\u00f6nne ihm noch manches Lebens-Jahr,", "tokens": ["Die", "g\u00f6n\u00b7ne", "ihm", "noch", "man\u00b7ches", "Le\u00b7bens\u00b7Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die schencke ihm recht viel Vergn\u00fcglichkeit,", "tokens": ["Die", "schen\u00b7cke", "ihm", "recht", "viel", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die leite ihn durch diese Wander-Zeit,", "tokens": ["Die", "lei\u00b7te", "ihn", "durch", "die\u00b7se", "Wan\u00b7der\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das thue sie, ", "tokens": ["Das", "thue", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Das immerdar in lautrer Liebe wallt,", "tokens": ["Das", "im\u00b7mer\u00b7dar", "in", "laut\u00b7rer", "Lie\u00b7be", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du festes Schlo\u00df, und gegen alle Schilde", "tokens": ["Du", "fes\u00b7tes", "Schlo\u00df", ",", "und", "ge\u00b7gen", "al\u00b7le", "Schil\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "KON", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Widrigen, ", "tokens": ["Der", "Wid\u00b7ri\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Nur sperr uns da den Einla\u00df nimmer zu,", "tokens": ["Nur", "sperr", "uns", "da", "den", "Ein\u00b7la\u00df", "nim\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn wenn die Kraft der Finsterni\u00df bey Nacht", "tokens": ["Denn", "wenn", "die", "Kraft", "der", "Fins\u00b7ter\u00b7ni\u00df", "bey", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich hier und da genug zu schaffen macht;", "tokens": ["Sich", "hier", "und", "da", "ge\u00b7nug", "zu", "schaf\u00b7fen", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}