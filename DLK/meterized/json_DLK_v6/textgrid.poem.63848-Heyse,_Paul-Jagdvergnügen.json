{"textgrid.poem.63848": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Jagdvergn\u00fcgen", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun knallt es wieder an allen Enden", "tokens": ["Nun", "knallt", "es", "wie\u00b7der", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In \u00d6lbaumhalden und Rebgel\u00e4nden,", "tokens": ["In", "\u00d6l\u00b7baum\u00b7hal\u00b7den", "und", "Reb\u00b7ge\u00b7l\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den kleinen V\u00f6geln den Tod zu bringen,", "tokens": ["Den", "klei\u00b7nen", "V\u00f6\u00b7geln", "den", "Tod", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die kaum erst sch\u00fcchtern ihr Liedchen singen.", "tokens": ["Die", "kaum", "erst", "sch\u00fcch\u00b7tern", "ihr", "Lied\u00b7chen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Denn mit dem Fr\u00fchling an dieser K\u00fcste", "tokens": ["Denn", "mit", "dem", "Fr\u00fch\u00b7ling", "an", "die\u00b7ser", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Erwacht den Menschen das Mordgel\u00fcste.", "tokens": ["Er\u00b7wacht", "den", "Men\u00b7schen", "das", "Mord\u00b7ge\u00b7l\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Mit Schie\u00dfgewehren von allen Arten", "tokens": ["Mit", "Schie\u00df\u00b7ge\u00b7weh\u00b7ren", "von", "al\u00b7len", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Schleichen sie durch den Gottesgarten,", "tokens": ["Schlei\u00b7chen", "sie", "durch", "den", "Got\u00b7tes\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Zumal den Sonn- und Feiertag", "tokens": ["Zu\u00b7mal", "den", "Sonn", "und", "Fei\u00b7er\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Man so am liebsten \u00bbheiligen\u00ab mag,", "tokens": ["Man", "so", "am", "liebs\u00b7ten", "\u00bb", "hei\u00b7li\u00b7gen", "\u00ab", "mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "ADV", "PTKA", "ADJD", "$(", "ADJA", "$(", "VMFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Und wo sie ersp\u00e4hn ein klein Gefieder \u2013", "tokens": ["Und", "wo", "sie", "er\u00b7sp\u00e4hn", "ein", "klein", "Ge\u00b7fie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "ADJD", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Piff paff! \u2013 da taumelt's zerfledert nieder.", "tokens": ["Piff", "paff", "!", "\u2013", "da", "tau\u00b7melt's", "zer\u00b7fle\u00b7dert", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "KOUS", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Den armen Braten, der in zwei Bissen", "tokens": ["Den", "ar\u00b7men", "Bra\u00b7ten", ",", "der", "in", "zwei", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "CARD", "NN"], "meter": "-+-+-++-+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "Verschluckt ist, k\u00f6nnten sie leichtlich missen,", "tokens": ["Ver\u00b7schluckt", "ist", ",", "k\u00f6nn\u00b7ten", "sie", "leicht\u00b7lich", "mis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VMFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und mit dem spr\u00fchenden Vogeldunst", "tokens": ["Und", "mit", "dem", "spr\u00fc\u00b7hen\u00b7den", "Vo\u00b7gel\u00b7dunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.16": {"text": "Zu treffen, ist auch keine sondre Kunst.", "tokens": ["Zu", "tref\u00b7fen", ",", "ist", "auch", "kei\u00b7ne", "sond\u00b7re", "Kunst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Es ist nur eben ihr liebster Sport,", "tokens": ["Es", "ist", "nur", "e\u00b7ben", "ihr", "liebs\u00b7ter", "Sport", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Stets zu betreiben den V\u00f6gelmord,", "tokens": ["Stets", "zu", "be\u00b7trei\u00b7ben", "den", "V\u00f6\u00b7gel\u00b7mord", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und da \u00bbbekanntlich!\u00ab nicht wie wir", "tokens": ["Und", "da", "\u00bb", "be\u00b7kannt\u00b7lich", "!", "\u00ab", "nicht", "wie", "wir"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "$(", "ADV", "$.", "$(", "PTKNEG", "KOKOM", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Den Schmerz der Wunde sp\u00fcrt ein Tier,", "tokens": ["Den", "Schmerz", "der", "Wun\u00b7de", "sp\u00fcrt", "ein", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Auch Tiere keine Seele haben,", "tokens": ["Auch", "Tie\u00b7re", "kei\u00b7ne", "See\u00b7le", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "K\u00f6nnen getrost die b\u00f6sen Knaben", "tokens": ["K\u00f6n\u00b7nen", "ge\u00b7trost", "die", "b\u00f6\u00b7sen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Singv\u00f6gel in enge K\u00e4fige setzen", "tokens": ["Sing\u00b7v\u00f6\u00b7gel", "in", "en\u00b7ge", "K\u00e4\u00b7fi\u00b7ge", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Und mit Leimruten, Dohnen und Netzen", "tokens": ["Und", "mit", "Leim\u00b7ru\u00b7ten", ",", "Doh\u00b7nen", "und", "Net\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.25": {"text": "Und Pulver und Blei nach ihnen zielen. \u2013", "tokens": ["Und", "Pul\u00b7ver", "und", "Blei", "nach", "ih\u00b7nen", "zie\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "D\u00fcrft' ich nur einmal Gottvater spielen,", "tokens": ["D\u00fcrft'", "ich", "nur", "ein\u00b7mal", "Gott\u00b7va\u00b7ter", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Nur auf ein St\u00fcndlein, \u2013 ich f\u00fchre dazwischen,", "tokens": ["Nur", "auf", "ein", "St\u00fcnd\u00b7lein", ",", "\u2013", "ich", "f\u00fch\u00b7re", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "$(", "PPER", "VVFIN", "PAV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Und wo ich irgend t\u00e4t' erwischen", "tokens": ["Und", "wo", "ich", "ir\u00b7gend", "t\u00e4t'", "er\u00b7wi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Ich lie\u00df' im von einem handfesten Engel", "tokens": ["Ich", "lie\u00df'", "im", "von", "ei\u00b7nem", "hand\u00b7fes\u00b7ten", "En\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "F\u00fcnfzig mit feuriger Rute geben \u2013", "tokens": ["F\u00fcnf\u00b7zig", "mit", "feu\u00b7ri\u00b7ger", "Ru\u00b7te", "ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.31": {"text": "Die Jagdlust verging' ihm f\u00fcrs ganze Leben!", "tokens": ["Die", "Jagd\u00b7lust", "ver\u00b7ging'", "ihm", "f\u00fcrs", "gan\u00b7ze", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}