{"textgrid.poem.45389": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Achseltr\u00e4gerei", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Friedrich Wilhelm der Vierte,", "tokens": ["K\u00f6\u00b7nig", "Fried\u00b7rich", "Wil\u00b7helm", "der", "Vier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Denk an Friedrich Wilhelm den Dritten.", "tokens": ["Denk", "an", "Fried\u00b7rich", "Wil\u00b7helm", "den", "Drit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NE", "ART", "ADJA", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wo irgend ein Ulm und Austerlitz,", "tokens": ["Wo", "ir\u00b7gend", "ein", "Ulm", "und", "Aus\u00b7ter\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Liegt auch ein Jena inmitten.", "tokens": ["Liegt", "auch", "ein", "Je\u00b7na", "in\u00b7mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}