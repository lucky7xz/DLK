{"dta.poem.10472": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxIX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein schlechtes Wetter,", "tokens": ["Das", "ist", "ein", "schlech\u00b7tes", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es regnet und st\u00fcrmt und schnei't;", "tokens": ["Es", "reg\u00b7net", "und", "st\u00fcrmt", "und", "schnei't", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sitze am Fenster und schaue", "tokens": ["Ich", "sit\u00b7ze", "am", "Fens\u00b7ter", "und", "schau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Hinaus in die Dunkelheit.", "tokens": ["Hin\u00b7aus", "in", "die", "Dun\u00b7kel\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Da schimmert ein einsames Lichtchen,", "tokens": ["Da", "schim\u00b7mert", "ein", "ein\u00b7sa\u00b7mes", "Licht\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das wandelt langsam fort;", "tokens": ["Das", "wan\u00b7delt", "lang\u00b7sam", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein M\u00fctterchen mit dem Laternchen", "tokens": ["Ein", "M\u00fct\u00b7ter\u00b7chen", "mit", "dem", "La\u00b7tern\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wankt \u00fcber die Stra\u00dfe dort.", "tokens": ["Wankt", "\u00fc\u00b7ber", "die", "Stra\u00b7\u00dfe", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich glaube, Mehl und Eier", "tokens": ["Ich", "glau\u00b7be", ",", "Mehl", "und", "Ei\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Butter kaufte sie ein;", "tokens": ["Und", "But\u00b7ter", "kauf\u00b7te", "sie", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie will einen Kuchen backen", "tokens": ["Sie", "will", "ei\u00b7nen", "Ku\u00b7chen", "ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "F\u00fcr's gro\u00dfe T\u00f6chterlein.", "tokens": ["F\u00fcr's", "gro\u00b7\u00dfe", "T\u00f6ch\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die liegt zu Haus im Lehnstuhl,", "tokens": ["Die", "liegt", "zu", "Haus", "im", "Lehn\u00b7stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und blinzelt schl\u00e4frig in's Licht;", "tokens": ["Und", "blin\u00b7zelt", "schl\u00e4f\u00b7rig", "in's", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die goldnen Locken wallen", "tokens": ["Die", "gold\u00b7nen", "Lo\u00b7cken", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ueber das s\u00fc\u00dfe Gesicht.", "tokens": ["Ue\u00b7ber", "das", "s\u00fc\u00b7\u00dfe", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}