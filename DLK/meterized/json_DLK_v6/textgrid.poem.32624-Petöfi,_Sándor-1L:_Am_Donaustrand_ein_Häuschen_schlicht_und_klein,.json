{"textgrid.poem.32624": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Donaustrand ein H\u00e4uschen schlicht und klein,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Donaustrand ein H\u00e4uschen schlicht und klein,", "tokens": ["Am", "Do\u00b7naus\u00b7trand", "ein", "H\u00e4usc\u00b7hen", "schlicht", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Teuerstes auf Erden schlie\u00dft es ein;", "tokens": ["Mein", "Teu\u00b7ers\u00b7tes", "auf", "Er\u00b7den", "schlie\u00dft", "es", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der R\u00fchrung Tr\u00e4ne aus dem Auge quillt,", "tokens": ["Der", "R\u00fch\u00b7rung", "Tr\u00e4\u00b7ne", "aus", "dem", "Au\u00b7ge", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Seh' ich im Geiste dieses H\u00e4uschens Bild.", "tokens": ["Seh'", "ich", "im", "Geis\u00b7te", "die\u00b7ses", "H\u00e4usc\u00b7hens", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' ich es doch verlassen nimmermehr!", "tokens": ["H\u00e4tt'", "ich", "es", "doch", "ver\u00b7las\u00b7sen", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch wildes Sehnen jagt uns hin und her,", "tokens": ["Doch", "wil\u00b7des", "Seh\u00b7nen", "jagt", "uns", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich trieb es auch aus diesem Paradies,", "tokens": ["Mich", "trieb", "es", "auch", "aus", "die\u00b7sem", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So da\u00df ich's und mein M\u00fctterlein verlie\u00df!", "tokens": ["So", "da\u00df", "ich's", "und", "mein", "M\u00fct\u00b7ter\u00b7lein", "ver\u00b7lie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die gute Seele! Wie sie grausam litt", "tokens": ["Die", "gu\u00b7te", "See\u00b7le", "!", "Wie", "sie", "grau\u00b7sam", "litt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beim Abschied, da ich aus dem Hause schritt!", "tokens": ["Beim", "Ab\u00b7schied", ",", "da", "ich", "aus", "dem", "Hau\u00b7se", "schritt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Glut des Schmerzes griff nach ihr so rauh,", "tokens": ["Die", "Glut", "des", "Schmer\u00b7zes", "griff", "nach", "ihr", "so", "rauh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht zu l\u00f6schen durch der Tr\u00e4nen Tau!", "tokens": ["Und", "nicht", "zu", "l\u00f6\u00b7schen", "durch", "der", "Tr\u00e4\u00b7nen", "Tau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKZU", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr zitterndes Umarmen f\u00fchl' ich noch,", "tokens": ["Ihr", "zit\u00b7tern\u00b7des", "Um\u00b7ar\u00b7men", "f\u00fchl'", "ich", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch h\u00f6re ich ihr Flehen: \u00bbBleibe doch!\u00ab", "tokens": ["Noch", "h\u00f6\u00b7re", "ich", "ihr", "Fle\u00b7hen", ":", "\u00bb", "Blei\u00b7be", "doch", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "H\u00e4tt' ich nur damals schon die Welt gekannt,", "tokens": ["H\u00e4tt'", "ich", "nur", "da\u00b7mals", "schon", "die", "Welt", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer wei\u00df, ob diesem Flehn ich widerstand!", "tokens": ["Wer", "wei\u00df", ",", "ob", "die\u00b7sem", "Flehn", "ich", "wi\u00b7der\u00b7stand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch Hoffnung zauberte mir armem Tor", "tokens": ["Doch", "Hoff\u00b7nung", "zau\u00b7ber\u00b7te", "mir", "ar\u00b7mem", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Zukunft, einen Feengarten, vor, \u2013", "tokens": ["Die", "Zu\u00b7kunft", ",", "ei\u00b7nen", "Fe\u00b7en\u00b7gar\u00b7ten", ",", "vor", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erst in des Lebens wirrem Labyrinth", "tokens": ["Erst", "in", "des", "Le\u00b7bens", "wir\u00b7rem", "La\u00b7by\u00b7rinth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gewahren wir, wie schwer genarrt wir sind!", "tokens": ["Ge\u00b7wah\u00b7ren", "wir", ",", "wie", "schwer", "ge\u00b7narrt", "wir", "sind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ADJD", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auch mich verlockt' ein falscher Hoffnungsstrahl,", "tokens": ["Auch", "mich", "ver\u00b7lockt'", "ein", "fal\u00b7scher", "Hoff\u00b7nungs\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur Leiden fand ich ohne Wahl und Zahl,", "tokens": ["Nur", "Lei\u00b7den", "fand", "ich", "oh\u00b7ne", "Wahl", "und", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wo ich durch die Welt zog, fr\u00fch und sp\u00e4t,", "tokens": ["Und", "wo", "ich", "durch", "die", "Welt", "zog", ",", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit spitzen Dornen war mein Weg bes\u00e4t ...", "tokens": ["Mit", "spit\u00b7zen", "Dor\u00b7nen", "war", "mein", "Weg", "be\u00b7s\u00e4t", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ihr Freunde, kommt in meine Heimat Ihr,", "tokens": ["Ihr", "Freun\u00b7de", ",", "kommt", "in", "mei\u00b7ne", "Hei\u00b7mat", "Ihr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sucht meine Mutter auf und gr\u00fc\u00dft sie mir!", "tokens": ["Sucht", "mei\u00b7ne", "Mut\u00b7ter", "auf", "und", "gr\u00fc\u00dft", "sie", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Am Donaustrand ein H\u00e4uschen schlicht und klein,", "tokens": ["Am", "Do\u00b7naus\u00b7trand", "ein", "H\u00e4usc\u00b7hen", "schlicht", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Liegt's euch am Weg, werft einen Blick hinein!", "tokens": ["Liegt's", "euch", "am", "Weg", ",", "werft", "ei\u00b7nen", "Blick", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sagt ihr, da\u00df ohne Grund sie um mich weint,", "tokens": ["Sagt", "ihr", ",", "da\u00df", "oh\u00b7ne", "Grund", "sie", "um", "mich", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPR", "NN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sagt ihr, da\u00df mir des Gl\u00fcckes Sonne scheint \u2013", "tokens": ["Sagt", "ihr", ",", "da\u00df", "mir", "des", "Gl\u00fc\u00b7ckes", "Son\u00b7ne", "scheint", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es bricht ihr ja das Herz, wenn sie err\u00e4t,", "tokens": ["Es", "bricht", "ihr", "ja", "das", "Herz", ",", "wenn", "sie", "er\u00b7r\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie's ihrem Sohn, wie mir es elend geht!", "tokens": ["Wie's", "ih\u00b7rem", "Sohn", ",", "wie", "mir", "es", "e\u00b7lend", "geht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PWAV", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}