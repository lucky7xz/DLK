{"dta.poem.21570": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Aus den \u201eEls\u00e4ssischen Liedern\u201c.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wir haben Dich wieder erstritten", "tokens": ["Wir", "ha\u00b7ben", "Dich", "wie\u00b7der", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In wetternder V\u00f6lkerschlacht,", "tokens": ["In", "wet\u00b7tern\u00b7der", "V\u00f6l\u00b7ker\u00b7schlacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Geweint um Dich und gelitten", "tokens": ["Ge\u00b7weint", "um", "Dich", "und", "ge\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPER", "KON", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Opfer um Opfer gebracht.", "tokens": ["Und", "Op\u00b7fer", "um", "Op\u00b7fer", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Nun lassen wir Dich nimmer", "tokens": ["Nun", "las\u00b7sen", "wir", "Dich", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und halten Dich fest in der Hand,", "tokens": ["Und", "hal\u00b7ten", "Dich", "fest", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Bist wieder deutsch auf immer,", "tokens": ["Bist", "wie\u00b7der", "deutsch", "auf", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Flurherrliches Wasgenland!", "tokens": ["Flur\u00b7herr\u00b7li\u00b7ches", "Was\u00b7gen\u00b7land", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun rauscht Dir stolzer und freier", "tokens": ["Nun", "rauscht", "Dir", "stol\u00b7zer", "und", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den jauchzenden Rhein entlang", "tokens": ["Den", "jauch\u00b7zen\u00b7den", "Rhein", "ent\u00b7lang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPO"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die allemannische Leier,", "tokens": ["Die", "al\u00b7le\u00b7man\u00b7ni\u00b7sche", "Lei\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der allemannische Sang.", "tokens": ["Der", "al\u00b7le\u00b7man\u00b7ni\u00b7sche", "Sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Aus deinen Burgen und Bergen", "tokens": ["Aus", "dei\u00b7nen", "Bur\u00b7gen", "und", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da schweben durch Hallen und Thor", "tokens": ["Da", "schwe\u00b7ben", "durch", "Hal\u00b7len", "und", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit Nixen und Elfen und Zwergen", "tokens": ["Mit", "Ni\u00b7xen", "und", "El\u00b7fen", "und", "Zwer\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Viel alte Gestalten empor.", "tokens": ["Viel", "al\u00b7te", "Ge\u00b7stal\u00b7ten", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Sie haben gar lange geschwiegen", "tokens": ["Sie", "ha\u00b7ben", "gar", "lan\u00b7ge", "ge\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von mancher vielk\u00f6stlichen M\u00e4r,", "tokens": ["Von", "man\u00b7cher", "viel\u00b7k\u00f6st\u00b7li\u00b7chen", "M\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und mu\u00dften vergessen liegen", "tokens": ["Und", "mu\u00df\u00b7ten", "ver\u00b7ges\u00b7sen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "VVPP", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Schutt und Gem\u00e4uer umher.", "tokens": ["In", "Schutt", "und", "Ge\u00b7m\u00e4u\u00b7er", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Nun kommen sie l\u00e4chelnd und steigen", "tokens": ["Nun", "kom\u00b7men", "sie", "l\u00e4\u00b7chelnd", "und", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "An\u2019s leuchtende Sonnenlicht", "tokens": ["An's", "leuch\u00b7ten\u00b7de", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wollen Dir deuten und zeigen", "tokens": ["Und", "wol\u00b7len", "Dir", "deu\u00b7ten", "und", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "KON", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Was Deine Geschichte spricht:", "tokens": ["Was", "Dei\u00b7ne", "Ge\u00b7schich\u00b7te", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Da\u00df seit den undenklichsten Tagen", "tokens": ["Da\u00df", "seit", "den", "un\u00b7denk\u00b7lichs\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und trotz dem fr\u00e4nkischen Raub", "tokens": ["Und", "trotz", "dem", "fr\u00e4n\u00b7ki\u00b7schen", "Raub"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Eichen im Wasgau getragen", "tokens": ["Die", "Ei\u00b7chen", "im", "Was\u00b7gau", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das echte germanische Laub.", "tokens": ["Das", "ech\u00b7te", "ger\u00b7ma\u00b7ni\u00b7sche", "Laub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und da\u00df allzeiten inmitten", "tokens": ["Und", "da\u00df", "all\u00b7zei\u00b7ten", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Der welschen, werbenden List", "tokens": ["Der", "wel\u00b7schen", ",", "wer\u00b7ben\u00b7den", "List"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "VVINF", "$,", "VAFIN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dein Volk an ", "tokens": ["Dein", "Volk", "an"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Germanisch geblieben ist.             ", "tokens": ["Ger\u00b7ma\u00b7nisch", "ge\u00b7blie\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}