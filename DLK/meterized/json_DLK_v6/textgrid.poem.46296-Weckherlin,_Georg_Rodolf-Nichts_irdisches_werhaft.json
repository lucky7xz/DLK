{"textgrid.poem.46296": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Nichts irdisches werhaft", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geburt ist schlechter ruhm,", "tokens": ["Ge\u00b7burt", "ist", "schlech\u00b7ter", "ruhm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "ehr wie ein blitz vergehet,", "tokens": ["ehr", "wie", "ein", "blitz", "ver\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "sch\u00f6nheit ist eine blum,", "tokens": ["sch\u00f6n\u00b7heit", "ist", "ei\u00b7ne", "blum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "reichtum nicht lang bestehet,", "tokens": ["reich\u00b7tum", "nicht", "lang", "be\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "herrlichkeit ist ein glanz,", "tokens": ["herr\u00b7lich\u00b7keit", "ist", "ein", "glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "ruhm wie ein rauch verflieget,", "tokens": ["ruhm", "wie", "ein", "rauch", "ver\u00b7flie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "wollust ist nur ein danz,", "tokens": ["wol\u00b7lust", "ist", "nur", "ein", "danz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "die welt allzeit betrieget.", "tokens": ["die", "welt", "all\u00b7zeit", "be\u00b7trie\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ist dan geburt und ehr, sch\u00f6nheit und hab ein dunst,", "tokens": ["Ist", "dan", "ge\u00b7burt", "und", "ehr", ",", "sch\u00f6n\u00b7heit", "und", "hab", "ein", "dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "NN", "$,", "ADJD", "KON", "VAFIN", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ist herrlichkeit und ruhm, wollust und welt umsunst", "tokens": ["ist", "herr\u00b7lich\u00b7keit", "und", "ruhm", ",", "wol\u00b7lust", "und", "welt", "um\u00b7sunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "VMFIN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so la\u00df ich dich, herr, allein walten;", "tokens": ["so", "la\u00df", "ich", "dich", ",", "herr", ",", "al\u00b7lein", "wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "$,", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "und da\u00df ich mich m\u00f6g zu dir halten", "tokens": ["und", "da\u00df", "ich", "mich", "m\u00f6g", "zu", "dir", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADJD", "APPR", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "auf erden, herr, bleib du bei mir,", "tokens": ["auf", "er\u00b7den", ",", "herr", ",", "bleib", "du", "bei", "mir", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "im himmel la\u00df mich sein bei dir!", "tokens": ["im", "him\u00b7mel", "la\u00df", "mich", "sein", "bei", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIMP", "PPER", "PPOSAT", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}