{"textgrid.poem.43594": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fest gehalten den Z\u00fcgel,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fest gehalten den Z\u00fcgel,", "tokens": ["Fest", "ge\u00b7hal\u00b7ten", "den", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Beide F\u00fc\u00df' in dem B\u00fcgel,", "tokens": ["Bei\u00b7de", "F\u00fc\u00df'", "in", "dem", "B\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Kopf in die H\u00f6he, hervor die Brust!", "tokens": ["Kopf", "in", "die", "H\u00f6\u00b7he", ",", "her\u00b7vor", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "So zu reiten ist meine Lust.", "tokens": ["So", "zu", "rei\u00b7ten", "ist", "mei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wipp wapp! wipp wapp!", "tokens": ["Wipp", "wapp", "!", "wipp", "wapp", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "XY", "XY", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Geht's im Trab,", "tokens": ["Geht's", "im", "Trab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und alsdann", "tokens": ["Und", "als\u00b7dann"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "F\u00e4ngt mein Pferdchen munt'rer an,", "tokens": ["F\u00e4ngt", "mein", "Pferd\u00b7chen", "munt'\u00b7rer", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hopp hopp hopp hopp!", "tokens": ["Hopp", "hopp", "hopp", "hopp", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Im Galopp.", "tokens": ["Im", "Ga\u00b7lopp", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ich bin ein muthiger Reiter,", "tokens": ["Ich", "bin", "ein", "mut\u00b7hi\u00b7ger", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lustig immer weiter", "tokens": ["Lus\u00b7tig", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Geht es hopp hopp!", "tokens": ["Geht", "es", "hopp", "hopp", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Geht's im Galopp;", "tokens": ["Geht's", "im", "Ga\u00b7lopp", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Brauche nicht Peitsche, brauche nicht Sporn,", "tokens": ["Brau\u00b7che", "nicht", "Peit\u00b7sche", ",", "brau\u00b7che", "nicht", "Sporn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "$,", "VVFIN", "PTKNEG", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Blase lustig mein Postillonshorn.", "tokens": ["Bla\u00b7se", "lus\u00b7tig", "mein", "Pos\u00b7til\u00b7lons\u00b7horn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wenn mein Pferdchen nicht mehr will,", "tokens": ["Wenn", "mein", "Pferd\u00b7chen", "nicht", "mehr", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Steht es still:", "tokens": ["Steht", "es", "still", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wipp wapp!", "tokens": ["Wipp", "wapp", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "ITJ", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Sitz ab! Halt!", "tokens": ["Sitz", "ab", "!", "Halt", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}