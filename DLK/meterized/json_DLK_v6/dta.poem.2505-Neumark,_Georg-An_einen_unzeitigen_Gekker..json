{"dta.poem.2505": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "An einen unzeitigen Gekker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Schau erstlich ob du kanst dein Midasohr bedekken/", "tokens": ["Schau", "erst\u00b7lich", "ob", "du", "kanst", "dein", "Mi\u00b7da\u00b7sohr", "be\u00b7dek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das du so blikken lest. Wenn dieses nun geschehn/", "tokens": ["Das", "du", "so", "blik\u00b7ken", "lest", ".", "Wenn", "die\u00b7ses", "nun", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$.", "KOUS", "PDS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So komm nur wieder her/ dann wei\u00df man ", "tokens": ["So", "komm", "nur", "wie\u00b7der", "her", "/", "dann", "wei\u00df", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "$(", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "dern Raht.", "tokens": ["dern", "Raht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}