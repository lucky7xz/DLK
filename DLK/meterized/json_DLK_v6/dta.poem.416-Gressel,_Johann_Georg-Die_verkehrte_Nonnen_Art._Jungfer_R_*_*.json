{"dta.poem.416": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die verkehrte Nonnen Art.  \n Jungfer  R  * *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich freye gantz ungern. Wie soll ich das verstehen", "tokens": ["Ich", "frey\u00b7e", "gantz", "un\u00b7gern", ".", "Wie", "soll", "ich", "das", "ver\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PWAV", "VMFIN", "PPER", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du angenehmes Kind/ spricht dieses auch das Hertz?", "tokens": ["Du", "an\u00b7ge\u00b7neh\u00b7mes", "Kind", "/", "spricht", "die\u00b7ses", "auch", "das", "Hertz", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VVFIN", "PDS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein! man kan es dir schon am Gesichte sehen/", "tokens": ["Nein", "!", "man", "kan", "es", "dir", "schon", "am", "Ge\u00b7sich\u00b7te", "se\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIS", "VMFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Du hast kein Nonnen-Fleisch/ es ist dein blosser Schertz.", "tokens": ["Du", "hast", "kein", "Non\u00b7nen\u00b7Fleisch", "/", "es", "ist", "dein", "blos\u00b7ser", "Schertz", "."], "token_info": ["word", "word", "word", "word"