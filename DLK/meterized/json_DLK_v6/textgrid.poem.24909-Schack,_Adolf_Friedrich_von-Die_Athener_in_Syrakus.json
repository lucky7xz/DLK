{"textgrid.poem.24909": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Athener in Syrakus", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchmorgens auf seinem S\u00f6ller sa\u00df", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", "auf", "sei\u00b7nem", "S\u00f6l\u00b7ler", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Klearch mit dem Sohne Gorgias;", "tokens": ["Kle\u00b7arch", "mit", "dem", "Soh\u00b7ne", "Gor\u00b7gi\u00b7as", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Vor ihm, gedehnt an des H\u00fcgels Fu\u00df,", "tokens": ["Vor", "ihm", ",", "ge\u00b7dehnt", "an", "des", "H\u00fc\u00b7gels", "Fu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das unerme\u00dfliche Syrakus", "tokens": ["Das", "un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", "Sy\u00b7ra\u00b7kus"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Mit Tempeln und Hallen und Thermen,", "tokens": ["Mit", "Tem\u00b7peln", "und", "Hal\u00b7len", "und", "Ther\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und dr\u00fcber hinweg des Aetna Schnee", "tokens": ["Und", "dr\u00fc\u00b7ber", "hin\u00b7weg", "des", "A\u00b7et\u00b7na", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APZR", "ART", "NE", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und das hochgezinnte Epipol\u00e4", "tokens": ["Und", "das", "hoch\u00b7ge\u00b7zinn\u00b7te", "E\u00b7pi\u00b7po\u00b7l\u00e4"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und der H\u00e4fen tobendes L\u00e4rmen.", "tokens": ["Und", "der", "H\u00e4\u00b7fen", "to\u00b7ben\u00b7des", "L\u00e4r\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbdu wei\u00dft, Sohn, was ich dem Ares versprach,", "tokens": ["\u00bb", "du", "wei\u00dft", ",", "Sohn", ",", "was", "ich", "dem", "A\u00b7res", "ver\u00b7sprach", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "NN", "$,", "PWS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Als er die Macht der Athener zerbrach!", "tokens": ["Als", "er", "die", "Macht", "der", "A\u00b7the\u00b7ner", "zer\u00b7brach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Eh Boreas noch, der eisige, tobt,", "tokens": ["Eh", "Bo\u00b7re\u00b7as", "noch", ",", "der", "ei\u00b7si\u00b7ge", ",", "tobt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Mu\u00df ich, so wie ich im Kampfe gelobt,", "tokens": ["Mu\u00df", "ich", ",", "so", "wie", "ich", "im", "Kamp\u00b7fe", "ge\u00b7lobt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Im Tempel das Opfer ihm z\u00fcnden.", "tokens": ["Im", "Tem\u00b7pel", "das", "Op\u00b7fer", "ihm", "z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Geh, ruf mir den Meister des Bau's herbei!", "tokens": ["Geh", ",", "ruf", "mir", "den", "Meis\u00b7ter", "des", "Bau's", "her\u00b7bei", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Ob nun vollendet das Prachtthor sei", "tokens": ["Ob", "nun", "voll\u00b7en\u00b7det", "das", "Prachtt\u00b7hor", "sei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVPP", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und der Giebel, soll er mir k\u00fcnden.", "tokens": ["Und", "der", "Gie\u00b7bel", ",", "soll", "er", "mir", "k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "VMFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch sieh! dort naht er. \u2013 Du h\u00f6rtest, ich will", "tokens": ["Doch", "sieh", "!", "dort", "naht", "er", ".", "\u2013", "Du", "h\u00f6r\u00b7test", ",", "ich", "will"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADV", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "$,", "PPER", "VMFIN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor Winter den Tempel noch weihen, Thrasyll.", "tokens": ["Vor", "Win\u00b7ter", "den", "Tem\u00b7pel", "noch", "wei\u00b7hen", ",", "Thr\u00b7a\u00b7syll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADV", "VVINF", "$,", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Schon werden die Bl\u00e4tter herbstlich welk;", "tokens": ["Schon", "wer\u00b7den", "die", "Bl\u00e4t\u00b7ter", "herbst\u00b7lich", "welk", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sag an denn: Ruht bereits das Geb\u00e4lk", "tokens": ["Sag", "an", "denn", ":", "Ruht", "be\u00b7reits", "das", "Ge\u00b7b\u00e4lk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "KON", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Auf den marmornen Architraven?", "tokens": ["Auf", "den", "mar\u00b7mor\u00b7nen", "Ar\u00b7chit\u00b7ra\u00b7ven", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+---+-+", "measure": "anapaest.init"}, "line.6": {"text": "Wo nicht, so brauche die Gei\u00dfel zum Schlag", "tokens": ["Wo", "nicht", ",", "so", "brau\u00b7che", "die", "Gei\u00b7\u00dfel", "zum", "Schlag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und zwinge zur Arbeit Nacht wie Tag", "tokens": ["Und", "zwin\u00b7ge", "zur", "Ar\u00b7beit", "Nacht", "wie", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NN", "KOKOM", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die weichlichen attischen Sklaven!\u00ab", "tokens": ["Die", "weich\u00b7li\u00b7chen", "at\u00b7ti\u00b7schen", "Skla\u00b7ven", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Thrasyll darauf: \u00bbWenn, wie du verlangt,", "tokens": ["Thr\u00b7a\u00b7syll", "da\u00b7rauf", ":", "\u00bb", "Wenn", ",", "wie", "du", "ver\u00b7langt", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$.", "$(", "KOUS", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch in Vollendung der Bau nicht prangt,", "tokens": ["Noch", "in", "Vol\u00b7len\u00b7dung", "der", "Bau", "nicht", "prangt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bez\u00e4hme, Gebieter, die Ungeduld!", "tokens": ["Be\u00b7z\u00e4h\u00b7me", ",", "Ge\u00b7bie\u00b7ter", ",", "die", "Un\u00b7ge\u00b7duld", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein Chor des Euripides tr\u00e4gt die Schuld;", "tokens": ["Ein", "Chor", "des", "Eu\u00b7ri\u00b7pi\u00b7des", "tr\u00e4gt", "die", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sobald die Athener ihn singen,", "tokens": ["So\u00b7bald", "die", "A\u00b7the\u00b7ner", "ihn", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wird jeder der anderen Sklaven verlockt,", "tokens": ["Wird", "je\u00b7der", "der", "an\u00b7de\u00b7ren", "Skla\u00b7ven", "ver\u00b7lockt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Dem Klange zu lauschen; die Arbeit stockt,", "tokens": ["Dem", "Klan\u00b7ge", "zu", "lau\u00b7schen", ";", "die", "Ar\u00b7beit", "stockt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Nicht kann ich sie ferner erzwingen.\u00ab", "tokens": ["Nicht", "kann", "ich", "sie", "fer\u00b7ner", "er\u00b7zwin\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "++--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Klearch vernimmt's und erbla\u00dft vor Wut.", "tokens": ["Kle\u00b7arch", "ver\u00b7nimmt's", "und", "er\u00b7bla\u00dft", "vor", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "\u00bbmir, Vater, vertraue der Sklaven Hut,\u00ab", "tokens": ["\u00bb", "mir", ",", "Va\u00b7ter", ",", "ver\u00b7trau\u00b7e", "der", "Skla\u00b7ven", "Hut", ",", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ruft Gorgias da, \u00bbich sei ihr Vogt!", "tokens": ["Ruft", "Gor\u00b7gi\u00b7as", "da", ",", "\u00bb", "ich", "sei", "ihr", "Vogt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "$,", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eh winterlich st\u00fcrmend die See noch wogt,", "tokens": ["Eh", "win\u00b7ter\u00b7lich", "st\u00fcr\u00b7mend", "die", "See", "noch", "wogt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Den Tempel sie lass' ich vollenden!", "tokens": ["Den", "Tem\u00b7pel", "sie", "lass'", "ich", "voll\u00b7en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Fand doch durch dieser Athener Speer", "tokens": ["Fand", "doch", "durch", "die\u00b7ser", "A\u00b7the\u00b7ner", "Speer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Mein Bruder den Tod; das b\u00fc\u00dfen sie schwer,", "tokens": ["Mein", "Bru\u00b7der", "den", "Tod", ";", "das", "b\u00fc\u00b7\u00dfen", "sie", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$.", "PDS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wenn die Gei\u00dfel mir zuckt in den H\u00e4nden!\u00ab", "tokens": ["Wenn", "die", "Gei\u00b7\u00dfel", "mir", "zuckt", "in", "den", "H\u00e4n\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Den J\u00fcngling, der hoch von Zornglut flammt,", "tokens": ["Den", "J\u00fcng\u00b7ling", ",", "der", "hoch", "von", "Zorn\u00b7glut", "flammt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entsendet Klearch zu dem neuen Amt.", "tokens": ["Ent\u00b7sen\u00b7det", "Kle\u00b7arch", "zu", "dem", "neu\u00b7en", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und Tage verstreichen; im langen Zug", "tokens": ["Und", "Ta\u00b7ge", "ver\u00b7strei\u00b7chen", ";", "im", "lan\u00b7gen", "Zug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "$.", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Geht schon nach S\u00fcden der Kraniche Flug,", "tokens": ["Geht", "schon", "nach", "S\u00fc\u00b7den", "der", "Kra\u00b7ni\u00b7che", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Herbst hat die Haine gelichtet;", "tokens": ["Der", "Herbst", "hat", "die", "Hai\u00b7ne", "ge\u00b7lich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Da folgt der Vater dem Sohn, und bald", "tokens": ["Da", "folgt", "der", "Va\u00b7ter", "dem", "Sohn", ",", "und", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "KON", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ragt vor ihm der H\u00fcgel voll Pinienwald,", "tokens": ["Ragt", "vor", "ihm", "der", "H\u00fc\u00b7gel", "voll", "Pi\u00b7ni\u00b7en\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Auf dem er den Tempel errichtet.", "tokens": ["Auf", "dem", "er", "den", "Tem\u00b7pel", "er\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Fast glaubt er, da\u00df ihn das Auge tr\u00fcgt;", "tokens": ["Fast", "glaubt", "er", ",", "da\u00df", "ihn", "das", "Au\u00b7ge", "tr\u00fcgt", ";"], "token_info": ["word", "word", "word", "punct