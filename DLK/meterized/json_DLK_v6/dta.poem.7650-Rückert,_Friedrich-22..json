{"dta.poem.7650": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht auf die Schwalbe, die des Fr\u00fchlings Botschaft bringt,", "tokens": ["Nicht", "auf", "die", "Schwal\u00b7be", ",", "die", "des", "Fr\u00fch\u00b7lings", "Bot\u00b7schaft", "bringt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mir von ewiger Erneuung Lieder singt,", "tokens": ["Und", "mir", "von", "e\u00b7wi\u00b7ger", "Er\u00b7neu\u00b7ung", "Lie\u00b7der", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Freu' ich so sehr mich als auf einen Freundesgru\u00df,", "tokens": ["Freu'", "ich", "so", "sehr", "mich", "als", "auf", "ei\u00b7nen", "Freun\u00b7des\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPER", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der das mir bringt, was ich zum Leben haben mu\u00df:", "tokens": ["Der", "das", "mir", "bringt", ",", "was", "ich", "zum", "Le\u00b7ben", "ha\u00b7ben", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRELS", "PPER", "VVFIN", "$,", "PWS", "PPER", "APPRART", "NN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df Zeitenwechsel geht, fest die Gesinnung steht,", "tokens": ["Da\u00df", "Zei\u00b7ten\u00b7wech\u00b7sel", "geht", ",", "fest", "die", "Ge\u00b7sin\u00b7nung", "steht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist was mein Herz mit mehr als Fr\u00fchlingshauch durchweht.", "tokens": ["Ist", "was", "mein", "Herz", "mit", "mehr", "als", "Fr\u00fch\u00b7lings\u00b7hauch", "durch\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "APPR", "PIAT", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}