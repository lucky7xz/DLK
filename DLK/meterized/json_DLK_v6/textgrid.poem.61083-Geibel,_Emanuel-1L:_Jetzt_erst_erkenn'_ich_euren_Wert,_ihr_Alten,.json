{"textgrid.poem.61083": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jetzt erst erkenn' ich euren Wert, ihr Alten,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt erst erkenn' ich euren Wert, ihr Alten,", "tokens": ["Jetzt", "erst", "er\u00b7kenn'", "ich", "eu\u00b7ren", "Wert", ",", "ihr", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit ich auf eurem heil'gen Boden schreite;", "tokens": ["Seit", "ich", "auf", "eu\u00b7rem", "heil'\u00b7gen", "Bo\u00b7den", "schrei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lebendig wandelt ihr mir nun zur Seite,", "tokens": ["Le\u00b7ben\u00b7dig", "wan\u00b7delt", "ihr", "mir", "nun", "zur", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ein hoher Chor befreundeter Gestalten.", "tokens": ["Ein", "ho\u00b7her", "Chor", "be\u00b7freun\u00b7de\u00b7ter", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun lehret mich der G\u00f6tter ew'ges Walten", "tokens": ["Nun", "leh\u00b7ret", "mich", "der", "G\u00f6t\u00b7ter", "ew'\u00b7ges", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Greis von Chios in der Helden Streite,", "tokens": ["Der", "Greis", "von", "Chios", "in", "der", "Hel\u00b7den", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und m\u00e4chtig tr\u00e4gt mich Pindars Lied ins Weite,", "tokens": ["Und", "m\u00e4ch\u00b7tig", "tr\u00e4gt", "mich", "Pin\u00b7dars", "Lied", "ins", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem wie im Sturm die Fl\u00fcgel sich entfalten.", "tokens": ["Dem", "wie", "im", "Sturm", "die", "Fl\u00fc\u00b7gel", "sich", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "APPRART", "NN", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sanft spielt Horaz mit seinem leichten Spotte", "tokens": ["Sanft", "spielt", "Ho\u00b7raz", "mit", "sei\u00b7nem", "leich\u00b7ten", "Spot\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NE", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir um die Brust, indes den Blitz ergrimmt", "tokens": ["Mir", "um", "die", "Brust", ",", "in\u00b7des", "den", "Blitz", "er\u00b7grimmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich Juvenal erborgt vom Donnergotte.", "tokens": ["Sich", "Ju\u00b7ve\u00b7nal", "er\u00b7borgt", "vom", "Don\u00b7ner\u00b7got\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wehmutsvoll zu s\u00fc\u00dfer Klage stimmt", "tokens": ["Doch", "weh\u00b7muts\u00b7voll", "zu", "s\u00fc\u00b7\u00dfer", "Kla\u00b7ge", "stimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Tibull die Zither in umlaubter Grotte,", "tokens": ["Ti\u00b7bull", "die", "Zi\u00b7ther", "in", "um\u00b7laub\u00b7ter", "Grot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn fern im Blau der Stern des Abends glimmt.", "tokens": ["Wenn", "fern", "im", "Blau", "der", "Stern", "des", "A\u00b7bends", "glimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}