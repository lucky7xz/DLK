{"textgrid.poem.54648": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "62. Auff dieselbige", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist mein erste Nacht/ der Anfang meiner Tage/", "tokens": ["Di\u00df", "ist", "mein", "ers\u00b7te", "Nacht", "/", "der", "An\u00b7fang", "mei\u00b7ner", "Ta\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin' ich mit viel Angst der letzten Nacht nachjage.", "tokens": ["Dar\u00b7in'", "ich", "mit", "viel", "Angst", "der", "letz\u00b7ten", "Nacht", "nach\u00b7ja\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch weil ich meine Nacht Herr durch dein Licht verlohren/", "tokens": ["Doch", "weil", "ich", "mei\u00b7ne", "Nacht", "Herr", "durch", "dein", "Licht", "ver\u00b7loh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bin ich zwar in der Nacht/ doch nicht der Nacht gebohr\u1ebd.", "tokens": ["Bin", "ich", "zwar", "in", "der", "Nacht", "/", "doch", "nicht", "der", "Nacht", "ge\u00b7bohr\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$(", "ADV", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}