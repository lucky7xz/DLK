{"textgrid.poem.34873": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Lachen und Singen! Es blitzen und gaukeln", "tokens": ["Ein", "La\u00b7chen", "und", "Sin\u00b7gen", "!", "Es", "blit\u00b7zen", "und", "gau\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "KON", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Sonnenlichter. Die Wellen schaukeln", "tokens": ["Die", "Son\u00b7nen\u00b7lich\u00b7ter", ".", "Die", "Wel\u00b7len", "schau\u00b7keln"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den lustigen Kahn. Ich sa\u00df darin", "tokens": ["Den", "lus\u00b7ti\u00b7gen", "Kahn", ".", "Ich", "sa\u00df", "da\u00b7rin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PAV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit lieben Freunden und leichtem Sinn.", "tokens": ["Mit", "lie\u00b7ben", "Freun\u00b7den", "und", "leich\u00b7tem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Kahn zerbrach in eitel Tr\u00fcmmer,", "tokens": ["Der", "Kahn", "zer\u00b7brach", "in", "ei\u00b7tel", "Tr\u00fcm\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Freunde waren schlechte Schwimmer,", "tokens": ["Die", "Freun\u00b7de", "wa\u00b7ren", "schlech\u00b7te", "Schwim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie gingen unter, im Vaterland;", "tokens": ["Sie", "gin\u00b7gen", "un\u00b7ter", ",", "im", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "$,", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich warf der Sturm an den Seinestrand.", "tokens": ["Mich", "warf", "der", "Sturm", "an", "den", "Sei\u00b7nes\u00b7trand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab ein neues Schiff bestiegen,", "tokens": ["Ich", "hab", "ein", "neu\u00b7es", "Schiff", "be\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit neuen Genossen; es wogen und wiegen", "tokens": ["Mit", "neu\u00b7en", "Ge\u00b7nos\u00b7sen", ";", "es", "wo\u00b7gen", "und", "wie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die fremden Fluten mich hin und her \u2013", "tokens": ["Die", "frem\u00b7den", "Flu\u00b7ten", "mich", "hin", "und", "her", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "KON", "ADV", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie fern die Heimat! mein Herz wie schwer!", "tokens": ["Wie", "fern", "die", "Hei\u00b7mat", "!", "mein", "Herz", "wie", "schwer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$.", "PPOSAT", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und das ist wieder ein Singen und Lachen \u2013", "tokens": ["Und", "das", "ist", "wie\u00b7der", "ein", "Sin\u00b7gen", "und", "La\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es pfeift der Wind, die Planken krachen \u2013", "tokens": ["Es", "pfeift", "der", "Wind", ",", "die", "Plan\u00b7ken", "kra\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am Himmel erlischt der letzte Stern \u2013", "tokens": ["Am", "Him\u00b7mel", "er\u00b7lischt", "der", "letz\u00b7te", "Stern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie schwer mein Herz! die Heimat wie fern!", "tokens": ["Wie", "schwer", "mein", "Herz", "!", "die", "Hei\u00b7mat", "wie", "fern", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$.", "ART", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}