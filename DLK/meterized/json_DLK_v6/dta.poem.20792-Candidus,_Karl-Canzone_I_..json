{"dta.poem.20792": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   I .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Unendliches in Endlichem zu schauen", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", "in", "End\u00b7li\u00b7chem", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ersannen ihre G\u00f6tter die Hellenen,", "tokens": ["Er\u00b7san\u00b7nen", "ih\u00b7re", "G\u00f6t\u00b7ter", "die", "Hel\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil himmelher gottmenschliches Gebaren", "tokens": ["Weil", "him\u00b7mel\u00b7her", "gott\u00b7menschli\u00b7ches", "Ge\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Sinn entgegend\u00e4mmerte bei Jenen.", "tokens": ["Dem", "Sinn", "ent\u00b7ge\u00b7gen\u00b7d\u00e4m\u00b7mer\u00b7te", "bei", "Je\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es lag die Welt in heil'gem Morgengrauen.", "tokens": ["Es", "lag", "die", "Welt", "in", "heil'\u00b7gem", "Mor\u00b7gen\u00b7grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch m\u00e4chtig wollte Gott sich offenbaren,", "tokens": ["Doch", "m\u00e4ch\u00b7tig", "woll\u00b7te", "Gott", "sich", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und als erf\u00fcllet waren", "tokens": ["Und", "als", "er\u00b7f\u00fcl\u00b7let", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "VVPP", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Zeiten und sein Vollglanz nun hervortrat", "tokens": ["Die", "Zei\u00b7ten", "und", "sein", "Voll\u00b7glanz", "nun", "her\u00b7vor\u00b7trat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Im Mittler Jesus, sanken hin die G\u00f6tter", "tokens": ["Im", "Mitt\u00b7ler", "Je\u00b7sus", ",", "san\u00b7ken", "hin", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NE", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein traurig Spiel der Sp\u00f6tter,", "tokens": ["Ein", "trau\u00b7rig", "Spiel", "der", "Sp\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Hinsank das Fatum und zur\u00fcck der Chor trat.", "tokens": ["Hin\u00b7sank", "das", "Fa\u00b7tum", "und", "zu\u00b7r\u00fcck", "der", "Chor", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Des dunkeln Ahnens Zeichen und Umh\u00fcllung", "tokens": ["Des", "dun\u00b7keln", "Ah\u00b7nens", "Zei\u00b7chen", "und", "Um\u00b7h\u00fcl\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Schwand vor der Klarheit wirklicher Erf\u00fcllung.", "tokens": ["Schwand", "vor", "der", "Klar\u00b7heit", "wirk\u00b7li\u00b7cher", "Er\u00b7f\u00fcl\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.2": {"line.1": {"text": "Nicht konnten die olympischen Gestalten", "tokens": ["Nicht", "konn\u00b7ten", "die", "o\u00b7lym\u00b7pi\u00b7schen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Gottesmenschenthume sich vergleichen", "tokens": ["Dem", "Got\u00b7tes\u00b7men\u00b7schen\u00b7thu\u00b7me", "sich", "ver\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "De\u00df der da sprach: \u201ewer mich sieht, sieht den Vater.\u201c", "tokens": ["De\u00df", "der", "da", "sprach", ":", "\u201e", "wer", "mich", "sieht", ",", "sieht", "den", "Va\u00b7ter", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "ADV", "VVFIN", "$.", "$(", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Phidias entlockt des Mei\u00dfels Streichen", "tokens": ["Kein", "Phi\u00b7di\u00b7as", "ent\u00b7lockt", "des", "Mei\u00b7\u00dfels", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So warmes Leben und so hohes Walten.", "tokens": ["So", "war\u00b7mes", "Le\u00b7ben", "und", "so", "ho\u00b7hes", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In Fleisch und Blut, als wahrer Mensch, auftrat er", "tokens": ["In", "Fleisch", "und", "Blut", ",", "als", "wah\u00b7rer", "Mensch", ",", "auf\u00b7trat", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUS", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und angelweit aufthat er", "tokens": ["Und", "an\u00b7gel\u00b7weit", "auf\u00b7that", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Thore des Unendlichen f\u00fcr Jeden", "tokens": ["Die", "Tho\u00b7re", "des", "Un\u00b7end\u00b7li\u00b7chen", "f\u00fcr", "Je\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der seines Gottbewu\u00dftseins theilhaft werden", "tokens": ["Der", "sei\u00b7nes", "Gott\u00b7be\u00b7wu\u00dfts\u00b7eins", "theil\u00b7haft", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und Ewiges auf Erden", "tokens": ["Und", "E\u00b7wi\u00b7ges", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "APPR", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.11": {"text": "Darstellen mochte so in Thun wie Reden.", "tokens": ["Dar\u00b7stel\u00b7len", "moch\u00b7te", "so", "in", "Thun", "wie", "Re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Er heiligte f\u00fcr Alle sich da\u00df Alle", "tokens": ["Er", "hei\u00b7lig\u00b7te", "f\u00fcr", "Al\u00b7le", "sich", "da\u00df", "Al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "PRF", "KOUS", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zu G\u00f6ttern w\u00fcrden bei der G\u00f6tter Falle.", "tokens": ["Zu", "G\u00f6t\u00b7tern", "w\u00fcr\u00b7den", "bei", "der", "G\u00f6t\u00b7ter", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und war das Fatum attischer Trag\u00f6den", "tokens": ["Und", "war", "das", "Fa\u00b7tum", "at\u00b7ti\u00b7scher", "Tra\u00b7g\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ersch\u00fctternd wie bei sittlichen Gesetzen", "tokens": ["Er\u00b7sch\u00fct\u00b7ternd", "wie", "bei", "sitt\u00b7li\u00b7chen", "Ge\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhabne Unverbr\u00fcchlichkeit und S\u00fchne?", "tokens": ["Er\u00b7hab\u00b7ne", "Un\u00b7ver\u00b7br\u00fcch\u00b7lich\u00b7keit", "und", "S\u00fch\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ergreift euch nicht ein schauderndes Entsetzen", "tokens": ["Er\u00b7greift", "euch", "nicht", "ein", "schau\u00b7dern\u00b7des", "Ent\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn in den Schuldzusammenhang des bl\u00f6den", "tokens": ["Wenn", "in", "den", "Schuld\u00b7zu\u00b7sam\u00b7men\u00b7hang", "des", "bl\u00f6\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geschlechts eintretend untergeht der K\u00fchne", "tokens": ["Ge\u00b7schlechts", "ein\u00b7tre\u00b7tend", "un\u00b7ter\u00b7geht", "der", "K\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dort auf des Lebens B\u00fchne,", "tokens": ["Dort", "auf", "des", "Le\u00b7bens", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df seiner Unschuld theilhaft das Geschlecht sei?", "tokens": ["Da\u00df", "sei\u00b7ner", "Un\u00b7schuld", "theil\u00b7haft", "das", "Ge\u00b7schlecht", "sei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ist die Heroenwelt doch nur ein Ahnen", "tokens": ["Ist", "die", "He\u00b7ro\u00b7en\u00b7welt", "doch", "nur", "ein", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der wundervollen Bahnen", "tokens": ["Der", "wun\u00b7der\u00b7vol\u00b7len", "Bah\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Worauf der Geist uns f\u00fchrt zu dem was recht sei!", "tokens": ["Wo\u00b7rauf", "der", "Geist", "uns", "f\u00fchrt", "zu", "dem", "was", "recht", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "PWS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Vom Kreuz erst dunkelt schrecklich das Verh\u00e4ngni\u00df,", "tokens": ["Vom", "Kreuz", "erst", "dun\u00b7kelt", "schreck\u00b7lich", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Erst jene S\u00fchne tilgt der Welt Bedr\u00e4ngni\u00df.", "tokens": ["Erst", "je\u00b7ne", "S\u00fch\u00b7ne", "tilgt", "der", "Welt", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die ihr aus Schutt nun grabet G\u00f6tterbilder,", "tokens": ["Die", "ihr", "aus", "Schutt", "nun", "gra\u00b7bet", "G\u00f6t\u00b7ter\u00b7bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit euch will schwelgen ich im Werk der Musen,", "tokens": ["Mit", "euch", "will", "schwel\u00b7gen", "ich", "im", "Werk", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ob auch vor dem Donnerer durchf\u00e4hret", "tokens": ["Doch", "ob", "auch", "vor", "dem", "Don\u00b7ne\u00b7rer", "durch\u00b7f\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gotthaftes Ahnen der Beschauer Busen,", "tokens": ["Got\u00b7thaf\u00b7tes", "Ah\u00b7nen", "der", "Be\u00b7schau\u00b7er", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gotthafter d\u00e4ucht mir, trauter auch und milder", "tokens": ["Got\u00b7thaf\u00b7ter", "d\u00e4ucht", "mir", ",", "trau\u00b7ter", "auch", "und", "mil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "ADV", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein lebend Menschenantlitz das verkl\u00e4ret", "tokens": ["Ein", "le\u00b7bend", "Men\u00b7schen\u00b7ant\u00b7litz", "das", "ver\u00b7kl\u00e4\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "PDS", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Als Spiegel sich bew\u00e4hret", "tokens": ["Als", "Spie\u00b7gel", "sich", "be\u00b7w\u00e4h\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Herrlichkeit die sich vom Kreuz ergossen.", "tokens": ["Der", "Herr\u00b7lich\u00b7keit", "die", "sich", "vom", "Kreuz", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch ferner Zukunft bleibt, ach! vorbehalten", "tokens": ["Doch", "fer\u00b7ner", "Zu\u00b7kunft", "bleibt", ",", "ach", "!", "vor\u00b7be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "NN", "VVFIN", "$,", "ITJ", "$.", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das Leben zu gestalten,", "tokens": ["Das", "Le\u00b7ben", "zu", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Denn dies Geschlecht hat Fl\u00fcgel nicht noch Flossen.", "tokens": ["Denn", "dies", "Ge\u00b7schlecht", "hat", "Fl\u00fc\u00b7gel", "nicht", "noch", "Flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VAFIN", "NN", "PTKNEG", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Sie ", "tokens": ["Sie"], "token_info": ["word"], "pos": ["PPER"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Und noch die 