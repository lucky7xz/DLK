{"textgrid.poem.52792": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wetter steht grad \u00fcber der Erd,", "tokens": ["Ein", "Wet\u00b7ter", "steht", "grad", "\u00fc\u00b7ber", "der", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wenn's nur in's W\u00fcrttembergische f\u00e4hrt!", "tokens": ["Wenn's", "nur", "in's", "W\u00fcrt\u00b7tem\u00b7ber\u00b7gi\u00b7sche", "f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denn thut es bei uns sich entladen,", "tokens": ["Denn", "thut", "es", "bei", "uns", "sich", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So haben wir Hagelschaden.", "tokens": ["So", "ha\u00b7ben", "wir", "Ha\u00b7gel\u00b7scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Als Beispiel hat man es schon erlebt,", "tokens": ["Als", "Bei\u00b7spiel", "hat", "man", "es", "schon", "er\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PIS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df ein Gewitter vor\u00fcber schwebt,", "tokens": ["Da\u00df", "ein", "Ge\u00b7wit\u00b7ter", "vor\u00b7\u00fc\u00b7ber", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der g\u00fctige F\u00fcrst von Baden", "tokens": ["Der", "g\u00fc\u00b7ti\u00b7ge", "F\u00fcrst", "von", "Ba\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Thut sonst sich weh, in Gnaden.", "tokens": ["Thut", "sonst", "sich", "weh", ",", "in", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PRF", "PTKVZ", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch wenn auch Hagel sich herbewegt,", "tokens": ["Doch", "wenn", "auch", "Ha\u00b7gel", "sich", "her\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn's nur nicht in die Kirch' einschl\u00e4gt,", "tokens": ["Wenn's", "nur", "nicht", "in", "die", "Kirch'", "ein\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mitrei\u00dfend auf schrecklichen Pfaden", "tokens": ["Mit\u00b7rei\u00b7\u00dfend", "auf", "schreck\u00b7li\u00b7chen", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Des Pfarrers arme Waden.", "tokens": ["Des", "Pfar\u00b7rers", "ar\u00b7me", "Wa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie's neulich geschehen in Grimmelsbach,", "tokens": ["Wie's", "neu\u00b7lich", "ge\u00b7sche\u00b7hen", "in", "Grim\u00b7mels\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wo auch der Herr Amtmann Ungemach", "tokens": ["Wo", "auch", "der", "Herr", "Amt\u00b7mann", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NE", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erlitten auf Retiraden,", "tokens": ["Er\u00b7lit\u00b7ten", "auf", "Re\u00b7ti\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Indem er war eingeladen.", "tokens": ["In\u00b7dem", "er", "war", "ein\u00b7ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}