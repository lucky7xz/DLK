{"dta.poem.7646": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit Andacht hab' ich in den Regen aufgeblickt,", "tokens": ["Mit", "An\u00b7dacht", "hab'", "ich", "in", "den", "Re\u00b7gen", "auf\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der endlich, lang ersehnt, die durst'ge Welt erquickt.", "tokens": ["Der", "end\u00b7lich", ",", "lang", "er\u00b7sehnt", ",", "die", "dur\u00b7st'\u00b7ge", "Welt", "er\u00b7quickt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich habe wol f\u00fcr mich zu trinken stets gehabt,", "tokens": ["Ich", "ha\u00b7be", "wol", "f\u00fcr", "mich", "zu", "trin\u00b7ken", "stets", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "PTKZU", "VVFIN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hat nichts, weil die Welt gedurstet, mich gelabt.", "tokens": ["Doch", "hat", "nichts", ",", "weil", "die", "Welt", "ge\u00b7durs\u00b7tet", ",", "mich", "ge\u00b7labt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "$,", "KOUS", "ART", "NN", "VVPP", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun schweigend alle, die zuvor gedurstet, tranken,", "tokens": ["Nun", "schwei\u00b7gend", "al\u00b7le", ",", "die", "zu\u00b7vor", "ge\u00b7durs\u00b7tet", ",", "tran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "$,", "PRELS", "ADV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00dft' ich in meinem und in ihrem Namen danken.", "tokens": ["Mu\u00dft'", "ich", "in", "mei\u00b7nem", "und", "in", "ih\u00b7rem", "Na\u00b7men", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}