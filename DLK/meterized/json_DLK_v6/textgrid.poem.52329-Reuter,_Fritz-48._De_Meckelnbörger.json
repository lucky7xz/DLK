{"textgrid.poem.52329": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "48. De Meckelnb\u00f6rger", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dor was mal ens in Meckelnborg", "tokens": ["Dor", "was", "mal", "ens", "in", "Me\u00b7ckeln\u00b7borg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "ADV", "APPR", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "En oll Inspekter, sihr bekannt,", "tokens": ["En", "oll", "Ins\u00b7pek\u00b7ter", ",", "sihr", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Namen Krischan Korl Georg", "tokens": ["Mit", "Na\u00b7men", "Krisc\u00b7han", "Korl", "Ge\u00b7org"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un Zamel Ludwig Peiter Brandt;", "tokens": ["Un", "Za\u00b7mel", "Lud\u00b7wig", "Pei\u00b7ter", "Brandt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch alltausamen, de em kennten,", "tokens": ["Doch", "all\u00b7tau\u00b7sa\u00b7men", ",", "de", "em", "kenn\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "FM", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Em man den Schimmel-Brandten nennten,", "tokens": ["Em", "man", "den", "Schim\u00b7mel\u00b7Brand\u00b7ten", "nenn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nich wil hei just all schimmeln ded,", "tokens": ["Nich", "wil", "hei", "just", "all", "schim\u00b7meln", "ded", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Ne, wil hei einen Schimmel red.", "tokens": ["Ne", ",", "wil", "hei", "ei\u00b7nen", "Schim\u00b7mel", "red", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "NE", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Un de d\u00fcrft ok so swack nich sin,", "tokens": ["Un", "de", "d\u00fcrft", "ok", "so", "swack", "nich", "sin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Wenn hei s\u00fcll den Inspekter dr\u00e4gen,", "tokens": ["Wenn", "hei", "s\u00fcll", "den", "Ins\u00b7pek\u00b7ter", "dr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Denn unse Brandt, so as hei st\u00fcnn,", "tokens": ["Denn", "un\u00b7se", "Brandt", ",", "so", "as", "hei", "st\u00fcnn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "K\u00fcnn an dreihunnert Pund woll w\u00e4gen,", "tokens": ["K\u00fcnn", "an", "drei\u00b7hun\u00b7nert", "Pund", "woll", "w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und doran fehlt kein einzigst Pund.", "tokens": ["Und", "do\u00b7ran", "fehlt", "kein", "ein\u00b7zigst", "Pund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Un dorbi was hei kerngesund", "tokens": ["Un", "dor\u00b7bi", "was", "hei", "kern\u00b7ge\u00b7sund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Un hadd dorbi tau jeder Tid", "tokens": ["Un", "hadd", "dor\u00b7bi", "tau", "je\u00b7der", "Tid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "PIAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.16": {"text": "En ganz kaptalen Appetit.", "tokens": ["En", "ganz", "kap\u00b7ta\u00b7len", "Ap\u00b7pe\u00b7tit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So'n Schinken von en Pundner teihn,", "tokens": ["So'n", "Schin\u00b7ken", "von", "en", "Pund\u00b7ner", "teihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den putzt hei so taum Fr\u00fchst\u00fcck blo\u00df,", "tokens": ["Den", "putzt", "hei", "so", "taum", "Fr\u00fch\u00b7st\u00fcck", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "NN", "ADV", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.19": {"text": "Un dorbi was em gor nicks los,", "tokens": ["Un", "dor\u00b7bi", "was", "em", "gor", "nicks", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Un nicks nich was em antauseihn.", "tokens": ["Un", "nicks", "nich", "was", "em", "an\u00b7tau\u00b7seihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "PTKNEG", "PWS", "ADJA", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.21": {"text": "Un einmal s\u00e4d hei tau sin Fru: \u00bbDen Dunner Hagel!", "tokens": ["Un", "ein\u00b7mal", "s\u00e4d", "hei", "tau", "sin", "Fru", ":", "\u00bb", "Den", "Dun\u00b7ner", "Ha\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "FM", "FM", "FM", "FM", "NN", "$.", "$(", "ART", "ADJA", "NN", "$."], "meter": "+--+---+-+-+-", "measure": "dactylic.di.plus"}, "line.22": {"text": "So'n Gaus is doch en snakschen Vagel:", "tokens": ["So'n", "Gaus", "is", "doch", "en", "snak\u00b7schen", "Va\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "KON", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Von eine einzige allein dor ward", "tokens": ["Von", "ei\u00b7ne", "ein\u00b7zi\u00b7ge", "al\u00b7lein", "dor", "ward"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Taum Fr\u00fchst\u00fcck keiner richtig satt,", "tokens": ["Taum", "Fr\u00fch\u00b7st\u00fcck", "kei\u00b7ner", "rich\u00b7tig", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PIS", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Un ett man twei, de 'n beten grot,", "tokens": ["Un", "ett", "man", "twei", ",", "de", "'n", "be\u00b7ten", "grot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "NE", "NE", "VVFIN", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "Verdarwt man sick dat Middagbrod.\u00ab", "tokens": ["Ver\u00b7darwt", "man", "sick", "dat", "Mid\u00b7dag\u00b7brod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Dortau drunk hei en gaud Glas Win,", "tokens": ["Dor\u00b7tau", "drunk", "hei", "en", "gaud", "Glas", "Win", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Un denn recht velen m\u00fc\u00dft dat sin.", "tokens": ["Un", "denn", "recht", "ve\u00b7len", "m\u00fc\u00dft", "dat", "sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Eins f\u00fchrt hei nah den Wullmarkt tau Berlin.", "tokens": ["Eins", "f\u00fchrt", "hei", "nah", "den", "Wull\u00b7markt", "tau", "Ber\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ADJD", "ART", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bald was sin Wull denn ok verk\u00f6fft,", "tokens": ["Bald", "was", "sin", "Wull", "denn", "ok", "ver\u00b7k\u00f6fft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "NE", "KON", "NE", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Un hei hadd makt en gaud Gesch\u00e4ft,", "tokens": ["Un", "hei", "hadd", "makt", "en", "gaud", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch ihr hei wedder weg wull f\u00fchren,", "tokens": ["Doch", "ihr", "hei", "wed\u00b7der", "weg", "wull", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "KON", "ADV", "VMFIN", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wull hei de Stadt beseihn en beten", "tokens": ["Wull", "hei", "de", "Stadt", "be\u00b7seihn", "en", "be\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Un sick en beten verlustieren;", "tokens": ["Un", "sick", "en", "be\u00b7ten", "ver\u00b7lus\u00b7tie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "V\u00f6r allen wull hei \u00e4werst ", "tokens": ["V\u00f6r", "al\u00b7len", "wull", "hei", "\u00e4\u00b7werst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "En Bummler br\u00f6cht v\u00f6r gauden Lohn", "tokens": ["En", "Bumm\u00b7ler", "br\u00f6cht", "v\u00f6r", "gau\u00b7den", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Em denn ok nah 'ne Rest'ratschon,", "tokens": ["Em", "denn", "ok", "nah", "'ne", "Rest'\u00b7rat\u00b7schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo dat up't m\u00e4glich Finste was.", "tokens": ["Wo", "dat", "up't", "m\u00e4g\u00b7lich", "Fins\u00b7te", "was", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dat kamm em denn nu sch\u00f6n tau Pa\u00df,", "tokens": ["Dat", "kamm", "em", "denn", "nu", "sch\u00f6n", "tau", "Pa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Un as hei achter'n Disch hett seten,", "tokens": ["Un", "as", "hei", "ach\u00b7ter'n", "Disch", "hett", "se\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Dunn r\u00f6ppt hei: \u00bbH\u00fcr, min S\u00e4hn! Mark\u00fcr!", "tokens": ["Dunn", "r\u00f6ppt", "hei", ":", "\u00bb", "H\u00fcr", ",", "min", "S\u00e4hn", "!", "Mar\u00b7k\u00fcr", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "$(", "NN", "$,", "PPOSAT", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Oh, bring' mi mal en beten Eten!\u00ab", "tokens": ["Oh", ",", "bring'", "mi", "mal", "en", "be\u00b7ten", "E\u00b7ten", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "FM", "FM", "FM", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "\u00bbjawohl, mein Herr! Was w\u00fcnschen Sie?\u00ab", "tokens": ["\u00bb", "ja\u00b7wohl", ",", "mein", "Herr", "!", "Was", "w\u00fcn\u00b7schen", "Sie", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPOSAT", "NN", "$.", "PWS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Un giwwt den Oll'n 'ne Stripp Poppier,", "tokens": ["Un", "giwwt", "den", "Oll'n", "'ne", "Stripp", "Pop\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.17": {"text": "Worup dat all tau lesen stunn,", "tokens": ["Wo\u00b7rup", "dat", "all", "tau", "le\u00b7sen", "stunn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.18": {"text": "Wat jeder f\u00f6r sin Geld hir krigen kunn.", "tokens": ["Wat", "je\u00b7der", "f\u00f6r", "sin", "Geld", "hir", "kri\u00b7gen", "kunn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Na, Schimmel-Brandt, de lest un lest;", "tokens": ["Na", ",", "Schim\u00b7mel\u00b7Brandt", ",", "de", "lest", "un", "lest", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Hir's vel f\u00f6r Hunger un f\u00f6r D\u00f6st,", "tokens": ["Hir's", "vel", "f\u00f6r", "Hun\u00b7ger", "un", "f\u00f6r", "D\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Doch durt dat lang, ihr hei wat s\u00fcht,", "tokens": ["Doch", "durt", "dat", "lang", ",", "ihr", "hei", "wat", "s\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "$,", "PPOSAT", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Worup hei rechten Appetit", "tokens": ["Wo\u00b7rup", "hei", "rech\u00b7ten", "Ap\u00b7pe\u00b7tit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Un so'n rechten Giwwel hadd.", "tokens": ["Un", "so'n", "rech\u00b7ten", "Giw\u00b7wel", "hadd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Doch endlich seggt hei tau den Jungen:", "tokens": ["Doch", "end\u00b7lich", "seggt", "hei", "tau", "den", "Jun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+