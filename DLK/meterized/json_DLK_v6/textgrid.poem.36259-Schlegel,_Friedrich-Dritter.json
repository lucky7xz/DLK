{"textgrid.poem.36259": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Dritter", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df wei\u00dfe Rosen dir die Stirn umkr\u00e4nzen,", "tokens": ["La\u00df", "wei\u00b7\u00dfe", "Ro\u00b7sen", "dir", "die", "Stirn", "um\u00b7kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum sch\u00f6nen Zeichen, das die Freund' erfreue;", "tokens": ["Zum", "sch\u00f6\u00b7nen", "Zei\u00b7chen", ",", "das", "die", "Freund'", "er\u00b7freu\u00b7e", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie in dem milden Herzen reine Treue", "tokens": ["Wie", "in", "dem", "mil\u00b7den", "Her\u00b7zen", "rei\u00b7ne", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nie Farbe wechselt vor der T\u00e4uschung Gl\u00e4nzen.", "tokens": ["Nie", "Far\u00b7be", "wech\u00b7selt", "vor", "der", "T\u00e4u\u00b7schung", "Gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So schwebe heiter mit in unsern T\u00e4nzen,", "tokens": ["So", "schwe\u00b7be", "hei\u00b7ter", "mit", "in", "un\u00b7sern", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich an deiner, unsre Freud' erneue,", "tokens": ["Da\u00df", "sich", "an", "dei\u00b7ner", ",", "uns\u00b7re", "Freud'", "er\u00b7neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhalte du sie rein und fern von Reue,", "tokens": ["Er\u00b7hal\u00b7te", "du", "sie", "rein", "und", "fern", "von", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis Engel dich mit hellern Rosen kr\u00e4nzen.", "tokens": ["Bis", "En\u00b7gel", "dich", "mit", "hel\u00b7lern", "Ro\u00b7sen", "kr\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn wie der wei\u00dfe Schmuck der Seele Zeichen,", "tokens": ["Denn", "wie", "der", "wei\u00b7\u00dfe", "Schmuck", "der", "See\u00b7le", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die gern das Wort verh\u00fcllt in stillen Bildern,", "tokens": ["Die", "gern", "das", "Wort", "ver\u00b7h\u00fcllt", "in", "stil\u00b7len", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von treuer Lieb' und Unschuld nie zu weichen;", "tokens": ["Von", "treu\u00b7er", "Lieb'", "und", "Un\u00b7schuld", "nie", "zu", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So soll, da\u00df wir ungl\u00e4ubig nicht verwildern,", "tokens": ["So", "soll", ",", "da\u00df", "wir", "un\u00b7gl\u00e4u\u00b7big", "nicht", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUS", "PPER", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns deine Treue, was wir nie erreichen,", "tokens": ["Uns", "dei\u00b7ne", "Treu\u00b7e", ",", "was", "wir", "nie", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Urbild aller Treu' im Abglanz schildern.", "tokens": ["Das", "Ur\u00b7bild", "al\u00b7ler", "Treu'", "im", "Ab\u00b7glanz", "schil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}