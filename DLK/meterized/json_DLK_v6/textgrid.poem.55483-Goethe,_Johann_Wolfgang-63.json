{"textgrid.poem.55483": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "63", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf diesen Tr\u00fcmmern hab ich auch gesessen,", "tokens": ["Auf", "die\u00b7sen", "Tr\u00fcm\u00b7mern", "hab", "ich", "auch", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergn\u00fcgt getrunken und gegessen", "tokens": ["Ver\u00b7gn\u00fcgt", "ge\u00b7trun\u00b7ken", "und", "ge\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in die Welt hinausgeschaut:", "tokens": ["Und", "in", "die", "Welt", "hin\u00b7aus\u00b7ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War aber wenig nur davon erbaut.", "tokens": ["War", "a\u00b7ber", "we\u00b7nig", "nur", "da\u00b7von", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kein liebes Kind gedachte meiner,", "tokens": ["Kein", "lie\u00b7bes", "Kind", "ge\u00b7dach\u00b7te", "mei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich f\u00fcrwahr geh\u00f6rte keiner;", "tokens": ["Und", "ich", "f\u00fcr\u00b7wahr", "ge\u00b7h\u00f6r\u00b7te", "kei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So war die ganze Welt umgraut.", "tokens": ["So", "war", "die", "gan\u00b7ze", "Welt", "um\u00b7graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr wi\u00dft ja selbst, was sie erheitert,", "tokens": ["Ihr", "wi\u00dft", "ja", "selbst", ",", "was", "sie", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Horizonte stufenklar erweitert.", "tokens": ["Die", "Ho\u00b7ri\u00b7zon\u00b7te", "stu\u00b7fen\u00b7klar", "er\u00b7wei\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}