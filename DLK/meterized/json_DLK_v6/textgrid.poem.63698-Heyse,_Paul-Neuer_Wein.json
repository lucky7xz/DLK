{"textgrid.poem.63698": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Neuer Wein", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Rebenh\u00fcgel dicht gereiht", "tokens": ["O", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", "dicht", "ge\u00b7reiht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Voll lachenden Sonnenscheines!", "tokens": ["Voll", "la\u00b7chen\u00b7den", "Son\u00b7nen\u00b7schei\u00b7nes", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das ist die Zeit der Trunkenheit,", "tokens": ["Das", "ist", "die", "Zeit", "der", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Zeit des neuen Weines.", "tokens": ["Die", "Zeit", "des", "neu\u00b7en", "Wei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Mosthauch durch die L\u00fcfte zieht", "tokens": ["Ein", "Most\u00b7hauch", "durch", "die", "L\u00fcf\u00b7te", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Kellern und Spelunken;", "tokens": ["Aus", "Kel\u00b7lern", "und", "Spe\u00b7lun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von jeder Kelter schallt ein Lied,", "tokens": ["Von", "je\u00b7der", "Kel\u00b7ter", "schallt", "ein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein jedes Aug' spr\u00fcht Funken.", "tokens": ["Ein", "je\u00b7des", "Aug'", "spr\u00fcht", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Wagen schwanken hoch daher", "tokens": ["Die", "Wa\u00b7gen", "schwan\u00b7ken", "hoch", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit vollen Traubenkufen;", "tokens": ["Mit", "vol\u00b7len", "Trau\u00b7ben\u00b7ku\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Ochsenpaar ist auch nicht mehr", "tokens": ["Das", "Och\u00b7sen\u00b7paar", "ist", "auch", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ganz sicher auf den Hufen.", "tokens": ["Ganz", "si\u00b7cher", "auf", "den", "Hu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hast du den langen Storch gesehn?", "tokens": ["Hast", "du", "den", "lan\u00b7gen", "Storch", "ge\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er naschte vom jungen Weine.", "tokens": ["Er", "naschte", "vom", "jun\u00b7gen", "Wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun kann er nicht mehr grade stehn", "tokens": ["Nun", "kann", "er", "nicht", "mehr", "gra\u00b7de", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie sonst auf einem Beine.", "tokens": ["Wie", "sonst", "auf", "ei\u00b7nem", "Bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sogar das m\u00fcrrische Borstentier", "tokens": ["So\u00b7gar", "das", "m\u00fcr\u00b7ri\u00b7sche", "Bors\u00b7ten\u00b7tier"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Grunzt fr\u00f6hlich in seiner Klause;", "tokens": ["Grunzt", "fr\u00f6h\u00b7lich", "in", "sei\u00b7ner", "Klau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es d\u00fcnkt sich wie ein K\u00f6nig schier", "tokens": ["Es", "d\u00fcnkt", "sich", "wie", "ein", "K\u00f6\u00b7nig", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "ART", "NN", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim \u00fcppigen Trebernschmause.", "tokens": ["Beim", "\u00fcp\u00b7pi\u00b7gen", "Tre\u00b7bern\u00b7schmau\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Am tollsten l\u00e4rmt das Spatzengeschlecht,", "tokens": ["Am", "tolls\u00b7ten", "l\u00e4rmt", "das", "Spat\u00b7zen\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Jungen wie die \u00c4ltern.", "tokens": ["Die", "Jun\u00b7gen", "wie", "die", "\u00c4l\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie haben sich alle stark bezecht", "tokens": ["Sie", "ha\u00b7ben", "sich", "al\u00b7le", "stark", "be\u00b7zecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "PIS", "ADJD", "VVPP"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und taumeln um die Keltern.", "tokens": ["Und", "tau\u00b7meln", "um", "die", "Kel\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Weg, altes Herz, mit Sorg' und Harm!", "tokens": ["Weg", ",", "al\u00b7tes", "Herz", ",", "mit", "Sor\u00b7g'", "und", "Harm", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gib acht, nur \u00fcber ein kleines", "tokens": ["Gib", "acht", ",", "nur", "\u00fc\u00b7ber", "ein", "klei\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "CARD", "$,", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mitjauchzest du im trunknen Schwarm", "tokens": ["Mit\u00b7jauch\u00b7zest", "du", "im", "trunk\u00b7nen", "Schwarm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lob des neuen Weines!", "tokens": ["Das", "Lob", "des", "neu\u00b7en", "Wei\u00b7nes", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}