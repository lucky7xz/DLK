{"dta.poem.11035": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben  \n an  Mademoiselle  Weisen  \n in Merseburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe, voller Dankbegier,", "tokens": ["Ich", "ha\u00b7be", ",", "vol\u00b7ler", "Dank\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich, an der Wunder-vollen Zier", "tokens": ["Mich", ",", "an", "der", "Wun\u00b7der\u00b7vol\u00b7len", "Zier"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewachsner Blumen, oft ergetzet:", "tokens": ["Ge\u00b7wachs\u00b7ner", "Blu\u00b7men", ",", "oft", "er\u00b7get\u00b7zet", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch haben deine Blumen hier", "tokens": ["Doch", "ha\u00b7ben", "dei\u00b7ne", "Blu\u00b7men", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich gar fast aus mir selbst gesetzet,", "tokens": ["Mich", "gar", "fast", "aus", "mir", "selbst", "ge\u00b7set\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da sie so wohl, als jene, mir,", "tokens": ["Da", "sie", "so", "wohl", ",", "als", "je\u00b7ne", ",", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "KOUS", "PDS", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jm leiblichen, was geistigs weisen.", "tokens": ["Jm", "leib\u00b7li\u00b7chen", ",", "was", "geis\u00b7tigs", "wei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "$,", "PWS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie geben uns, wie sie, Gelegenheit,", "tokens": ["Sie", "ge\u00b7ben", "uns", ",", "wie", "sie", ",", "Ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "$,", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Durch geist-und leibliche Vortrefflichkeit,", "tokens": ["Durch", "geist\u00b7\u00b7und", "leib\u00b7li\u00b7che", "Vor\u00b7treff\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Geist-und K\u00f6rper Quell zu preisen.", "tokens": ["Der", "Geist\u00b7\u00b7und", "K\u00f6r\u00b7per", "Quell", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}