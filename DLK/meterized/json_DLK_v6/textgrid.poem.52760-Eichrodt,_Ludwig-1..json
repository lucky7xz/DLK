{"textgrid.poem.52760": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte gerne, wo an allem Ort", "tokens": ["Ich", "m\u00f6ch\u00b7te", "ger\u00b7ne", ",", "wo", "an", "al\u00b7lem", "Ort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "$,", "PWAV", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein hohes Fauenbild sich qu\u00e4lend sehnet", "tokens": ["Ein", "ho\u00b7hes", "Fau\u00b7en\u00b7bild", "sich", "qu\u00e4\u00b7lend", "seh\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach Liebesthat zu s\u00fc\u00dfem Liebeswort,", "tokens": ["Nach", "Lie\u00b7best\u00b7hat", "zu", "s\u00fc\u00b7\u00dfem", "Lie\u00b7bes\u00b7wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr nahen, da sie Gluth sich nahe w\u00e4hnet", "tokens": ["Ihr", "na\u00b7hen", ",", "da", "sie", "Gluth", "sich", "na\u00b7he", "w\u00e4h\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "$,", "KOUS", "PPER", "NN", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und harret einsam an dem Steine dort,", "tokens": ["Und", "har\u00b7ret", "ein\u00b7sam", "an", "dem", "Stei\u00b7ne", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dran sie gedankenvoll und pr\u00e4chtig lehnet:", "tokens": ["Dran", "sie", "ge\u00b7dan\u00b7ken\u00b7voll", "und", "pr\u00e4ch\u00b7tig", "leh\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich m\u00f6chte solcher Edeln jede finden", "tokens": ["Ich", "m\u00f6ch\u00b7te", "sol\u00b7cher", "E\u00b7deln", "je\u00b7de", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "PIAT", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu trauter Stunde, wenn die Strahlen schwinden.", "tokens": ["Zu", "trau\u00b7ter", "Stun\u00b7de", ",", "wenn", "die", "Strah\u00b7len", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die rohen Narren, M\u00e4nner sonst genannt,", "tokens": ["Die", "ro\u00b7hen", "Nar\u00b7ren", ",", "M\u00e4n\u00b7ner", "sonst", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gehn ihr vor\u00fcber, witzlos, ohne F\u00fchlen,", "tokens": ["Gehn", "ihr", "vor\u00b7\u00fc\u00b7ber", ",", "witz\u00b7los", ",", "oh\u00b7ne", "F\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mich hat ein unsagbarer Blick gebannt,", "tokens": ["Mich", "hat", "ein", "un\u00b7sag\u00b7ba\u00b7rer", "Blick", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am S\u00e4ulenschacht mu\u00df ich die Stirne k\u00fchlen,", "tokens": ["Am", "S\u00e4u\u00b7len\u00b7schacht", "mu\u00df", "ich", "die", "Stir\u00b7ne", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf reinen Formen weil' ich unverwandt,", "tokens": ["Auf", "rei\u00b7nen", "For\u00b7men", "weil'", "ich", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Aug' und Sinn in Reiz mich einzuw\u00fchlen.", "tokens": ["Mit", "Aug'", "und", "Sinn", "in", "Reiz", "mich", "ein\u00b7zu\u00b7w\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Schon ist der Finger neckend Spiel begonnen", "tokens": ["Schon", "ist", "der", "Fin\u00b7ger", "ne\u00b7ckend", "Spiel", "be\u00b7gon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}}}}