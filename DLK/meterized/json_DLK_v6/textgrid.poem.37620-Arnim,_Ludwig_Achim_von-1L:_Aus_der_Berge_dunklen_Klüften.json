{"textgrid.poem.37620": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus der Berge dunklen Kl\u00fcften", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus der Berge dunklen Kl\u00fcften", "tokens": ["Aus", "der", "Ber\u00b7ge", "dunk\u00b7len", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Braust nicht mehr die kalte Fluth,", "tokens": ["Braust", "nicht", "mehr", "die", "kal\u00b7te", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fenster \u00f6ffne ich den L\u00fcften", "tokens": ["Fens\u00b7ter", "\u00f6ff\u00b7ne", "ich", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Thor dem Jugendmuth;", "tokens": ["Und", "das", "Thor", "dem", "Ju\u00b7gend\u00b7muth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Springend geht's zum Thale nieder,", "tokens": ["Sprin\u00b7gend", "geht's", "zum", "Tha\u00b7le", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leicht befl\u00fcgelt ist das Herz,", "tokens": ["Leicht", "be\u00b7fl\u00fc\u00b7gelt", "ist", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00fchling breitet das Gefieder,", "tokens": ["Fr\u00fch\u00b7ling", "brei\u00b7tet", "das", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Luft erklingt wie edles Erz.", "tokens": ["Luft", "er\u00b7klingt", "wie", "ed\u00b7les", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Neue V\u00f6gel sind erschienen,", "tokens": ["Neu\u00b7e", "V\u00f6\u00b7gel", "sind", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fort in's Freie, in die Luft,", "tokens": ["Fort", "in's", "Frei\u00b7e", ",", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neues Schauspiel, gr\u00fcne B\u00fchnen,", "tokens": ["Neu\u00b7es", "Schau\u00b7spiel", ",", "gr\u00fc\u00b7ne", "B\u00fch\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nachtigall so sehnlich ruft:", "tokens": ["Nach\u00b7ti\u00b7gall", "so", "sehn\u00b7lich", "ruft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht das Schauspielhaus geschm\u00fccket", "tokens": ["Seht", "das", "Schau\u00b7spiel\u00b7haus", "ge\u00b7schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Dach aus Himmelblau,", "tokens": ["Mit", "dem", "Dach", "aus", "Him\u00b7mel\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wolken-Sch\u00e4flein sehn entz\u00fccket", "tokens": ["Wol\u00b7ken\u00b7Sch\u00e4f\u00b7lein", "sehn", "ent\u00b7z\u00fc\u00b7cket"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach dem hocherhabnen Bau.", "tokens": ["Nach", "dem", "ho\u00b7cher\u00b7hab\u00b7nen", "Bau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alle schweben im Verlangen", "tokens": ["Al\u00b7le", "schwe\u00b7ben", "im", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach des Tages Neuigkeit:", "tokens": ["Nach", "des", "Ta\u00b7ges", "Neu\u00b7ig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist der Vorhang aufgegangen?", "tokens": ["Ist", "der", "Vor\u00b7hang", "auf\u00b7ge\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches Schauspiel giebt man heut?", "tokens": ["Wel\u00b7ches", "Schau\u00b7spiel", "giebt", "man", "heut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ein Heldenspiel beginnen,", "tokens": ["Soll", "ein", "Hel\u00b7den\u00b7spiel", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "R\u00fcstet sich die frische Kraft?", "tokens": ["R\u00fcs\u00b7tet", "sich", "die", "fri\u00b7sche", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Soll die Lieb' in Lieb' zerinnen,", "tokens": ["Soll", "die", "Lieb'", "in", "Lieb'", "ze\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df sich neues Volk erschafft?", "tokens": ["Da\u00df", "sich", "neu\u00b7es", "Volk", "er\u00b7schafft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles dr\u00e4ngt sich noch zusammen,", "tokens": ["Al\u00b7les", "dr\u00e4ngt", "sich", "noch", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz an Herz und Baum an Baum,", "tokens": ["Herz", "an", "Herz", "und", "Baum", "an", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All aus einer Erde stammen,", "tokens": ["All", "aus", "ei\u00b7ner", "Er\u00b7de", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammend einer Liebe Traum:", "tokens": ["Flam\u00b7mend", "ei\u00b7ner", "Lie\u00b7be", "Traum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmlisch Spiel, die frischen Kr\u00e4nze", "tokens": ["Himm\u00b7lisch", "Spiel", ",", "die", "fri\u00b7schen", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Decken all mit gleichem Gr\u00fcn,", "tokens": ["De\u00b7cken", "all", "mit", "glei\u00b7chem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jenen, da\u00df er siegend gl\u00e4nze,", "tokens": ["Je\u00b7nen", ",", "da\u00df", "er", "sie\u00b7gend", "gl\u00e4n\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Diese, da\u00df sie drunter bl\u00fchn.", "tokens": ["Die\u00b7se", ",", "da\u00df", "sie", "drun\u00b7ter", "bl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "PAV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}