{"dta.poem.19323": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bedenke, wenn der Stolz des Denkens dich beth\u00f6rt,", "tokens": ["Be\u00b7den\u00b7ke", ",", "wenn", "der", "Stolz", "des", "Den\u00b7kens", "dich", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Welch eine Kleinigkeit dein Denken, Denker, st\u00f6rt.", "tokens": ["Welch", "ei\u00b7ne", "Klei\u00b7nig\u00b7keit", "dein", "Den\u00b7ken", ",", "Den\u00b7ker", ",", "st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPOSAT", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Bi\u00dfchen Weh im Kopf, ein Bi\u00dfchen Weh im Magen,", "tokens": ["Ein", "Bi\u00df\u00b7chen", "Weh", "im", "Kopf", ",", "ein", "Bi\u00df\u00b7chen", "Weh", "im", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Fu\u00df, der doch nichts scheint zum Denken beizutragen.", "tokens": ["Im", "Fu\u00df", ",", "der", "doch", "nichts", "scheint", "zum", "Den\u00b7ken", "bei\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht irren kann dich nur der Feldschlacht heisres Klirren,", "tokens": ["Nicht", "ir\u00b7ren", "kann", "dich", "nur", "der", "Feld\u00b7schlacht", "heis\u00b7res", "Klir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwirren kann dich schon der M\u00fccke leisres Schwirren.", "tokens": ["Ver\u00b7wir\u00b7ren", "kann", "dich", "schon", "der", "M\u00fc\u00b7cke", "leis\u00b7res", "Schwir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und h\u00e4ttest du wie Gott nun eine Welt gedacht,", "tokens": ["Und", "h\u00e4t\u00b7test", "du", "wie", "Gott", "nun", "ei\u00b7ne", "Welt", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "KOKOM", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00e4tte sie, o Spott, ein M\u00fccklein umgebracht.", "tokens": ["So", "h\u00e4t\u00b7te", "sie", ",", "o", "Spott", ",", "ein", "M\u00fcck\u00b7lein", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum ist es gut, da\u00df du nur denkest schon Gedachtes,", "tokens": ["Drum", "ist", "es", "gut", ",", "da\u00df", "du", "nur", "den\u00b7kest", "schon", "Ge\u00b7dach\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und im Gedanken nur nachmachst von Gott Gemachtes.", "tokens": ["Und", "im", "Ge\u00b7dan\u00b7ken", "nur", "nach\u00b7machst", "von", "Gott", "Ge\u00b7mach\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADV", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}