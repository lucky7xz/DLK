{"textgrid.poem.38691": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Paradiesisch umgestaltet", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Paradiesisch umgestaltet", "tokens": ["Pa\u00b7ra\u00b7die\u00b7sisch", "um\u00b7ge\u00b7stal\u00b7tet"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Prangt die Flur in goldnem Glanz;", "tokens": ["Prangt", "die", "Flur", "in", "gold\u00b7nem", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freude jubelt; Liebe waltet;", "tokens": ["Freu\u00b7de", "ju\u00b7belt", ";", "Lie\u00b7be", "wal\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf, beginnt den Maientanz!", "tokens": ["Auf", ",", "be\u00b7ginnt", "den", "Mai\u00b7en\u00b7tanz", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Forsts geheimer Dichte", "tokens": ["In", "des", "Forsts", "ge\u00b7hei\u00b7mer", "Dich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Girrt und fl\u00f6tet Minnelaut;", "tokens": ["Girrt", "und", "fl\u00f6\u00b7tet", "Min\u00b7ne\u00b7laut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unterm Gr\u00fcn im Abendlichte,", "tokens": ["Un\u00b7term", "Gr\u00fcn", "im", "A\u00b7bend\u00b7lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kosen Br\u00e4utigam und Braut.", "tokens": ["Ko\u00b7sen", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "B\u00e4ll' und Opern freun den St\u00e4dter,", "tokens": ["B\u00e4ll'", "und", "O\u00b7pern", "freun", "den", "St\u00e4d\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Assembleen die St\u00e4dterin:", "tokens": ["As\u00b7sem\u00b7bleen", "die", "St\u00e4d\u00b7te\u00b7rin", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns entz\u00fcckt der Fr\u00fchlings\u00e4ther,", "tokens": ["Uns", "ent\u00b7z\u00fcckt", "der", "Fr\u00fch\u00b7lings\u00b7\u00e4t\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns der Haine Baldachin!", "tokens": ["Uns", "der", "Hai\u00b7ne", "Bal\u00b7da\u00b7ch\u00b7in", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Kr\u00f6nt der frohen Weisheit Becher;", "tokens": ["Kr\u00f6nt", "der", "fro\u00b7hen", "Weis\u00b7heit", "Be\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Horcht der Wipfel Silberschall;", "tokens": ["Horcht", "der", "Wip\u00b7fel", "Sil\u00b7ber\u00b7schall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Webt verschwiegne Bl\u00e4tterd\u00e4cher;", "tokens": ["Webt", "ver\u00b7schwieg\u00b7ne", "Bl\u00e4t\u00b7ter\u00b7d\u00e4\u00b7cher", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruht auf Moos' am Wasserfall!", "tokens": ["Ruht", "auf", "Moos'", "am", "Was\u00b7ser\u00b7fall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit des Sinngr\u00fcns blauen Klocken", "tokens": ["Mit", "des", "Sinn\u00b7gr\u00fcns", "blau\u00b7en", "Klo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fcckt der holden Jungfraun Haar;", "tokens": ["Schm\u00fcckt", "der", "hol\u00b7den", "Jung\u00b7fraun", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tanzt, beweht von Bl\u00fctenflocken,", "tokens": ["Tanzt", ",", "be\u00b7weht", "von", "Bl\u00fc\u00b7ten\u00b7flo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wallt im Zwielicht Paar und Paar.", "tokens": ["Wallt", "im", "Zwie\u00b7licht", "Paar", "und", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heute Ku\u00df auf Ku\u00df der Trauten,", "tokens": ["Heu\u00b7te", "Ku\u00df", "auf", "Ku\u00df", "der", "Trau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "J\u00fcngling! die sich dir ergab:", "tokens": ["J\u00fcng\u00b7ling", "!", "die", "sich", "dir", "er\u00b7gab", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PRF", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Viel, ach! viel der Z\u00e4hren thauten", "tokens": ["Viel", ",", "ach", "!", "viel", "der", "Z\u00e4h\u00b7ren", "thau\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ITJ", "$.", "ADV", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Schon auf junger Br\u00e4ute Grab.", "tokens": ["Schon", "auf", "jun\u00b7ger", "Br\u00e4u\u00b7te", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}