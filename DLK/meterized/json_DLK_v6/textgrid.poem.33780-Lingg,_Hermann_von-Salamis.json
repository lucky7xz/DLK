{"textgrid.poem.33780": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Salamis", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schm\u00fccket die Schiffe mit Persertroph\u00e4'n,", "tokens": ["Schm\u00fc\u00b7cket", "die", "Schif\u00b7fe", "mit", "Per\u00b7ser\u00b7tro\u00b7ph\u00e4'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Lasset die purpurnen Segel sich bl\u00e4hn!", "tokens": ["Las\u00b7set", "die", "pur\u00b7pur\u00b7nen", "Se\u00b7gel", "sich", "bl\u00e4hn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Efeu umflattert die Masten und fliegt,", "tokens": ["E\u00b7feu", "um\u00b7flat\u00b7tert", "die", "Mas\u00b7ten", "und", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Evoe, der m\u00e4chtige Feind ist besiegt!", "tokens": ["E\u00b7voe", ",", "der", "m\u00e4ch\u00b7ti\u00b7ge", "Feind", "ist", "be\u00b7siegt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir zerbrachen, o Meer, wir zerbrachen das Band,", "tokens": ["Wir", "zer\u00b7bra\u00b7chen", ",", "o", "Meer", ",", "wir", "zer\u00b7bra\u00b7chen", "das", "Band", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Das der persische F\u00fcrst um den Nacken dir wand.", "tokens": ["Das", "der", "per\u00b7si\u00b7sche", "F\u00fcrst", "um", "den", "Na\u00b7cken", "dir", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Du entrollst nun befreit, dich erbittert nicht mehr", "tokens": ["Du", "ent\u00b7rollst", "nun", "be\u00b7freit", ",", "dich", "er\u00b7bit\u00b7tert", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,", "PPER", "VVFIN", "PTKNEG", "ADV"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Das verha\u00dfte Gestampf von den Rossen, die schwer", "tokens": ["Das", "ver\u00b7ha\u00df\u00b7te", "Ge\u00b7stampf", "von", "den", "Ros\u00b7sen", ",", "die", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Dein wogender Bug,", "tokens": ["Dein", "wo\u00b7gen\u00b7der", "Bug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dein br\u00fcckengefesselter Zorn ertrug.", "tokens": ["Dein", "br\u00fc\u00b7cken\u00b7ge\u00b7fes\u00b7sel\u00b7ter", "Zorn", "er\u00b7trug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Verh\u00e4ngnis kam \u00fcber Xerxes und stieg", "tokens": ["Das", "Ver\u00b7h\u00e4ng\u00b7nis", "kam", "\u00fc\u00b7ber", "Xe\u00b7rxes", "und", "stieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "KON", "VVFIN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Aus den Wellen empor zum hellenischen Sieg.", "tokens": ["Aus", "den", "Wel\u00b7len", "em\u00b7por", "zum", "hel\u00b7le\u00b7ni\u00b7schen", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dem Tyrannen, dem Herrn, der in Willk\u00fcr thront,", "tokens": ["Dem", "Ty\u00b7ran\u00b7nen", ",", "dem", "Herrn", ",", "der", "in", "Will\u00b7k\u00fcr", "thront", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Nicht erlag ihm das Volk, das am Meerstrand wohnt;", "tokens": ["Nicht", "er\u00b7lag", "ihm", "das", "Volk", ",", "das", "am", "Meer\u00b7strand", "wohnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Denn es st\u00e4hlte der Alte, der Herrscher der Flut,", "tokens": ["Denn", "es", "st\u00e4hl\u00b7te", "der", "Al\u00b7te", ",", "der", "Herr\u00b7scher", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Mit unendlichem Mut", "tokens": ["Mit", "un\u00b7end\u00b7li\u00b7chem", "Mut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Sein geliebtes Geschlecht f\u00fcr die Seeschlacht.", "tokens": ["Sein", "ge\u00b7lieb\u00b7tes", "Ge\u00b7schlecht", "f\u00fcr", "die", "See\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Rings jetzt, wo entz\u00fcckter die Woge vernimmt", "tokens": ["Rings", "jetzt", ",", "wo", "ent\u00b7z\u00fcck\u00b7ter", "die", "Wo\u00b7ge", "ver\u00b7nimmt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein ionisches Lied, da erbraust sie und stimmt", "tokens": ["Ein", "i\u00b7o\u00b7nisc\u00b7hes", "Lied", ",", "da", "er\u00b7braust", "sie", "und", "stimmt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In den P\u00e4an mit ein, es erbl\u00fchn, es erbl\u00fchn", "tokens": ["In", "den", "P\u00e4\u00b7an", "mit", "ein", ",", "es", "er\u00b7bl\u00fchn", ",", "es", "er\u00b7bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "$,", "PPER", "VVINF", "$,", "PPER", "VVINF"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Nach den herrlichen M\u00fch'n", "tokens": ["Nach", "den", "herr\u00b7li\u00b7chen", "M\u00fch'n"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dithyrambische Tage der Freiheit.", "tokens": ["Dit\u00b7hy\u00b7ram\u00b7bi\u00b7sche", "Ta\u00b7ge", "der", "Frei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}}}}