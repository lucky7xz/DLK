{"dta.poem.15615": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube nicht da\u00df ich im Mittelpunkte stehe,", "tokens": ["Ich", "glau\u00b7be", "nicht", "da\u00df", "ich", "im", "Mit\u00b7tel\u00b7punk\u00b7te", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Unendlichkeit um mich sich dienstbar drehe;", "tokens": ["Und", "die", "Un\u00b7end\u00b7lich\u00b7keit", "um", "mich", "sich", "dienst\u00b7bar", "dre\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch glaub' ich, da\u00df ich darf mir ordnen zum Vergn\u00fcgen", "tokens": ["Doch", "glaub'", "ich", ",", "da\u00df", "ich", "darf", "mir", "ord\u00b7nen", "zum", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "PPER", "VVINF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bilder der Fantasie aus ew'gen Sternenz\u00fcgen;", "tokens": ["Bil\u00b7der", "der", "Fan\u00b7ta\u00b7sie", "aus", "ew'\u00b7gen", "Ster\u00b7nen\u00b7z\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Bald als Verliebter sehn ein Blatt mit goldnen Schriften,", "tokens": ["Bald", "als", "Ver\u00b7lieb\u00b7ter", "sehn", "ein", "Blatt", "mit", "gold\u00b7nen", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bald als Kind ein Dach, besetzt mit goldnen Stiften.", "tokens": ["Und", "bald", "als", "Kind", "ein", "Dach", ",", "be\u00b7setzt", "mit", "gold\u00b7nen", "Stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOUS", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Allein vom Halse soll die Wissenschaft mir bleiben,", "tokens": ["Al\u00b7lein", "vom", "Hal\u00b7se", "soll", "die", "Wis\u00b7sen\u00b7schaft", "mir", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, was ich treib' im Spiel, als trocknen Ernst will treiben,", "tokens": ["Die", ",", "was", "ich", "treib'", "im", "Spiel", ",", "als", "trock\u00b7nen", "Ernst", "will", "trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die kindisch wird, wenn ihr aus Selbstsucht es bedunkt,", "tokens": ["Die", "kin\u00b7disch", "wird", ",", "wenn", "ihr", "aus", "Selbst\u00b7sucht", "es", "be\u00b7dunkt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im All ihr P\u00fcnktchen sei vom All der Mittelpunkt.", "tokens": ["Im", "All", "ihr", "P\u00fcnkt\u00b7chen", "sei", "vom", "All", "der", "Mit\u00b7tel\u00b7punkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}