{"textgrid.poem.64311": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "31. An die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe, heilige Natur,", "tokens": ["S\u00fc\u00b7\u00dfe", ",", "hei\u00b7li\u00b7ge", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df mich gehn auf deiner Spur!", "tokens": ["La\u00df", "mich", "gehn", "auf", "dei\u00b7ner", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Leite mich an deiner Hand,", "tokens": ["Lei\u00b7te", "mich", "an", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Kind am G\u00e4ngelband!", "tokens": ["Wie", "ein", "Kind", "am", "G\u00e4n\u00b7gel\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich dann erm\u00fcdet bin,", "tokens": ["Wenn", "ich", "dann", "er\u00b7m\u00fc\u00b7det", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fcck ich dir am Busen hin,", "tokens": ["R\u00fcck", "ich", "dir", "am", "Bu\u00b7sen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Atme s\u00fc\u00dfe Himmelslust,", "tokens": ["At\u00b7me", "s\u00fc\u00b7\u00dfe", "Him\u00b7mels\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hangend an der Mutter Brust.", "tokens": ["Han\u00b7gend", "an", "der", "Mut\u00b7ter", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, mir ist so wohl bei dir!", "tokens": ["Ach", ",", "mir", "ist", "so", "wohl", "bei", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will dich lieben f\u00fcr und f\u00fcr.", "tokens": ["Will", "dich", "lie\u00b7ben", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich gehn auf deiner Spur,", "tokens": ["La\u00df", "mich", "gehn", "auf", "dei\u00b7ner", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfe, heilige Natur!", "tokens": ["S\u00fc\u00b7\u00dfe", ",", "hei\u00b7li\u00b7ge", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}