{"dta.poem.13043": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eallein, was halt\u2019 ich mich bey diesen Kriegern auf,", "tokens": ["\u201e", "al\u00b7lein", ",", "was", "halt'", "ich", "mich", "bey", "die\u00b7sen", "Krie\u00b7gern", "auf", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PWS", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201efort, lassen wir das Volck in seinem Waffen-Lauf!", "tokens": ["\u201e", "fort", ",", "las\u00b7sen", "wir", "das", "Volck", "in", "sei\u00b7nem", "Waf\u00b7fen\u00b7Lauf", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ebetrachten wir das Haupt, die F\u00fchrer dieser Schaaren,", "tokens": ["\u201e", "be\u00b7trach\u00b7ten", "wir", "das", "Haupt", ",", "die", "F\u00fch\u00b7rer", "die\u00b7ser", "Schaa\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie stets den Tapfersten vor dem Gesichte waren;", "tokens": ["\u201e", "die", "stets", "den", "Tap\u00b7fers\u00b7ten", "vor", "dem", "Ge\u00b7sich\u00b7te", "wa\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "345\u201dScheints nicht, als w\u00e4ren sie Beschirmer, Schuz und Schild,", "tokens": ["\"", "Scheints", "nicht", ",", "als", "w\u00e4\u00b7ren", "sie", "Be\u00b7schir\u00b7mer", ",", "Schuz", "und", "Schild", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PTKNEG", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edie Vormaur ihres Heers, das von dem Ebenbild", "tokens": ["\u201e", "die", "Vor\u00b7maur", "ih\u00b7res", "Heers", ",", "das", "von", "dem", "E\u00b7ben\u00b7bild"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ederselben angefrischt, ermuntert und beseelet", "tokens": ["\u201e", "der\u00b7sel\u00b7ben", "an\u00b7ge\u00b7frischt", ",", "er\u00b7mun\u00b7tert", "und", "be\u00b7see\u00b7let"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PDAT", "VVPP", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201edie wahre Tapferkeit in seiner Brust verh\u00f6hlet?", "tokens": ["\u201e", "die", "wah\u00b7re", "Tap\u00b7fer\u00b7keit", "in", "sei\u00b7ner", "Brust", "ver\u00b7h\u00f6h\u00b7let", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eda sieht man was der Werth des wahren Adels ist,", "tokens": ["\u201e", "da", "sieht", "man", "was", "der", "Werth", "des", "wah\u00b7ren", "A\u00b7dels", "ist", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "PWS", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "350\u201dWeil er alldort sich nicht als nach den Thaten mi\u00dft;", "tokens": ["\"", "Weil", "er", "all\u00b7dort", "sich", "nicht", "als", "nach", "den", "Tha\u00b7ten", "mi\u00dft", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "PRF", "PTKNEG", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201emit Unzaghaftigkeit den Arm zum K\u00e4mpfen reget,", "tokens": ["\u201e", "mit", "Un\u00b7zag\u00b7haf\u00b7tig\u00b7keit", "den", "Arm", "zum", "K\u00e4mp\u00b7fen", "re\u00b7get", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201emit Tugend, nicht mit Stolz den Heerzogs-Stab beweget.", "tokens": ["\u201e", "mit", "Tu\u00b7gend", ",", "nicht", "mit", "Stolz", "den", "Heer\u00b7zogs\u00b7Stab", "be\u00b7we\u00b7get", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewie manches junges Haupt war vor zu schwach gesch\u00e4zt,", "tokens": ["\u201e", "wie", "man\u00b7ches", "jun\u00b7ges", "Haupt", "war", "vor", "zu", "schwach", "ge\u00b7sch\u00e4zt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "ADJA", "NN", "VAFIN", "APPR", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201edem doch der Sieg den Kranz des Lorbers aufgesezt?", "tokens": ["\u201e", "dem", "doch", "der", "Sieg", "den", "Kranz", "des", "Lor\u00b7bers", "auf\u00b7ge\u00b7sezt", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "355\u201dWie mancher hatte sich den theuren Ruhm erworben,", "tokens": ["\"", "Wie", "man\u00b7cher", "hat\u00b7te", "sich", "den", "theu\u00b7ren", "Ruhm", "er\u00b7wor\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "VAFIN", "PRF", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eda\u00df er dem Vaterland zu Lieb im Sieg gestorben?", "tokens": ["\u201e", "da\u00df", "er", "dem", "Va\u00b7ter\u00b7land", "zu", "Lieb", "im", "Sieg", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}