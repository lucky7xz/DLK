{"textgrid.poem.61700": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wenn der Tag die Nacht gek\u00fc\u00dft,", "tokens": ["Und", "wenn", "der", "Tag", "die", "Nacht", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da stirbt sie hin in s\u00fc\u00dfem Tod;", "tokens": ["Da", "stirbt", "sie", "hin", "in", "s\u00fc\u00b7\u00dfem", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr seliges Verbluten,", "tokens": ["Ihr", "se\u00b7li\u00b7ges", "Ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist das Morgenrot.", "tokens": ["Das", "ist", "das", "Mor\u00b7gen\u00b7rot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich liebe dich wie die Nacht den Tag,", "tokens": ["Ich", "lie\u00b7be", "dich", "wie", "die", "Nacht", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich kann dich nie erwerben \u2013", "tokens": ["Ich", "kann", "dich", "nie", "er\u00b7wer\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O d\u00fcrft' ich denn an deinem Ku\u00df", "tokens": ["O", "d\u00fcrft'", "ich", "denn", "an", "dei\u00b7nem", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verblutend sterben!", "tokens": ["Ver\u00b7blu\u00b7tend", "ster\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}