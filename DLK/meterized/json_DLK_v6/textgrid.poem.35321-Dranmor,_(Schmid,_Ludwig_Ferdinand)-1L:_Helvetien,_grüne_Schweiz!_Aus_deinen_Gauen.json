{"textgrid.poem.35321": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Helvetien, gr\u00fcne Schweiz! Aus deinen Gauen", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Helvetien, gr\u00fcne Schweiz! Aus deinen Gauen", "tokens": ["Hel\u00b7ve\u00b7ti\u00b7en", ",", "gr\u00fc\u00b7ne", "Schweiz", "!", "Aus", "dei\u00b7nen", "Gau\u00b7en"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist trotzig einst ein Knabe fortgegangen,", "tokens": ["Ist", "trot\u00b7zig", "einst", "ein", "Kna\u00b7be", "fort\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als tausend W\u00fcnsche ihre L\u00f6wenklauen", "tokens": ["Als", "tau\u00b7send", "W\u00fcn\u00b7sche", "ih\u00b7re", "L\u00f6\u00b7wenk\u00b7lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "CARD", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um seines Herzens weiche Rinde schlangen.", "tokens": ["Um", "sei\u00b7nes", "Her\u00b7zens", "wei\u00b7che", "Rin\u00b7de", "schlan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da waren viele, ihm den Weg zu zeigen;", "tokens": ["Da", "wa\u00b7ren", "vie\u00b7le", ",", "ihm", "den", "Weg", "zu", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freundschaft kam mit mancher Lebensregel;", "tokens": ["Die", "Freund\u00b7schaft", "kam", "mit", "man\u00b7cher", "Le\u00b7bens\u00b7re\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihm aber hing der Himmel voller Geigen,", "tokens": ["Ihm", "a\u00b7ber", "hing", "der", "Him\u00b7mel", "vol\u00b7ler", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stolzen Mutes ging er unter Segel.", "tokens": ["Und", "stol\u00b7zen", "Mu\u00b7tes", "ging", "er", "un\u00b7ter", "Se\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist nun lange her. \u2013 Ich war der Knabe,", "tokens": ["Das", "ist", "nun", "lan\u00b7ge", "her", ".", "\u2013", "Ich", "war", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "PTKVZ", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und niemand hat in meiner Brust gelesen,", "tokens": ["Und", "nie\u00b7mand", "hat", "in", "mei\u00b7ner", "Brust", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich geseufzt, geweint am Wanderstabe:", "tokens": ["Was", "ich", "ge\u00b7seufzt", ",", "ge\u00b7weint", "am", "Wan\u00b7der\u00b7sta\u00b7be", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bballein! Allein! Und so will ich genesen?", "tokens": ["\u00bb", "al\u00b7lein", "!", "Al\u00b7lein", "!", "Und", "so", "will", "ich", "ge\u00b7ne\u00b7sen", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "ADV", "$.", "KON", "ADV", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Allein! Allein! Und das der Wildnis Segen?\u00ab", "tokens": ["Al\u00b7lein", "!", "Al\u00b7lein", "!", "Und", "das", "der", "Wild\u00b7nis", "Se\u00b7gen", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "KON", "PDS", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O k\u00f6nnte, wer mit meinem Blute schriebe,", "tokens": ["O", "k\u00f6nn\u00b7te", ",", "wer", "mit", "mei\u00b7nem", "Blu\u00b7te", "schrie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "$,", "PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Worte jenes Dichters widerlegen:", "tokens": ["Die", "Wor\u00b7te", "je\u00b7nes", "Dich\u00b7ters", "wi\u00b7der\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbdem Ha\u00df entfloh ich, aber auch der Liebe!\u00ab", "tokens": ["\u00bb", "dem", "Ha\u00df", "ent\u00b7floh", "ich", ",", "a\u00b7ber", "auch", "der", "Lie\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,", "ADV", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du, Heimat, warst es nicht, die mich verfluchte,", "tokens": ["Du", ",", "Hei\u00b7mat", ",", "warst", "es", "nicht", ",", "die", "mich", "ver\u00b7fluch\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Helvetia, Riesin! Du verziehst dem Zwerge,", "tokens": ["Hel\u00b7ve\u00b7tia", ",", "Rie\u00b7sin", "!", "Du", "ver\u00b7ziehst", "dem", "Zwer\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als er die goldnen Triften wieder suchte,", "tokens": ["Als", "er", "die", "gold\u00b7nen", "Trif\u00b7ten", "wie\u00b7der", "such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die stillen Th\u00e4ler sah, die freien Berge,", "tokens": ["Die", "stil\u00b7len", "Th\u00e4\u00b7ler", "sah", ",", "die", "frei\u00b7en", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die himmelhohen, ew'gen Gletscherw\u00e4nde.", "tokens": ["Die", "him\u00b7mel\u00b7ho\u00b7hen", ",", "ew'\u00b7gen", "Glet\u00b7scher\u00b7w\u00e4n\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du warst es nicht, die seine Freude st\u00f6rte,", "tokens": ["Du", "warst", "es", "nicht", ",", "die", "sei\u00b7ne", "Freu\u00b7de", "st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als er von deiner Seeen Fruchtgel\u00e4nde", "tokens": ["Als", "er", "von", "dei\u00b7ner", "Se\u00b7e\u00b7en", "Frucht\u00b7ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Den Donner ferner Katarakte h\u00f6rte.", "tokens": ["Den", "Don\u00b7ner", "fer\u00b7ner", "Ka\u00b7ta\u00b7rak\u00b7te", "h\u00f6r\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ja, V\u00f6gel flogen aus den offnen Bauern,", "tokens": ["Ja", ",", "V\u00f6\u00b7gel", "flo\u00b7gen", "aus", "den", "off\u00b7nen", "Bau\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die drau\u00dfen ihre Freiheit grausam b\u00fc\u00dften \u2013", "tokens": ["Die", "drau\u00b7\u00dfen", "ih\u00b7re", "Frei\u00b7heit", "grau\u00b7sam", "b\u00fc\u00df\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So dacht' ich gramvoll, als die morschen Mauern", "tokens": ["So", "dacht'", "ich", "gram\u00b7voll", ",", "als", "die", "mor\u00b7schen", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des \u00f6den Vaterhauses mich begr\u00fc\u00dften.", "tokens": ["Des", "\u00f6\u00b7den", "Va\u00b7ter\u00b7hau\u00b7ses", "mich", "be\u00b7gr\u00fc\u00df\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Zu fr\u00fch die Regung und wie Schaum zerflossen \u2013", "tokens": ["Zu", "fr\u00fch", "die", "Re\u00b7gung", "und", "wie", "Schaum", "zer\u00b7flos\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "KON", "PWAV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf einmal schien mir alles wieder fremde;", "tokens": ["Auf", "ein\u00b7mal", "schien", "mir", "al\u00b7les", "wie\u00b7der", "frem\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PIS", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch nicht genug gelitten und genossen,", "tokens": ["Noch", "nicht", "ge\u00b7nug", "ge\u00b7lit\u00b7ten", "und", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt schon die Heimkehr \u2013 und ein Totenhemde?", "tokens": ["Jetzt", "schon", "die", "Heim\u00b7kehr", "\u2013", "und", "ein", "To\u00b7ten\u00b7hem\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O da\u00df ich diese Worte nie gesprochen,", "tokens": ["O", "da\u00df", "ich", "die\u00b7se", "Wor\u00b7te", "nie", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PDAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und da\u00df ich nie den Blick gewendet h\u00e4tte!", "tokens": ["Und", "da\u00df", "ich", "nie", "den", "Blick", "ge\u00b7wen\u00b7det", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn jetzt ersehnt sich, einsam und gebrochen,", "tokens": ["Denn", "jetzt", "er\u00b7sehnt", "sich", ",", "ein\u00b7sam", "und", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Pilger nichts als eine Ruhest\u00e4tte.", "tokens": ["Der", "Pil\u00b7ger", "nichts", "als", "ei\u00b7ne", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Nach Frieden ringt sein Herz, das todeswunde;", "tokens": ["Nach", "Frie\u00b7den", "ringt", "sein", "Herz", ",", "das", "to\u00b7des\u00b7wun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bild nur taucht empor aus wirren Tr\u00e4umen:", "tokens": ["Ein", "Bild", "nur", "taucht", "em\u00b7por", "aus", "wir\u00b7ren", "Tr\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Strohdach, dort, in einem k\u00fchlen Grunde", "tokens": ["Ein", "Stroh\u00b7dach", ",", "dort", ",", "in", "ei\u00b7nem", "k\u00fch\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und rings umz\u00e4unt von fruchtbeladnen B\u00e4umen.", "tokens": ["Und", "rings", "um\u00b7z\u00e4unt", "von", "frucht\u00b7be\u00b7lad\u00b7nen", "B\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So reicht die Bruderhand dem Reisem\u00fcden,", "tokens": ["So", "reicht", "die", "Bru\u00b7der\u00b7hand", "dem", "Rei\u00b7se\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er sich l\u00f6se von dem Zauberbanne;", "tokens": ["Da\u00df", "er", "sich", "l\u00f6\u00b7se", "von", "dem", "Zau\u00b7ber\u00b7ban\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er gibt ihn hin, den sonnetrunknen S\u00fcden,", "tokens": ["Er", "gibt", "ihn", "hin", ",", "den", "son\u00b7ne\u00b7trunk\u00b7nen", "S\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr eine einz'ge schneebehangne Tanne.", "tokens": ["F\u00fcr", "ei\u00b7ne", "einz'\u00b7ge", "schnee\u00b7be\u00b7hang\u00b7ne", "Tan\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "So ruft ihn wieder nach dem armen Neste,", "tokens": ["So", "ruft", "ihn", "wie\u00b7der", "nach", "dem", "ar\u00b7men", "Nes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh' neues Herzeleid den Weg ihm abgeschnitten \u2013", "tokens": ["Eh'", "neu\u00b7es", "Her\u00b7ze\u00b7leid", "den", "Weg", "ihm", "ab\u00b7ge\u00b7schnit\u00b7ten", "\u20