{"textgrid.poem.62060": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[da\u00df die Sterne bl\u00e4sser werden]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df die Sterne bl\u00e4sser werden,", "tokens": ["Da\u00df", "die", "Ster\u00b7ne", "bl\u00e4s\u00b7ser", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Herz vor Leiden gl\u00fcht,", "tokens": ["Wenn", "das", "Herz", "vor", "Lei\u00b7den", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tte nimmer ich gedacht!", "tokens": ["H\u00e4t\u00b7te", "nim\u00b7mer", "ich", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn das Herz vor Freuden lacht,", "tokens": ["Wenn", "das", "Herz", "vor", "Freu\u00b7den", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes Sternlein Feuer spr\u00fcht,", "tokens": ["Je\u00b7des", "Stern\u00b7lein", "Feu\u00b7er", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Sterne dunkler werden. \u2013", "tokens": ["Und", "die", "Ster\u00b7ne", "dunk\u00b7ler", "wer\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "VAINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}