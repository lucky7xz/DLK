{"textgrid.poem.54226": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erlaube, da\u00df ich darff nach deinen Wohlseyn fragen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erlaube, da\u00df ich darff nach deinen Wohlseyn fragen,", "tokens": ["Er\u00b7lau\u00b7be", ",", "da\u00df", "ich", "darff", "nach", "dei\u00b7nen", "Wohl\u00b7seyn", "fra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem ich lange Zeit gar nichts von dir geh\u00f6rt,", "tokens": ["Nach\u00b7dem", "ich", "lan\u00b7ge", "Zeit", "gar", "nichts", "von", "dir", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "ADV", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Schwester mu\u00df ich wohl vor dich auch Sorge tragen,", "tokens": ["Als", "Schwes\u00b7ter", "mu\u00df", "ich", "wohl", "vor", "dich", "auch", "Sor\u00b7ge", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da deine Treflichkeit ein jeglicher verehrt.", "tokens": ["Da", "dei\u00b7ne", "Tref\u00b7lich\u00b7keit", "ein", "jeg\u00b7li\u00b7cher", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "PIAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein Vetter reist von hier, ich darff nicht lange s\u00e4umen,", "tokens": ["Dein", "Vet\u00b7ter", "reist", "von", "hier", ",", "ich", "darff", "nicht", "lan\u00b7ge", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und hohl im Augenblick mein Saitenspiel herbey,", "tokens": ["Und", "hohl", "im", "Au\u00b7gen\u00b7blick", "mein", "Sai\u00b7ten\u00b7spiel", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du kennst die St\u00fcmperey von meinen schlechten Reimen,", "tokens": ["Du", "kennst", "die", "St\u00fcm\u00b7pe\u00b7rey", "von", "mei\u00b7nen", "schlech\u00b7ten", "Rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch weist du, da\u00df mein Kiel flieht alle Heucheley.", "tokens": ["Doch", "weist", "du", ",", "da\u00df", "mein", "Kiel", "flieht", "al\u00b7le", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dein edelm\u00fcthger Geist kan meine Schw\u00e4che leiden,", "tokens": ["Dein", "e\u00b7del\u00b7m\u00fcth\u00b7ger", "Geist", "kan", "mei\u00b7ne", "Schw\u00e4\u00b7che", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob meine Muse schon der Deinigen nicht gleicht,", "tokens": ["Ob", "mei\u00b7ne", "Mu\u00b7se", "schon", "der", "Dei\u00b7ni\u00b7gen", "nicht", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "PPOSS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "An der die gantze Welt mu\u00df Aug und Hertze weyden,", "tokens": ["An", "der", "die", "gant\u00b7ze", "Welt", "mu\u00df", "Aug", "und", "Hert\u00b7ze", "wey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VMFIN", "NN", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil Feuer und Verstand aus ieder Zeile leucht.", "tokens": ["Weil", "Feu\u00b7er", "und", "Ver\u00b7stand", "aus", "ie\u00b7der", "Zei\u00b7le", "leucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Hertze hast du l\u00e4ngst zu deinen Dienst und Willen,", "tokens": ["Mein", "Hert\u00b7ze", "hast", "du", "l\u00e4ngst", "zu", "dei\u00b7nen", "Dienst", "und", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es ist dir, wie du weist, vor andern zugedacht.", "tokens": ["Es", "ist", "dir", ",", "wie", "du", "weist", ",", "vor", "an\u00b7dern", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bishero schlug ich mich mit hundert tausend Grillen,", "tokens": ["Bis\u00b7he\u00b7ro", "schlug", "ich", "mich", "mit", "hun\u00b7dert", "tau\u00b7send", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ob die von Bre\u00dfler noch auf meine Neigung acht.", "tokens": ["Ob", "die", "von", "Bre\u00df\u00b7ler", "noch", "auf", "mei\u00b7ne", "Nei\u00b7gung", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NE", "ADV", "APPR", "PPOSAT", "NN", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du kanst, ich glaub es wohl, an mich nicht vielmahl dencken,", "tokens": ["Du", "kanst", ",", "ich", "glaub", "es", "wohl", ",", "an", "mich", "nicht", "viel\u00b7mahl", "den\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil dein besch\u00e4fftger Geist dir selbiges verbeut,", "tokens": ["Weil", "dein", "be\u00b7sch\u00e4fft\u00b7ger", "Geist", "dir", "sel\u00b7bi\u00b7ges", "ver\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn wer sein Aug und Kiel mu\u00df hohen Dingen schencken,", "tokens": ["Denn", "wer", "sein", "Aug", "und", "Kiel", "mu\u00df", "ho\u00b7hen", "Din\u00b7gen", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "KON", "NE", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der dencket, ists nicht wahr? an keine Kleinigkeit.", "tokens": ["Der", "den\u00b7cket", ",", "ists", "nicht", "wahr", "?", "an", "kei\u00b7ne", "Klei\u00b7nig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VAFIN", "PTKNEG", "PTKVZ", "$.", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch, da dein naher Freund, der dir die Hand wird k\u00fcssen,", "tokens": ["Doch", ",", "da", "dein", "na\u00b7her", "Freund", ",", "der", "dir", "die", "Hand", "wird", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Von seiner Reise mir vorhero Nachricht gab,", "tokens": ["Von", "sei\u00b7ner", "Rei\u00b7se", "mir", "vor\u00b7he\u00b7ro", "Nach\u00b7richt", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Und auch von meiner Schrifft sich will begleitet wissen,", "tokens": ["Und", "auch", "von", "mei\u00b7ner", "Schrifft", "sich", "will", "be\u00b7glei\u00b7tet", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "PRF", "VMFIN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So schick ich dieses Blat an dich durch selbgen ab.", "tokens": ["So", "schick", "ich", "die\u00b7ses", "Blat", "an", "dich", "durch", "selb\u00b7gen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "APPR", "PRF", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es soll den Nahmen dir nur blo\u00df von mir erwehnen,", "tokens": ["Es", "soll", "den", "Nah\u00b7men", "dir", "nur", "blo\u00df", "von", "mir", "er\u00b7weh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Damit du selbigen nicht gantz und gar vergist,", "tokens": ["Da\u00b7mit", "du", "sel\u00b7bi\u00b7gen", "nicht", "gantz", "und", "gar", "ver\u00b7gist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wiederum den Weg zu deiner Huld mir b\u00e4hnen,", "tokens": ["Und", "wie\u00b7de\u00b7rum", "den", "Weg", "zu", "dei\u00b7ner", "Huld", "mir", "b\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Woran, ich schw\u00f6r es dir, mir viel gelegen ist.", "tokens": ["Wo\u00b7ran", ",", "ich", "schw\u00f6r", "es", "dir", ",", "mir", "viel", "ge\u00b7le\u00b7gen", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wirstu mit deinen Kiel und Antwort mich beehren,", "tokens": ["Wirs\u00b7tu", "mit", "dei\u00b7nen", "Kiel", "und", "Ant\u00b7wort", "mich", "be\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So brech ich halb entz\u00fcckt davon das Siegel auf.", "tokens": ["So", "brech", "ich", "halb", "ent\u00b7z\u00fcckt", "da\u00b7von", "das", "Sie\u00b7gel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP", "PAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dich und die Nachtigall mag ich am liebsten h\u00f6ren,", "tokens": ["Dich", "und", "die", "Nach\u00b7ti\u00b7gall", "mag", "ich", "am", "liebs\u00b7ten", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "VMFIN", "PPER", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "O Freundin, s\u00e4ume nicht, ich warte schmertzlich drauf.", "tokens": ["O", "Freun\u00b7din", ",", "s\u00e4u\u00b7me", "nicht", ",", "ich", "war\u00b7te", "schmertz\u00b7lich", "drauf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}