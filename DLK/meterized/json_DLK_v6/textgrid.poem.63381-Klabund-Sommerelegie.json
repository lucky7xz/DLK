{"textgrid.poem.63381": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Sommerelegie", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sommer. Ich bin so m\u00fcde.", "tokens": ["Som\u00b7mer", ".", "Ich", "bin", "so", "m\u00fc\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Alles noch braun und leer.", "tokens": ["Al\u00b7les", "noch", "braun", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "F\u00f6rster mit B\u00fcchse und R\u00fcde.", "tokens": ["F\u00f6rs\u00b7ter", "mit", "B\u00fcch\u00b7se", "und", "R\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Jagd \u00fcber Moore und Meer.", "tokens": ["Jagd", "\u00fc\u00b7ber", "Moo\u00b7re", "und", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "M\u00f6wen in silbernen Binsen.", "tokens": ["M\u00f6\u00b7wen", "in", "sil\u00b7ber\u00b7nen", "Bin\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Alpen gezahnt und gezackt.", "tokens": ["Al\u00b7pen", "ge\u00b7zahnt", "und", "ge\u00b7zackt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sterbende Hasen linsen", "tokens": ["Ster\u00b7ben\u00b7de", "Ha\u00b7sen", "lin\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "In den Mondkatarakt.", "tokens": ["In", "den", "Mond\u00b7ka\u00b7ta\u00b7rakt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ner Falter im Himmel,", "tokens": ["Sch\u00f6\u00b7ner", "Fal\u00b7ter", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sieh, mir versagt der Blick,", "tokens": ["Sieh", ",", "mir", "ver\u00b7sagt", "der", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Deiner Fl\u00fcge Gewimmel", "tokens": ["Dei\u00b7ner", "Fl\u00fc\u00b7ge", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "F\u00e4llt in sich selber zur\u00fcck.", "tokens": ["F\u00e4llt", "in", "sich", "sel\u00b7ber", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "K\u00fche, die niemand melkte,", "tokens": ["K\u00fc\u00b7he", ",", "die", "nie\u00b7mand", "melk\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mit dem Euter so fahl,", "tokens": ["Mit", "dem", "Eu\u00b7ter", "so", "fahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und das verw\u00f6lkte, verwelkte,", "tokens": ["Und", "das", "ver\u00b7w\u00f6lk\u00b7te", ",", "ver\u00b7welk\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "G\u00f6ttliche Bacchanal \u2013", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Bac\u00b7cha\u00b7nal", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Deutschland ist untergegangen", "tokens": ["Deutschland", "ist", "un\u00b7ter\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "VAFIN", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In einem Bad von Stahl.", "tokens": ["In", "ei\u00b7nem", "Bad", "von", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heraldische Drachen und Schlangen", "tokens": ["Her\u00b7al\u00b7di\u00b7sche", "Dra\u00b7chen", "und", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Beten zum biblischen Baal.", "tokens": ["Be\u00b7ten", "zum", "bib\u00b7li\u00b7schen", "Baal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Ein bl\u00fchender Weidenstengel", "tokens": ["Ein", "bl\u00fc\u00b7hen\u00b7der", "Wei\u00b7dens\u00b7ten\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Erschl\u00e4gt diese ganze Welt.", "tokens": ["Er\u00b7schl\u00e4gt", "die\u00b7se", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schlafe, mein Stahlbadeengel,", "tokens": ["Schla\u00b7fe", ",", "mein", "Stahl\u00b7ba\u00b7deen\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schlaf, Nie-gelungen-Held.", "tokens": ["Schlaf", ",", "Nie\u00b7ge\u00b7lun\u00b7gen\u00b7Held", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}