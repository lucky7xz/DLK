{"dta.poem.7696": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Geiziger, der mit Begier sein Gold beschaut,", "tokens": ["Ein", "Gei\u00b7zi\u00b7ger", ",", "der", "mit", "Be\u00b7gier", "sein", "Gold", "be\u00b7schaut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und am verborgnen Schatz mit Andacht sich erbaut;", "tokens": ["Und", "am", "ver\u00b7borg\u00b7nen", "Schatz", "mit", "An\u00b7dacht", "sich", "er\u00b7baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der au\u00dferm Anblick nichts von seinem Gut genie\u00dft,", "tokens": ["Der", "au\u00b7\u00dferm", "An\u00b7blick", "nichts", "von", "sei\u00b7nem", "Gut", "ge\u00b7nie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur den Kasten auf und zu den Kasten schlie\u00dft;", "tokens": ["Und", "nur", "den", "Kas\u00b7ten", "auf", "und", "zu", "den", "Kas\u00b7ten", "schlie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKVZ", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist doch vern\u00fcnftiger als manch vern\u00fcnft'ger Mann,", "tokens": ["Ist", "doch", "ver\u00b7n\u00fcnf\u00b7ti\u00b7ger", "als", "manch", "ver\u00b7n\u00fcnft'\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der einen edlern Schatz als goldenen gewann,", "tokens": ["Der", "ei\u00b7nen", "ed\u00b7lern", "Schatz", "als", "gol\u00b7de\u00b7nen", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "KOUS", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der ein lebendig Gut besitzt von Fleisch und Blut,", "tokens": ["Der", "ein", "le\u00b7ben\u00b7dig", "Gut", "be\u00b7sitzt", "von", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit dessen Anblick er sich nichts zu Gute thut.", "tokens": ["Mit", "des\u00b7sen", "An\u00b7blick", "er", "sich", "nichts", "zu", "Gu\u00b7te", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PRF", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Was, Vater, hilft es dir, da\u00df Gott dir Kinder gab,", "tokens": ["Was", ",", "Va\u00b7ter", ",", "hilft", "es", "dir", ",", "da\u00df", "Gott", "dir", "Kin\u00b7der", "gab", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "KOUS", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihnen du den Blick gleichg\u00fcltig wendest ab?", "tokens": ["Wenn", "ih\u00b7nen", "du", "den", "Blick", "gleich\u00b7g\u00fcl\u00b7tig", "wen\u00b7dest", "ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn du aus reiner Lust nach ihnen schauest selten,", "tokens": ["Wenn", "du", "aus", "rei\u00b7ner", "Lust", "nach", "ih\u00b7nen", "schau\u00b7est", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fast nur, wann du willst befehlen oder schelten!", "tokens": ["Und", "fast", "nur", ",", "wann", "du", "willst", "be\u00b7feh\u00b7len", "o\u00b7der", "schel\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PWAV", "PPER", "VMFIN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}