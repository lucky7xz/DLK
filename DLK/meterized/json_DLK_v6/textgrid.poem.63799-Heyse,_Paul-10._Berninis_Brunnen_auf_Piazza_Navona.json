{"textgrid.poem.63799": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10. Berninis Brunnen auf Piazza Navona", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, er ist nur ein Manierist, doch manchmal im gr\u00f6\u00dften", "tokens": ["Ja", ",", "er", "ist", "nur", "ein", "Ma\u00b7nie\u00b7rist", ",", "doch", "manch\u00b7mal", "im", "gr\u00f6\u00df\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Stil, de\u00df wilder Humor jeden Stilisten besch\u00e4mt.", "tokens": ["Stil", ",", "de\u00df", "wil\u00b7der", "Hu\u00b7mor", "je\u00b7den", "Sti\u00b7lis\u00b7ten", "be\u00b7sch\u00e4mt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dies Flu\u00dfg\u00f6ttergesindel, das ungeschlachte, die Bestien", "tokens": ["Dies", "Flu\u00df\u00b7g\u00f6t\u00b7ter\u00b7ge\u00b7sin\u00b7del", ",", "das", "un\u00b7ge\u00b7schlach\u00b7te", ",", "die", "Be\u00b7sti\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "NN", "$,", "ART", "ADJA", "$,", "ART", "NN"], "meter": "-++--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Um den zerkl\u00fcfteten Fels, vom Obelisken bekr\u00f6nt \u2013", "tokens": ["Um", "den", "zer\u00b7kl\u00fcf\u00b7te\u00b7ten", "Fels", ",", "vom", "O\u00b7be\u00b7lis\u00b7ken", "be\u00b7kr\u00f6nt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "H\u00e4tt' ein Gr\u00f6\u00dferer hier sich so gro\u00df aus dem Handel gezogen,", "tokens": ["H\u00e4tt'", "ein", "Gr\u00f6\u00b7\u00dfe\u00b7rer", "hier", "sich", "so", "gro\u00df", "aus", "dem", "Han\u00b7del", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PRF", "ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+---+-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Mit so guter Manier hier ein Stilist uns erg\u00f6tzt?", "tokens": ["Mit", "so", "gu\u00b7ter", "Ma\u00b7nier", "hier", "ein", "Sti\u00b7list", "uns", "er\u00b7g\u00f6tzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}