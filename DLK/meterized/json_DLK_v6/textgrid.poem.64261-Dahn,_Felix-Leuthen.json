{"textgrid.poem.64261": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Leuthen", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch' blutig Ringen um den Zettelbusch!", "tokens": ["Welch'", "blu\u00b7tig", "Rin\u00b7gen", "um", "den", "Zet\u00b7tel\u00b7busch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es steht die Schlacht, das grimm'ge Dorfgefecht!", "tokens": ["Es", "steht", "die", "Schlacht", ",", "das", "grim\u00b7m'\u00b7ge", "Dorf\u00b7ge\u00b7fecht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Hier blitzt das Bajonett, hier schneiden pfeifend", "tokens": ["Hier", "blitzt", "das", "Ba\u00b7jo\u00b7nett", ",", "hier", "schnei\u00b7den", "pfei\u00b7fend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Husarens\u00e4bel durch die graue Luft", "tokens": ["Hu\u00b7sa\u00b7ren\u00b7s\u00e4\u00b7bel", "durch", "die", "grau\u00b7e", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Und dumpf erkracht der Donner der Kartaunen.", "tokens": ["Und", "dumpf", "er\u00b7kracht", "der", "Don\u00b7ner", "der", "Kar\u00b7tau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jetzt, schr\u00e4ge Schlachtordnung, bew\u00e4hre dich!", "tokens": ["Jetzt", ",", "schr\u00e4\u00b7ge", "Schlach\u00b7tord\u00b7nung", ",", "be\u00b7w\u00e4h\u00b7re", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ja, sie bew\u00e4hrt sich! Wie durch Nacht der Morgen", "tokens": ["Ja", ",", "sie", "be\u00b7w\u00e4hrt", "sich", "!", "Wie", "durch", "Nacht", "der", "Mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PRF", "$.", "PWAV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bricht sieghaft durch die dr\u00e4uende Gefahr,", "tokens": ["Bricht", "sieg\u00b7haft", "durch", "die", "dr\u00e4u\u00b7en\u00b7de", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.9": {"text": "Dein Feldherrngeist, o Friedrich, und dein Stern!", "tokens": ["Dein", "Feld\u00b7herrn\u00b7geist", ",", "o", "Fried\u00b7rich", ",", "und", "dein", "Stern", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NE", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}