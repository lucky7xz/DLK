{"textgrid.poem.53111": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Amor, Hertzen-Binder,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Amor, Hertzen-Binder,", "tokens": ["O", "A\u00b7mor", ",", "Hert\u00b7zen\u00b7Bin\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du Herr der Freundlicheit", "tokens": ["Du", "Herr", "der", "Freund\u00b7li\u00b7cheit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd aller guten Zeit,", "tokens": ["Vnd", "al\u00b7ler", "gu\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du zwietracht-\u00fcberwinder!", "tokens": ["Du", "zwie\u00b7tracht\u00b7\u00fcber\u00b7win\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-++-+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Du grosser Wolfahrt-Heger,", "tokens": ["Du", "gros\u00b7ser", "Wolfahr\u00b7tHe\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie, da\u00df die gantze Welt", "tokens": ["Wie", ",", "da\u00df", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir hin zu Fusse f\u00e4llt,", "tokens": ["Dir", "hin", "zu", "Fus\u00b7se", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd folget deinem L\u00e4ger:", "tokens": ["Vnd", "fol\u00b7get", "dei\u00b7nem", "L\u00e4\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie weistu ein-zusperren", "tokens": ["Wie", "weis\u00b7tu", "ein\u00b7zu\u00b7sper\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Scepters gantze Macht!", "tokens": ["Des", "Scep\u00b7ters", "gant\u00b7ze", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir dient der Krohnen Pracht,", "tokens": ["Dir", "dient", "der", "Kroh\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Knecht auch sampt dem Herren.", "tokens": ["Der", "Knecht", "auch", "sampt", "dem", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das Alter wird gerissen", "tokens": ["Das", "Al\u00b7ter", "wird", "ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwar an dein strenges Joch,", "tokens": ["Zwar", "an", "dein", "stren\u00b7ges", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Jugend pflegstu doch", "tokens": ["Die", "Ju\u00b7gend", "pflegs\u00b7tu", "doch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am meisten ein-zu-schliessen.", "tokens": ["Am", "meis\u00b7ten", "ein\u00b7zu\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du machst dich in die Wangen", "tokens": ["Du", "machst", "dich", "in", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Frawen-Bilder hin,", "tokens": ["Der", "Fra\u00b7wen\u00b7Bil\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd f\u00fchrst den starcken Sinn", "tokens": ["Vnd", "f\u00fchrst", "den", "star\u00b7cken", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der M\u00e4nner so gefangen.", "tokens": ["Der", "M\u00e4n\u00b7ner", "so", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was keine Macht kan brechen,", "tokens": ["Was", "kei\u00b7ne", "Macht", "kan", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Stahl, kein fallend Bley,", "tokens": ["Kein", "Stahl", ",", "kein", "fal\u00b7lend", "Bley", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was keine Tyranney,", "tokens": ["Was", "kei\u00b7ne", "Ty\u00b7ran\u00b7ney", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Weist endlich du zu schw\u00e4chen.", "tokens": ["Weist", "end\u00b7lich", "du", "zu", "schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du hast die Welt gelehret", "tokens": ["Du", "hast", "die", "Welt", "ge\u00b7leh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das, was sie gutes hat,", "tokens": ["Das", ",", "was", "sie", "gu\u00b7tes", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJA", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Daher auch Dorff vnd Stadt", "tokens": ["Da\u00b7her", "auch", "Dorff", "vnd", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dir billich zugeh\u00f6ret:", "tokens": ["Dir", "bil\u00b7lich", "zu\u00b7ge\u00b7h\u00f6\u00b7ret", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da\u00df wir die Felder bawen,", "tokens": ["Da\u00df", "wir", "die", "Fel\u00b7der", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nach Ehr vnd G\u00fcttern stehn,", "tokens": ["Nach", "Ehr", "vnd", "G\u00fct\u00b7tern", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Tieff in das Erdreich gehn,", "tokens": ["Tieff", "in", "das", "Er\u00b7dreich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vns Wind vnd Wellen trawen,", "tokens": ["Vns", "Wind", "vnd", "Wel\u00b7len", "tra\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wodurch wir zugenommen,", "tokens": ["Wo\u00b7durch", "wir", "zu\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In aller Pracht vnd Zier,", "tokens": ["In", "al\u00b7ler", "Pracht", "vnd", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df eigentlich von dir,", "tokens": ["Mu\u00df", "ei\u00b7gent\u00b7lich", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du Welt-Bereicher, kommen.", "tokens": ["Du", "Welt\u00b7Be\u00b7rei\u00b7cher", ",", "kom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du endest Angst vnd Leiden;", "tokens": ["Du", "en\u00b7dest", "Angst", "vnd", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Greiffst du, O Amor, an", "tokens": ["Greiffst", "du", ",", "O", "A\u00b7mor", ",", "an"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "NE", "NE", "$,", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd hilffst, so tr\u00e4get man", "tokens": ["Vnd", "hilffst", ",", "so", "tr\u00e4\u00b7get", "man"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des Creutzes Last mit Frewden.", "tokens": ["Des", "Creut\u00b7zes", "Last", "mit", "Frew\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Durch dich mu\u00df alles werden,", "tokens": ["Durch", "dich", "mu\u00df", "al\u00b7les", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Vieh vnd Menschen noht,", "tokens": ["Was", "Vieh", "vnd", "Men\u00b7schen", "noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ohn dich k\u00f6mpt weder Brodt", "tokens": ["Ohn", "dich", "k\u00f6mpt", "we\u00b7der", "Brodt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Noch Wein-wachs aus der Erden:", "tokens": ["Noch", "Wein\u00b7wachs", "aus", "der", "Er\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wie sch\u00f6n die V\u00f6gel singen,", "tokens": ["Wie", "sch\u00f6n", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie fr\u00f6hlich durch das Meer", "tokens": ["Wie", "fr\u00f6h\u00b7lich", "durch", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Fische Schaar, das Heer", "tokens": ["Der", "Fi\u00b7sche", "Schaar", ",", "das", "Heer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Thier' im Walde springen;", "tokens": ["Der", "Thier'", "im", "Wal\u00b7de", "sprin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Wie lustig sich mit T\u00e4ntzen", "tokens": ["Wie", "lus\u00b7tig", "sich", "mit", "T\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Volck der Sternen macht,", "tokens": ["Das", "Volck", "der", "Ster\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie helle bey der Nacht", "tokens": ["Wie", "hel\u00b7le", "bey", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie vmb den Mond her gl\u00e4ntzen,", "tokens": ["Sie", "vmb", "den", "Mond", "her", "gl\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APZR", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wie schnell der Sonnen R\u00e4der,", "tokens": ["Wie", "schnell", "der", "Son\u00b7nen", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie lieblich Lufft vnd Wind,", "tokens": ["Wie", "lieb\u00b7lich", "Lufft", "vnd", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie angenehm vns sind", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", "vns", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Brunnen, Fl\u00fcsse, B\u00e4der.", "tokens": ["Die", "Brun\u00b7nen", ",", "Fl\u00fcs\u00b7se", ",", "B\u00e4\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Doch were nichts zu sp\u00fcren", "tokens": ["Doch", "we\u00b7re", "nichts", "zu", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allem, was man kennt,", "tokens": ["Von", "al\u00b7lem", ",", "was", "man", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn du das Regiment", "tokens": ["Wenn", "du", "das", "Re\u00b7gi\u00b7ment"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht, Amor, soltest f\u00fchren.", "tokens": ["Nicht", ",", "A\u00b7mor", ",", "sol\u00b7test", "f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "NE", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Gl\u00fcckseelig ist die Stunde,", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "ist", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kriegt anders Zeit hie stat,", "tokens": ["Kriegt", "an\u00b7ders", "Zeit", "hie", "stat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da Gott gezeugt dich hat,", "tokens": ["Da", "Gott", "ge\u00b7zeugt", "dich", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aus seines Hertzens Grunde!", "tokens": ["Aus", "sei\u00b7nes", "Hert\u00b7zens", "Grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Man hat von keinen Plagen", "tokens": ["Man", "hat", "von", "kei\u00b7nen", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da jrgends wo gewust,", "tokens": ["Da", "jr\u00b7gends", "wo", "ge\u00b7wust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PWAV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd nur von lauter Lust", "tokens": ["Vnd", "nur", "von", "lau\u00b7ter", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Frewde k\u00f6nnen sagen.", "tokens": ["Vnd", "Frew\u00b7de", "k\u00f6n\u00b7nen", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Da war kein Ha\u00df verhanden,", "tokens": ["Da", "war", "kein", "Ha\u00df", "ver\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Argwohn vnd kein Streit,", "tokens": ["Kein", "Arg\u00b7wohn", "vnd", "kein", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fried' vnd Gerechtigkeit", "tokens": ["Fried'", "vnd", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Sind umb dich her gestanden.", "tokens": ["Sind", "umb", "dich", "her", "ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "APZR", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Man sieht auch jetzund Leben", "tokens": ["Man", "sieht", "auch", "je\u00b7tzund", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd grosses Wolergehn", "tokens": ["Vnd", "gros\u00b7ses", "Wo\u00b7ler\u00b7gehn"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An allen Orten stehn,", "tokens": ["An", "al\u00b7len", "Or\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo Du dich hinbegeben.", "tokens": ["Wo", "Du", "dich", "hin\u00b7be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "So komm nun, dein Begn\u00fcgen", "tokens": ["So", "komm", "nun", ",", "dein", "Be\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vmbschlie\u00df' auch dieses Paar", "tokens": ["Vmb\u00b7schlie\u00df'", "auch", "die\u00b7ses", "Paar"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In Eintracht jmmerdar,", "tokens": ["In", "Ein\u00b7tracht", "jm\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Ehlich jetzt sich f\u00fcgen!", "tokens": ["Die", "Eh\u00b7lich", "jetzt", "sich", "f\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Du bist es, den wir singen,", "tokens": ["Du", "bist", "es", ",", "den", "wir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du vnd das wahre Gut,", "tokens": ["Du", "vnd", "das", "wah\u00b7re", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der vns das liebste thut,", "tokens": ["Der", "vns", "das", "liebs\u00b7te", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gott selbst f\u00fcr allen Dingen.", "tokens": ["Gott", "selbst", "f\u00fcr", "al\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Wir werden angetrieben", "tokens": ["Wir", "wer\u00b7den", "an\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu sagen: Er allein", "tokens": ["Zu", "sa\u00b7gen", ":", "Er", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df selbst die Liebe seyn,", "tokens": ["Mu\u00df", "selbst", "die", "Lie\u00b7be", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Er so rein kan \u00fcben.", "tokens": ["Die", "Er", "so", "rein", "kan", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "O selig, selig weren", "tokens": ["O", "se\u00b7lig", ",", "se\u00b7lig", "we\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "ADJD", "$,", "ADJD", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir Menschen allerseit,", "tokens": ["Wir", "Men\u00b7schen", "al\u00b7ler\u00b7seit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die wir durch Ha\u00df vnd Streit", "tokens": ["Die", "wir", "durch", "Ha\u00df", "vnd", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erb\u00e4rmlich vns verzehren,", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "vns", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Wenn doch auch vns die Liebe,", "tokens": ["Wenn", "doch", "auch", "vns", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die alles hie vnd da,", "tokens": ["Die", "al\u00b7les", "hie", "vnd", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd selbst den Himmel, ja", "tokens": ["Vnd", "selbst", "den", "Him\u00b7mel", ",", "ja"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am meisten Gott treibt, triebe!", "tokens": ["Am", "meis\u00b7ten", "Gott", "treibt", ",", "trie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}