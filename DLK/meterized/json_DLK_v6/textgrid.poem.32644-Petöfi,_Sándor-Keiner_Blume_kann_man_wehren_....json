{"textgrid.poem.32644": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Keiner Blume kann man wehren ...", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Keiner Blume kann man wehren, da\u00df sie bl\u00fcht,", "tokens": ["Kei\u00b7ner", "Blu\u00b7me", "kann", "man", "weh\u00b7ren", ",", "da\u00df", "sie", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PIS", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn einher der wunderreiche Fr\u00fchling zieht!", "tokens": ["Wenn", "ein\u00b7her", "der", "wun\u00b7der\u00b7rei\u00b7che", "Fr\u00fch\u00b7ling", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nun \u2013 die Liebe ist der Fr\u00fchling; Lenz und Mai", "tokens": ["Nun", "\u2013", "die", "Lie\u00b7be", "ist", "der", "Fr\u00fch\u00b7ling", ";", "Lenz", "und", "Mai"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$(", "ART", "NN", "VAFIN", "ART", "NN", "$.", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ist das M\u00e4dchen; bl\u00fcht es, \u2013 bl\u00fchen alle zwei!", "tokens": ["Ist", "das", "M\u00e4d\u00b7chen", ";", "bl\u00fcht", "es", ",", "\u2013", "bl\u00fc\u00b7hen", "al\u00b7le", "zwei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$.", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PIAT", "CARD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Da ich dich erblickt, du Sch\u00f6nste, die es gibt,", "tokens": ["Da", "ich", "dich", "er\u00b7blickt", ",", "du", "Sch\u00f6ns\u00b7te", ",", "die", "es", "gibt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,", "PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "War in deine sch\u00f6ne Seel' ich gleich verliebt,", "tokens": ["War", "in", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Seel'", "ich", "gleich", "ver\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Diese Seele, die in stiller, milder Pracht", "tokens": ["Die\u00b7se", "See\u00b7le", ",", "die", "in", "stil\u00b7ler", ",", "mil\u00b7der", "Pracht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Aus dem Zauberspiegel deiner Augen lacht!", "tokens": ["Aus", "dem", "Zau\u00b7ber\u00b7spie\u00b7gel", "dei\u00b7ner", "Au\u00b7gen", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Und die bange Frage in mein Herze schlich:", "tokens": ["Und", "die", "ban\u00b7ge", "Fra\u00b7ge", "in", "mein", "Her\u00b7ze", "schlich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Liebst du einen andern, oder liebst du mich?", "tokens": ["Liebst", "du", "ei\u00b7nen", "an\u00b7dern", ",", "o\u00b7der", "liebst", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "KON", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ein Gedanke jagt den n\u00e4chsten allemal,", "tokens": ["Ein", "Ge\u00b7dan\u00b7ke", "jagt", "den", "n\u00e4chs\u00b7ten", "al\u00b7le\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wie im Herbst die Wolke jagt den Sonnenstrahl.", "tokens": ["Wie", "im", "Herbst", "die", "Wol\u00b7ke", "jagt", "den", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "W\u00fc\u00dfte ich, da\u00df deine Rosenwange zart,", "tokens": ["W\u00fc\u00df\u00b7te", "ich", ",", "da\u00df", "dei\u00b7ne", "Ro\u00b7sen\u00b7wan\u00b7ge", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df dein Purpurm\u00fcndchen eines andern harrt,", "tokens": ["Da\u00df", "dein", "Pur\u00b7pur\u00b7m\u00fcnd\u00b7chen", "ei\u00b7nes", "an\u00b7dern", "harrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Ach, dann w\u00e4r' mein Leben eitel Qual und Pein,", "tokens": ["Ach", ",", "dann", "w\u00e4r'", "mein", "Le\u00b7ben", "ei\u00b7tel", "Qual", "und", "Pein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Lieber wollt' ich auf der Stell' begraben sein.", "tokens": ["Lie\u00b7ber", "wollt'", "ich", "auf", "der", "Stell'", "be\u00b7gra\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "APPR", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Leuchte mir, du Stern, der mich mit Gl\u00fcck erf\u00fcllt,", "tokens": ["Leuch\u00b7te", "mir", ",", "du", "Stern", ",", "der", "mich", "mit", "Gl\u00fcck", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Da\u00df mein Leben nicht des Kummers Nacht umh\u00fcllt;", "tokens": ["Da\u00df", "mein", "Le\u00b7ben", "nicht", "des", "Kum\u00b7mers", "Nacht", "um\u00b7h\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "S\u00fc\u00dfe Perle, schenke deine Liebe mir,", "tokens": ["S\u00fc\u00b7\u00dfe", "Per\u00b7le", ",", "schen\u00b7ke", "dei\u00b7ne", "Lie\u00b7be", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Segnen m\u00f6ge tausendmal dich Gott daf\u00fcr!", "tokens": ["Seg\u00b7nen", "m\u00f6\u00b7ge", "tau\u00b7send\u00b7mal", "dich", "Gott", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADV", "PPER", "NN", "PAV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}