{"textgrid.poem.65869": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "143", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine Hausfrau sorgsam sich beeilt,", "tokens": ["Wie", "ei\u00b7ne", "Haus\u00b7frau", "sorg\u00b7sam", "sich", "be\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Federvieh zu fahn, das ihr entronnen,", "tokens": ["Ein", "Fe\u00b7der\u00b7vieh", "zu", "fahn", ",", "das", "ihr", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den S\u00e4ugling niedersetzt, und unverweilt", "tokens": ["Den", "S\u00e4ug\u00b7ling", "nie\u00b7der\u00b7setzt", ",", "und", "un\u00b7ver\u00b7weilt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Vogel nachl\u00e4uft, den sie gern gewonnen:", "tokens": ["Dem", "Vo\u00b7gel", "nach\u00b7l\u00e4uft", ",", "den", "sie", "gern", "ge\u00b7won\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Derweil mit Schrei'n ihr unberaten Kind", "tokens": ["Der\u00b7weil", "mit", "Schrei'n", "ihr", "un\u00b7be\u00b7ra\u00b7ten", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie aufzuhalten ringt, die emsiglich", "tokens": ["Sie", "auf\u00b7zu\u00b7hal\u00b7ten", "ringt", ",", "die", "em\u00b7sig\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVIZU", "VVFIN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was vor ihr herl\u00e4uft zu erhaschen sinnt,", "tokens": ["Was", "vor", "ihr", "her\u00b7l\u00e4uft", "zu", "er\u00b7ha\u00b7schen", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Unachtsam wie ihr Kn\u00e4blein \u00e4ngstet sich:", "tokens": ["U\u00b7nacht\u00b7sam", "wie", "ihr", "Kn\u00e4\u00b7blein", "\u00e4ngs\u00b7tet", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So l\u00e4ufst du hinter dem, was dir entweicht,", "tokens": ["So", "l\u00e4ufst", "du", "hin\u00b7ter", "dem", ",", "was", "dir", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und ich, dein Kind, dir nach in tr\u00fcbem Mut.", "tokens": ["Und", "ich", ",", "dein", "Kind", ",", "dir", "nach", "in", "tr\u00fc\u00b7bem", "Mut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Allein blick' um dich, wenn du's nun erreicht,", "tokens": ["Al\u00b7lein", "blick'", "um", "dich", ",", "wenn", "du's", "nun", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "\u00dcb' Mutterpflichten, k\u00fcsse mich, sei gut!", "tokens": ["\u00dcb'", "Mut\u00b7ter\u00b7pflich\u00b7ten", ",", "k\u00fcs\u00b7se", "mich", ",", "sei", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So will ich bitten, da\u00df dir's werd erf\u00fcllt,", "tokens": ["So", "will", "ich", "bit\u00b7ten", ",", "da\u00df", "dir's", "werd", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Kommst du zur\u00fcck, und wird mein Schrei'n gestillt.", "tokens": ["Kommst", "du", "zu\u00b7r\u00fcck", ",", "und", "wird", "mein", "Schrei'n", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}