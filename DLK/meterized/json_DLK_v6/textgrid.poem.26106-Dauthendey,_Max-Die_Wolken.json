{"textgrid.poem.26106": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Wolken", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken, die sich wie im Schlaf hindehnenden,", "tokens": ["Die", "Wol\u00b7ken", ",", "die", "sich", "wie", "im", "Schlaf", "hin\u00b7deh\u00b7nen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "KOKOM", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Hinziehend \u00fcber des Himmels Abgrund, den g\u00e4hnenden,", "tokens": ["Hin\u00b7zie\u00b7hend", "\u00fc\u00b7ber", "des", "Him\u00b7mels", "Ab\u00b7grund", ",", "den", "g\u00e4h\u00b7nen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie verleben ihre Tage im Schweben.", "tokens": ["Sie", "ver\u00b7le\u00b7ben", "ih\u00b7re", "Ta\u00b7ge", "im", "Schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn sie sich \u00fcber die \u00c4cker hinheben,", "tokens": ["Wenn", "sie", "sich", "\u00fc\u00b7ber", "die", "\u00c4\u00b7cker", "hin\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind sie wie Frauen, welche der Erde die Br\u00fcste geben,", "tokens": ["Sind", "sie", "wie", "Frau\u00b7en", ",", "wel\u00b7che", "der", "Er\u00b7de", "die", "Br\u00fcs\u00b7te", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sind sie wie Betten, ausgebreitet dem Liebesgel\u00fcste;", "tokens": ["Sind", "sie", "wie", "Bet\u00b7ten", ",", "aus\u00b7ge\u00b7brei\u00b7tet", "dem", "Lie\u00b7bes\u00b7ge\u00b7l\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Sind sie wie schreckende, d\u00fcstere Schattenger\u00fcste,", "tokens": ["Sind", "sie", "wie", "schre\u00b7cken\u00b7de", ",", "d\u00fcs\u00b7te\u00b7re", "Schat\u00b7ten\u00b7ge\u00b7r\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind sie die Herde der Sehnenden", "tokens": ["Sind", "sie", "die", "Her\u00b7de", "der", "Seh\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.3": {"text": "In der Sehnsucht blauer unendlicher W\u00fcste.", "tokens": ["In", "der", "Sehn\u00b7sucht", "blau\u00b7er", "un\u00b7end\u00b7li\u00b7cher", "W\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "ADJA", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}