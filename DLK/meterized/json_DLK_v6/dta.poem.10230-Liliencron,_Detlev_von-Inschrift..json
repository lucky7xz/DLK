{"dta.poem.10230": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Inschrift.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Nach raschem Ritt im Regen waren wir           ", "tokens": ["Nach", "ra\u00b7schem", "Ritt", "im", "Re\u00b7gen", "wa\u00b7ren", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VAFIN", "PPER"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf einem Gottesacker angekommen", "tokens": ["Auf", "ei\u00b7nem", "Got\u00b7te\u00b7sa\u00b7cker", "an\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und abgesessen. Ungesehen, konnten", "tokens": ["Und", "ab\u00b7ge\u00b7ses\u00b7sen", ".", "Un\u00b7ge\u00b7se\u00b7hen", ",", "konn\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVPP", "$.", "NN", "$,", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach allen Seiten frei wir uns bewegen", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", "frei", "wir", "uns", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und vorpreschen, die Feldwachen zu tr\u00f6sten.", "tokens": ["Und", "vor\u00b7pre\u00b7schen", ",", "die", "Feld\u00b7wa\u00b7chen", "zu", "tr\u00f6s\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIZU", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nur wenig Kreuze. Rasch band das Piquet", "tokens": ["Nur", "we\u00b7nig", "Kreu\u00b7ze", ".", "Rasch", "band", "das", "Pi\u00b7quet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$.", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Halfter an die winzigen Todeszeichen.", "tokens": ["Die", "Half\u00b7ter", "an", "die", "win\u00b7zi\u00b7gen", "To\u00b7des\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ich selber lehnte bald den m\u00fcden Kopf", "tokens": ["Ich", "sel\u00b7ber", "lehn\u00b7te", "bald", "den", "m\u00fc\u00b7den", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf eines Grabes H\u00fcgel und schlief ein....", "tokens": ["Auf", "ei\u00b7nes", "Gra\u00b7bes", "H\u00fc\u00b7gel", "und", "schlief", "ein", "...."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Hell wieherte im Nebeldunst mein Wallach", "tokens": ["Hell", "wie\u00b7her\u00b7te", "im", "Ne\u00b7bel\u00b7dunst", "mein", "Wal\u00b7lach"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Und sprengte j\u00e4h die weichen Sclavenketten,", "tokens": ["Und", "spreng\u00b7te", "j\u00e4h", "die", "wei\u00b7chen", "Scla\u00b7ven\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die unbewu\u00dft und traumlos mich umwanden.", "tokens": ["Die", "un\u00b7be\u00b7wu\u00dft", "und", "traum\u00b7los", "mich", "um\u00b7wan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Noch schlafend lagen um mich die Dragoner,", "tokens": ["Noch", "schla\u00b7fend", "la\u00b7gen", "um", "mich", "die", "Dra\u00b7go\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Bedeckt mit Reif die M\u00e4ntel und die B\u00e4rte,", "tokens": ["Be\u00b7deckt", "mit", "Reif", "die", "M\u00e4n\u00b7tel", "und", "die", "B\u00e4r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Pferde standen mit gesenkten M\u00e4hnen.", "tokens": ["Die", "Pfer\u00b7de", "stan\u00b7den", "mit", "ge\u00b7senk\u00b7ten", "M\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nur ab und an ein Schnaufen und ein Scharren,", "tokens": ["Nur", "ab", "und", "an", "ein", "Schnau\u00b7fen", "und", "ein", "Schar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "APPR", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ein Knistern an den S\u00e4tteln, und ein Klirren", "tokens": ["Ein", "Knis\u00b7tern", "an", "den", "S\u00e4t\u00b7teln", ",", "und", "ein", "Klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der Kettchen, wenn sie aneinander klangen.", "tokens": ["Der", "Kett\u00b7chen", ",", "wenn", "sie", "an\u00b7ein\u00b7an\u00b7der", "klan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Den Carabiner in den F\u00e4usten haltend,", "tokens": ["Den", "Ca\u00b7ra\u00b7bi\u00b7ner", "in", "den", "F\u00e4us\u00b7ten", "hal\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Schritt schweren Tritts der Posten auf und nieder. \u2014", "tokens": ["Schritt", "schwe\u00b7ren", "Tritts", "der", "Pos\u00b7ten", "auf", "und", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Tief eine Stille war es; leises Rauschen", "tokens": ["Tief", "ei\u00b7ne", "Stil\u00b7le", "war", "es", ";", "lei\u00b7ses", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "NN", "VAFIN", "PPER", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Zog morgenschauernd durch die Trauerkr\u00e4nze\u2026", "tokens": ["Zog", "mor\u00b7gen\u00b7schau\u00b7ernd", "durch", "die", "Trau\u00b7er\u00b7kr\u00e4n\u00b7ze", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.23": {"text": "Ich hob den Kopf und drehte mich, um Namen", "tokens": ["Ich", "hob", "den", "Kopf", "und", "dreh\u00b7te", "mich", ",", "um", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "$,", "KOUI", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und Inschrift an dem kleinen Kreuz zu lesen,", "tokens": ["Und", "In\u00b7schrift", "an", "dem", "klei\u00b7nen", "Kreuz", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Das mir zu H\u00e4upten stand, und las im Zwielicht,", "tokens": ["Das", "mir", "zu", "H\u00e4up\u00b7ten", "stand", ",", "und", "las", "im", "Zwie\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Das Auge hart an die vergoldeten,", "tokens": ["Das", "Au\u00b7ge", "hart", "an", "die", "ver\u00b7gol\u00b7de\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Vom Wetter schwarz gef\u00e4rbten Lettern dr\u00e4ngend:", "tokens": ["Vom", "Wet\u00b7ter", "schwarz", "ge\u00b7f\u00e4rb\u00b7ten", "Let\u00b7tern", "dr\u00e4n\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "\u201egestritten viel \u2014 gelitten mehr \u2014 gestorben\u201c.", "tokens": ["\u201e", "ge\u00b7strit\u00b7ten", "viel", "ge\u00b7lit\u00b7ten", "mehr", "ge\u00b7stor\u00b7ben", "\u201c", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVPP", "ADV", "$(", "VVPP", "ADV", "$(", "VVPP", "$(", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Fr\u00fchrote Lichter schwammen um die Worte,", "tokens": ["Fr\u00fch\u00b7ro\u00b7te", "Lich\u00b7ter", "schwam\u00b7men", "um", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.30": {"text": "Die bleischwer sich in meine Seele senkten.", "tokens": ["Die", "blei\u00b7schwer", "sich", "in", "mei\u00b7ne", "See\u00b7le", "senk\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Zum Denken doch ward mir nicht Zeit gelassen,", "tokens": ["Zum", "Den\u00b7ken", "doch", "ward", "mir", "nicht", "Zeit", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "PTKNEG", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Denn: \u201eAn die Pferde\u201c hie\u00df es \u2026 \u201eAuf \u2014 \u2014 gesessen!\u201c", "tokens": ["Denn", ":", "\u201e", "An", "die", "Pfer\u00b7de", "\u201c", "hie\u00df", "es", "\u2026", "\u201e", "Auf", "ge\u00b7ses\u00b7sen", "!", "\u201c"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "$.", "$(", "APPR", "ART", "NN", "$(", "VVFIN", "PPER", "$(", "$(", "APPR", "$(", "$(", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Wir trabten, sonnbegr\u00fc\u00dft, ins Thal hinunter,", "tokens": ["Wir", "trab\u00b7ten", ",", "sonn\u00b7be\u00b7gr\u00fc\u00dft", ",", "ins", "Thal", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Um, Freund und Feind, aus dunkelroten Rosen", "tokens": ["Um", ",", "Freund", "und", "Feind", ",", "aus", "dun\u00b7kel\u00b7ro\u00b7ten", "Ro\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "$,", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Auf gr\u00fcnem Rasen einen Strau\u00df zu flechten.", "tokens": ["Auf", "gr\u00fc\u00b7nem", "Ra\u00b7sen", "ei\u00b7nen", "Strau\u00df", "zu", "flech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}