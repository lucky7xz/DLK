{"textgrid.poem.36351": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Andenken", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Kind in Weibesbl\u00fcte,", "tokens": ["Es", "ist", "ein", "Kind", "in", "Wei\u00b7bes\u00b7bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das steht mir ewig im Gem\u00fcte,", "tokens": ["Das", "steht", "mir", "e\u00b7wig", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimmt mir die Gedanken hin;", "tokens": ["Und", "nimmt", "mir", "die", "Ge\u00b7dan\u00b7ken", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sie ist, fliehn dahin die Tage,", "tokens": ["Wo", "sie", "ist", ",", "fliehn", "da\u00b7hin", "die", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$,", "VVFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und ist sie fern, so f\u00fcllt die Klage", "tokens": ["Und", "ist", "sie", "fern", ",", "so", "f\u00fcllt", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mir wonnevoll den stillen Sinn.", "tokens": ["Mir", "won\u00b7ne\u00b7voll", "den", "stil\u00b7len", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df mich von ihrer Sch\u00f6nheit schweigen,", "tokens": ["La\u00df", "mich", "von", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bleibt in Ewigkeit mein eigen,", "tokens": ["Sie", "bleibt", "in", "E\u00b7wig\u00b7keit", "mein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bring' ich sie zum Opfer dar:", "tokens": ["Doch", "bring'", "ich", "sie", "zum", "Op\u00b7fer", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seele, die du einzig liebtest,", "tokens": ["Die", "See\u00b7le", ",", "die", "du", "ein\u00b7zig", "lieb\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die du in Lieb' und Leiden \u00fcbtest,", "tokens": ["Die", "du", "in", "Lieb'", "und", "Lei\u00b7den", "\u00fcb\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "O Herr! dein Bild und dein Altar.", "tokens": ["O", "Herr", "!", "dein", "Bild", "und", "dein", "Al\u00b7tar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In Trauer leuchtet ihre Sch\u00f6ne,", "tokens": ["In", "Trau\u00b7er", "leuch\u00b7tet", "ih\u00b7re", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Brust entflie\u00dfen Klaget\u00f6ne,", "tokens": ["Der", "Brust", "ent\u00b7flie\u00b7\u00dfen", "Kla\u00b7ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist des Himmels Zauberkind;", "tokens": ["Sie", "ist", "des", "Him\u00b7mels", "Zau\u00b7ber\u00b7kind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach k\u00f6nnte sie vom Schmerz genesen,", "tokens": ["Ach", "k\u00f6nn\u00b7te", "sie", "vom", "Schmerz", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VMFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und k\u00f6nnt' ich ihre Banden l\u00f6sen,", "tokens": ["Und", "k\u00f6nnt'", "ich", "ih\u00b7re", "Ban\u00b7den", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich eilte zu ihr, schnell und lind.", "tokens": ["Ich", "eil\u00b7te", "zu", "ihr", ",", "schnell", "und", "lind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie w\u00fcrden wir uns selig f\u00fchlen,", "tokens": ["Wie", "w\u00fcr\u00b7den", "wir", "uns", "se\u00b7lig", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es bl\u00fchte aus den Fr\u00fchlingsspielen", "tokens": ["Es", "bl\u00fch\u00b7te", "aus", "den", "Fr\u00fch\u00b7lings\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein himmlisch Leben wohl hervor;", "tokens": ["Ein", "himm\u00b7lisch", "Le\u00b7ben", "wohl", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Geistes Arm wollt' ich sie fassen,", "tokens": ["Mit", "Geis\u00b7tes", "Arm", "wollt'", "ich", "sie", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die zarte Seele nie mehr lassen,", "tokens": ["Die", "zar\u00b7te", "See\u00b7le", "nie", "mehr", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und risse sie zu Gott empor.", "tokens": ["Und", "ris\u00b7se", "sie", "zu", "Gott", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dort s\u00e4nken wir am Throne nieder,", "tokens": ["Dort", "s\u00e4n\u00b7ken", "wir", "am", "Thro\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und s\u00e4ngen dankbar Liebeslieder,", "tokens": ["Und", "s\u00e4n\u00b7gen", "dank\u00b7bar", "Lie\u00b7bes\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und blieben ewig dann bei Gott;", "tokens": ["Und", "blie\u00b7ben", "e\u00b7wig", "dann", "bei", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da endet alles Erdenweinen,", "tokens": ["Da", "en\u00b7det", "al\u00b7les", "Er\u00b7den\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kann uns das Leben nicht vereinen,", "tokens": ["Kann", "uns", "das", "Le\u00b7ben", "nicht", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verm\u00e4hl' uns du, o s\u00fc\u00dfer Tod!", "tokens": ["Ver\u00b7m\u00e4hl'", "uns", "du", ",", "o", "s\u00fc\u00b7\u00dfer", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und soll ich denn bei ihr nicht weilen,", "tokens": ["Und", "soll", "ich", "denn", "bei", "ihr", "nicht", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00df mich durch die Welten eilen,", "tokens": ["So", "la\u00df", "mich", "durch", "die", "Wel\u00b7ten", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sende deinen Diener fort;", "tokens": ["Und", "sen\u00b7de", "dei\u00b7nen", "Die\u00b7ner", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So weit den Geist die Fl\u00fcgel tragen,", "tokens": ["So", "weit", "den", "Geist", "die", "Fl\u00fc\u00b7gel", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So weit der Liebe Wellen schlagen,", "tokens": ["So", "weit", "der", "Lie\u00b7be", "Wel\u00b7len", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein treuer Bote deinem Wort.", "tokens": ["Ein", "treu\u00b7er", "Bo\u00b7te", "dei\u00b7nem", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihr Geister aber, euch befehle", "tokens": ["Ihr", "Geis\u00b7ter", "a\u00b7ber", ",", "euch", "be\u00b7feh\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Obhut ich die Engelseele", "tokens": ["Zur", "Ob\u00b7hut", "ich", "die", "En\u00b7gel\u00b7see\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da\u00df ihr die Schwester wohl bewacht;", "tokens": ["Da\u00df", "ihr", "die", "Schwes\u00b7ter", "wohl", "be\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eilt zu ihr hin auf jede Klage,", "tokens": ["Eilt", "zu", "ihr", "hin", "auf", "je\u00b7de", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umschwebet ihre fl\u00fccht'gen Tage,", "tokens": ["Um\u00b7schwe\u00b7bet", "ih\u00b7re", "fl\u00fccht'\u00b7gen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gie\u00dft Licht in ihrer Leiden Nacht.", "tokens": ["Gie\u00dft", "Licht", "in", "ih\u00b7rer", "Lei\u00b7den", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}