{"textgrid.poem.61867": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweig, wenn dir vom \u00dcberflusse", "tokens": ["Schweig", ",", "wenn", "dir", "vom", "\u00dc\u00b7berf\u00b7lus\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nend nicht die Seele schwoll!", "tokens": ["T\u00f6\u00b7nend", "nicht", "die", "See\u00b7le", "schwoll", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht an jedem Tag zum Schusse", "tokens": ["Nicht", "an", "je\u00b7dem", "Tag", "zum", "Schus\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seinen Bogen spannt Apoll.", "tokens": ["Sei\u00b7nen", "Bo\u00b7gen", "spannt", "A\u00b7poll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Keinen wahrlich darf's verdrie\u00dfen,", "tokens": ["Kei\u00b7nen", "wahr\u00b7lich", "dar\u00b7f's", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADV", "PAV", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da\u00df zu tieferm Ernst geweiht,", "tokens": ["Da\u00df", "zu", "tie\u00b7ferm", "Ernst", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seltner dir die Weisen flie\u00dfen", "tokens": ["Selt\u00b7ner", "dir", "die", "Wei\u00b7sen", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als in muntrer Jugendzeit.", "tokens": ["Als", "in", "mun\u00b7trer", "Ju\u00b7gend\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch mit Fug wird dir's ver\u00fcbelt,", "tokens": ["Doch", "mit", "Fug", "wird", "dir's", "ver\u00b7\u00fc\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wenn du Form und Reim erzwingst,", "tokens": ["Wenn", "du", "Form", "und", "Reim", "er\u00b7zwingst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, was frostig ausgegr\u00fcbelt,", "tokens": ["Und", ",", "was", "fros\u00b7tig", "aus\u00b7ge\u00b7gr\u00fc\u00b7belt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als begeistert Lied uns bringst.", "tokens": ["Als", "be\u00b7geis\u00b7tert", "Lied", "uns", "bringst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}