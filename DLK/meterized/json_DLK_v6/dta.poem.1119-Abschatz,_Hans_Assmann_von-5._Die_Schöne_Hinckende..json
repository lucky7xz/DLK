{"dta.poem.1119": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "5. Die Sch\u00f6ne Hinckende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df dieses Wunder-Bild/ der Abgott vieler Hertzen/ ", "tokens": ["Mu\u00df", "die\u00b7ses", "Wun\u00b7der\u00b7Bild", "/", "der", "Ab\u00b7gott", "vie\u00b7ler", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff ungewissem Grund ungleicher Pfeiler stehn?", "tokens": ["Auff", "un\u00b7ge\u00b7wis\u00b7sem", "Grund", "un\u00b7glei\u00b7cher", "Pfei\u00b7ler", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was Pracht und Witz erhebt/ macht Demuth noch so sch\u00f6n/", "tokens": ["Was", "Pracht", "und", "Witz", "er\u00b7hebt", "/", "macht", "De\u00b7muth", "noch", "so", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$(", "VVFIN", "NN", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was will uns denn an ihr derselben Bildni\u00df schmertzen?", "tokens": ["Was", "will", "uns", "denn", "an", "ihr", "der\u00b7sel\u00b7ben", "Bild\u00b7ni\u00df", "schmert\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Pflegt nicht die K\u00f6nigin der g\u00f6ldnen Himmels-Kertzen", "tokens": ["Pflegt", "nicht", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "g\u00f6ld\u00b7nen", "Him\u00b7mels\u00b7Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch wechsels-weise sich zu sencken/ zu erh\u00f6hn?", "tokens": ["Auch", "wech\u00b7sels\u00b7wei\u00b7se", "sich", "zu", "sen\u00b7cken", "/", "zu", "er\u00b7h\u00f6hn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der sch\u00f6nen Venus Wirth/ Vulcan/ mu\u00df hinckend gehn:", "tokens": ["Der", "sch\u00f6\u00b7nen", "Ve\u00b7nus", "Wirth", "/", "Vul\u00b7can", "/", "mu\u00df", "hin\u00b7ckend", "gehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "NE", "$(", "VMFIN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man sieht sein sch\u00f6nes Feur mit falschen Springen schertzen.", "tokens": ["Man", "sieht", "sein", "sch\u00f6\u00b7nes", "Feur", "mit", "fal\u00b7schen", "Sprin\u00b7gen", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vielleicht wird sie dadurch/ Verliebte/ minder k\u00fchn;", "tokens": ["Viel\u00b7leicht", "wird", "sie", "da\u00b7durch", "/", "Ver\u00b7lieb\u00b7te", "/", "min\u00b7der", "k\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "$(", "NN", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und w\u00e4r auch dieses nicht/ so hilfft doch solcher Mangel/", "tokens": ["Und", "w\u00e4r", "auch", "die\u00b7ses", "nicht", "/", "so", "hilfft", "doch", "sol\u00b7cher", "Man\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PDS", "PTKNEG", "$(", "ADV", "VVFIN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df sie euch nicht so bald kan aus den Augen fliehn.", "tokens": ["Da\u00df", "sie", "euch", "nicht", "so", "bald", "kan", "aus", "den", "Au\u00b7gen", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "ADV", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das kleinste Theil der Welt sieht die zwey Himmels-Angel", "tokens": ["Das", "kleins\u00b7te", "Theil", "der", "Welt", "sieht", "die", "zwey", "Him\u00b7mels\u00b7An\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In gleichem Stande ruhn; ie mehr sich einer neigt/", "tokens": ["In", "glei\u00b7chem", "Stan\u00b7de", "ruhn", ";", "ie", "mehr", "sich", "ei\u00b7ner", "neigt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "ADV", "ADV", "PRF", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Jemehr sein Gegentheil dort in die H\u00f6he steigt.", "tokens": ["Je\u00b7mehr", "sein", "Ge\u00b7gen\u00b7theil", "dort", "in", "die", "H\u00f6\u00b7he", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}