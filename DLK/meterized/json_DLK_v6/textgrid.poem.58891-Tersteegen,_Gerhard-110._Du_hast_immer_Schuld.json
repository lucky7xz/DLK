{"textgrid.poem.58891": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "110. Du hast immer Schuld", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gibt man dich eines Fehlers schuld", "tokens": ["Gibt", "man", "dich", "ei\u00b7nes", "Feh\u00b7lers", "schuld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PRF", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Unrecht und du wirst gest\u00f6ret,", "tokens": ["Mit", "Un\u00b7recht", "und", "du", "wirst", "ge\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So fehlt dir Demut und Geduld;", "tokens": ["So", "fehlt", "dir", "De\u00b7mut", "und", "Ge\u00b7duld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Christ, der will nicht sein geehret.", "tokens": ["Ein", "Christ", ",", "der", "will", "nicht", "sein", "ge\u00b7eh\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VMFIN", "PTKNEG", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}