{"dta.poem.10425": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben mich gequ\u00e4let,", "tokens": ["Sie", "ha\u00b7ben", "mich", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ge\u00e4rgert blau und bla\u00df,", "tokens": ["Ge\u00b7\u00e4r\u00b7gert", "blau", "und", "bla\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Einen mit ihrer Liebe,", "tokens": ["Die", "Ei\u00b7nen", "mit", "ih\u00b7rer", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Andern mit ihrem Ha\u00df.", "tokens": ["Die", "An\u00b7dern", "mit", "ih\u00b7rem", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie haben das Brod mir vergiftet,", "tokens": ["Sie", "ha\u00b7ben", "das", "Brod", "mir", "ver\u00b7gif\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie gossen mir Gift in's Glas,", "tokens": ["Sie", "gos\u00b7sen", "mir", "Gift", "in's", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Einen mit ihrer Liebe,", "tokens": ["Die", "Ei\u00b7nen", "mit", "ih\u00b7rer", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Andern mit ihrem Ha\u00df.", "tokens": ["Die", "An\u00b7dern", "mit", "ih\u00b7rem", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch die mich am meisten gequ\u00e4let,", "tokens": ["Doch", "die", "mich", "am", "meis\u00b7ten", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PRF", "PTKA", "PIS", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ge\u00e4rgert und betr\u00fcbt,", "tokens": ["Ge\u00b7\u00e4r\u00b7gert", "und", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hat mich nie gehasset,", "tokens": ["Die", "hat", "mich", "nie", "ge\u00b7has\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hat mich nie geliebt.", "tokens": ["Und", "hat", "mich", "nie", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}