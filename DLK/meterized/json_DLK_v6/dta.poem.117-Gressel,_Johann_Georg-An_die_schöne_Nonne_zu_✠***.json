{"dta.poem.117": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die sch\u00f6ne Nonne zu \u2720***", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.56", "nl:0.43"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Den Himmel kan man wol hier unbarmhertzig nennen/", "tokens": ["Den", "Him\u00b7mel", "kan", "man", "wol", "hier", "un\u00b7barm\u00b7hert\u00b7zig", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er die Sch\u00f6nheit selbst ins Closter hat versteckt/", "tokens": ["Weil", "er", "die", "Sch\u00f6n\u00b7heit", "selbst", "ins", "Clos\u00b7ter", "hat", "ver\u00b7steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihren Anmuths-Glantz mit Stein und Kalck", "tokens": ["Und", "ih\u00b7ren", "An\u00b7muths\u00b7Glantz", "mit", "Stein", "und", "Kalck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "bedeckt/", "tokens": ["be\u00b7deckt", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Da\u00df diesen ", "tokens": ["Da\u00df", "die\u00b7sen"], "token_info": ["word", "word"], "pos": ["KOUS", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Doch in den ", "tokens": ["Doch", "in", "den"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Ein jeder Pfaffe w\u00fcnscht dis liebliche ", "tokens": ["Ein", "je\u00b7der", "Pfaf\u00b7fe", "w\u00fcnscht", "dis", "lieb\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PDS", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Doch nach den Port umsonst sich Sinn und Sehnsucht streckt", "tokens": ["Doch", "nach", "den", "Port", "um\u00b7sonst", "sich", "Sinn", "und", "Sehn\u00b7sucht", "streckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NE", "VVFIN", "PRF", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn ihre Muschel l\u00e4st sich nicht vom Pfaffen trennen.", "tokens": ["Denn", "ih\u00b7re", "Mu\u00b7schel", "l\u00e4st", "sich", "nicht", "vom", "Pfaf\u00b7fen", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Allein! was n\u00fctzt ein Schatz/ der immer ist verschlossen?", "tokens": ["Al\u00b7lein", "!", "was", "n\u00fctzt", "ein", "Schatz", "/", "der", "im\u00b7mer", "ist", "ver\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "VVFIN", "ART", "NN", "$(", "ART", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nichts! und das Feld verdorrt/ so niemahls wird begossen;", "tokens": ["Nichts", "!", "und", "das", "Feld", "ver\u00b7dorrt", "/", "so", "nie\u00b7mahls", "wird", "be\u00b7gos\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "KON", "ART", "NN", "VVPP", "$(", "ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vollkommen kan man nicht dieselbe Sch\u00f6nheit heissen/", "tokens": ["Voll\u00b7kom\u00b7men", "kan", "man", "nicht", "die\u00b7sel\u00b7be", "Sch\u00f6n\u00b7heit", "heis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "PTKNEG", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Soll nun der Sch\u00f6nheit-Pracht bey dir im Schmucke gleissen?", "tokens": ["Soll", "nun", "der", "Sch\u00f6n\u00b7heit\u00b7Pracht", "bey", "dir", "im", "Schmu\u00b7cke", "gleis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So must du Rosen-Krantz und ich dein ", "tokens": ["So", "must", "du", "Ro\u00b7sen\u00b7Krantz", "und", "ich", "dein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "NN", "KON", "PPER", "PPOSAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}