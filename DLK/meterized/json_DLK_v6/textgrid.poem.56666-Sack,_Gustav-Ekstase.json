{"textgrid.poem.56666": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Ekstase", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh dieser griesgr\u00e4mig graue Himmel!", "tokens": ["Oh", "die\u00b7ser", "gries\u00b7gr\u00e4\u00b7mig", "grau\u00b7e", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "K\u00f6nnt ich mit diesen beiden H\u00e4nden", "tokens": ["K\u00f6nnt", "ich", "mit", "die\u00b7sen", "bei\u00b7den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "zusammenballen die nebele Feuchte,", "tokens": ["zu\u00b7sam\u00b7men\u00b7bal\u00b7len", "die", "ne\u00b7be\u00b7le", "Feuch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und einen Sonnenstrahl drein fesselnd", "tokens": ["und", "ei\u00b7nen", "Son\u00b7nen\u00b7strahl", "drein", "fes\u00b7selnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "zu einer Perle pressen dies Gemisch,", "tokens": ["zu", "ei\u00b7ner", "Per\u00b7le", "pres\u00b7sen", "dies", "Ge\u00b7misch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "zu einer durchsichtigen goldblauen Perle!", "tokens": ["zu", "ei\u00b7ner", "durch\u00b7sich\u00b7ti\u00b7gen", "gold\u00b7blau\u00b7en", "Per\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--++-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Oh dieser m\u00fcrrisch pfeifende n\u00f6rgelnde Wind!", "tokens": ["Oh", "die\u00b7ser", "m\u00fcr\u00b7risch", "pfei\u00b7fen\u00b7de", "n\u00f6r\u00b7geln\u00b7de", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDAT", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "K\u00f6nnt ich mit diesen beiden Armen", "tokens": ["K\u00f6nnt", "ich", "mit", "die\u00b7sen", "bei\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "zusammenschlingen den ruhlosen N\u00f6rgler,", "tokens": ["zu\u00b7sam\u00b7men\u00b7schlin\u00b7gen", "den", "ruh\u00b7lo\u00b7sen", "N\u00f6rg\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ein blankes Bekenntnis ihm einhauchend", "tokens": ["ein", "blan\u00b7kes", "Be\u00b7kennt\u00b7nis", "ihm", "ein\u00b7hau\u00b7chend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "ihn dann loslassen, eine fegende Erkl\u00e4rung,", "tokens": ["ihn", "dann", "los\u00b7las\u00b7sen", ",", "ei\u00b7ne", "fe\u00b7gen\u00b7de", "Er\u00b7kl\u00e4\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wolkenzerrei\u00dfend, l\u00e4nderdurcht\u00f6nend!", "tokens": ["wol\u00b7ken\u00b7zer\u00b7rei\u00b7\u00dfend", ",", "l\u00e4n\u00b7der\u00b7durch\u00b7t\u00f6\u00b7nend", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}