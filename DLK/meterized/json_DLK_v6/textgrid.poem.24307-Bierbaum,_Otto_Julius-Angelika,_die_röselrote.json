{"textgrid.poem.24307": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Angelika, die r\u00f6selrote", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Angelika, die r\u00f6selrote, h\u00e4ngt", "tokens": ["An\u00b7ge\u00b7li\u00b7ka", ",", "die", "r\u00f6\u00b7sel\u00b7ro\u00b7te", ",", "h\u00e4ngt"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PRELS", "VVFIN", "$,", "VVFIN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf dunklem Efeu ruhend \u00fcber die Terrasse", "tokens": ["Auf", "dunk\u00b7lem", "E\u00b7feu", "ru\u00b7hend", "\u00fc\u00b7ber", "die", "Ter\u00b7ras\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Verlangend nieder zu dem Rosenbusch", "tokens": ["Ver\u00b7lan\u00b7gend", "nie\u00b7der", "zu", "dem", "Ro\u00b7sen\u00b7busch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "PTKVZ", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinen gelben Bl\u00fcten, die im Winde", "tokens": ["Mit", "sei\u00b7nen", "gel\u00b7ben", "Bl\u00fc\u00b7ten", ",", "die", "im", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Leis auf und nieder gehn, wie z\u00e4rtliche Gedanken", "tokens": ["Leis", "auf", "und", "nie\u00b7der", "gehn", ",", "wie", "z\u00e4rt\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im Herzen eines M\u00e4dchens, das halb tr\u00e4umt,", "tokens": ["Im", "Her\u00b7zen", "ei\u00b7nes", "M\u00e4d\u00b7chens", ",", "das", "halb", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Halb wacht. \u2013 Schwarz, wie ein Trotz aus alter Zeit,", "tokens": ["Halb", "wacht", ".", "\u2013", "Schwarz", ",", "wie", "ein", "Trotz", "aus", "al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "$(", "NE", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "W\u00e4chst die Badia aus dem Silbergrau", "tokens": ["W\u00e4chst", "die", "Ba\u00b7dia", "aus", "dem", "Sil\u00b7ber\u00b7grau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NE", "APPR", "ART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.9": {"text": "Des sanften \u00d6lbaumh\u00fcgels. Hinter mir", "tokens": ["Des", "sanf\u00b7ten", "\u00d6l\u00b7baum\u00b7h\u00fc\u00b7gels", ".", "Hin\u00b7ter", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Babbelt ein Bettler seinen leeren Spruch", "tokens": ["Bab\u00b7belt", "ein", "Bett\u00b7ler", "sei\u00b7nen", "lee\u00b7ren", "Spruch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.11": {"text": "Vom Paradiese, Jesus, Seligkeit", "tokens": ["Vom", "Pa\u00b7ra\u00b7die\u00b7se", ",", "Je\u00b7sus", ",", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "NE", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und h\u00e4lt den alten Hut mir zitternd hin:", "tokens": ["Und", "h\u00e4lt", "den", "al\u00b7ten", "Hut", "mir", "zit\u00b7ternd", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein altes Kind, rotnasig: l\u00e4cherlich", "tokens": ["Ein", "al\u00b7tes", "Kind", ",", "rot\u00b7na\u00b7sig", ":", "l\u00e4\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "$.", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und r\u00fchrend. Zehn Centesimi erh\u00f6hn", "tokens": ["Und", "r\u00fch\u00b7rend", ".", "Zehn", "Cen\u00b7te\u00b7si\u00b7mi", "er\u00b7h\u00f6hn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "CARD", "NN", "VVINF"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Ihm seine Lebensfreude sichtbarlich. \u2013", "tokens": ["Ihm", "sei\u00b7ne", "Le\u00b7bens\u00b7freu\u00b7de", "sicht\u00b7bar\u00b7lich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Sonne brennt. Fr\u00e4ulein Angelika", "tokens": ["Die", "Son\u00b7ne", "brennt", ".", "Fr\u00e4u\u00b7lein", "An\u00b7ge\u00b7li\u00b7ka"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "NE"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Sehnt sich noch immer nach den roten Rosen.", "tokens": ["Sehnt", "sich", "noch", "im\u00b7mer", "nach", "den", "ro\u00b7ten", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Zwei Lodenr\u00f6cke s\u00e4chseln mir vorbei.", "tokens": ["Zwei", "Lo\u00b7den\u00b7r\u00f6\u00b7cke", "s\u00e4ch\u00b7seln", "mir", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier ist gut ausruhn. Hier vergi\u00dft sich schnell,", "tokens": ["Hier", "ist", "gut", "aus\u00b7ruhn", ".", "Hier", "ver\u00b7gi\u00dft", "sich", "schnell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VVINF", "$.", "ADV", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was, ach, im Norden \u00fcberl\u00e4stig wird", "tokens": ["Was", ",", "ach", ",", "im", "Nor\u00b7den", "\u00fc\u00b7berl\u00b7\u00e4s\u00b7tig", "wird"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "$,", "ITJ", "$,", "APPRART", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und klettenklammrig lange kleben bleibt:", "tokens": ["Und", "klet\u00b7ten\u00b7klamm\u00b7rig", "lan\u00b7ge", "kle\u00b7ben", "bleibt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Geist der Schwere. Satanas, der Sorgen", "tokens": ["Der", "Geist", "der", "Schwe\u00b7re", ".", "Sa\u00b7ta\u00b7nas", ",", "der", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schiel\u00e4ugiger K\u00f6nig, mit dem Peitschenstiel", "tokens": ["Schie\u00b7l\u00e4u\u00b7gi\u00b7ger", "K\u00f6\u00b7nig", ",", "mit", "dem", "Peit\u00b7schen\u00b7stiel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "In haariger Faust, entweicht, den Schwanz verklemmt,", "tokens": ["In", "haa\u00b7ri\u00b7ger", "Faust", ",", "ent\u00b7weicht", ",", "den", "Schwanz", "ver\u00b7klemmt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und wird in San Domenico zum Vetturino,", "tokens": ["Und", "wird", "in", "San", "Do\u00b7me\u00b7ni\u00b7co", "zum", "Vet\u00b7tu\u00b7ri\u00b7no", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NE", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der dich: \u00bbSignor, vuole? Due Lire", "tokens": ["Der", "dich", ":", "\u00bb", "Sig\u00b7nor", ",", "vu\u00b7o\u00b7le", "?", "Due", "Li\u00b7re"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "$.", "$(", "NE", "$,", "VVFIN", "$.", "NE", "NE"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Fin al Firenze!\u00ab blo\u00df ein bi\u00dfchen langweilt.", "tokens": ["Fin", "al", "Fi\u00b7ren\u00b7ze", "!", "\u00ab", "blo\u00df", "ein", "bi\u00df\u00b7chen", "lang\u00b7weilt", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$.", "$(", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}