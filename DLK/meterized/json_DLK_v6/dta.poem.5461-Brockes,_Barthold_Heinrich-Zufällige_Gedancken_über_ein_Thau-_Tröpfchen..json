{"dta.poem.5461": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuf\u00e4llige Gedancken \u00fcber ein Thau-  \n Tr\u00f6pfchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich, nach verschwundner Nacht,", "tokens": ["Wie", "ich", ",", "nach", "ver\u00b7schwund\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00fcngst, im angestrahlten Thau,", "tokens": ["J\u00fcngst", ",", "im", "an\u00b7ge\u00b7strahl\u00b7ten", "Thau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Tropfen Meng\u2019 und Pracht", "tokens": ["In", "der", "Trop\u00b7fen", "Meng'", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Tausend Sonnen-Spiegel schau;", "tokens": ["Tau\u00b7send", "Son\u00b7nen\u00b7Spie\u00b7gel", "schau", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zieht, vor andern, Blick und Sinn", "tokens": ["Zieht", ",", "vor", "an\u00b7dern", ",", "Blick", "und", "Sinn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PIS", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ein vor andern helles Tr\u00f6pfgen, durch sein Funckeln, zu sich", "tokens": ["Ein", "vor", "an\u00b7dern", "hel\u00b7les", "Tr\u00f6pf\u00b7gen", ",", "durch", "sein", "Fun\u00b7ckeln", ",", "zu", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "APPR", "ADJA", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "PRF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "hin;", "tokens": ["hin", ";"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Da ich denn, mit Lust erf\u00fcllt,", "tokens": ["Da", "ich", "denn", ",", "mit", "Lust", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Nicht nur ein klein Sonnen-Bild,", "tokens": ["Nicht", "nur", "ein", "klein", "Son\u00b7nen\u00b7Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Auf der R\u00fcndung \u00e4ussern H\u00f6he,", "tokens": ["Auf", "der", "R\u00fcn\u00b7dung", "\u00e4us\u00b7sern", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Als ein blitzend Lichtgen, sehe;", "tokens": ["Als", "ein", "blit\u00b7zend", "Licht\u00b7gen", ",", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Sondern, da der Thau so klar,", "tokens": ["Son\u00b7dern", ",", "da", "der", "Thau", "so", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie die reinesten Cristallen;", "tokens": ["Wie", "die", "rei\u00b7nes\u00b7ten", "Cris\u00b7tal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Seh ich dieses Lichtgen gar", "tokens": ["Seh", "ich", "die\u00b7ses", "Licht\u00b7gen", "gar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Durch des Tr\u00f6pfchens C\u00f6rper fallen", "tokens": ["Durch", "des", "Tr\u00f6pf\u00b7chens", "C\u00f6r\u00b7per", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Auf ein nah gewachsnes Blat,", "tokens": ["Auf", "ein", "nah", "ge\u00b7wachs\u00b7nes", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Wo es denn verl\u00e4ngt, gespitzt,", "tokens": ["Wo", "es", "denn", "ver\u00b7l\u00e4ngt", ",", "ge\u00b7spitzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Die Figur von einem Strahle, der in langem Strich-", "tokens": ["Die", "Fi\u00b7gur", "von", "ei\u00b7nem", "Strah\u00b7le", ",", "der", "in", "lan\u00b7gem", "Strich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "TRUNC"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "blitzt,", "tokens": ["blitzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Durch das Blat gedr\u00fcckt, erh\u00e4lt. Wie ich solchen nun", "tokens": ["Durch", "das", "Blat", "ge\u00b7dr\u00fcckt", ",", "er\u00b7h\u00e4lt", ".", "Wie", "ich", "sol\u00b7chen", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "VVFIN", "$.", "PWAV", "PPER", "PIAT", "ADV"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "betrachte,", "tokens": ["be\u00b7trach\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.22": {"text": "Und so wol auf seine L\u00e4ng\u2019, als den runden Ursprung, achte;", "tokens": ["Und", "so", "wol", "auf", "sei\u00b7ne", "L\u00e4ng'", ",", "als", "den", "run\u00b7den", "Ur\u00b7sprung", ",", "ach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "F\u00e4llt von ungefehr mir bey:", "tokens": ["F\u00e4llt", "von", "un\u00b7ge\u00b7fehr", "mir", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Ob die\u00df nicht vielleicht ein Bild strahlender Cometen sey?", "tokens": ["Ob", "die\u00df", "nicht", "viel\u00b7leicht", "ein", "Bild", "strah\u00b7len\u00b7der", "Co\u00b7me\u00b7ten", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PTKNEG", "ADV", "ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "----+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Wie! gedacht\u2019 ich, wenn der C\u00f6rper der Cometen bey der", "tokens": ["Wie", "!", "ge\u00b7dacht'", "ich", ",", "wenn", "der", "C\u00f6r\u00b7per", "der", "Co\u00b7me\u00b7ten", "bey", "der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.26": {"text": "R\u00fcnde,", "tokens": ["R\u00fcn\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "(so wie ich hier in dem Tropfen, welcher gantz durchsichtig,", "tokens": ["(", "so", "wie", "ich", "hier", "in", "dem", "Trop\u00b7fen", ",", "wel\u00b7cher", "gantz", "durch\u00b7sich\u00b7tig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "PPER", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.28": {"text": "finde)", "tokens": ["fin\u00b7de", ")"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}}}}}