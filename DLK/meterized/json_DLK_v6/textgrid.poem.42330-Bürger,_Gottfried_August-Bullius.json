{"textgrid.poem.42330": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Bullius", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zwischen manchem wilden Haufen", "tokens": ["Was", "zwi\u00b7schen", "man\u00b7chem", "wil\u00b7den", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Bullius, der Adlermann,", "tokens": ["Sich", "Bul\u00b7li\u00b7us", ",", "der", "Ad\u00b7ler\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An H\u00f6rnern endlich abgelaufen,", "tokens": ["An", "H\u00f6r\u00b7nern", "end\u00b7lich", "ab\u00b7ge\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das l\u00e4uft sein Weib ihm wieder an,", "tokens": ["Das", "l\u00e4uft", "sein", "Weib", "ihm", "wie\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}