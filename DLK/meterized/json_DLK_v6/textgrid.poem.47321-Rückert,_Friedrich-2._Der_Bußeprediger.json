{"textgrid.poem.47321": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2. Der Bu\u00dfeprediger", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich nach Gewohnheit sa\u00df in der Schenke neulich,", "tokens": ["Als", "ich", "nach", "Ge\u00b7wohn\u00b7heit", "sa\u00df", "in", "der", "Schen\u00b7ke", "neu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir zu machen Erdennot durch das Glas erfreulich;", "tokens": ["Mir", "zu", "ma\u00b7chen", "Er\u00b7den\u00b7not", "durch", "das", "Glas", "er\u00b7freu\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "NN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Kam ein Bu\u00dfeprediger mit bestaubtem Kragen", "tokens": ["Kam", "ein", "Bu\u00b7\u00df\u00b7ep\u00b7re\u00b7di\u00b7ger", "mit", "be\u00b7staub\u00b7tem", "Kra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Und hub an den Wein zu schm\u00e4hn, weit- und l\u00e4sterm\u00e4ulig.", "tokens": ["Und", "hub", "an", "den", "Wein", "zu", "schm\u00e4hn", ",", "weit", "und", "l\u00e4s\u00b7ter\u00b7m\u00e4u\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "TRUNC", "KON", "ADJD", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Selber sich in heiligen Eifer redend, malt' er", "tokens": ["Sel\u00b7ber", "sich", "in", "hei\u00b7li\u00b7gen", "Ei\u00b7fer", "re\u00b7dend", ",", "malt'", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PPER"], "meter": "+-+-+--++--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Den verdammten Freund mir mit Farben ganz abscheulich;", "tokens": ["Den", "ver\u00b7damm\u00b7ten", "Freund", "mir", "mit", "Far\u00b7ben", "ganz", "ab\u00b7scheu\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+--+--++-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "H\u00e4tt' ich ihm geglaubt, so war in dem H\u00f6llenrachen", "tokens": ["H\u00e4tt'", "ich", "ihm", "ge\u00b7glaubt", ",", "so", "war", "in", "dem", "H\u00f6l\u00b7len\u00b7ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "$,", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Von den Drachen keiner so ganz entsetzlich greulich.", "tokens": ["Von", "den", "Dra\u00b7chen", "kei\u00b7ner", "so", "ganz", "ent\u00b7setz\u00b7lich", "greu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Und so tobt er weiter, bis sein Gesicht in Flammen", "tokens": ["Und", "so", "tobt", "er", "wei\u00b7ter", ",", "bis", "sein", "Ge\u00b7sicht", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Selber gl\u00fcht, ein H\u00f6llenschlund, r\u00f6tlich, tr\u00fcb' und bl\u00e4ulich.", "tokens": ["Sel\u00b7ber", "gl\u00fcht", ",", "ein", "H\u00f6l\u00b7len\u00b7schlund", ",", "r\u00f6t\u00b7lich", ",", "tr\u00fcb'", "und", "bl\u00e4u\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "$,", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Meinem Schenken winkt' ich, der ihm ein Glas kredenzte", "tokens": ["Mei\u00b7nem", "Schen\u00b7ken", "winkt'", "ich", ",", "der", "ihm", "ein", "Glas", "kre\u00b7denz\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Und mit schelm'schen Blicken es unterst\u00fctzte treulich.", "tokens": ["Und", "mit", "schel\u00b7m'\u00b7schen", "Bli\u00b7cken", "es", "un\u00b7ter\u00b7st\u00fctz\u00b7te", "treu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Erstlich str\u00e4ubte sich der Held, sprach den Fluch und Segen;", "tokens": ["Erst\u00b7lich", "str\u00e4ub\u00b7te", "sich", "der", "Held", ",", "sprach", "den", "Fluch", "und", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN", "$,", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Endlich nahm er's an den Mund, schl\u00fcrfte leckerm\u00e4ulig.", "tokens": ["End\u00b7lich", "nahm", "er's", "an", "den", "Mund", ",", "schl\u00fcrf\u00b7te", "le\u00b7cker\u00b7m\u00e4u\u00b7lig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Mildere Beredsamkeit drauf entflo\u00df den Lippen,", "tokens": ["Mil\u00b7de\u00b7re", "Be\u00b7red\u00b7sam\u00b7keit", "drauf", "ent\u00b7flo\u00df", "den", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Paradiesisch lustentz\u00fcckt, himmlisch morgent\u00e4ulich.", "tokens": ["Pa\u00b7ra\u00b7die\u00b7sisch", "lus\u00b7tent\u00b7z\u00fcckt", ",", "himm\u00b7lisch", "mor\u00b7gen\u00b7t\u00e4u\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit dem Schenken tanzt' er um, sang das Lob des Weines,", "tokens": ["Mit", "dem", "Schen\u00b7ken", "tanzt'", "er", "um", ",", "sang", "das", "Lob", "des", "Wei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Und den alten Schm\u00e4hgesang widerrief er reulich.", "tokens": ["Und", "den", "al\u00b7ten", "Schm\u00e4h\u00b7ge\u00b7sang", "wi\u00b7der\u00b7rief", "er", "reu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "\u00bbo Hafis!\u00ab sprach er zu mir, \u00bbWein ist Seelenwollust,", "tokens": ["\u00bb", "o", "Ha\u00b7fis", "!", "\u00ab", "sprach", "er", "zu", "mir", ",", "\u00bb", "Wein", "ist", "See\u00b7len\u00b7wol\u00b7lust", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$.", "$(", "VVFIN", "PPER", "APPR", "PPER", "$,", "$(", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie der Himmelsm\u00e4dchen Ku\u00df ewig neu jungfr\u00e4ulich.\u00ab", "tokens": ["Wie", "der", "Him\u00b7mels\u00b7m\u00e4d\u00b7chen", "Ku\u00df", "e\u00b7wig", "neu", "jung\u00b7fr\u00e4u\u00b7lich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "ADJD", "ADJD", "$.", "$("], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}