{"textgrid.poem.62884": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Pommer ward von einem Schusse lahm,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Pommer ward von einem Schusse lahm,", "tokens": ["Ein", "Pom\u00b7mer", "ward", "von", "ei\u00b7nem", "Schus\u00b7se", "lahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der seinem Herrn, den er besch\u00fctzen wollte,", "tokens": ["Der", "sei\u00b7nem", "Herrn", ",", "den", "er", "be\u00b7sch\u00fct\u00b7zen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verr\u00e4therisch das Leben nahm.", "tokens": ["Ver\u00b7r\u00e4\u00b7the\u00b7risch", "das", "Le\u00b7ben", "nahm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unwissend, wie er nun sein Brod gewinnen sollte,", "tokens": ["Un\u00b7wis\u00b7send", ",", "wie", "er", "nun", "sein", "Brod", "ge\u00b7win\u00b7nen", "soll\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kroch er betr\u00fcbt bis in die n\u00e4chste Stadt,", "tokens": ["Kroch", "er", "be\u00b7tr\u00fcbt", "bis", "in", "die", "n\u00e4chs\u00b7te", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "An deren Thor ein Kater zu ihm trat,", "tokens": ["An", "de\u00b7ren", "Thor", "ein", "Ka\u00b7ter", "zu", "ihm", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dem eines Abtes Koch vor wenig Tagen,", "tokens": ["Dem", "ei\u00b7nes", "Ab\u00b7tes", "Koch", "vor", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NE", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weil er ein Rebhuhn stahl, das Bein zerschlagen.", "tokens": ["Weil", "er", "ein", "Reb\u00b7huhn", "stahl", ",", "das", "Bein", "zer\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bedr\u00e4ngte werden gleich bekannt:", "tokens": ["Be\u00b7dr\u00e4ng\u00b7te", "wer\u00b7den", "gleich", "be\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie unterhielten sich von ihren Ungl\u00fccksf\u00e4llen.", "tokens": ["Sie", "un\u00b7ter\u00b7hiel\u00b7ten", "sich", "von", "ih\u00b7ren", "Un\u00b7gl\u00fccks\u00b7f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zuletzt sprach Mauz: Freund, la\u00df uns durch das Land", "tokens": ["Zu\u00b7letzt", "sprach", "Mauz", ":", "Freund", ",", "la\u00df", "uns", "durch", "das", "Land"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$.", "NN", "$,", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Als ein paar treue Spie\u00dfgesellen", "tokens": ["Als", "ein", "paar", "treu\u00b7e", "Spie\u00df\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Hausieren gehn. Der Pommer sagte, nein;", "tokens": ["Hau\u00b7sie\u00b7ren", "gehn", ".", "Der", "Pom\u00b7mer", "sag\u00b7te", ",", "nein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ART", "NN", "VVFIN", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Wir sind zwar beyde lahm; allein", "tokens": ["Wir", "sind", "zwar", "bey\u00b7de", "lahm", ";", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PTKVZ", "$.", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich m\u00f6chte doch nicht gern mit dir verglichen werden.", "tokens": ["Ich", "m\u00f6ch\u00b7te", "doch", "nicht", "gern", "mit", "dir", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "War dieses Stolz? \u2013 Nur ihr k\u00f6nnt Richter seyn,", "tokens": ["War", "die\u00b7ses", "Stolz", "?", "\u2013", "Nur", "ihr", "k\u00f6nnt", "Rich\u00b7ter", "seyn", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$.", "$(", "ADV", "PPER", "VVFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ihr zartern Seelen. Kann auf Erden", "tokens": ["Ihr", "zar\u00b7tern", "See\u00b7len", ".", "Kann", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Tugend gr\u00f6\u00dfers Leiden drohn,", "tokens": ["Der", "Tu\u00b7gend", "gr\u00f6\u00b7\u00dfers", "Lei\u00b7den", "drohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Als Pr\u00fcfungen, die ihren Werth erh\u00f6hen,", "tokens": ["Als", "Pr\u00fc\u00b7fun\u00b7gen", ",", "die", "ih\u00b7ren", "Werth", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Mit des verworfnen Lasters Lohn", "tokens": ["Mit", "des", "ver\u00b7worf\u00b7nen", "Las\u00b7ters", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Vor aller Welt vermengt zu sehen?", "tokens": ["Vor", "al\u00b7ler", "Welt", "ver\u00b7mengt", "zu", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}