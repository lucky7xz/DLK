{"dta.poem.10330": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Es treibt mich hin, es treibt mich her!", "tokens": ["Es", "treibt", "mich", "hin", ",", "es", "treibt", "mich", "her", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach wenigen Stunden dann soll ich sie schauen", "tokens": ["Nach", "we\u00b7ni\u00b7gen", "Stun\u00b7den", "dann", "soll", "ich", "sie", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie selber die Sch\u00f6nste der sch\u00f6nen Jungfrauen;", "tokens": ["Sie", "sel\u00b7ber", "die", "Sch\u00f6ns\u00b7te", "der", "sch\u00f6\u00b7nen", "Jung\u00b7frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du treues Herz, was pochst du schwer!", "tokens": ["Du", "treu\u00b7es", "Herz", ",", "was", "pochst", "du", "schwer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Stunden sind aber ein faules Volk!", "tokens": ["Die", "Stun\u00b7den", "sind", "a\u00b7ber", "ein", "fau\u00b7les", "Volk", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schleppen sich behaglich tr\u00e4ge,", "tokens": ["Schlep\u00b7pen", "sich", "be\u00b7hag\u00b7lich", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleichen g\u00e4hnend ihre Wege;", "tokens": ["Schlei\u00b7chen", "g\u00e4h\u00b7nend", "ih\u00b7re", "We\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tummle dich, du faules Volk!", "tokens": ["Tumm\u00b7le", "dich", ",", "du", "fau\u00b7les", "Volk", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tobende Eile mich treibend erfa\u00df't!", "tokens": ["To\u00b7ben\u00b7de", "Ei\u00b7le", "mich", "trei\u00b7bend", "er\u00b7fa\u00df't", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Aber wohl niemals liebten die Horen;", "tokens": ["A\u00b7ber", "wohl", "nie\u00b7mals", "lieb\u00b7ten", "die", "Ho\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Heimlich im grausamen Bunde verschworen,", "tokens": ["Heim\u00b7lich", "im", "grau\u00b7sa\u00b7men", "Bun\u00b7de", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Spotten sie t\u00fcckisch der Liebenden Hast.", "tokens": ["Spot\u00b7ten", "sie", "t\u00fc\u00b7ckisch", "der", "Lie\u00b7ben\u00b7den", "Hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}