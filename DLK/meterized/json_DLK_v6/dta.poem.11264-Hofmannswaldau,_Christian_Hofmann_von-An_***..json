{"dta.poem.11264": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An ***.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verstehst du die natur? Wie stellest du denn mir", "tokens": ["Ver\u00b7stehst", "du", "die", "na\u00b7tur", "?", "Wie", "stel\u00b7lest", "du", "denn", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die himmels-k\u00fcgligen bald so, bald anders f\u00fcr?", "tokens": ["Die", "him\u00b7mels\u00b7k\u00fcg\u00b7li\u00b7gen", "bald", "so", ",", "bald", "an\u00b7ders", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "ADV", "ADV", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mich deucht, es heist von dir und deinem vielen stellen,", "tokens": ["Mich", "deucht", ",", "es", "heist", "von", "dir", "und", "dei\u00b7nem", "vie\u00b7len", "stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPER", "KON", "PPOSAT", "PIAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du spielst damit, als wie die kinder mit den b\u00e4llen.", "tokens": ["Du", "spielst", "da\u00b7mit", ",", "als", "wie", "die", "kin\u00b7der", "mit", "den", "b\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "KOKOM", "ART", "NN", "APPR", "ART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}