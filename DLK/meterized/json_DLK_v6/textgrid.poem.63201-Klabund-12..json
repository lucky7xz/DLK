{"textgrid.poem.63201": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Stadt in Schnee und k\u00fchlem Mondlicht liegt.", "tokens": ["Die", "Stadt", "in", "Schnee", "und", "k\u00fch\u00b7lem", "Mond\u00b7licht", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Schlitten schweben und der Nordwind schweift.", "tokens": ["Die", "Schlit\u00b7ten", "schwe\u00b7ben", "und", "der", "Nord\u00b7wind", "schweift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soldaten gehen glitzernd und bereift,", "tokens": ["Sol\u00b7da\u00b7ten", "ge\u00b7hen", "glit\u00b7zernd", "und", "be\u00b7reift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Frauen sind in Pelze eingeschmiegt.", "tokens": ["Und", "Frau\u00b7en", "sind", "in", "Pel\u00b7ze", "ein\u00b7ge\u00b7schmiegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo winkt ein Fasching, dass du dich entlarvst?", "tokens": ["Wo", "winkt", "ein", "Fa\u00b7sching", ",", "dass", "du", "dich", "ent\u00b7larvst", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bewahr dein heisses Herz zu eigener Tat", "tokens": ["Be\u00b7wahr", "dein", "heis\u00b7ses", "Herz", "zu", "ei\u00b7ge\u00b7ner", "Tat"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und hoffe, dass ein holder Fr\u00fchling naht,", "tokens": ["Und", "hof\u00b7fe", ",", "dass", "ein", "hol\u00b7der", "Fr\u00fch\u00b7ling", "naht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo du es wieder allen zeigen darfst ...", "tokens": ["Wo", "du", "es", "wie\u00b7der", "al\u00b7len", "zei\u00b7gen", "darfst", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}