{"textgrid.poem.66436": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Stupor", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Stupor spricht: bei meiner Ehre!", "tokens": ["Herr", "Stu\u00b7por", "spricht", ":", "bei", "mei\u00b7ner", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Deutscher hat Genie!", "tokens": ["Kein", "Deut\u00b7scher", "hat", "Ge\u00b7nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbsie h\u00e4tten recht, Herr Stupor, w\u00e4re", "tokens": ["\u00bb", "sie", "h\u00e4t\u00b7ten", "recht", ",", "Herr", "Stu\u00b7por", ",", "w\u00e4\u00b7re"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "$,", "NN", "NE", "$,", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das ganze Volk so dumm wie Sie.\u00ab", "tokens": ["Das", "gan\u00b7ze", "Volk", "so", "dumm", "wie", "Sie", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "KOKOM", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}