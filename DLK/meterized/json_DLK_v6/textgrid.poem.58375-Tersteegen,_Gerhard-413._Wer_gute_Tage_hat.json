{"textgrid.poem.58375": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "413. Wer gute Tage hat", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer gern in unverr\u00fcckter Still'", "tokens": ["Wer", "gern", "in", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "Still'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und guten Tagen leben will,", "tokens": ["Und", "gu\u00b7ten", "Ta\u00b7gen", "le\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df seine Ruh bei allen F\u00e4llen", "tokens": ["Mu\u00df", "sei\u00b7ne", "Ruh", "bei", "al\u00b7len", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein in Gottes Willen stellen.", "tokens": ["Al\u00b7lein", "in", "Got\u00b7tes", "Wil\u00b7len", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}