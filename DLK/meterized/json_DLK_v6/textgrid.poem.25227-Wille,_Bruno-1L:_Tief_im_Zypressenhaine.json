{"textgrid.poem.25227": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tief im Zypressenhaine", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Zypressenhaine", "tokens": ["Tief", "im", "Zyp\u00b7res\u00b7sen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Fand ich ein Totenhaus.", "tokens": ["Fand", "ich", "ein", "To\u00b7ten\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf eingesunknem Steine", "tokens": ["Auf", "ein\u00b7ge\u00b7sun\u00b7knem", "Stei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lag d\u00fcrr ein Rosenstrau\u00df.", "tokens": ["Lag", "d\u00fcrr", "ein", "Ro\u00b7sen\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es raunten scheu die Zweige:", "tokens": ["Es", "raun\u00b7ten", "scheu", "die", "Zwei\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbhier schlummert eine Maid.", "tokens": ["\u00bb", "hier", "schlum\u00b7mert", "ei\u00b7ne", "Maid", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie starb an Liebe. Neige", "tokens": ["Sie", "starb", "an", "Lie\u00b7be", ".", "Nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich vor dem heilgen Leid!\u00ab", "tokens": ["Dich", "vor", "dem", "heil\u00b7gen", "Leid", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da weint ich vor Erbarmen:", "tokens": ["Da", "weint", "ich", "vor", "Er\u00b7bar\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gibt es kein Avalun,", "tokens": ["Gibt", "es", "kein", "A\u00b7val\u00b7un", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo in geliebten Armen", "tokens": ["Wo", "in", "ge\u00b7lieb\u00b7ten", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Rosen Br\u00e4ute ruhn?", "tokens": ["Auf", "Ro\u00b7sen", "Br\u00e4u\u00b7te", "ruhn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Herz, das im Geloder", "tokens": ["O", "Herz", ",", "das", "im", "Ge\u00b7lo\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Liebe fromm vergl\u00fcht/", "tokens": ["Der", "Lie\u00b7be", "fromm", "ver\u00b7gl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Avalun ist Moder,", "tokens": ["Dein", "A\u00b7val\u00b7un", "ist", "Mo\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo keine Rose bl\u00fcht.", "tokens": ["Wo", "kei\u00b7ne", "Ro\u00b7se", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Tr\u00e4nen, seid dem Staube", "tokens": ["Ihr", "Tr\u00e4\u00b7nen", ",", "seid", "dem", "Stau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der w\u00fcsten Gruft ein Born/", "tokens": ["Der", "w\u00fcs\u00b7ten", "Gruft", "ein", "Born", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht da\u00df eine Laube", "tokens": ["Viel\u00b7leicht", "da\u00df", "ei\u00b7ne", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich w\u00f6lbt von Rosendorn ...", "tokens": ["Sich", "w\u00f6lbt", "von", "Ro\u00b7sen\u00b7dorn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}