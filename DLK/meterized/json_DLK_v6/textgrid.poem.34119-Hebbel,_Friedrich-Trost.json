{"textgrid.poem.34119": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Perlen hast du ges\u00e4't, auf einmal beginnt es zu hageln,", "tokens": ["Per\u00b7len", "hast", "du", "ge\u00b7s\u00e4't", ",", "auf", "ein\u00b7mal", "be\u00b7ginnt", "es", "zu", "ha\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$,", "APPR", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Und man erblickt sie nicht mehr; hoff' auf die Sonne, sie kommt!", "tokens": ["Und", "man", "er\u00b7blickt", "sie", "nicht", "mehr", ";", "hoff'", "auf", "die", "Son\u00b7ne", ",", "sie", "kommt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}