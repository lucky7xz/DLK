["KON", "ADJD", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Seither ist unser Frey in Dienstbarkeit verkehret,", "tokens": ["Sei\u00b7ther", "ist", "un\u00b7ser", "Frey", "in", "Dienst\u00b7bar\u00b7keit", "ver\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Die Haut ist abgestreifft, das Marck ist au\u00dfgezehret.", "tokens": ["Die", "Haut", "ist", "ab\u00b7ge\u00b7streifft", ",", "das", "Marck", "ist", "au\u00df\u00b7ge\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}