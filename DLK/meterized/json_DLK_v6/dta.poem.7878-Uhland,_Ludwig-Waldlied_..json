{"dta.poem.7878": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Waldlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Im Walde geh\u2019 ich wohlgemuth,", "tokens": ["Im", "Wal\u00b7de", "geh'", "ich", "wohl\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir graut vor R\u00e4ubern nicht;", "tokens": ["Mir", "graut", "vor", "R\u00e4u\u00b7bern", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein liebend Herz ist all mein Gut,", "tokens": ["Ein", "lie\u00b7bend", "Herz", "ist", "all", "mein", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sucht kein B\u00f6sewicht.", "tokens": ["Das", "sucht", "kein", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was rauscht, was raschelt durch den Busch?", "tokens": ["Was", "rauscht", ",", "was", "ra\u00b7schelt", "durch", "den", "Busch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ein M\u00f6rder, der mir droht?", "tokens": ["Ein", "M\u00f6r\u00b7der", ",", "der", "mir", "droht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Liebchen kommt gesprungen, husch!", "tokens": ["Mein", "Lieb\u00b7chen", "kommt", "ge\u00b7sprun\u00b7gen", ",", "husch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und herzt mich fast zu Tod.", "tokens": ["Und", "herzt", "mich", "fast", "zu", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}