{"textgrid.poem.42256": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Der gro\u00dfe Mann", "genre": "verse", "period": "N.A.", "pub_year": 1779, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Ding, das mich verdreu\u00dft,", "tokens": ["Es", "ist", "ein", "Ding", ",", "das", "mich", "ver\u00b7dreu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Schwindel oder Schmeichelgeist", "tokens": ["Wenn", "Schwin\u00b7del", "o\u00b7der", "Schmei\u00b7chel\u00b7geist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gemeines Ma\u00df f\u00fcr gro\u00dfes preist.", "tokens": ["Ge\u00b7mei\u00b7nes", "Ma\u00df", "f\u00fcr", "gro\u00b7\u00dfes", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du, Geist der Wahrheit, sag' es an:", "tokens": ["Du", ",", "Geist", "der", "Wahr\u00b7heit", ",", "sag'", "es", "an", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer ist, wer ist ein gro\u00dfer Mann?", "tokens": ["Wer", "ist", ",", "wer", "ist", "ein", "gro\u00b7\u00dfer", "Mann", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ruhmverschwendung Acht und Bann!", "tokens": ["Der", "Ruhm\u00b7ver\u00b7schwen\u00b7dung", "Acht", "und", "Bann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der, dem die Gottheit Sinn beschert,", "tokens": ["Der", ",", "dem", "die", "Got\u00b7theit", "Sinn", "be\u00b7schert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gr\u00f6\u00dfe, Bild, Verhalt und Wert,", "tokens": ["Der", "Gr\u00f6\u00b7\u00dfe", ",", "Bild", ",", "Ver\u00b7halt", "und", "Wert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aller Wesen Kraft ihm lehrt;", "tokens": ["Und", "al\u00b7ler", "We\u00b7sen", "Kraft", "ihm", "lehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des weit umfassender Verstand,", "tokens": ["Des", "weit", "um\u00b7fas\u00b7sen\u00b7der", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie einen Ball mit hohler Hand", "tokens": ["Wie", "ei\u00b7nen", "Ball", "mit", "hoh\u00b7ler", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ganzes Weltsystem umspannt;", "tokens": ["Ein", "gan\u00b7zes", "Welt\u00b7sys\u00b7tem", "um\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Der wei\u00df, was Gro\u00dfes hie und da,", "tokens": ["Der", "wei\u00df", ",", "was", "Gro\u00b7\u00dfes", "hie", "und", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu allen Zeiten, fern und nah,", "tokens": ["Zu", "al\u00b7len", "Zei\u00b7ten", ",", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo, und wann, und wie geschah;", "tokens": ["Und", "wo", ",", "und", "wann", ",", "und", "wie", "ge\u00b7schah", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "KON", "PWAV", "$,", "KON", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Mann, der die Natur vertraut,", "tokens": ["Der", "Mann", ",", "der", "die", "Na\u00b7tur", "ver\u00b7traut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleichwie ein Br\u00e4utigam die Braut,", "tokens": ["Gleich\u00b7wie", "ein", "Br\u00e4u\u00b7ti\u00b7gam", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An allen Reizen nackend schaut;", "tokens": ["An", "al\u00b7len", "Rei\u00b7zen", "na\u00b7ckend", "schaut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und warm an ihres Busens Glut,", "tokens": ["Und", "warm", "an", "ih\u00b7res", "Bu\u00b7sens", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verm\u00f6gen stets und Heldenmut", "tokens": ["Ver\u00b7m\u00f6\u00b7gen", "stets", "und", "Hel\u00b7den\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Lieb' und Leben saugend, ruht;", "tokens": ["Und", "Lieb'", "und", "Le\u00b7ben", "sau\u00b7gend", ",", "ruht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und nun, was je ein Erdenmann", "tokens": ["Und", "nun", ",", "was", "je", "ein", "Er\u00b7den\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Menschenheil gekonnt und kann,", "tokens": ["F\u00fcr", "Men\u00b7schen\u00b7heil", "ge\u00b7konnt", "und", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wofern er will, desgleichen kann;", "tokens": ["Wo\u00b7fern", "er", "will", ",", "des\u00b7glei\u00b7chen", "kann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dabei in seiner Zeit und Welt,", "tokens": ["Da\u00b7bei", "in", "sei\u00b7ner", "Zeit", "und", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sein Beruf ihn hingestellt,", "tokens": ["Wo", "sein", "Be\u00b7ruf", "ihn", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch That der Kunst die Wage h\u00e4lt:", "tokens": ["Durch", "That", "der", "Kunst", "die", "Wa\u00b7ge", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Der ist ein Mann, und der ist gro\u00df!", "tokens": ["Der", "ist", "ein", "Mann", ",", "und", "der", "ist", "gro\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KON", "ART", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ringt sich aus der Menschheit Scho\u00df", "tokens": ["Doch", "ringt", "sich", "aus", "der", "Menschheit", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Jahrhundertelang kaum Einer los.", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te\u00b7lang", "kaum", "Ei\u00b7ner", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}