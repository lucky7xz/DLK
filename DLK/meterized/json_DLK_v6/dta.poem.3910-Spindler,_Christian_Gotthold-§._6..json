{"dta.poem.3910": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 6.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich Verliebte nun gantz in geheim ver-", "tokens": ["Wenn", "sich", "Ver\u00b7lieb\u00b7te", "nun", "gantz", "in", "ge\u00b7heim", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "VVFIN", "ADV", "ADV", "APPR", "ADJD", "TRUNC"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "schreiben,", "tokens": ["schrei\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Einander Lebenslang getreu und hold zu bleiben,", "tokens": ["Ein\u00b7an\u00b7der", "Le\u00b7bens\u00b7lang", "ge\u00b7treu", "und", "hold", "zu", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis auf der Eltern Ja; so ists gar wohl vergunt,", "tokens": ["Bis", "auf", "der", "El\u00b7tern", "Ja", ";", "so", "ists", "gar", "wohl", "ver\u00b7gunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKANT", "$.", "ADV", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur aber weiter nichts: Ein K\u00fc\u00dfgen auf den Mund.", "tokens": ["Nur", "a\u00b7ber", "wei\u00b7ter", "nichts", ":", "Ein", "K\u00fc\u00df\u00b7gen", "auf", "den", "Mund", "."],