{"dta.poem.3059": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "105.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Leichtgl\u00e4ubigkeit ist nicht nur Mangel an Verstand,", "tokens": ["Leicht\u00b7gl\u00e4u\u00b7big\u00b7keit", "ist", "nicht", "nur", "Man\u00b7gel", "an", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch von Einbildungskraft ist sie ein Unterpfand.", "tokens": ["Auch", "von", "Ein\u00b7bil\u00b7dungs\u00b7kraft", "ist", "sie", "ein", "Un\u00b7ter\u00b7pfand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer wenig fa\u00dft, wird schnell Unfa\u00dfliches verneinen;", "tokens": ["Wer", "we\u00b7nig", "fa\u00dft", ",", "wird", "schnell", "Un\u00b7fa\u00df\u00b7li\u00b7ches", "ver\u00b7nei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "VAFIN", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer viel sich denken kann, dem wird viel m\u00f6glich scheinen.", "tokens": ["Wer", "viel", "sich", "den\u00b7ken", "kann", ",", "dem", "wird", "viel", "m\u00f6g\u00b7lich", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PRF", "VVINF", "VMFIN", "$,", "PDS", "VAFIN", "ADV", "ADJD", "V