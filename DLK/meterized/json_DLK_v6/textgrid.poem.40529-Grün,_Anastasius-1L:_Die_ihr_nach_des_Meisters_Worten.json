{"textgrid.poem.40529": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ihr nach des Meisters Worten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr nach des Meisters Worten", "tokens": ["Die", "ihr", "nach", "des", "Meis\u00b7ters", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmelspforten werth zu sein,", "tokens": ["Him\u00b7mel\u00b7spfor\u00b7ten", "werth", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kunstgeformte, ehrne Pforten,", "tokens": ["Kunst\u00b7ge\u00b7form\u00b7te", ",", "ehr\u00b7ne", "Pfor\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft den deutschen Wandrer ein!", "tokens": ["La\u00dft", "den", "deut\u00b7schen", "Wand\u00b7rer", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "D\u00fcstre, dunkle Taufkapelle,", "tokens": ["D\u00fcst\u00b7re", ",", "dunk\u00b7le", "Tauf\u00b7ka\u00b7pel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner heil'gen Nacht entfleu\u00dft", "tokens": ["Dei\u00b7ner", "heil'\u00b7gen", "Nacht", "ent\u00b7fleu\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manch ein Strahl der Himmelshelle,", "tokens": ["Manch", "ein", "Strahl", "der", "Him\u00b7mels\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Senkend sich in meinen Geist.", "tokens": ["Sen\u00b7kend", "sich", "in", "mei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vor mir steht ein greiser Priester,", "tokens": ["Vor", "mir", "steht", "ein", "grei\u00b7ser", "Pries\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Segen betend f\u00fcr ein Kind,", "tokens": ["Se\u00b7gen", "be\u00b7tend", "f\u00fcr", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des heil'gen Bornes gie\u00dft er", "tokens": ["Und", "des", "heil'\u00b7gen", "Bor\u00b7nes", "gie\u00dft", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des T\u00e4uflings Stirne lind.", "tokens": ["Auf", "des", "T\u00e4uf\u00b7lings", "Stir\u00b7ne", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meine H\u00e4nde m\u00f6cht' ich legen", "tokens": ["Mei\u00b7ne", "H\u00e4n\u00b7de", "m\u00f6cht'", "ich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf das Kind, ich fremder Mann,", "tokens": ["Auf", "das", "Kind", ",", "ich", "frem\u00b7der", "Mann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend l\u00e4ngst mein voller Segen", "tokens": ["W\u00e4h\u00b7rend", "l\u00e4ngst", "mein", "vol\u00b7ler", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lind und leis sein Haupt umrann;", "tokens": ["Lind", "und", "leis", "sein", "Haupt", "um\u00b7rann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Segen, der wie Fr\u00fchthaus Fallen", "tokens": ["Se\u00b7gen", ",", "der", "wie", "Fr\u00fch\u00b7thaus", "Fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "KOKOM", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Menschenpfl\u00e4nzchen tr\u00e4nkt", "tokens": ["Die\u00b7ses", "Men\u00b7schen\u00b7pfl\u00e4nz\u00b7chen", "tr\u00e4nkt"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00df und \u00fcberreich mit Allem,", "tokens": ["S\u00fc\u00df", "und", "\u00fc\u00b7berr\u00b7eich", "mit", "Al\u00b7lem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ein Leben Sch\u00f6nes denkt!", "tokens": ["Was", "ein", "Le\u00b7ben", "Sch\u00f6\u00b7nes", "denkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schlie\u00dft euch wieder, Himmelspforten,", "tokens": ["Schlie\u00dft", "euch", "wie\u00b7der", ",", "Him\u00b7mel\u00b7spfor\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn sein Erdenlauf beginnt!", "tokens": ["Denn", "sein", "Er\u00b7den\u00b7lauf", "be\u00b7ginnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandernd fort zu fernen Orten,", "tokens": ["Wan\u00b7dernd", "fort", "zu", "fer\u00b7nen", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' ich nie dich wieder, Kind!", "tokens": ["Seh'", "ich", "nie", "dich", "wie\u00b7der", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Knab' und Mann wirst du in Jahren,", "tokens": ["Knab'", "und", "Mann", "wirst", "du", "in", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungestalt vielleicht und wild;", "tokens": ["Un\u00b7ge\u00b7stalt", "viel\u00b7leicht", "und", "wild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ich werd' es nie erfahren,", "tokens": ["Doch", "ich", "werd'", "es", "nie", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, ich seh' dich sch\u00f6n und mild!", "tokens": ["Ach", ",", "ich", "seh'", "dich", "sch\u00f6n", "und", "mild", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Hunger wird dein Aug' verwildern,", "tokens": ["Hun\u00b7ger", "wird", "dein", "Aug'", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Armut bringt vielleicht dir Qual!", "tokens": ["Ar\u00b7mut", "bringt", "viel\u00b7leicht", "dir", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, in meines Segens Bildern", "tokens": ["Ach", ",", "in", "mei\u00b7nes", "Se\u00b7gens", "Bil\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sitzest du am Freudenmahl!", "tokens": ["Sit\u00b7zest", "du", "am", "Freu\u00b7den\u00b7mahl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Deiner Mutter Pulse stocken,", "tokens": ["Dei\u00b7ner", "Mut\u00b7ter", "Pul\u00b7se", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich verr\u00e4th des Freundes Wort!", "tokens": ["Dich", "ver\u00b7r\u00e4\u00b7th", "des", "Freun\u00b7des", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ach, nicht h\u00f6r' ich jene Glocken,", "tokens": ["Ach", ",", "nicht", "h\u00f6r'", "ich", "je\u00b7ne", "Glo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht h\u00f6r' ich jenes Wort!", "tokens": ["Und", "nicht", "h\u00f6r'", "ich", "je\u00b7nes", "Wort", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und es h\u00f6hnte dich, dir fluchte,", "tokens": ["Und", "es", "h\u00f6hn\u00b7te", "dich", ",", "dir", "fluch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die du einzig liebst, o Graus!", "tokens": ["Die", "du", "ein\u00b7zig", "liebst", ",", "o", "Graus", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, in meinem Sinn doch suchte", "tokens": ["Ach", ",", "in", "mei\u00b7nem", "Sinn", "doch", "such\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich die treu'ste Braut dir aus!", "tokens": ["Ich", "die", "treu'\u00b7ste", "Braut", "dir", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Bot'st dein Herz, gequ\u00e4lt vom Leben,", "tokens": ["Bot'st", "dein", "Herz", ",", "ge\u00b7qu\u00e4lt", "vom", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jung dem eignen Schwerte dar!", "tokens": ["Jung", "dem", "eig\u00b7nen", "Schwer\u00b7te", "dar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach ich hab' dir doch gegeben", "tokens": ["Ach", "ich", "hab'", "dir", "doch", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar so sch\u00f6nes wei\u00dfes Haar!", "tokens": ["Gar", "so", "sch\u00f6\u00b7nes", "wei\u00b7\u00dfes", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "So vielleicht dem Fluch erlegen,", "tokens": ["So", "viel\u00b7leicht", "dem", "Fluch", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der dein Erdenloos gebannt,", "tokens": ["Der", "dein", "Er\u00b7den\u00b7loos", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahnst du's nie, wie einst der Segen", "tokens": ["Ahnst", "du's", "nie", ",", "wie", "einst", "der", "Se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADV", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fromm an deiner Wiege stand;", "tokens": ["Fromm", "an", "dei\u00b7ner", "Wie\u00b7ge", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wie der Mann aus fremder Ferne,", "tokens": ["Wie", "der", "Mann", "aus", "frem\u00b7der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Betend \u00fcber dich gebeugt,", "tokens": ["Be\u00b7tend", "\u00fc\u00b7ber", "dich", "ge\u00b7beugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit des Segens Born dich gerne,", "tokens": ["Mit", "des", "Se\u00b7gens", "Born", "dich", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Junges Pfl\u00e4nzchen, gro\u00dfges\u00e4ugt.", "tokens": ["Jun\u00b7ges", "Pfl\u00e4nz\u00b7chen", ",", "gro\u00df\u00b7ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Bist der sch\u00f6ne Baum mit nichten,", "tokens": ["Bist", "der", "sch\u00f6\u00b7ne", "Baum", "mit", "nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den er freudig ragen hie\u00df!", "tokens": ["Den", "er", "freu\u00b7dig", "ra\u00b7gen", "hie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darbst an Bl\u00fcthen, kargst mit Fr\u00fcchten,", "tokens": ["Darbst", "an", "Bl\u00fc\u00b7then", ",", "kargst", "mit", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die er reich dich tragen lie\u00df!", "tokens": ["Die", "er", "reich", "dich", "tra\u00b7gen", "lie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Doch, verarmt an Bl\u00fcthenschimmer,", "tokens": ["Doch", ",", "ver\u00b7armt", "an", "Bl\u00fc\u00b7then\u00b7schim\u00b7mer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Stamm und Mark verdorrt,", "tokens": ["Und", "in", "Stamm", "und", "Mark", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Bl\u00fchst im Herzen mir noch immer", "tokens": ["Bl\u00fchst", "im", "Her\u00b7zen", "mir", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PPER", "ADV", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Du dein bl\u00fchend Leben fort.", "tokens": ["Du", "dein", "bl\u00fc\u00b7hend", "Le\u00b7ben", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}