{"dta.poem.17994": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "237.  Wie man Gottes Tochter Mutter un\u0304  \n Braut wird.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.57", "nl:0.42"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gotts Tochter Mutter Braut kan jede Seele werden/", "tokens": ["Gotts", "Toch\u00b7ter", "Mut\u00b7ter", "Braut", "kan", "je\u00b7de", "See\u00b7le", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "NN", "VMFIN", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Gott zum Vater Sohn und Br\u00e4utgam nihmt auff", "tokens": ["Die", "Gott", "zum", "Va\u00b7ter", "Sohn", "und", "Br\u00e4ut\u00b7gam", "nihmt", "auff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "NN", "KON", "NE", "VVFIN", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Erden.", "tokens": ["Er\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}