{"textgrid.poem.35777": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Mutter! bleibst so lange,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Mutter! bleibst so lange,", "tokens": ["Ach", "Mut\u00b7ter", "!", "bleibst", "so", "lan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es wird uns Kindern bange,", "tokens": ["Es", "wird", "uns", "Kin\u00b7dern", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Abend ist so kalt;", "tokens": ["Der", "A\u00b7bend", "ist", "so", "kalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Winde schaurig wehen", "tokens": ["Die", "Win\u00b7de", "schau\u00b7rig", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und lange Schatten gehen", "tokens": ["Und", "lan\u00b7ge", "Schat\u00b7ten", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und L\u00f6wen br\u00fcllen durch den Wald.", "tokens": ["Und", "L\u00f6\u00b7wen", "br\u00fcl\u00b7len", "durch", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Weit sind wir heut gegangen", "tokens": ["Weit", "sind", "wir", "heut", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und tragen nun Verlangen", "tokens": ["Und", "tra\u00b7gen", "nun", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach unsrer Mutter Schoo\u00df;", "tokens": ["Nach", "uns\u00b7rer", "Mut\u00b7ter", "Schoo\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Komm, trockne unsre Thr\u00e4nen,", "tokens": ["Komm", ",", "trock\u00b7ne", "uns\u00b7re", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "L\u00f6s' auf dies bange Sehnen,", "tokens": ["L\u00f6s'", "auf", "dies", "ban\u00b7ge", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDS", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mach' unsre m\u00fcden Herzen los.", "tokens": ["Mach'", "uns\u00b7re", "m\u00fc\u00b7den", "Her\u00b7zen", "los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du sagtest uns am Morgen:", "tokens": ["Du", "sag\u00b7test", "uns", "am", "Mor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir sollten ohne Sorgen", "tokens": ["Wir", "soll\u00b7ten", "oh\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von deiner Schwelle gehn;", "tokens": ["Von", "dei\u00b7ner", "Schwel\u00b7le", "gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn wir den Berg erklommen", "tokens": ["Wenn", "wir", "den", "Berg", "er\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die Nacht gekommen,", "tokens": ["Und", "wenn", "die", "Nacht", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dann w\u00fcrden wir dich wiedersehn.", "tokens": ["Dann", "w\u00fcr\u00b7den", "wir", "dich", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir mu\u00dften m\u00fchsam wallen", "tokens": ["Wir", "mu\u00df\u00b7ten", "m\u00fch\u00b7sam", "wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und viele sind gefallen", "tokens": ["Und", "vie\u00b7le", "sind", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mancher ging voran.", "tokens": ["Und", "man\u00b7cher", "ging", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Oft mu\u00dften wir auch weinen;", "tokens": ["Oft", "mu\u00df\u00b7ten", "wir", "auch", "wei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch Dornen und auf Steinen,", "tokens": ["Durch", "Dor\u00b7nen", "und", "auf", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch Hitz' und Sturm ging unsre Bahn.", "tokens": ["Durch", "Hitz'", "und", "Sturm", "ging", "uns\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun geht der Tag zu Ende,", "tokens": ["Nun", "geht", "der", "Tag", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drum heben wir die H\u00e4nde", "tokens": ["Drum", "he\u00b7ben", "wir", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und suchen deine Hand;", "tokens": ["Und", "su\u00b7chen", "dei\u00b7ne", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Thu' auf die traute Zelle!", "tokens": ["Thu'", "auf", "die", "trau\u00b7te", "Zel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sind wieder an der Stelle,", "tokens": ["Sind", "wie\u00b7der", "an", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da du uns hast hinausgesandt.", "tokens": ["Da", "du", "uns", "hast", "hin\u00b7aus\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df uns in gr\u00fcnen Wiegen", "tokens": ["La\u00df", "uns", "in", "gr\u00fc\u00b7nen", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im wei\u00dfen Hemdlein liegen", "tokens": ["Im", "wei\u00b7\u00dfen", "Hemd\u00b7lein", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So tief und still und dicht;", "tokens": ["So", "tief", "und", "still", "und", "dicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df Thr\u00e4nen uns befeuchten,", "tokens": ["La\u00df", "Thr\u00e4\u00b7nen", "uns", "be\u00b7feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df auf uns niederleuchten", "tokens": ["La\u00df", "auf", "uns", "nie\u00b7der\u00b7leuch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein ewig klares Mondgesicht.", "tokens": ["Dein", "e\u00b7wig", "kla\u00b7res", "Mond\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Den Schleier, blau gewoben,", "tokens": ["Den", "Schlei\u00b7er", ",", "blau", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den breite weit aus oben,", "tokens": ["Den", "brei\u00b7te", "weit", "aus", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drin la\u00df uns hoffend ruhn.", "tokens": ["Drin", "la\u00df", "uns", "hof\u00b7fend", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Einst wird es wieder tagen,", "tokens": ["Einst", "wird", "es", "wie\u00b7der", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann wird der Vater sagen:", "tokens": ["Dann", "wird", "der", "Va\u00b7ter", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbsteht auf, ihr Kindlein, alle nun!\u00ab", "tokens": ["\u00bb", "steht", "auf", ",", "ihr", "Kin\u00b7dlein", ",", "al\u00b7le", "nun", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PIS", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}