{"textgrid.poem.48432": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "3. Lob eines Soldaten zu Rosse", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein frischer Heldenmut ist \u00fcber alle Sch\u00e4tze,", "tokens": ["Ein", "fri\u00b7scher", "Hel\u00b7den\u00b7mut", "ist", "\u00fc\u00b7ber", "al\u00b7le", "Sch\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist \u00fcber allen Neid. Er selbst ist sein Gesetze,", "tokens": ["ist", "\u00fc\u00b7ber", "al\u00b7len", "Neid", ".", "Er", "selbst", "ist", "sein", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "$.", "PPER", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "sein Mal, sein Sold, sein Preis. Er rei\u00dfet durch die Zeit,", "tokens": ["sein", "Mal", ",", "sein", "Sold", ",", "sein", "Preis", ".", "Er", "rei\u00b7\u00dfet", "durch", "die", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "vergn\u00fcget sich durch sich, l\u00e4\u00dft bei sich Ruh' und Streit", "tokens": ["ver\u00b7gn\u00fc\u00b7get", "sich", "durch", "sich", ",", "l\u00e4\u00dft", "bei", "sich", "Ruh'", "und", "Streit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PRF", "$,", "VVFIN", "APPR", "PRF", "NN", "KON", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "in gleicher Wage stehn. Den adelichen Rittern", "tokens": ["in", "glei\u00b7cher", "Wa\u00b7ge", "stehn", ".", "Den", "a\u00b7de\u00b7li\u00b7chen", "Rit\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wird dieses angeborn. Wenn Andre stehn und zittern,", "tokens": ["wird", "die\u00b7ses", "an\u00b7ge\u00b7born", ".", "Wenn", "And\u00b7re", "stehn", "und", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$.", "KOUS", "PIS", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "beseufzen ihren Tod und bitten um Quartier,", "tokens": ["be\u00b7seuf\u00b7zen", "ih\u00b7ren", "Tod", "und", "bit\u00b7ten", "um", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "so setzen sie den Ruhm auch tausent Leben f\u00fcr,", "tokens": ["so", "set\u00b7zen", "sie", "den", "Ruhm", "auch", "tau\u00b7sent", "Le\u00b7ben", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "CARD", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die man erbitten mu\u00df, und doch nur Schande geben.", "tokens": ["die", "man", "er\u00b7bit\u00b7ten", "mu\u00df", ",", "und", "doch", "nur", "Schan\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "KON", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie sterben tugendhaft, wenn sie noch k\u00f6nten leben.", "tokens": ["Sie", "ster\u00b7ben", "tu\u00b7gend\u00b7haft", ",", "wenn", "sie", "noch", "k\u00f6n\u00b7ten", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was ist das f\u00fcr ein Feind, der seinem Feinde fleht?", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Feind", ",", "der", "sei\u00b7nem", "Fein\u00b7de", "fleht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist einer so gesinnt, wenn er zum Treffen geht,", "tokens": ["Ist", "ei\u00b7ner", "so", "ge\u00b7sinnt", ",", "wenn", "er", "zum", "Tref\u00b7fen", "geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "VVPP", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "so bleib' er lieber da! Ein redlicher Soldate", "tokens": ["so", "bleib'", "er", "lie\u00b7ber", "da", "!", "Ein", "red\u00b7li\u00b7cher", "Sol\u00b7da\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "darf nicht in Zweifel stehn, ob auch der Sieg gerate.", "tokens": ["darf", "nicht", "in", "Zwei\u00b7fel", "stehn", ",", "ob", "auch", "der", "Sieg", "ge\u00b7ra\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Sieg hat er bei sich, wenn er sich dapfer h\u00e4lt.", "tokens": ["Den", "Sieg", "hat", "er", "bei", "sich", ",", "wenn", "er", "sich", "dap\u00b7fer", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PRF", "$,", "KOUS", "PPER", "PRF", "PAV", "VVFIN", "$."], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.16": {"text": "Was kann ihm helfen wol das \u00fcberwunden Geld?", "tokens": ["Was", "kann", "ihm", "hel\u00b7fen", "wol", "das", "\u00fc\u00b7berw\u00b7un\u00b7den", "Geld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das er zu geben beut, ist doch schon alles seine.", "tokens": ["Das", "er", "zu", "ge\u00b7ben", "beut", ",", "ist", "doch", "schon", "al\u00b7les", "sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "PIS", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er richt' ihn lieber hin, so bleibt sein Eidschwur reine,", "tokens": ["Er", "richt'", "ihn", "lie\u00b7ber", "hin", ",", "so", "bleibt", "sein", "Eid\u00b7schwur", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "und er versichert sich, schl\u00e4gt er in heute tot,", "tokens": ["und", "er", "ver\u00b7si\u00b7chert", "sich", ",", "schl\u00e4gt", "er", "in", "heu\u00b7te", "tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$,", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "so darf er morgen nicht f\u00fcr ihm stehn in der Not,", "tokens": ["so", "darf", "er", "mor\u00b7gen", "nicht", "f\u00fcr", "ihm", "stehn", "in", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "da\u00df er sich r\u00e4chen wird. Wann will der Krieg sich enden,", "tokens": ["da\u00df", "er", "sich", "r\u00e4\u00b7chen", "wird", ".", "Wann", "will", "der", "Krieg", "sich", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VAFIN", "$.", "PWAV", "VMFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "wenn er verschonen will und sich zur Gnade wenden,", "tokens": ["wenn", "er", "ver\u00b7scho\u00b7nen", "will", "und", "sich", "zur", "Gna\u00b7de", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "KON", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "wenn man soll ernsthaft sein? Vor Alters ging' es nicht.", "tokens": ["wenn", "man", "soll", "ernst\u00b7haft", "sein", "?", "Vor", "Al\u00b7ters", "ging'", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ADJD", "VAINF", "$.", "APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Es wurden K\u00f6nige beim Treffen hingericht.", "tokens": ["Es", "wur\u00b7den", "K\u00f6\u00b7ni\u00b7ge", "beim", "Tref\u00b7fen", "hin\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was gilt hier die Person? Ein Feind hat mit dem Namen", "tokens": ["Was", "gilt", "hier", "die", "Per\u00b7son", "?", "Ein", "Feind", "hat", "mit", "dem", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$.", "ART", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "sein Leben schon verb\u00fcrt. So bleibt er stets bei Samen,", "tokens": ["sein", "Le\u00b7ben", "schon", "ver\u00b7b\u00fcrt", ".", "So", "bleibt", "er", "stets", "bei", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$.", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "wenn man ihn ab l\u00e4\u00dft ziehn. Und er gedenkt es doch,", "tokens": ["wenn", "man", "ihn", "ab", "l\u00e4\u00dft", "ziehn", ".", "Und", "er", "ge\u00b7denkt", "es", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKVZ", "VVFIN", "VVINF", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "und pfiff' er einmal noch so auf ein lindes Loch.", "tokens": ["und", "pfiff'", "er", "ein\u00b7mal", "noch", "so", "auf", "ein", "lin\u00b7des", "Loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es lehrt ihn di\u00df die Not. Wenn er zur Freiheit k\u00f6mmet,", "tokens": ["Es", "lehrt", "ihn", "di\u00df", "die", "Not", ".", "Wenn", "er", "zur", "Frei\u00b7heit", "k\u00f6m\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "ART", "NN", "$.", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "so sieht und tichtet er, wie er ihm Vorteil nimmet,", "tokens": ["so", "sieht", "und", "tich\u00b7tet", "er", ",", "wie", "er", "ihm", "Vor\u00b7teil", "nim\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "nimt aller Sachen wahr, im Fall' da\u00df er vermerkt,", "tokens": ["nimt", "al\u00b7ler", "Sa\u00b7chen", "wahr", ",", "im", "Fall'", "da\u00df", "er", "ver\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,", "APPRART", "NN", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "da\u00df sein Verschoner liegt; er sieht, wie er sich st\u00e4rkt,", "tokens": ["da\u00df", "sein", "Ver\u00b7scho\u00b7ner", "liegt", ";", "er", "sieht", ",", "wie", "er", "sich", "st\u00e4rkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "vergi\u00dft der alten Treu' und seiner guten Worte,", "tokens": ["ver\u00b7gi\u00dft", "der", "al\u00b7ten", "Treu'", "und", "sei\u00b7ner", "gu\u00b7ten", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "setzt unvermerkt an ihn und f\u00e4llt ihn an dem Orte,", "tokens": ["setzt", "un\u00b7ver\u00b7merkt", "an", "ihn", "und", "f\u00e4llt", "ihn", "an", "dem", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "da man es nicht gedacht. Alsdenn so ists zu spat,", "tokens": ["da", "man", "es", "nicht", "ge\u00b7dacht", ".", "Als\u00b7denn", "so", "ists", "zu", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VVPP", "$.", "ADV", "ADV", "VAFIN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "wenn man bereuen will, da\u00df man geschonet hat", "tokens": ["wenn", "man", "be\u00b7reu\u00b7en", "will", ",", "da\u00df", "man", "ge\u00b7scho\u00b7net", "hat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "KOUS", "PIS", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "und so barmherzig war. Ich hab' es wol erfahren.", "tokens": ["und", "so", "barm\u00b7her\u00b7zig", "war", ".", "Ich", "hab'", "es", "wol", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dem Kriege zieh' ich nach nun bei so vielen Jahren,", "tokens": ["Dem", "Krie\u00b7ge", "zieh'", "ich", "nach", "nun", "bei", "so", "vie\u00b7len", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADV", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "ich wei\u00df des Krieges Brauch. Ich gebe kein Quartier,", "tokens": ["ich", "wei\u00df", "des", "Krie\u00b7ges", "Brauch", ".", "Ich", "ge\u00b7be", "kein", "Quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "und k\u00e4m' ein General und K\u00f6nig selbst mir f\u00fcr.", "tokens": ["und", "k\u00e4m'", "ein", "Ge\u00b7ne\u00b7ral", "und", "K\u00f6\u00b7nig", "selbst", "mir", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN", "ADV", "PPER", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich achte dessen nicht, da\u00df er von h\u00f6herm Stamme", "tokens": ["Ich", "ach\u00b7te", "des\u00b7sen", "nicht", ",", "da\u00df", "er", "von", "h\u00f6\u00b7herm", "Stam\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PTKNEG", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "als ich geboren ist. Di\u00df eben macht die Flamme,", "tokens": ["als", "ich", "ge\u00b7bo\u00b7ren", "ist", ".", "Di\u00df", "e\u00b7ben", "macht", "die", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$.", "PDS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "da\u00df ich mehr siegen will, indem er gr\u00f6\u00dfer ist", "tokens": ["da\u00df", "ich", "mehr", "sie\u00b7gen", "will", ",", "in\u00b7dem", "er", "gr\u00f6\u00b7\u00dfer", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "an Ahnen, nicht an Mut. Ein dapfrer Geist erkiest", "tokens": ["an", "Ah\u00b7nen", ",", "nicht", "an", "Mut", ".", "Ein", "dapf\u00b7rer", "Geist", "er\u00b7kiest"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "ihm stets ein H\u00f6hers aus, mit dem er m\u00f6ge ringen.", "tokens": ["ihm", "stets", "ein", "H\u00f6\u00b7hers", "aus", ",", "mit", "dem", "er", "m\u00f6\u00b7ge", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Der Ruhm der w\u00e4chst mit ihm, da\u00df er aus hohen Dingen", "tokens": ["Der", "Ruhm", "der", "w\u00e4chst", "mit", "ihm", ",", "da\u00df", "er", "aus", "ho\u00b7hen", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "vorhin entsprossen ist. Kein Adel dient vor mich,", "tokens": ["vor\u00b7hin", "ent\u00b7spros\u00b7sen", "ist", ".", "Kein", "A\u00b7del", "dient", "vor", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$.", "PIAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "di\u00df Schwert das adelt mich. Mein Rittersitz bin ich.", "tokens": ["di\u00df", "Schwert", "das", "a\u00b7delt", "mich", ".", "Mein", "Rit\u00b7ter\u00b7sitz", "bin", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Mein Leib ist mein Palast. Ein Krieger ist vergn\u00fcget,", "tokens": ["Mein", "Leib", "ist", "mein", "Pa\u00b7last", ".", "Ein", "Krie\u00b7ger", "ist", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "da\u00df er von einem Mahl' ans andre so viel krieget,", "tokens": ["da\u00df", "er", "von", "ei\u00b7nem", "Mahl'", "ans", "and\u00b7re", "so", "viel", "krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPRART", "PIS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "als er ben\u00f6tigt ist. Was hilft ihm Land und Gut?", "tokens": ["als", "er", "be\u00b7n\u00f6\u00b7tigt", "ist", ".", "Was", "hilft", "ihm", "Land", "und", "Gut", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$.", "PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die Feigen sehn auf di\u00df. Ich zahle bares Blut", "tokens": ["Die", "Fei\u00b7gen", "sehn", "auf", "di\u00df", ".", "Ich", "zah\u00b7le", "ba\u00b7res", "Blut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDS", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "vor meine G\u00fcter aus. Wem lie\u00df ichs wol zu erben,", "tokens": ["vor", "mei\u00b7ne", "G\u00fc\u00b7ter", "aus", ".", "Wem", "lie\u00df", "ichs", "wol", "zu", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "PWS", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "solt' etwan heute noch ich vor dem Feinde sterben,", "tokens": ["solt'", "et\u00b7wan", "heu\u00b7te", "noch", "ich", "vor", "dem", "Fein\u00b7de", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "den ich mir w\u00fcndsche stets? Ich lobe meinen Sinn,", "tokens": ["den", "ich", "mir", "w\u00fcnd\u00b7sche", "stets", "?", "Ich", "lo\u00b7be", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "mein Leben lieb' ich nicht. Ein Ander' ziehe hin", "tokens": ["mein", "Le\u00b7ben", "lieb'", "ich", "nicht", ".", "Ein", "An\u00b7der'", "zie\u00b7he", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "und karge, wie er will. Wir kommen leicht zu Gelde", "tokens": ["und", "kar\u00b7ge", ",", "wie", "er", "will", ".", "Wir", "kom\u00b7men", "leicht", "zu", "Gel\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$.", "PPER", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "und leichtlich wieder drum. Wir nehmens auf dem Felde", "tokens": ["und", "leicht\u00b7lich", "wie\u00b7der", "drum", ".", "Wir", "neh\u00b7mens", "auf", "dem", "Fel\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "PAV", "$.", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "und gebens in der Stadt. Uns ehret Iederman,", "tokens": ["und", "ge\u00b7bens", "in", "der", "Stadt", ".", "Uns", "eh\u00b7ret", "Ie\u00b7der\u00b7man", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "und wer nicht will, der mu\u00df. Es ist uns untertan", "tokens": ["und", "wer", "nicht", "will", ",", "der", "mu\u00df", ".", "Es", "ist", "uns", "un\u00b7ter\u00b7tan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PTKNEG", "VMFIN", "$,", "ART", "VMFIN", "$.", "PPER", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "nicht ein Land nur: die Welt, die mu\u00df uns Herren hei\u00dfen,", "tokens": ["nicht", "ein", "Land", "nur", ":", "die", "Welt", ",", "die", "mu\u00df", "uns", "Her\u00b7ren", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADV", "$.", "ART", "NN", "$,", "PDS", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.62": {"text": "wenn Herren uneins sind. Wir k\u00f6nnen uns nicht bei\u00dfen,", "tokens": ["wenn", "Her\u00b7ren", "un\u00b7eins", "sind", ".", "Wir", "k\u00f6n\u00b7nen", "uns", "nicht", "bei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAFIN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "wie alte M\u00fctter tun. Man schl\u00e4gt mit F\u00e4usten drein,", "tokens": ["wie", "al\u00b7te", "M\u00fct\u00b7ter", "tun", ".", "Man", "schl\u00e4gt", "mit", "F\u00e4us\u00b7ten", "drein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$.", "PIS", "VVFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "mit Degen und Pistol, wenn man nicht Freund will sein.", "tokens": ["mit", "De\u00b7gen", "und", "Pis\u00b7tol", ",", "wenn", "man", "nicht", "Freund", "will", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,", "KOUS", "PIS", "PTKNEG", "NN", "VMFIN", "VAINF", "$."], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}, "line.65": {"text": "Und das hei\u00dft recht geherrscht. Wir sterben, wie wir leben,", "tokens": ["Und", "das", "hei\u00dft", "recht", "ge\u00b7herrscht", ".", "Wir", "ster\u00b7ben", ",", "wie", "wir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "VVPP", "$.", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "frisch, dapfer, ritterlich. Wir sind dem Tod ergeben,", "tokens": ["frisch", ",", "dap\u00b7fer", ",", "rit\u00b7ter\u00b7lich", ".", "Wir", "sind", "dem", "Tod", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PAV", "$,", "ADJD", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.67": {"text": "wir wuchern auf das Blut. Das teure Gut, der Tod,", "tokens": ["wir", "wu\u00b7chern", "auf", "das", "Blut", ".", "Das", "teu\u00b7re", "Gut", ",", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "ist keines Ieden Kauf. Uns ist es t\u00e4glich Brot,", "tokens": ["ist", "kei\u00b7nes", "Ie\u00b7den", "Kauf", ".", "Uns", "ist", "es", "t\u00e4g\u00b7lich", "Brot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "was Andern seltsam ist. Wer w\u00fcndscht ihm doch zu siechen", "tokens": ["was", "An\u00b7dern", "selt\u00b7sam", "ist", ".", "Wer", "w\u00fcnd\u00b7scht", "ihm", "doch", "zu", "sie\u00b7chen"], "token_info": ["word", "word", "word", "word", "punc