{"textgrid.poem.63986": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "4. Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist dir gar ein gl\u00fccklicher Mann,", "tokens": ["Das", "ist", "dir", "gar", "ein", "gl\u00fcck\u00b7li\u00b7cher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der nicht mal mehr sich freuen kann,", "tokens": ["Der", "nicht", "mal", "mehr", "sich", "freu\u00b7en", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gl\u00fccklich ist er.", "tokens": ["So", "gl\u00fcck\u00b7lich", "ist", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So kommen jeden Morgen wir her,", "tokens": ["So", "kom\u00b7men", "je\u00b7den", "Mor\u00b7gen", "wir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "So kommen uns alle Tage daher.", "tokens": ["So", "kom\u00b7men", "uns", "al\u00b7le", "Ta\u00b7ge", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PAV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}