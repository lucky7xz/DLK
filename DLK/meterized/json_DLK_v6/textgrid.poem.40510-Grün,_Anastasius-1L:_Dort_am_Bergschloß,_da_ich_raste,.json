{"textgrid.poem.40510": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort am Bergschlo\u00df, da ich raste,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort am Bergschlo\u00df, da ich raste,", "tokens": ["Dort", "am", "Berg\u00b7schlo\u00df", ",", "da", "ich", "ras\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4dt der Bl\u00fcthenbaum mich ein,", "tokens": ["L\u00e4dt", "der", "Bl\u00fc\u00b7then\u00b7baum", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freundlich winkt der Vogt zu Gaste", "tokens": ["Freund\u00b7lich", "winkt", "der", "Vogt", "zu", "Gas\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem vollen Becher Wein.", "tokens": ["Mit", "dem", "vol\u00b7len", "Be\u00b7cher", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Urahn und seine G\u00e4ste", "tokens": ["Den", "Ur\u00b7ahn", "und", "sei\u00b7ne", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hat die\u00df Kelchglas schon geletzt,", "tokens": ["Hat", "die\u00df", "Kelchg\u00b7las", "schon", "ge\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und an ihrem Hochzeitfeste", "tokens": ["Und", "an", "ih\u00b7rem", "Hoch\u00b7zeit\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ahnfrau diesen Baum gesetzt.", "tokens": ["Ahn\u00b7frau", "die\u00b7sen", "Baum", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drum wie seinen Bl\u00fcthenregen", "tokens": ["Drum", "wie", "sei\u00b7nen", "Bl\u00fc\u00b7then\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "KOKOM", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Ueber mich der Baum jetzt streut,", "tokens": ["Ue\u00b7ber", "mich", "der", "Baum", "jetzt", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcnkt's mich wie ein Ahnensegen", "tokens": ["D\u00fcnkt's", "mich", "wie", "ein", "Ah\u00b7nen\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der alten fernen Zeit.", "tokens": ["Aus", "der", "al\u00b7ten", "fer\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie ich, vom Born zu nippen,", "tokens": ["Und", "wie", "ich", ",", "vom", "Born", "zu", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Glas ber\u00fchrt den Mund,", "tokens": ["Mit", "dem", "Glas", "be\u00b7r\u00fchrt", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist's, als ob des Ahnherrn Lippen", "tokens": ["Ist's", ",", "als", "ob", "des", "Ahn\u00b7herrn", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOKOM", "KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00f6ten mir den Gru\u00df zum Bund.", "tokens": ["B\u00f6\u00b7ten", "mir", "den", "Gru\u00df", "zum", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Die in weiter Welt sich mieden,", "tokens": ["Die", "in", "wei\u00b7ter", "Welt", "sich", "mie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einte dieses Glases Kreis;", "tokens": ["Ein\u00b7te", "die\u00b7ses", "Gla\u00b7ses", "Kreis", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was durch Zeit und Land geschieden,", "tokens": ["Was", "durch", "Zeit", "und", "Land", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dr\u00fcckt hier Lipp' an Lippe leis.", "tokens": ["Dr\u00fcckt", "hier", "Lipp'", "an", "Lip\u00b7pe", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von Geschlechten zu Geschlechten", "tokens": ["Von", "Ge\u00b7schlech\u00b7ten", "zu", "Ge\u00b7schlech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlinge sich der heil'ge Bund!", "tokens": ["Schlin\u00b7ge", "sich", "der", "heil'\u00b7ge", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fort und fort sein Band zu flechten,", "tokens": ["Fort", "und", "fort", "sein", "Band", "zu", "flech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiht, o Glas, dich Herz und Mund!", "tokens": ["Weiht", ",", "o", "Glas", ",", "dich", "Herz", "und", "Mund", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "NN", "$,", "PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Diesen Ku\u00df, zu fernen Tagen,", "tokens": ["Die\u00b7sen", "Ku\u00df", ",", "zu", "fer\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn zu Staube l\u00e4ngst ich bin,", "tokens": ["Wenn", "zu", "Stau\u00b7be", "l\u00e4ngst", "ich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst du auf die Lippen tragen", "tokens": ["Sollst", "du", "auf", "die", "Lip\u00b7pen", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer sp\u00e4ten Enkelin.", "tokens": ["Ei\u00b7ner", "sp\u00e4\u00b7ten", "En\u00b7ke\u00b7lin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "F\u00fcr den Enkel Gru\u00df und Segen", "tokens": ["F\u00fcr", "den", "En\u00b7kel", "Gru\u00df", "und", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich dir, o Baum, vertrau'n,", "tokens": ["Will", "ich", "dir", ",", "o", "Baum", ",", "ver\u00b7trau'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$,", "FM", "NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df du ihn als Bl\u00fcthenregen", "tokens": ["Da\u00df", "du", "ihn", "als", "Bl\u00fc\u00b7then\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um sein Haupt magst niederthau'n.", "tokens": ["Um", "sein", "Haupt", "magst", "nie\u00b7dert\u00b7hau'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUI", "PPOSAT", "NN", "VMFIN", "ADV", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}