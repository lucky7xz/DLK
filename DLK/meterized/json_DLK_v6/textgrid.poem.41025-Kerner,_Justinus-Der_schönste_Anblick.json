{"textgrid.poem.41025": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der sch\u00f6nste Anblick", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist's, wenn zwei Sterne", "tokens": ["Sch\u00f6n", "ist's", ",", "wenn", "zwei", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nah sich stehn am Firmament,", "tokens": ["Nah", "sich", "stehn", "am", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n, wenn zweier Rosen", "tokens": ["Sch\u00f6n", ",", "wenn", "zwei\u00b7er", "Ro\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "R\u00f6te ineinander brennt.", "tokens": ["R\u00f6\u00b7te", "in\u00b7ein\u00b7an\u00b7der", "brennt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch in Wahrheit! immer", "tokens": ["Doch", "in", "Wahr\u00b7heit", "!", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$.", "ADV"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist's am sch\u00f6nsten anzusehn:", "tokens": ["Ist's", "am", "sch\u00f6ns\u00b7ten", "an\u00b7zu\u00b7sehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie zwei, so sich lieben,", "tokens": ["Wie", "zwei", ",", "so", "sich", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "$,", "ADV", "PRF", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Selig beieinander stehn.", "tokens": ["Se\u00b7lig", "bei\u00b7ein\u00b7an\u00b7der", "stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}