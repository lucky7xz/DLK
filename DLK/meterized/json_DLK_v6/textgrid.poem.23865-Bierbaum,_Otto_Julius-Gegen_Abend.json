{"textgrid.poem.23865": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gegen Abend", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun h\u00e4ngt nur noch am Kirchturmknopf", "tokens": ["Nun", "h\u00e4ngt", "nur", "noch", "am", "Kirch\u00b7turm\u00b7knopf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der letzte Sonnenschein;", "tokens": ["Der", "letz\u00b7te", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald werden auch die H\u00f6hen", "tokens": ["Bald", "wer\u00b7den", "auch", "die", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ganz ohne Sonne sein.", "tokens": ["Ganz", "oh\u00b7ne", "Son\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Silberglanz dann \u00fcberall;", "tokens": ["Und", "Sil\u00b7ber\u00b7glanz", "dann", "\u00fc\u00b7be\u00b7rall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Mondes blasses Licht", "tokens": ["Des", "Mon\u00b7des", "blas\u00b7ses", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umsch\u00fcttet unsre Laube,", "tokens": ["Um\u00b7sch\u00fct\u00b7tet", "uns\u00b7re", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umleuchtet dein Gesicht.", "tokens": ["Um\u00b7leuch\u00b7tet", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Mond, das Licht der K\u00fcsse,", "tokens": ["Der", "Mond", ",", "das", "Licht", "der", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das alles zaubrisch macht:", "tokens": ["Das", "al\u00b7les", "zaub\u00b7risch", "macht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm, Nacht, mit deinen Gnaden,", "tokens": ["Komm", ",", "Nacht", ",", "mit", "dei\u00b7nen", "Gna\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du liebereiche Nacht!", "tokens": ["Du", "lie\u00b7be\u00b7rei\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}