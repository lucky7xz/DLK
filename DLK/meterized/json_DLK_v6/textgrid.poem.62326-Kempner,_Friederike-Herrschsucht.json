{"textgrid.poem.62326": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Herrschsucht", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du allgemeinste, ganz gemeine Leidenschaft,", "tokens": ["Du", "all\u00b7ge\u00b7meins\u00b7te", ",", "ganz", "ge\u00b7mei\u00b7ne", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die niemals etwas Gutes oder Sch\u00f6nes schafft.", "tokens": ["Die", "nie\u00b7mals", "et\u00b7was", "Gu\u00b7tes", "o\u00b7der", "Sch\u00f6\u00b7nes", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von Lieb' ist in dir keine Spur,", "tokens": ["Von", "Lieb'", "ist", "in", "dir", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "APPR", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist die reine Selbstsucht nur.", "tokens": ["Du", "bist", "die", "rei\u00b7ne", "Selbst\u00b7sucht", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}