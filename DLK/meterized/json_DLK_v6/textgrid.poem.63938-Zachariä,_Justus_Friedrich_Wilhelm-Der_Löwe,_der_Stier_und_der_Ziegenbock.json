{"textgrid.poem.63938": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der L\u00f6we, der Stier und der Ziegenbock", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn erst der M\u00e4chtige dir droht,", "tokens": ["Wenn", "erst", "der", "M\u00e4ch\u00b7ti\u00b7ge", "dir", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schw\u00f6rt auch der Klein're dir den Tod.", "tokens": ["Schw\u00f6rt", "auch", "der", "Klein'\u00b7re", "dir", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein starker Stier, sonst unverzagt,", "tokens": ["Ein", "star\u00b7ker", "Stier", ",", "sonst", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward von dem L\u00f6wen doch gejagt,", "tokens": ["Ward", "von", "dem", "L\u00f6\u00b7wen", "doch", "ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und floh nach seinem Stalle zu.", "tokens": ["Und", "floh", "nach", "sei\u00b7nem", "Stal\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Ziegenbock stand da in Ruh',", "tokens": ["Ein", "Zie\u00b7gen\u00b7bock", "stand", "da", "in", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und hielt dem Stier sogleich verwegen", "tokens": ["Und", "hielt", "dem", "Stier", "sog\u00b7leich", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit wildem Blick sein Horn entgegen.", "tokens": ["Mit", "wil\u00b7dem", "Blick", "sein", "Horn", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Ochse wich in vollem Lauf", "tokens": ["Der", "O\u00b7chse", "wich", "in", "vol\u00b7lem", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den H\u00f6rnern aus, und sprach darauf:", "tokens": ["Den", "H\u00f6r\u00b7nern", "aus", ",", "und", "sprach", "da\u00b7rauf", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich f\u00fcrchte mich, Freund, nicht vor dir;", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", ",", "Freund", ",", "nicht", "vor", "dir", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Allein der L\u00f6w' ist hinter mir.", "tokens": ["Al\u00b7lein", "der", "L\u00f6\u00b7w'", "ist", "hin\u00b7ter", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "APPR", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}