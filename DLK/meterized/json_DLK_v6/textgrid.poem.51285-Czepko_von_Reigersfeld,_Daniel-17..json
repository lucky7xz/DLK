{"textgrid.poem.51285": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ersprie\u00dflich ist der Thau: vergeht doch unterm Sehn:", "tokens": ["Er\u00b7sprie\u00df\u00b7lich", "ist", "der", "Thau", ":", "ver\u00b7geht", "doch", "un\u00b7term", "Sehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Annehmlich ist die Lufft, entwischt doch unterm Wehn:", "tokens": ["An\u00b7nehm\u00b7lich", "ist", "die", "Lufft", ",", "ent\u00b7wischt", "doch", "un\u00b7term", "Wehn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonn ist klar, und ist betr\u00fcglich so als rein,", "tokens": ["Die", "Sonn", "ist", "klar", ",", "und", "ist", "be\u00b7tr\u00fcg\u00b7lich", "so", "als", "rein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "VAFIN", "ADJD", "ADV", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tag ist sch\u00f6n, und ist verg\u00e4nglich so als fein,", "tokens": ["Der", "Tag", "ist", "sch\u00f6n", ",", "und", "ist", "ver\u00b7g\u00e4ng\u00b7lich", "so", "als", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "VAFIN", "ADJD", "ADV", "KOUS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Nacht ist k\u00fchl, und wird vom Donner offt versehrt,", "tokens": ["Die", "Nacht", "ist", "k\u00fchl", ",", "und", "wird", "vom", "Don\u00b7ner", "offt", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schlaff ist s\u00fc\u00df, und wird von Tr\u00e4umen offt verst\u00f6rt.", "tokens": ["Der", "Schlaff", "ist", "s\u00fc\u00df", ",", "und", "wird", "von", "Tr\u00e4u\u00b7men", "offt", "ver\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So ist das Leben auch, das wir am Halse tragen,", "tokens": ["So", "ist", "das", "Le\u00b7ben", "auch", ",", "das", "wir", "am", "Hal\u00b7se", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein Theil ist voller Freud', ein Theil ist voller Plagen.", "tokens": ["Ein", "Theil", "ist", "vol\u00b7ler", "Freud'", ",", "ein", "Theil", "ist", "vol\u00b7ler", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}