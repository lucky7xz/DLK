{"dta.poem.10412": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Aus meinen gro\u00dfen Schmerzen", "tokens": ["Aus", "mei\u00b7nen", "gro\u00b7\u00dfen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mach' ich die kleinen Lieder;", "tokens": ["Mach'", "ich", "die", "klei\u00b7nen", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die heben ihr klingend Gefieder", "tokens": ["Die", "he\u00b7ben", "ihr", "klin\u00b7gend", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVPP", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und flattern nach ihrem Herzen.", "tokens": ["Und", "flat\u00b7tern", "nach", "ih\u00b7rem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Sie fanden den Weg zur Trauten,", "tokens": ["Sie", "fan\u00b7den", "den", "Weg", "zur", "Trau\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch kommen sie wieder und klagen,", "tokens": ["Doch", "kom\u00b7men", "sie", "wie\u00b7der", "und", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und klagen, und wollen nicht sagen,", "tokens": ["Und", "kla\u00b7gen", ",", "und", "wol\u00b7len", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Was sie im Herzen schauten.", "tokens": ["Was", "sie", "im", "Her\u00b7zen", "schau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}