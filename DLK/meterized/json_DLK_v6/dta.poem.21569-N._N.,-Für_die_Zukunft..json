{"dta.poem.21569": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr die Zukunft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wer heut\u2019 nicht die eigenen Zeiten versteht,", "tokens": ["Wer", "heut'", "nicht", "die", "ei\u00b7ge\u00b7nen", "Zei\u00b7ten", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den lasse der Himmel nur sterben,", "tokens": ["Den", "las\u00b7se", "der", "Him\u00b7mel", "nur", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Eh\u2019 die glimmende Welt in Flammen aufgeht", "tokens": ["Eh'", "die", "glim\u00b7men\u00b7de", "Welt", "in", "Flam\u00b7men", "auf\u00b7geht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und die Marmorpal\u00e4ste verderben;", "tokens": ["Und", "die", "Mar\u00b7mor\u00b7pa\u00b7l\u00e4s\u00b7te", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Eh\u2019 die Throne versinken im siedenden Meer", "tokens": ["Eh'", "die", "Thro\u00b7ne", "ver\u00b7sin\u00b7ken", "im", "sie\u00b7den\u00b7den", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und der Blutrauch dampft durch die Gassen einher.", "tokens": ["Und", "der", "Blu\u00b7trauch", "dampft", "durch", "die", "Gas\u00b7sen", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcckselig die Menschen, die taumelfroh", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "die", "Men\u00b7schen", ",", "die", "tau\u00b7mel\u00b7froh"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Sich durch das Jahrhundert trollen,", "tokens": ["Sich", "durch", "das", "Jahr\u00b7hun\u00b7dert", "trol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Champagner trinken, ob lichterloh", "tokens": ["Cham\u00b7pag\u00b7ner", "trin\u00b7ken", ",", "ob", "lich\u00b7ter\u00b7loh"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch drau\u00dfen die Blitze rollen,", "tokens": ["Auch", "drau\u00b7\u00dfen", "die", "Blit\u00b7ze", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die nie beim Gelag\u2019 der Gedanke bedr\u00e4ut:", "tokens": ["Die", "nie", "beim", "Ge\u00b7lag'", "der", "Ge\u00b7dan\u00b7ke", "be\u00b7dr\u00e4ut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "\u201edie Welt kann nimmer so bleiben wie heut\u201c.", "tokens": ["\u201e", "die", "Welt", "kann", "nim\u00b7mer", "so", "blei\u00b7ben", "wie", "heut", "\u201c", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "ADV", "ADV", "VVINF", "KOKOM", "ADV", "$(", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Hier Haufen von Gold und Demant und Geschmeid\u2019,", "tokens": ["Hier", "Hau\u00b7fen", "von", "Gold", "und", "De\u00b7mant", "und", "Ge\u00b7schmeid'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dort auch nicht ein Heller zu finden;", "tokens": ["Dort", "auch", "nicht", "ein", "Hel\u00b7ler", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Hier brausende, sausende Herrlichkeit,", "tokens": ["Hier", "brau\u00b7sen\u00b7de", ",", "sau\u00b7sen\u00b7de", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dort trockene Schwarzbrodrinden.", "tokens": ["Dort", "tro\u00b7cke\u00b7ne", "Schwarz\u00b7brod\u00b7rin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gott-Vater im Himmel schick\u2019 einen Prophet\u2019,", "tokens": ["Got\u00b7tVa\u00b7ter", "im", "Him\u00b7mel", "schick'", "ei\u00b7nen", "Pro\u00b7phet'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der der Welt in\u2019s Gewissen zu reden versteht.", "tokens": ["Der", "der", "Welt", "in's", "Ge\u00b7wis\u00b7sen", "zu", "re\u00b7den", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Schick\u2019 einen Propheten in\u2019s g\u00e4hrende Land,", "tokens": ["Schick'", "ei\u00b7nen", "Pro\u00b7phe\u00b7ten", "in's", "g\u00e4h\u00b7ren\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der soll die Pal\u00e4ste besuchen,", "tokens": ["Der", "soll", "die", "Pa\u00b7l\u00e4s\u00b7te", "be\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der soll an die marmorspiegelnde Wand", "tokens": ["Der", "soll", "an", "die", "mar\u00b7mor\u00b7spie\u00b7geln\u00b7de", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In Flammenschrift schreiben und buchen,", "tokens": ["In", "Flam\u00b7men\u00b7schrift", "schrei\u00b7ben", "und", "bu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Auf da\u00df es die Prasser mit Grausen erfa\u00dft:", "tokens": ["Auf", "da\u00df", "es", "die", "Pras\u00b7ser", "mit", "Grau\u00b7sen", "er\u00b7fa\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Auf einem Vulkan steht unser Palast.", "tokens": ["Auf", "ei\u00b7nem", "Vul\u00b7kan", "steht", "un\u00b7ser", "Pa\u00b7last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Auf da\u00df sie gewarnt, noch eh\u2019 es zu sp\u00e4t,", "tokens": ["Auf", "da\u00df", "sie", "ge\u00b7warnt", ",", "noch", "eh'", "es", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVPP", "$,", "ADV", "KOUS", "PPER", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Eh\u2019 die Wogen des Aufruhrs st\u00fcrmen,", "tokens": ["Eh'", "die", "Wo\u00b7gen", "des", "Auf\u00b7ruhrs", "st\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Eh\u2019 die rohe Gewalt wie die Sense m\u00e4ht", "tokens": ["Eh'", "die", "ro\u00b7he", "Ge\u00b7walt", "wie", "die", "Sen\u00b7se", "m\u00e4ht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "KOKOM", "ART", "NN", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und die Barrikaden sich th\u00fcrmen;", "tokens": ["Und", "die", "Bar\u00b7ri\u00b7ka\u00b7den", "sich", "th\u00fcr\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der hungernde Haufe mit Pechkranz und Blei", "tokens": ["Der", "hun\u00b7gern\u00b7de", "Hau\u00b7fe", "mit", "Pech\u00b7kranz", "und", "Blei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ertrotzt, da\u00df das Gl\u00fcck auch ihm hold nun sei.", "tokens": ["Er\u00b7trotzt", ",", "da\u00df", "das", "Gl\u00fcck", "auch", "ihm", "hold", "nun", "sei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "ADV", "PPER", "ADJD", "ADV", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Dann gilt nichts Heiliges mehr auf der Welt,", "tokens": ["Dann", "gilt", "nichts", "Hei\u00b7li\u00b7ges", "mehr", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es st\u00fcrzen Kirch\u2019 und Kapellen.", "tokens": ["Es", "st\u00fcr\u00b7zen", "Kirch'", "und", "Ka\u00b7pel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Liebe verroht und der Glaube zerschellt,", "tokens": ["Die", "Lie\u00b7be", "ver\u00b7roht", "und", "der", "Glau\u00b7be", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Das Mitleid begraben die Wellen.", "tokens": ["Das", "Mit\u00b7leid", "be\u00b7gra\u00b7ben", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Die Massen nur raufen sich um das Gold,", "tokens": ["Die", "Mas\u00b7sen", "nur", "rau\u00b7fen", "sich", "um", "das", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das \u00fcber die dampfenden Tr\u00fcmmer rollt.", "tokens": ["Das", "\u00fc\u00b7ber", "die", "damp\u00b7fen\u00b7den", "Tr\u00fcm\u00b7mer", "rollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}