{"textgrid.poem.41099": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O w\u00e4r' ich wie Dein sch\u00f6ner Garten,", "tokens": ["O", "w\u00e4r'", "ich", "wie", "Dein", "sch\u00f6\u00b7ner", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KOKOM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der selbst beim Eise Rosen tr\u00e4gt,", "tokens": ["Der", "selbst", "beim", "Ei\u00b7se", "Ro\u00b7sen", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Draus einen Strau\u00df von hundert Arten", "tokens": ["Draus", "ei\u00b7nen", "Strau\u00df", "von", "hun\u00b7dert", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NE", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir in die lieben H\u00e4nde legt!", "tokens": ["Dir", "in", "die", "lie\u00b7ben", "H\u00e4n\u00b7de", "legt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O w\u00e4r' ich wie ein Stern in L\u00fcften,", "tokens": ["O", "w\u00e4r'", "ich", "wie", "ein", "Stern", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der noch so alt, mit neuem Schein", "tokens": ["Der", "noch", "so", "alt", ",", "mit", "neu\u00b7em", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Wettern und aus Nebeld\u00fcften", "tokens": ["Aus", "Wet\u00b7tern", "und", "aus", "Ne\u00b7bel\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir blickt' ins sch\u00f6ne Aug' hinein!", "tokens": ["Dir", "blickt'", "ins", "sch\u00f6\u00b7ne", "Aug'", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O w\u00e4r' ein Wein ich, der, je l\u00e4nger", "tokens": ["O", "w\u00e4r'", "ein", "Wein", "ich", ",", "der", ",", "je", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "PPER", "$,", "PRELS", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er lebt, nur desto geist'ger schmeckt;", "tokens": ["Er", "lebt", ",", "nur", "des\u00b7to", "geist'\u00b7ger", "schmeckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann w\u00fcrd' ich, ein gl\u00fccksel'ger S\u00e4nger,", "tokens": ["Dann", "w\u00fcrd'", "ich", ",", "ein", "gl\u00fcck\u00b7sel'\u00b7ger", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets frisch zu Deinem Fest erweckt.", "tokens": ["Stets", "frisch", "zu", "Dei\u00b7nem", "Fest", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch ach, ich bin kein Rosengarten,", "tokens": ["Doch", "ach", ",", "ich", "bin", "kein", "Ro\u00b7sen\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Stern bin ich, \u2013 ich bin kein Wein,", "tokens": ["Kein", "Stern", "bin", "ich", ",", "\u2013", "ich", "bin", "kein", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "$,", "$(", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Gram und Alter brachen Scharten", "tokens": ["Und", "Gram", "und", "Al\u00b7ter", "bra\u00b7chen", "Schar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir l\u00e4ngst schon in das Herz hinein.", "tokens": ["Mir", "l\u00e4ngst", "schon", "in", "das", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und doch bl\u00fcht im zerri\u00dfnen Herzen", "tokens": ["Und", "doch", "bl\u00fcht", "im", "zer\u00b7ri\u00df\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mir tief noch eine Rose rot,", "tokens": ["Mir", "tief", "noch", "ei\u00b7ne", "Ro\u00b7se", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die brachen nicht der Erde Schmerzen,", "tokens": ["Die", "bra\u00b7chen", "nicht", "der", "Er\u00b7de", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der bringt kein Frost, kein Alter Tod.", "tokens": ["Der", "bringt", "kein", "Frost", ",", "kein", "Al\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja! ist mein Alter noch so tr\u00fcbe,", "tokens": ["Ja", "!", "ist", "mein", "Al\u00b7ter", "noch", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenk' ich ", "tokens": ["Ge\u00b7denk'", "ich"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die Rose hei\u00dft: Du bist mir gut.", "tokens": ["Die", "Ro\u00b7se", "hei\u00dft", ":", "Du", "bist", "mir", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O bleib ", "tokens": ["O", "bleib"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Noch Rosen w\u00fcnscht, noch Sonnenglanz,", "tokens": ["Noch", "Ro\u00b7sen", "w\u00fcnscht", ",", "noch", "Son\u00b7nen\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mutterfrieden, Gottes Segen,", "tokens": ["Der", "Mut\u00b7ter\u00b7frie\u00b7den", ",", "Got\u00b7tes", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er schon tot, vergessen ganz.", "tokens": ["Ist", "er", "schon", "tot", ",", "ver\u00b7ges\u00b7sen", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}