{"textgrid.poem.59522": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Frischauf!", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fahnen wehen, frischauf zur Schlacht!", "tokens": ["Die", "Fah\u00b7nen", "we\u00b7hen", ",", "frisc\u00b7hauf", "zur", "Schlacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schlagt mutig drein!", "tokens": ["Schlagt", "mu\u00b7tig", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es klingt Musik, die uns fr\u00f6hlich macht,", "tokens": ["Es", "klingt", "Mu\u00b7sik", ",", "die", "uns", "fr\u00f6h\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ins Herz hinein,", "tokens": ["Ins", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die Pfeifen und Trommeln mit s\u00fc\u00dfem Klang", "tokens": ["Die", "Pfei\u00b7fen", "und", "Trom\u00b7meln", "mit", "s\u00fc\u00b7\u00dfem", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das Feld entlang.", "tokens": ["Das", "Feld", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In die Schlacht, in die Schlacht hinein!", "tokens": ["In", "die", "Schlacht", ",", "in", "die", "Schlacht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Wer m\u00f6chte bleiben, wenn's lustig geht,", "tokens": ["Wer", "m\u00f6ch\u00b7te", "blei\u00b7ben", ",", "wenn's", "lus\u00b7tig", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "VVINF", "$,", "KOUS", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im stillen Haus?", "tokens": ["Im", "stil\u00b7len", "Haus", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wohlauf! Wenn Jugend in Bl\u00fcte steht,", "tokens": ["Wohl\u00b7auf", "!", "Wenn", "Ju\u00b7gend", "in", "Bl\u00fc\u00b7te", "steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinaus, hinaus,", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Wo frisch und munter das Leben fliegt!", "tokens": ["Wo", "frisch", "und", "mun\u00b7ter", "das", "Le\u00b7ben", "fliegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wen das vergn\u00fcgt,", "tokens": ["Wen", "das", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "In die Schlacht, in die Schlacht hinaus!", "tokens": ["In", "die", "Schlacht", ",", "in", "die", "Schlacht", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wer m\u00f6chte bleiben, wann alles klingt", "tokens": ["Wer", "m\u00f6ch\u00b7te", "blei\u00b7ben", ",", "wann", "al\u00b7les", "klingt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "VVINF", "$,", "PWAV", "PIS", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Saus und Braus?", "tokens": ["In", "Saus", "und", "Braus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wann Ruhm die silbernen Glocken ringt", "tokens": ["Wann", "Ruhm", "die", "sil\u00b7ber\u00b7nen", "Glo\u00b7cken", "ringt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In die Welt hinaus?", "tokens": ["In", "die", "Welt", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wer s\u00e4\u00dfe mit Weibern am Herde dann?", "tokens": ["Wer", "s\u00e4\u00b7\u00dfe", "mit", "Wei\u00b7bern", "am", "Her\u00b7de", "dann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das kann ", "tokens": ["Das", "kann"], "token_info": ["word", "word"], "pos": ["PDS", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "In die Schlacht, in die Schlacht hinaus!", "tokens": ["In", "die", "Schlacht", ",", "in", "die", "Schlacht", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ein K\u00f6nigsleben, Soldaten Gut,", "tokens": ["Ein", "K\u00f6\u00b7nigs\u00b7le\u00b7ben", ",", "Sol\u00b7da\u00b7ten", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mu\u00df unser sein;", "tokens": ["Mu\u00df", "un\u00b7ser", "sein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wir tragen blanker den k\u00fchnen Mut", "tokens": ["Wir", "tra\u00b7gen", "blan\u00b7ker", "den", "k\u00fch\u00b7nen", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Schwertesschein,", "tokens": ["Als", "Schwer\u00b7tes\u00b7schein", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und wer in dem fr\u00f6hlichen Streite f\u00e4llt,", "tokens": ["Und", "wer", "in", "dem", "fr\u00f6h\u00b7li\u00b7chen", "Strei\u00b7te", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der hei\u00dft ein Held.", "tokens": ["Der", "hei\u00dft", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In die Schlacht, in die Schlacht hinein!", "tokens": ["In", "die", "Schlacht", ",", "in", "die", "Schlacht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Dem klingt Musik, die er leiden mag,", "tokens": ["Dem", "klingt", "Mu\u00b7sik", ",", "die", "er", "lei\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gar lustig drein;", "tokens": ["Gar", "lus\u00b7tig", "drein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nicht sch\u00f6ner klingt es am J\u00fcngsten Tag", "tokens": ["Nicht", "sch\u00f6\u00b7ner", "klingt", "es", "am", "J\u00fcng\u00b7sten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ins Grab hinein.", "tokens": ["Ins", "Grab", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "O seliger Tod des Soldaten Tod!", "tokens": ["O", "se\u00b7li\u00b7ger", "Tod", "des", "Sol\u00b7da\u00b7ten", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Noch sind wir rot \u2013", "tokens": ["Noch", "sind", "wir", "rot", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In die Schlacht, in die Schlacht hinein!", "tokens": ["In", "die", "Schlacht", ",", "in", "die", "Schlacht", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}