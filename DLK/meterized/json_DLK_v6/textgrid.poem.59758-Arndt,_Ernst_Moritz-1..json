{"textgrid.poem.59758": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eines reinen Auges Klarheit,", "tokens": ["Ei\u00b7nes", "rei\u00b7nen", "Au\u00b7ges", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines tapfern Mundes Wahrheit,", "tokens": ["Ei\u00b7nes", "tap\u00b7fern", "Mun\u00b7des", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer treuen Rechte Schwur \u2013", "tokens": ["Ei\u00b7ner", "treu\u00b7en", "Rech\u00b7te", "Schwur", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese drei geweihten Dinge", "tokens": ["Die\u00b7se", "drei", "ge\u00b7weih\u00b7ten", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heben hoch zum Sonnenringe", "tokens": ["He\u00b7ben", "hoch", "zum", "Son\u00b7nen\u00b7rin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Nacht der Erdenflur.", "tokens": ["Aus", "der", "Nacht", "der", "Er\u00b7den\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch auf Erden sollst du weilen,", "tokens": ["Doch", "auf", "Er\u00b7den", "sollst", "du", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streiche mit den D\u00fcstern teilen,", "tokens": ["Strei\u00b7che", "mit", "den", "D\u00fcs\u00b7tern", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Banner L\u00fcge bl\u00e4ht.", "tokens": ["De\u00b7ren", "Ban\u00b7ner", "L\u00fc\u00b7ge", "bl\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier gilt's Licht und Recht zu wahren", "tokens": ["Hier", "gilt's", "Licht", "und", "Recht", "zu", "wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und den Gott zu offenbaren,", "tokens": ["Und", "den", "Gott", "zu", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der aus Menschenn\u00fcstern weht.", "tokens": ["Der", "aus", "Men\u00b7schen\u00b7n\u00fcs\u00b7tern", "weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Deine Erde sollst du tragen", "tokens": ["Dei\u00b7ne", "Er\u00b7de", "sollst", "du", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dich mit dem Geist zerschlagen,", "tokens": ["Und", "dich", "mit", "dem", "Geist", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der die finstern Fahnen schwellt,", "tokens": ["Der", "die", "fins\u00b7tern", "Fah\u00b7nen", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene drei geweihten Dinge", "tokens": ["Je\u00b7ne", "drei", "ge\u00b7weih\u00b7ten", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind die diamantne Klinge,", "tokens": ["Sind", "die", "di\u00b7a\u00b7mant\u00b7ne", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die Lug und Teufel f\u00e4llt.", "tokens": ["Durch", "die", "Lug", "und", "Teu\u00b7fel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}