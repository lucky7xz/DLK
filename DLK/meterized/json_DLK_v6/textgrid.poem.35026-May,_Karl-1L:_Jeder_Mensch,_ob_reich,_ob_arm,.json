{"textgrid.poem.35026": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Jeder Mensch, ob reich, ob arm,", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder Mensch, ob reich, ob arm,", "tokens": ["Je\u00b7der", "Mensch", ",", "ob", "reich", ",", "ob", "arm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ADJD", "$,", "KOUS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ein Pl\u00e4tzchen haben,", "tokens": ["Mu\u00df", "ein", "Pl\u00e4tz\u00b7chen", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo er sich bei Freud' und Harm", "tokens": ["Wo", "er", "sich", "bei", "Freud'", "und", "Harm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Traulich kann vergraben.", "tokens": ["Trau\u00b7lich", "kann", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meines ist ein K\u00e4mmerlein,", "tokens": ["Mei\u00b7nes", "ist", "ein", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Niedrig zwar und enge:", "tokens": ["Nied\u00b7rig", "zwar", "und", "en\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Tief wird's kaum vier Ellen sein,", "tokens": ["Tief", "wird's", "kaum", "vier", "El\u00b7len", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "CARD", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieben in der L\u00e4nge.", "tokens": ["Sie\u00b7ben", "in", "der", "L\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "W\u00e4nde hat es ihrer vier,", "tokens": ["W\u00e4n\u00b7de", "hat", "es", "ih\u00b7rer", "vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPOSAT", "CARD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber keine Decke;", "tokens": ["A\u00b7ber", "kei\u00b7ne", "De\u00b7cke", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Denn statt dieser zeigt sich Dir", "tokens": ["Denn", "statt", "die\u00b7ser", "zeigt", "sich", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "VVFIN", "PRF", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Eine lange Ecke.", "tokens": ["Ei\u00b7ne", "lan\u00b7ge", "E\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Fast den allerkleinsten Raum", "tokens": ["Fast", "den", "al\u00b7ler\u00b7kleins\u00b7ten", "Raum"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "F\u00fcllt des Fensters Zierde,", "tokens": ["F\u00fcllt", "des", "Fens\u00b7ters", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Und vom Himmel seh ich kaum", "tokens": ["Und", "vom", "Him\u00b7mel", "seh", "ich", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "F\u00fcnf Zoll in's Gevierte.", "tokens": ["F\u00fcnf", "Zoll", "in's", "Ge\u00b7vier\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Was die M\u00f6bel anbelangt,", "tokens": ["Was", "die", "M\u00f6\u00b7bel", "an\u00b7be\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Sind's meist Invaliden;", "tokens": ["Sin\u00b7d's", "meist", "In\u00b7va\u00b7li\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.19": {"text": "Denn was Andre abgedankt", "tokens": ["Denn", "was", "And\u00b7re", "ab\u00b7ge\u00b7dankt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "PIS", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Wohnt bei mir in Frieden.", "tokens": ["Wohnt", "bei", "mir", "in", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "An den W\u00e4nden ringsherum", "tokens": ["An", "den", "W\u00e4n\u00b7den", "rings\u00b7he\u00b7rum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Siehst Du gro\u00dfe Geister,", "tokens": ["Siehst", "Du", "gro\u00b7\u00dfe", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Im Repositorium", "tokens": ["Im", "Re\u00b7po\u00b7si\u00b7to\u00b7ri\u00b7um"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Werke dieser Meister.", "tokens": ["Wer\u00b7ke", "die\u00b7ser", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Noch ein Liebchen sei erw\u00e4hnt,", "tokens": ["Noch", "ein", "Lieb\u00b7chen", "sei", "er\u00b7w\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Das ich oft ergreife,", "tokens": ["Das", "ich", "oft", "er\u00b7grei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Das dort traulich winkend lehnt:", "tokens": ["Das", "dort", "trau\u00b7lich", "win\u00b7kend", "lehnt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Meine lange Pfeife.", "tokens": ["Mei\u00b7ne", "lan\u00b7ge", "Pfei\u00b7fe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "Also schaut mein Pl\u00e4tzchen aus,", "tokens": ["Al\u00b7so", "schaut", "mein", "Pl\u00e4tz\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "'s liebste mir auf Erden.", "tokens": ["'s", "liebs\u00b7te", "mir", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "W\u00e4r' ich nicht ein lustig Haus,", "tokens": ["W\u00e4r'", "ich", "nicht", "ein", "lus\u00b7tig", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "M\u00f6cht ich Klausner werden.", "tokens": ["M\u00f6cht", "ich", "Klaus\u00b7ner", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}