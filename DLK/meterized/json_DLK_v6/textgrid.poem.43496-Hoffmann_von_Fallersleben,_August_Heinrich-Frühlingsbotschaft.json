{"textgrid.poem.43496": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsbotschaft", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kuckuck, Kuckuck ruft aus dem Wald:", "tokens": ["Ku\u00b7ckuck", ",", "Ku\u00b7ckuck", "ruft", "aus", "dem", "Wald", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Lasset uns singen,", "tokens": ["Las\u00b7set", "uns", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Tanzen und springen!", "tokens": ["Tan\u00b7zen", "und", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Fr\u00fchling, Fr\u00fchling wird es nun bald.", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", "wird", "es", "nun", "bald", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Kuckuck, Kuckuck l\u00e4\u00dft nicht sein Schrei'n:", "tokens": ["Ku\u00b7ckuck", ",", "Ku\u00b7ckuck", "l\u00e4\u00dft", "nicht", "sein", "Schrei'n", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Kommt in die Felder,", "tokens": ["Kommt", "in", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wiesen und W\u00e4lder!", "tokens": ["Wie\u00b7sen", "und", "W\u00e4l\u00b7der", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Fr\u00fchling, Fr\u00fchling, stelle dich ein!", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", ",", "stel\u00b7le", "dich", "ein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Kuckuck, Kuckuck, trefflicher Held!", "tokens": ["Ku\u00b7ckuck", ",", "Ku\u00b7ckuck", ",", "treff\u00b7li\u00b7cher", "Held", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Was du gesungen,", "tokens": ["Was", "du", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ist dir gelungen:", "tokens": ["Ist", "dir", "ge\u00b7lun\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Winter, Winter r\u00e4umet das Feld.", "tokens": ["Win\u00b7ter", ",", "Win\u00b7ter", "r\u00e4u\u00b7met", "das", "Feld", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}