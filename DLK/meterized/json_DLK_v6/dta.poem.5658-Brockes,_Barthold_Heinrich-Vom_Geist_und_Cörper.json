{"dta.poem.5658": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df soll man offtermahls bedencken:", "tokens": ["Die\u00df", "soll", "man", "off\u00b7ter\u00b7mahls", "be\u00b7den\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein C\u00f6rper seyn; heist: Theile haben,", "tokens": ["Ein", "C\u00f6r\u00b7per", "seyn", ";", "heist", ":", "Thei\u00b7le", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$.", "VVFIN", "$.", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die man zertrennen kan, versetzen und umschr\u00e4ncken.", "tokens": ["Die", "man", "zer\u00b7tren\u00b7nen", "kan", ",", "ver\u00b7set\u00b7zen", "und", "um\u00b7schr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Geist zu seyn hingegen", "tokens": ["Ein", "Geist", "zu", "seyn", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VAINF", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Heist f\u00fchlen, wehlen, \u00fcberlegen,", "tokens": ["Heist", "f\u00fch\u00b7len", ",", "weh\u00b7len", ",", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVINF", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Heist auf dasjenige, was man gef\u00fchlet, achten,", "tokens": ["Heist", "auf", "das\u00b7je\u00b7ni\u00b7ge", ",", "was", "man", "ge\u00b7f\u00fch\u00b7let", ",", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PDS", "$,", "PRELS", "PIS", "VVPP", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich selbst verstehn, erkennen und betrachten.", "tokens": ["Sich", "selbst", "ver\u00b7stehn", ",", "er\u00b7ken\u00b7nen", "und", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "$,", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}