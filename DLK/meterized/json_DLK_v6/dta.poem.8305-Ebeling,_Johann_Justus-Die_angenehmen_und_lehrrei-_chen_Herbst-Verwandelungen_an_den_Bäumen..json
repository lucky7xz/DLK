{"dta.poem.8305": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die angenehmen und lehrrei-  \n chen Herbst-Verwandelungen an  \n den B\u00e4umen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nunmehr ist im Reich der Floren,", "tokens": ["Nun\u00b7mehr", "ist", "im", "Reich", "der", "Flo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der smaragdne Schmuk, das Gr\u00fcn,", "tokens": ["Der", "sma\u00b7ragd\u00b7ne", "Schmuk", ",", "das", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als der Hofnung Bild verlohren,", "tokens": ["Als", "der", "Hof\u00b7nung", "Bild", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das an denen B\u00e4umen schien;", "tokens": ["Das", "an", "de\u00b7nen", "B\u00e4u\u00b7men", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da der Herbst die welken Bl\u00e4tter", "tokens": ["Da", "der", "Herbst", "die", "wel\u00b7ken", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In ein schimmernd Gold verkehrt,", "tokens": ["In", "ein", "schim\u00b7mernd", "Gold", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind sie nur versch\u00f6nert, netter", "tokens": ["Sind", "sie", "nur", "ver\u00b7sch\u00f6\u00b7nert", ",", "net\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und in ihren Glanz vermehrt,", "tokens": ["Und", "in", "ih\u00b7ren", "Glanz", "ver\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da die heitren Sonnenstrahlen,", "tokens": ["Da", "die", "hei\u00b7tren", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie wie die Rubinen mahlen.", "tokens": ["Sie", "wie", "die", "Ru\u00b7bi\u00b7nen", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aller Dinge Aenderungen", "tokens": ["Al\u00b7ler", "Din\u00b7ge", "A\u00b7en\u00b7de\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "--+----+-", "measure": "anapaest.init"}, "line.2": {"text": "In dem Reiche der Natur,", "tokens": ["In", "dem", "Rei\u00b7che", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind nur blos Verwandelungen", "tokens": ["Sind", "nur", "blos", "Ver\u00b7wan\u00b7de\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wechselreicher Kreatur:", "tokens": ["Wech\u00b7sel\u00b7rei\u00b7cher", "Kre\u00b7a\u00b7tur", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was uns scheint als wie verlohren,", "tokens": ["Was", "uns", "scheint", "als", "wie", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KOUS", "KOKOM", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird im Kreislauf dieser Zeit,", "tokens": ["Wird", "im", "Kreis\u00b7lauf", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Anders wiederum gebohren,", "tokens": ["An\u00b7ders", "wie\u00b7de\u00b7rum", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und als unzerst\u00f6hrt verneut:", "tokens": ["Und", "als", "un\u00b7zer\u00b7st\u00f6hrt", "ver\u00b7neut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dieses kan man kl\u00e4rlich zeigen,", "tokens": ["Die\u00b7ses", "kan", "man", "kl\u00e4r\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "An dem Schmuk der Aest und Zweigen.", "tokens": ["An", "dem", "Schmuk", "der", "A\u00b7est", "und", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Welche sch\u00f6ne Augenweide,", "tokens": ["Wel\u00b7che", "sch\u00f6\u00b7ne", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man in die G\u00e4rten blikt,", "tokens": ["Wenn", "man", "in", "die", "G\u00e4r\u00b7ten", "blikt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo zuvor im gr\u00fcnen Kleide,", "tokens": ["Wo", "zu\u00b7vor", "im", "gr\u00fc\u00b7nen", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uns der B\u00e4ume Pracht entz\u00fckt:", "tokens": ["Uns", "der", "B\u00e4u\u00b7me", "Pracht", "ent\u00b7z\u00fckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nunmehr ist das Gr\u00fcn entflogen", "tokens": ["Nun\u00b7mehr", "ist", "das", "Gr\u00fcn", "ent\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Laub im hellen Glanz", "tokens": ["Und", "das", "Laub", "im", "hel\u00b7len", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als mit Goldblech \u00fcberzogen,", "tokens": ["Als", "mit", "Gold\u00b7blech", "\u00fc\u00b7berz\u00b7o\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Gipfel runder Kranz,", "tokens": ["Und", "der", "Gip\u00b7fel", "run\u00b7der", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Scheinet in der heitren Ferne,", "tokens": ["Schei\u00b7net", "in", "der", "hei\u00b7tren", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Als wenn er voll g\u00fcldner Sterne.", "tokens": ["Als", "wenn", "er", "voll", "g\u00fcld\u00b7ner", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sieh O! Mensch die weise Weise", "tokens": ["Sieh", "O", "!", "Mensch", "die", "wei\u00b7se", "Wei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsers Sch\u00f6pfers hierin an,", "tokens": ["Un\u00b7sers", "Sch\u00f6p\u00b7fers", "hie\u00b7rin", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und erwege ihm zum Preise,", "tokens": ["Und", "er\u00b7we\u00b7ge", "ihm", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was sein Allmachts-Finger kan!", "tokens": ["Was", "sein", "All\u00b7machts\u00b7Fin\u00b7ger", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da der Fr\u00fchling ist verschwunden,", "tokens": ["Da", "der", "Fr\u00fch\u00b7ling", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Sommerszeit vorbei", "tokens": ["Und", "die", "Som\u00b7mers\u00b7zeit", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Herbst sich eingefunden:", "tokens": ["Und", "der", "Herbst", "sich", "ein\u00b7ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird der Hofnung Liverei,", "tokens": ["Wird", "der", "Hof\u00b7nung", "Li\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da nun die Erf\u00fcllung kommen,", "tokens": ["Da", "nun", "die", "Er\u00b7f\u00fcl\u00b7lung", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Weislich wieder abgenommen.", "tokens": ["Weis\u00b7lich", "wie\u00b7der", "ab\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Diese Welt das Schauger\u00fcste", "tokens": ["Die\u00b7se", "Welt", "das", "Schau\u00b7ge\u00b7r\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist stets herrlich ausgeziert,", "tokens": ["Ist", "stets", "herr\u00b7lich", "aus\u00b7ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da ein Vorwurf s\u00fcsser L\u00fcste,", "tokens": ["Da", "ein", "Vor\u00b7wurf", "s\u00fcs\u00b7ser", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die begiergen Sinnen r\u00fchrt;", "tokens": ["Die", "be\u00b7gier\u00b7gen", "Sin\u00b7nen", "r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn wir etwas gnug besehen,", "tokens": ["Wenn", "wir", "et\u00b7was", "gnug", "be\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df es, ehe man es meint,", "tokens": ["Mu\u00df", "es", ",", "e\u00b7he", "man", "es", "meint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich verwandeln, nicht vergehen,", "tokens": ["Sich", "ver\u00b7wan\u00b7deln", ",", "nicht", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da ein andrer Schmuk erscheint,", "tokens": ["Da", "ein", "an\u00b7drer", "Schmuk", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Den das Aug mit Lust erblikket,", "tokens": ["Den", "das", "Aug", "mit", "Lust", "er\u00b7blik\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenn er wieder vorger\u00fckket.", "tokens": ["Wenn", "er", "wie\u00b7der", "vor\u00b7ge\u00b7r\u00fck\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "G\u00e4rten sind im Erdenkreise", "tokens": ["G\u00e4r\u00b7ten", "sind", "im", "Er\u00b7den\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein Schauplatz anzusehn;", "tokens": ["Als", "ein", "Schau\u00b7platz", "an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ein sch\u00f6nes Lust-Geh\u00e4use:", "tokens": ["Als", "ein", "sch\u00f6\u00b7nes", "Lust\u00b7Ge\u00b7h\u00e4u\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die B\u00e4ume die da stehn,", "tokens": ["Und", "die", "B\u00e4u\u00b7me", "die", "da", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADV", "VVINF", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Sind in ihrem holden Gr\u00fcnen", "tokens": ["Sind", "in", "ih\u00b7rem", "hol\u00b7den", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns zur Augenlust gemacht,", "tokens": ["Uns", "zur", "Au\u00b7gen\u00b7lust", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und als sch\u00f6ne Kunst-Maschinen", "tokens": ["Und", "als", "sch\u00f6\u00b7ne", "Kunst\u00b7Ma\u00b7schi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit der nett belaubten Pracht,", "tokens": ["Mit", "der", "nett", "be\u00b7laub\u00b7ten", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Die sich oft gleichsam verdrehen,", "tokens": ["Die", "sich", "oft", "gleich\u00b7sam", "ver\u00b7dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Und in neuer Farb aufgehen.", "tokens": ["Und", "in", "neu\u00b7er", "Farb", "auf\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn beim hellen Sonnenlichte", "tokens": ["Wenn", "beim", "hel\u00b7len", "Son\u00b7nen\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns der Herbst die B\u00e4ume zeigt,", "tokens": ["Uns", "der", "Herbst", "die", "B\u00e4u\u00b7me", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein aufmerksam Gesichte", "tokens": ["Und", "ein", "auf\u00b7merk\u00b7sam", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seine Blikke darauf neigt:", "tokens": ["Sei\u00b7ne", "Blik\u00b7ke", "da\u00b7rauf", "neigt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So kan man bei stillen Freuden,", "tokens": ["So", "kan", "man", "bei", "stil\u00b7len", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "An der bunten Zierlichkeit", "tokens": ["An", "der", "bun\u00b7ten", "Zier\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aug und Herze reichlich weiden,", "tokens": ["Aug", "und", "Her\u00b7ze", "reich\u00b7lich", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da zu dieser Jahres-Zeit", "tokens": ["Da", "zu", "die\u00b7ser", "Jah\u00b7res\u00b7Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Das, was uns zuvor erg\u00f6tzet,", "tokens": ["Das", ",", "was", "uns", "zu\u00b7vor", "er\u00b7g\u00f6t\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Noch in die Verwundrung sezzet.", "tokens": ["Noch", "in", "die", "Ver\u00b7wund\u00b7rung", "sez\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn der Nord f\u00e4ngt an zu rasen,", "tokens": ["Wenn", "der", "Nord", "f\u00e4ngt", "an", "zu", "ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliegt die bunte Herrlichkeit,", "tokens": ["Fliegt", "die", "bun\u00b7te", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wird durch sein hauchend Blasen,", "tokens": ["Und", "wird", "durch", "sein", "hau\u00b7chend", "Bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Auf den falben Grund gestreut,", "tokens": ["Auf", "den", "fal\u00b7ben", "Grund", "ge\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da mu\u00df dieses schimmernd Prangen,", "tokens": ["Da", "mu\u00df", "die\u00b7ses", "schim\u00b7mernd", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PDAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch mit Gold die Erde ziern,", "tokens": ["Noch", "mit", "Gold", "die", "Er\u00b7de", "zi\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis es in den Staub vergangen", "tokens": ["Bis", "es", "in", "den", "Staub", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da wir es gar bald verliern;", "tokens": ["Da", "wir", "es", "gar", "bald", "ver\u00b7li\u00b7ern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Da es in gar wenig Stunden,", "tokens": ["Da", "es", "in", "gar", "we\u00b7nig", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie ein scheinend Nichts verschwunden.", "tokens": ["Wie", "ein", "schei\u00b7nend", "Nichts", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sch\u00f6nes Bild von unsern Leben!", "tokens": ["Sch\u00f6\u00b7nes", "Bild", "von", "un\u00b7sern", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df uns \u00f6fters herrlich scheint,", "tokens": ["Da\u00df", "uns", "\u00f6f\u00b7ters", "herr\u00b7lich", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch viele Lust kan geben,", "tokens": ["Und", "auch", "vie\u00b7le", "Lust", "kan", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das doch aber eh mans meint,", "tokens": ["Das", "doch", "a\u00b7ber", "eh", "mans", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Jahre Lauf vergehet:", "tokens": ["Mit", "der", "Jah\u00b7re", "Lauf", "ver\u00b7ge\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da die Sch\u00f6nheit, Pracht, und Geld", "tokens": ["Da", "die", "Sch\u00f6n\u00b7heit", ",", "Pracht", ",", "und", "Geld"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "NN", "$,", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wie ein welkend Laub verwehet,", "tokens": ["Wie", "ein", "wel\u00b7kend", "Laub", "ver\u00b7we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und zulezt noch in der Welt,", "tokens": ["Und", "zu\u00b7lezt", "noch", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn sie sich von uns entfernen,", "tokens": ["Wenn", "sie", "sich", "von", "uns", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lassen, da\u00df sie eitel lernen.", "tokens": ["Las\u00b7sen", ",", "da\u00df", "sie", "ei\u00b7tel", "ler\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In der Jugend holden Lenzen,", "tokens": ["In", "der", "Ju\u00b7gend", "hol\u00b7den", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fchen wir mit Anmuht sch\u00f6n,", "tokens": ["Bl\u00fc\u00b7hen", "wir", "mit", "An\u00b7muht", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da wir in den gr\u00fcnen Kr\u00e4nzen,", "tokens": ["Da", "wir", "in", "den", "gr\u00fc\u00b7nen", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hofnungs-voll dem Gl\u00fck nachgehn.", "tokens": ["Hof\u00b7nungs\u00b7voll", "dem", "Gl\u00fck", "nach\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Gl\u00fckkes-Sonn uns scheinet,", "tokens": ["Wenn", "die", "Gl\u00fc\u00b7kkes\u00b7Sonn", "uns", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind wir einem Baume gleich,", "tokens": ["Sind", "wir", "ei\u00b7nem", "Bau\u00b7me", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Davon man im Sommer meinet,", "tokens": ["Da\u00b7von", "man", "im", "Som\u00b7mer", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Da\u00df er pr\u00e4chtig, fruchtbahr, reich,", "tokens": ["Da\u00df", "er", "pr\u00e4ch\u00b7tig", ",", "frucht\u00b7bahr", ",", "reich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Der zulezt h\u00f6rt auf zu prangen,", "tokens": ["Der", "zu\u00b7lezt", "h\u0