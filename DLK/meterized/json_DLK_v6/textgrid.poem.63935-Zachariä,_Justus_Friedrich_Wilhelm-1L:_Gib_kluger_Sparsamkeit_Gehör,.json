{"textgrid.poem.63935": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gib kluger Sparsamkeit Geh\u00f6r,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib kluger Sparsamkeit Geh\u00f6r,", "tokens": ["Gib", "klu\u00b7ger", "Spar\u00b7sam\u00b7keit", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rechne lieber Jahre mehr,", "tokens": ["Und", "rech\u00b7ne", "lie\u00b7ber", "Jah\u00b7re", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als du vielleicht zu leben hast;", "tokens": ["Als", "du", "viel\u00b7leicht", "zu", "le\u00b7ben", "hast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit dich nicht des Mangels Last,", "tokens": ["Da\u00b7mit", "dich", "nicht", "des", "Man\u00b7gels", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf den die Jugend sorglos blickt,", "tokens": ["Auf", "den", "die", "Ju\u00b7gend", "sorg\u00b7los", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Alter doppelt schwerer dr\u00fcckt.", "tokens": ["Im", "Al\u00b7ter", "dop\u00b7pelt", "schwe\u00b7rer", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwei Elstern waren Nachbarinnen;", "tokens": ["Zwei", "Els\u00b7tern", "wa\u00b7ren", "Nach\u00b7ba\u00b7rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum wurden sie des Herbstes innen,", "tokens": ["Kaum", "wur\u00b7den", "sie", "des", "Herbs\u00b7tes", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So trugen sie mit regem Flei\u00dfe", "tokens": ["So", "tru\u00b7gen", "sie", "mit", "re\u00b7gem", "Flei\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sich auf den Winter ihre Speise", "tokens": ["Sich", "auf", "den", "Win\u00b7ter", "ih\u00b7re", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An guten Eicheln, braunen N\u00fcssen,", "tokens": ["An", "gu\u00b7ten", "Ei\u00b7cheln", ",", "brau\u00b7nen", "N\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und was sonst Elstern haben m\u00fcssen,", "tokens": ["Und", "was", "sonst", "Els\u00b7tern", "ha\u00b7ben", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Jedwed' in einen hohlen Baum,", "tokens": ["Jed\u00b7wed'", "in", "ei\u00b7nen", "hoh\u00b7len", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und gaben keiner Sorge Raum.", "tokens": ["Und", "ga\u00b7ben", "kei\u00b7ner", "Sor\u00b7ge", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Winter kam vom hohen Brocken,", "tokens": ["Der", "Win\u00b7ter", "kam", "vom", "ho\u00b7hen", "Bro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das Haupt umringt mit Eis und Flocken;", "tokens": ["Das", "Haupt", "um\u00b7ringt", "mit", "Eis", "und", "Flo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der freie Strom ward pl\u00f6tzlich hart,", "tokens": ["Der", "frei\u00b7e", "Strom", "ward", "pl\u00f6tz\u00b7lich", "hart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die sterbende Natur erstarrt.", "tokens": ["Die", "ster\u00b7ben\u00b7de", "Na\u00b7tur", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.13": {"text": "Die Elstern zehrten ohne Klagen", "tokens": ["Die", "Els\u00b7tern", "zehr\u00b7ten", "oh\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Vom Vorrath, den sie eingetragen;", "tokens": ["Vom", "Vor\u00b7rath", ",", "den", "sie", "ein\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Doch flog die eine manches Mal", "tokens": ["Doch", "flog", "die", "ei\u00b7ne", "man\u00b7ches", "Mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bei'm ersten besten Sonnenstrahl", "tokens": ["Bei'm", "ers\u00b7ten", "bes\u00b7ten", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Hinaus in's Feld und suchte sich,", "tokens": ["Hin\u00b7aus", "in's", "Feld", "und", "such\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "An Rain und H\u00fcgeln, k\u00fcmmerlich,", "tokens": ["An", "Rain", "und", "H\u00fc\u00b7geln", ",", "k\u00fcm\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Was noch zu essen dienlich war.", "tokens": ["Was", "noch", "zu", "es\u00b7sen", "dien\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die andre nahm dies sp\u00f6ttisch wahr", "tokens": ["Die", "and\u00b7re", "nahm", "dies", "sp\u00f6t\u00b7tisch", "wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PDS", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und sprach: F\u00fcrwahr, Frau Nachbarin,", "tokens": ["Und", "sprach", ":", "F\u00fcr\u00b7wahr", ",", "Frau", "Nach\u00b7ba\u00b7rin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie lange denkt Ihr denn noch hin", "tokens": ["Wie", "lan\u00b7ge", "denkt", "Ihr", "denn", "noch", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Mit Eurem Vorrath auszukommen?", "tokens": ["Mit", "Eu\u00b7rem", "Vor\u00b7rath", "aus\u00b7zu\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Habt Ihr die Lerche nicht vernommen,", "tokens": ["Habt", "Ihr", "die", "Ler\u00b7che", "nicht", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die munter schon im Saatfeld singt", "tokens": ["Die", "mun\u00b7ter", "schon", "im", "Saat\u00b7feld", "singt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und uns den Fr\u00fchling wieder bringt?", "tokens": ["Und", "uns", "den", "Fr\u00fch\u00b7ling", "wie\u00b7der", "bringt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Winter kann nicht l\u00e4nger w\u00e4hren,", "tokens": ["Der", "Win\u00b7ter", "kann", "nicht", "l\u00e4n\u00b7ger", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und sicher k\u00f6nntet Ihr verzehren,", "tokens": ["Und", "si\u00b7cher", "k\u00f6nn\u00b7tet", "Ihr", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Was hier schon aufgesammelt ist,", "tokens": ["Was", "hier", "schon", "auf\u00b7ge\u00b7sam\u00b7melt", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und sonst verdirbt und Niemand fri\u00dft.", "tokens": ["Und", "sonst", "ver\u00b7dirbt", "und", "Nie\u00b7mand", "fri\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Lebt so wie ich in Freud und Scherz.", "tokens": ["Lebt", "so", "wie", "ich", "in", "Freud", "und", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Denn neue Nahrung bringt der M\u00e4rz.", "tokens": ["Denn", "neu\u00b7e", "Nah\u00b7rung", "bringt", "der", "M\u00e4rz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, (sprach die andere darauf)", "tokens": ["Ja", ",", "(", "sprach", "die", "an\u00b7de\u00b7re", "da\u00b7rauf", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "$(", "VVFIN", "ART", "PIS", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Schein nach h\u00f6rt der Winter auf;", "tokens": ["Dem", "Schein", "nach", "h\u00f6rt", "der", "Win\u00b7ter", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, uns zum gr\u00f6\u00dften Ungemach,", "tokens": ["Doch", ",", "uns", "zum", "gr\u00f6\u00df\u00b7ten", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt oft ein sp\u00e4ter Frost noch nach.", "tokens": ["Kommt", "oft", "ein", "sp\u00e4\u00b7ter", "Frost", "noch", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bleibt mir was \u00fcbrig, nun wohlan!", "tokens": ["Bleibt", "mir", "was", "\u00fcb\u00b7rig", ",", "nun", "wo\u00b7hlan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was ich nicht selbst verzehren kann,", "tokens": ["Was", "ich", "nicht", "selbst", "ver\u00b7zeh\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wird unter dieses Baumes Rinden", "tokens": ["Wird", "un\u00b7ter", "die\u00b7ses", "Bau\u00b7mes", "Rin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Noch immer seinen Mann wohl finden.", "tokens": ["Noch", "im\u00b7mer", "sei\u00b7nen", "Mann", "wohl", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie hatte Recht. Denn pl\u00f6tzlich kam,", "tokens": ["Sie", "hat\u00b7te", "Recht", ".", "Denn", "pl\u00f6tz\u00b7lich", "kam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schon der Lenz den Anfang nahm,", "tokens": ["Da", "schon", "der", "Lenz", "den", "An\u00b7fang", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein neuer Winter. Tiefer Schnee", "tokens": ["Ein", "neu\u00b7er", "Win\u00b7ter", ".", "Tie\u00b7fer", "Schnee"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bedeckte traurig Thal und H\u00f6h',", "tokens": ["Be\u00b7deck\u00b7te", "trau\u00b7rig", "Thal", "und", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lag verschied'ne Wochen lang", "tokens": ["Und", "lag", "ver\u00b7schie\u00b7d'\u00b7ne", "Wo\u00b7chen", "lang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zu manches Thieres Untergang.", "tokens": ["Zu", "man\u00b7ches", "Thie\u00b7res", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Lenz erschien, wie man gedacht;", "tokens": ["Kein", "Lenz", "er\u00b7schien", ",", "wie", "man", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PWAV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Hunger kam mit ganzer Macht,", "tokens": ["Der", "Hun\u00b7ger", "kam", "mit", "gan\u00b7zer", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ihre Nachbarin erfriert,", "tokens": ["Und", "ih\u00b7re", "Nach\u00b7ba\u00b7rin", "er\u00b7friert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Weil sie nicht richtig kalkulirt.", "tokens": ["Weil", "sie", "nicht", "rich\u00b7tig", "kal\u00b7ku\u00b7lirt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}