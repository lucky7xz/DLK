{"dta.poem.1262": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Gem\u00fctte/ welches mehr als Wind und See zu fliehen/", "tokens": ["Ge\u00b7m\u00fct\u00b7te", "/", "wel\u00b7ches", "mehr", "als", "Wind", "und", "See", "zu", "flie\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PWS", "ADV", "KOUS", "NN", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das mich geben lernt der Liebe gutte Nacht/", "tokens": ["Und", "das", "mich", "ge\u00b7ben", "lernt", "der", "Lie\u00b7be", "gut\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVINF", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht hoffe/ da\u00df du mich/ nachdem ich frey gemacht", "tokens": ["Nicht", "hof\u00b7fe", "/", "da\u00df", "du", "mich", "/", "nach\u00b7dem", "ich", "frey", "ge\u00b7macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "$(", "KOUS", "PPER", "PRF", "$(", "KOUS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wirst wieder an dich ziehen.", "tokens": ["Wirst", "wie\u00b7der", "an", "dich", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Th\u00f6richt mu\u00df der Schiffer seyn/", "tokens": ["Th\u00f6\u00b7richt", "mu\u00df", "der", "Schif\u00b7fer", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VAINF", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der dem Schiffbruch ist entgangen/", "tokens": ["Der", "dem", "Schiff\u00b7bruch", "ist", "ent\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und an einem Felsen-Stein", "tokens": ["Und", "an", "ei\u00b7nem", "Fel\u00b7sen\u00b7Stein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch das zweyte mahl bleibt hangen.", "tokens": ["Noch", "das", "zwey\u00b7te", "mahl", "bleibt", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADV", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer will auff glattes Ei\u00df und deine Worte bauen?", "tokens": ["Wer", "will", "auff", "glat\u00b7tes", "Ei\u00df", "und", "dei\u00b7ne", "Wor\u00b7te", "bau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je mehr man schleust die Hand/ ie minder man dich h\u00e4lt/", "tokens": ["Je", "mehr", "man", "schleust", "die", "Hand", "/", "ie", "min\u00b7der", "man", "dich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VVFIN", "ART", "NN", "$(", "ADV", "ADV", "PIS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ungl\u00fccklich/ wen der Schlu\u00df des Himmels hat bestellt/", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "/", "wen", "der", "Schlu\u00df", "des", "Him\u00b7mels", "hat", "be\u00b7stellt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PWS", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach dir sich umzuschauen.", "tokens": ["Nach", "dir", "sich", "um\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du bist ihm zur Qual bestimmt/", "tokens": ["Du", "bist", "ihm", "zur", "Qual", "be\u00b7stimmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wie der Stein in Sisiffs H\u00e4nden/", "tokens": ["Wie", "der", "Stein", "in", "Si\u00b7siffs", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NE", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der/ wenn er zur Spitze k\u00fcmmt/", "tokens": ["Der", "/", "wenn", "er", "zur", "Spit\u00b7ze", "k\u00fcmmt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wieder pfleget umzuwenden.", "tokens": ["Wie\u00b7der", "pfle\u00b7get", "um\u00b7zu\u00b7wen\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Sinn/ der ohne Wahl und kurtze Stunden lie-", "tokens": ["Dein", "Sinn", "/", "der", "oh\u00b7ne", "Wahl", "und", "kurt\u00b7ze", "Stun\u00b7den", "lie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "ART", "APPR", "NN", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat unter so viel Glutt erstecket meinen Brand/", "tokens": ["Hat", "un\u00b7ter", "so", "viel", "Glutt", "er\u00b7ste\u00b7cket", "mei\u00b7nen", "Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und mir darvor di\u00df Ei\u00df/ die K\u00e4lte/ zugewandt", "tokens": ["Und", "mir", "dar\u00b7vor", "di\u00df", "Ei\u00df", "/", "die", "K\u00e4l\u00b7te", "/", "zu\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "PAV", "PDS", "NN", "$(", "ART", "NN", "$(", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die meine Brust umgiebet.", "tokens": ["Die", "mei\u00b7ne", "Brust", "um\u00b7gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Deine Fessel sind entzwey/", "tokens": ["Dei\u00b7ne", "Fes\u00b7sel", "sind", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Ketten sind gebrochen/", "tokens": ["Dei\u00b7ne", "Ket\u00b7ten", "sind", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hylas ist der Bande frey/", "tokens": ["Hy\u00b7las", "ist", "der", "Ban\u00b7de", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und von Fillis lo\u00dfgesprochen.", "tokens": ["Und", "von", "Fil\u00b7lis", "lo\u00df\u00b7ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Izt soll ein Lorber-Krantz mein k\u00fchnes Haubt umg\u00fcr-", "tokens": ["Izt", "soll", "ein", "Lor\u00b7ber\u00b7Krantz", "mein", "k\u00fch\u00b7nes", "Haubt", "um\u00b7g\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und meiner Freyheit Ruhm erh\u00f6hn das gantze Rund.", "tokens": ["Und", "mei\u00b7ner", "Frey\u00b7heit", "Ruhm", "er\u00b7h\u00f6hn", "das", "gant\u00b7ze", "Rund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Izt soll mein Palmen-Zweig der Erde machen kundt", "tokens": ["Izt", "soll", "mein", "Pal\u00b7men\u00b7Zweig", "der", "Er\u00b7de", "ma\u00b7chen", "kundt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Schwachheit deiner Myrthen.", "tokens": ["Die", "Schwach\u00b7heit", "dei\u00b7ner", "Myr\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Fama gr\u00e4bt in Marmor ein", "tokens": ["Fa\u00b7ma", "gr\u00e4bt", "in", "Mar\u00b7mor", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir zum stetem Sieges-Zeichen", "tokens": ["Mir", "zum", "ste\u00b7tem", "Sie\u00b7ges\u00b7Zei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Flammen/ die ein Hertz bestreichen/", "tokens": ["Flam\u00b7men", "/", "die", "ein", "Hertz", "be\u00b7strei\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber ausgeloschen seyn.", "tokens": ["A\u00b7ber", "aus\u00b7ge\u00b7lo\u00b7schen", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}