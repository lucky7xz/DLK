{"dta.poem.9589": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie will ihn mahlen lehren.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Clorinden kam ohnl\u00e4ngst die lust zu mahlen an/", "tokens": ["Clo\u00b7rin\u00b7den", "kam", "ohn\u00b7l\u00e4ngst", "die", "lust", "zu", "mah\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sprach: komm her/ es ist im augenblick gethan", "tokens": ["Sie", "sprach", ":", "komm", "her", "/", "es", "ist", "im", "au\u00b7gen\u00b7blick", "ge\u00b7than"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PTKVZ", "$(", "PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was giebstu/ da\u00df ich dich im zeichnen unterrichte?", "tokens": ["Was", "giebs\u00b7tu", "/", "da\u00df", "ich", "dich", "im", "zeich\u00b7nen", "un\u00b7ter\u00b7rich\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Drauff zog sie mich zu sich vor ihren spiegel hin/", "tokens": ["Drauff", "zog", "sie", "mich", "zu", "sich", "vor", "ih\u00b7ren", "spie\u00b7gel", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und zeigte mir so fort ihr englisches gesichte/", "tokens": ["Und", "zeig\u00b7te", "mir", "so", "fort", "ihr", "eng\u00b7li\u00b7sches", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den anmuths-vollen mund/ das wohlgestalte kin/", "tokens": ["Den", "an\u00b7muths\u00b7vol\u00b7len", "mund", "/", "das", "wohl\u00b7ge\u00b7stal\u00b7te", "kin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die zunderreiche glut der purpurfarbnen wangen/", "tokens": ["Die", "zun\u00b7der\u00b7rei\u00b7che", "glut", "der", "pur\u00b7purf\u00b7arb\u00b7nen", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der h\u00e4nde weissen schnee/ der haare lichtes prangen/", "tokens": ["Der", "h\u00e4n\u00b7de", "weis\u00b7sen", "schnee", "/", "der", "haa\u00b7re", "lich\u00b7tes", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des leibes treffligkeit/ der stirne hohen sitz/", "tokens": ["Des", "lei\u00b7bes", "tref\u00b7flig\u00b7keit", "/", "der", "stir\u00b7ne", "ho\u00b7hen", "sitz", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der augen helles feur und diamantnen blitz.", "tokens": ["Der", "au\u00b7gen", "hel\u00b7les", "feur", "und", "di\u00b7a\u00b7mant\u00b7nen", "blitz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O! rieff ich/ h\u00f6r itzt auff ein solches bild zu mahlen;", "tokens": ["O", "!", "rieff", "ich", "/", "h\u00f6r", "itzt", "auff", "ein", "sol\u00b7ches", "bild", "zu", "mah\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "$(", "VVFIN", "ADV", "APPR", "ART", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sonst mu\u00df ich allzu theur den blick davon bezahlen!", "tokens": ["Sonst", "mu\u00df", "ich", "all\u00b7zu", "theur", "den", "blick", "da\u00b7von", "be\u00b7zah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKA", "ADJD", "ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}