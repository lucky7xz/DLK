{"textgrid.poem.65670": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Halt fest!", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt fest, halt fest, der Freiheit Hort, o Hellas, halt ihn fest!", "tokens": ["Halt", "fest", ",", "halt", "fest", ",", "der", "Frei\u00b7heit", "Hort", ",", "o", "Hel\u00b7las", ",", "halt", "ihn", "fest", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "ART", "NN", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Weh dir! Dir w\u00e4re besser dann, du h\u00e4ttest nie die Hand,", "tokens": ["Weh", "dir", "!", "Dir", "w\u00e4\u00b7re", "bes\u00b7ser", "dann", ",", "du", "h\u00e4t\u00b7test", "nie", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "ADV", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Nach ihm zu greifen, losgedreht aus deinem Sklavenband!", "tokens": ["Nach", "ihm", "zu", "grei\u00b7fen", ",", "los\u00b7ge\u00b7dreht", "aus", "dei\u00b7nem", "Skla\u00b7ven\u00b7band", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Halt fest, halt fest, wie Jener einst gethan, dein Heldensohn,", "tokens": ["Halt", "fest", ",", "halt", "fest", ",", "wie", "Je\u00b7ner", "einst", "ge\u00b7than", ",", "dein", "Hel\u00b7den\u00b7sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PWAV", "PDAT", "ADV", "VVPP", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Als aus dem Feld von Marathon die Perserhorden flohn.", "tokens": ["Als", "aus", "dem", "Feld", "von", "Ma\u00b7ra\u00b7thon", "die", "Per\u00b7ser\u00b7hor\u00b7den", "flohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Da fa\u00dfte der ein volles Boot hart an des Meeres Strand,", "tokens": ["Da", "fa\u00df\u00b7te", "der", "ein", "vol\u00b7les", "Boot", "hart", "an", "des", "Mee\u00b7res", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Und hielt es an dem Schnabel fest mit seiner starken Hand;", "tokens": ["Und", "hielt", "es", "an", "dem", "Schna\u00b7bel", "fest", "mit", "sei\u00b7ner", "star\u00b7ken", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Die rechte ward ihm abgehaun, da griff die linke zu,", "tokens": ["Die", "rech\u00b7te", "ward", "ihm", "ab\u00b7ge\u00b7haun", ",", "da", "griff", "die", "lin\u00b7ke", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Die link' auch fiel zu Boden hin, und flugs in einem Nu", "tokens": ["Die", "link'", "auch", "fiel", "zu", "Bo\u00b7den", "hin", ",", "und", "flugs", "in", "ei\u00b7nem", "Nu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "APPR", "NN", "PTKVZ", "$,", "KON", "ADV", "APPR", "ART", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Packt' er die Beute, wie ein Leu, mit seinen Z\u00e4hnen an,", "tokens": ["Packt'", "er", "die", "Beu\u00b7te", ",", "wie", "ein", "Leu", ",", "mit", "sei\u00b7nen", "Z\u00e4h\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Und bi\u00df sich ein, und wankte nicht, bis da\u00df er sie gewann.", "tokens": ["Und", "bi\u00df", "sich", "ein", ",", "und", "wank\u00b7te", "nicht", ",", "bis", "da\u00df", "er", "sie", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "$,", "KON", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Wenn auf dich ein der Heiden Schwarm in wilden Wogen braust!", "tokens": ["Wenn", "auf", "dich", "ein", "der", "Hei\u00b7den", "Schwarm", "in", "wil\u00b7den", "Wo\u00b7gen", "braust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Halt fest, halt fest, und mu\u00df es sein, wirf deinen wunden Leib", "tokens": ["Halt", "fest", ",", "halt", "fest", ",", "und", "mu\u00df", "es", "sein", ",", "wirf", "dei\u00b7nen", "wun\u00b7den", "Leib"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "KON", "VMFIN", "PPER", "VAINF", "$,", "VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Ganz \u00fcber ihn und blute dich zu Tod, als freies Weib!", "tokens": ["Ganz", "\u00fc\u00b7ber", "ihn", "und", "blu\u00b7te", "dich", "zu", "Tod", ",", "als", "frei\u00b7es", "Weib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "KON", "VVFIN", "PRF", "APPR", "NN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}