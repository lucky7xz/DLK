{"textgrid.poem.35236": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach der Blumen schneller Flucht", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach der Blumen schneller Flucht", "tokens": ["Nach", "der", "Blu\u00b7men", "schnel\u00b7ler", "Flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Prangt die r\u00f6thlich gelbe Frucht,", "tokens": ["Prangt", "die", "r\u00f6th\u00b7lich", "gel\u00b7be", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die laubbegr\u00fcnten Reben", "tokens": ["Und", "die", "laub\u00b7be\u00b7gr\u00fcn\u00b7ten", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schenken Freudenbecher ein.", "tokens": ["Schen\u00b7ken", "Freu\u00b7den\u00b7be\u00b7cher", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, es ist der Menschen Leben", "tokens": ["Ach", ",", "es", "ist", "der", "Men\u00b7schen", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weh' und Weinen ohne Wein!", "tokens": ["Weh'", "und", "Wei\u00b7nen", "oh\u00b7ne", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In dem kalten Nordenland", "tokens": ["In", "dem", "kal\u00b7ten", "Nor\u00b7den\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ber\u00fchmt das Pelzgewand.", "tokens": ["Ist", "be\u00b7r\u00fchmt", "das", "Pelz\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcchse, Marder, B\u00e4renh\u00e4ute,", "tokens": ["F\u00fcch\u00b7se", ",", "Mar\u00b7der", ",", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zobel, Luchs und Reihenthier,", "tokens": ["Zo\u00b7bel", ",", "Luchs", "und", "Rei\u00b7hen\u00b7thier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hitzen selber rauhe Leute,", "tokens": ["Hit\u00b7zen", "sel\u00b7ber", "rau\u00b7he", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Wein uns w\u00e4rmet hier.", "tokens": ["Wie", "der", "Wein", "uns", "w\u00e4r\u00b7met", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In dem hei\u00dfen S\u00fcdenland", "tokens": ["In", "dem", "hei\u00b7\u00dfen", "S\u00fc\u00b7den\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt der Sonnenstrahlen Brand", "tokens": ["Bringt", "der", "Son\u00b7nen\u00b7strah\u00b7len", "Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pomeranzen, Oel, Granaten,", "tokens": ["Po\u00b7me\u00b7ran\u00b7zen", ",", "O\u00b7el", ",", "Gra\u00b7na\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Pfeben", "tokens": ["Pfe\u00b7ben"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Sie zu k\u00fchlen, wann sie braten:", "tokens": ["Sie", "zu", "k\u00fch\u00b7len", ",", "wann", "sie", "bra\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns beliebt der Wein allhier.", "tokens": ["Uns", "be\u00b7liebt", "der", "Wein", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Von der Donau bis zum Rhein", "tokens": ["Von", "der", "Do\u00b7nau", "bis", "zum", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "APPR", "APPRART", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4get jeder H\u00fcgel Wein,", "tokens": ["Tr\u00e4\u00b7get", "je\u00b7der", "H\u00fc\u00b7gel", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und viel Eichen, zu befassen", "tokens": ["Und", "viel", "Ei\u00b7chen", ",", "zu", "be\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solchen s\u00fc\u00dfen Keltersaft.", "tokens": ["Sol\u00b7chen", "s\u00fc\u00b7\u00dfen", "Kel\u00b7ter\u00b7saft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer will dann die Deutschen hassen,", "tokens": ["Wer", "will", "dann", "die", "Deut\u00b7schen", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn sie lieben diese Kraft?", "tokens": ["Wenn", "sie", "lie\u00b7ben", "die\u00b7se", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht, wir folgen der Natur", "tokens": ["Seht", ",", "wir", "fol\u00b7gen", "der", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und betreten ihre Spur,", "tokens": ["Und", "be\u00b7tre\u00b7ten", "ih\u00b7re", "Spur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn wir unsre kalten M\u00e4gen", "tokens": ["Wenn", "wir", "uns\u00b7re", "kal\u00b7ten", "M\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der sauern Arbeitzeit", "tokens": ["Nach", "der", "sau\u00b7ern", "Ar\u00b7beit\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hitzen mit dem Winzersegen", "tokens": ["Hit\u00b7zen", "mit", "dem", "Win\u00b7zer\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In beschr\u00e4nkter Fr\u00f6hlichkeit.", "tokens": ["In", "be\u00b7schr\u00e4nk\u00b7ter", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}