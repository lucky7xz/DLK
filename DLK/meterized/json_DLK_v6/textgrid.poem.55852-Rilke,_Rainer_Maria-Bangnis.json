{"textgrid.poem.55852": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Bangnis", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im welken Walde ist ein Vogelruf,", "tokens": ["Im", "wel\u00b7ken", "Wal\u00b7de", "ist", "ein", "Vo\u00b7gel\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der sinnlos scheint in diesem welken Walde.", "tokens": ["der", "sinn\u00b7los", "scheint", "in", "die\u00b7sem", "wel\u00b7ken", "Wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dennoch ruht der runde Vogelruf", "tokens": ["Und", "den\u00b7noch", "ruht", "der", "run\u00b7de", "Vo\u00b7gel\u00b7ruf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in dieser Weile, die ihn schuf,", "tokens": ["in", "die\u00b7ser", "Wei\u00b7le", ",", "die", "ihn", "schuf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "breit wie ein Himmel auf dem welken Walde.", "tokens": ["breit", "wie", "ein", "Him\u00b7mel", "auf", "dem", "wel\u00b7ken", "Wal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gef\u00fcgig r\u00e4umt sich alles in den Schrei:", "tokens": ["Ge\u00b7f\u00fc\u00b7gig", "r\u00e4umt", "sich", "al\u00b7les", "in", "den", "Schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das ganze Land scheint lautlos drin zu liegen,", "tokens": ["Das", "gan\u00b7ze", "Land", "scheint", "laut\u00b7los", "drin", "zu", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "der gro\u00dfe Wind scheint sich hineinzuschmiegen,", "tokens": ["der", "gro\u00b7\u00dfe", "Wind", "scheint", "sich", "hin\u00b7ein\u00b7zu\u00b7schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "und die Minute, welche weiter will,", "tokens": ["und", "die", "Mi\u00b7nu\u00b7te", ",", "wel\u00b7che", "wei\u00b7ter", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "ist bleich und still, als ob sie Dinge w\u00fc\u00dfte,", "tokens": ["ist", "bleich", "und", "still", ",", "als", "ob", "sie", "Din\u00b7ge", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "an denen jeder sterben m\u00fc\u00dfte,", "tokens": ["an", "de\u00b7nen", "je\u00b7der", "ster\u00b7ben", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "aus ihm herausgestiegen.", "tokens": ["aus", "ihm", "her\u00b7aus\u00b7ge\u00b7stie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}