{"textgrid.poem.64587": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein rosenfarben neu Gewand,", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein rosenfarben neu Gewand,", "tokens": ["Dein", "ro\u00b7sen\u00b7far\u00b7ben", "neu", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleichg\u00fcltig hab' ich's angeblickt:", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "hab'", "ich's", "an\u00b7ge\u00b7blickt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kind, wenn mich nur ein herrlich Buch entz\u00fcckt,", "tokens": ["Kind", ",", "wenn", "mich", "nur", "ein", "herr\u00b7lich", "Buch", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "So seh' ich nie auf seinen Marmorband.", "tokens": ["So", "seh'", "ich", "nie", "auf", "sei\u00b7nen", "Mar\u00b7mor\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}