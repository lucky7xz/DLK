{"textgrid.poem.54471": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geht/ geht ihr Feinde geht! beschlisset Rath vnd Rencke/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geht/ geht ihr Feinde geht! beschlisset Rath vnd Rencke/", "tokens": ["Geht", "/", "geht", "ihr", "Fein\u00b7de", "geht", "!", "be\u00b7schlis\u00b7set", "Rath", "vnd", "Ren\u00b7cke", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stellt Gottes Wei\u00dfheit nach! seyd fleissig drauff bedacht!", "tokens": ["Stellt", "Got\u00b7tes", "Wei\u00df\u00b7heit", "nach", "!", "seyd", "fleis\u00b7sig", "drauff", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "PTKVZ", "$.", "VAFIN", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Legt Stricke/ Netz vnd Garn/ ja sinnet Tag vnd Nacht:", "tokens": ["Legt", "Stri\u00b7cke", "/", "Netz", "vnd", "Garn", "/", "ja", "sin\u00b7net", "Tag", "vnd", "Nacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "NN", "KON", "NN", "$(", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr richtet doch nichts au\u00df/ wie hoch es auch euch kr\u00e4ncke!", "tokens": ["Ihr", "rich\u00b7tet", "doch", "nichts", "au\u00df", "/", "wie", "hoch", "es", "auch", "euch", "kr\u00e4n\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "PTKVZ", "$(", "PWAV", "ADJD", "PPER", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein K\u00f6nig/ dem ich mich Erb- vnd Leibeigen schencke", "tokens": ["Mein", "K\u00f6\u00b7nig", "/", "dem", "ich", "mich", "Er\u00b7b", "vnd", "Leib\u00b7ei\u00b7gen", "schen\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "PRF", "TRUNC", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Acht keinen Heuchel-Schein/ auch keiner Waffen Macht/", "tokens": ["Acht", "kei\u00b7nen", "Heu\u00b7chel\u00b7Schein", "/", "auch", "kei\u00b7ner", "Waf\u00b7fen", "Macht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PIAT", "NN", "$(", "ADV", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch keiner Menschen List/ auch keiner Zungen Pracht", "tokens": ["Auch", "kei\u00b7ner", "Men\u00b7schen", "List", "/", "auch", "kei\u00b7ner", "Zun\u00b7gen", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "NE", "$(", "ADV", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er mercket was ihr dicht/ vnd kennet was ich dencke.", "tokens": ["Er", "mer\u00b7cket", "was", "ihr", "dicht", "/", "vnd", "ken\u00b7net", "was", "ich", "den\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "ADJD", "$(", "KON", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Tag ist nah' an dem die itzt vermu ite Welt", "tokens": ["Der", "Tag", "ist", "nah'", "an", "dem", "die", "itzt", "ver\u00b7mu", "i\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PRELS", "ART", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+----+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Entdeckt f\u00fcr Gottes Thron/ wird zeigen was f\u00fcr Geld/", "tokens": ["Ent\u00b7deckt", "f\u00fcr", "Got\u00b7tes", "Thron", "/", "wird", "zei\u00b7gen", "was", "f\u00fcr", "Geld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "$(", "VAFIN", "VVINF", "PRELS", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Vnd Vberschrifft vnd Bild sie iederzeit getragen.", "tokens": ["Vnd", "Vber\u00b7schrifft", "vnd", "Bild", "sie", "ie\u00b7der\u00b7zeit", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Weh dem/ vnd ewig weh/ der dort nicht wird bestehn/", "tokens": ["Weh", "dem", "/", "vnd", "e\u00b7wig", "weh", "/", "der", "dort", "nicht", "wird", "be\u00b7stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$(", "KON", "ADJD", "PTKVZ", "$(", "ART", "ADV", "PTKNEG", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den vnser K\u00f6nig weit wird heissen von sich gehn", "tokens": ["Den", "vn\u00b7ser", "K\u00f6\u00b7nig", "weit", "wird", "heis\u00b7sen", "von", "sich", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "VVPP", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In Abgrund ernster Angst/ vnd nicht erdichter Plagen.", "tokens": ["In", "Ab\u00b7grund", "erns\u00b7ter", "Angst", "/", "vnd", "nicht", "er\u00b7dich\u00b7ter", "Pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$(", "KON", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}