{"dta.poem.7163": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch ist mit Recht von mir die Gnade hoch zu sch\u00e4tzen,", "tokens": ["Noch", "ist", "mit", "Recht", "von", "mir", "die", "Gna\u00b7de", "hoch", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "APPR", "PPER", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df, da ich den ", "tokens": ["Da\u00df", ",", "da", "ich", "den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Jm abgewichnen Jahr mir vorgenommen,", "tokens": ["Jm", "ab\u00b7ge\u00b7wich\u00b7nen", "Jahr", "mir", "vor\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich ebenfalls damit begl\u00fcckt zu Stande kommen!", "tokens": ["Ich", "e\u00b7ben\u00b7falls", "da\u00b7mit", "be\u00b7gl\u00fcckt", "zu", "Stan\u00b7de", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PAV", "VVPP", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}