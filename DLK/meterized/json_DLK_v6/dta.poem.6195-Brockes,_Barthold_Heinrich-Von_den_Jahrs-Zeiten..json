{"dta.poem.6195": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So breitet das Gestirn voll Flammen, Glantz und Brunst,", "tokens": ["So", "brei\u00b7tet", "das", "Ge\u00b7stirn", "voll", "Flam\u00b7men", ",", "Glantz", "und", "Brunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf unsern runden Creis, der immer um sie flieht,", "tokens": ["Auf", "un\u00b7sern", "run\u00b7den", "Creis", ",", "der", "im\u00b7mer", "um", "sie", "flieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erst einen heissen Schein. Der Landmann, braun gef\u00e4rbt,", "tokens": ["Erst", "ei\u00b7nen", "heis\u00b7sen", "Schein", ".", "Der", "Land\u00b7mann", ",", "braun", "ge\u00b7f\u00e4rbt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erhitzt durch seinen Strahl,", "tokens": ["Er\u00b7hitzt", "durch", "sei\u00b7nen", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erhebt so dann und erbt", "tokens": ["Er\u00b7hebt", "so", "dann", "und", "erbt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vom gelben Feld\u2019, im lauen Schweisse,", "tokens": ["Vom", "gel\u00b7ben", "Feld'", ",", "im", "lau\u00b7en", "Schweis\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Unschuld-volle Frucht von seinem Fleisse.", "tokens": ["Die", "Un\u00b7schuld\u00b7vol\u00b7le", "Frucht", "von", "sei\u00b7nem", "Fleis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Garben sammlet er und bindet", "tokens": ["In", "Gar\u00b7ben", "samm\u00b7let", "er", "und", "bin\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das, woran er sein Gl\u00fcck und seine Freude findet.", "tokens": ["Das", ",", "wo\u00b7ran", "er", "sein", "Gl\u00fcck", "und", "sei\u00b7ne", "Freu\u00b7de", "fin\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}