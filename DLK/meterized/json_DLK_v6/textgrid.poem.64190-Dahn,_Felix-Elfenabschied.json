{"textgrid.poem.64190": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Elfenabschied", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lebet wohl, ihr lichten Heiden,", "tokens": ["Le\u00b7bet", "wohl", ",", "ihr", "lich\u00b7ten", "Hei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brauner Acker, gr\u00fcner Rain,", "tokens": ["Brau\u00b7ner", "A\u00b7cker", ",", "gr\u00fc\u00b7ner", "Rain", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebet wohl, wir m\u00fcssen scheiden,", "tokens": ["Le\u00b7bet", "wohl", ",", "wir", "m\u00fcs\u00b7sen", "schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mondenglanz und Sternenschein.", "tokens": ["Mon\u00b7den\u00b7glanz", "und", "Ster\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In den Scho\u00df der Erde steigen,", "tokens": ["In", "den", "Scho\u00df", "der", "Er\u00b7de", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Tiefe tauchen wir:", "tokens": ["In", "die", "Tie\u00b7fe", "tau\u00b7chen", "wir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer f\u00fchren wir den Reigen", "tokens": ["Nim\u00b7mer", "f\u00fch\u00b7ren", "wir", "den", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem duft'gen Waldrevier.", "tokens": ["Auf", "dem", "duft'\u00b7gen", "Wald\u00b7re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rings von allen T\u00fcrmen l\u00e4utet", "tokens": ["Rings", "von", "al\u00b7len", "T\u00fcr\u00b7men", "l\u00e4u\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der verha\u00dften Glocken Braus", "tokens": ["Der", "ver\u00b7ha\u00df\u00b7ten", "Glo\u00b7cken", "Braus"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein jeder Schlag bedeutet:", "tokens": ["Und", "ein", "je\u00b7der", "Schlag", "be\u00b7deu\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbgeister, euer Reich ist aus!\u00ab", "tokens": ["\u00bb", "geis\u00b7ter", ",", "eu\u00b7er", "Reich", "ist", "aus", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "$,", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sang und Sitte sind geschwunden", "tokens": ["Sang", "und", "Sit\u00b7te", "sind", "ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vergessen Zucht und Recht;", "tokens": ["Und", "ver\u00b7ges\u00b7sen", "Zucht", "und", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaub' und Treu wird nicht gefunden,", "tokens": ["Glaub'", "und", "Treu", "wird", "nicht", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spottend lebt ein frech Geschlecht.", "tokens": ["Spot\u00b7tend", "lebt", "ein", "frech", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht mehr lassen fromme H\u00e4nde", "tokens": ["Nicht", "mehr", "las\u00b7sen", "from\u00b7me", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVINF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns die letzten \u00c4hren stehn,", "tokens": ["Uns", "die", "letz\u00b7ten", "\u00c4h\u00b7ren", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbst die Kinder ohne Spende", "tokens": ["Selbst", "die", "Kin\u00b7der", "oh\u00b7ne", "Spen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unserm Herd vor\u00fcbergehn.", "tokens": ["Un\u00b7serm", "Herd", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl, es sei! \u2013 Ihr sollt nun schaffen", "tokens": ["Wohl", ",", "es", "sei", "!", "\u2013", "Ihr", "sollt", "nun", "schaf\u00b7fen"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "$.", "$(", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst, allein, in Ernt' und Saat:", "tokens": ["Selbst", ",", "al\u00b7lein", ",", "in", "Ernt'", "und", "Saat", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht, den Nutzen zu erraffen,", "tokens": ["Steht", ",", "den", "Nut\u00b7zen", "zu", "er\u00b7raf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam auf der eignen Tat.", "tokens": ["Ein\u00b7sam", "auf", "der", "eig\u00b7nen", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nimmer treibt am Rad den Faden", "tokens": ["Nim\u00b7mer", "treibt", "am", "Rad", "den", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frommer Magd die Geisterhand,", "tokens": ["From\u00b7mer", "Magd", "die", "Geis\u00b7ter\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer hilft sie Garben laden,", "tokens": ["Nim\u00b7mer", "hilft", "sie", "Gar\u00b7ben", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann dem Knecht die St\u00e4rke schwand.", "tokens": ["Wann", "dem", "Knecht", "die", "St\u00e4r\u00b7ke", "schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lebe wohl, du Wiesenquelle,", "tokens": ["Le\u00b7be", "wohl", ",", "du", "Wie\u00b7sen\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "B\u00fchl und Halde, Trift und Saat,", "tokens": ["B\u00fchl", "und", "Hal\u00b7de", ",", "Trift", "und", "Saat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lebe wohl, du heil'ge Schwelle,", "tokens": ["Le\u00b7be", "wohl", ",", "du", "heil'\u00b7ge", "Schwel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der wir sch\u00fctzend oft genaht.", "tokens": ["Der", "wir", "sch\u00fct\u00b7zend", "oft", "ge\u00b7naht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lebe Tenne wohl und Speicher,", "tokens": ["Le\u00b7be", "Ten\u00b7ne", "wohl", "und", "Spei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo uns oft der Tanz geletzt:", "tokens": ["Wo", "uns", "oft", "der", "Tanz", "ge\u00b7letzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, an K\u00f6rnern wirst du reicher,", "tokens": ["Ach", ",", "an", "K\u00f6r\u00b7nern", "wirst", "du", "rei\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an Segen \u00e4rmer jetzt.", "tokens": ["Und", "an", "Se\u00b7gen", "\u00e4r\u00b7mer", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Bald ruft ihr uns an, zu helfen,", "tokens": ["Bald", "ruft", "ihr", "uns", "an", ",", "zu", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wann ihr schwer im Frone keucht, \u2013", "tokens": ["Wann", "ihr", "schwer", "im", "Fro\u00b7ne", "keucht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nimmer schaut die Elfen,", "tokens": ["A\u00b7ber", "nim\u00b7mer", "schaut", "die", "El\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer sie einmal hat verscheucht.", "tokens": ["Wer", "sie", "ein\u00b7mal", "hat", "ver\u00b7scheucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}