{"textgrid.poem.46068": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Sprudelstein und die Perlen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was toset da unten?", "tokens": ["Was", "to\u00b7set", "da", "un\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was will sich entfalten?", "tokens": ["Was", "will", "sich", "ent\u00b7fal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im Tanze, im bunten", "tokens": ["Im", "Tan\u00b7ze", ",", "im", "bun\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ersp\u00e4h' ich Gestalten", "tokens": ["Er\u00b7sp\u00e4h'", "ich", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "So sch\u00e4umend, so luftig,", "tokens": ["So", "sch\u00e4u\u00b7mend", ",", "so", "luf\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So tr\u00e4umend, so duftig.", "tokens": ["So", "tr\u00e4u\u00b7mend", ",", "so", "duf\u00b7tig", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Im siedenden Bade", "tokens": ["Im", "sie\u00b7den\u00b7den", "Ba\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die reine Najade", "tokens": ["Die", "rei\u00b7ne", "Na\u00b7ja\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Mu\u00df immerfort rauschen", "tokens": ["Mu\u00df", "im\u00b7mer\u00b7fort", "rau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADV", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Und m\u00f6chte doch lauschen;", "tokens": ["Und", "m\u00f6ch\u00b7te", "doch", "lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Dumpf h\u00f6rt sie ein Rufen", "tokens": ["Dumpf", "h\u00f6rt", "sie", "ein", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Der t\u00fcckischen Zwerge", "tokens": ["Der", "t\u00fc\u00b7cki\u00b7schen", "Zwer\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Im Grunde der Berge:", "tokens": ["Im", "Grun\u00b7de", "der", "Ber\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "\u00bbauf! brechen wir Stufen,", "tokens": ["\u00bb", "auf", "!", "bre\u00b7chen", "wir", "Stu\u00b7fen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Lichtfarbig krystallen", "tokens": ["Licht\u00b7far\u00b7big", "krys\u00b7tal\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.16": {"text": "Durch funkelnde Hallen,", "tokens": ["Durch", "fun\u00b7keln\u00b7de", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Auf! dringet zum Bade", "tokens": ["Auf", "!", "drin\u00b7get", "zum", "Ba\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Der reinen Najade.\u00ab", "tokens": ["Der", "rei\u00b7nen", "Na\u00b7ja\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Da betet die Nymphe:", "tokens": ["Da", "be\u00b7tet", "die", "Nym\u00b7phe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "\u00bbgott! h\u00fct' mich vor Schimpfe,", "tokens": ["\u00bb", "gott", "!", "h\u00fct'", "mich", "vor", "Schimp\u00b7fe", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$.", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Gott! schlie\u00dfe mich ein!\u00ab", "tokens": ["Gott", "!", "schlie\u00b7\u00dfe", "mich", "ein", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Da nahet das Klopfen,", "tokens": ["Da", "na\u00b7het", "das", "Klop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da brechen sie ein,", "tokens": ["Da", "bre\u00b7chen", "sie", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Da starren die Tropfen", "tokens": ["Da", "star\u00b7ren", "die", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Erschreckt zum Gestein,", "tokens": ["Er\u00b7schreckt", "zum", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Da fa\u00dft sie der Strudel", "tokens": ["Da", "fa\u00dft", "sie", "der", "Stru\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Der siedende Sprudel,", "tokens": ["Der", "sie\u00b7den\u00b7de", "Spru\u00b7del", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Und treibt ihre T\u00fccke", "tokens": ["Und", "treibt", "ih\u00b7re", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Zum Abgrund zur\u00fccke.", "tokens": ["Zum", "Ab\u00b7grund", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Da zuckt aus dem Bade", "tokens": ["Da", "zuckt", "aus", "dem", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Die reine Najade", "tokens": ["Die", "rei\u00b7ne", "Na\u00b7ja\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Und legt an den Rand", "tokens": ["Und", "legt", "an", "den", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Den Sprudelstein hart,", "tokens": ["Den", "Spru\u00b7del\u00b7stein", "hart", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Ein Zeugnis, ein Pfand,", "tokens": ["Ein", "Zeug\u00b7nis", ",", "ein", "Pfand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Da\u00df siedende Fluten,", "tokens": ["Da\u00df", "sie\u00b7den\u00b7de", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Vom Wunder erstarrt,", "tokens": ["Vom", "Wun\u00b7der", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.19": {"text": "Den Reinen, den Guten", "tokens": ["Den", "Rei\u00b7nen", ",", "den", "Gu\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Selbst Schutzw\u00e4nde bauen,", "tokens": ["Selbst", "Schutz\u00b7w\u00e4n\u00b7de", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "So Gott sie vertrauen \u2013", "tokens": ["So", "Gott", "sie", "ver\u00b7trau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Dran m\u00f6gen die Lauen", "tokens": ["Dran", "m\u00f6\u00b7gen", "die", "Lau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Sich redlich erbauen!", "tokens": ["Sich", "red\u00b7lich", "er\u00b7bau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}