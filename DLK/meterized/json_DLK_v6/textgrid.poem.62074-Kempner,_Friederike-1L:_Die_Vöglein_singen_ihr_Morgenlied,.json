{"textgrid.poem.62074": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die V\u00f6glein singen ihr Morgenlied,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die V\u00f6glein singen ihr Morgenlied,", "tokens": ["Die", "V\u00f6\u00b7glein", "sin\u00b7gen", "ihr", "Mor\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Man h\u00f6rt den Jubel im ganzen Gebiet,", "tokens": ["Man", "h\u00f6rt", "den", "Ju\u00b7bel", "im", "gan\u00b7zen", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Ost die purpurne Sonne gl\u00fcht", "tokens": ["Im", "Ost", "die", "pur\u00b7pur\u00b7ne", "Son\u00b7ne", "gl\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und sendet Strahlen nach West und S\u00fcd.", "tokens": ["Und", "sen\u00b7det", "Strah\u00b7len", "nach", "West", "und", "S\u00fcd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "NE", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Allein in meinem stillen Gemach,", "tokens": ["Al\u00b7lein", "in", "mei\u00b7nem", "stil\u00b7len", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Umrankt von \u00fcppigem Bl\u00e4tterdach,", "tokens": ["Um\u00b7rankt", "von", "\u00fcp\u00b7pi\u00b7gem", "Bl\u00e4t\u00b7ter\u00b7dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So sa\u00df ich tr\u00e4umend \u2013 ach, tr\u00e4umend wach \u2013", "tokens": ["So", "sa\u00df", "ich", "tr\u00e4u\u00b7mend", "\u2013", "ach", ",", "tr\u00e4u\u00b7mend", "wach", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$(", "XY", "$,", "ADJD", "ADJD", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dachte und sann gar eifrig nach \u2013", "tokens": ["Und", "dach\u00b7te", "und", "sann", "gar", "eif\u00b7rig", "nach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "APPR", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Den Kopf auf beide H\u00e4nde gest\u00fctzt: \u2013", "tokens": ["Den", "Kopf", "auf", "bei\u00b7de", "H\u00e4n\u00b7de", "ge\u00b7st\u00fctzt", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbhat es gez\u00fcndet, hat es gen\u00fctzt?", "tokens": ["\u00bb", "hat", "es", "ge\u00b7z\u00fcn\u00b7det", ",", "hat", "es", "ge\u00b7n\u00fctzt", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "VVPP", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Was ich geschrieben, so frei und frisch?\u00ab", "tokens": ["Was", "ich", "ge\u00b7schrie\u00b7ben", ",", "so", "frei", "und", "frisch", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und kindisch schlug ich auf den Tisch.", "tokens": ["Und", "kin\u00b7disch", "schlug", "ich", "auf", "den", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbist dies der Lohn f\u00fcr alle M\u00fch',", "tokens": ["\u00bb", "ist", "dies", "der", "Lohn", "f\u00fcr", "al\u00b7le", "M\u00fch'", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PDS", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Wirklichkeit und Poesie?", "tokens": ["F\u00fcr", "Wirk\u00b7lich\u00b7keit", "und", "Poe\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wen k\u00fcmmert's wohl, wer steht mir nah,", "tokens": ["Wen", "k\u00fcm\u00b7mert's", "wohl", ",", "wer", "steht", "mir", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "PWS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Steht alles nicht noch feindlich da?\u00ab", "tokens": ["Steht", "al\u00b7les", "nicht", "noch", "feind\u00b7lich", "da", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da horch, da sieh! Was sprengt heran?", "tokens": ["Da", "horch", ",", "da", "sieh", "!", "Was", "sprengt", "he\u00b7ran", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "KOUS", "PPER", "$.", "PWS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch pr\u00e4chtiges, gl\u00e4nzendes Viergespann!", "tokens": ["Welch", "pr\u00e4ch\u00b7ti\u00b7ges", ",", "gl\u00e4n\u00b7zen\u00b7des", "Vier\u00b7ge\u00b7spann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Apollo selber im Sonnenwagen:", "tokens": ["A\u00b7pol\u00b7lo", "sel\u00b7ber", "im", "Son\u00b7nen\u00b7wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Kannst Du Dich jetzo noch beklagen?", "tokens": ["Kannst", "Du", "Dich", "jet\u00b7zo", "noch", "be\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}