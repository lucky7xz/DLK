{"dta.poem.21859": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Wiltu/ Reuter/ mich nicht reiten lassen/", "tokens": ["Wil\u00b7tu", "/", "Reu\u00b7ter", "/", "mich", "nicht", "rei\u00b7ten", "las\u00b7sen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "NE", "$(", "PPER", "PTKNEG", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "warum g\u00f6nnstu mir denn Zeug und Feld?", "tokens": ["wa\u00b7rum", "g\u00f6nns\u00b7tu", "mir", "denn", "Zeug", "und", "Feld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "wil dein Sattel nur mir an-sich-massen/", "tokens": ["wil", "dein", "Sat\u00b7tel", "nur", "mir", "an\u00b7sich\u00b7mas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PPER", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wett\u2019 ich/ wer den besten Ritt erh\u00e4lt.", "tokens": ["wett'", "ich", "/", "wer", "den", "bes\u00b7ten", "Ritt", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Stich dein Klepper an/ und renne zu/", "tokens": ["Stich", "dein", "Klep\u00b7per", "an", "/", "und", "ren\u00b7ne", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "VVFIN", "PTKZU", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "ich bin so ein Kavalier als du.", "tokens": ["ich", "bin", "so", "ein", "Ka\u00b7va\u00b7lier", "als", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "KOUS", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}