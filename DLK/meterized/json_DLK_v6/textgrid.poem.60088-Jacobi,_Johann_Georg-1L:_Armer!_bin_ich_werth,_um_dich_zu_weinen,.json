{"textgrid.poem.60088": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Armer! bin ich werth, um dich zu weinen,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Armer! bin ich werth, um dich zu weinen,", "tokens": ["Ar\u00b7mer", "!", "bin", "ich", "werth", ",", "um", "dich", "zu", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADJD", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hier im fernen deutschen Vaterland,", "tokens": ["Hier", "im", "fer\u00b7nen", "deut\u00b7schen", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "O so la\u00df mich. \u2013 Armer! von den Deinen,", "tokens": ["O", "so", "la\u00df", "mich", ".", "\u2013", "Ar\u00b7mer", "!", "von", "den", "Dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "PPER", "$.", "$(", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Als du Wahrheit suchtest, weggebannt!", "tokens": ["Als", "du", "Wahr\u00b7heit", "such\u00b7test", ",", "weg\u00b7ge\u00b7bannt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ausgeh\u00f6hnt von stolzen Atheisten,", "tokens": ["Aus\u00b7ge\u00b7h\u00f6hnt", "von", "stol\u00b7zen", "A\u00b7theis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Weil du Glauben trugst in deiner Brust,", "tokens": ["Weil", "du", "Glau\u00b7ben", "trugst", "in", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und verfolgt von Priestern unter Christen,", "tokens": ["Und", "ver\u00b7folgt", "von", "Pries\u00b7tern", "un\u00b7ter", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Gleich als d\u00fcnkte sie Verfolgen Engelslust!", "tokens": ["Gleich", "als", "d\u00fcnk\u00b7te", "sie", "Ver\u00b7fol\u00b7gen", "En\u00b7gels\u00b7lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Ach! geh\u00f6hnt, weil dir ein innres Wort der Weihe", "tokens": ["Ach", "!", "ge\u00b7h\u00f6hnt", ",", "weil", "dir", "ein", "inn\u00b7res", "Wort", "der", "Wei\u00b7he"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "F\u00fcr dein Elend k\u00fcnft'gen Trost verhie\u00df;", "tokens": ["F\u00fcr", "dein", "E\u00b7lend", "k\u00fcnft'\u00b7gen", "Trost", "ver\u00b7hie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Ach! verfolgt, weil ohne Lieb' und Treue", "tokens": ["Ach", "!", "ver\u00b7folgt", ",", "weil", "oh\u00b7ne", "Lieb'", "und", "Treu\u00b7e"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVPP", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Man umsonst dir seinen Glauben pries!", "tokens": ["Man", "um\u00b7sonst", "dir", "sei\u00b7nen", "Glau\u00b7ben", "pries", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Konntest nicht den bittern Lohn verschmerzen", "tokens": ["Konn\u00b7test", "nicht", "den", "bit\u00b7tern", "Lohn", "ver\u00b7schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Von dem Volke, dem du wohlgethan;", "tokens": ["Von", "dem", "Vol\u00b7ke", ",", "dem", "du", "wohl\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Gingst hinweg mit deinem Bruder-Herzen,", "tokens": ["Gingst", "hin\u00b7weg", "mit", "dei\u00b7nem", "Bru\u00b7der\u00b7Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Gingst allein auf deiner Dornen-Bahn.", "tokens": ["Gingst", "al\u00b7lein", "auf", "dei\u00b7ner", "Dor\u00b7nen\u00b7Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Armer, Guter! schlafe jetzt in Frieden,", "tokens": ["Ar\u00b7mer", ",", "Gu\u00b7ter", "!", "schla\u00b7fe", "jetzt", "in", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Wo kein Priester dich, kein Gottesl\u00e4ugner dr\u00e4ngt;", "tokens": ["Wo", "kein", "Pries\u00b7ter", "dich", ",", "kein", "Got\u00b7tes\u00b7l\u00e4ug\u00b7ner", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PPER", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Guter, Edler! bist dahin geschieden,", "tokens": ["Gu\u00b7ter", ",", "Ed\u00b7ler", "!", "bist", "da\u00b7hin", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$.", "VAFIN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Wo die Einfalt ihre Kron' empf\u00e4ngt;", "tokens": ["Wo", "die", "Ein\u00b7falt", "ih\u00b7re", "Kron'", "emp\u00b7f\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Wo dem M\u00e4rtyrer aus allen Himmelslichtern", "tokens": ["Wo", "dem", "M\u00e4r\u00b7ty\u00b7rer", "aus", "al\u00b7len", "Him\u00b7mels\u00b7lich\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.22": {"text": "Sein errungner Glanz entgegen scheint,", "tokens": ["Sein", "er\u00b7rung\u00b7ner", "Glanz", "ent\u00b7ge\u00b7gen", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Lieb' und Ruh in allen Angesichtern", "tokens": ["Lieb'", "und", "Ruh", "in", "al\u00b7len", "An\u00b7ge\u00b7sich\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Um den hier Versto\u00dfnen sich vereint. \u2013", "tokens": ["Um", "den", "hier", "Ver\u00b7sto\u00df\u00b7nen", "sich", "ver\u00b7eint", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "ADV", "NN", "PRF", "VVPP", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Hin zu jenen B\u00e4umen will ich wallen,", "tokens": ["Hin", "zu", "je\u00b7nen", "B\u00e4u\u00b7men", "will", "ich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Wo, von Menschen weit, dein Lager sanfter ist:", "tokens": ["Wo", ",", "von", "Men\u00b7schen", "weit", ",", "dein", "La\u00b7ger", "sanf\u00b7ter", "ist", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "NN", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "O da werden blut'ge Thr\u00e4nen fallen", "tokens": ["O", "da", "wer\u00b7den", "blut'\u00b7ge", "Thr\u00e4\u00b7nen", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Ums Jahrhundert, wenn es dich vergi\u00dft.", "tokens": ["Ums", "Jahr\u00b7hun\u00b7dert", ",", "wenn", "es", "dich", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}