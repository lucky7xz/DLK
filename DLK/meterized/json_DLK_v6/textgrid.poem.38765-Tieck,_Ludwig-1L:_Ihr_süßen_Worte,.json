{"textgrid.poem.38765": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr s\u00fc\u00dfen Worte,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr s\u00fc\u00dfen Worte,", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfen", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Ihr leichtbeschwingte sanfte Reime,", "tokens": ["Ihr", "leicht\u00b7be\u00b7schwing\u00b7te", "sanf\u00b7te", "Rei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die mit dem zarten Klange,", "tokens": ["Die", "mit", "dem", "zar\u00b7ten", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Summend mit dem harmon'schen Fl\u00fcgel", "tokens": ["Sum\u00b7mend", "mit", "dem", "har\u00b7mon'\u00b7schen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Durch die B\u00e4ume", "tokens": ["Durch", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ueber Berge fliegt und H\u00fcgel,", "tokens": ["Ue\u00b7ber", "Ber\u00b7ge", "fliegt", "und", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebkoset euch auf eurem Gange", "tokens": ["Lieb\u00b7ko\u00b7set", "euch", "auf", "eu\u00b7rem", "Gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und nahet euch dem\u00fcthig jenem Orte,", "tokens": ["Und", "na\u00b7het", "euch", "de\u00b7m\u00fct\u00b7hig", "je\u00b7nem", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wo reizend steht die Sch\u00f6nste unter Sch\u00f6nen,", "tokens": ["Wo", "rei\u00b7zend", "steht", "die", "Sch\u00f6ns\u00b7te", "un\u00b7ter", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da wollet t\u00f6nen", "tokens": ["Da", "wol\u00b7let", "t\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Und sagen, klagen, wie mein Herz schon lange", "tokens": ["Und", "sa\u00b7gen", ",", "kla\u00b7gen", ",", "wie", "mein", "Herz", "schon", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "VVFIN", "$,", "KOKOM", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Entbehrt des Lebens. Sch\u00fcrzet euch zum Reisen,", "tokens": ["Ent\u00b7behrt", "des", "Le\u00b7bens", ".", "Sch\u00fcr\u00b7zet", "euch", "zum", "Rei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die lieblichste der Weisen", "tokens": ["Die", "lieb\u00b7lichs\u00b7te", "der", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Legt an, und weint und lacht, wenn euch zum Gru\u00dfe", "tokens": ["Legt", "an", ",", "und", "weint", "und", "lacht", ",", "wenn", "euch", "zum", "Gru\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Sprache fehlt, so redet wie die Liebenden im Kusse.", "tokens": ["Die", "Spra\u00b7che", "fehlt", ",", "so", "re\u00b7det", "wie", "die", "Lie\u00b7ben\u00b7den", "im", "Kus\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Ihr goldnen Sterne,", "tokens": ["Ihr", "gold\u00b7nen", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vom hohen blauen Meer ihr Blicke,", "tokens": ["Vom", "ho\u00b7hen", "blau\u00b7en", "Meer", "ihr", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir sonst hold gewinket,", "tokens": ["Die", "mir", "sonst", "hold", "ge\u00b7win\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollt jetzt mit zorn'gen Lichtern scheinen?", "tokens": ["Wollt", "jetzt", "mit", "zorn'\u00b7gen", "Lich\u00b7tern", "schei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jenes Gl\u00fccke,", "tokens": ["Je\u00b7nes", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ach! das seeligste Vereinen", "tokens": ["Ach", "!", "das", "see\u00b7ligs\u00b7te", "Ver\u00b7ei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie ich es halten will, versinket,", "tokens": ["Wie", "ich", "es", "hal\u00b7ten", "will", ",", "ver\u00b7sin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich mu\u00df klagen: wieder ist sie ferne!", "tokens": ["Und", "ich", "mu\u00df", "kla\u00b7gen", ":", "wie\u00b7der", "ist", "sie", "fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$.", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein Widerhall t\u00f6nt aus dem tiefen Herzen,", "tokens": ["Ein", "Wi\u00b7der\u00b7hall", "t\u00f6nt", "aus", "dem", "tie\u00b7fen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und alle Schmerzen", "tokens": ["Und", "al\u00b7le", "Schmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Beleben heben sich und jeder trinket", "tokens": ["Be\u00b7le\u00b7ben", "he\u00b7ben", "sich", "und", "je\u00b7der", "trin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mit schadenfroher Angst von meinen Thr\u00e4nen.", "tokens": ["Mit", "scha\u00b7den\u00b7fro\u00b7her", "Angst", "von", "mei\u00b7nen", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ach! d\u00fcrft ich w\u00e4hnen", "tokens": ["Ach", "!", "d\u00fcrft", "ich", "w\u00e4h\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Da\u00df bald im Wechsel jener Morgen tage,", "tokens": ["Da\u00df", "bald", "im", "Wech\u00b7sel", "je\u00b7ner", "Mor\u00b7gen", "ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der schlagend Herz zum Herzen, Ku\u00df zum Zwillingskusse trage.", "tokens": ["Der", "schla\u00b7gend", "Herz", "zum", "Her\u00b7zen", ",", "Ku\u00df", "zum", "Zwil\u00b7lings\u00b7kus\u00b7se", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$,", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Schon vormals brannten", "tokens": ["Schon", "vor\u00b7mals", "brann\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die Blicke, hingejagt vom Sehnen", "tokens": ["Die", "Bli\u00b7cke", ",", "hin\u00b7ge\u00b7jagt", "vom", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das sp\u00e4hend \u00fcber Kl\u00fcfte", "tokens": ["Das", "sp\u00e4\u00b7hend", "\u00fc\u00b7ber", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch kaum erkannte sein Verlangen,", "tokens": ["Noch", "kaum", "er\u00b7kann\u00b7te", "sein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wehmuths-Thr\u00e4nen", "tokens": ["Weh\u00b7muths\u00b7Thr\u00e4\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Mu\u00dften in den Augen hangen,", "tokens": ["Mu\u00df\u00b7ten", "in", "den", "Au\u00b7gen", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Gartenlauben schienen Gr\u00fcfte:", "tokens": ["Die", "Gar\u00b7ten\u00b7lau\u00b7ben", "schie\u00b7nen", "Gr\u00fcf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis liebend sich die Seelen dann erkannten,", "tokens": ["Bis", "lie\u00b7bend", "sich", "die", "See\u00b7len", "dann", "er\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da brach nun auf, gelockt von Fr\u00fchlings-Sonne", "tokens": ["Da", "brach", "nun", "auf", ",", "ge\u00b7lockt", "von", "Fr\u00fch\u00b7lings\u00b7Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die goldne Wonne,", "tokens": ["Die", "gold\u00b7ne", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Mit Kosen Rosen hauchten Liebesd\u00fcfte,", "tokens": ["Mit", "Ko\u00b7sen", "Ro\u00b7sen", "hauch\u00b7ten", "Lie\u00b7bes\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Farben gl\u00e4nzten nur von ihrer Sch\u00f6ne,", "tokens": ["Die", "Far\u00b7ben", "gl\u00e4nz\u00b7ten", "nur", "von", "ih\u00b7rer", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Es sagten alle T\u00f6ne:", "tokens": ["Es", "sag\u00b7ten", "al\u00b7le", "T\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sie liebt! ich sah den Wald und Garten prangen,", "tokens": ["Sie", "liebt", "!", "ich", "sah", "den", "Wald", "und", "Gar\u00b7ten", "pran\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Von jedem gr\u00fcnen Zweige schaukelnd Liebesg\u00f6tter hangen.", "tokens": ["Von", "je\u00b7dem", "gr\u00fc\u00b7nen", "Zwei\u00b7ge", "schau\u00b7kelnd", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Doch kaum gefunden", "tokens": ["Doch", "kaum", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wonach die durstgen Augen lange", "tokens": ["Wo\u00b7nach", "die", "durst\u00b7gen", "Au\u00b7gen", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und das Geh\u00f6r geschmachtet,", "tokens": ["Und", "das", "Ge\u00b7h\u00f6r", "ge\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als beide deine Lieb' empfangen,", "tokens": ["Als", "bei\u00b7de", "dei\u00b7ne", "Lieb'", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie so bange", "tokens": ["Wie", "so", "ban\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wieder alle Freuden schwanden,", "tokens": ["Wie\u00b7der", "al\u00b7le", "Freu\u00b7den", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Das Herz, vom Licht gek\u00fc\u00dft, umnachtet,", "tokens": ["Das", "Herz", ",", "vom", "Licht", "ge\u00b7k\u00fc\u00dft", ",", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ungeheilet bluteten die Wunden.", "tokens": ["Und", "un\u00b7ge\u00b7hei\u00b7let", "blu\u00b7te\u00b7ten", "die", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Soll nicht im Dunkel neu Entz\u00fccken leuchten?", "tokens": ["Soll", "nicht", "im", "Dun\u00b7kel", "neu", "Ent\u00b7z\u00fc\u00b7cken", "leuch\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPRART", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auf rosenrothem feuchten", "tokens": ["Auf", "ro\u00b7sen\u00b7ro\u00b7them", "feuch\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mund bebtet schwebtet K\u00fcsse ihr und lachtet.", "tokens": ["Mund", "beb\u00b7tet", "schweb\u00b7tet", "K\u00fcs\u00b7se", "ihr", "und", "lach\u00b7tet", "."], "token