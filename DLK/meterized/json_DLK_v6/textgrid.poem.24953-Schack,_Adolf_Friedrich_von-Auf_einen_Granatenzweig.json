{"textgrid.poem.24953": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Granatenzweig", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank, Freundin, da\u00df dem Winterm\u00fcden,", "tokens": ["Dank", ",", "Freun\u00b7din", ",", "da\u00df", "dem", "Win\u00b7ter\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den hier des Nordens Eis umstarrt,", "tokens": ["Den", "hier", "des", "Nor\u00b7dens", "Eis", "um\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dir und dem geliebten S\u00fcden", "tokens": ["Von", "dir", "und", "dem", "ge\u00b7lieb\u00b7ten", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Gru\u00df in diesem Zweige ward!", "tokens": ["Ein", "Gru\u00df", "in", "die\u00b7sem", "Zwei\u00b7ge", "ward", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon hat, getr\u00e4nkt von meiner Schale,", "tokens": ["Schon", "hat", ",", "ge\u00b7tr\u00e4nkt", "von", "mei\u00b7ner", "Scha\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er sich mit Bl\u00fcten reich geschm\u00fcckt,", "tokens": ["Er", "sich", "mit", "Bl\u00fc\u00b7ten", "reich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und duftet wie im M\u00fchlenthale", "tokens": ["Und", "duf\u00b7tet", "wie", "im", "M\u00fch\u00b7lent\u00b7ha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Amalfis, wo du ihn gepfl\u00fcckt.", "tokens": ["A\u00b7mal\u00b7fis", ",", "wo", "du", "ihn", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "+----+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und w\u00e4hrend matt durchs Flockentreiben", "tokens": ["Und", "w\u00e4h\u00b7rend", "matt", "durchs", "Flo\u00b7cken\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die bleiche Sonne drau\u00dfen strahlt,", "tokens": ["Die", "blei\u00b7che", "Son\u00b7ne", "drau\u00b7\u00dfen", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Blumen Eises an die Scheiben", "tokens": ["Und", "Blu\u00b7men", "Ei\u00b7ses", "an", "die", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der frostige Dezember malt,", "tokens": ["Der", "fros\u00b7ti\u00b7ge", "De\u00b7zem\u00b7ber", "malt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Schwebt mir beim Fr\u00fchlingsduft hier innen,", "tokens": ["Schwebt", "mir", "beim", "Fr\u00fch\u00b7lings\u00b7duft", "hier", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der aus den roten Kelchen quillt,", "tokens": ["Der", "aus", "den", "ro\u00b7ten", "Kel\u00b7chen", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Traum und Wachen vor den Sinnen", "tokens": ["Im", "Traum", "und", "Wa\u00b7chen", "vor", "den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein und Italiens Zauberbild.", "tokens": ["Dein", "und", "I\u00b7ta\u00b7li\u00b7ens", "Zau\u00b7ber\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "NE", "NN", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Hoch seh' ich ob den Meergestaden", "tokens": ["Hoch", "seh'", "ich", "ob", "den", "Meer\u00b7ge\u00b7sta\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dich an den Felsenrand gelehnt,", "tokens": ["Dich", "an", "den", "Fel\u00b7sen\u00b7rand", "ge\u00b7lehnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem mit sch\u00e4umenden Kaskaden", "tokens": ["An", "dem", "mit", "sch\u00e4u\u00b7men\u00b7den", "Kas\u00b7ka\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wilde Schlucht der M\u00fchlen g\u00e4hnt.", "tokens": ["Die", "wil\u00b7de", "Schlucht", "der", "M\u00fch\u00b7len", "g\u00e4hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Schellenklang der Tarantellen", "tokens": ["Den", "Schel\u00b7len\u00b7klang", "der", "Ta\u00b7ran\u00b7tel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vernehm' ich, der das Thal durchhallt", "tokens": ["Ver\u00b7nehm'", "ich", ",", "der", "das", "Thal", "durch\u00b7hallt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rauschend mit den Wasserf\u00e4llen,", "tokens": ["Und", "rau\u00b7schend", "mit", "den", "Was\u00b7ser\u00b7f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den tosenden, nach oben schallt;", "tokens": ["Den", "to\u00b7sen\u00b7den", ",", "nach", "o\u00b7ben", "schallt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gel\u00e4chter und Gesang dazwischen,", "tokens": ["Ge\u00b7l\u00e4ch\u00b7ter", "und", "Ge\u00b7sang", "da\u00b7zwi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb von der Flut nur \u00fcbert\u00e4ubt,", "tokens": ["Halb", "von", "der", "Flut", "nur", "\u00fc\u00b7bert\u00b7\u00e4ubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die donnernd hier und dort mit Zischen", "tokens": ["Die", "don\u00b7nernd", "hier", "und", "dort", "mit", "Zi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinsinkt und wieder aufw\u00e4rts st\u00e4ubt;", "tokens": ["Hin\u00b7sinkt", "und", "wie\u00b7der", "auf\u00b7w\u00e4rts", "st\u00e4ubt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und zitternd bei dem Wogenrollen", "tokens": ["Und", "zit\u00b7ternd", "bei", "dem", "Wo\u00b7gen\u00b7rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Senkt ein Granatbaum an dem Rand", "tokens": ["Senkt", "ein", "Gra\u00b7nat\u00b7baum", "an", "dem", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die Aeste tief, die bl\u00fctenvollen,", "tokens": ["Die", "A\u00b7es\u00b7te", "tief", ",", "die", "bl\u00fc\u00b7ten\u00b7vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinunter von der Felsenwand;", "tokens": ["Hin\u00b7un\u00b7ter", "von", "der", "Fel\u00b7sen\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du aber beugst dich zu der Neige", "tokens": ["Du", "a\u00b7ber", "beugst", "dich", "zu", "der", "Nei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Abgrunds, \u00fcber dem er hangt,", "tokens": ["Des", "Ab\u00b7grunds", ",", "\u00fc\u00b7ber", "dem", "er", "hangt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einen brichst du mir der Zweige,", "tokens": ["Und", "ei\u00b7nen", "brichst", "du", "mir", "der", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der in dem reichsten Schmucke prangt.", "tokens": ["Der", "in", "dem", "reichs\u00b7ten", "Schmu\u00b7cke", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Oft tr\u00e4um' ich so, und beim Erwachen \u2013", "tokens": ["Oft", "tr\u00e4um'", "ich", "so", ",", "und", "beim", "Er\u00b7wa\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh da! vor Augen hab' ich ihn;", "tokens": ["Sieh", "da", "!", "vor", "Au\u00b7gen", "hab'", "ich", "ihn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "APPR", "NN", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch t\u00f6nt im Ohre mir das Lachen,", "tokens": ["Noch", "t\u00f6nt", "im", "Oh\u00b7re", "mir", "das", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch das Geklirr vom Tamburin.", "tokens": ["Noch", "das", "Ge\u00b7klirr", "vom", "Tam\u00b7bu\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Noch blitzt vom Schaum der Katarakte", "tokens": ["Noch", "blitzt", "vom", "Schaum", "der", "Ka\u00b7ta\u00b7rak\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf jedem Blatt der feuchte Staub;", "tokens": ["Auf", "je\u00b7dem", "Blatt", "der", "feuch\u00b7te", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ist, als zittre von dem Takte", "tokens": ["Mir", "ist", ",", "als", "zitt\u00b7re", "von", "dem", "Tak\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wassersturzes noch das Laub.", "tokens": ["Des", "Was\u00b7ser\u00b7stur\u00b7zes", "noch", "das", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Mag denn der Sturm des Winters w\u00fcten,", "tokens": ["Mag", "denn", "der", "Sturm", "des", "Win\u00b7ters", "w\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich, Freundin, sch\u00fctzt ein Talisman;", "tokens": ["Mich", ",", "Freun\u00b7din", ",", "sch\u00fctzt", "ein", "Ta\u00b7lis\u00b7man", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stets haucht mich aus des Zweiges Bl\u00fcten", "tokens": ["Stets", "haucht", "mich", "aus", "des", "Zwei\u00b7ges", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein und Italiens Odem an.", "tokens": ["Dein", "und", "I\u00b7ta\u00b7li\u00b7ens", "O\u00b7dem", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "NE", "NE", "PTKVZ", "$."], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}}}}}