{"textgrid.poem.25242": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es nebelt. Knorriger Eichenheld,", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es nebelt. Knorriger Eichenheld,", "tokens": ["Es", "ne\u00b7belt", ".", "Knor\u00b7ri\u00b7ger", "Ei\u00b7chen\u00b7held", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "NN", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schon wird dein Lockenhaupt herbstlich bleich,", "tokens": ["Schon", "wird", "dein", "Lo\u00b7cken\u00b7haupt", "herbst\u00b7lich", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und raschelnd die braune Eichel f\u00e4llt.", "tokens": ["Und", "ra\u00b7schelnd", "die", "brau\u00b7ne", "Ei\u00b7chel", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Doch bl\u00fcht dir heimlich ein K\u00f6nigreich.", "tokens": ["Doch", "bl\u00fcht", "dir", "heim\u00b7lich", "ein", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df nebeln, dunkeln! Schlaf! Es ist sp\u00e4t!", "tokens": ["La\u00df", "ne\u00b7beln", ",", "dun\u00b7keln", "!", "Schlaf", "!", "Es", "ist", "sp\u00e4t", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "ADJA", "$.", "NN", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im Wintertraum k\u00fc\u00dft dich die Sonnenmaid,", "tokens": ["Im", "Win\u00b7ter\u00b7traum", "k\u00fc\u00dft", "dich", "die", "Son\u00b7nen\u00b7maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und aus den Keimen, die du ges\u00e4t,", "tokens": ["Und", "aus", "den", "Kei\u00b7men", ",", "die", "du", "ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sprie\u00dft tausendfach deine Jugendzeit.", "tokens": ["Sprie\u00dft", "tau\u00b7send\u00b7fach", "dei\u00b7ne", "Ju\u00b7gend\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}}}}}