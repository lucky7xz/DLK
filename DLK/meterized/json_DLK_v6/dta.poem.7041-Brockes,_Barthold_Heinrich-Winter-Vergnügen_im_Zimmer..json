{"dta.poem.7041": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Winter-Vergn\u00fcgen  \n im Zimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT gieb da\u00df, so von mir, als meiner kleinen Heerde,", "tokens": ["GoTT", "gieb", "da\u00df", ",", "so", "von", "mir", ",", "als", "mei\u00b7ner", "klei\u00b7nen", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KOUS", "$,", "ADV", "APPR", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein\u2019 Allmacht, wie gef\u00fchlt, auch so erkennet werde!", "tokens": ["Dein'", "All\u00b7macht", ",", "wie", "ge\u00b7f\u00fchlt", ",", "auch", "so", "er\u00b7ken\u00b7net", "wer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVPP", "$,", "ADV", "ADV", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und la\u00df uns doch davor, o Vater! Dir allein", "tokens": ["Und", "la\u00df", "uns", "doch", "da\u00b7vor", ",", "o", "Va\u00b7ter", "!", "Dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PAV", "$,", "FM", "NN", "$.", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In \u00f6ffterer Betrachtung danckbar seyn!", "tokens": ["In", "\u00f6ff\u00b7te\u00b7rer", "Be\u00b7trach\u00b7tung", "dan\u00b7ck\u00b7bar", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}