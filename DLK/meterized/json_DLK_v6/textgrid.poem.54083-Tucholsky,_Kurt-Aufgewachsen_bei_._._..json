{"textgrid.poem.54083": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Aufgewachsen bei . . .", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir gefallen die Beine nicht,", "tokens": ["Dir", "ge\u00b7fal\u00b7len", "die", "Bei\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "dir gef\u00e4llt die Kleine nicht,", "tokens": ["dir", "ge\u00b7f\u00e4llt", "die", "Klei\u00b7ne", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "dir gef\u00e4llt die Gro\u00dfe nicht,", "tokens": ["dir", "ge\u00b7f\u00e4llt", "die", "Gro\u00b7\u00dfe", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "und du magst die Sauce nicht.", "tokens": ["und", "du", "magst", "die", "Sau\u00b7ce", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir gef\u00e4llt der Opel nicht,", "tokens": ["Dir", "ge\u00b7f\u00e4llt", "der", "O\u00b7pel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "und du w\u00e4rst kein Popel nicht,", "tokens": ["und", "du", "w\u00e4rst", "kein", "Po\u00b7pel", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "und dir schmeckt der Steinwein nicht,", "tokens": ["und", "dir", "schmeckt", "der", "Stein\u00b7wein", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und dir schmeckt der Rheinwein nicht . . .", "tokens": ["und", "dir", "schmeckt", "der", "Rhein\u00b7wein", "nicht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lieber Freund, besinn dich drauf:", "tokens": ["Lie\u00b7ber", "Freund", ",", "be\u00b7sinn", "dich", "drauf", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Worauf herauf \u2013?", "tokens": ["Wo\u00b7rauf", "her\u00b7auf", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PAV", "PTKVZ", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Bist du denn so reich und sch\u00f6n?", "tokens": ["Bist", "du", "denn", "so", "reich", "und", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du lieblich anzusehn?", "tokens": ["Bist", "du", "lieb\u00b7lich", "an\u00b7zu\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist du elegant und schick?", "tokens": ["Bist", "du", "e\u00b7le\u00b7gant", "und", "schick", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Untenrum nicht reichlich dick?", "tokens": ["Un\u00b7ten\u00b7rum", "nicht", "reich\u00b7lich", "dick", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bist du mit dem Mordskrawall", "tokens": ["Bist", "du", "mit", "dem", "Mords\u00b7kra\u00b7wall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wohl aus einem ersten Stall?", "tokens": ["wohl", "aus", "ei\u00b7nem", "ers\u00b7ten", "Stall", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer schreist du nach Niveau . . .", "tokens": ["Im\u00b7mer", "schreist", "du", "nach", "Ni\u00b7vea\u00b7u", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "$.", "$.", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "lebst du denn zu Hause so?", "tokens": ["lebst", "du", "denn", "zu", "Hau\u00b7se", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Du \u2013 mit deinem Lebenslauf:", "tokens": ["Du", "\u2013", "mit", "dei\u00b7nem", "Le\u00b7bens\u00b7lauf", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Worauf herauf \u2013?", "tokens": ["Wo\u00b7rauf", "her\u00b7auf", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PAV", "PTKVZ", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Stell dich mit dem Doppelkinn", "tokens": ["Stell", "dich", "mit", "dem", "Dop\u00b7pel\u00b7kinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "mal vor einen Spiegel hin:", "tokens": ["mal", "vor", "ei\u00b7nen", "Spie\u00b7gel", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Frauen auch mal sieben:", "tokens": ["Wenn", "die", "Frau\u00b7en", "auch", "mal", "sie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "welches M\u00e4dchen soll dich lieben?", "tokens": ["wel\u00b7ches", "M\u00e4d\u00b7chen", "soll", "dich", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sage selbst!", "tokens": ["Sa\u00b7ge", "selbst", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wenn die Kellner Augen haben:", "tokens": ["Wenn", "die", "Kell\u00b7ner", "Au\u00b7gen", "ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "wof\u00fcr halten sie dich Knaben?", "tokens": ["wo\u00b7f\u00fcr", "hal\u00b7ten", "sie", "dich", "Kna\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sage selbst!", "tokens": ["Sa\u00b7ge", "selbst", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "In dem reichen Kaufmannshaus:", "tokens": ["In", "dem", "rei\u00b7chen", "Kauf\u00b7manns\u00b7haus", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "wie siehst du im Smoking aus?", "tokens": ["wie", "siehst", "du", "im", "Smo\u00b7king", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Sage selbst!", "tokens": ["Sa\u00b7ge", "selbst", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Mach nicht immer solche Faxen.", "tokens": ["Mach", "nicht", "im\u00b7mer", "sol\u00b7che", "Fa\u00b7xen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Mensch, es ist ja halb Berlin", "tokens": ["Mensch", ",", "es", "ist", "ja", "halb", "Ber\u00b7lin"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "aufgewachsen, aufgewachsen", "tokens": ["auf\u00b7ge\u00b7wach\u00b7sen", ",", "auf\u00b7ge\u00b7wach\u00b7sen"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "bei den gr\u00fcnen Jalousien \u2013!", "tokens": ["bei", "den", "gr\u00fc\u00b7nen", "Ja\u00b7lou\u00b7si\u00b7en", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}