{"textgrid.poem.34586": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "9. Der Todt", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist der Tod der Frommen?", "tokens": ["Was", "ist", "der", "Tod", "der", "From\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schl\u00fcssel zu dem Leben,", "tokens": ["Ein", "Schl\u00fcs\u00b7sel", "zu", "dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gr\u00e4ntzstein b\u00f6ser Zeit,", "tokens": ["Ein", "Gr\u00e4ntz\u00b7stein", "b\u00f6\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schlafftrunck alter Reben,", "tokens": ["Ein", "Schlaff\u00b7trunck", "al\u00b7ter", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Fried auff Krieg und Streit,", "tokens": ["Ein", "Fried", "auff", "Krieg", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ein F\u00fchrer zu der Sonne,", "tokens": ["Ein", "F\u00fch\u00b7rer", "zu", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Steg ins Vaterland,", "tokens": ["Ein", "Steg", "ins", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein Auffgang aller Wonne,", "tokens": ["Ein", "Auff\u00b7gang", "al\u00b7ler", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ein Trieb von grosser Hand,", "tokens": ["Ein", "Trieb", "von", "gros\u00b7ser", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ein Zunder zu dem Lichte,", "tokens": ["Ein", "Zun\u00b7der", "zu", "dem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ein Flug in jene Welt,", "tokens": ["Ein", "Flug", "in", "je\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ein Paradiesgerichte,", "tokens": ["Ein", "Pa\u00b7ra\u00b7dies\u00b7ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ein Schlag, der alles f\u00e4llt.", "tokens": ["Ein", "Schlag", ",", "der", "al\u00b7les", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ein Abtritt aller Plagen,", "tokens": ["Ein", "Ab\u00b7tritt", "al\u00b7ler", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Ein Baum vor alle Noth,", "tokens": ["Ein", "Baum", "vor", "al\u00b7le", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Was soll ich ferner sagen?", "tokens": ["Was", "soll", "ich", "fer\u00b7ner", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Di\u00df alles ist der Tod.", "tokens": ["Di\u00df", "al\u00b7les", "ist", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}