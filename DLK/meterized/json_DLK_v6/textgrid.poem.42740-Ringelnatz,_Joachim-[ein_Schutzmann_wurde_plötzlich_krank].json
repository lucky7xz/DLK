{"textgrid.poem.42740": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[ein Schutzmann wurde pl\u00f6tzlich krank]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schutzmann wurde pl\u00f6tzlich krank", "tokens": ["Ein", "Schutz\u00b7mann", "wur\u00b7de", "pl\u00f6tz\u00b7lich", "krank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und setzte sich auf eine Bank.", "tokens": ["Und", "setz\u00b7te", "sich", "auf", "ei\u00b7ne", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort sa\u00df bereits ein Stachelschwein.", "tokens": ["Dort", "sa\u00df", "be\u00b7reits", "ein", "Sta\u00b7chel\u00b7schwein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Schutzmann setzte sich hinein.", "tokens": ["Der", "Schutz\u00b7mann", "setz\u00b7te", "sich", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schrie er: \u00bbAu!\u00ab und schrie er: \u00bbOh!\u00ab", "tokens": ["Da", "schrie", "er", ":", "\u00bb", "Au", "!", "\u00ab", "und", "schrie", "er", ":", "\u00bb", "Oh", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "$(", "KON", "VVFIN", "PPER", "$.", "$(", "ITJ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kratzte sich an dem Po-lizeihelm.", "tokens": ["Und", "kratz\u00b7te", "sich", "an", "dem", "Po\u00b7li\u00b7zei\u00b7helm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}