{"textgrid.poem.52030": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein edle Compagnie fieng nechst das Ring Spiel an,", "tokens": ["Ein", "ed\u00b7le", "Com\u00b7pag\u00b7nie", "fi\u00b7eng", "nechst", "das", "Ring", "Spiel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das voller Kurtzweil pflegt von Hand zu Hand zu gehen:", "tokens": ["Das", "vol\u00b7ler", "Kurt\u00b7zweil", "pflegt", "von", "Hand", "zu", "Hand", "zu", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als in dem Circkel must ein lustig M\u00e4gdlein stehen,", "tokens": ["Als", "in", "dem", "Cir\u00b7ckel", "must", "ein", "lus\u00b7tig", "M\u00e4gd\u00b7lein", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VMFIN", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat es den Spiel Ring drauff ins Nachbarn Hand gethan.", "tokens": ["Hat", "es", "den", "Spiel", "Ring", "drauff", "ins", "Nach\u00b7barn", "Hand", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "PAV", "APPRART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nu schaut der Ring fiel ihm zum Kleider Th\u00fcrlein ein:", "tokens": ["Nu", "schaut", "der", "Ring", "fiel", "ihm", "zum", "Klei\u00b7der", "Th\u00fcr\u00b7lein", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie seltzam ist der Fall! Was hilffts es ist geschehen,", "tokens": ["Wie", "selt\u00b7zam", "ist", "der", "Fall", "!", "Was", "hilffts", "es", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O Einfalt volles Kind! Was wiltu dr\u00fcber flehen?", "tokens": ["O", "Ein\u00b7falt", "vol\u00b7les", "Kind", "!", "Was", "wil\u00b7tu", "dr\u00fc\u00b7ber", "fle\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$.", "PWS", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wo sein Finger ist, da w\u00fcnscht der Ring zu seyn.", "tokens": ["Da", "wo", "sein", "Fin\u00b7ger", "ist", ",", "da", "w\u00fcnscht", "der", "Ring", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}