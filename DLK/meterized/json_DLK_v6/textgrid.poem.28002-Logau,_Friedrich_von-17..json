{"textgrid.poem.28002": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es achtet in der Welt nichts Virnula so sehre,", "tokens": ["Es", "ach\u00b7tet", "in", "der", "Welt", "nichts", "Vir\u00b7nu\u00b7la", "so", "seh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PIS", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie billich, als die Zucht und angeboren Ehre;", "tokens": ["Wie", "bil\u00b7lich", ",", "als", "die", "Zucht", "und", "an\u00b7ge\u00b7bo\u00b7ren", "Eh\u00b7re", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Damit sie ihr mit Macht nicht etwa werd entnummen,", "tokens": ["Da\u00b7mit", "sie", "ihr", "mit", "Macht", "nicht", "et\u00b7wa", "werd", "ent\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "PTKNEG", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So hat sie nechst ein Freund von ihr geschenckt bekummen.", "tokens": ["So", "hat", "sie", "nechst", "ein", "Freund", "von", "ihr", "ge\u00b7schenckt", "be\u00b7kum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}