{"dta.poem.12701": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den tod J. M. von Jlkusch bey  \n Cracau aus Polen,   J. U. C.  \n D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Polen ist kein weg ins himmelreich zu finden,", "tokens": ["Aus", "Po\u00b7len", "ist", "kein", "weg", "ins", "him\u00b7mel\u00b7reich", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIAT", "ADV", "APPRART", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum eil ich was ich kan. So schertzte liederlich", "tokens": ["Drum", "eil", "ich", "was", "ich", "kan", ".", "So", "schertz\u00b7te", "lie\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$.", "ADV", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Frantzmann ", "tokens": ["Ein", "Frantz\u00b7mann"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Da\u00df dich dein Polen nicht dem himmel soll verbinden.", "tokens": ["Da\u00df", "dich", "dein", "Po\u00b7len", "nicht", "dem", "him\u00b7mel", "soll", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch du erkennest schon, da\u00df hierinn alles gleich,", "tokens": ["Doch", "du", "er\u00b7ken\u00b7nest", "schon", ",", "da\u00df", "hie\u00b7rinn", "al\u00b7les", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "KOUS", "ADV", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es trifft ein freyer Pol auch hier das Polus-reich.", "tokens": ["Es", "trifft", "ein", "frey\u00b7er", "Pol", "auch", "hier", "das", "Po\u00b7lus\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}