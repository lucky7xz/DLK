{"textgrid.poem.62167": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[nicht im Reichtum wohnt das Gl\u00fcck]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht im Reichtum wohnt das Gl\u00fcck,", "tokens": ["Nicht", "im", "Reich\u00b7tum", "wohnt", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, es weichet scheu zur\u00fcck", "tokens": ["Ach", ",", "es", "wei\u00b7chet", "scheu", "zu\u00b7r\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor den vielen Eitelkeiten,", "tokens": ["Vor", "den", "vie\u00b7len", "Ei\u00b7tel\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sich rasch durch's Geld bereiten.", "tokens": ["Die", "sich", "rasch", "durch's", "Geld", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht im Reichtum wohnt das Gl\u00fcck,", "tokens": ["Nicht", "im", "Reich\u00b7tum", "wohnt", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, es weichet scheu zur\u00fcck", "tokens": ["Ach", ",", "es", "wei\u