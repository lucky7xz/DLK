{"dta.poem.16334": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer einmal hier hat in geliebtem Angesicht", "tokens": ["Wer", "ein\u00b7mal", "hier", "hat", "in", "ge\u00b7lieb\u00b7tem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Todes Bild gesehn, vergi\u00dft es ewig nicht.", "tokens": ["Des", "To\u00b7des", "Bild", "ge\u00b7sehn", ",", "ver\u00b7gi\u00dft", "es", "e\u00b7wig", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Schatten legt, wohin fortan dein Auge schaut,", "tokens": ["Der", "Schat\u00b7ten", "legt", ",", "wo\u00b7hin", "for\u00b7tan", "dein", "Au\u00b7ge", "schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich \u00fcber alles was dir lieb ist oder traut.", "tokens": ["Sich", "\u00fc\u00b7ber", "al\u00b7les", "was", "dir", "lieb", "ist", "o\u00b7der", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIS", "PWS", "PPER", "ADJD", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}