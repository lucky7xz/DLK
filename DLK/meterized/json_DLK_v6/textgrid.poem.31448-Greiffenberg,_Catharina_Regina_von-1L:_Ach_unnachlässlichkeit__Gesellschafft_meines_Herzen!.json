{"textgrid.poem.31448": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach unnachl\u00e4sslichkeit / Gesellschafft meines Herzen!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach unnachl\u00e4sslichkeit / Gesellschafft meines Herzen!", "tokens": ["Ach", "un\u00b7nach\u00b7l\u00e4ss\u00b7lich\u00b7keit", "/", "Ge\u00b7sell\u00b7schafft", "mei\u00b7nes", "Her\u00b7zen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "auf jrdisch hast verkn\u00fcpft / den sonst so freyen Geist:", "tokens": ["auf", "jr\u00b7disch", "hast", "ver\u00b7kn\u00fcpft", "/", "den", "sonst", "so", "frey\u00b7en", "Geist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$(", "ART", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "viel eh er aus dem Leib / als deinen banden / reist;", "tokens": ["viel", "eh", "er", "aus", "dem", "Leib", "/", "als", "dei\u00b7nen", "ban\u00b7den", "/", "reist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "$(", "KOUS", "PPOSAT", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "leidt unerleidbarkeit / verschmerzet alle schmerzen.", "tokens": ["leidt", "un\u00b7er\u00b7leid\u00b7bar\u00b7keit", "/", "ver\u00b7schmer\u00b7zet", "al\u00b7le", "schmer\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$(", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja / die Welt-\u00e4nderung ist Ihm ein blo\u00dfes scherzen:", "tokens": ["Ja", "/", "die", "Welt\u00b7\u00e4n\u00b7de\u00b7rung", "ist", "Ihm", "ein", "blo\u00b7\u00dfes", "scher\u00b7zen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "ART", "NN", "VAFIN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann Atlas / der sein Ziel / der Donner schon zerschmeist /", "tokens": ["Wann", "At\u00b7las", "/", "der", "sein", "Ziel", "/", "der", "Don\u00b7ner", "schon", "zer\u00b7schmeist", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$(", "ART", "PPOSAT", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "das schicksel neue Berg' auf ebner Rennbahn weist:", "tokens": ["das", "schick\u00b7sel", "neu\u00b7e", "Ber\u00b7g'", "auf", "eb\u00b7ner", "Renn\u00b7bahn", "weist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "noch brinnet ewig hell die hohe Hoffnung-Kerzen.", "tokens": ["noch", "brin\u00b7net", "e\u00b7wig", "hell", "die", "ho\u00b7he", "Hoff\u00b7nung\u00b7Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du Erden-Ewigkeit / du starke Engel Tugend /", "tokens": ["Du", "Er\u00b7den\u00b7Ewig\u00b7keit", "/", "du", "star\u00b7ke", "En\u00b7gel", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PPER", "ADJA", "NN", "NN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ach allbesiegende un\u00fcberwindlichkeit!", "tokens": ["Ach", "all\u00b7be\u00b7sie\u00b7gen\u00b7de", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "du weisest deine Krafft in meiner zarten Jugend.", "tokens": ["du", "wei\u00b7sest", "dei\u00b7ne", "Krafft", "in", "mei\u00b7ner", "zar\u00b7ten", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mir wird dadurch mein wunsch / dir Ehr durch mich / bereit.", "tokens": ["Mir", "wird", "da\u00b7durch", "mein", "wunsch", "/", "dir", "Ehr", "durch", "mich", "/", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "PPOSAT", "ADJD", "$(", "PPER", "NN", "APPR", "PPER", "$(", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Fahr' / Edle Freundin / fort! wann H\u00f6ll und Welt zerspringen:", "tokens": ["Fahr'", "/", "Ed\u00b7le", "Freun\u00b7din", "/", "fort", "!", "wann", "H\u00f6ll", "und", "Welt", "zer\u00b7sprin\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "PTKVZ", "$.", "PWAV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "so soll uns unser sach / ob Gott will / doch gelingen.", "tokens": ["so", "soll", "uns", "un\u00b7ser", "sach", "/", "ob", "Gott", "will", "/", "doch", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJD", "$(", "KOUS", "NN", "VMFIN", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}