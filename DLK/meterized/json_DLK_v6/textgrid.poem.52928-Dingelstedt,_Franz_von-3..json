{"textgrid.poem.52928": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt gepredigt, nun ein Jahr, die neue, treue, freie Zeit;", "tokens": ["Ihr", "habt", "ge\u00b7pre\u00b7digt", ",", "nun", "ein", "Jahr", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wann wird die M\u00e4r denn endlich wahr, die neue, treue, freie Zeit?", "tokens": ["Wann", "wird", "die", "M\u00e4r", "denn", "end\u00b7lich", "wahr", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "KON", "ADV", "ADJD", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Der Becker hat und die Geselln geknetet und geheizt genug,", "tokens": ["Der", "Be\u00b7cker", "hat", "und", "die", "Ge\u00b7selln", "ge\u00b7kne\u00b7tet", "und", "ge\u00b7heizt", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "KON", "ART", "NN", "VVPP", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und immer ist das Brot nicht gar, die neue, treue, freie Zeit.", "tokens": ["Und", "im\u00b7mer", "ist", "das", "Brot", "nicht", "gar", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Ihr sa\u00dft schon lange auf dem Ei und gackertet in alle Welt,", "tokens": ["Ihr", "sa\u00dft", "schon", "lan\u00b7ge", "auf", "dem", "Ei", "und", "ga\u00b7cker\u00b7tet", "in", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Allein noch kroch nicht aus der Aar, die neue, treue, freie Zeit.", "tokens": ["Al\u00b7lein", "noch", "kroch", "nicht", "aus", "der", "Aar", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Ein stolzes Wort habt Ihr gewagt, nun eilt, da\u00df es zu Ende kommt,", "tokens": ["Ein", "stol\u00b7zes", "Wort", "habt", "Ihr", "ge\u00b7wagt", ",", "nun", "eilt", ",", "da\u00df", "es", "zu", "En\u00b7de", "kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Und macht uns andern offenbar die neue, treue, freie Zeit.", "tokens": ["Und", "macht", "uns", "an\u00b7dern", "of\u00b7fen\u00b7bar", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJD", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Von ferne klang es \u2013 ha, wie sch\u00f6n! \u2013 von deutscher V\u00f6lker Einigkeit,", "tokens": ["Von", "fer\u00b7ne", "klang", "es", "\u2013", "ha", ",", "wie", "sch\u00f6n", "!", "\u2013", "von", "deut\u00b7scher", "V\u00f6l\u00b7ker", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$(", "ITJ", "$,", "PWAV", "ADJD", "$.", "$(", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.10": {"text": "Man sah sie schon ganz nah und klar, die neue, treue, freie Zeit;", "tokens": ["Man", "sah", "sie", "schon", "ganz", "nah", "und", "klar", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.11": {"text": "Hoch schwebte sie am Kr\u00f6nungsfest ob Euerer entz\u00fcckten Stadt", "tokens": ["Hoch", "schweb\u00b7te", "sie", "am", "Kr\u00f6\u00b7nungs\u00b7fest", "ob", "Eu\u00b7e\u00b7rer", "ent\u00b7z\u00fcck\u00b7ten", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+---+-+", "measure": "unknown.measure.septa"}, "line.12": {"text": "Und trat zum Huldigungsaltar, die neue, treue, freie Zeit,", "tokens": ["Und", "trat", "zum", "Hul\u00b7di\u00b7gungs\u00b7al\u00b7tar", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Sie streifte im Vor\u00fcberwehn selbst mit des Fittigs goldnem Saum", "tokens": ["Sie", "streif\u00b7te", "im", "Vor\u00b7\u00fc\u00b7berw\u00b7ehn", "selbst", "mit", "des", "Fit\u00b7tigs", "gold\u00b7nem", "Saum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.14": {"text": "Den K\u00f6nig und der N\u00e4chsten Schar, die neue, treue, freie Zeit;", "tokens": ["Den", "K\u00f6\u00b7nig", "und", "der", "N\u00e4chs\u00b7ten", "Schar", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.15": {"text": "Doch als nun eine kecke Faust besitzes-froh ergreifen wollt',", "tokens": ["Doch", "als", "nun", "ei\u00b7ne", "ke\u00b7cke", "Faust", "be\u00b7sit\u00b7zes\u00b7froh", "er\u00b7grei\u00b7fen", "wollt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.16": {"text": "Wie die Gelegenheit beim Haar, die neue, treue, freie Zeit,", "tokens": ["Wie", "die", "Ge\u00b7le\u00b7gen\u00b7heit", "beim", "Haar", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.17": {"text": "Da flatterte sie scheu hinweg, und drohend hie\u00df es: Sachte, Freund,", "tokens": ["Da", "flat\u00b7ter\u00b7te", "sie", "scheu", "hin\u00b7weg", ",", "und", "dro\u00b7hend", "hie\u00df", "es", ":", "Sach\u00b7te", ",", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "PPER", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.18": {"text": "Sonst bringt sie dich noch in Gefahr, die neue, treue, freie Zeit.", "tokens": ["Sonst", "bringt", "sie", "dich", "noch", "in", "Ge\u00b7fahr", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.19": {"text": "Ihr schwieget \u2013 und wir \u2013 m\u00e4uschenstill, und nur zuweilen flistert's noch:", "tokens": ["Ihr", "schwie\u00b7get", "\u2013", "und", "wir", "\u2013", "m\u00e4u\u00b7schen\u00b7still", ",", "und", "nur", "zu\u00b7wei\u00b7len", "flis\u00b7tert's", "noch", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "PPER", "$(", "ADJD", "$,", "KON", "ADV", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.20": {"text": "Sie macht sich doch auch gar zu rar, die neue, treue, freie Zeit!", "tokens": ["Sie", "macht", "sich", "doch", "auch", "gar", "zu", "rar", ",", "die", "neu\u00b7e", ",", "treu\u00b7e", ",", "frei\u00b7e", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "ADV", "PTKA", "ADJD", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}