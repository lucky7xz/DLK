{"textgrid.poem.43294": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Alles w\u00e4hrt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Alles w\u00e4hrt", "tokens": ["Das", "Al\u00b7les", "w\u00e4hrt"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nur kurze Zeit,", "tokens": ["Nur", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Dann wird es j\u00e4hlings thauen,", "tokens": ["Dann", "wird", "es", "j\u00e4h\u00b7lings", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann wird gar schnell", "tokens": ["Dann", "wird", "gar", "schnell"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im blauen Kleid", "tokens": ["Im", "blau\u00b7en", "Kleid"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Himmel niederschauen.", "tokens": ["Der", "Him\u00b7mel", "nie\u00b7der\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der alte Thurm", "tokens": ["Der", "al\u00b7te", "Thurm"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wird wieder grau", "tokens": ["Wird", "wie\u00b7der", "grau"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und alle Schwalben kommen,", "tokens": ["Und", "al\u00b7le", "Schwal\u00b7ben", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es kommen dann,", "tokens": ["Es", "kom\u00b7men", "dann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Allm\u00e4lig, lau,", "tokens": ["A\u00b7llm\u00e4\u00b7lig", ",", "lau", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Maid\u00fcfte angeschwommen.", "tokens": ["Mai\u00b7d\u00fcf\u00b7te", "an\u00b7ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie locken Dich", "tokens": ["Sie", "lo\u00b7cken", "Dich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Hin durch den Wald", "tokens": ["Hin", "durch", "den", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Zu Deiner liebsten Stelle ...", "tokens": ["Zu", "Dei\u00b7ner", "liebs\u00b7ten", "Stel\u00b7le", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Weib! ... wenn es thaut,", "tokens": ["Weib", "!", "...", "wenn", "es", "thaut", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Dann bin ich bald", "tokens": ["Dann", "bin", "ich", "bald"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "\u00bbherr Meister!\u00ab \u2013 nicht Geselle.", "tokens": ["\u00bb", "herr", "Meis\u00b7ter", "!", "\u00ab", "\u2013", "nicht", "Ge\u00b7sel\u00b7le", "."], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$.", "$(", "$(", "PTKNEG", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}