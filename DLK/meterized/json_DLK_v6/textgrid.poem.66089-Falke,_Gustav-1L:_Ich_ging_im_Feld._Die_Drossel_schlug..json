{"textgrid.poem.66089": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging im Feld. Die Drossel schlug.", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging im Feld. Die Drossel schlug.", "tokens": ["Ich", "ging", "im", "Feld", ".", "Die", "Dros\u00b7sel", "schlug", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein lindes weiches Wehen trug", "tokens": ["Ein", "lin\u00b7des", "wei\u00b7ches", "We\u00b7hen", "trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von einem wilden Apfelbaum", "tokens": ["Von", "ei\u00b7nem", "wil\u00b7den", "Ap\u00b7fel\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Bl\u00fctenblatt, einen Fr\u00fchlingsflaum.", "tokens": ["Ein", "Bl\u00fc\u00b7ten\u00b7blatt", ",", "ei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7flaum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Da kam aus Osten, h\u00fcgelab,", "tokens": ["Da", "kam", "aus", "Os\u00b7ten", ",", "h\u00fc\u00b7ge\u00b7lab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trug keinen Hut und keinen Stab", "tokens": ["Trug", "kei\u00b7nen", "Hut", "und", "kei\u00b7nen", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00fchrte keinen Ranzen mit,", "tokens": ["Und", "f\u00fchr\u00b7te", "kei\u00b7nen", "Ran\u00b7zen", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Tag im leichten Wanderschritt.", "tokens": ["Der", "Tag", "im", "leich\u00b7ten", "Wan\u00b7der\u00b7schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf seine helle Stirne fiel", "tokens": ["Auf", "sei\u00b7ne", "hel\u00b7le", "Stir\u00b7ne", "fiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein frei Gelock, des Windes Spiel.", "tokens": ["Ein", "frei", "Ge\u00b7lock", ",", "des", "Win\u00b7des", "Spiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Kleid umgab der Glieder Pracht,", "tokens": ["Kein", "Kleid", "um\u00b7gab", "der", "Glie\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nackt schritt er, wie ihn Gott erdacht.", "tokens": ["Nackt", "schritt", "er", ",", "wie", "ihn", "Gott", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur eine Sonnenblume hielt", "tokens": ["Nur", "ei\u00b7ne", "Son\u00b7nen\u00b7blu\u00b7me", "hielt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er in der Linken. Hochgestielt,", "tokens": ["Er", "in", "der", "Lin\u00b7ken", ".", "Hoch\u00b7ges\u00b7tielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der goldne Sternkelch scheitelnah", "tokens": ["Der", "gold\u00b7ne", "Stern\u00b7kelch", "schei\u00b7tel\u00b7nah"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm schwankend \u00fcber die Schulter sah.", "tokens": ["Ihm", "schwan\u00b7kend", "\u00fc\u00b7ber", "die", "Schul\u00b7ter", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "So ging er strahlend gradeaus,", "tokens": ["So", "ging", "er", "strah\u00b7lend", "gra\u00b7de\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber ihm zog mit Gebraus", "tokens": ["Und", "\u00fc\u00b7ber", "ihm", "zog", "mit", "Ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ein Schwarm von wei\u00dfen Schw\u00e4nen mit.", "tokens": ["Ein", "Schwarm", "von", "wei\u00b7\u00dfen", "Schw\u00e4\u00b7nen", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wuchs, wie er das Feld durchschritt,", "tokens": ["Er", "wuchs", ",", "wie", "er", "das", "Feld", "durch\u00b7schritt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und stand zuletzt am Horizont,", "tokens": ["Und", "stand", "zu\u00b7letzt", "am", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Riese, flammend \u00fcbersonnt.", "tokens": ["Ein", "Rie\u00b7se", ",", "flam\u00b7mend", "\u00fc\u00b7ber\u00b7sonnt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um ihn, wie lichte W\u00f6lkchen sahn", "tokens": ["Um", "ihn", ",", "wie", "lich\u00b7te", "W\u00f6lk\u00b7chen", "sahn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "PWAV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die V\u00f6gel aus, Schwan neben Schwan.", "tokens": ["Die", "V\u00f6\u00b7gel", "aus", ",", "Schwan", "ne\u00b7ben", "Schwan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "NN", "APPR", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Und aus dem wei\u00dfen Glitzermeer", "tokens": ["Und", "aus", "dem", "wei\u00b7\u00dfen", "Glit\u00b7zer\u00b7meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gr\u00fc\u00dfte die gelbe Blume her.", "tokens": ["Gr\u00fc\u00df\u00b7te", "die", "gel\u00b7be", "Blu\u00b7me", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}