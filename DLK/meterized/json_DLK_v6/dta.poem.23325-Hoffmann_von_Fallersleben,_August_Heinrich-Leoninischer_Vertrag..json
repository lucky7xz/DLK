{"dta.poem.23325": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Leoninischer Vertrag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.42", "af:0.28", "id:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weh dir! weh dir, mein Vaterland!", "tokens": ["Weh", "dir", "!", "weh", "dir", ",", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Bund, dein eigner Saul,", "tokens": ["Der", "Bund", ",", "dein", "eig\u00b7ner", "Saul", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat dir gebunden jede Hand,", "tokens": ["Hat", "dir", "ge\u00b7bun\u00b7den", "je\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geknebelt dir das Maul.", "tokens": ["Ge\u00b7kne\u00b7belt", "dir", "das", "Maul", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den Knebel weg, den Strick entzwei!", "tokens": ["Den", "Kne\u00b7bel", "weg", ",", "den", "Strick", "ent\u00b7zwei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Frei sollst und mu\u00dft du sein!", "tokens": ["Frei", "sollst", "und", "mu\u00dft", "du", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KON", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und machst du dich nicht endlich frei,", "tokens": ["Und", "machst", "du", "dich", "nicht", "end\u00b7lich", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So schlag der Teufel drein!", "tokens": ["So", "schlag", "der", "Teu\u00b7fel", "drein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}