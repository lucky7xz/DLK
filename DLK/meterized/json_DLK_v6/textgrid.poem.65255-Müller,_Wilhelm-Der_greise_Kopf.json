{"textgrid.poem.65255": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der greise Kopf", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Reif hatt' einen wei\u00dfen Schein", "tokens": ["Der", "Reif", "hatt'", "ei\u00b7nen", "wei\u00b7\u00dfen", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir \u00fcber's Haar gestreuet.", "tokens": ["Mir", "\u00fc\u00b7ber's", "Haar", "ge\u00b7streu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da meint' ich schon ein Greis zu sein,", "tokens": ["Da", "meint'", "ich", "schon", "ein", "Greis", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hab' mich sehr gefreuet.", "tokens": ["Und", "hab'", "mich", "sehr", "ge\u00b7freu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch bald ist er hinweggethaut,", "tokens": ["Doch", "bald", "ist", "er", "hin\u00b7weg\u00b7get\u00b7haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' wieder schwarze Haare,", "tokens": ["Hab'", "wie\u00b7der", "schwar\u00b7ze", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mir's vor meiner Jugend graut \u2013", "tokens": ["Da\u00df", "mir's", "vor", "mei\u00b7ner", "Ju\u00b7gend", "graut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie weit noch bis zur Bahre!", "tokens": ["Wie", "weit", "noch", "bis", "zur", "Bah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vom Abendroth zum Morgenlicht", "tokens": ["Vom", "A\u00b7ben\u00b7droth", "zum", "Mor\u00b7gen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward mancher Kopf zum Greise.", "tokens": ["Ward", "man\u00b7cher", "Kopf", "zum", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer glaubt's? Und meiner ward es nicht", "tokens": ["Wer", "glaubt's", "?", "Und", "mei\u00b7ner", "ward", "es", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "KON", "PPOSAT", "VAFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf dieser ganzen Reise!", "tokens": ["Auf", "die\u00b7ser", "gan\u00b7zen", "Rei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}