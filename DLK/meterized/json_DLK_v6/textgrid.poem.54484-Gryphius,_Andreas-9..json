{"textgrid.poem.54484": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr! der du dich der Menschen blinden Schaaren/", "tokens": ["Herr", "!", "der", "du", "dich", "der", "Men\u00b7schen", "blin\u00b7den", "Schaa\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "PRF", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Nacht vnd Tod erschrecket", "tokens": ["Die", "Nacht", "vnd", "Tod", "er\u00b7schre\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nunmehr entdecket/", "tokens": ["Nun\u00b7mehr", "ent\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "La\u00df deinen Knecht in stillem Friede fahren:", "tokens": ["La\u00df", "dei\u00b7nen", "Knecht", "in", "stil\u00b7lem", "Frie\u00b7de", "fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Herr nim' mich hin: La\u00df nach den tr\u00fcben Jahren:", "tokens": ["Herr", "nim'", "mich", "hin", ":", "La\u00df", "nach", "den", "tr\u00fc\u00b7ben", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "VVIMP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mich lo\u00df! weil der erwecket", "tokens": ["Mich", "lo\u00df", "!", "weil", "der", "er\u00b7we\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "KOUS", "ART", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der/ was beflecket", "tokens": ["Der", "/", "was", "be\u00b7fle\u00b7cket"], "token_info": ["word", "punct", "word", "word"], "pos": ["ART", "$(", "PWS", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Abw\u00e4scht: vnd heil't/ die/ die schon auff der Bahren", "tokens": ["Ab\u00b7w\u00e4scht", ":", "vnd", "heil't", "/", "die", "/", "die", "schon", "auff", "der", "Bah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "VVFIN", "$(", "ART", "$(", "ART", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Mein betr\u00fcbtes Angesicht", "tokens": ["Mein", "be\u00b7tr\u00fcb\u00b7tes", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Siht das edle Lebens-Licht", "tokens": ["Siht", "das", "ed\u00b7le", "Le\u00b7bens\u00b7Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Das du bereitet hast: es sich die neue Sonne!", "tokens": ["Das", "du", "be\u00b7rei\u00b7tet", "hast", ":", "es", "sich", "die", "neu\u00b7e", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$.", "PPER", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den Trost/ der vns erquicket", "tokens": ["Den", "Trost", "/", "der", "vns", "er\u00b7quic\u00b7ket"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wenn/ Noth vnd Sterben dr\u00fccket", "tokens": ["Wenn", "/", "Noth", "vnd", "Ster\u00b7ben", "dr\u00fc\u00b7cket"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Es sicht Israels Ehr/ vnd aller V\u00f6lcker Wonne.", "tokens": ["Es", "sicht", "Is\u00b7raels", "Ehr", "/", "vnd", "al\u00b7ler", "V\u00f6l\u00b7cker", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$(", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}