{"textgrid.poem.26107": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und Jahr um Jahr flog wie ein schwarzer Rab' \u00fcber den Flu\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen stehen die schmeichelnden Maiwolken und verwehen,", "tokens": ["Drau\u00b7\u00dfen", "ste\u00b7hen", "die", "schmei\u00b7cheln\u00b7den", "Mai\u00b7wol\u00b7ken", "und", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVINF", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ins gr\u00fcne Tal unter der streichelnden Sonne fliehen Maienwinde,", "tokens": ["Ins", "gr\u00fc\u00b7ne", "Tal", "un\u00b7ter", "der", "strei\u00b7cheln\u00b7den", "Son\u00b7ne", "flie\u00b7hen", "Mai\u00b7en\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+---+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Und alte Wege ziehen jungblumig \u00fcber der Erde Rinde.", "tokens": ["Und", "al\u00b7te", "We\u00b7ge", "zie\u00b7hen", "jung\u00b7blu\u00b7mig", "\u00fc\u00b7ber", "der", "Er\u00b7de", "Rin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+--+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Alle die Wege bin ich gegangen mit \u00fcberraschten Wangen,", "tokens": ["Al\u00b7le", "die", "We\u00b7ge", "bin", "ich", "ge\u00b7gan\u00b7gen", "mit", "\u00fc\u00b7berr\u00b7aschten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "So weit vom Haus meine Augen hinlangen;", "tokens": ["So", "weit", "vom", "Haus", "mei\u00b7ne", "Au\u00b7gen", "hin\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Half Jahre begraben und aufbauen mit Jugendbangen.", "tokens": ["Half", "Jah\u00b7re", "be\u00b7gra\u00b7ben", "und", "auf\u00b7bau\u00b7en", "mit", "Ju\u00b7gend\u00b7ban\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Jahr um Jahr flog wie ein schwarzer Rab' \u00fcber den Flu\u00df,", "tokens": ["Und", "Jahr", "um", "Jahr", "flog", "wie", "ein", "schwar\u00b7zer", "Rab'", "\u00fc\u00b7ber", "den", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+---+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Der dem Wasser nur schnell sein dunkles Spiegelbild gab;", "tokens": ["Der", "dem", "Was\u00b7ser", "nur", "schnell", "sein", "dunk\u00b7les", "Spie\u00b7gel\u00b7bild", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und alle die Jahre wollten, Geliebteste, nur deinen Ku\u00df.", "tokens": ["Und", "al\u00b7le", "die", "Jah\u00b7re", "woll\u00b7ten", ",", "Ge\u00b7lieb\u00b7tes\u00b7te", ",", "nur", "dei\u00b7nen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "VMFIN", "$,", "NN", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}}}}}