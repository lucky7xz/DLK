{"textgrid.poem.26692": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei willkommen an des Morgens goldnen Toren,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei willkommen an des Morgens goldnen Toren,", "tokens": ["Sei", "will\u00b7kom\u00b7men", "an", "des", "Mor\u00b7gens", "gold\u00b7nen", "To\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Sei willkommen unserm Freudegru\u00df,", "tokens": ["Sei", "will\u00b7kom\u00b7men", "un\u00b7serm", "Freu\u00b7de\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dieses Tages holder Genius,", "tokens": ["Die\u00b7ses", "Ta\u00b7ges", "hol\u00b7der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der den Vielgeliebten uns geboren! \u2013", "tokens": ["Der", "den", "Viel\u00b7ge\u00b7lieb\u00b7ten", "uns", "ge\u00b7bo\u00b7ren", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In erhabener Pracht \u2013", "tokens": ["In", "er\u00b7ha\u00b7be\u00b7ner", "Pracht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Schimmernd tritt er aus der Nacht", "tokens": ["Schim\u00b7mernd", "tritt", "er", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Erdens\u00f6hne keiner,", "tokens": ["Wie", "der", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gro\u00df und trefflich, wie der Sieben einer,", "tokens": ["Gro\u00df", "und", "treff\u00b7lich", ",", "wie", "der", "Sie\u00b7ben", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "NN", "ART", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Die am Throne dienen, schwebt er her.", "tokens": ["Die", "am", "Thro\u00b7ne", "die\u00b7nen", ",", "schwebt", "er", "her", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbstreut mir Blumen \u2013 \u2013 Seht, da bin ich wieder\u00ab", "tokens": ["\u00bb", "streut", "mir", "Blu\u00b7men", "\u2013", "\u2013", "Seht", ",", "da", "bin", "ich", "wie\u00b7der", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "NN", "$(", "$(", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "(ruft er l\u00e4chelnd von dem Himmel nieder)", "tokens": ["(", "ruft", "er", "l\u00e4\u00b7chelnd", "von", "dem", "Him\u00b7mel", "nie\u00b7der", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00bbstreut mir Blumen \u2013 Ich bins wieder,", "tokens": ["\u00bb", "streut", "mir", "Blu\u00b7men", "\u2013", "Ich", "bins", "wie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "NN", "$(", "PPER", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der den Teuren euch gebar,", "tokens": ["Der", "den", "Teu\u00b7ren", "euch", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich bin mehr, als meine andern Br\u00fcder,", "tokens": ["Ich", "bin", "mehr", ",", "als", "mei\u00b7ne", "an\u00b7dern", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Ihren Liebling nennt mich weit und breit", "tokens": ["Ih\u00b7ren", "Lieb\u00b7ling", "nennt", "mich", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Unsre Mutter \u2013 Ewigkeit.\u00ab", "tokens": ["Uns\u00b7re", "Mut\u00b7ter", "\u2013", "E\u00b7wig\u00b7keit", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "(stolz und W\u00fcrde sprach aus der Geb\u00e4rde)", "tokens": ["(", "stolz", "und", "W\u00fcr\u00b7de", "sprach", "aus", "der", "Ge\u00b7b\u00e4r\u00b7de", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00bbeinen Edeln gab ich dieser Erde!", "tokens": ["\u00bb", "ei\u00b7nen", "E\u00b7deln", "gab", "ich", "die\u00b7ser", "Er\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fchlt die Menschheit, wen ich ihr geboren?", "tokens": ["F\u00fchlt", "die", "Menschheit", ",", "wen", "ich", "ihr", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Kennt die Erde meinen Liebling schon?\u00ab", "tokens": ["Kennt", "die", "Er\u00b7de", "mei\u00b7nen", "Lieb\u00b7ling", "schon", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Oder schallen leiser in der Menschen Ohren", "tokens": ["O\u00b7der", "schal\u00b7len", "lei\u00b7ser", "in", "der", "Men\u00b7schen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "\u00bbseine Taten als vor Gottes Thron?", "tokens": ["\u00bb", "sei\u00b7ne", "Ta\u00b7ten", "als", "vor", "Got\u00b7tes", "Thron", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "KOKOM", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Las die Welt in seiner sch\u00f6nen Seele?", "tokens": ["Las", "die", "Welt", "in", "sei\u00b7ner", "sch\u00f6\u00b7nen", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Beugte sich vor seiner gro\u00dfen Seele", "tokens": ["Beug\u00b7te", "sich", "vor", "sei\u00b7ner", "gro\u00b7\u00dfen", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Ehrerbietig sein Jahrhundert schon?", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "sein", "Jahr\u00b7hun\u00b7dert", "schon", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Wuchsen zur Vollendung auf die Keime,", "tokens": ["Wuch\u00b7sen", "zur", "Vol\u00b7len\u00b7dung", "auf", "die", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.11": {"text": "Die ich damals in sein Herz ges\u00e4t?", "tokens": ["Die", "ich", "da\u00b7mals", "in", "sein", "Herz", "ge\u00b7s\u00e4t", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Ist die Welt so sch\u00f6n, wie seine Tr\u00e4ume?", "tokens": ["Ist", "die", "Welt", "so", "sch\u00f6n", ",", "wie", "sei\u00b7ne", "Tr\u00e4u\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Fand er diesen, der ihn ganz versteht?", "tokens": ["Fand", "er", "die\u00b7sen", ",", "der", "ihn", "ganz", "ver\u00b7steht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "O dann la\u00dft mich stolzer durch den Himmel schweben,", "tokens": ["O", "dann", "la\u00dft", "mich", "stol\u00b7zer", "durch", "den", "Him\u00b7mel", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Ich hab ihn gegeben!", "tokens": ["Ich", "hab", "ihn", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Jetzt vollend ich meinen Sonnenlauf,", "tokens": ["Jetzt", "vol\u00b7lend", "ich", "mei\u00b7nen", "Son\u00b7nen\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aber hinter meinem R\u00fccken leuchtet", "tokens": ["A\u00b7ber", "hin\u00b7ter", "mei\u00b7nem", "R\u00fc\u00b7cken", "leuch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schon ein neuer \u2013 sch\u00f6nrer Morgen auf.", "tokens": ["Schon", "ein", "neu\u00b7er", "\u2013", "sch\u00f6n\u00b7rer", "Mor\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$(", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einen Engel tragen seine goldnen Fl\u00fcgel,", "tokens": ["Ei\u00b7nen", "En\u00b7gel", "tra\u00b7gen", "sei\u00b7ne", "gold\u00b7nen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "In des Engels silberklarem Spiegel", "tokens": ["In", "des", "En\u00b7gels", "sil\u00b7ber\u00b7kla\u00b7rem", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Liegt ein Himmel \u2013 und die Ewigkeit.", "tokens": ["Liegt", "ein", "Him\u00b7mel", "\u2013", "und", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Schamrot st\u00fcrz ich in das Meer der Zeit,", "tokens": ["Scham\u00b7rot", "st\u00fcrz", "ich", "in", "das", "Meer", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Nur das Leben", "tokens": ["Nur", "das", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Konnt ich meinem teuren Liebling geben \u2013", "tokens": ["Konnt", "ich", "mei\u00b7nem", "teu\u00b7ren", "Lieb\u00b7ling", "ge\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Dieser Engel \u2013 wie erbleicht mein Ruhm \u2013", "tokens": ["Die\u00b7ser", "En\u00b7gel", "\u2013", "wie", "er\u00b7bleicht", "mein", "Ruhm", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "PWAV", "ADJD", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wandelts in Elysium.\u00ab", "tokens": ["Wan\u00b7delts", "in", "E\u00b7ly\u00b7si\u00b7um", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Seraph sprachs \u2013 \u2013 \u2013 Du liegst in unsern Armen \u2013", "tokens": ["Der", "Se\u00b7raph", "sprachs", "\u2013", "\u2013", "\u2013", "Du", "liegst", "in", "un\u00b7sern", "Ar\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "$(", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wir f\u00fchlen, da\u00df du ", "tokens": ["Wir", "f\u00fch\u00b7len", ",", "da\u00df", "du"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-++", "measure": "unknown.measure.tri"}}}}}