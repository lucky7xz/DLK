{"textgrid.poem.30054": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein grosser Mann! dein Hertz ist, Milo, klein;", "tokens": ["Du", "bist", "ein", "gros\u00b7ser", "Mann", "!", "dein", "Hertz", "ist", ",", "Mi\u00b7lo", ",", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "$,", "NE", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du sagst, es sey so recht und m\u00fcsse billich seyn.", "tokens": ["Du", "sagst", ",", "es", "sey", "so", "recht", "und", "m\u00fcs\u00b7se", "bil\u00b7lich", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Hertze, das zwar klein, sey doch ein solcher Gast,", "tokens": ["Dein", "Hert\u00b7ze", ",", "das", "zwar", "klein", ",", "sey", "doch", "ein", "sol\u00b7cher", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "$,", "VAFIN", "ADV", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fcr den nicht nach Person, nach Werth, geh\u00f6re Rast.", "tokens": ["F\u00fcr", "den", "nicht", "nach", "Per\u00b7son", ",", "nach", "Werth", ",", "ge\u00b7h\u00f6\u00b7re", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKNEG", "APPR", "NE", "$,", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So, so! sonst ist bekant, manch grosses F\u00fcrsten-Haus", "tokens": ["So", ",", "so", "!", "sonst", "ist", "be\u00b7kant", ",", "manch", "gros\u00b7ses", "F\u00fcrs\u00b7ten\u00b7Haus"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "$.", "ADV", "VAFIN", "ADJD", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat einen kleinen Zwerg f\u00fcr einen grossen Claus.", "tokens": ["Hat", "ei\u00b7nen", "klei\u00b7nen", "Zwerg", "f\u00fcr", "ei\u00b7nen", "gros\u00b7sen", "Claus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}