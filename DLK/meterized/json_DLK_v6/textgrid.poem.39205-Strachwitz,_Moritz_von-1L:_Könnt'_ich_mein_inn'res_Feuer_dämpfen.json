{"textgrid.poem.39205": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nnt' ich mein inn'res Feuer d\u00e4mpfen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich mein inn'res Feuer d\u00e4mpfen", "tokens": ["K\u00f6nnt'", "ich", "mein", "inn'\u00b7res", "Feu\u00b7er", "d\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Str\u00f6men von dem eignen Blut,", "tokens": ["Mit", "Str\u00f6\u00b7men", "von", "dem", "eig\u00b7nen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich Dich ritterlich erk\u00e4mpfen,", "tokens": ["K\u00f6nnt'", "ich", "Dich", "rit\u00b7ter\u00b7lich", "er\u00b7k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann w\u00e4re frei und hoch mein Mut.", "tokens": ["Dann", "w\u00e4\u00b7re", "frei", "und", "hoch", "mein", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wollt' ich dann den Nacken heben", "tokens": ["Wie", "wollt'", "ich", "dann", "den", "Na\u00b7cken", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und rufen stolz und trotziglich:", "tokens": ["Und", "ru\u00b7fen", "stolz", "und", "trot\u00b7zig\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jetzt bist Du mein, geliebtes Leben,", "tokens": ["Jetzt", "bist", "Du", "mein", ",", "ge\u00b7lieb\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein, denn ich blutete f\u00fcr Dich!", "tokens": ["Mein", ",", "denn", "ich", "blu\u00b7te\u00b7te", "f\u00fcr", "Dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich schweife durch die \u00f6de Halle,", "tokens": ["Ich", "schwei\u00b7fe", "durch", "die", "\u00f6\u00b7de", "Hal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dumpf an der W\u00f6lbung rauscht mein Tritt;", "tokens": ["Dumpf", "an", "der", "W\u00f6l\u00b7bung", "rauscht", "mein", "Tritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So steigt herauf, Ihr Geister alle,", "tokens": ["So", "steigt", "her\u00b7auf", ",", "Ihr", "Geis\u00b7ter", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Eure Schwerter bringt Euch mit;", "tokens": ["Und", "Eu\u00b7re", "Schwer\u00b7ter", "bringt", "Euch", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zersprengt die S\u00e4rge, brecht die Quadern", "tokens": ["Zer\u00b7sprengt", "die", "S\u00e4r\u00b7ge", ",", "brecht", "die", "Qua\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und tretet vor mich wild und stark;", "tokens": ["Und", "tre\u00b7tet", "vor", "mich", "wild", "und", "stark", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon kocht die Schlacht in meinen Adern,", "tokens": ["Schon", "kocht", "die", "Schlacht", "in", "mei\u00b7nen", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und auch in mir ist Streitermark!", "tokens": ["Und", "auch", "in", "mir", "ist", "Strei\u00b7ter\u00b7mark", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da seid Ihr schon, mit wilder Flamme", "tokens": ["Da", "seid", "Ihr", "schon", ",", "mit", "wil\u00b7der", "Flam\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sticht durchs Visier das Auge klar,", "tokens": ["Sticht", "durchs", "Vi\u00b7sier", "das", "Au\u00b7ge", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Vom goldbekr\u00f6nten Helmeskamme,", "tokens": ["Vom", "gold\u00b7be\u00b7kr\u00f6n\u00b7ten", "Hel\u00b7mes\u00b7kam\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da rauscht des Adlers Schwingenpaar;", "tokens": ["Da", "rauscht", "des", "Ad\u00b7lers", "Schwin\u00b7gen\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es strahlt der Leib in Silberschuppen,", "tokens": ["Es", "strahlt", "der", "Leib", "in", "Sil\u00b7ber\u00b7schup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vom Sporn zur Schulter geht das Schwert,", "tokens": ["Vom", "Sporn", "zur", "Schul\u00b7ter", "geht", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gold prahlt und Scharlach auf den Kruppen,", "tokens": ["Gold", "prahlt", "und", "Schar\u00b7lach", "auf", "den", "Krup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die blanke Stange bei\u00dft das Pferd.", "tokens": ["Die", "blan\u00b7ke", "Stan\u00b7ge", "bei\u00dft", "das", "Pferd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schwingt Euch hinan, entrollt die Banner,", "tokens": ["Schwingt", "Euch", "hi\u00b7nan", ",", "ent\u00b7rollt", "die", "Ban\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Erhebt den Feldruf, sto\u00dft ins Horn!", "tokens": ["Er\u00b7hebt", "den", "Feld\u00b7ruf", ",", "sto\u00dft", "ins", "Horn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Leichtbewehrten, Bogenspanner,", "tokens": ["Ihr", "Leicht\u00b7be\u00b7wehr\u00b7ten", ",", "Bo\u00b7gen\u00b7span\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schw\u00e4rmt lustig an der Spitze vorn!", "tokens": ["Schw\u00e4rmt", "lus\u00b7tig", "an", "der", "Spit\u00b7ze", "vorn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ballt, Reiterf\u00e4hnlein, Euch zusammen,", "tokens": ["Ballt", ",", "Rei\u00b7ter\u00b7f\u00e4hn\u00b7lein", ",", "Euch", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schlie\u00dft Schild an Schild und Speer an Speer,", "tokens": ["Schlie\u00dft", "Schild", "an", "Schild", "und", "Speer", "an", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00dft Panzer dicht an Panzer flammen", "tokens": ["La\u00dft", "Pan\u00b7zer", "dicht", "an", "Pan\u00b7zer", "flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "ADJD", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sprengt, ein ehr'ner Keil, daher!", "tokens": ["Und", "sprengt", ",", "ein", "ehr'\u00b7ner", "Keil", ",", "da\u00b7her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Darauf und dran! Die Sch\u00e4fte splittern,", "tokens": ["Da\u00b7rauf", "und", "dran", "!", "Die", "Sch\u00e4f\u00b7te", "split\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "KON", "PAV", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dumpf durch das Treffen kracht der Sto\u00df.", "tokens": ["Dumpf", "durch", "das", "Tref\u00b7fen", "kracht", "der", "Sto\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun zieht das Schwert und la\u00dft's gewittern", "tokens": ["Nun", "zieht", "das", "Schwert", "und", "la\u00dft's", "ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf die Helme h\u00e4mmert los!", "tokens": ["Und", "auf", "die", "Hel\u00b7me", "h\u00e4m\u00b7mert", "los", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier w\u00e4lzt sich zuckend Ro\u00df und Reiter,", "tokens": ["Hier", "w\u00e4lzt", "sich", "zu\u00b7ckend", "Ro\u00df", "und", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "VVPP", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier steigt ein Arm, die Schiene klafft,", "tokens": ["Hier", "steigt", "ein", "Arm", ",", "die", "Schie\u00b7ne", "klafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eindringt das Schwert und schlitzt sie weiter,", "tokens": ["Ein\u00b7dringt", "das", "Schwert", "und", "schlitzt", "sie", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sprudelnd quillt der Lebenssaft.", "tokens": ["Und", "spru\u00b7delnd", "quillt", "der", "Le\u00b7bens\u00b7saft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier haut die Axt vom Panzer Sp\u00e4ne,", "tokens": ["Hier", "haut", "die", "Axt", "vom", "Pan\u00b7zer", "Sp\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier k\u00fc\u00dft ein Federbusch den Sand,", "tokens": ["Hier", "k\u00fc\u00dft", "ein", "Fe\u00b7der\u00b7busch", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier sinkt ein Haupt bet\u00e4ubt zur M\u00e4hne,", "tokens": ["Hier", "sinkt", "ein", "Haupt", "be\u00b7t\u00e4ubt", "zur", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von den Z\u00fcgeln l\u00e4\u00dft die Hand.", "tokens": ["Und", "von", "den", "Z\u00fc\u00b7geln", "l\u00e4\u00dft", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier f\u00e4hrt ein Pfeil durchs Helmesgitter,", "tokens": ["Hier", "f\u00e4hrt", "ein", "Pfeil", "durchs", "Hel\u00b7mes\u00b7git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier rasselt kunstrecht Hieb in Hieb,", "tokens": ["Hier", "ras\u00b7selt", "kuns\u00b7trecht", "Hieb", "in", "Hieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hier unterm Hufe stirbt ein Ritter,", "tokens": ["Hier", "un\u00b7term", "Hu\u00b7fe", "stirbt", "ein", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sein letzter Seufzer ist sein Lieb. \u2013", "tokens": ["Sein", "letz\u00b7ter", "Seuf\u00b7zer", "ist", "sein", "Lieb", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha! Schwerterleuchten, Helmbuschwehen,", "tokens": ["Ha", "!", "Schwer\u00b7ter\u00b7leuch\u00b7ten", ",", "Helm\u00b7bu\u00b7schwe\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ha! Kampfesnacht und Siegestag,", "tokens": ["Ha", "!", "Kamp\u00b7fes\u00b7nacht", "und", "Sie\u00b7ge\u00b7stag", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich darf Euch nur im Traume sehen,", "tokens": ["Ich", "darf", "Euch", "nur", "im", "Trau\u00b7me", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie stark ich Euch beschw\u00f6ren mag!", "tokens": ["Wie", "stark", "ich", "Euch", "be\u00b7schw\u00f6\u00b7ren", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr spottet mein, Ihr toten H\u00fcnen,", "tokens": ["Ihr", "spot\u00b7tet", "mein", ",", "Ihr", "to\u00b7ten", "H\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Toll nennt mich die vern\u00fcnft'ge Welt;", "tokens": ["Toll", "nennt", "mich", "die", "ver\u00b7n\u00fcnft'\u00b7ge", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein Ritterschlag ist zu verdienen,", "tokens": ["Kein", "Rit\u00b7ter\u00b7schlag", "ist", "zu", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da wo zum Ritter schl\u00e4gt das Geld. \u2013", "tokens": ["Da", "wo", "zum", "Rit\u00b7ter", "schl\u00e4gt", "das", "Geld", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PWAV", "APPRART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du aber, Herrin, l\u00e4chelst wieder:", "tokens": ["Du", "a\u00b7ber", ",", "Her\u00b7rin", ",", "l\u00e4\u00b7chelst", "wie\u00b7der", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bin ja Dein auch ohne Streit!", "tokens": ["Ich", "bin", "ja", "Dein", "auch", "oh\u00b7ne", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das eben ist's, das schl\u00e4gt mich nieder", "tokens": ["Das", "e\u00b7ben", "ist's", ",", "das", "schl\u00e4gt", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hebt mich doch zur selben Zeit.", "tokens": ["Und", "hebt", "mich", "doch", "zur", "sel\u00b7ben", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts ist, des ich mich nicht erk\u00fchnte,", "tokens": ["Nichts", "ist", ",", "des", "ich", "mich", "nicht", "er\u00b7k\u00fchn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "PPER", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Und w\u00e4r's ein sichrer Tod f\u00fcr mich,", "tokens": ["Und", "w\u00e4r's", "ein", "sich\u00b7rer", "Tod", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil ich Dich gerne ", "tokens": ["Weil", "ich", "Dich", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Deswegen st\u00fcrb' ich gern f\u00fcr Dich.", "tokens": ["Des\u00b7we\u00b7gen", "st\u00fcrb'", "ich", "gern", "f\u00fcr", "Dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}