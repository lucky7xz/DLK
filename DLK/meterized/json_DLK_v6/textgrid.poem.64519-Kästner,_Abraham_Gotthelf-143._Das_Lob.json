{"textgrid.poem.64519": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "143. Das Lob", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der nie das Werk, allein den Autor richtet", "tokens": ["Der", "nie", "das", "Werk", ",", "al\u00b7lein", "den", "Au\u00b7tor", "rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O h\u00e4tte man ihn doch, da\u00df du es warst, belehrt!", "tokens": ["O", "h\u00e4t\u00b7te", "man", "ihn", "doch", ",", "da\u00df", "du", "es", "warst", ",", "be\u00b7lehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So w\u00e4r dein Lied nicht durch sein Lob entehrt.", "tokens": ["So", "w\u00e4r", "dein", "Lied", "nicht", "durch", "sein", "Lob", "ent\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}