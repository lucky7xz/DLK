{"textgrid.poem.62263": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[und wo seid ihr, meine Tr\u00e4ume]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wo seid ihr, meine Tr\u00e4ume,", "tokens": ["Und", "wo", "seid", "ihr", ",", "mei\u00b7ne", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wo bist Du, h\u00f6chstes Gl\u00fcck!", "tokens": ["Und", "wo", "bist", "Du", ",", "h\u00f6chs\u00b7tes", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Das ich stets und tief empfunden;", "tokens": ["Das", "ich", "stets", "und", "tief", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, ihr seid nur wei\u00dfe Sch\u00e4ume!", "tokens": ["Ach", ",", "ihr", "seid", "nur", "wei\u00b7\u00dfe", "Sch\u00e4u\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00e4ume, die ins Meer gesunken,", "tokens": ["Sch\u00e4u\u00b7me", ",", "die", "ins", "Meer", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Meer der Wirklichkeit,", "tokens": ["In", "das", "Meer", "der", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenes tr\u00fcbe, graue Wasser. \u2013", "tokens": ["Je\u00b7nes", "tr\u00fc\u00b7be", ",", "grau\u00b7e", "Was\u00b7ser", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDAT", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Gl\u00fcck ist drin ertrunken!", "tokens": ["Und", "das", "Gl\u00fcck", "ist", "drin", "er\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}