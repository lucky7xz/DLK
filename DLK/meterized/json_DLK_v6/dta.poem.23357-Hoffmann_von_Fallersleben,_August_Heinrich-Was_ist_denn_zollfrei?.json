{"dta.poem.23357": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Was ist denn zollfrei?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Besteuert ist die ganze Welt", "tokens": ["Be\u00b7steu\u00b7ert", "ist", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alles drum und dran:", "tokens": ["Und", "al\u00b7les", "drum", "und", "dran", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "KON", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gewerbe, Handel, Gut und Geld,", "tokens": ["Ge\u00b7wer\u00b7be", ",", "Han\u00b7del", ",", "Gut", "und", "Geld", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weg, Wasser, Weib und Mann.", "tokens": ["Weg", ",", "Was\u00b7ser", ",", "Weib", "und", "Mann", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wem w\u00e4re nicht das Leben theuer,", "tokens": ["Wem", "w\u00e4\u00b7re", "nicht", "das", "Le\u00b7ben", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wof\u00fcr man zahlt so manche Steuer?", "tokens": ["Wo\u00b7f\u00fcr", "man", "zahlt", "so", "man\u00b7che", "Steu\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Besteuert ist der Bissen Brot,", "tokens": ["Be\u00b7steu\u00b7ert", "ist", "der", "Bis\u00b7sen", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den man im Schwei\u00df gewinnt!", "tokens": ["Den", "man", "im", "Schwei\u00df", "ge\u00b7winnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Besteuert ist sogar der Tod,", "tokens": ["Be\u00b7steu\u00b7ert", "ist", "so\u00b7gar", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil wir am Ziele sind.", "tokens": ["Weil", "wir", "am", "Zie\u00b7le", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur zu erzeugen unsers Gleichen", "tokens": ["Nur", "zu", "er\u00b7zeu\u00b7gen", "un\u00b7sers", "Glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist frei den Armen und den Reichen.", "tokens": ["Ist", "frei", "den", "Ar\u00b7men", "und", "den", "Rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}