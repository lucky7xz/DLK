{"textgrid.poem.48549": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Hier ist Nichts denn finstre Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist Nichts denn finstre Nacht,", "tokens": ["Hier", "ist", "Nichts", "denn", "finst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "blinde Schatten, schwarze H\u00f6len,", "tokens": ["blin\u00b7de", "Schat\u00b7ten", ",", "schwar\u00b7ze", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da die einversperrten Seelen", "tokens": ["da", "die", "ein\u00b7ver\u00b7sperr\u00b7ten", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "kaum nicht werden umgebracht.", "tokens": ["kaum", "nicht", "wer\u00b7den", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O die dreimal armen Seelen,", "tokens": ["O", "die", "drei\u00b7mal", "ar\u00b7men", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die sich also m\u00fcssen qu\u00e4len!", "tokens": ["die", "sich", "al\u00b7so", "m\u00fcs\u00b7sen", "qu\u00e4\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer ist jener, den du siehst?", "tokens": ["Wer", "ist", "je\u00b7ner", ",", "den", "du", "siehst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ists nicht der, der n\u00e4chtlich sorgend,", "tokens": ["Ists", "nicht", "der", ",", "der", "n\u00e4cht\u00b7lich", "sor\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "t\u00e4glich traurend, allzeit borgend,", "tokens": ["t\u00e4g\u00b7lich", "trau\u00b7rend", ",", "all\u00b7zeit", "bor\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "arm bei gro\u00dfem Reichtum ist?", "tokens": ["arm", "bei", "gro\u00b7\u00dfem", "Reich\u00b7tum", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich erbarmt der armen Seelen,", "tokens": ["Mich", "er\u00b7barmt", "der", "ar\u00b7men", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "die sich so in ihm mu\u00df qu\u00e4len.", "tokens": ["die", "sich", "so", "in", "ihm", "mu\u00df", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "ADV", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Dieser sucht sein h\u00f6chstes Gut", "tokens": ["Die\u00b7ser", "sucht", "sein", "h\u00f6chs\u00b7tes", "Gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "in der Kost und braunen Trauben,", "tokens": ["in", "der", "Kost", "und", "brau\u00b7nen", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "kreucht mitt' Rock' und mit der Schauben,", "tokens": ["kreucht", "mitt'", "Rock'", "und", "mit", "der", "Schau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "tut, was Blut nimmt und den Mut.", "tokens": ["tut", ",", "was", "Blut", "nimmt", "und", "den", "Mut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es ist leichte zu gedenken,", "tokens": ["Es", "ist", "leich\u00b7te", "zu", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "wie die Seele di\u00df mu\u00df kr\u00e4nken.", "tokens": ["wie", "die", "See\u00b7le", "di\u00df", "mu\u00df", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PDS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der, der hier so hoch trit her,", "tokens": ["Der", ",", "der", "hier", "so", "hoch", "trit", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "der ists, den die Ehrend\u00fcnste", "tokens": ["der", "ists", ",", "den", "die", "Eh\u00b7ren\u00b7d\u00fcns\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "und die leichten Hofeg\u00fcnste", "tokens": ["und", "die", "leich\u00b7ten", "Ho\u00b7fe\u00b7g\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "machen auf den Schein so schwer.", "tokens": ["ma\u00b7chen", "auf", "den", "Schein", "so", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Solt' es stehn bei seiner Seelen,", "tokens": ["Solt'", "es", "stehn", "bei", "sei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "sie w\u00fcrd' ihr ein Bessers w\u00e4hlen.", "tokens": ["sie", "w\u00fcrd'", "ihr", "ein", "Bes\u00b7sers", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ist Plato, was Porphyr,", "tokens": ["Was", "ist", "Pla\u00b7to", ",", "was", "Por\u00b7phyr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "$,", "PRELS", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kleobulus, Periander,", "tokens": ["Kleo\u00b7bu\u00b7lus", ",", "Pe\u00b7ri\u00b7an\u00b7der", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Simonides, Aristander", "tokens": ["Si\u00b7mo\u00b7ni\u00b7des", ",", "A\u00b7ris\u00b7tan\u00b7der"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und der Gro\u00dfe von Stagyr?", "tokens": ["und", "der", "Gro\u00b7\u00dfe", "von", "Sta\u00b7gyr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "NE", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Heiden sind sie, taub an Ohren,", "tokens": ["Hei\u00b7den", "sind", "sie", ",", "taub", "an", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "blind an Augen, gro\u00dfe Toren.", "tokens": ["blind", "an", "Au\u00b7gen", ",", "gro\u00b7\u00dfe", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Giebt mir nun die Nacht den Tag?", "tokens": ["Giebt", "mir", "nun", "die", "Nacht", "den", "Tag", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kein Stern kan sich selbst nicht malen,", "tokens": ["Kein", "Stern", "kan", "sich", "selbst", "nicht", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ph\u00f6be selbst borgt' ihre Stralen", "tokens": ["Ph\u00f6\u00b7be", "selbst", "borgt'", "ih\u00b7re", "Stra\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und verleiht sie, weil sie mag.", "tokens": ["und", "ver\u00b7leiht", "sie", ",", "weil", "sie", "mag."], "token_info": ["word", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Sie und ihr Volk mu\u00df erblinden,", "tokens": ["Sie", "und", "ihr", "Volk", "mu\u00df", "er\u00b7blin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "steigt ihr Bruder von der Inden.", "tokens": ["steigt", "ihr", "Bru\u00b7der", "von", "der", "In\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Eitel ists und ohne Frucht,", "tokens": ["Ei\u00b7tel", "ists", "und", "oh\u00b7ne", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "was ihr Eiteln ohne Fr\u00fcchte", "tokens": ["was", "ihr", "Ei\u00b7teln", "oh\u00b7ne", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "von fr\u00fch' an bis unter Lichte", "tokens": ["von", "fr\u00fch'", "an", "bis", "un\u00b7ter", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "in den falschen B\u00fcchern sucht,", "tokens": ["in", "den", "fal\u00b7schen", "B\u00fc\u00b7chern", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "nur da\u00df ihr im Reden-Kriegen", "tokens": ["nur", "da\u00df", "ihr", "im", "Re\u00b7den\u00b7Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hinterlistig ob m\u00f6gt siegen.", "tokens": ["hin\u00b7ter\u00b7lis\u00b7tig", "ob", "m\u00f6gt", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOUS", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mein Gott! Was vertr\u00e4gt man nicht,", "tokens": ["Mein", "Gott", "!", "Was", "ver\u00b7tr\u00e4gt", "man", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "freuret, schwitzet, fastet, wachet,", "tokens": ["freu\u00b7ret", ",", "schwit\u00b7zet", ",", "fas\u00b7tet", ",", "wa\u00b7chet", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "leidet, da\u00df ein Andrer lachet,", "tokens": ["lei\u00b7det", ",", "da\u00df", "ein", "A\u00b7ndrer", "la\u00b7chet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dem es an Vernunft gebricht,", "tokens": ["dem", "es", "an", "Ver\u00b7nunft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "bis man etwas angewonet,", "tokens": ["bis", "man", "et\u00b7was", "an\u00b7ge\u00b7wo\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "das doch endlich wenig lonet.", "tokens": ["das", "doch", "end\u00b7lich", "we\u00b7nig", "lo\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Soll mir denn ein blasses Blat", "tokens": ["Soll", "mir", "denn", "ein", "blas\u00b7ses", "Blat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "so verzaubern Farb' und Sinnen,", "tokens": ["so", "ver\u00b7zau\u00b7bern", "Fa\u00b7rb'", "und", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "soll ich Sch\u00f6nheit hei\u00dfen k\u00f6nnen,", "tokens": ["soll", "ich", "Sch\u00f6n\u00b7heit", "hei\u00b7\u00dfen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was viel Runzeln macht und hat,", "tokens": ["was", "viel", "Run\u00b7zeln", "macht", "und", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "KON", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "und mir durch die Pest der Schriften", "tokens": ["und", "mir", "durch", "die", "Pest", "der", "Schrif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "lassen Seel' und Mark vergiften?", "tokens": ["las\u00b7sen", "Seel'", "und", "Mark", "ver\u00b7gif\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "O ihr Weisen auf den Schein,", "tokens": ["O", "ihr", "Wei\u00b7sen", "auf", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wer bezahlt euch Leib und Leben,", "tokens": ["wer", "be\u00b7zahlt", "euch", "Leib", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "das ihr blicklich hin m\u00fc\u00dft geben,", "tokens": ["d