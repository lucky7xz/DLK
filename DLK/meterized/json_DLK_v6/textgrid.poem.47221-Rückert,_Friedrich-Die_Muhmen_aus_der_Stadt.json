{"textgrid.poem.47221": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Muhmen aus der Stadt", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neulich kamen unsre Muhmen", "tokens": ["Neu\u00b7lich", "ka\u00b7men", "uns\u00b7re", "Muh\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Stadt gefahren,", "tokens": ["Aus", "der", "Stadt", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die von Stroh gemachte Blumen", "tokens": ["Die", "von", "Stroh", "ge\u00b7mach\u00b7te", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trugen in den Haaren.", "tokens": ["Tru\u00b7gen", "in", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und geschwinde ging ich echte", "tokens": ["Und", "ge\u00b7schwin\u00b7de", "ging", "ich", "ech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiesenblumen holen,", "tokens": ["Wie\u00b7sen\u00b7blu\u00b7men", "ho\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und das schlechte Strohgeflechte", "tokens": ["Und", "das", "schlech\u00b7te", "Stroh\u00b7ge\u00b7flech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warf ich in die Kohlen.", "tokens": ["Warf", "ich", "in", "die", "Koh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und ich dachte, mir zu Gnaden", "tokens": ["Und", "ich", "dach\u00b7te", ",", "mir", "zu", "Gna\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich sie verpflichtet;", "tokens": ["Hab'", "ich", "sie", "ver\u00b7pflich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch sie sprachen, einen Schaden", "tokens": ["Doch", "sie", "spra\u00b7chen", ",", "ei\u00b7nen", "Scha\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich angerichtet.", "tokens": ["Hab'", "ich", "an\u00b7ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Sind nicht sch\u00f6ner von der Wiese", "tokens": ["Sind", "nicht", "sch\u00f6\u00b7ner", "von", "der", "Wie\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Feuernelken?", "tokens": ["Mei\u00b7ne", "Feu\u00b7ern\u00b7el\u00b7ken", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbja, doch l\u00e4nger dauern diese,", "tokens": ["\u00bb", "ja", ",", "doch", "l\u00e4n\u00b7ger", "dau\u00b7ern", "die\u00b7se", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "ADJD", "VVFIN", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne zu verwelken.\u00ab", "tokens": ["Oh\u00b7ne", "zu", "ver\u00b7wel\u00b7ken", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ei, so m\u00fc\u00dft ihr, sch\u00f6ne Muhmen,", "tokens": ["Ei", ",", "so", "m\u00fc\u00dft", "ihr", ",", "sch\u00f6\u00b7ne", "Muh\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier nur immer bleiben,", "tokens": ["Hier", "nur", "im\u00b7mer", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich denk' euch frische Blumen", "tokens": ["Und", "ich", "denk'", "euch", "fri\u00b7sche", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich aufzutreiben.", "tokens": ["T\u00e4g\u00b7lich", "auf\u00b7zu\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}