{"dta.poem.10878": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nach der Erndte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohin ist jetzt das Segens-Meer,", "tokens": ["Wo\u00b7hin", "ist", "jetzt", "das", "Se\u00b7gens\u00b7Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das auf dem Felde wallete?", "tokens": ["Das", "auf", "dem", "Fel\u00b7de", "wal\u00b7le\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe ja, so weit ich seh,", "tokens": ["Ich", "se\u00b7he", "ja", ",", "so", "weit", "ich", "seh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ungewohntes gro\u00dfes Leer.", "tokens": ["Ein", "un\u00b7ge\u00b7wohn\u00b7tes", "gro\u00b7\u00dfes", "Leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die scharfen Blicke schauen nichts;", "tokens": ["Die", "schar\u00b7fen", "Bli\u00b7cke", "schau\u00b7en", "nichts", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie m\u00f6gen noch so weit sich strecken.", "tokens": ["Sie", "m\u00f6\u00b7gen", "noch", "so", "weit", "sich", "stre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die vorge Freude des Gesichts", "tokens": ["Die", "vor\u00b7ge", "Freu\u00b7de", "des", "Ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist nirgend weiter zu entdecken.", "tokens": ["Ist", "nir\u00b7gend", "wei\u00b7ter", "zu", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch o gesegnet Leer! wie sch\u00f6n", "tokens": ["Doch", "o", "ge\u00b7seg\u00b7net", "Leer", "!", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "FM", "VVPP", "NN", "$.", "PWAV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ist dein erw\u00fcnschtes Nichts zu sehn!", "tokens": ["Ist", "dein", "er\u00b7w\u00fcnschtes", "Nichts", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Die sch\u00f6ne Frucht, des Blicks Ergetzen,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Frucht", ",", "des", "Blicks", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der reiche Schatz, der nichts zu sch\u00e4tzen,", "tokens": ["Der", "rei\u00b7che", "Schatz", ",", "der", "nichts", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der uns so lange Zeit erfreut,", "tokens": ["Der", "uns", "so", "lan\u00b7ge", "Zeit", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ist nicht nur gl\u00fccklich abgemeyt;", "tokens": ["Ist", "nicht", "nur", "gl\u00fcck\u00b7lich", "ab\u00b7ge\u00b7meyt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Man kunnt ihn, ohne Sturm und Regen,", "tokens": ["Man", "kunnt", "ihn", ",", "oh\u00b7ne", "Sturm", "und", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Gottlob in unsre Scheuren legen!", "tokens": ["Gott\u00b7lob", "in", "uns\u00b7re", "Scheu\u00b7ren", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Daher das Feld, auch ohne Pracht,", "tokens": ["Da\u00b7her", "das", "Feld", ",", "auch", "oh\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Den Augen neue Freude macht.", "tokens": ["Den", "Au\u00b7gen", "neu\u00b7e", "Freu\u00b7de", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Was unser Gott uns nun bescheert,", "tokens": ["Was", "un\u00b7ser", "Gott", "uns", "nun", "be\u00b7scheert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Ist fernern Denkens ja wohl werth.", "tokens": ["Ist", "fer\u00b7nern", "Den\u00b7kens", "ja", "wohl", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Auf recht bewundernswerthe Weise,", "tokens": ["Auf", "recht", "be\u00b7wun\u00b7derns\u00b7wert\u00b7he", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Ist aus der Erde Korn, die Speise,", "tokens": ["Ist", "aus", "der", "Er\u00b7de", "Korn", ",", "die", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wodurch wir uns ern\u00e4hren sollen,", "tokens": ["Wo\u00b7durch", "wir", "uns", "er\u00b7n\u00e4h\u00b7ren", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Dem Schein nach recht hervor gequollen.", "tokens": ["Dem", "Schein", "nach", "recht", "her\u00b7vor", "ge\u00b7quol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der Saft ist, durch so manche R\u00f6hre,", "tokens": ["Der", "Saft", "ist", ",", "durch", "so", "man\u00b7che", "R\u00f6h\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Von unten auf bis zu der Aehre,", "tokens": ["Von", "un\u00b7ten", "auf", "bis", "zu", "der", "A\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Jm holen Halm, empor gef\u00fchrt.", "tokens": ["Jm", "ho\u00b7len", "Halm", ",", "em\u00b7por", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Er hat best\u00e4ndig zirkulirt,", "tokens": ["Er", "hat", "be\u00b7st\u00e4n\u00b7dig", "zir\u00b7ku\u00b7lirt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wie man, nachdem man es ergr\u00fcndet,", "tokens": ["Wie", "man", ",", "nach\u00b7dem", "man", "es", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Den Trieb in allen Pflanzen findet.", "tokens": ["Den", "Trieb", "in", "al\u00b7len", "Pflan\u00b7zen", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Erwege denn, vern\u00fcnftge Seele!", "tokens": ["Er\u00b7we\u00b7ge", "denn", ",", "ver\u00b7n\u00fcnft\u00b7ge", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Sprich, wer formirte die Can\u00e4le,", "tokens": ["Sprich", ",", "wer", "for\u00b7mir\u00b7te", "die", "Ca\u00b7n\u00e4\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Von wem ist dieser zarte Saft,", "tokens": ["Von", "wem", "ist", "die\u00b7ser", "zar\u00b7te", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VAFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Voll Segens-reicher Nahrungskraft,", "tokens": ["Voll", "Se\u00b7gens\u00b7rei\u00b7cher", "Nah\u00b7rungs\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "F\u00fcr uns, auch f\u00fcr das Vieh bereitet?", "tokens": ["F\u00fcr", "uns", ",", "auch", "f\u00fcr", "das", "Vieh", "be\u00b7rei\u00b7tet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Wer hat es dergestalt geleitet?", "tokens": ["Wer", "hat", "es", "der\u00b7ge\u00b7stalt", "ge\u00b7lei\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wer lie\u00df es in den Z\u00e4serlein", "tokens": ["Wer", "lie\u00df", "es", "in", "den", "Z\u00e4\u00b7ser\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Der Wurzel, die kaum sichtbar seyn,", "tokens": ["Der", "Wur\u00b7zel", ",", "die", "kaum", "sicht\u00b7bar", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Jm finstern Schoo\u00df der feuchten Erden,", "tokens": ["Jm", "fins\u00b7tern", "Schoo\u00df", "der", "feuch\u00b7ten", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Zertheilt und als verdauet werden?", "tokens": ["Zer\u00b7theilt", "und", "als", "ver\u00b7dau\u00b7et", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "KOUS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Wer bildete die sch\u00f6nen Aehren,", "tokens": ["Wer", "bil\u00b7de\u00b7te", "die", "sch\u00f6\u00b7nen", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Das nette Korn, die zarte Bl\u00fchte?", "tokens": ["Das", "net\u00b7te", "Korn", ",", "die", "zar\u00b7te", "Bl\u00fch\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Durch wessen unumschr\u00e4nkte G\u00fcte", "tokens": ["Durch", "wes\u00b7sen", "un\u00b7um\u00b7schr\u00e4nk\u00b7te", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Konnt es so reichlich sich vermehren,", "tokens": ["Konnt", "es", "so", "reich\u00b7lich", "sich", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Da\u00df auch die st\u00e4rksten Leiterwagen", "tokens": ["Da\u00df", "auch", "die", "st\u00e4rks\u00b7ten", "Lei\u00b7ter\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Nur kaum die schweren Lasten tragen?", "tokens": ["Nur", "kaum", "die", "schwe\u00b7ren", "Las\u00b7ten", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Da doch nur wenig Zeit zuvor,", "tokens": ["Da", "doch", "nur", "we\u00b7nig", "Zeit", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Der S\u00e4mann alles Samenkorn,", "tokens": ["Der", "S\u00e4\u00b7mann", "al\u00b7les", "Sa\u00b7men\u00b7korn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Woraus nun solch Gewicht entsprossen,", "tokens": ["Wo\u00b7raus", "nun", "solch", "Ge\u00b7wicht", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "In wenig S\u00e4cken eingeschlossen.", "tokens": ["In", "we\u00b7nig", "S\u00e4\u00b7cken", "ein\u00b7ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Je minder wir nun alles fassen,", "tokens": ["Je", "min\u00b7der", "wir", "nun", "al\u00b7les", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Je minder mu\u00df man unterlassen,", "tokens": ["Je", "min\u00b7der", "mu\u00df", "man", "un\u00b7ter\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "An den, in Ehrfurcht, zu gedenken,", "tokens": ["An", "den", ",", "in", "Ehr\u00b7furcht", ",", "zu", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPR", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Der uns, durch seine weise F\u00fchrung,", "tokens": ["Der", "uns", ",", "durch", "sei\u00b7ne", "wei\u00b7se", "F\u00fch\u00b7rung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"tex