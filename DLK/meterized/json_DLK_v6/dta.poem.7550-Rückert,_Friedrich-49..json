{"dta.poem.7550": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "49.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer den kennt, der allein gewirkt hat und gedacht,", "tokens": ["Wer", "den", "kennt", ",", "der", "al\u00b7lein", "ge\u00b7wirkt", "hat", "und", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wird sich nicht r\u00fchmen da\u00df er selbst ein Werk vollbracht.", "tokens": ["Wird", "sich", "nicht", "r\u00fch\u00b7men", "da\u00df", "er", "selbst", "ein", "Werk", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PTKNEG", "VVINF", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Gute das du thust, das thut in dir der Gute,", "tokens": ["Das", "Gu\u00b7te", "das", "du", "thust", ",", "das", "thut", "in", "dir", "der", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "PDS", "VVFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nur das B\u00f6se kannst du thun aus eignem Muthe.", "tokens": ["Und", "nur", "das", "B\u00f6\u00b7se", "kannst", "du", "thun", "aus", "eig\u00b7nem", "Mu\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "PPER", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das B\u00f6s' ist, da\u00df du nicht gedenk des Guten bist;", "tokens": ["Das", "B\u00f6s'", "ist", ",", "da\u00df", "du", "nicht", "ge\u00b7denk", "des", "Gu\u00b7ten", "bist", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sein gedenk du thust, mu\u00df gut seyn wie er ist.", "tokens": ["Was", "sein", "ge\u00b7denk", "du", "thust", ",", "mu\u00df", "gut", "seyn", "wie", "er", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "VVFIN", "PPER", "VVFIN", "$,", "VMFIN", "ADJD", "VAINF", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}