{"textgrid.poem.38227": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Spinnerlied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spinn, M\u00e4gdlein, spinn!", "tokens": ["Spinn", ",", "M\u00e4gd\u00b7lein", ",", "spinn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So wachsen dir die Sinn,", "tokens": ["So", "wach\u00b7sen", "dir", "die", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wachsen dir die gelbe Haar,", "tokens": ["Wach\u00b7sen", "dir", "die", "gel\u00b7be", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommen dir die kluge Jahr!", "tokens": ["Kom\u00b7men", "dir", "die", "klu\u00b7ge", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ehr, M\u00e4gdlein, ehr", "tokens": ["Ehr", ",", "M\u00e4gd\u00b7lein", ",", "ehr"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die alte Spinnkunst sehr;", "tokens": ["Die", "al\u00b7te", "Spinn\u00b7kunst", "sehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Adam hackt und Eva spann,", "tokens": ["A\u00b7dam", "hackt", "und", "E\u00b7va", "spann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeigen uns die Tugend-Bahn.", "tokens": ["Zei\u00b7gen", "uns", "die", "Tu\u00b7gen\u00b7dBahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lieb, M\u00e4gdlein, lieb", "tokens": ["Lieb", ",", "M\u00e4gd\u00b7lein", ",", "lieb"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJD"], "meter": "++-+", "measure": "iambic.di"}, "line.2": {"text": "Der Hanna ihren Trieb;", "tokens": ["Der", "Han\u00b7na", "ih\u00b7ren", "Trieb", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie mit der Spindel kann", "tokens": ["Wie", "sie", "mit", "der", "Spin\u00b7del", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4hren ihren blinden Mann.", "tokens": ["N\u00e4h\u00b7ren", "ih\u00b7ren", "blin\u00b7den", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Prei\u00df, M\u00e4gdlein, prei\u00df", "tokens": ["Prei\u00df", ",", "M\u00e4gd\u00b7lein", ",", "prei\u00df"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Mutter Gottes Flei\u00df;", "tokens": ["Der", "Mut\u00b7ter", "Got\u00b7tes", "Flei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Diese heilge Himmelskron", "tokens": ["Die\u00b7se", "heil\u00b7ge", "Him\u00b7mels\u00b7kron"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spann ein R\u00f6cklein ihrem Sohn.", "tokens": ["Spann", "ein", "R\u00f6ck\u00b7lein", "ih\u00b7rem", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sing, M\u00e4gdlein, sing,", "tokens": ["Sing", ",", "M\u00e4gd\u00b7lein", ",", "sing", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und sey fein guter Ding;", "tokens": ["Und", "sey", "fein", "gu\u00b7ter", "Ding", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fang dein Spinnen lustig an,", "tokens": ["Fang", "dein", "Spin\u00b7nen", "lus\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mach ein frommes End daran.", "tokens": ["Mach", "ein", "from\u00b7mes", "End", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Lern, M\u00e4gdlein, lern,", "tokens": ["Lern", ",", "M\u00e4gd\u00b7lein", ",", "lern", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "++-+", "measure": "iambic.di"}, "line.2": {"text": "So hast du Gl\u00fcck und Stern;", "tokens": ["So", "hast", "du", "Gl\u00fcck", "und", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lerne bei dem Spinnen fort", "tokens": ["Ler\u00b7ne", "bei", "dem", "Spin\u00b7nen", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Furcht und Gotteswort.", "tokens": ["Got\u00b7tes", "Furcht", "und", "Got\u00b7tes\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Glaub, M\u00e4gdlein, glaub,", "tokens": ["Glaub", ",", "M\u00e4gd\u00b7lein", ",", "glaub", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dein Leben sey nur Staub;", "tokens": ["Dein", "Le\u00b7ben", "sey", "nur", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du k\u00f6mmst so schnell ins Grab,", "tokens": ["Da\u00df", "du", "k\u00f6mmst", "so", "schnell", "ins", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als dir bricht der Faden ab.", "tokens": ["Als", "dir", "bricht", "der", "Fa\u00b7den", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lob, M\u00e4gdlein, lob,", "tokens": ["Lob", ",", "M\u00e4gd\u00b7lein", ",", "lob", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dem Sch\u00f6pfer halte Prob;", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "hal\u00b7te", "Prob", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df dir Glaub und Hoffnung wachs,", "tokens": ["Da\u00df", "dir", "Glaub", "und", "Hoff\u00b7nung", "wachs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dein Garn und wie dein Flachs.", "tokens": ["Wie", "dein", "Garn", "und", "wie", "dein", "Flachs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "KON", "PWAV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Dank, M\u00e4gdlein, dank", "tokens": ["Dank", ",", "M\u00e4gd\u00b7lein", ",", "dank"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Dem Herrn, da\u00df du nicht krank,", "tokens": ["Dem", "Herrn", ",", "da\u00df", "du", "nicht", "krank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du kannst fein oft und viel", "tokens": ["Da\u00df", "du", "kannst", "fein", "oft", "und", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Treiben dieses Rockenspiel.", "tokens": ["Trei\u00b7ben", "die\u00b7ses", "Ro\u00b7cken\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dank, M\u00e4gdlein, dank.", "tokens": ["Dank", ",", "M\u00e4gd\u00b7lein", ",", "dank", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}