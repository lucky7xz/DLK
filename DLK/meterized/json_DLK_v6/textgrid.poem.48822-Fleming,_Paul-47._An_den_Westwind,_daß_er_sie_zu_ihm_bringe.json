{"textgrid.poem.48822": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "47. An den Westwind, da\u00df er sie zu ihm bringe", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fleuch, feuchter Zefyr, aus, fleuch, wie nach deiner Floren", "tokens": ["Fleuch", ",", "feuch\u00b7ter", "Ze\u00b7fyr", ",", "aus", ",", "fleuch", ",", "wie", "nach", "dei\u00b7ner", "Flo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PTKVZ", "$,", "VVFIN", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du itzt noch pflegst zu tun, such meinen Aufenthalt,", "tokens": ["du", "itzt", "noch", "pflegst", "zu", "tun", ",", "such", "mei\u00b7nen", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PTKZU", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ob er bei deinem ist, durch diesen Tannenwald.", "tokens": ["ob", "er", "bei", "dei\u00b7nem", "ist", ",", "durch", "die\u00b7sen", "Tan\u00b7nen\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "VAFIN", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Such! wie du deine hast, so hab ich sie verloren.", "tokens": ["Such", "!", "wie", "du", "dei\u00b7ne", "hast", ",", "so", "hab", "ich", "sie", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "PPER", "PPOSAT", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Such sie und sag ihr das in ihre leise Ohren:", "tokens": ["Such", "sie", "und", "sag", "ihr", "das", "in", "ih\u00b7re", "lei\u00b7se", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "KON", "VVFIN", "PPER", "PDS", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort ist er, der dich w\u00fcndscht, da g\u00f6tliche Gestalt,", "tokens": ["Dort", "ist", "er", ",", "der", "dich", "w\u00fcnd\u00b7scht", ",", "da", "g\u00f6t\u00b7li\u00b7che", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "dort ist er, der dich hofft. Erfreust du ihn nicht bald,", "tokens": ["dort", "ist", "er", ",", "der", "dich", "hofft", ".", "Er\u00b7freust", "du", "ihn", "nicht", "bald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so hat er seinen Ort zum Grabe schon erkoren.", "tokens": ["so", "hat", "er", "sei\u00b7nen", "Ort", "zum", "Gra\u00b7be", "schon", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nim sie, so bald sie will, in deinen Blumenscho\u00df,", "tokens": ["Nim", "sie", ",", "so", "bald", "sie", "will", ",", "in", "dei\u00b7nen", "Blu\u00b7men\u00b7scho\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df keine tr\u00fcbe Luft auf meine Sch\u00f6nheit sto\u00df',", "tokens": ["da\u00df", "kei\u00b7ne", "tr\u00fc\u00b7be", "Luft", "auf", "mei\u00b7ne", "Sch\u00f6n\u00b7heit", "sto\u00df'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und h\u00fclle sie in dich und la\u00df es Niemand wissen.", "tokens": ["und", "h\u00fcl\u00b7le", "sie", "in", "dich", "und", "la\u00df", "es", "Nie\u00b7mand", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "KON", "VVIMP", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hier wart' ich, meine Post, ich warte mit Begier,", "tokens": ["Hier", "wart'", "ich", ",", "mei\u00b7ne", "Post", ",", "ich", "war\u00b7te", "mit", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dich bald zu nehmen an, mit tausent G\u00f6ttern hier,", "tokens": ["dich", "bald", "zu", "neh\u00b7men", "an", ",", "mit", "tau\u00b7sent", "G\u00f6t\u00b7tern", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "PTKVZ", "$,", "APPR", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "sie, meiner Augen Trost, mit hunderttausent K\u00fcssen.", "tokens": ["sie", ",", "mei\u00b7ner", "Au\u00b7gen", "Trost", ",", "mit", "hun\u00b7dert\u00b7tau\u00b7sent", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "NN", "$,", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}