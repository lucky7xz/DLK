{"textgrid.poem.60754": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du r\u00fchmst die Zeit, in welcher deine Kaste", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du r\u00fchmst die Zeit, in welcher deine Kaste", "tokens": ["Du", "r\u00fchmst", "die", "Zeit", ",", "in", "wel\u00b7cher", "dei\u00b7ne", "Kas\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geno\u00df ein ruhig Gl\u00fcck?", "tokens": ["Ge\u00b7no\u00df", "ein", "ru\u00b7hig", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was aber, au\u00dfer einer Puderquaste,", "tokens": ["Was", "a\u00b7ber", ",", "au\u00b7\u00dfer", "ei\u00b7ner", "Pu\u00b7der\u00b7quas\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lie\u00df jene goldne Zeit zur\u00fcck?", "tokens": ["Lie\u00df", "je\u00b7ne", "gold\u00b7ne", "Zeit", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kann blo\u00df Vergangnes dein Gem\u00fct erg\u00f6tzen,", "tokens": ["Kann", "blo\u00df", "Ver\u00b7gang\u00b7nes", "dein", "Ge\u00b7m\u00fct", "er\u00b7g\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht frische, warme Tat?", "tokens": ["Nicht", "fri\u00b7sche", ",", "war\u00b7me", "Tat", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was blickst du r\u00fcckw\u00e4rts nach den alten G\u00f6tzen,", "tokens": ["Was", "blickst", "du", "r\u00fcck\u00b7w\u00e4rts", "nach", "den", "al\u00b7ten", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Julian, der Apostat?", "tokens": ["Wie", "Ju\u00b7li\u00b7an", ",", "der", "A\u00b7pos\u00b7tat", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es f\u00fchrt die Freiheit ihren goldnen Morgen", "tokens": ["Es", "f\u00fchrt", "die", "Frei\u00b7heit", "ih\u00b7ren", "gold\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Strahlenglanz herbei!", "tokens": ["Im", "Strah\u00b7len\u00b7glanz", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Finstern, sagst du, schlich sie lang verborgen:", "tokens": ["Im", "Fins\u00b7tern", ",", "sagst", "du", ",", "schlich", "sie", "lang", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das war die Schuld der Tyrannei.", "tokens": ["Das", "war", "die", "Schuld", "der", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer spr\u00e4che laut, wenn's ein Despot verwehret,", "tokens": ["Wer", "spr\u00e4\u00b7che", "laut", ",", "wenn's", "ein", "Des\u00b7pot", "ver\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Allen schlie\u00dft den Mund?", "tokens": ["Der", "Al\u00b7len", "schlie\u00dft", "den", "Mund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Selbst Christi Wort, das alle Welt verehret,", "tokens": ["Selbst", "Chris\u00b7ti", "Wort", ",", "das", "al\u00b7le", "Welt", "ver\u00b7eh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War lang nur ein geheimer Bund.", "tokens": ["War", "lang", "nur", "ein", "ge\u00b7hei\u00b7mer", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nicht B\u00f6se blo\u00df verbergen ihre Taten,", "tokens": ["Nicht", "B\u00f6\u00b7se", "blo\u00df", "ver\u00b7ber\u00b7gen", "ih\u00b7re", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch Tugend h\u00fcllt sich ein:", "tokens": ["Auch", "Tu\u00b7gend", "h\u00fcllt", "sich", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Vaterland, auf offnem Markt verraten,", "tokens": ["Das", "Va\u00b7ter\u00b7land", ",", "auf", "off\u00b7nem", "Markt", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weint seine Tr\u00e4ne ganz allein!", "tokens": ["Weint", "sei\u00b7ne", "Tr\u00e4\u00b7ne", "ganz", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Den Herrscher, sagst du, soll ein Zepter zieren,", "tokens": ["Den", "Herr\u00b7scher", ",", "sagst", "du", ",", "soll", "ein", "Zep\u00b7ter", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das unumschr\u00e4nkt befiehlt,", "tokens": ["Das", "un\u00b7um\u00b7schr\u00e4nkt", "be\u00b7fiehlt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als st\u00fcnd ein Mensch er zwischen wilden Tieren,", "tokens": ["Als", "st\u00fcnd", "ein", "Mensch", "er", "zwi\u00b7schen", "wil\u00b7den", "Tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach denen seine Flinte zielt!", "tokens": ["Nach", "de\u00b7nen", "sei\u00b7ne", "Flin\u00b7te", "zielt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du willst der Rede setzen ihre Schranke,", "tokens": ["Du", "willst", "der", "Re\u00b7de", "set\u00b7zen", "ih\u00b7re", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einkerkern Schrift und Wort?", "tokens": ["Ein\u00b7ker\u00b7kern", "Schrift", "und", "Wort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Umsonst! Es w\u00e4lzt sich jeder Glutgedanke", "tokens": ["Um\u00b7sonst", "!", "Es", "w\u00e4lzt", "sich", "je\u00b7der", "Glut\u00b7ge\u00b7dan\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bacchantisch und unsterblich fort!", "tokens": ["Bac\u00b7chan\u00b7tisch", "und", "uns\u00b7terb\u00b7lich", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Umsonst, Verstockter, tadelst du das Neue,", "tokens": ["Um\u00b7sonst", ",", "Ver\u00b7stock\u00b7ter", ",", "ta\u00b7delst", "du", "das", "Neu\u00b7e", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allm\u00e4chtig herrscht die Zeit:", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "herrscht", "die", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwar eine sch\u00f6ne Tugend ist die Treue,", "tokens": ["Zwar", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Tu\u00b7gend", "ist", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch sch\u00f6ner ist Gerechtigkeit!", "tokens": ["Doch", "sch\u00f6\u00b7ner", "ist", "Ge\u00b7rech\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und ist es neu, was einst der Weltgemeinde", "tokens": ["Und", "ist", "es", "neu", ",", "was", "einst", "der", "Welt\u00b7ge\u00b7mein\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freiheit verliehn und Glanz,", "tokens": ["Frei\u00b7heit", "ver\u00b7liehn", "und", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor jenem f\u00fcnften Karl und seinem Feinde,", "tokens": ["Vor", "je\u00b7nem", "f\u00fcnf\u00b7ten", "Karl", "und", "sei\u00b7nem", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NE", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem schn\u00f6den Unterdr\u00fccker Franz?", "tokens": ["Dem", "schn\u00f6\u00b7den", "Un\u00b7ter\u00b7dr\u00fc\u00b7cker", "Franz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Und sollt ich sterben einst wie Ulrich Hutten,", "tokens": ["Und", "sollt", "ich", "ster\u00b7ben", "einst", "wie", "Ul\u00b7rich", "Hut\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "KOKOM", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verlassen und allein,", "tokens": ["Ver\u00b7las\u00b7sen", "und", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Abziehn den Heuchlern will ich ihre Kutten:", "tokens": ["Ab\u00b7ziehn", "den", "Heuch\u00b7lern", "will", "ich", "ih\u00b7re", "Kut\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Nicht lohnt's der M\u00fche, schlecht zu sein!", "tokens": ["Nicht", "lohnt's", "der", "M\u00fc\u00b7he", ",", "schlecht", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "ART", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}