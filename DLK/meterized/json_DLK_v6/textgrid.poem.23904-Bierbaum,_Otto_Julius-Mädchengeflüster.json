{"textgrid.poem.23904": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchengefl\u00fcster", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gefl\u00fcster aus M\u00e4dchenmunde", "tokens": ["Ge\u00b7fl\u00fcs\u00b7ter", "aus", "M\u00e4d\u00b7chen\u00b7mun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In sommern\u00e4chtiger Stunde,", "tokens": ["In", "som\u00b7mer\u00b7n\u00e4ch\u00b7ti\u00b7ger", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das ist wie M\u00e4rchengesumm;", "tokens": ["Das", "ist", "wie", "M\u00e4r\u00b7chen\u00b7ge\u00b7summ", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Drin raunt das Werden der Zeiten,", "tokens": ["Drin", "raunt", "das", "Wer\u00b7den", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Viel Lachen und viel Leiden,", "tokens": ["Viel", "La\u00b7chen", "und", "viel", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wie beim Wiegenliede der Mutter steh ich stumm.", "tokens": ["Und", "wie", "beim", "Wie\u00b7gen\u00b7lie\u00b7de", "der", "Mut\u00b7ter", "steh", "ich", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie wissen nicht, was sie fragen,", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "was", "sie", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie wissen nicht, was sie sagen,", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "was", "sie", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ihrer Worte Klang", "tokens": ["Und", "ih\u00b7rer", "Wor\u00b7te", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist doch ein tiefes K\u00fcnden", "tokens": ["Ist", "doch", "ein", "tie\u00b7fes", "K\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus allen Lebens Gr\u00fcnden;", "tokens": ["Aus", "al\u00b7len", "Le\u00b7bens", "Gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie wird es mir beim Klange der Glocken am Ostern bang.", "tokens": ["Wie", "wird", "es", "mir", "beim", "Klan\u00b7ge", "der", "Glo\u00b7cken", "am", "Os\u00b7tern", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus scheuen D\u00e4mmerungen", "tokens": ["Aus", "scheu\u00b7en", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wispern des Lebens Zungen;", "tokens": ["Wis\u00b7pern", "des", "Le\u00b7bens", "Zun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das ewige R\u00e4tsel lallt.", "tokens": ["Das", "e\u00b7wi\u00b7ge", "R\u00e4t\u00b7sel", "lallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da wird es den M\u00e4dchen bange", "tokens": ["Da", "wird", "es", "den", "M\u00e4d\u00b7chen", "ban\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Vor ihrem eigenen Klange,", "tokens": ["Vor", "ih\u00b7rem", "ei\u00b7ge\u00b7nen", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aus dem das Wellenversinken der Welten widerhallt.", "tokens": ["Aus", "dem", "das", "Wel\u00b7len\u00b7ver\u00b7sin\u00b7ken", "der", "Wel\u00b7ten", "wi\u00b7der\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}