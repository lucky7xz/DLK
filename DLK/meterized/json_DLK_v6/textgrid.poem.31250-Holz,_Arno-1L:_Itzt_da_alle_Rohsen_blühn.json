{"textgrid.poem.31250": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Itzt/ da alle Rohsen bl\u00fchn/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Itzt/ da alle Rohsen bl\u00fchn/", "tokens": ["Itzt", "/", "da", "al\u00b7le", "Roh\u00b7sen", "bl\u00fchn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "dafelt man bloh\u00df noch im Gr\u00fcn/", "tokens": ["da\u00b7felt", "man", "bloh\u00df", "noch", "im", "Gr\u00fcn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPRART", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "wo drey wunder-nette Bircken", "tokens": ["wo", "drey", "wun\u00b7der\u00b7net\u00b7te", "Bir\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "eine Wasen-Banck \u00fcmbzircken.", "tokens": ["ei\u00b7ne", "Wa\u00b7sen\u00b7Banck", "\u00fcm\u00b7bzir\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kleine Bluhmen blau und wei\u00df", "tokens": ["Klei\u00b7ne", "Bluh\u00b7men", "blau", "und", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "z\u00e4ubern dort ein Paradei\u00df/", "tokens": ["z\u00e4u\u00b7bern", "dort", "ein", "Pa\u00b7ra\u00b7dei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "dreyn sich K\u00e4ferckens und Hummeln/", "tokens": ["dreyn", "sich", "K\u00e4\u00b7fer\u00b7ckens", "und", "Hum\u00b7meln", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PRF", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "ja s\u00e4lbst Schmetterlinge dummeln.", "tokens": ["ja", "s\u00e4lbst", "Schmet\u00b7ter\u00b7lin\u00b7ge", "dum\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Grawitetisch Schritt for Schritt/", "tokens": ["Gra\u00b7wi\u00b7te\u00b7tisch", "Schritt", "for", "Schritt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$("], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "jeder nimbt sich Seine mit/", "tokens": ["je\u00b7der", "nimbt", "sich", "Sei\u00b7ne", "mit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PPOSAT", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "durch die bundten Laub-Verh\u00e4nge/", "tokens": ["durch", "die", "bund\u00b7ten", "Laub\u00b7Ver\u00b7h\u00e4n\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wandeln wir die Tulpen-G\u00e4nge.", "tokens": ["wan\u00b7deln", "wir", "die", "Tul\u00b7pen\u00b7G\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie verzukkt enthaucht ein Ah/", "tokens": ["Wie", "ver\u00b7zukkt", "ent\u00b7haucht", "ein", "Ah", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "itzt so sind wir endlich da/", "tokens": ["itzt", "so", "sind", "wir", "end\u00b7lich", "da", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "lihblich r\u00e4ucht es allenthalben", "tokens": ["lih\u00b7blich", "r\u00e4ucht", "es", "al\u00b7len\u00b7thal\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "und die Lufft durchtzwittschern Schwalben.", "tokens": ["und", "die", "Lufft", "durcht\u00b7zwitt\u00b7schern", "Schwal\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Chloe/ geu\u00df uns Koffe ein/", "tokens": ["Chloe", "/", "geu\u00df", "uns", "Kof\u00b7fe", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVIMP", "PPER", "NN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "der erfreut itzt mehr denn Wein/", "tokens": ["der", "er\u00b7freut", "itzt", "mehr", "denn", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "ADV", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zu gebakknem Lamms-Geschlinge", "tokens": ["zu", "ge\u00b7bak\u00b7knem", "Lamms\u00b7Ge\u00b7schlin\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "machen sich itzt Pfifferlinge!", "tokens": ["ma\u00b7chen", "sich", "itzt", "Pfif\u00b7fer\u00b7lin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Butter-Milch mit Bayrisch-Kraut", "tokens": ["But\u00b7ter\u00b7Milch", "mit", "Bay\u00b7rischKraut"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "schafft uns nicht zu grohbe Haut/", "tokens": ["schafft", "uns", "nicht", "zu", "groh\u00b7be", "Haut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "freundlich reichen wir einander", "tokens": ["freund\u00b7lich", "rei\u00b7chen", "wir", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "blau gekochten Bley und Zander.", "tokens": ["blau", "ge\u00b7koch\u00b7ten", "Bley", "und", "Zan\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Pamfilenchen streicht galant", "tokens": ["Pam\u00b7fi\u00b7len\u00b7chen", "streicht", "ga\u00b7lant"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "NE"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Kowjar-Schnittgens for Palant/", "tokens": ["Ko\u00b7wja\u00b7rSchnitt\u00b7gens", "for", "Pa\u00b7lant", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zahrt durch ihr korallnes Pf\u00f6rtgen", "tokens": ["zahrt", "durch", "ihr", "ko\u00b7rall\u00b7nes", "Pf\u00f6rt\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schihbt er ihr ein Erdbeer-D\u00f6rtgen.", "tokens": ["schihbt", "er", "ihr", "ein", "Erd\u00b7beer\u00b7D\u00f6rt\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doris drukkt sich rund und froh", "tokens": ["Do\u00b7ris", "drukkt", "sich", "rund", "und", "froh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "r\u00e4cht an ihren Florido/", "tokens": ["r\u00e4cht", "an", "ih\u00b7ren", "Flo\u00b7ri\u00b7do", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Damon angelt unterm Dische/", "tokens": ["Da\u00b7mon", "an\u00b7gelt", "un\u00b7term", "Di\u00b7sche", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "da\u00df er Flaviens Fu\u00df erwische.", "tokens": ["da\u00df", "er", "Fla\u00b7vi\u00b7ens", "Fu\u00df", "er\u00b7wi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "sichs f\u00fcrtrefflig musicirt/", "tokens": ["sichs", "f\u00fcr\u00b7treff\u00b7lig", "mu\u00b7si\u00b7cirt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harffen/ Lauten/ Zymbeln/ Geigen/", "tokens": ["Harf\u00b7fen", "/", "Lau\u00b7ten", "/", "Zym\u00b7beln", "/", "Gei\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NE", "$(", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "itzt d\u00fcrfft ihr nicht lenger schweigen!", "tokens": ["itzt", "d\u00fcrfft", "ihr", "nicht", "len\u00b7ger", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Stimmt die Kehlen/ Mann for Mann/", "tokens": ["Stimmt", "die", "Keh\u00b7len", "/", "Mann", "for", "Mann", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "alles hebt zu singen an/", "tokens": ["al\u00b7les", "hebt", "zu", "sin\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKZU", "VVINF", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rosilis und Philirille/", "tokens": ["Ro\u00b7si\u00b7lis", "und", "Phi\u00b7li\u00b7ril\u00b7le", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "keine h\u00e4lt ihr M\u00e4ulgen stille!", "tokens": ["kei\u00b7ne", "h\u00e4lt", "ihr", "M\u00e4ul\u00b7gen", "stil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit der sch\u00f6nen Galathee", "tokens": ["Mit", "der", "sch\u00f6\u00b7nen", "Ga\u00b7la\u00b7thee"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "w\u00e4ltz ich mich schon fast im Klee/", "tokens": ["w\u00e4ltz", "ich", "mich", "schon", "fast", "im", "Klee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "lasst uns mit gef\u00fclltem Pantzen", "tokens": ["lasst", "uns", "mit", "ge\u00b7f\u00fcll\u00b7tem", "Pant\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "rund \u00fcmb dihse B\u00e4umlein dantzen!", "tokens": ["rund", "\u00fcmb", "dih\u00b7se", "B\u00e4um\u00b7lein", "dant\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles jubelt/ juhcht und schreyt:", "tokens": ["Al\u00b7les", "ju\u00b7belt", "/", "juhcht", "und", "schreyt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "ADJD", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "O du Sonnen-s\u00fcsse Zeit!", "tokens": ["O", "du", "Son\u00b7nen\u00b7s\u00fcs\u00b7se", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nakkt/ auff hundret weissen W\u00f6lckgen/", "tokens": ["Nakkt", "/", "auff", "hund\u00b7ret", "weis\u00b7sen", "W\u00f6lck\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "APPR", "VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "siht uns zu ein ", "tokens": ["siht", "uns", "zu", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART"], "meter": "+--+", "measure": "iambic.di.chol"}}}}}