{"textgrid.poem.61230": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun str\u00f6met klar von oben", "tokens": ["Nun", "str\u00f6\u00b7met", "klar", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tag ins Land herein,", "tokens": ["Der", "Tag", "ins", "Land", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus tiefem Blau gewoben", "tokens": ["Aus", "tie\u00b7fem", "Blau", "ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und lichtem Sonnenschein.", "tokens": ["Und", "lich\u00b7tem", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es will noch einmal bl\u00fchen", "tokens": ["Es", "will", "noch", "ein\u00b7mal", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wald, bevor er starb;", "tokens": ["Der", "Wald", ",", "be\u00b7vor", "er", "starb", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er prangt in goldnem Gl\u00fchen", "tokens": ["Er", "prangt", "in", "gold\u00b7nem", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und l\u00e4chelt purpurfarb.", "tokens": ["Und", "l\u00e4\u00b7chelt", "pur\u00b7purf\u00b7arb", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und fern im Glanze schlie\u00dfet", "tokens": ["Und", "fern", "im", "Glan\u00b7ze", "schlie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich Berg an Berg gereiht,", "tokens": ["Sich", "Berg", "an", "Berg", "ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Sabbatstille flie\u00dfet", "tokens": ["Und", "Sab\u00b7bats\u00b7til\u00b7le", "flie\u00b7\u00dfet"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Tale weit und breit.", "tokens": ["Im", "Ta\u00b7le", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was will dich's wundernehmen,", "tokens": ["Was", "will", "dich's", "wun\u00b7der\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Freund, zu dieser Frist,", "tokens": ["O", "Freund", ",", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df deine Brust ihr Gr\u00e4men", "tokens": ["Da\u00df", "dei\u00b7ne", "Brust", "ihr", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie einen Traum vergi\u00dft?", "tokens": ["Wie", "ei\u00b7nen", "Traum", "ver\u00b7gi\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df du der alten Sorgen", "tokens": ["Da\u00df", "du", "der", "al\u00b7ten", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit L\u00e4cheln nur gedenkst", "tokens": ["Mit", "L\u00e4\u00b7cheln", "nur", "ge\u00b7denkst"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in den goldnen Morgen", "tokens": ["Und", "in", "den", "gold\u00b7nen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich voll und froh versenkst?", "tokens": ["Dich", "voll", "und", "froh", "ver\u00b7senkst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O gib dich hin dem Frieden", "tokens": ["O", "gib", "dich", "hin", "dem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sauge diesen Glanz,", "tokens": ["Und", "sau\u00b7ge", "die\u00b7sen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der aller Welt beschieden,", "tokens": ["Der", "al\u00b7ler", "Welt", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In deine Seele ganz.", "tokens": ["In", "dei\u00b7ne", "See\u00b7le", "ganz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "La\u00df Ruh' und Lied sich gatten", "tokens": ["La\u00df", "Ruh'", "und", "Lied", "sich", "gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei frommem Harfenklang,", "tokens": ["Bei", "from\u00b7mem", "Har\u00b7fen\u00b7klang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der letzten Trauer Schatten", "tokens": ["Der", "letz\u00b7ten", "Trau\u00b7er", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vers\u00fchne mit Gesang.", "tokens": ["Ver\u00b7s\u00fch\u00b7ne", "mit", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Sonne heb entgegen", "tokens": ["Der", "Son\u00b7ne", "heb", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Becher jungen Weins,", "tokens": ["Den", "Be\u00b7cher", "jun\u00b7gen", "Weins", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und heischt der Trunk den Segen,", "tokens": ["Und", "heischt", "der", "Trunk", "den", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So w\u00fcnsche segnend eins:", "tokens": ["So", "w\u00fcn\u00b7sche", "seg\u00b7nend", "eins", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da\u00df, wenn nach Freud' und Leide", "tokens": ["Da\u00df", ",", "wenn", "nach", "Freud'", "und", "Lei\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Herz einst brechen will,", "tokens": ["Dein", "Herz", "einst", "bre\u00b7chen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie dieser Herbst es scheide", "tokens": ["Wie", "die\u00b7ser", "Herbst", "es", "schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So heiter, gro\u00df und still.", "tokens": ["So", "hei\u00b7ter", ",", "gro\u00df", "und", "still", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}