{"dta.poem.21536": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Melpomene .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Der Beifall rauscht durch das volle Haus,", "tokens": ["Der", "Bei\u00b7fall", "rauscht", "durch", "das", "vol\u00b7le", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Hoch dem holden Juwele!", "tokens": ["Ein", "Hoch", "dem", "hol\u00b7den", "Ju\u00b7we\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du triebst uns den lebenden Athem heraus", "tokens": ["Du", "triebst", "uns", "den", "le\u00b7ben\u00b7den", "A\u00b7them", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Aus der mitleidgefolterten Seele!", "tokens": ["Aus", "der", "mit\u00b7leid\u00b7ge\u00b7fol\u00b7ter\u00b7ten", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Du strahlendes Kleinod hervor, nur hervor!", "tokens": ["Du", "strah\u00b7len\u00b7des", "Klei\u00b7nod", "her\u00b7vor", ",", "nur", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "$,", "ADV", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "War das ein K\u00e4mpfen, ein Ringen!", "tokens": ["War", "das", "ein", "K\u00e4mp\u00b7fen", ",", "ein", "Rin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Den Dank dir die Kr\u00e4nze, der jubelnde Chor,", "tokens": ["Den", "Dank", "dir", "die", "Kr\u00e4n\u00b7ze", ",", "der", "ju\u00b7beln\u00b7de", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die jauchzenden Stimmen dir bringen.", "tokens": ["Die", "jauch\u00b7zen\u00b7den", "Stim\u00b7men", "dir", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ganz waren sie dein; es erbla\u00dfte und schwand", "tokens": ["Ganz", "wa\u00b7ren", "sie", "dein", ";", "es", "er\u00b7bla\u00df\u00b7te", "und", "schwand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$.", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Aus den Mienen das t\u00e4gliche L\u00fcgen,", "tokens": ["Aus", "den", "Mie\u00b7nen", "das", "t\u00e4g\u00b7li\u00b7che", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Es ruhten so heilig die Augen gebannt", "tokens": ["Es", "ruh\u00b7ten", "so", "hei\u00b7lig", "die", "Au\u00b7gen", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "VVPP"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "An deinen zerrissenen Z\u00fcgen.", "tokens": ["An", "dei\u00b7nen", "zer\u00b7ris\u00b7se\u00b7nen", "Z\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Durch das athemlos stille Geschlecht da ging", "tokens": ["Durch", "das", "at\u00b7hem\u00b7los", "stil\u00b7le", "Ge\u00b7schlecht", "da", "ging"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "ADV", "VVFIN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ein leises, bebendes St\u00f6hnen,", "tokens": ["Ein", "lei\u00b7ses", ",", "be\u00b7ben\u00b7des", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Als der Leib dir in zuckenden Schmerzen hing,", "tokens": ["Als", "der", "Leib", "dir", "in", "zu\u00b7cken\u00b7den", "Schmer\u00b7zen", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Die Stimme in flammenden T\u00f6nen.", "tokens": ["Die", "Stim\u00b7me", "in", "flam\u00b7men\u00b7den", "T\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Sie konnten nicht lassen die Blicke davon", "tokens": ["Sie", "konn\u00b7ten", "nicht", "las\u00b7sen", "die", "Bli\u00b7cke", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "ART", "NN", "PAV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf dich in Todespein starrend;", "tokens": ["Auf", "dich", "in", "To\u00b7de\u00b7spein", "star\u00b7rend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Da waren sie dein, ein weicher Thon", "tokens": ["Da", "wa\u00b7ren", "sie", "dein", ",", "ein", "wei\u00b7cher", "Thon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$,", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des belebenden Sch\u00f6pferhauchs harrend.", "tokens": ["Des", "be\u00b7le\u00b7ben\u00b7den", "Sch\u00f6p\u00b7fer\u00b7hauchs", "har\u00b7rend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Da stand\u2019st du, und \u00fcber dein Antlitz schlich", "tokens": ["Da", "stan\u00b7d'st", "du", ",", "und", "\u00fc\u00b7ber", "dein", "Ant\u00b7litz", "schlich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Der eherne Todesbote,", "tokens": ["Der", "e\u00b7her\u00b7ne", "To\u00b7des\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Gesenkt in den Wahnsinn des Schmerzes erblich", "tokens": ["Ge\u00b7senkt", "in", "den", "Wahn\u00b7sinn", "des", "Schmer\u00b7zes", "er\u00b7blich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Es leise, das gluthendurchlohte. \u2014", "tokens": ["Es", "lei\u00b7se", ",", "das", "glu\u00b7then\u00b7durch\u00b7loh\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "$,", "PDS", "VVFIN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Der Vorhang fiel und mit Donnergebraus", "tokens": ["Der", "Vor\u00b7hang", "fiel", "und", "mit", "Don\u00b7ner\u00b7ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "APPR", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In str\u00f6mendem Beifalls-Schreien,", "tokens": ["In", "str\u00f6\u00b7men\u00b7dem", "Bei\u00b7falls\u00b7Schrei\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In l\u00e4rmendem Hoch sie und wildem Applaus", "tokens": ["In", "l\u00e4r\u00b7men\u00b7dem", "Hoch", "sie", "und", "wil\u00b7dem", "Ap\u00b7plaus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "KON", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Vom erdr\u00fcckenden Joch sich befreien.", "tokens": ["Vom", "er\u00b7dr\u00fc\u00b7cken\u00b7den", "Joch", "sich", "be\u00b7frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und wie das gewaltige Haus auch erbebt,", "tokens": ["Und", "wie", "das", "ge\u00b7wal\u00b7ti\u00b7ge", "Haus", "auch", "er\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie z\u00f6gert \u2014 nicht will sie sich zeigen,", "tokens": ["Sie", "z\u00f6\u00b7gert", "nicht", "will", "sie", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PTKNEG", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bis endlich der Vorhang sich rauschend erhebt,", "tokens": ["Bis", "end\u00b7lich", "der", "Vor\u00b7hang", "sich", "rau\u00b7schend", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und athemlos lauschendes Schweigen.", "tokens": ["Und", "at\u00b7hem\u00b7los", "lau\u00b7schen\u00b7des", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Da steht sie, seht, und r\u00fchrt sich nicht \u2014", "tokens": ["Da", "steht", "sie", ",", "seht", ",", "und", "r\u00fchrt", "sich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "KON", "VVFIN", "PRF", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufeinander die Z\u00e4hne gebissen,", "tokens": ["Auf\u00b7ein\u00b7an\u00b7der", "die", "Z\u00e4h\u00b7ne", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und es zuckt so mild durch ihr brennend Gesicht", "tokens": ["Und", "es", "zuckt", "so", "mild", "durch", "ihr", "bren\u00b7nend", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch der Schmerz, der das Herz ihr zerrissen \u2014", "tokens": ["Noch", "der", "Schmerz", ",", "der", "das", "Herz", "ihr", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVPP", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und es raunt ihr zu: \u2019s war blendender Schein,", "tokens": ["Und", "es", "raunt", "ihr", "zu", ":", "'s", "war", "blen\u00b7den\u00b7der", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nicht Leben, du hatt\u2019st es vergessen,", "tokens": ["Nicht", "Le\u00b7ben", ",", "du", "hatt'st", "es", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Was zogst du auch von dem innersten Sein", "tokens": ["Was", "zogst", "du", "auch", "von", "dem", "in\u00b7ners\u00b7ten", "Sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den h\u00fcllenden Schleier vermessen.", "tokens": ["Den", "h\u00fcl\u00b7len\u00b7den", "Schlei\u00b7er", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Und es fa\u00dft sie ein Ekel und wilder Gram", "tokens": ["Und", "es", "fa\u00dft", "sie", "ein", "E\u00b7kel", "und", "wil\u00b7der", "Gram"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "KON", "ADJA", "NN"], "meter": "--+-----+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und rei\u00dft sie aus tr\u00e4umendem W\u00e4hnen \u2014", "tokens": ["Und", "rei\u00dft", "sie", "aus", "tr\u00e4u\u00b7men\u00b7dem", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So nackt \u2014 so blo\u00df \u2014 o die gl\u00fchende Scham \u2014", "tokens": ["So", "nackt", "so", "blo\u00df", "o", "die", "gl\u00fc\u00b7hen\u00b7de", "Scham"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "ADV", "ADV", "$(", "FM", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Wo die l\u00fcsternen Blicke mich h\u00f6hnen. \u2014", "tokens": ["Wo", "die", "l\u00fcs\u00b7ter\u00b7nen", "Bli\u00b7cke", "mich", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVINF", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "So war\u2019s in Traum von der hehren Macht", "tokens": ["So", "wa\u00b7r's", "in", "Traum", "von", "der", "heh\u00b7ren", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Der Kunst \u2014 was ist\u2019s, da\u00df ich\u2019s hehle \u2014", "tokens": ["Der", "Kunst", "was", "ist's", ",", "da\u00df", "ich's", "heh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "VAFIN", "$,", "KOUS", "PIS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich habe verschachert \u2014 jetzt bin ich erwacht \u2014", "tokens": ["Ich", "ha\u00b7be", "ver\u00b7scha\u00b7chert", "jetzt", "bin", "ich", "er\u00b7wacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Des K\u00fcnstlers lebendige Seele.", "tokens": ["Des", "K\u00fcnst\u00b7lers", "le\u00b7ben\u00b7di\u00b7ge", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}