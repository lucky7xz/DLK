{"textgrid.poem.52493": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "13. Bergreiselied", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf mutig! Die H\u00f6h' ist erstiegen:", "tokens": ["Auf", "mu\u00b7tig", "!", "Die", "H\u00f6h'", "ist", "er\u00b7stie\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ihr Freunde, wo bleibt ihr zur\u00fcck?", "tokens": ["Ihr", "Freun\u00b7de", ",", "wo", "bleibt", "ihr", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie herrlich die Th\u00e4ler dort liegen!", "tokens": ["Wie", "herr\u00b7lich", "die", "Th\u00e4\u00b7ler", "dort", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Tief unten verliert sich mein Blick.", "tokens": ["Tief", "un\u00b7ten", "ver\u00b7liert", "sich", "mein", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ich atme die s\u00fc\u00dfesten D\u00fcfte,", "tokens": ["Ich", "at\u00b7me", "die", "s\u00fc\u00b7\u00dfes\u00b7ten", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Schon wallet viel leichter mein Blut;", "tokens": ["Schon", "wal\u00b7let", "viel", "leich\u00b7ter", "mein", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Schon trink' ich \u00e4therische L\u00fcfte,", "tokens": ["Schon", "trink'", "ich", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und jauchze, und schwinge den Hut!", "tokens": ["Und", "jauch\u00b7ze", ",", "und", "schwin\u00b7ge", "den", "Hut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Dort setzen die Hirten zu Mahle", "tokens": ["Dort", "set\u00b7zen", "die", "Hir\u00b7ten", "zu", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Auf moosichte Steine uns hin", "tokens": ["Auf", "moo\u00b7sich\u00b7te", "Stei\u00b7ne", "uns", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Voll lieblicher Milch eine Schale;", "tokens": ["Voll", "lieb\u00b7li\u00b7cher", "Milch", "ei\u00b7ne", "Scha\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein K\u00f6rbchen, mit Fr\u00fcchten darin.", "tokens": ["Ein", "K\u00f6rb\u00b7chen", ",", "mit", "Fr\u00fcch\u00b7ten", "da\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Kommt, la\u00dft uns zusammen itzt leeren", "tokens": ["Kommt", ",", "la\u00dft", "uns", "zu\u00b7sam\u00b7men", "itzt", "lee\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "ADV", "ADV", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Den sch\u00e4umenden vollen Pokal,", "tokens": ["Den", "sch\u00e4u\u00b7men\u00b7den", "vol\u00b7len", "Po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und schallen, der Freiheit zu Ehren,", "tokens": ["Und", "schal\u00b7len", ",", "der", "Frei\u00b7heit", "zu", "Eh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Ges\u00e4nge hinab in das Thal.", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "hin\u00b7ab", "in", "das", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Hier sprudeln aus Felsen die Quellen", "tokens": ["Hier", "spru\u00b7deln", "aus", "Fel\u00b7sen", "die", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hinunter zum bl\u00e4ulichen See;", "tokens": ["Hin\u00b7un\u00b7ter", "zum", "bl\u00e4u\u00b7li\u00b7chen", "See", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dort weiden, beim Klange der Schellen,", "tokens": ["Dort", "wei\u00b7den", ",", "beim", "Klan\u00b7ge", "der", "Schel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Rinder im blumichten Klee.", "tokens": ["Die", "Rin\u00b7der", "im", "blu\u00b7mich\u00b7ten", "Klee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Ich seh' auf die schroffeste Spitze", "tokens": ["Ich", "seh'", "auf", "die", "schrof\u00b7fes\u00b7te", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sch\u00fcchternen Gemsen entfliehn;", "tokens": ["Die", "sch\u00fcch\u00b7ter\u00b7nen", "Gem\u00b7sen", "ent\u00b7fliehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Tief unter mir zucken die Blitze", "tokens": ["Tief", "un\u00b7ter", "mir", "zu\u00b7cken", "die", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und schweben die Wolken dahin.", "tokens": ["Und", "schwe\u00b7ben", "die", "Wol\u00b7ken", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Wann Sterne am Himmel schon flimmern,", "tokens": ["Wann", "Ster\u00b7ne", "am", "Him\u00b7mel", "schon", "flim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und D\u00e4mmerung sinket ins Thal,", "tokens": ["Und", "D\u00e4m\u00b7me\u00b7rung", "sin\u00b7ket", "ins", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und rosig die Gletscher noch schimmern", "tokens": ["Und", "ro\u00b7sig", "die", "Glet\u00b7scher", "noch", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ADV", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im letzten ersterbenden Strahl;", "tokens": ["Im", "letz\u00b7ten", "ers\u00b7ter\u00b7ben\u00b7den", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wallen wir fr\u00f6hlich und munter,", "tokens": ["Dann", "wal\u00b7len", "wir", "fr\u00f6h\u00b7lich", "und", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mit Reisern von Tannen geschm\u00fcckt,", "tokens": ["Mit", "Rei\u00b7sern", "von", "Tan\u00b7nen", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ins stillere D\u00f6rfchen hinunter,", "tokens": ["Ins", "stil\u00b7le\u00b7re", "D\u00f6rf\u00b7chen", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Wo s\u00fc\u00dfere Ruh' uns erquickt.", "tokens": ["Wo", "s\u00fc\u00b7\u00dfe\u00b7re", "Ruh'", "uns", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}