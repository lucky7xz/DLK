{"textgrid.poem.61679": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das H\u00f6chste bleibt ein freier Wille,", "tokens": ["Das", "H\u00f6chs\u00b7te", "bleibt", "ein", "frei\u00b7er", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, unverwirrt von Fleisch und Blut,", "tokens": ["Der", ",", "un\u00b7ver\u00b7wirrt", "von", "Fleisch", "und", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich selbst getreu in Sturm und Stille", "tokens": ["Sich", "selbst", "ge\u00b7treu", "in", "Sturm", "und", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Gute, weil es gut ist, tut.", "tokens": ["Das", "Gu\u00b7te", ",", "weil", "es", "gut", "ist", ",", "tut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}