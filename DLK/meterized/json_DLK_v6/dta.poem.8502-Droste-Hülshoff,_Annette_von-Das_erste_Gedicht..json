{"dta.poem.8502": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Das erste Gedicht.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf meiner Heimath Grunde", "tokens": ["Auf", "mei\u00b7ner", "Hei\u00b7math", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht ein Zinnenbau,", "tokens": ["Da", "steht", "ein", "Zin\u00b7nen\u00b7bau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut finster in die Runde", "tokens": ["Schaut", "fins\u00b7ter", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Wimpern schwer und grau.", "tokens": ["Aus", "Wim\u00b7pern", "schwer", "und", "grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An seines Fensters Gittern", "tokens": ["An", "sei\u00b7nes", "Fens\u00b7ters", "Git\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wimmert des Kauzes Schrei,", "tokens": ["Wim\u00b7mert", "des", "Kau\u00b7zes", "Schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und dr\u00fcber siehst du wittern", "tokens": ["Und", "dr\u00fc\u00b7ber", "siehst", "du", "wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den sonnentrunknen Weih.", "tokens": ["Den", "son\u00b7nen\u00b7trunk\u00b7nen", "Weih", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein W\u00e4chter fest wie Klippen,", "tokens": ["Ein", "W\u00e4ch\u00b7ter", "fest", "wie", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von keinem Sturm bewegt,", "tokens": ["Von", "kei\u00b7nem", "Sturm", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der in den harten Rippen", "tokens": ["Der", "in", "den", "har\u00b7ten", "Rip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar manche Kugel tr\u00e4gt;", "tokens": ["Gar", "man\u00b7che", "Ku\u00b7gel", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Mahner auch, ein strenger,", "tokens": ["Ein", "Mah\u00b7ner", "auch", ",", "ein", "stren\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "De\u00df Giebel, gr\u00fcn und feucht,", "tokens": ["De\u00df", "Gie\u00b7bel", ",", "gr\u00fcn", "und", "feucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit spitzem Hut und F\u00e4nger", "tokens": ["Mit", "spit\u00b7zem", "Hut", "und", "F\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Hauses Geist besteigt.", "tokens": ["Des", "Hau\u00b7ses", "Geist", "be\u00b7steigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sieht ihn das Gesinde", "tokens": ["Und", "sieht", "ihn", "das", "Ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Fahnenschafte steh\u2019n,", "tokens": ["Am", "Fah\u00b7nen\u00b7schaf\u00b7te", "steh'n", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich wirbelnd vor dem Winde", "tokens": ["Sich", "wir\u00b7belnd", "vor", "dem", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit leisem Schreie dreh\u2019n,", "tokens": ["Mit", "lei\u00b7sem", "Schrei\u00b7e", "dreh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann pocht im Schlo\u00dfgem\u00e4uer", "tokens": ["Dann", "pocht", "im", "Schlo\u00df\u00b7ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gewi\u00df die Todtenuhr,", "tokens": ["Ge\u00b7wi\u00df", "die", "Tod\u00b7te\u00b7nuhr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Oder ein t\u00fcckisch Feuer", "tokens": ["O\u00b7der", "ein", "t\u00fc\u00b7ckisch", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fri\u00dft glimmend unter\u2019m Flur.", "tokens": ["Fri\u00dft", "glim\u00b7mend", "un\u00b7ter'm", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie hab\u2019 ich ihn umstrichen", "tokens": ["Wie", "hab'", "ich", "ihn", "um\u00b7stri\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als Kind oft stundenlang,", "tokens": ["Als", "Kind", "oft", "stun\u00b7den\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bin heimlich dann geschlichen", "tokens": ["Bin", "heim\u00b7lich", "dann", "ge\u00b7schli\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den schwer verp\u00f6nten Gang", "tokens": ["Den", "schwer", "ver\u00b7p\u00f6n\u00b7ten", "Gang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinauf die Wendelstiege,", "tokens": ["Hin\u00b7auf", "die", "Wen\u00b7del\u00b7stie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die unter\u2019m Tritte bog,", "tokens": ["Die", "un\u00b7ter'm", "Trit\u00b7te", "bog", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bis zu des Sturmes Wiege,", "tokens": ["Bis", "zu", "des", "Stur\u00b7mes", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Hahnenbalken hoch.", "tokens": ["Zum", "Hah\u00b7nen\u00b7bal\u00b7ken", "hoch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und sa\u00df ich auf dem Balken", "tokens": ["Und", "sa\u00df", "ich", "auf", "dem", "Bal\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im D\u00e4mmerstrahle falb,", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7strah\u00b7le", "falb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich f\u00fchlend halb als Falken,", "tokens": ["Mich", "f\u00fch\u00b7lend", "halb", "als", "Fal\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als Mauereule halb,", "tokens": ["Als", "Mau\u00b7e\u00b7reu\u00b7le", "halb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann hab\u2019 ich aus dem Brodem", "tokens": ["Dann", "hab'", "ich", "aus", "dem", "Bro\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Geist citirt mit Muth,", "tokens": ["Den", "Geist", "ci\u00b7tirt", "mit", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich, Hauch von seinem Odem,", "tokens": ["Ich", ",", "Hauch", "von", "sei\u00b7nem", "O\u00b7dem", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Blut von seinem Blut.", "tokens": ["Und", "Blut", "von", "sei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch als nun immer tiefer", "tokens": ["Doch", "als", "nun", "im\u00b7mer", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schlangenstiege sank,", "tokens": ["Die", "Schlan\u00b7gen\u00b7stie\u00b7ge", "sank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als schiefer stets und schiefer", "tokens": ["Als", "schie\u00b7fer", "stets", "und", "schie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADV", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dr\u00e4ute die Stufenbank:", "tokens": ["Dr\u00e4u\u00b7te", "die", "Stu\u00b7fen\u00b7bank", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da klomm\u2019 ich sonder Harren", "tokens": ["Da", "klomm'", "ich", "son\u00b7der", "Har\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hinan den Zinnenring,", "tokens": ["Hi\u00b7nan", "den", "Zin\u00b7nen\u00b7ring", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und in des Daches Sparren", "tokens": ["Und", "in", "des", "Da\u00b7ches", "Spar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Barg ich ein heimlich Ding.", "tokens": ["Barg", "ich", "ein", "heim\u00b7lich", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das sollten Enkel finden,", "tokens": ["Das", "soll\u00b7ten", "En\u00b7kel", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn einst der Thurm zerbrach,", "tokens": ["Wenn", "einst", "der", "Thurm", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sollte Etwas k\u00fcnden,", "tokens": ["Es", "soll\u00b7te", "Et\u00b7was", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das mir am Herzen lag;", "tokens": ["Das", "mir", "am", "Her\u00b7zen", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun sinn\u2019 ich oft vergebens,", "tokens": ["Nun", "sinn'", "ich", "oft", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was mich so aufgeregt,", "tokens": ["Was", "mich", "so", "auf\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Was mit Gefahr des Lebens", "tokens": ["Was", "mit", "Ge\u00b7fahr", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich in den Spalt gelegt.", "tokens": ["Ich", "in", "den", "Spalt", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Vielleicht mit Glasopalen", "tokens": ["Viel\u00b7leicht", "mit", "Gla\u00b7so\u00b7pa\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ring \u2014 ein Dockenkleid \u2014", "tokens": ["Ein", "Ring", "ein", "Do\u00b7cken\u00b7kleid"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$("], "meter": "-++-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Das herrlich sollte strahlen", "tokens": ["Das", "herr\u00b7lich", "soll\u00b7te", "strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die zuk\u00fcnft\u2019ge Zeit;", "tokens": ["In", "die", "zu\u00b7k\u00fcnft'\u00b7ge", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn da\u00df es hell geflittert,", "tokens": ["Denn", "da\u00df", "es", "hell", "ge\u00b7flit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir wie im Traume scheint,", "tokens": ["Mir", "wie", "im", "Trau\u00b7me", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und da\u00df ich sehr gezittert", "tokens": ["Und", "da\u00df", "ich", "sehr", "ge\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und bitterlich geweint.", "tokens": ["Und", "bit\u00b7ter\u00b7lich", "ge\u00b7weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit einmal will mir\u2019s tagen!", "tokens": ["Mit", "ein\u00b7mal", "will", "mir's", "ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es war \u2014 ich irre nicht \u2014", "tokens": ["Es", "war", "ich", "ir\u00b7re", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VVFIN", "PTKNEG", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "In Goldpapier geschlagen", "tokens": ["In", "Gold\u00b7pa\u00b7pier", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein allererst Gedicht.", "tokens": ["Mein", "al\u00b7le\u00b7rerst", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Lied vom H\u00e4hnchen, was ich", "tokens": ["Mein", "Lied", "vom", "H\u00e4hn\u00b7chen", ",", "was", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "PWS", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "So still gemacht, bei Seit\u2019,", "tokens": ["So", "still", "ge\u00b7macht", ",", "bei", "Seit'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich so gesch\u00e4mt und das ich", "tokens": ["Mich", "so", "ge\u00b7sch\u00e4mt", "und", "das", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "KON", "PRELS", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Der Ewigkeit geweiht.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wolltest so hoch du fahren,", "tokens": ["Woll\u00b7test", "so", "hoch", "du", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du th\u00f6richt Kind? Wer wei\u00df?", "tokens": ["Du", "th\u00f6\u00b7richt", "Kind", "?", "Wer", "wei\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$.", "PWS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vielleicht nach drei\u00dfig Jahren", "tokens": ["Viel\u00b7leicht", "nach", "drei\u00b7\u00dfig", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Treibt schwach dein Lorbeerreis.", "tokens": ["Treibt", "schwach", "dein", "Lor\u00b7beer\u00b7reis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du wirst noch schwer und blutig", "tokens": ["Du", "wirst", "noch", "schwer", "und", "blu\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch manche Schule geh\u2019n;", "tokens": ["Durch", "man\u00b7che", "Schu\u00b7le", "geh'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und dann nicht halb so muthig", "tokens": ["Und", "dann", "nicht", "halb", "so", "mut\u00b7hig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vor deiner Nachwelt steh\u2019n.", "tokens": ["Vor", "dei\u00b7ner", "Nach\u00b7welt", "steh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zerfallen am Gew\u00e4nde", "tokens": ["Zer\u00b7fal\u00b7len", "am", "Ge\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist l\u00e4ngst der Stiege Rund,", "tokens": ["Ist", "l\u00e4ngst", "der", "Stie\u00b7ge", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum liegt noch vom Gel\u00e4nde", "tokens": ["Kaum", "liegt", "noch", "vom", "Ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein morsches Brett am Grund;", "tokens": ["Ein", "mor\u00b7sches", "Brett", "am", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn die Balken knarren,", "tokens": ["Und", "wenn", "die", "Bal\u00b7ken", "knar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Sturm die Fahne kreis\u2019t,", "tokens": ["Im", "Sturm", "die", "Fah\u00b7ne", "kreis't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann gleitet an den Sparren", "tokens": ["Dann", "glei\u00b7tet", "an", "den", "Spar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht mehr des Ahnen Geist.", "tokens": ["Nicht", "mehr", "des", "Ah\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es schien ihm \u00fcbel hausen", "tokens": ["Es", "schien", "ihm", "\u00fc\u00b7bel", "hau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dieser Zeiten Lauf;", "tokens": ["In", "die\u00b7ser", "Zei\u00b7ten", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich aber stehe drau\u00dfen", "tokens": ["Ich", "a\u00b7ber", "ste\u00b7he", "drau\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und schau\u2019 die Wand hinauf;", "tokens": ["Und", "schau'", "die", "Wand", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sp\u00e4h\u2019 durch der Sonne Lodern,", "tokens": ["Sp\u00e4h'", "durch", "der", "Son\u00b7ne", "Lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In welcher Ritze wohl", "tokens": ["In", "wel\u00b7cher", "Rit\u00b7ze", "wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es einsam mag vermodern,", "tokens": [