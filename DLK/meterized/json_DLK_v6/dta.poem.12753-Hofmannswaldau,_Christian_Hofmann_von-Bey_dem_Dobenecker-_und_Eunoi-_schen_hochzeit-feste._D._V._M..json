{"dta.poem.12753": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Bey dem Dobenecker- und Eunoi-  \n schen hochzeit-feste.  \n D. V. M.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf, sch\u00e4fer!", "tokens": ["Auf", ",", "sch\u00e4\u00b7fer", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["APPR", "$,", "ADJD", "$."], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Auf! wo ihr immer steckt,", "tokens": ["Auf", "!", "wo", "ihr", "im\u00b7mer", "steckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn euch der herbst erschreckt", "tokens": ["Wenn", "euch", "der", "herbst", "er\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit den betr\u00fcbten tagen,", "tokens": ["Mit", "den", "be\u00b7tr\u00fcb\u00b7ten", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die unsre felder plagen.", "tokens": ["Die", "uns\u00b7re", "fel\u00b7der", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf, sch\u00e4fer!", "tokens": ["Auf", ",", "sch\u00e4\u00b7fer", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["APPR", "$,", "ADJD", "$."], "meter": "++-", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Verla\u00dft das faule dach,", "tokens": ["Ver\u00b7la\u00dft", "das", "fau\u00b7le", "dach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und folgt den nymphen nach,", "tokens": ["Und", "folgt", "den", "nym\u00b7phen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die heute mit den hirten", "tokens": ["Die", "heu\u00b7te", "mit", "den", "hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bem\u00fchet sind um myrten.", "tokens": ["Be\u00b7m\u00fc\u00b7het", "sind", "um", "myr\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geschwinde!", "tokens": ["Ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Erscheinet!", "tokens": ["Er\u00b7schei\u00b7net", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Mit einem frohen chor;", "tokens": ["Mit", "ei\u00b7nem", "fro\u00b7hen", "chor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sucht alle lust hervor,", "tokens": ["Sucht", "al\u00b7le", "lust", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch fl\u00f6ten und schalmeyen,", "tokens": ["Durch", "fl\u00f6\u00b7ten", "und", "schal\u00b7me\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nebst angenehmen reyen.", "tokens": ["Nebst", "an\u00b7ge\u00b7neh\u00b7men", "re\u00b7yen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Erscheinet!", "tokens": ["Er\u00b7schei\u00b7net", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Zu ehren!", "tokens": ["Zu", "eh\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der l\u00e4ngst gew\u00fcnschten zeit,", "tokens": ["Der", "l\u00e4ngst", "ge\u00b7w\u00fcnschten", "zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die itzund hoch erfreut", "tokens": ["Die", "it\u00b7zund", "hoch", "er\u00b7freut"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zwey treu-verbundne hertzen", "tokens": ["Zwey", "treu\u00b7ver\u00b7bund\u00b7ne", "hert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bey ihren hochzeit-kertzen.", "tokens": ["Bey", "ih\u00b7ren", "hoch\u00b7zeit\u00b7kert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu ehren!", "tokens": ["Zu", "eh\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Dem liebsten!", "tokens": ["Dem", "liebs\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Der nach viel m\u00fch und schwei\u00df", "tokens": ["Der", "nach", "viel", "m\u00fch", "und", "schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erlanget diesen prei\u00df:", "tokens": ["Er\u00b7lan\u00b7get", "die\u00b7sen", "prei\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df nun in seinen armen", "tokens": ["Da\u00df", "nun", "in", "sei\u00b7nen", "ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Maria wird erwarmen.", "tokens": ["Ma\u00b7ria", "wird", "er\u00b7war\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Dem liebsten!", "tokens": ["Dem", "liebs\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Der liebsten!", "tokens": ["Der", "liebs\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Die ihre frohe brust", "tokens": ["Die", "ih\u00b7re", "fro\u00b7he", "brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erf\u00fcllet sieht mit lust,", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "sieht", "mit", "lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch ihres liebsten blicke,", "tokens": ["Durch", "ih\u00b7res", "liebs\u00b7ten", "bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trotz aller neider t\u00fccke.", "tokens": ["Trotz", "al\u00b7ler", "nei\u00b7der", "t\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der liebsten!", "tokens": ["Der", "liebs\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.7": {"line.1": {"text": "Drum singet!", "tokens": ["Drum", "sin\u00b7get", "!"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Da\u00df wiesen, thal und wald", "tokens": ["Da\u00df", "wie\u00b7sen", ",", "thal", "und", "wald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "VVPP", "$,", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von eurem thon erschallt,", "tokens": ["Von", "eu\u00b7rem", "thon", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df auch die schafe springen", "tokens": ["Da\u00df", "auch", "die", "scha\u00b7fe", "sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von eurem hellen klingen.", "tokens": ["Von", "eu\u00b7rem", "hel\u00b7len", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum singet!", "tokens": ["Drum", "sin\u00b7get", "!"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "O himmel!", "tokens": ["O", "him\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Verkn\u00fcpffe dieses band", "tokens": ["Ver\u00b7kn\u00fcpf\u00b7fe", "die\u00b7ses", "band"], "token_info": ["word", "word", "word"], "pos": ["NN", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch einen guten stand,", "tokens": ["Durch", "ei\u00b7nen", "gu\u00b7ten", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df ihre fette trifften", "tokens": ["La\u00df", "ih\u00b7re", "fet\u00b7te", "triff\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit mi\u00dfwachs nicht vergifften.", "tokens": ["Mit", "mi\u00df\u00b7wachs", "nicht", "ver\u00b7giff\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "O himmel!", "tokens": ["O", "him\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.9": {"line.1": {"text": "Beschl\u00fcsset!", "tokens": ["Be\u00b7schl\u00fcs\u00b7set", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und nehmt die pfeiff in acht,", "tokens": ["Und", "nehmt", "die", "pfeiff", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ihr was neues macht,", "tokens": ["Da\u00df", "ihr", "was", "neu\u00b7es", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn wir in einer wiegen,", "tokens": ["Wenn", "wir", "in", "ei\u00b7ner", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sehn einen pachter liegen.", "tokens": ["Sehn", "ei\u00b7nen", "pach\u00b7ter", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beschl\u00fcsset!", "tokens": ["Be\u00b7schl\u00fcs\u00b7set", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}