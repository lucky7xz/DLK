{"dta.poem.15661": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Mathematiker ist darum nur gelungen", "tokens": ["Dem", "Ma\u00b7the\u00b7ma\u00b7ti\u00b7ker", "ist", "da\u00b7rum", "nur", "ge\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PAV", "ADV", "VVPP"], "meter": "-+-+----+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So Vieles, weil er zieht aus Allem Folgerungen.", "tokens": ["So", "Vie\u00b7les", ",", "weil", "er", "zieht", "aus", "Al\u00b7lem", "Fol\u00b7ge\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "KOUS", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er folgert, wenn er auch nicht sieht wozu es frommt,", "tokens": ["Er", "fol\u00b7gert", ",", "wenn", "er", "auch", "nicht", "sieht", "wo\u00b7zu", "es", "frommt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erwartend ob es ihm einmal zu Statten kommt.", "tokens": ["Er\u00b7war\u00b7tend", "ob", "es", "ihm", "ein\u00b7mal", "zu", "Stat\u00b7ten", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf einmal sieht er, wie Unn\u00fctzes selber n\u00fctzt,", "tokens": ["Auf", "ein\u00b7mal", "sieht", "er", ",", "wie", "Un\u00b7n\u00fct\u00b7zes", "sel\u00b7ber", "n\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "$,", "PWAV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn Allergr\u00f6\u00dftes sich auf Allerkleinstes st\u00fctzt.", "tokens": ["Wenn", "Al\u00b7ler\u00b7gr\u00f6\u00df\u00b7tes", "sich", "auf", "Al\u00b7ler\u00b7kleins\u00b7tes", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}