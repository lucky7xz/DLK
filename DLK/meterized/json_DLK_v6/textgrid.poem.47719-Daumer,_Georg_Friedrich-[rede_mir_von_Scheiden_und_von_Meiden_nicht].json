{"textgrid.poem.47719": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[rede mir von Scheiden und von Meiden nicht]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rede mir von Scheiden und von Meiden nicht,", "tokens": ["Re\u00b7de", "mir", "von", "Schei\u00b7den", "und", "von", "Mei\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Allzu hart uns ohne Noth zu peinigen!", "tokens": ["All\u00b7zu", "hart", "uns", "oh\u00b7ne", "Noth", "zu", "pei\u00b7ni\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Deine Wege, seien es die rauhsten auch,", "tokens": ["Dei\u00b7ne", "We\u00b7ge", ",", "sei\u00b7en", "es", "die", "rauhs\u00b7ten", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER", "ART", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Bleiben ohne Widerruf die meinigen,", "tokens": ["Blei\u00b7ben", "oh\u00b7ne", "Wi\u00b7der\u00b7ruf", "die", "mei\u00b7ni\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "PPOSS", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Denn wo du, der Bruder meiner Seele gehst,", "tokens": ["Denn", "wo", "du", ",", "der", "Bru\u00b7der", "mei\u00b7ner", "See\u00b7le", "gehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Geh' auch ich, das Schwesterchen der deinigen.", "tokens": ["Geh'", "auch", "ich", ",", "das", "Schwes\u00b7ter\u00b7chen", "der", "dei\u00b7ni\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "$,", "ART", "NN", "ART", "PPOSS", "$."], "meter": "+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Wo du weilend deine Waffe h\u00e4ngen wirst,", "tokens": ["Wo", "du", "wei\u00b7lend", "dei\u00b7ne", "Waf\u00b7fe", "h\u00e4n\u00b7gen", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Hangen hier, die Treue zu bescheinigen", "tokens": ["Han\u00b7gen", "hier", ",", "die", "Treu\u00b7e", "zu", "be\u00b7schei\u00b7ni\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Meiner Brust, soll meines Hauptes Kr\u00e4nzchen auch;", "tokens": ["Mei\u00b7ner", "Brust", ",", "soll", "mei\u00b7nes", "Haup\u00b7tes", "Kr\u00e4nz\u00b7chen", "auch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Trennen nichts, es soll uns Alles einigen.", "tokens": ["Tren\u00b7nen", "nichts", ",", "es", "soll", "uns", "Al\u00b7les", "ei\u00b7ni\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PPER", "VMFIN", "PPER", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}