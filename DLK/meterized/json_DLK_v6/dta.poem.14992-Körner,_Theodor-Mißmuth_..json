{"dta.poem.14992": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Mi\u00dfmuth .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Schwelgend in der Tage Gl\u00fcck.", "tokens": ["Schwel\u00b7gend", "in", "der", "Ta\u00b7ge", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blutig hassend deine Dr\u00e4nger,", "tokens": ["Blu\u00b7tig", "has\u00b7send", "dei\u00b7ne", "Dr\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hielt nicht Lied und Liebe l\u00e4nger", "tokens": ["Hielt", "nicht", "Lied", "und", "Lie\u00b7be", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "NN", "KON", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seiner Seele Sturm zur\u00fcck.", "tokens": ["Sei\u00b7ner", "See\u00b7le", "Sturm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er brach mit wundem Herzen", "tokens": ["Und", "er", "brach", "mit", "wun\u00b7dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der Freunde Reihn,", "tokens": ["Aus", "der", "Freun\u00b7de", "Reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Tauchte in der Trennung Schmerzen, \u2014", "tokens": ["Tauch\u00b7te", "in", "der", "Tren\u00b7nung", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und war dein.", "tokens": ["Und", "war", "dein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Thr\u00e4nend hat er oft die Blicke", "tokens": ["Thr\u00e4\u00b7nend", "hat", "er", "oft", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zur Vergangenheit gesandt,", "tokens": ["Zur", "Ver\u00b7gan\u00b7gen\u00b7heit", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf des Lieds melod'scher Br\u00fccke", "tokens": ["Auf", "des", "Lieds", "me\u00b7lo\u00b7d'\u00b7scher", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Stieg der Geist zum alten Gl\u00fccke", "tokens": ["Stieg", "der", "Geist", "zum", "al\u00b7ten", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Liebe goldnes Land.", "tokens": ["In", "der", "Lie\u00b7be", "gold\u00b7nes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach er schw\u00e4rmte nur vergebens,", "tokens": ["Ach", "er", "schw\u00e4rm\u00b7te", "nur", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn der Stunden rohe Hast", "tokens": ["Denn", "der", "Stun\u00b7den", "ro\u00b7he", "Hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Warf ihn in den L\u00e4rm des Lebens,", "tokens": ["Warf", "ihn", "in", "den", "L\u00e4rm", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sturmgefa\u00dft.", "tokens": ["Sturm\u00b7ge\u00b7fa\u00dft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Doch was soll er im Gedr\u00e4nge", "tokens": ["Doch", "was", "soll", "er", "im", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Schlachten Morgenroth?", "tokens": ["Oh\u00b7ne", "Schlach\u00b7ten", "Mor\u00b7gen\u00b7roth", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gieb die friedlichen Ges\u00e4nge,", "tokens": ["Gieb", "die", "fried\u00b7li\u00b7chen", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder gieb des Krieges Strenge", "tokens": ["O\u00b7der", "gieb", "des", "Krie\u00b7ges", "Stren\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gieb mir Lieder, oder Tod.", "tokens": ["Gieb", "mir", "Lie\u00b7der", ",", "o\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df mir der Begeistrung Thr\u00e4nen,", "tokens": ["La\u00df", "mir", "der", "Be\u00b7geis\u00b7trung", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df mir meine Liebes-Nacht,", "tokens": ["La\u00df", "mir", "mei\u00b7ne", "Lie\u00b7bes\u00b7Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder wirf mein freudig Sehnen", "tokens": ["O\u00b7der", "wirf", "mein", "freu\u00b7dig", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In die Schlacht! \u2014", "tokens": ["In", "die", "Schlacht", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Um mich donnern die Kanonen,", "tokens": ["Um", "mich", "don\u00b7nern", "die", "Ka\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferne Cimbeln schmettern drein;", "tokens": ["Fer\u00b7ne", "Cim\u00b7beln", "schmet\u00b7tern", "drein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschland wirft um seine Kronen,", "tokens": ["Deutschland", "wirft", "um", "sei\u00b7ne", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hier soll ich ruhig wohnen", "tokens": ["Und", "hier", "soll", "ich", "ru\u00b7hig", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und des Stromes W\u00e4chter seyn?", "tokens": ["Und", "des", "Stro\u00b7mes", "W\u00e4ch\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll ich in der Prosa sterben? \u2014", "tokens": ["Soll", "ich", "in", "der", "Pro\u00b7sa", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Poesie, du Flammenquell,", "tokens": ["Poe\u00b7sie", ",", "du", "Flam\u00b7men\u00b7quell", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Brich nur los mit leuchtendem Verderben,", "tokens": ["Brich", "nur", "los", "mit", "leuch\u00b7ten\u00b7dem", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Aber schnell!", "tokens": ["A\u00b7ber", "schnell", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}