{"dta.poem.668": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Die augen sindt vom weynen gar verdorben/ ", "tokens": ["Die", "au\u00b7gen", "sindt", "vom", "wey\u00b7nen", "gar", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die herbe flutt durchbeist die wangen/", "tokens": ["Die", "her\u00b7be", "flutt", "durch\u00b7beist", "die", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Hertz mit heisser angst vmbfangen", "tokens": ["Das", "Hertz", "mit", "heis\u00b7ser", "angst", "vmb\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist schier in bangikeit erstorben/ ", "tokens": ["Ist", "schier", "in", "ban\u00b7gi\u00b7keit", "ers\u00b7tor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wenn ein Schiff sich scheitert an die klippen", "tokens": ["Wie", "wenn", "ein", "Schiff", "sich", "schei\u00b7tert", "an", "die", "klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PRF", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So h\u00f6rt man das ein jeder rufft:", "tokens": ["So", "h\u00f6rt", "man", "das", "ein", "je\u00b7der", "rufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das winseln dringt durch Well\u2019 vnd lufft.", "tokens": ["Das", "win\u00b7seln", "dringt", "durch", "Well'", "vnd", "lufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch jedes wort stirbt auff den lippen.", "tokens": ["Doch", "je\u00b7des", "wort", "stirbt", "auff", "den", "lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}