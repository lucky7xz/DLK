{"textgrid.poem.55376": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Schlu\u00dfpoetik", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sage, Muse, sag dem Dichter,", "tokens": ["Sa\u00b7ge", ",", "Mu\u00b7se", ",", "sag", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er denn es machen soll!", "tokens": ["Wie", "er", "denn", "es", "ma\u00b7chen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der wunderlichsten Richter", "tokens": ["Denn", "der", "wun\u00b7der\u00b7lichs\u00b7ten", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die liebe Welt so voll.", "tokens": ["Ist", "die", "lie\u00b7be", "Welt", "so", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer hab ich doch den rechten,", "tokens": ["Im\u00b7mer", "hab", "ich", "doch", "den", "rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klaren Weg im Lied gezeigt,", "tokens": ["Kla\u00b7ren", "Weg", "im", "Lied", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer war es doch den schlechten,", "tokens": ["Im\u00b7mer", "war", "es", "doch", "den", "schlech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcstren Pfaden abgeneigt.", "tokens": ["D\u00fcst\u00b7ren", "Pfa\u00b7den", "ab\u00b7ge\u00b7neigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber was die Herren wollten,", "tokens": ["A\u00b7ber", "was", "die", "Her\u00b7ren", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward mir niemals ganz bekannt;", "tokens": ["Ward", "mir", "nie\u00b7mals", "ganz", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie w\u00fc\u00dften, was sie sollten,", "tokens": ["Wenn", "sie", "w\u00fc\u00df\u00b7ten", ",", "was", "sie", "soll\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r es auch wohl bald genannt.", "tokens": ["W\u00e4r", "es", "auch", "wohl", "bald", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbwillst du dir ein Ma\u00df bereiten,", "tokens": ["\u00bb", "willst", "du", "dir", "ein", "Ma\u00df", "be\u00b7rei\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaue, was den Edlen mi\u00dft,", "tokens": ["Schau\u00b7e", ",", "was", "den", "Ed\u00b7len", "mi\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihn auch entstellt zu Zeiten,", "tokens": ["Was", "ihn", "auch", "ent\u00b7stellt", "zu", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Leichtsinn sich vergi\u00dft.", "tokens": ["Wenn", "der", "Leicht\u00b7sinn", "sich", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solch ein Inhalt deiner S\u00e4nge,", "tokens": ["Solch", "ein", "In\u00b7halt", "dei\u00b7ner", "S\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der erbauet, der gef\u00e4llt,", "tokens": ["Der", "er\u00b7bau\u00b7et", ",", "der", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im w\u00fcstesten Gedr\u00e4nge", "tokens": ["Und", "im", "w\u00fcs\u00b7tes\u00b7ten", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dankt's die stille, be\u00dfre Welt.", "tokens": ["Dankt's", "die", "stil\u00b7le", ",", "be\u00df\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Frage nicht nach anderm Titel,", "tokens": ["Fra\u00b7ge", "nicht", "nach", "an\u00b7derm", "Ti\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reinem Willen bleibt sein Recht!", "tokens": ["Rei\u00b7nem", "Wil\u00b7len", "bleibt", "sein", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Schurken la\u00df dem B\u00fcttel", "tokens": ["Und", "die", "Schur\u00b7ken", "la\u00df", "dem", "B\u00fct\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Narren dem Geschlecht.\u00ab", "tokens": ["Und", "die", "Nar\u00b7ren", "dem", "Ge\u00b7schlecht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}