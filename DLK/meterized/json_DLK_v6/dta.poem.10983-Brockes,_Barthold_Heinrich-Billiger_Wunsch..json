{"dta.poem.10983": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Billiger Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit erstaunendem Vergn\u00fcgen,", "tokens": ["Mit", "er\u00b7stau\u00b7nen\u00b7dem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollte billig alle Welt,", "tokens": ["Soll\u00b7te", "bil\u00b7lig", "al\u00b7le", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der Weltkreis in sich h\u00e4lt,", "tokens": ["Was", "der", "Welt\u00b7kreis", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sich alle Dinge f\u00fcgen,", "tokens": ["Wie", "sich", "al\u00b7le", "Din\u00b7ge", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie des Sch\u00f6pfers Werk so sch\u00f6n,", "tokens": ["Wie", "des", "Sch\u00f6p\u00b7fers", "Werk", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Betrachtungs-w\u00fcrdig, sehn.", "tokens": ["Und", "Be\u00b7trach\u00b7tungs\u00b7w\u00fcr\u00b7dig", ",", "sehn", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df mich, Herr! wie wundersch\u00f6n,", "tokens": ["La\u00df", "mich", ",", "Herr", "!", "wie", "wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$.", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Dinge, die geschehn,", "tokens": ["Al\u00b7le", "Din\u00b7ge", ",", "die", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die blo\u00df durch dich entstehn,", "tokens": ["Und", "die", "blo\u00df", "durch", "dich", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Worin lauter Wunder liegen,", "tokens": ["Wo\u00b7rin", "lau\u00b7ter", "Wun\u00b7der", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Mich bestreben, anzusehn,", "tokens": ["Mich", "be\u00b7stre\u00b7ben", ",", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mit erstaunendem Vergn\u00fcgen!", "tokens": ["Mit", "er\u00b7stau\u00b7nen\u00b7dem", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}