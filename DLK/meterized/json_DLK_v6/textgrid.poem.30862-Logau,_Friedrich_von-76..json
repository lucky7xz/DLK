{"textgrid.poem.30862": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als zu singen wie Apollo Marsyas ihm hat getraut,", "tokens": ["Als", "zu", "sin\u00b7gen", "wie", "A\u00b7pol\u00b7lo", "Mar\u00b7syas", "ihm", "hat", "ge\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "KOKOM", "NE", "NE", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Hat er nichts hierdurch gewonnen, hat verloren seine Haut;", "tokens": ["Hat", "er", "nichts", "hier\u00b7durch", "ge\u00b7won\u00b7nen", ",", "hat", "ver\u00b7lo\u00b7ren", "sei\u00b7ne", "Haut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "PAV", "VVPP", "$,", "VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch beweinten ihn die Nymphen. O, wer wird wol mein Papier,", "tokens": ["Doch", "be\u00b7wein\u00b7ten", "ihn", "die", "Nym\u00b7phen", ".", "O", ",", "wer", "wird", "wol", "mein", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$.", "NE", "$,", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wann es Hochmut wird beschimpffen, gleichwol etwas achten hier?", "tokens": ["Wann", "es", "Hoch\u00b7mut", "wird", "be\u00b7schimpf\u00b7fen", ",", "gleich\u00b7wol", "et\u00b7was", "ach\u00b7ten", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VAFIN", "VVPP", "$,", "ADV", "ADV", "ADJA", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}