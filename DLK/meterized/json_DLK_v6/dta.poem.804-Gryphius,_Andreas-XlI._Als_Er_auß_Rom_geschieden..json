{"dta.poem.804": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlI.  \n Als Er au\u00df Rom geschieden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ade! begriff der welt/ Stadt der nichts gleich gewesen/ ", "tokens": ["A\u00b7de", "!", "be\u00b7griff", "der", "welt", "/", "Stadt", "der", "nichts", "gleich", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$(", "NN", "ART", "PIS", "ADV", "VAPP", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Vnd nichts zu gleichen ist/ In der man alles siht", "tokens": ["Vnd", "nichts", "zu", "glei\u00b7chen", "ist", "/", "In", "der", "man", "al\u00b7les", "siht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PTKZU", "VVINF", "VAFIN", "$(", "APPR", "ART", "PIS", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was zwischen Ost vnd West/ vnd Nord vnd Suden bl\u00fcht.", "tokens": ["Was", "zwi\u00b7schen", "Ost", "vnd", "West", "/", "vnd", "Nord", "vnd", "Su\u00b7den", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "KON", "NN", "$(", "KON", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was die Natur erdacht/ was je ein Mensch gelesen.", "tokens": ["Was", "die", "Na\u00b7tur", "er\u00b7dacht", "/", "was", "je", "ein", "Mensch", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$(", "PWS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du/ derer Aschen man/ nur nicht vorhin mit B\u00e4sen", "tokens": ["Du", "/", "de\u00b7rer", "A\u00b7schen", "man", "/", "nur", "nicht", "vor\u00b7hin", "mit", "B\u00e4\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PDS", "NN", "PIS", "$(", "ADV", "PTKNEG", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff einen hauffen k\u00e4hrt/ in der man sich bem\u00fcht", "tokens": ["Auff", "ei\u00b7nen", "hauf\u00b7fen", "k\u00e4hrt", "/", "in", "der", "man", "sich", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$(", "APPR", "PRELS", "PIS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu suchen wo dein grau\u00df/ (fliht tr\u00fcben Jahre! fliht!)", "tokens": ["Zu", "su\u00b7chen", "wo", "dein", "grau\u00df", "/", "(", "fliht", "tr\u00fc\u00b7ben", "Jah\u00b7re", "!", "fliht", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "PWAV", "PPOSAT", "NN", "$(", "$(", "VVFIN", "ADJA", "NN", "$.", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bist nach dem fall erh\u00f6ht/ nach langem Ach/ gen\u00e4sen.", "tokens": ["Bist", "nach", "dem", "fall", "er\u00b7h\u00f6ht", "/", "nach", "lan\u00b7gem", "Ach", "/", "ge\u00b7n\u00e4\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$(", "APPR", "ADJA", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jhr Wunder der gem\u00e4ld/ jhr pr\u00e4chtigen Pall\u00e4st/", "tokens": ["Ihr", "Wun\u00b7der", "der", "ge\u00b7m\u00e4ld", "/", "jhr", "pr\u00e4ch\u00b7ti\u00b7gen", "Pal\u00b7l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob den die kunst erstarr\u2019t/ du starck bewehrte Fest/", "tokens": ["Ob", "den", "die", "kunst", "er\u00b7starr't", "/", "du", "starck", "be\u00b7wehr\u00b7te", "Fest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVPP", "$(", "PPER", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Herrlichs ", "tokens": ["Du", "Herr\u00b7lichs"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Jhr B\u00fccher/ G\u00e4rten/ gr\u00fcfft\u2019; Jhr Bilder/ Nadeln/ Stein/", "tokens": ["Ihr", "B\u00fc\u00b7cher", "/", "G\u00e4r\u00b7ten", "/", "gr\u00fcfft'", ";", "Ihr", "Bil\u00b7der", "/", "Na\u00b7deln", "/", "Stein", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "$(", "VVFIN", "$.", "PPOSAT", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr/ die di\u00df vnd noch mehr schli\u00df\u2019t in die Sinnen eyn/", "tokens": ["Ihr", "/", "die", "di\u00df", "vnd", "noch", "mehr", "schli\u00df't", "in", "die", "Sin\u00b7nen", "eyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "PDS", "KON", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "ART", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ade! Man kan euch nicht satt mit zwey Augen schawen.", "tokens": ["A\u00b7de", "!", "Man", "kan", "euch", "nicht", "satt", "mit", "zwey", "Au\u00b7gen", "scha\u00b7wen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIS", "VMFIN", "PPER", "PTKNEG", "ADJD", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "+--+-+++-+--+", "measure": "iambic.septa.invert"}}}}}