{"dta.poem.6799": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von Spiegeln und Fern-  \n Gl\u00e4sern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch des Crystalls Betr\u00fcgereyen,", "tokens": ["Durch", "des", "Crys\u00b7talls", "Be\u00b7tr\u00fc\u00b7ge\u00b7re\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wird offt die Sch\u00f6nheit selbst in He\u00dflichkeit verkehrt.", "tokens": ["Wird", "offt", "die", "Sch\u00f6n\u00b7heit", "selbst", "in", "He\u00df\u00b7lich\u00b7keit", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald siehet man vom Zwerg ein scheu\u00dflich Abentheuer,", "tokens": ["Bald", "sie\u00b7het", "man", "vom", "Zwerg", "ein", "scheu\u00df\u00b7lich", "A\u00b7bent\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und bald ein Riesen-Bild, sehr gro\u00df und ungeheuer.", "tokens": ["Und", "bald", "ein", "Rie\u00b7sen\u00b7Bild", ",", "sehr", "gro\u00df", "und", "un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Figuren, die vertiefft, sind bald darinn zu sehn,", "tokens": ["Fi\u00b7gu\u00b7ren", ",", "die", "ver\u00b7tiefft", ",", "sind", "bald", "da\u00b7rinn", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,", "VAFIN", "ADV", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bald andre, die herauf und vorwerts stehn.", "tokens": ["Bald", "and\u00b7re", ",", "die", "her\u00b7auf", "und", "vor\u00b7werts", "stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Offt scheint ein Gegenwurff sich gantz heraus zu r\u00fccken,", "tokens": ["Offt", "scheint", "ein", "Ge\u00b7gen\u00b7wurff", "sich", "gantz", "he\u00b7raus", "zu", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADV", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den man doch billig sollt\u2019 im Spiegel-Glas erblicken.", "tokens": ["Den", "man", "doch", "bil\u00b7lig", "sollt'", "im", "Spie\u00b7gel\u00b7Glas", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}