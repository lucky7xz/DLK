{"textgrid.poem.55235": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Versus Memoriales", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.85", "ro:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Invocavit wir rufen laut,", "tokens": ["In\u00b7vo\u00b7ca\u00b7vit", "wir", "ru\u00b7fen", "laut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Reminiscere o w\u00e4r ich Braut!", "tokens": ["Re\u00b7mi\u00b7nis\u00b7ce\u00b7re", "o", "w\u00e4r", "ich", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die Oculi gehn hin und her;", "tokens": ["Die", "O\u00b7cu\u00b7li", "gehn", "hin", "und", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Laetare dr\u00fcber nicht so sehr.", "tokens": ["Lae\u00b7ta\u00b7re", "dr\u00fc\u00b7ber", "nicht", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "PTKNEG", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O Judica uns nicht so streng!", "tokens": ["O", "Ju\u00b7di\u00b7ca", "uns", "nicht", "so", "streng", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Palmarum streuen wir die Meng.", "tokens": ["Pal\u00b7ma\u00b7rum", "streu\u00b7en", "wir", "die", "Meng", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Auf Ostereier freun sich hie", "tokens": ["Auf", "Os\u00b7te\u00b7rei\u00b7er", "freun", "sich", "hie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Viel Quasi modo geniti.", "tokens": ["Viel", "Qua\u00b7si", "mo\u00b7do", "ge\u00b7ni\u00b7ti", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Misericordias brauchen wir all,", "tokens": ["Mi\u00b7se\u00b7ri\u00b7cor\u00b7di\u00b7as", "brau\u00b7chen", "wir", "all", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIAT", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Jubilate ist ein seltner Fall.", "tokens": ["Ju\u00b7bi\u00b7la\u00b7te", "ist", "ein", "selt\u00b7ner", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Cantate freut der Menschen Sinn,", "tokens": ["Can\u00b7ta\u00b7te", "freut", "der", "Men\u00b7schen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Rogate bringt nicht viel Gewinn.", "tokens": ["Ro\u00b7ga\u00b7te", "bringt", "nicht", "viel", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Exaudi uns zu dieser Frist,", "tokens": ["Ex\u00b7au\u00b7di", "uns", "zu", "die\u00b7ser", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Spiritus, der du der letzte bist.", "tokens": ["Spi\u00b7ri\u00b7tus", ",", "der", "du", "der", "letz\u00b7te", "bist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ART", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}