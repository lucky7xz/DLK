{"textgrid.poem.49513": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zweytes Rondeau", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Rock von Regen, Wind und Schnee", "tokens": ["Den", "Rock", "von", "Re\u00b7gen", ",", "Wind", "und", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat nun die JahrsZeit ausgezogen.", "tokens": ["Hat", "nun", "die", "Jahrs", "Zeit", "aus\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr ist ein sch\u00f6nerer von Klee", "tokens": ["Ihr", "ist", "ein", "sch\u00f6\u00b7ne\u00b7rer", "von", "Klee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sonnenstralen angeflogen.", "tokens": ["Und", "Son\u00b7nen\u00b7stra\u00b7len", "an\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Myrtill singt mit der Galathee:", "tokens": ["Myr\u00b7till", "singt", "mit", "der", "Ga\u00b7la\u00b7thee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Den Rock von Regen, Wind und Schnee", "tokens": ["Den", "Rock", "von", "Re\u00b7gen", ",", "Wind", "und", "Schnee"], "token_info": ["word", "word", "word"