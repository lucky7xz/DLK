{"textgrid.poem.53612": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Abschied von Noske", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da gehst Du hin und singst nicht mehr.", "tokens": ["Da", "gehst", "Du", "hin", "und", "singst", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben oft von Dir gesungen,", "tokens": ["Wir", "ha\u00b7ben", "oft", "von", "Dir", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "hell hat hier unser Lied geklungen.", "tokens": ["hell", "hat", "hier", "un\u00b7ser", "Lied", "ge\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es ist uns stets vorbeigelungen:", "tokens": ["Es", "ist", "uns", "stets", "vor\u00b7bei\u00b7ge\u00b7lun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du bliebst und ruiniertest uns noch mehr.", "tokens": ["Du", "bliebst", "und", "ru\u00b7i\u00b7nier\u00b7test", "uns", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jetzt gehst Du hin und singst nicht mehr.", "tokens": ["Jetzt", "gehst", "Du", "hin", "und", "singst", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du warst der B\u00fcrger starker Mann.", "tokens": ["Du", "warst", "der", "B\u00fcr\u00b7ger", "star\u00b7ker", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du schliefst, indes die Adjutanten,", "tokens": ["Du", "schliefst", ",", "in\u00b7des", "die", "Ad\u00b7ju\u00b7tan\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "die den Proleten rasch erkannten,", "tokens": ["die", "den", "Pro\u00b7le\u00b7ten", "rasch", "er\u00b7kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Trupps f\u00fcr ihren Zweck verwandten \u2013", "tokens": ["die", "Trupps", "f\u00fcr", "ih\u00b7ren", "Zweck", "ver\u00b7wand\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Landauer, Liebknecht! was lag Dir daran!", "tokens": ["Lan\u00b7dau\u00b7er", ",", "Lieb\u00b7knecht", "!", "was", "lag", "Dir", "da\u00b7ran", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PWS", "VVFIN", "PPER", "PAV", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Du warst der B\u00fcrger starker Mann.", "tokens": ["Du", "warst", "der", "B\u00fcr\u00b7ger", "star\u00b7ker", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du gehst. Koch Dir Kamillentee.", "tokens": ["Du", "gehst", ".", "Koch", "Dir", "Ka\u00b7mil\u00b7len\u00b7tee", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und denk im K\u00e4ppchen Du an Kappen.", "tokens": ["Und", "denk", "im", "K\u00e4pp\u00b7chen", "Du", "an", "Kap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Einst fuhrst Du mit den Landsknechtsknappen.", "tokens": ["Einst", "fuhrst", "Du", "mit", "den", "Lands\u00b7knechts\u00b7knap\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt h\u00e4ltst Du es mit Schusters Rappen \u2013", "tokens": ["Jetzt", "h\u00e4ltst", "Du", "es", "mit", "Schus\u00b7ters", "Rap\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "vorbei Importen und Gesangssoiree . . .", "tokens": ["vor\u00b7bei", "Im\u00b7por\u00b7ten", "und", "Ge\u00b7sangs\u00b7soi\u00b7ree", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du gehst. Koch Dir Kamillentee.", "tokens": ["Du", "gehst", ".", "Koch", "Dir", "Ka\u00b7mil\u00b7len\u00b7tee", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gehst Du? Noch ist Dein Ungeist da.", "tokens": ["Gehst", "Du", "?", "Noch", "ist", "Dein", "Un\u00b7geist", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch wollen alle sies nicht wissen:", "tokens": ["Noch", "wol\u00b7len", "al\u00b7le", "sies", "nicht", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir bleiben tief in K\u00fcmmernissen,", "tokens": ["Wir", "blei\u00b7ben", "tief", "in", "K\u00fcm\u00b7mer\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "bis wir das Achselst\u00fcck zerrissen.", "tokens": ["bis", "wir", "das", "Ach\u00b7sel\u00b7st\u00fcck", "zer\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lange noch Tat\u00fc-tata?", "tokens": ["Wie", "lan\u00b7ge", "noch", "Ta\u00b7t\u00fc\u00b7ta\u00b7ta", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du gingst. Und bist noch immer da.", "tokens": ["Du", "gingst", ".", "Und", "bist", "noch", "im\u00b7mer", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}