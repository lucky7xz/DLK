{"dta.poem.6115": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von C\u00f6rpern, welche schwer, erforscht man die Natur.", "tokens": ["Von", "C\u00f6r\u00b7pern", ",", "wel\u00b7che", "schwer", ",", "er\u00b7forscht", "man", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADJD", "$,", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie scheinen, als ob sie zum Mittelpunet der Erden", "tokens": ["Sie", "schei\u00b7nen", ",", "als", "ob", "sie", "zum", "Mit\u00b7tel\u00b7pu\u00b7net", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Herabgezogen werden:", "tokens": ["Her\u00b7ab\u00b7ge\u00b7zo\u00b7gen", "wer\u00b7den", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach ihrem Lauff, der Dicke, der Figur,", "tokens": ["Nach", "ih\u00b7rem", "Lauff", ",", "der", "Di\u00b7cke", ",", "der", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Berechnet man den Grund von ihrem Fallen.", "tokens": ["Be\u00b7rech\u00b7net", "man", "den", "Grund", "von", "ih\u00b7rem", "Fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was vor ein Trieb regiert von ihnen allen", "tokens": ["Was", "vor", "ein", "Trieb", "re\u00b7giert", "von", "ih\u00b7nen", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "APPR", "PPER", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Jm Sincken, ihre Fahrt? Was kan sie lencken,", "tokens": ["Jm", "Sin\u00b7cken", ",", "ih\u00b7re", "Fahrt", "?", "Was", "kan", "sie", "len\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PPOSAT", "NN", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df sie bald langsam sich, bald schneller abwerts sencken?", "tokens": ["Da\u00df", "sie", "bald", "lang\u00b7sam", "sich", ",", "bald", "schnel\u00b7ler", "ab\u00b7werts", "sen\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PRF", "$,", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man mu\u00df nichts von geheim-bewegenden Gesetzen,", "tokens": ["Man", "mu\u00df", "nichts", "von", "ge\u00b7heim\u00b7be\u00b7we\u00b7gen\u00b7den", "Ge\u00b7set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nichts von Vereinungs-Trieb, noch kleinen H\u00e4cklein schw\u00e4tzen.", "tokens": ["Nichts", "von", "Ver\u00b7ei\u00b7nungs\u00b7Trieb", ",", "noch", "klei\u00b7nen", "H\u00e4ck\u00b7lein", "schw\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "$,", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein eintzig\u2019 Ursach ist allein,", "tokens": ["Ein", "eint\u00b7zig'", "Ur\u00b7sach", "ist", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "(und zwar die klar und allgemein)", "tokens": ["(", "und", "zwar", "die", "klar", "und", "all\u00b7ge\u00b7mein", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So C\u00f6rper schwer und leichte macht.", "tokens": ["So", "C\u00f6r\u00b7per", "schwer", "und", "leich\u00b7te", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJD", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dieselbige Bewegung ird\u2019scher Theile,", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", "Be\u00b7we\u00b7gung", "ird'\u00b7scher", "Thei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wodurch so manche Welt in runde Form gebracht,", "tokens": ["Wo\u00b7durch", "so", "man\u00b7che", "Welt", "in", "run\u00b7de", "Form", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Verursacht noch, da\u00df unsre Lufft in Eile", "tokens": ["Ver\u00b7ur\u00b7sacht", "noch", ",", "da\u00df", "uns\u00b7re", "Lufft", "in", "Ei\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Auch andre C\u00f6rper dr\u00fcckt,", "tokens": ["Auch", "and\u00b7re", "C\u00f6r\u00b7per", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Und sie herab zum irdschen Klumpen schickt.", "tokens": ["Und", "sie", "her\u00b7ab", "zum", "ird\u00b7schen", "Klum\u00b7pen", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Daher kommts, da\u00df so wol von Menschen, als von Thieren,", "tokens": ["Da\u00b7her", "kommts", ",", "da\u00df", "so", "wol", "von", "Men\u00b7schen", ",", "als", "von", "Thie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KOUS", "ADV", "ADV", "APPR", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die C\u00f6rper, so durch Lufft umgeben und erf\u00fcllt,", "tokens": ["Die", "C\u00f6r\u00b7per", ",", "so", "durch", "Lufft", "um\u00b7ge\u00b7ben", "und", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und die sich alle gleich aus ird\u2019schem Stoff formiren,", "tokens": ["Und", "die", "sich", "al\u00b7le", "gleich", "aus", "ird'\u00b7schem", "Stoff", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRF", "PIS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit F\u00fcssen gegen sich,", "tokens": ["Mit", "F\u00fcs\u00b7sen", "ge\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Rund um den Erd-Kreis her marschiren.", "tokens": ["Rund", "um", "den", "Erd\u00b7Kreis", "her", "mar\u00b7schi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APZR", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}