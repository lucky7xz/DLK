{"dta.poem.19245": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, wer aus Finsternis auf einmal tritt ins Licht,", "tokens": ["Wie", ",", "wer", "aus", "Fins\u00b7ter\u00b7nis", "auf", "ein\u00b7mal", "tritt", "ins", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWS", "APPR", "NN", "APPR", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geblendet ist und sieht vor lauter Sehen nicht;", "tokens": ["Ge\u00b7blen\u00b7det", "ist", "und", "sieht", "vor", "lau\u00b7ter", "Se\u00b7hen", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wie hinwiederum wer aus dem vollen Stral", "tokens": ["Und", "wie", "hin\u00b7wie\u00b7de\u00b7rum", "wer", "aus", "dem", "vol\u00b7len", "Stral"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Tages pl\u00f6tzlich tritt in v\u00f6llig dunkeln Saal:", "tokens": ["Des", "Ta\u00b7ges", "pl\u00f6tz\u00b7lich", "tritt", "in", "v\u00f6l\u00b7lig", "dun\u00b7keln", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Auge starrt, bis es dem Wechsel sich gew\u00f6hnt,", "tokens": ["Das", "Au\u00b7ge", "starrt", ",", "bis", "es", "dem", "Wech\u00b7sel", "sich", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mit der innern Welt die \u00e4u\u00dfre sich vers\u00f6hnt;", "tokens": ["Und", "mit", "der", "in\u00b7nern", "Welt", "die", "\u00e4u\u00df\u00b7re", "sich", "ver\u00b7s\u00f6hnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bis dort das Auge lernt im Glanze sich zu weiden,", "tokens": ["Bis", "dort", "das", "Au\u00b7ge", "lernt", "im", "Glan\u00b7ze", "sich", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hier die Gegenst\u00e4nd' im Dunkel unterscheiden:", "tokens": ["Und", "hier", "die", "Ge\u00b7gen\u00b7st\u00e4nd'", "im", "Dun\u00b7kel", "un\u00b7ter\u00b7schei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "So kann ein Menschenherz viel Gl\u00fcck und Ungl\u00fcck fassen,", "tokens": ["So", "kann", "ein", "Men\u00b7schen\u00b7herz", "viel", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ists am gl\u00fccklichsten in seiner Ruh gelassen;", "tokens": ["Doch", "ists", "am", "gl\u00fcck\u00b7lichs\u00b7ten", "in", "sei\u00b7ner", "Ruh", "ge\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "ADJA", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Von Glanz geblendet nicht, noch auch von Nacht umh\u00fcllt,", "tokens": ["Von", "Glanz", "ge\u00b7blen\u00b7det", "nicht", ",", "noch", "auch", "von", "Nacht", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "PTKNEG", "$,", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von sanftged\u00e4mpftem Licht Aug' und Gem\u00fcth erf\u00fcllt.", "tokens": ["Von", "sanft\u00b7ge\u00b7d\u00e4mpf\u00b7tem", "Licht", "Aug'", "und", "Ge\u00b7m\u00fcth", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}