{"textgrid.poem.54545": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du forderst Herr von uns zum Neu-Jahr dein Geschencke/", "tokens": ["Du", "for\u00b7derst", "Herr", "von", "uns", "zum", "Neu\u00b7Jahr", "dein", "Ge\u00b7schen\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das du auf kurtze Frist uns und der Welt vertraut.", "tokens": ["Das", "du", "auf", "kurt\u00b7ze", "Frist", "uns", "und", "der", "Welt", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "PPER", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nimm den! Ich la\u00df es hin; es hat die Welt geschaut", "tokens": ["Nimm", "den", "!", "Ich", "la\u00df", "es", "hin", ";", "es", "hat", "die", "Welt", "ge\u00b7schaut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wie sich in der Welt ein Mensch in Schmertzen kr\u00e4ncke.", "tokens": ["Und", "wie", "sich", "in", "der", "Welt", "ein", "Mensch", "in", "Schmert\u00b7zen", "kr\u00e4n\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es schaut nun freudig an/ wie lieb reich Gott bedencke/", "tokens": ["Es", "schaut", "nun", "freu\u00b7dig", "an", "/", "wie", "lieb", "reich", "Gott", "be\u00b7den\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$(", "PWAV", "ADJD", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Er durchs reine Bad abwusch/ wof\u00fcr ihm graut/", "tokens": ["Den", "Er", "durchs", "rei\u00b7ne", "Bad", "ab\u00b7wusch", "/", "wo\u00b7f\u00fcr", "ihm", "graut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NE", "ADJD", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dem Er ein Grab allhier und dort ein Schlo\u00df auffbaut", "tokens": ["Dem", "Er", "ein", "Grab", "all\u00b7hier", "und", "dort", "ein", "Schlo\u00df", "auff\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "KON", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zu sich rufft/ da\u00df Er sich nicht von Ihm ablencke.", "tokens": ["Und", "zu", "sich", "rufft", "/", "da\u00df", "Er", "sich", "nicht", "von", "Ihm", "ab\u00b7len\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVFIN", "$(", "KOUS", "PPER", "PRF", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Zeuch hin! zeuch fr\u00f6lich hin! Ob schon mein Hertze bricht/", "tokens": ["Zeuch", "hin", "!", "zeuch", "fr\u00f6\u00b7lich", "hin", "!", "Ob", "schon", "mein", "Hert\u00b7ze", "bricht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVIMP", "ADJD", "PTKVZ", "$.", "KOUS", "ADV", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Beklag ich liebstes Kind doch deinen Abschied nicht/", "tokens": ["Be\u00b7klag", "ich", "liebs\u00b7tes", "Kind", "doch", "dei\u00b7nen", "Ab\u00b7schied", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil dir weit besser ist als ich je w\u00fcntschen k\u00f6nnen.", "tokens": ["Weil", "dir", "weit", "bes\u00b7ser", "ist", "als", "ich", "je", "w\u00fcnt\u00b7schen", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VAFIN", "KOUS", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.12": {"text": "Du forderst grosser Gott di\u00df werthe Pfand von mir;", "tokens": ["Du", "for\u00b7derst", "gros\u00b7ser", "Gott", "di\u00df", "wert\u00b7he", "Pfand", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "PDS", "VVFIN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich weigre ferner nicht. Wolan es zeucht zu dir:", "tokens": ["Ich", "weig\u00b7re", "fer\u00b7ner", "nicht", ".", "Wo\u00b7lan", "es", "zeucht", "zu", "dir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$.", "PWAV", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was wirst hergegen du mir vor ein Neu-Jahr g\u00f6nnen?", "tokens": ["Was", "wirst", "her\u00b7ge\u00b7gen", "du", "mir", "vor", "ein", "Neu\u00b7Jahr", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}