{"textgrid.poem.33490": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Mondlandschaft", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oben brennt das gelbe Mutterauge.", "tokens": ["O\u00b7ben", "brennt", "das", "gel\u00b7be", "Mut\u00b7ter\u00b7au\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcberall liegt Nacht wie blaues Tuch.", "tokens": ["\u00dc\u00b7be\u00b7rall", "liegt", "Nacht", "wie", "blau\u00b7es", "Tuch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fraglos ist, da\u00df ich jetzt Atem sauge.", "tokens": ["Frag\u00b7los", "ist", ",", "da\u00df", "ich", "jetzt", "A\u00b7tem", "sau\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "KOUS", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ich bin nur ein kleines Bilderbuch.", "tokens": ["Ich", "bin", "nur", "ein", "klei\u00b7nes", "Bil\u00b7der\u00b7buch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "H\u00e4user fangen Tr\u00e4ume bunter Schl\u00e4fer", "tokens": ["H\u00e4u\u00b7ser", "fan\u00b7gen", "Tr\u00e4u\u00b7me", "bun\u00b7ter", "Schl\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wie in Netzen in den Fenstern auf.", "tokens": ["Wie", "in", "Net\u00b7zen", "in", "den", "Fens\u00b7tern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Autos kriechen wie Marienk\u00e4fer", "tokens": ["Au\u00b7tos", "krie\u00b7chen", "wie", "Ma\u00b7ri\u00b7en\u00b7k\u00e4\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "NE"], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Leuchtende Stra\u00dfen hinauf.", "tokens": ["Leuch\u00b7ten\u00b7de", "Stra\u00b7\u00dfen", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}