{"textgrid.poem.26170": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es ist ein Geisterreich neu in der Luft entstanden", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch in der Abendd\u00e4mmerung leuchten Akazienbl\u00fcten hell,", "tokens": ["Noch", "in", "der", "A\u00b7bend\u00b7d\u00e4m\u00b7me\u00b7rung", "leuch\u00b7ten", "A\u00b7ka\u00b7zi\u00b7en\u00b7bl\u00fc\u00b7ten", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Dein Fu\u00df geht durch den dichten Duft kaum von der Stell',", "tokens": ["Dein", "Fu\u00df", "geht", "durch", "den", "dich\u00b7ten", "Duft", "kaum", "von", "der", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist ein Geisterreich neu in der Luft entstanden;", "tokens": ["Es", "ist", "ein", "Geis\u00b7ter\u00b7reich", "neu", "in", "der", "Luft", "ent\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Und neue Wege \u00fcber deinem Haupt sich fanden,", "tokens": ["Und", "neu\u00b7e", "We\u00b7ge", "\u00fc\u00b7ber", "dei\u00b7nem", "Haupt", "sich", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dir dein Fu\u00df nicht mehr am Boden gehen will.", "tokens": ["Da\u00df", "dir", "dein", "Fu\u00df", "nicht", "mehr", "am", "Bo\u00b7den", "ge\u00b7hen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Geist der Bl\u00fcten zieht zum Himmel stark und still.", "tokens": ["Der", "Geist", "der", "Bl\u00fc\u00b7ten", "zieht", "zum", "Him\u00b7mel", "stark", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Reich der D\u00fcfte dringt dir in das tiefste Mark,", "tokens": ["Das", "Reich", "der", "D\u00fcf\u00b7te", "dringt", "dir", "in", "das", "tiefs\u00b7te", "Mark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du im Dunkel Lieder h\u00f6rst, wo niemand singt,", "tokens": ["Da\u00df", "du", "im", "Dun\u00b7kel", "Lie\u00b7der", "h\u00f6rst", ",", "wo", "nie\u00b7mand", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "NN", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und die Akazie an dem Weg dich aus dir aufzusehen zwingt.", "tokens": ["Und", "die", "A\u00b7ka\u00b7zie", "an", "dem", "Weg", "dich", "aus", "dir", "auf\u00b7zu\u00b7se\u00b7hen", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "PRF", "APPR", "PPER", "VVIZU", "VVFIN", "$."], "meter": "--+--+-+-+-+-+-+", "measure": "anapaest.di.plus"}}}}}