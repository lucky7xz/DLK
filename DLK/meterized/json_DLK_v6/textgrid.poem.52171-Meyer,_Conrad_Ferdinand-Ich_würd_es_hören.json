{"textgrid.poem.52171": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Ich w\u00fcrd es h\u00f6ren", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4g dort ich unterm Firneschein", "tokens": ["L\u00e4g", "dort", "ich", "un\u00b7term", "Fir\u00b7ne\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf hoher Alp begraben,", "tokens": ["Auf", "ho\u00b7her", "Alp", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich schliefe mitten im Juchhein", "tokens": ["Ich", "schlie\u00b7fe", "mit\u00b7ten", "im", "Juch\u00b7hein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der wilden Hirtenknaben.", "tokens": ["Der", "wil\u00b7den", "Hir\u00b7ten\u00b7kna\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo sonst ich lag im s\u00fc\u00dfen Tag,", "tokens": ["Wo", "sonst", "ich", "lag", "im", "s\u00fc\u00b7\u00dfen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4g ich in dunkeln Decken,", "tokens": ["L\u00e4g", "ich", "in", "dun\u00b7keln", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Laue Krach und dumpfer Schlag,", "tokens": ["Der", "Lau\u00b7e", "Krach", "und", "dum\u00b7pfer", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er w\u00fcrde mich nicht wecken.", "tokens": ["Er", "w\u00fcr\u00b7de", "mich", "nicht", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und k\u00e4me schwarzer Sturm gerauscht", "tokens": ["Und", "k\u00e4\u00b7me", "schwar\u00b7zer", "Sturm", "ge\u00b7rauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sch\u00fcttelte die Tannen,", "tokens": ["Und", "sch\u00fct\u00b7tel\u00b7te", "die", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er f\u00fchre, von mir unbelauscht,", "tokens": ["Er", "f\u00fch\u00b7re", ",", "von", "mir", "un\u00b7be\u00b7lauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor\u00fcber und von dannen.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "und", "von", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch kl\u00e4nge sanfter Glockenchor,", "tokens": ["Doch", "kl\u00e4n\u00b7ge", "sanf\u00b7ter", "Glo\u00b7cken\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lie\u00dfe wohl mich st\u00f6ren", "tokens": ["Ich", "lie\u00b7\u00dfe", "wohl", "mich", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lauscht ein Weilchen gern empor,", "tokens": ["Und", "lauscht", "ein", "Weil\u00b7chen", "gern", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herdgel\u00e4ut zu h\u00f6ren.", "tokens": ["Das", "Herd\u00b7ge\u00b7l\u00e4ut", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}