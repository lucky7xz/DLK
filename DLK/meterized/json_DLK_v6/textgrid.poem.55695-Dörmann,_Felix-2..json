{"textgrid.poem.55695": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halb unbewu\u00dft war ich hinausgegangen ...", "tokens": ["Halb", "un\u00b7be\u00b7wu\u00dft", "war", "ich", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu einer stillen, gr\u00fcnen Ruhest\u00e4tte,", "tokens": ["Zu", "ei\u00b7ner", "stil\u00b7len", ",", "gr\u00fc\u00b7nen", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu einem schattenk\u00fchlen Schlummerbette,", "tokens": ["Zu", "ei\u00b7nem", "schat\u00b7ten\u00b7k\u00fch\u00b7len", "Schlum\u00b7mer\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem ein Herz vergi\u00dft sein Glutverlangen.", "tokens": ["In", "dem", "ein", "Herz", "ver\u00b7gi\u00dft", "sein", "Glut\u00b7ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wieder fa\u00dfte mich das alte Bangen,", "tokens": ["Und", "wie\u00b7der", "fa\u00df\u00b7te", "mich", "das", "al\u00b7te", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ich so gerne l\u00e4ngst begraben h\u00e4tte,", "tokens": ["Das", "ich", "so", "ger\u00b7ne", "l\u00e4ngst", "be\u00b7gra\u00b7ben", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wieder h\u00f6rte ich Dein dumpfes: \u00bbRette!\u00ab", "tokens": ["Und", "wie\u00b7der", "h\u00f6r\u00b7te", "ich", "Dein", "dum\u00b7pfes", ":", "\u00bb", "Ret\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das mich verfolgt in N\u00e4chten, qualvoll langen.", "tokens": ["Das", "mich", "ver\u00b7folgt", "in", "N\u00e4ch\u00b7ten", ",", "qual\u00b7voll", "lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und meiner Brust, der hei\u00dfen, sehnsuchtstollen,", "tokens": ["Und", "mei\u00b7ner", "Brust", ",", "der", "hei\u00b7\u00dfen", ",", "sehn\u00b7sucht\u00b7stol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VVINF", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erstickte Liebeslaute sich entrangen,", "tokens": ["Er\u00b7stick\u00b7te", "Lie\u00b7bes\u00b7lau\u00b7te", "sich", "ent\u00b7ran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schwere Reuetr\u00e4nen niederquollen.", "tokens": ["Und", "schwe\u00b7re", "Reu\u00b7e\u00b7tr\u00e4\u00b7nen", "nie\u00b7der\u00b7quol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da lebenswarm noch gl\u00fchten Deine Wangen,", "tokens": ["Da", "le\u00b7bens\u00b7warm", "noch", "gl\u00fch\u00b7ten", "Dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War ich zu kalt, um Freundschaft nur zu zollen,", "tokens": ["War", "ich", "zu", "kalt", ",", "um", "Freund\u00b7schaft", "nur", "zu", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "$,", "KOUI", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jetzt, nach Deinem Tod, dies Glutverlangen?", "tokens": ["Und", "jetzt", ",", "nach", "Dei\u00b7nem", "Tod", ",", "dies", "Glut\u00b7ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}