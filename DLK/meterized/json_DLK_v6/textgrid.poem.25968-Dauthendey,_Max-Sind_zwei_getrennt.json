{"textgrid.poem.25968": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Sind zwei getrennt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh droben den Mond zwischen T\u00fcrmen h\u00e4ngen,", "tokens": ["Sieh", "dro\u00b7ben", "den", "Mond", "zwi\u00b7schen", "T\u00fcr\u00b7men", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Er konnte die Nacht aus dem Himmel verdr\u00e4ngen.", "tokens": ["Er", "konn\u00b7te", "die", "Nacht", "aus", "dem", "Him\u00b7mel", "ver\u00b7dr\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er h\u00e4ngt wie der Schein alles Sehnenden oben,", "tokens": ["Er", "h\u00e4ngt", "wie", "der", "Schein", "al\u00b7les", "Seh\u00b7nen\u00b7den", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "PIAT", "NN", "ADV", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie Helle, die sich voll Hoffnung gehoben.", "tokens": ["Wie", "Hel\u00b7le", ",", "die", "sich", "voll", "Hoff\u00b7nung", "ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "PRELS", "PRF", "ADJD", "NN", "VVPP", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und sind zwei getrennt, auch in fremdesten Gassen,", "tokens": ["Und", "sind", "zwei", "ge\u00b7trennt", ",", "auch", "in", "frem\u00b7des\u00b7ten", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "CARD", "VVPP", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verliebten wird niemals ihr Himmel verblassen,", "tokens": ["Ver\u00b7lieb\u00b7ten", "wird", "nie\u00b7mals", "ihr", "Him\u00b7mel", "ver\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr Himmel, der kann ihre Augen aufhellen", "tokens": ["Ihr", "Him\u00b7mel", ",", "der", "kann", "ih\u00b7re", "Au\u00b7gen", "auf\u00b7hel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Durch brennende Botschaften zwischen zwei Schwellen.", "tokens": ["Durch", "bren\u00b7nen\u00b7de", "Bot\u00b7schaf\u00b7ten", "zwi\u00b7schen", "zwei", "Schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}}}}}