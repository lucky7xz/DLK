{"dta.poem.10538": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Am Fenster stand die Mutter,", "tokens": ["Am", "Fens\u00b7ter", "stand", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Bette lag der Sohn.", "tokens": ["Im", "Bet\u00b7te", "lag", "der", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewillst du nicht aufstehn, Wilhelm,", "tokens": ["\u201e", "willst", "du", "nicht", "auf\u00b7stehn", ",", "Wil\u00b7helm", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu schau'n die Prozession?\u201c \u2014", "tokens": ["Zu", "schau'n", "die", "Pro\u00b7zes\u00b7si\u00b7on", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "\u201eich bin so krank, o Mutter,", "tokens": ["\u201e", "ich", "bin", "so", "krank", ",", "o", "Mut\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich nicht h\u00f6r' und seh';", "tokens": ["Da\u00df", "ich", "nicht", "h\u00f6r'", "und", "seh'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich denk' an das todte Gretchen,", "tokens": ["Ich", "denk'", "an", "das", "tod\u00b7te", "Gret\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da thut das Herz mir weh.\u201c \u2014", "tokens": ["Da", "thut", "das", "Herz", "mir", "weh", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201esteh' auf, wir wollen nach Kevlaar,", "tokens": ["\u201e", "steh'", "auf", ",", "wir", "wol\u00b7len", "nach", "Kev\u00b7laar", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm Buch und Rosenkranz;", "tokens": ["Nimm", "Buch", "und", "Ro\u00b7sen\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mutter Gottes heilt dir", "tokens": ["Die", "Mut\u00b7ter", "Got\u00b7tes", "heilt", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein krankes Herze ganz.\u201c", "tokens": ["Dein", "kran\u00b7kes", "Her\u00b7ze", "ganz", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es flattern die Kirchenfahnen,", "tokens": ["Es", "flat\u00b7tern", "die", "Kir\u00b7chen\u00b7fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es singt im Kirchenton;", "tokens": ["Es", "singt", "im", "Kir\u00b7chen\u00b7ton", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist zu C\u00f6lln am Rheine,", "tokens": ["Das", "ist", "zu", "C\u00f6lln", "am", "Rhei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da geht die Prozession.", "tokens": ["Da", "geht", "die", "Pro\u00b7zes\u00b7si\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die Mutter folgt der Menge,", "tokens": ["Die", "Mut\u00b7ter", "folgt", "der", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Sohn, den f\u00fchret sie,", "tokens": ["Den", "Sohn", ",", "den", "f\u00fch\u00b7ret", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie singen beide im Chore:", "tokens": ["Sie", "sin\u00b7gen", "bei\u00b7de", "im", "Cho\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gelobt sey'st du Marie!", "tokens": ["Ge\u00b7lobt", "sey'st", "du", "Ma\u00b7rie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}