{"textgrid.poem.63769": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Andre Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh nur, wie str\u00f6mt's hinein in Sant' Agnese!", "tokens": ["Sieh", "nur", ",", "wie", "str\u00f6mt's", "hin\u00b7ein", "in", "Sant'", "Ag\u00b7ne\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "VVFIN", "ADV", "APPR", "NE", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist denn der guten Heil'gen Festtag heute?", "tokens": ["Ist", "denn", "der", "gu\u00b7ten", "Heil'\u00b7gen", "Fest\u00b7tag", "heu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So triumphierend st\u00fcrmt das Turmgel\u00e4ute,", "tokens": ["So", "tri\u00b7um\u00b7phie\u00b7rend", "st\u00fcrmt", "das", "Turm\u00b7ge\u00b7l\u00e4u\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als ob der Papst heut selbst die Messe l\u00e4se.", "tokens": ["Als", "ob", "der", "Papst", "heut", "selbst", "die", "Mes\u00b7se", "l\u00e4\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zu Fu\u00df, zu Wagen \u2013 Bettler und Marchese", "tokens": ["Zu", "Fu\u00df", ",", "zu", "Wa\u00b7gen", "\u2013", "Bett\u00b7ler", "und", "Mar\u00b7che\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Kampf, da\u00df man ein Pl\u00e4tzchen noch erbeute \u2013", "tokens": ["Im", "Kampf", ",", "da\u00df", "man", "ein", "Pl\u00e4tz\u00b7chen", "noch", "er\u00b7beu\u00b7te", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PIS", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sagt, was begibt sich drin, ihr guten Leute?", "tokens": ["Sagt", ",", "was", "be\u00b7gibt", "sich", "drin", ",", "ihr", "gu\u00b7ten", "Leu\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "VVFIN", "PRF", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbeh! Fra Giovanni singt, Signor Inglese.\u00ab", "tokens": ["\u00bb", "eh", "!", "Fra", "Gio\u00b7van\u00b7ni", "singt", ",", "Sig\u00b7nor", "I\u00b7ngle\u00b7se", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "$.", "NE", "NE", "VVFIN", "$,", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja so, der M\u00f6nch, der alle Welt entz\u00fcckt!", "tokens": ["Ja", "so", ",", "der", "M\u00f6nch", ",", "der", "al\u00b7le", "Welt", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "St\u00fcnd' heut der Heiland wieder auf, er m\u00fc\u00dfte", "tokens": ["St\u00fcnd'", "heut", "der", "Hei\u00b7land", "wie\u00b7der", "auf", ",", "er", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$,", "PPER", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den k\u00fcrzern ziehn vor diesem Prachttenore.", "tokens": ["Den", "k\u00fcr\u00b7zern", "ziehn", "vor", "die\u00b7sem", "Pracht\u00b7te\u00b7no\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Kirche tr\u00e4gt, seit sie der Purpur schm\u00fcckt,", "tokens": ["Die", "Kir\u00b7che", "tr\u00e4gt", ",", "seit", "sie", "der", "Pur\u00b7pur", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nach ausverkauften H\u00e4usern ein Gel\u00fcste,", "tokens": ["Nach", "aus\u00b7ver\u00b7kauf\u00b7ten", "H\u00e4u\u00b7sern", "ein", "Ge\u00b7l\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und gleich der Oper macht sie gern Furore.", "tokens": ["Und", "gleich", "der", "O\u00b7per", "macht", "sie", "gern", "Fu\u00b7ro\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}