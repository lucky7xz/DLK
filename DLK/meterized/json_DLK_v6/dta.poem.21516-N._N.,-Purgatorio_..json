{"dta.poem.21516": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Purgatorio .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Zieh\u2019 ein, o Schmerz,", "tokens": ["Zieh'", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und weihe dies Herz,", "tokens": ["Und", "wei\u00b7he", "dies", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das lange sich deiner gewehrt hat!", "tokens": ["Das", "lan\u00b7ge", "sich", "dei\u00b7ner", "ge\u00b7wehrt", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "PPOSAT", "VVPP", "VAFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und in flammendem Groll", "tokens": ["Und", "in", "flam\u00b7men\u00b7dem", "Groll"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Gegen des Lebens Zoll,", "tokens": ["Ge\u00b7gen", "des", "Le\u00b7bens", "Zoll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Gegen deine Macht sich emp\u00f6rt hat!", "tokens": ["Ge\u00b7gen", "dei\u00b7ne", "Macht", "sich", "em\u00b7p\u00f6rt", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Zieh\u2019 ein, o Schmerz,", "tokens": ["Zieh'", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und l\u00e4ut\u2019re dies Herz \u2014", "tokens": ["Und", "l\u00e4ut'\u00b7re", "dies", "Herz"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich geb\u2019 es besiegt dir zu eigen!", "tokens": ["Ich", "geb'", "es", "be\u00b7siegt", "dir", "zu", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und erbarmungslos", "tokens": ["Und", "er\u00b7bar\u00b7mungs\u00b7los"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Entl\u00f6s\u2019 deinem Schoo\u00df", "tokens": ["Ent\u00b7l\u00f6s'", "dei\u00b7nem", "Schoo\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Der Qualen nachtlockigen Reigen!", "tokens": ["Der", "Qua\u00b7len", "nacht\u00b7lo\u00b7cki\u00b7gen", "Rei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Zieh\u2019 ein, o Schmerz,", "tokens": ["Zieh'", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und heil\u2019ge dies Herz \u2014", "tokens": ["Und", "heil'\u00b7ge", "dies", "Herz"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Furch\u2019 deine Flammenspuren!", "tokens": ["Furch'", "dei\u00b7ne", "Flam\u00b7men\u00b7spu\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was morsch ist, zerbrich,", "tokens": ["Was", "morsch", "ist", ",", "zer\u00b7brich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bis das Gemeine entwich,", "tokens": ["Bis", "das", "Ge\u00b7mei\u00b7ne", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Und die Flitter von dannen fuhren!", "tokens": ["Und", "die", "Flit\u00b7ter", "von", "dan\u00b7nen", "fuh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Zieh\u2019 ein, o Schmerz,", "tokens": ["Zieh'", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und pflanze in\u2019s Herz", "tokens": ["Und", "pflan\u00b7ze", "in's", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Weltenr&#228;thsel Erkenntni\u00df!               ", "tokens": ["Der", "Wel\u00b7tenr", "&#228;", "th\u00b7sel", "Er\u00b7kennt\u00b7ni\u00df", "!"], "token_info": ["word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was gesucht ich so lang\u2019", "tokens": ["Was", "ge\u00b7sucht", "ich", "so", "lang'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In gl\u00fchendem Drang,", "tokens": ["In", "gl\u00fc\u00b7hen\u00b7dem", "Drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Entschlei\u2019re in ernstem Gest\u00e4ndni\u00df!", "tokens": ["Ent\u00b7schlei'\u00b7re", "in", "erns\u00b7tem", "Ge\u00b7st\u00e4nd\u00b7ni\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Zieh\u2019 ein, o Schmerz,", "tokens": ["Zieh'", "ein", ",", "o", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ents\u00fcnd\u2019ge dies Herz \u2014", "tokens": ["Ent\u00b7s\u00fcn\u00b7d'\u00b7ge", "dies", "Herz"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "$("], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Ich geb\u2019 es besiegt dir zu eigen! \u2014", "tokens": ["Ich", "geb'", "es", "be\u00b7siegt", "dir", "zu", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "PTKA", "ADJD", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bis in flammender Pracht", "tokens": ["Bis", "in", "flam\u00b7men\u00b7der", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Aus Schl\u00fcnden der Nacht", "tokens": ["Aus", "Schl\u00fcn\u00b7den", "der", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Der Erl\u00f6sung Sonne wird steigen!", "tokens": ["Der", "Er\u00b7l\u00f6\u00b7sung", "Son\u00b7ne", "wird", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}