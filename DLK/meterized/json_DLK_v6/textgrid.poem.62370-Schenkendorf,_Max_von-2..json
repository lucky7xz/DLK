{"textgrid.poem.62370": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O k\u00f6nnt' ich zu dir fliegen,", "tokens": ["O", "k\u00f6nnt'", "ich", "zu", "dir", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein V\u00f6gelein, in Eil,", "tokens": ["Ein", "V\u00f6\u00b7ge\u00b7lein", ",", "in", "Eil", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An deine Brust mich schmiegen,", "tokens": ["An", "dei\u00b7ne", "Brust", "mich", "schmie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da tr\u00e4fe mich kein Pfeil.", "tokens": ["Da", "tr\u00e4\u00b7fe", "mich", "kein", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O g\u00e4lt es nur zu schwimmen", "tokens": ["O", "g\u00e4lt", "es", "nur", "zu", "schwim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch wilde, weite See,", "tokens": ["Durch", "wil\u00b7de", ",", "wei\u00b7te", "See", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Oder hinan zu klimmen,", "tokens": ["O\u00b7der", "hi\u00b7nan", "zu", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Die steilste Felsenh\u00f6h!", "tokens": ["Die", "steils\u00b7te", "Fel\u00b7sen\u00b7h\u00f6h", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das w\u00e4re wol ein Leichtes", "tokens": ["Das", "w\u00e4\u00b7re", "wol", "ein", "Leich\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um solch ein Himmelsgut.", "tokens": ["Um", "solch", "ein", "Him\u00b7mels\u00b7gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein kein Blick erreicht es,", "tokens": ["Al\u00b7lein", "kein", "Blick", "er\u00b7reicht", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein W\u00fcnschen und kein Muth.", "tokens": ["Kein", "W\u00fcn\u00b7schen", "und", "kein", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch mu\u00df ich stets mich wenden", "tokens": ["Doch", "mu\u00df", "ich", "stets", "mich", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu deiner Gegend hin,", "tokens": ["Zu", "dei\u00b7ner", "Ge\u00b7gend", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und immer Gr\u00fc\u00dfe senden", "tokens": ["Und", "im\u00b7mer", "Gr\u00fc\u00b7\u00dfe", "sen\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll treuem Liebessinn.", "tokens": ["Voll", "treu\u00b7em", "Lie\u00b7bes\u00b7sinn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}