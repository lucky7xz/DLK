{"textgrid.poem.47806": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Im Herbste 1850", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und schauen auch von Turm und Tore", "tokens": ["Und", "schau\u00b7en", "auch", "von", "Turm", "und", "To\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Feinde Wappen jetzt herab,", "tokens": ["Der", "Fein\u00b7de", "Wap\u00b7pen", "jetzt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rissen sie die Trikolore", "tokens": ["Und", "ris\u00b7sen", "sie", "die", "Tri\u00b7ko\u00b7lo\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit w\u00fcster Faust von Kreuz und Grab;", "tokens": ["Mit", "w\u00fcs\u00b7ter", "Faust", "von", "Kreuz", "und", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und m\u00fc\u00dften wir nach diesen Tagen", "tokens": ["Und", "m\u00fc\u00df\u00b7ten", "wir", "nach", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Herd und Heimat bettelnd gehn \u2013", "tokens": ["Von", "Herd", "und", "Hei\u00b7mat", "bet\u00b7telnd", "gehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wollen's nicht zu laut beklagen;", "tokens": ["Wir", "wol\u00b7len's", "nicht", "zu", "laut", "be\u00b7kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PTKA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mag, was da mu\u00df, mit uns geschehn!", "tokens": ["Mag", ",", "was", "da", "mu\u00df", ",", "mit", "uns", "ge\u00b7schehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PRELS", "ADV", "VMFIN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wenn wir h\u00fclfelos verderben,", "tokens": ["Und", "wenn", "wir", "h\u00fcl\u00b7fe\u00b7los", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo keiner unsre Schmerzen kennt,", "tokens": ["Wo", "kei\u00b7ner", "uns\u00b7re", "Schmer\u00b7zen", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir lassen unsern sp\u00e4tsten Erben", "tokens": ["Wir", "las\u00b7sen", "un\u00b7sern", "sp\u00e4ts\u00b7ten", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein treu besiegelt Testament;", "tokens": ["Ein", "treu", "be\u00b7sie\u00b7gelt", "Tes\u00b7ta\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn kommen wird das frische Werde,", "tokens": ["Denn", "kom\u00b7men", "wird", "das", "fri\u00b7sche", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das auch bei uns die Nacht besiegt,", "tokens": ["Das", "auch", "bei", "uns", "die", "Nacht", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tag, wo diese deutsche Erde", "tokens": ["Der", "Tag", ",", "wo", "die\u00b7se", "deut\u00b7sche", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Ring des gro\u00dfen Reiches liegt.", "tokens": ["Im", "Ring", "des", "gro\u00b7\u00dfen", "Rei\u00b7ches", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Wehe nur und eine Schande", "tokens": ["Ein", "We\u00b7he", "nur", "und", "ei\u00b7ne", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird bleiben, wenn die Nacht verschwand:", "tokens": ["Wird", "blei\u00b7ben", ",", "wenn", "die", "Nacht", "ver\u00b7schwand", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df in dem eignen Heimatlande", "tokens": ["Da\u00df", "in", "dem", "eig\u00b7nen", "Hei\u00b7mat\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Feind die Bundeshelfer fand;", "tokens": ["Der", "Feind", "die", "Bun\u00b7des\u00b7hel\u00b7fer", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df uns von unsern eignen Br\u00fcdern", "tokens": ["Da\u00df", "uns", "von", "un\u00b7sern", "eig\u00b7nen", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der bittre Sto\u00df zum Herzen drang,", "tokens": ["Der", "bitt\u00b7re", "Sto\u00df", "zum", "Her\u00b7zen", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die einst mit deutschen Wiegenliedern", "tokens": ["Die", "einst", "mit", "deut\u00b7schen", "Wie\u00b7gen\u00b7lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter in den Schlummer sang;", "tokens": ["Die", "Mut\u00b7ter", "in", "den", "Schlum\u00b7mer", "sang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die einst von deutscher Frauen Munde", "tokens": ["Die", "einst", "von", "deut\u00b7scher", "Frau\u00b7en", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Liebe holden Laut getauscht,", "tokens": ["Der", "Lie\u00b7be", "hol\u00b7den", "Laut", "ge\u00b7tauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die in des Vaters Sterbestunde", "tokens": ["Die", "in", "des", "Va\u00b7ters", "Ster\u00b7be\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Schmerz auf deutsches Wort gelauscht.", "tokens": ["Mit", "Schmerz", "auf", "deut\u00b7sches", "Wort", "ge\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht viele sind's und leicht zu kennen \u2013", "tokens": ["Nicht", "vie\u00b7le", "sin\u00b7d's", "und", "leicht", "zu", "ken\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "KON", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "O haltet ein! Ihr d\u00fcrft sie nicht", "tokens": ["O", "hal\u00b7tet", "ein", "!", "Ihr", "d\u00fcrft", "sie", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Mitleid noch im Zorne nennen,", "tokens": ["In", "Mit\u00b7leid", "noch", "im", "Zor\u00b7ne", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht in Geschichte noch Gedicht.", "tokens": ["Nicht", "in", "Ge\u00b7schich\u00b7te", "noch", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "La\u00dft sie, wenn frei die Herzen klopfen,", "tokens": ["La\u00dft", "sie", ",", "wenn", "frei", "die", "Her\u00b7zen", "klop\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergessen und verschollen sein,", "tokens": ["Ver\u00b7ges\u00b7sen", "und", "ver\u00b7schol\u00b7len", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mischet nicht die Wermutstropfen", "tokens": ["Und", "mi\u00b7schet", "nicht", "die", "Wer\u00b7muts\u00b7trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In den bekr\u00e4nzten deutschen Wein!", "tokens": ["In", "den", "be\u00b7kr\u00e4nz\u00b7ten", "deut\u00b7schen", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}