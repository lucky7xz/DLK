{"textgrid.poem.24997": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Kuckuck", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stimme, die im Fr\u00fchlingswinde", "tokens": ["Stim\u00b7me", ",", "die", "im", "Fr\u00fch\u00b7lings\u00b7win\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fernher durch das Laubgr\u00fcn hallt,", "tokens": ["Fern\u00b7her", "durch", "das", "Laub\u00b7gr\u00fcn", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6nt dein Ruf, wie einst dem Kinde,", "tokens": ["T\u00f6nt", "dein", "Ruf", ",", "wie", "einst", "dem", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu mir aus dem Buchenwald?", "tokens": ["Neu", "mir", "aus", "dem", "Bu\u00b7chen\u00b7wald", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jahre, mehr als du dem Knaben,", "tokens": ["Jah\u00b7re", ",", "mehr", "als", "du", "dem", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Muntrer Vogel, prophezeit,", "tokens": ["Mun\u00b7trer", "Vo\u00b7gel", ",", "pro\u00b7phe\u00b7zeit", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind seitdem verrollt; begraben", "tokens": ["Sind", "seit\u00b7dem", "ver\u00b7rollt", ";", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PAV", "VVPP", "$.", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Liegt die goldne Jugendzeit.", "tokens": ["Liegt", "die", "gold\u00b7ne", "Ju\u00b7gend\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hin die erste zauberische", "tokens": ["Hin", "die", "ers\u00b7te", "zau\u00b7be\u00b7ri\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmerhelle vor dem Tag,", "tokens": ["D\u00e4m\u00b7mer\u00b7hel\u00b7le", "vor", "dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als der Tau in Morgenfrische", "tokens": ["Als", "der", "Tau", "in", "Mor\u00b7gen\u00b7fri\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf des Lebens Bl\u00fcten lag,", "tokens": ["Auf", "des", "Le\u00b7bens", "Bl\u00fc\u00b7ten", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hin der Rausch, als himmelw\u00e4rts mir", "tokens": ["Hin", "der", "Rausch", ",", "als", "him\u00b7mel\u00b7w\u00e4rts", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "ADV", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In der Jugend erstem Stolz", "tokens": ["In", "der", "Ju\u00b7gend", "ers\u00b7tem", "Stolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich die Seele hob, das Herz mir", "tokens": ["Sich", "die", "See\u00b7le", "hob", ",", "das", "Herz", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An geliebten Blicken schmolz!", "tokens": ["An", "ge\u00b7lieb\u00b7ten", "Bli\u00b7cken", "schmolz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du indes, Unsterblich-Froher,", "tokens": ["Du", "in\u00b7des", ",", "Un\u00b7sterb\u00b7lich\u00b7Fro\u00b7her", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Hast in deiner Waldeslust", "tokens": ["Hast", "in", "dei\u00b7ner", "Wal\u00b7des\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts von Trauer, nichts von hoher", "tokens": ["Nichts", "von", "Trau\u00b7er", ",", "nichts", "von", "ho\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "APPR", "NN", "$,", "PIS", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnungen Verbl\u00fchn gewu\u00dft.", "tokens": ["Hoff\u00b7nun\u00b7gen", "Ver\u00b7bl\u00fchn", "ge\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Neu dir keimt, wenn es gefallen,", "tokens": ["Neu", "dir", "keimt", ",", "wenn", "es", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mai f\u00fcr Mai das Laub empor,", "tokens": ["Mai", "f\u00fcr", "Mai", "das", "Laub", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch gr\u00fcne Bl\u00e4tterhallen", "tokens": ["Und", "durch", "gr\u00fc\u00b7ne", "Bl\u00e4t\u00b7ter\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweifst du fr\u00f6hlich wie zuvor.", "tokens": ["Schweifst", "du", "fr\u00f6h\u00b7lich", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Juble fort in deinen Hainen,", "tokens": ["Jub\u00b7le", "fort", "in", "dei\u00b7nen", "Hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00e4hrend, nie mehr zu erstehn,", "tokens": ["W\u00e4h\u00b7rend", ",", "nie", "mehr", "zu", "er\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unser Gl\u00fcck und unsre kleinen", "tokens": ["Un\u00b7ser", "Gl\u00fcck", "und", "uns\u00b7re", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leben in den Wind verwehn!", "tokens": ["Le\u00b7ben", "in", "den", "Wind", "ver\u00b7wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}