{"textgrid.poem.33810": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Lose der Dauer", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Gletschereis wird kein Atom verwesen,", "tokens": ["Im", "Glet\u00b7sche\u00b7reis", "wird", "kein", "A\u00b7tom", "ver\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im d\u00fcrren Sand bleibt unversehrt die Leiche;", "tokens": ["Im", "d\u00fcr\u00b7ren", "Sand", "bleibt", "un\u00b7ver\u00b7sehrt", "die", "Lei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der taube Stein bleibt ewig sich der gleiche,", "tokens": ["Der", "tau\u00b7be", "Stein", "bleibt", "e\u00b7wig", "sich", "der", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PRF", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nur die Bl\u00fcte wird vom Tod gelesen.", "tokens": ["Und", "nur", "die", "Bl\u00fc\u00b7te", "wird", "vom", "Tod", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Griechenland ist fl\u00fccht'ger Traum gewesen,", "tokens": ["Ein", "Grie\u00b7chen\u00b7land", "ist", "fl\u00fccht'\u00b7ger", "Traum", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum sch\u00f6nsten Glauben sprach die Zeit: Erbleiche!", "tokens": ["Zum", "sch\u00f6ns\u00b7ten", "Glau\u00b7ben", "sprach", "die", "Zeit", ":", "Er\u00b7blei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wandellos aus Tr\u00fcmmern gr\u00f6\u00dfrer Reiche", "tokens": ["Doch", "wan\u00b7del\u00b7los", "aus", "Tr\u00fcm\u00b7mern", "gr\u00f6\u00df\u00b7rer", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "ADJA", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Starrt jene Mumienherrschaft der Sinesen.", "tokens": ["Starrt", "je\u00b7ne", "Mu\u00b7mi\u00b7en\u00b7herr\u00b7schaft", "der", "Si\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein Nachen schwankt, wo Flotten einst gelandet;", "tokens": ["Ein", "Na\u00b7chen", "schwankt", ",", "wo", "Flot\u00b7ten", "einst", "ge\u00b7lan\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Bett vergi\u00dft der Strom, die Spur vom Zuge", "tokens": ["Sein", "Bett", "ver\u00b7gi\u00dft", "der", "Strom", ",", "die", "Spur", "vom", "Zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der fr\u00fchsten V\u00f6lkerheere liegt versandet.", "tokens": ["Der", "fr\u00fchs\u00b7ten", "V\u00f6l\u00b7ker\u00b7hee\u00b7re", "liegt", "ver\u00b7san\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nur Wind und Wolken stets im alten Fluge", "tokens": ["Nur", "Wind", "und", "Wol\u00b7ken", "stets", "im", "al\u00b7ten", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ziehn hin und her, und Flut und Ebbe brandet,", "tokens": ["Ziehn", "hin", "und", "her", ",", "und", "Flut", "und", "Eb\u00b7be", "bran\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nur der Wechsel kommt nicht aus der Fuge.", "tokens": ["Und", "nur", "der", "Wech\u00b7sel", "kommt", "nicht", "aus", "der", "Fu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}