{"textgrid.poem.42171": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Menschlichkeit", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch ist nackt geschaffen, ist nackt;", "tokens": ["Der", "Mensch", "ist", "nackt", "ge\u00b7schaf\u00b7fen", ",", "ist", "nackt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Daraus erkl\u00e4rt sich seine Vertracktheit.", "tokens": ["Da\u00b7raus", "er\u00b7kl\u00e4rt", "sich", "sei\u00b7ne", "Ver\u00b7trackt\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wird er vom Wind bei der Wolle gepackt,", "tokens": ["Wird", "er", "vom", "Wind", "bei", "der", "Wol\u00b7le", "ge\u00b7packt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann sch\u00e4mt er sich seiner kl\u00e4glichen Nacktheit.", "tokens": ["Dann", "sch\u00e4mt", "er", "sich", "sei\u00b7ner", "kl\u00e4g\u00b7li\u00b7chen", "Nackt\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Dort, wo es dem rohen P\u00f6bel graut,", "tokens": ["Dort", ",", "wo", "es", "dem", "ro\u00b7hen", "P\u00f6\u00b7bel", "graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sind der Seele zarteste Saiten zu finden;", "tokens": ["Sind", "der", "See\u00b7le", "zar\u00b7tes\u00b7te", "Sai\u00b7ten", "zu", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat einer gar eine durchschimmernde Haut,", "tokens": ["Hat", "ei\u00b7ner", "gar", "ei\u00b7ne", "durch\u00b7schim\u00b7mern\u00b7de", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du sollst ihn nicht z\u00fcchtigen, sondern ergr\u00fcnden.", "tokens": ["Du", "sollst", "ihn", "nicht", "z\u00fcch\u00b7ti\u00b7gen", ",", "son\u00b7dern", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Ist einer \u00fcber und \u00fcber behaart,", "tokens": ["Ist", "ei\u00b7ner", "\u00fc\u00b7ber", "und", "\u00fc\u00b7ber", "be\u00b7haart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPR", "KON", "APPR", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann magst du ihn nach Gefallen bewitzeln.", "tokens": ["Dann", "magst", "du", "ihn", "nach", "Ge\u00b7fal\u00b7len", "be\u00b7wit\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kitzliche zu kitzeln ist Knabenart;", "tokens": ["Kitz\u00b7li\u00b7che", "zu", "kit\u00b7zeln", "ist", "Kna\u00b7be\u00b7nart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "NN", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ein Mann liebt vielmehr den Kitzelnden zu kitzeln.", "tokens": ["Ein", "Mann", "liebt", "viel\u00b7mehr", "den", "Kit\u00b7zeln\u00b7den", "zu", "kit\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}