{"textgrid.poem.34325": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie freundlich tr\u00e4gst du mich auf deinem gr\u00fcnen R\u00fccken,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie freundlich tr\u00e4gst du mich auf deinem gr\u00fcnen R\u00fccken,", "tokens": ["Wie", "freund\u00b7lich", "tr\u00e4gst", "du", "mich", "auf", "dei\u00b7nem", "gr\u00fc\u00b7nen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uralter Rhein!", "tokens": ["Ur\u00b7al\u00b7ter", "Rhein", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie suchest du mein Aug' empfindlich zu erquicken", "tokens": ["Wie", "su\u00b7chest", "du", "mein", "Aug'", "emp\u00b7find\u00b7lich", "zu", "er\u00b7qui\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Ufer voller Wein!", "tokens": ["Durch", "U\u00b7fer", "vol\u00b7ler", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und hab ich doch die tausend Lustgestalten", "tokens": ["Und", "hab", "ich", "doch", "die", "tau\u00b7send", "Lust\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Tief im Ged\u00e4chtni\u00df zu behalten", "tokens": ["Tief", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "zu", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Nun weder Dinte noch Papier,", "tokens": ["Nun", "we\u00b7der", "Din\u00b7te", "noch", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nur dieses Herz, das dich empfindet, hier!", "tokens": ["Nur", "die\u00b7ses", "Herz", ",", "das", "dich", "emp\u00b7fin\u00b7det", ",", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es scheinet fast, du liebest, Allzugro\u00dfer,", "tokens": ["Es", "schei\u00b7net", "fast", ",", "du", "lie\u00b7best", ",", "All\u00b7zu\u00b7gro\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nicht mehr der Maler Prunk, der Dichter Klang,", "tokens": ["Nicht", "mehr", "der", "Ma\u00b7ler", "Prunk", ",", "der", "Dich\u00b7ter", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Es scheint, du willst wie Schlosser,", "tokens": ["Es", "scheint", ",", "du", "willst", "wie", "Schlos\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Nur stummen Dank.", "tokens": ["Nur", "stum\u00b7men", "Dank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}