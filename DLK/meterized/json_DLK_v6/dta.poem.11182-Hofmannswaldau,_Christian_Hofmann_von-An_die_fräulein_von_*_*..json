{"dta.poem.11182": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An die fr\u00e4ulein von * *.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6ne Helena! dein glantz ist ungemein,", "tokens": ["Du", "sch\u00f6\u00b7ne", "He\u00b7le\u00b7na", "!", "dein", "glantz", "ist", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wolte dir das hertz aus deiner brust entf\u00fchren;", "tokens": ["Ich", "wol\u00b7te", "dir", "das", "hertz", "aus", "dei\u00b7ner", "brust", "ent\u00b7f\u00fch\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df mein eigen hertz in flammen sich verliehren:", "tokens": ["So", "mu\u00df", "mein", "ei\u00b7gen", "hertz", "in", "flam\u00b7men", "sich", "ver\u00b7lieh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "NN", "APPR", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Troja kan ich wohl, nur nicht dein Paris seyn.", "tokens": ["Dein", "Tro\u00b7ja", "kan", "ich", "wohl", ",", "nur", "nicht", "dein", "Pa\u00b7ris", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "$,", "ADV", "PTKNEG", "PPOSAT", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}