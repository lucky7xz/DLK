{"textgrid.poem.63224": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist so sanft, durch diesen Herbst zu eilen", "tokens": ["Es", "ist", "so", "sanft", ",", "durch", "die\u00b7sen", "Herbst", "zu", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dieses Blau des Himmels zu betrachten,", "tokens": ["Und", "die\u00b7ses", "Blau", "des", "Him\u00b7mels", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bei spielerischen Kindern zu verweilen", "tokens": ["Bei", "spie\u00b7le\u00b7ri\u00b7schen", "Kin\u00b7dern", "zu", "ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf den guten Gang des Greises achten.", "tokens": ["Und", "auf", "den", "gu\u00b7ten", "Gang", "des", "Grei\u00b7ses", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Adler glitzert auf der Zitadelle.", "tokens": ["Ein", "Ad\u00b7ler", "glit\u00b7zert", "auf", "der", "Zi\u00b7ta\u00b7del\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Leoparde raschelt Bellinzona.", "tokens": ["Ein", "Le\u00b7o\u00b7par\u00b7de", "ra\u00b7schelt", "Bel\u00b7lin\u00b7zo\u00b7na", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf seinem gelben und gefleckten Felle", "tokens": ["Auf", "sei\u00b7nem", "gel\u00b7ben", "und", "ge\u00b7fleck\u00b7ten", "Fel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Reitet die sch\u00f6nste Frau der Welt: Ilona.", "tokens": ["Rei\u00b7tet", "die", "sch\u00f6ns\u00b7te", "Frau", "der", "Welt", ":", "I\u00b7lo\u00b7na", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$.", "NE", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Sie l\u00e4chelt. Und ich hebe meine H\u00e4nde.", "tokens": ["Sie", "l\u00e4\u00b7chelt", ".", "Und", "ich", "he\u00b7be", "mei\u00b7ne", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie winkt. Ich sinke seufzend vor ihr nieder.", "tokens": ["Sie", "winkt", ".", "Ich", "sin\u00b7ke", "seuf\u00b7zend", "vor", "ihr", "nie\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es scheint das ausgebreitete Gel\u00e4nde", "tokens": ["Es", "scheint", "das", "aus\u00b7ge\u00b7brei\u00b7te\u00b7te", "Ge\u00b7l\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um ihre Brust gespannt als goldnes Mieder.", "tokens": ["Um", "ih\u00b7re", "Brust", "ge\u00b7spannt", "als", "gold\u00b7nes", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVPP", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O lass die Landschaft von der H\u00fcfte fallen!", "tokens": ["O", "lass", "die", "Land\u00b7schaft", "von", "der", "H\u00fcf\u00b7te", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entferne doch den Himmel aus den Blicken!", "tokens": ["Ent\u00b7fer\u00b7ne", "doch", "den", "Him\u00b7mel", "aus", "den", "Bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sei ein Mensch! Die Abendglocken schallen.", "tokens": ["Und", "sei", "ein", "Mensch", "!", "Die", "A\u00b7bend\u00b7glo\u00b7cken", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du darfst begl\u00fcckt sein, Mensch, und darfst begl\u00fccken.", "tokens": ["Du", "darfst", "be\u00b7gl\u00fcckt", "sein", ",", "Mensch", ",", "und", "darfst", "be\u00b7gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "VVPP", "VAINF", "$,", "NN", "$,", "KON", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}