{"textgrid.poem.60588": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Horaz", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du, der s\u00fcsse T\u00f6ne", "tokens": ["O", "du", ",", "der", "s\u00fcs\u00b7se", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus neuen Saiten zwang,", "tokens": ["Aus", "neu\u00b7en", "Sai\u00b7ten", "zwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit der feurigsten Cam\u00f6ne,", "tokens": ["Und", "mit", "der", "feu\u00b7rigs\u00b7ten", "Ca\u00b7m\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die am Parnasse sang,", "tokens": ["Die", "am", "Par\u00b7nas\u00b7se", "sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich in des Ruhmes Tempel schwang.", "tokens": ["Sich", "in", "des", "Ruh\u00b7mes", "Tem\u00b7pel", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh auf drey Freunde nieder,", "tokens": ["Sieh", "auf", "drey", "Freun\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Flaccus! denn sie flehn:", "tokens": ["O", "Flac\u00b7cus", "!", "denn", "sie", "flehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie gl\u00fchn, die Muse deiner Lieder", "tokens": ["Sie", "gl\u00fchn", ",", "die", "Mu\u00b7se", "dei\u00b7ner", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrem Reiz zu sehn,", "tokens": ["In", "ih\u00b7rem", "Reiz", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den Scholiasten plump verschm\u00e4hn:", "tokens": ["Den", "Scho\u00b7li\u00b7as\u00b7ten", "plump", "ver\u00b7schm\u00e4hn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der mit geheimer Zierde", "tokens": ["Der", "mit", "ge\u00b7hei\u00b7mer", "Zier\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den feinern Geist vergn\u00fcgt,", "tokens": ["Den", "fei\u00b7nern", "Geist", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann sie, mit immer gleicher W\u00fcrde,", "tokens": ["Wann", "sie", ",", "mit", "im\u00b7mer", "glei\u00b7cher", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald unter Myrthen liegt,", "tokens": ["Bald", "un\u00b7ter", "Myr\u00b7then", "liegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und bald im schnellen Sturmwind fliegt.", "tokens": ["Und", "bald", "im", "schnel\u00b7len", "Sturm\u00b7wind", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie schift mit starkem Fl\u00fcgel", "tokens": ["Sie", "schift", "mit", "star\u00b7kem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In ungest\u00fcmer Luft,", "tokens": ["In", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Luft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin sie, vom geweihten H\u00fcgel", "tokens": ["Wo\u00b7hin", "sie", ",", "vom", "ge\u00b7weih\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und junger Bluhmen Duft,", "tokens": ["Und", "jun\u00b7ger", "Bluh\u00b7men", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Taumel der Begeistrung ruft.", "tokens": ["Ein", "Tau\u00b7mel", "der", "Be\u00b7geis\u00b7trung", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Erschein uns an dem Tage,", "tokens": ["Er\u00b7schein", "uns", "an", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der dir geheiligt ist!", "tokens": ["Der", "dir", "ge\u00b7hei\u00b7ligt", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df kein Unheiliger uns plage,", "tokens": ["Da\u00df", "kein", "Un\u00b7hei\u00b7li\u00b7ger", "uns", "pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der \u00fcber Preu\u00dfens Zwist", "tokens": ["Der", "\u00fc\u00b7ber", "Preu\u00b7\u00dfens", "Zwist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den ganzen Helikon vergi\u00dft!", "tokens": ["Den", "gan\u00b7zen", "He\u00b7li\u00b7kon", "ver\u00b7gi\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon h\u00f6r ich deiner Leyer", "tokens": ["Schon", "h\u00f6r", "ich", "dei\u00b7ner", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Alk\u00e4isch edlen Ton!", "tokens": ["Al\u00b7k\u00e4\u00b7isch", "ed\u00b7len", "Ton", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer brennt nicht selbst von deinem Feuer!", "tokens": ["Wer", "brennt", "nicht", "selbst", "von", "dei\u00b7nem", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gebt Wein! Zu lange schon", "tokens": ["Gebt", "Wein", "!", "Zu", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "NN", "$.", "APPR", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "S\u00e4umt Bacchus, der uns nie geflohn!", "tokens": ["S\u00e4umt", "Bac\u00b7chus", ",", "der", "uns", "nie", "ge\u00b7flohn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit jauchzendem Entz\u00fccken", "tokens": ["Mit", "jauch\u00b7zen\u00b7dem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eil, eil er schnell herbey,", "tokens": ["Eil", ",", "eil", "er", "schnell", "her\u00b7bey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Geists in feuervollen Blicken,", "tokens": ["Voll", "Geists", "in", "feu\u00b7er\u00b7vol\u00b7len", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll einer Raserey,", "tokens": ["Voll", "ei\u00b7ner", "Ra\u00b7se\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die keuscher Musen w\u00fcrdig sey!", "tokens": ["Die", "keu\u00b7scher", "Mu\u00b7sen", "w\u00fcr\u00b7dig", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht sch\u00f6ner sah Ly\u00e4en", "tokens": ["Nicht", "sch\u00f6\u00b7ner", "sah", "Ly\u00b7\u00e4\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein holder Aufenthalt,", "tokens": ["Dein", "hol\u00b7der", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf Tiburs wasserreichen H\u00f6hen", "tokens": ["Auf", "Ti\u00b7burs", "was\u00b7ser\u00b7rei\u00b7chen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo manch bejahrter Wald", "tokens": ["Wo", "manch", "be\u00b7jahr\u00b7ter", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von deinem Namen widerschallt.", "tokens": ["Von", "dei\u00b7nem", "Na\u00b7men", "wi\u00b7der\u00b7schallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Weg, die sich weise d\u00fcnken,", "tokens": ["Weg", ",", "die", "sich", "wei\u00b7se", "d\u00fcn\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In strenger Weisheit Tracht!", "tokens": ["In", "stren\u00b7ger", "Weis\u00b7heit", "Tracht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich, ich will mit Horazen trinken,", "tokens": ["Ich", ",", "ich", "will", "mit", "Ho\u00b7ra\u00b7zen", "trin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bis jeder Stern der Nacht", "tokens": ["Bis", "je\u00b7der", "Stern", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An seinem Orte funkelnd wacht.", "tokens": ["An", "sei\u00b7nem", "Or\u00b7te", "fun\u00b7kelnd", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}