{"textgrid.poem.65656": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Suliotin", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' die Spindel lang' gedreht, hab' manche Winternacht", "tokens": ["Ich", "hab'", "die", "Spin\u00b7del", "lang'", "ge\u00b7dreht", ",", "hab'", "man\u00b7che", "Win\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$,", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Gewebt am Stuhl, und froh dabei an's neue Kleid gedacht.", "tokens": ["Ge\u00b7webt", "am", "Stuhl", ",", "und", "froh", "da\u00b7bei", "an's", "neu\u00b7e", "Kleid", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "KON", "ADJD", "PAV", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ich hab' die Heerden auf den H\u00f6hn geh\u00fctet manchen Tag,", "tokens": ["Ich", "hab'", "die", "Heer\u00b7den", "auf", "den", "H\u00f6hn", "ge\u00b7h\u00fc\u00b7tet", "man\u00b7chen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und bin geklettert ohne Noth den jungen Ziegen nach;", "tokens": ["Und", "bin", "ge\u00b7klet\u00b7tert", "oh\u00b7ne", "Noth", "den", "jun\u00b7gen", "Zie\u00b7gen", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ich habe meinen Kleinen auch manch Kinderspiel gezeigt,", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Klei\u00b7nen", "auch", "manch", "Kin\u00b7der\u00b7spiel", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Und Sprung und Lauf und Schu\u00df und Wurf ward mir mit ihnen leicht.", "tokens": ["Und", "Sprung", "und", "Lauf", "und", "Schu\u00df", "und", "Wurf", "ward", "mir", "mit", "ih\u00b7nen", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "VAFIN", "PPER", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Jetzt schleif' ich einen Stahl f\u00fcr mich und drehe Sennen mir \u2013", "tokens": ["Jetzt", "schleif'", "ich", "ei\u00b7nen", "Stahl", "f\u00fcr", "mich", "und", "dre\u00b7he", "Sen\u00b7nen", "mir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPER", "KON", "ADJA", "NN", "PPER", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Mein Herr, mein Hort, mein Herz, o nimm mich in den Kampf mit dir!", "tokens": ["Mein", "Herr", ",", "mein", "Hort", ",", "mein", "Herz", ",", "o", "nimm", "mich", "in", "den", "Kampf", "mit", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "FM", "VVIMP", "PPER", "APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Ich kenne jeden Felsenpfad auf Suli's steilen H\u00f6hn,", "tokens": ["Ich", "ken\u00b7ne", "je\u00b7den", "Fel\u00b7sen\u00b7pfad", "auf", "Su\u00b7li's", "stei\u00b7len", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Und wo die flinke Gemse zagt, da kann ich sicher stehn.", "tokens": ["Und", "wo", "die", "flin\u00b7ke", "Gem\u00b7se", "zagt", ",", "da", "kann", "ich", "si\u00b7cher", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Hast du noch nicht gesehn, was ich vermag im Sprung und Lauf,", "tokens": ["Hast", "du", "noch", "nicht", "ge\u00b7sehn", ",", "was", "ich", "ver\u00b7mag", "im", "Sprung", "und", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,", "PWS", "PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Wohlan, so gieb ein Probest\u00fcck mir mit den M\u00e4nnern auf!", "tokens": ["Wo\u00b7hlan", ",", "so", "gieb", "ein", "Pro\u00b7be\u00b7st\u00fcck", "mir", "mit", "den", "M\u00e4n\u00b7nern", "auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVIMP", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Und eine Klippe zeige mir auf Suli weit und breit,", "tokens": ["Und", "ei\u00b7ne", "Klip\u00b7pe", "zei\u00b7ge", "mir", "auf", "Su\u00b7li", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Die ich dir nicht erklettern kann zu aller Frauen Neid.", "tokens": ["Die", "ich", "dir", "nicht", "er\u00b7klet\u00b7tern", "kann", "zu", "al\u00b7ler", "Frau\u00b7en", "Neid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.15": {"text": "Den Vogel treff' ich in der Lust, wo's gilt nur einen Scherz \u2013", "tokens": ["Den", "Vo\u00b7gel", "treff'", "ich", "in", "der", "Lust", ",", "wo's", "gilt", "nur", "ei\u00b7nen", "Scherz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PWAV", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Meinst du, verfehlen k\u00f6nnt' ich ja des gro\u00dfen Feindes Herz?", "tokens": ["Meinst", "du", ",", "ver\u00b7feh\u00b7len", "k\u00f6nnt'", "ich", "ja", "des", "gro\u00b7\u00dfen", "Fein\u00b7des", "Herz", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVINF", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "Mein Herr, mein Hort, mein Herz, o nimm mich in den Kampf mit dir!", "tokens": ["Mein", "Herr", ",", "mein", "Hort", ",", "mein", "Herz", ",", "o", "nimm", "mich", "in", "den", "Kampf", "mit", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "FM", "VVIMP", "PPER", "APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "Mein T\u00f6chterchen kann spinnen schon. \u2013 Was sitz' ich l\u00e4nger hier?", "tokens": ["Mein", "T\u00f6ch\u00b7ter\u00b7chen", "kann", "spin\u00b7nen", "schon", ".", "\u2013", "Was", "sitz'", "ich", "l\u00e4n\u00b7ger", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVFIN", "ADV", "$.", "$(", "PWS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.19": {"text": "Mein j\u00fcngster Knabe steht allein. \u2013 Was ist mein Arm ihm werth?", "tokens": ["Mein", "j\u00fcngs\u00b7ter", "Kna\u00b7be", "steht", "al\u00b7lein", ".", "\u2013", "Was", "ist", "mein", "Arm", "ihm", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "$.", "$(", "PWS", "VAFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "Mein \u00e4ltester geht auf die Jagd. \u2013 Was sorg' ich f\u00fcr den Herd?", "tokens": ["Mein", "\u00e4l\u00b7tes\u00b7ter", "geht", "auf", "die", "Jagd", ".", "\u2013", "Was", "sor\u00b7g'", "ich", "f\u00fcr", "den", "Herd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.21": {"text": "Mit dir, mit dir will ich ins Feld! da hab' ich meinen Stand,", "tokens": ["Mit", "dir", ",", "mit", "dir", "will", "ich", "ins", "Feld", "!", "da", "hab'", "ich", "mei\u00b7nen", "Stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VMFIN", "PPER", "APPRART", "NN", "$.", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "Bei dir, bei dir, da, Brust an Brust, da, Liebster, Hand in Hand!", "tokens": ["Bei", "dir", ",", "bei", "dir", ",", "da", ",", "Brust", "an", "Brust", ",", "da", ",", "Liebs\u00b7ter", ",", "Hand", "in", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "KOUS", "$,", "NN", "APPR", "NN", "$,", "KOUS", "$,", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "Und sollt' ich fallen, sieh nicht hin, und denke nicht an mich,", "tokens": ["Und", "sollt'", "ich", "fal\u00b7len", ",", "sieh", "nicht", "hin", ",", "und", "den\u00b7ke", "nicht", "an", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "VVIMP", "PTKNEG", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Denk an den Feind, denk an den Kampf, und denke, Herz an dich,", "tokens": ["Denk", "an", "den", "Feind", ",", "denk", "an", "den", "Kampf", ",", "und", "den\u00b7ke", ",", "Herz", "an", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "$,", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.25": {"text": "An unsre Kinder, an dein Haus, an Suli's heil'ge H\u00f6hn,", "tokens": ["An", "uns\u00b7re", "Kin\u00b7der", ",", "an", "dein", "Haus", ",", "an", "Su\u00b7li's", "heil'\u00b7ge", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "An unsres Gottes Tempel, die auf ihren Gipfeln stehn,", "tokens": ["An", "uns\u00b7res", "Got\u00b7tes", "Tem\u00b7pel", ",", "die", "auf", "ih\u00b7ren", "Gip\u00b7feln", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.27": {"text": "An deiner Heldenv\u00e4ter Staub, und dann an eine Gruft", "tokens": ["An", "dei\u00b7ner", "Hel\u00b7den\u00b7v\u00e4\u00b7ter", "Staub", ",", "und", "dann", "an", "ei\u00b7ne", "Gruft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.28": {"text": "F\u00fcr mich, f\u00fcr dich, in freier Erd' und unter freier Luft!", "tokens": ["F\u00fcr", "mich", ",", "f\u00fcr", "dich", ",", "in", "frei\u00b7er", "Erd'", "und", "un\u00b7ter", "frei\u00b7er", "Luft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}