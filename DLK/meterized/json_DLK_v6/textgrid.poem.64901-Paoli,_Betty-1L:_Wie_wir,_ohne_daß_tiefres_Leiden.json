{"textgrid.poem.64901": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wir, ohne da\u00df tiefres Leiden", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wir, ohne da\u00df tiefres Leiden", "tokens": ["Wie", "wir", ",", "oh\u00b7ne", "da\u00df", "tief\u00b7res", "Lei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUI", "KOUS", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit seinem Stachel uns zerfleischt,", "tokens": ["Mit", "sei\u00b7nem", "Sta\u00b7chel", "uns", "zer\u00b7fleischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erkaltet von dem Freunde scheiden,", "tokens": ["Er\u00b7kal\u00b7tet", "von", "dem", "Freun\u00b7de", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der feig und niedrig uns get\u00e4uscht:", "tokens": ["Der", "feig", "und", "nied\u00b7rig", "uns", "ge\u00b7t\u00e4uscht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So scheid' ich ohne Gram noch Beben,", "tokens": ["So", "scheid'", "ich", "oh\u00b7ne", "Gram", "noch", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne Zorn und ohne Ach,", "tokens": ["Und", "oh\u00b7ne", "Zorn", "und", "oh\u00b7ne", "Ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kalt und verachtend von dem Leben;", "tokens": ["Kalt", "und", "ver\u00b7ach\u00b7tend", "von", "dem", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Es hielt mir nicht, was es versprach.", "tokens": ["Es", "hielt", "mir", "nicht", ",", "was", "es", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}