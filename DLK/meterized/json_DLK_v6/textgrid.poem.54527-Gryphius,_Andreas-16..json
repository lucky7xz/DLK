{"textgrid.poem.54527": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach grosser Gott! Ach Trost der gantzen Welt/", "tokens": ["Ach", "gros\u00b7ser", "Gott", "!", "Ach", "Trost", "der", "gant\u00b7zen", "Welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$.", "ITJ", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der in uns wirckt/ und Abba ruffen lehret/", "tokens": ["Der", "in", "uns", "wirckt", "/", "und", "Ab\u00b7ba", "ruf\u00b7fen", "leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$(", "KON", "NE", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch den uns Gott unendlich seufftzen h\u00f6ret/", "tokens": ["Durch", "den", "uns", "Gott", "un\u00b7end\u00b7lich", "seufft\u00b7zen", "h\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "NN", "ADJD", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach tr\u00f6ste/ wenn der Menschen Trost verf\u00e4llt.", "tokens": ["Ach", "tr\u00f6s\u00b7te", "/", "wenn", "der", "Men\u00b7schen", "Trost", "ver\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$(", "KOUS", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erquicke mich wenn meiner Glieder Zelt", "tokens": ["Er\u00b7qui\u00b7cke", "mich", "wenn", "mei\u00b7ner", "Glie\u00b7der", "Zelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Entspannet wird; indem sich Hitze mehret/", "tokens": ["Ent\u00b7span\u00b7net", "wird", ";", "in\u00b7dem", "sich", "Hit\u00b7ze", "meh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$.", "KOUS", "PRF", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und Todes-Angst den matten Geist versehret/", "tokens": ["Und", "To\u00b7des\u00b7Angst", "den", "mat\u00b7ten", "Geist", "ver\u00b7seh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Ewigkeit uns auf die Wage stellt.", "tokens": ["Und", "E\u00b7wig\u00b7keit", "uns", "auf", "die", "Wa\u00b7ge", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ach zeige mir/ da\u00df Gott mich vor sein Kind gesch\u00e4tzet/", "tokens": ["Ach", "zei\u00b7ge", "mir", "/", "da\u00df", "Gott", "mich", "vor", "sein", "Kind", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$(", "KOUS", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df Christus unser Fleisch auf diesen Thron versetzet/", "tokens": ["Da\u00df", "Chris\u00b7tus", "un\u00b7ser", "Fleisch", "auf", "die\u00b7sen", "Thron", "ver\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von dem er \u00fcber Tod und Leben Urtheil hegt/", "tokens": ["Von", "dem", "er", "\u00fc\u00b7ber", "Tod", "und", "Le\u00b7ben", "Ur\u00b7theil", "hegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "NN", "KON", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach Herr/ damit ich nichts mit dieser Welt verliehre/", "tokens": ["Ach", "Herr", "/", "da\u00b7mit", "ich", "nichts", "mit", "die\u00b7ser", "Welt", "ver\u00b7lieh\u00b7re", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "KOUS", "PPER", "PIS", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df mich ein Augenblick in stete Wollust f\u00fchre/", "tokens": ["Da\u00df", "mich", "ein", "Au\u00b7gen\u00b7blick", "in", "ste\u00b7te", "Wol\u00b7lust", "f\u00fch\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gib da\u00df ich sey bereit/ wenn meine Stunde schl\u00e4gt.", "tokens": ["Gib", "da\u00df", "ich", "sey", "be\u00b7reit", "/", "wenn", "mei\u00b7ne", "Stun\u00b7de", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "VAFIN", "ADJD", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}