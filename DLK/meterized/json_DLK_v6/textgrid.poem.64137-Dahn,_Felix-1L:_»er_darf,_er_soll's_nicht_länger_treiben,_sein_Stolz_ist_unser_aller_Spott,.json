{"textgrid.poem.64137": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bber darf, er soll's nicht l\u00e4nger treiben, sein Stolz ist unser aller Spott,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bber darf, er soll's nicht l\u00e4nger treiben, sein Stolz ist unser aller Spott,", "tokens": ["\u00bb", "er", "darf", ",", "er", "soll's", "nicht", "l\u00e4n\u00b7ger", "trei\u00b7ben", ",", "sein", "Stolz", "ist", "un\u00b7ser", "al\u00b7ler", "Spott", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Er soll nicht mehr im Lande bleiben, der durch uns hingeht wie ein Gott.", "tokens": ["Er", "soll", "nicht", "mehr", "im", "Lan\u00b7de", "blei\u00b7ben", ",", "der", "durch", "uns", "hin\u00b7geht", "wie", "ein", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$,", "PRELS", "APPR", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+---+--+", "measure": "iambic.septa.chol"}}, "stanza.2": {"line.1": {"text": "Er lacht beim Ruf der M\u00fcnsterglocken, tr\u00e4gt Tag und Nacht sein breites Schwert,", "tokens": ["Er", "lacht", "beim", "Ruf", "der", "M\u00fcns\u00b7ter\u00b7glo\u00b7cken", ",", "tr\u00e4gt", "Tag", "und", "Nacht", "sein", "brei\u00b7tes", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und trotzig sch\u00fcttelt er die Locken, wenn man ihn unsere Sitte lehrt.", "tokens": ["Und", "trot\u00b7zig", "sch\u00fct\u00b7telt", "er", "die", "Lo\u00b7cken", ",", "wenn", "man", "ihn", "un\u00b7se\u00b7re", "Sit\u00b7te", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PIS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+--+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit fremden Weisen, k\u00fchn und wilde, bezwang er unsrer Skalden Kunst: \u2013", "tokens": ["Mit", "frem\u00b7den", "Wei\u00b7sen", ",", "k\u00fchn", "und", "wil\u00b7de", ",", "be\u00b7zwang", "er", "uns\u00b7rer", "Skal\u00b7den", "Kunst", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJA", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Verbann' ihn, K\u00f6nigin Gunilde, nicht l\u00e4nger schirm' ihn deine Gunst.", "tokens": ["Ver\u00b7bann'", "ihn", ",", "K\u00f6\u00b7ni\u00b7gin", "Gu\u00b7nil\u00b7de", ",", "nicht", "l\u00e4n\u00b7ger", "schirm'", "ihn", "dei\u00b7ne", "Gunst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "NE", "$,", "PTKNEG", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Er kam, ein Fl\u00fcchtling, sturmverschlagen, ans Land und niemand wei\u00df woher:", "tokens": ["Er", "kam", ",", "ein", "Fl\u00fccht\u00b7ling", ",", "sturm\u00b7ver\u00b7schla\u00b7gen", ",", "ans", "Land", "und", "nie\u00b7mand", "wei\u00df", "wo\u00b7her", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "VVFIN", "$,", "APPRART", "NN", "KON", "PIS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die Welle soll ihn wieder tragen, den Wilden, in das wilde Meer.\u00ab", "tokens": ["Die", "Wel\u00b7le", "soll", "ihn", "wie\u00b7der", "tra\u00b7gen", ",", "den", "Wil\u00b7den", ",", "in", "das", "wil\u00b7de", "Meer", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Vom Drachenhelm bis auf die Sohlen stand er geh\u00fcllt in schwarzes Erz:", "tokens": ["Vom", "Dra\u00b7chen\u00b7helm", "bis", "auf", "die", "Soh\u00b7len", "stand", "er", "ge\u00b7h\u00fcllt", "in", "schwar\u00b7zes", "Erz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Er schwieg: nur manchmal flog verstohlen sein Blick durchs Fenster k\u00fcstenw\u00e4rts.", "tokens": ["Er", "schwieg", ":", "nur", "manch\u00b7mal", "flog", "ver\u00b7stoh\u00b7len", "sein", "Blick", "durchs", "Fens\u00b7ter", "k\u00fcs\u00b7ten\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "VVFIN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Er stand zun\u00e4chst an ihrem Throne, gest\u00fctzt auf seinen hohen Schild;", "tokens": ["Er", "stand", "zu\u00b7n\u00e4chst", "an", "ih\u00b7rem", "Thro\u00b7ne", ",", "ge\u00b7st\u00fctzt", "auf", "sei\u00b7nen", "ho\u00b7hen", "Schild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Sie l\u00e4chelt unter ihrer Krone und dr\u00e4ut ihm mit dem Finger mild:", "tokens": ["Sie", "l\u00e4\u00b7chelt", "un\u00b7ter", "ih\u00b7rer", "Kro\u00b7ne", "und", "dr\u00e4ut", "ihm", "mit", "dem", "Fin\u00b7ger", "mild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbihr h\u00f6rt, wie schwer sie Euch verklagen: wie wollt Ihr Euch verteid'gen? Sprecht.\u00ab", "tokens": ["\u00bb", "ihr", "h\u00f6rt", ",", "wie", "schwer", "sie", "Euch", "ver\u00b7kla\u00b7gen", ":", "wie", "wollt", "Ihr", "Euch", "ver\u00b7tei\u00b7d'\u00b7gen", "?", "Sprecht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVINF", "$.", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$.", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch er, den Blick emporgeschlagen, sprach: \u00bbK\u00f6nigin, sie haben recht.", "tokens": ["Doch", "er", ",", "den", "Blick", "em\u00b7por\u00b7ge\u00b7schla\u00b7gen", ",", "sprach", ":", "\u00bb", "K\u00f6\u00b7ni\u00b7gin", ",", "sie", "ha\u00b7ben", "recht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "VVINF", "$,", "VVFIN", "$.", "$(", "NN", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.8": {"line.1": {"text": "Ich f\u00fchle hoch mich, unvergleichbar, ob diesen frommen, zahmen Herrn", "tokens": ["Ich", "f\u00fch\u00b7le", "hoch", "mich", ",", "un\u00b7ver\u00b7gleich\u00b7bar", ",", "ob", "die\u00b7sen", "from\u00b7men", ",", "zah\u00b7men", "Herrn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "$,", "ADJD", "$,", "KOUS", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und ihrem Sinn so unerreichbar, wie ihrem Arm der Morgenstern.\u00ab", "tokens": ["Und", "ih\u00b7rem", "Sinn", "so", "un\u00b7er\u00b7reich\u00b7bar", ",", "wie", "ih\u00b7rem", "Arm", "der", "Mor\u00b7gens\u00b7tern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+-+--", "measure": "iambic.septa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbh\u00f6rst du sein freches \u00dcberheben! Auf, werft den H\u00f6hnemund ins Meer!\u00ab", "tokens": ["\u00bb", "h\u00f6rst", "du", "sein", "fre\u00b7ches", "\u00dc\u00b7ber\u00b7he\u00b7ben", "!", "Auf", ",", "werft", "den", "H\u00f6h\u00b7ne\u00b7mund", "ins", "Meer", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$.", "APPR", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Sie aber sprach mit leisem Beben: \u00bbUnd, Fremdling, dieser Stolz, woher?\u00ab", "tokens": ["Sie", "a\u00b7ber", "sprach", "mit", "lei\u00b7sem", "Be\u00b7ben", ":", "\u00bb", "Und", ",", "Fremd\u00b7ling", ",", "die\u00b7ser", "Stolz", ",", "wo\u00b7her", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$.", "$(", "KON", "$,", "NN", "$,", "PDAT", "NN", "$,", "PWAV", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbwoher? Nicht, weil dem neuen Glauben sich nie dies freie Haupt gebeugt,", "tokens": ["\u00bb", "wo\u00b7her", "?", "Nicht", ",", "weil", "dem", "neu\u00b7en", "Glau\u00b7ben", "sich", "nie", "dies", "frei\u00b7e", "Haupt", "ge\u00b7beugt", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PTKNEG", "$,", "KOUS", "ART", "ADJA", "NN", "PRF", "ADV", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Nicht, weil ich, wie der Falk die Tauben, die Christenritter oft gescheucht,", "tokens": ["Nicht", ",", "weil", "ich", ",", "wie", "der", "Falk", "die", "Tau\u00b7ben", ",", "die", "Chris\u00b7ten\u00b7rit\u00b7ter", "oft", "ge\u00b7scheucht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.11": {"line.1": {"text": "Nicht, weil wie Heklas Feueratem mein Lied all' ihre Singkunst schmolz,", "tokens": ["Nicht", ",", "weil", "wie", "He\u00b7klas", "Feu\u00b7e\u00b7ra\u00b7tem", "mein", "Lied", "all'", "ih\u00b7re", "Sing\u00b7kunst", "schmolz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "KOKOM", "NE", "NE", "PPOSAT", "NN", "PIAT", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Nein, nicht auf mir und meinen Taten, \u2013 auf einem Weibe ruht der Stolz.", "tokens": ["Nein", ",", "nicht", "auf", "mir", "und", "mei\u00b7nen", "Ta\u00b7ten", ",", "\u2013", "auf", "ei\u00b7nem", "Wei\u00b7be", "ruht", "der", "Stolz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKNEG", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,", "$(", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.12": {"line.1": {"text": "Wohl mag sein Haupt zu Sternen heben und f\u00fchlen sich den G\u00f6ttern gleich", "tokens": ["Wohl", "mag", "sein", "Haupt", "zu", "Ster\u00b7nen", "he\u00b7ben", "und", "f\u00fch\u00b7len", "sich", "den", "G\u00f6t\u00b7tern", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "KON", "VVFIN", "PRF", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der Mann, dem Seel' und Leib gegeben die sch\u00f6nste Maid im Nordenreich.\u00ab", "tokens": ["Der", "Mann", ",", "dem", "Seel'", "und", "Leib", "ge\u00b7ge\u00b7ben", "die", "sch\u00f6ns\u00b7te", "Maid", "im", "Nor\u00b7den\u00b7reich", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "ART", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbund wo, du Prahler,\u00ab scholl's im Saale, \u00bbund wer ist dieses Wunderweib?\u00ab", "tokens": ["\u00bb", "und", "wo", ",", "du", "Prah\u00b7ler", ",", "\u00ab", "scholl's", "im", "Saa\u00b7le", ",", "\u00bb", "und", "wer", "ist", "die\u00b7ses", "Wun\u00b7der\u00b7weib", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PWAV", "$,", "PPER", "NN", "$,", "$(", "VVFIN", "APPRART", "NN", "$,", "$(", "KON", "PWS", "VAFIN", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Da warf den Schild von schwarzem Stahle er m\u00e4chtig \u00fcber seinen Leib,", "tokens": ["Da", "warf", "den", "Schild", "von", "schwar\u00b7zem", "Stah\u00b7le", "er", "m\u00e4ch\u00b7tig", "\u00fc\u00b7ber", "sei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.14": {"line.1": {"text": "Sein breites Schwert schwang er mit Schalle und auf den Thronsitz sprang er hin:", "tokens": ["Sein", "brei\u00b7tes", "Schwert", "schwang", "er", "mit", "Schal\u00b7le", "und", "auf", "den", "Thron\u00b7sitz", "sprang", "er", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+---+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "\u00bbdies Weib? wohlan, ihr kennt es alle: hier steht es, eure K\u00f6nigin!\u00ab", "tokens": ["\u00bb", "dies", "Weib", "?", "wo\u00b7hlan", ",", "ihr", "kennt", "es", "al\u00b7le", ":", "hier", "steht", "es", ",", "eu\u00b7re", "K\u00f6\u00b7ni\u00b7gin", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "NN", "$.", "ADV", "$,", "PPER", "VVFIN", "PPER", "PIS", "$.", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.15": {"line.1": {"text": "\u00bbha, Tod dem Frevler,\u00ab klang es wieder und alle Klingen wurden blo\u00df.", "tokens": ["\u00bb", "ha", ",", "Tod", "dem", "Frev\u00b7ler", ",", "\u00ab", "klang", "es", "wie\u00b7der", "und", "al\u00b7le", "Klin\u00b7gen", "wur\u00b7den", "blo\u00df", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "wo