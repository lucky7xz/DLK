{"dta.poem.12653": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Sophronillen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.71", "af:0.14", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel gebe dir so viel vergn\u00fcgte stunden,", "tokens": ["Der", "him\u00b7mel", "ge\u00b7be", "dir", "so", "viel", "ver\u00b7gn\u00fcg\u00b7te", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So viel als schnecken-blut auf deinen wangen liegt,", "tokens": ["So", "viel", "als", "schne\u00b7cken\u00b7blut", "auf", "dei\u00b7nen", "wan\u00b7gen", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel als lilien den zarten hals umwunden,", "tokens": ["So", "viel", "als", "li\u00b7li\u00b7en", "den", "zar\u00b7ten", "hals", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "So viel als rosen stehn, da, wo man k\u00fcsse kriegt.", "tokens": ["So", "viel", "als", "ro\u00b7sen", "stehn", ",", "da", ",", "wo", "man", "k\u00fcs\u00b7se", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "KOKOM", "ADJD", "VVINF", "$,", "KOUS", "$,", "PWAV", "PIS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die sehn sucht sch\u00e4rffte mir die feder und die sinnen,", "tokens": ["Die", "sehn", "sucht", "sch\u00e4rff\u00b7te", "mir", "die", "fe\u00b7der", "und", "die", "sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie gab den vorsatz mir zum schreiben in die hand,", "tokens": ["Sie", "gab", "den", "vor\u00b7satz", "mir", "zum", "schrei\u00b7ben", "in", "die", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPRART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und hie\u00df mich diesen brieff, den du hier siehst, beginnen,", "tokens": ["Und", "hie\u00df", "mich", "die\u00b7sen", "brieff", ",", "den", "du", "hier", "siehst", ",", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und den dir meine hand nicht ohne furcht gesandt.", "tokens": ["Und", "den", "dir", "mei\u00b7ne", "hand", "nicht", "oh\u00b7ne", "furcht", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht ohne furcht, sag ich, wer wagt sich zu der sonnen,", "tokens": ["Nicht", "oh\u00b7ne", "furcht", ",", "sag", "ich", ",", "wer", "wagt", "sich", "zu", "der", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df nicht sein fl\u00fcgel-werck vom feuer wird versehrt?", "tokens": ["Da\u00df", "nicht", "sein", "fl\u00fc\u00b7gel\u00b7\u00b7werck", "vom", "feu\u00b7er", "wird", "ver\u00b7sehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Zum himmel, wo der blitz zu uns kommt abgeronnen,", "tokens": ["Zum", "him\u00b7mel", ",", "wo", "der", "blitz", "zu", "uns", "kommt", "ab\u00b7ge\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "APPR", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der alle welt vor ihm sich hefftig f\u00fcrchten lehrt.", "tokens": ["Der", "al\u00b7le", "welt", "vor", "ihm", "sich", "heff\u00b7tig", "f\u00fcrch\u00b7ten", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPER", "PRF", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch nicht allein die furcht, die wohnt in meinem hertzen,", "tokens": ["Doch", "nicht", "al\u00b7lein", "die", "furcht", ",", "die", "wohnt", "in", "mei\u00b7nem", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die liebe steht nebst ihr, und wartet, was dein geist", "tokens": ["Die", "lie\u00b7be", "steht", "nebst", "ihr", ",", "und", "war\u00b7tet", ",", "was", "dein", "geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hierauf vor antwort schickt, ob schmertzen vor das schertzen,", "tokens": ["Hier\u00b7auf", "vor", "ant\u00b7wort", "schickt", ",", "ob", "schmert\u00b7zen", "vor", "das", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVFIN", "$,", "KOUS", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ob ihn dein brieff vor lust mit Coloqvinten speist.", "tokens": ["Ob", "ihn", "dein", "brieff", "vor", "lust", "mit", "Co\u00b7lo\u00b7qvin\u00b7ten", "speist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wage zwar zuviel, da\u00df ich mich unterwinde,", "tokens": ["Ich", "wa\u00b7ge", "zwar", "zu\u00b7viel", ",", "da\u00df", "ich", "mich", "un\u00b7ter\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "KOUS", "PPER", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und hier von liebe schreib, ich wei\u00df ja mehr als wohl,", "tokens": ["Und", "hier", "von", "lie\u00b7be", "schreib", ",", "ich", "wei\u00df", "ja", "mehr", "als", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "PIAT", "KOKOM", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df di\u00df bey dir so viel gilt als wie eine s\u00fcnde,", "tokens": ["Da\u00df", "di\u00df", "bey", "dir", "so", "viel", "gilt", "als", "wie", "ei\u00b7ne", "s\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "PPER", "ADV", "ADV", "VVFIN", "KOUS", "KOKOM", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ja da\u00df man hier sich der nur gantz enthalten soll.", "tokens": ["Ja", "da\u00df", "man", "hier", "sich", "der", "nur", "gantz", "ent\u00b7hal\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PIS", "ADV", "PRF", "ART", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jedoch verzeihe mir, o G\u00f6ttin! di\u00df verbrechen,", "tokens": ["Je\u00b7doch", "ver\u00b7zei\u00b7he", "mir", ",", "o", "G\u00f6t\u00b7tin", "!", "di\u00df", "ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein hertz hast du vielmehr ja itzt gantz in gewalt,", "tokens": ["Mein", "hertz", "hast", "du", "viel\u00b7mehr", "ja", "itzt", "gantz", "in", "ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dasselbe kanstu nun der s\u00fcnde wegen sprechen,", "tokens": ["Das\u00b7sel\u00b7be", "kans\u00b7tu", "nun", "der", "s\u00fcn\u00b7de", "we\u00b7gen", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "ADJA", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Was gilts, es giebt die schuld der himmlischen gestalt,", "tokens": ["Was", "gilts", ",", "es", "giebt", "die", "schuld", "der", "himm\u00b7li\u00b7schen", "ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VVFIN", "ART", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die dein gesichte tr\u00e4gt, und so wirstu die flammen,", "tokens": ["Die", "dein", "ge\u00b7sich\u00b7te", "tr\u00e4gt", ",", "und", "so", "wirs\u00b7tu", "die", "flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "VVFIN", "$,", "KON", "ADV", "VAFIN", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die deine sch\u00f6nheit hat in meiner brust entz\u00fcndt,", "tokens": ["Die", "dei\u00b7ne", "sch\u00f6n\u00b7heit", "hat", "in", "mei\u00b7ner", "brust", "ent\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und die du selbst erzeugt, nicht gar so bald verdammen,", "tokens": ["Und", "die", "du", "selbst", "er\u00b7zeugt", ",", "nicht", "gar", "so", "bald", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "VVPP", "$,", "PTKNEG", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wer wei\u00df, ob man nicht hier sonst andre mittel findt.", "tokens": ["Wer", "wei\u00df", ",", "ob", "man", "nicht", "hier", "sonst", "and\u00b7re", "mit\u00b7tel", "findt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Jedoch ich schreibe viel, doch w\u00e4r es nicht vonn\u00f6then,", "tokens": ["Je\u00b7doch", "ich", "schrei\u00b7be", "viel", ",", "doch", "w\u00e4r", "es", "nicht", "von\u00b7n\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Schreib nur ein g\u00fctig ja auf meinem brieff zur\u00fcck,", "tokens": ["Schreib", "nur", "ein", "g\u00fc\u00b7tig", "ja", "auf", "mei\u00b7nem", "brieff", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo nicht, so wird dein nein mich \u00e4rmsten bald ert\u00f6den,", "tokens": ["Wo", "nicht", ",", "so", "wird", "dein", "nein", "mich", "\u00e4rms\u00b7ten", "bald", "er\u00b7t\u00f6\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VAFIN", "PPOSAT", "PTKANT", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn deine gunst ist ja mein leben und mein gl\u00fcck.", "tokens": ["Denn", "dei\u00b7ne", "gunst", "ist", "ja", "mein", "le\u00b7ben", "und", "mein", "gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "VVINF", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}}}}}