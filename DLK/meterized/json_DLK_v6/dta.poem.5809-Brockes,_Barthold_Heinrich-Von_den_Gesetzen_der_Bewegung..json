{"dta.poem.5809": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Gesetzen der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sein unver\u00e4nderlich, unsterblich fruchtbar Wesen", "tokens": ["Sein", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", ",", "uns\u00b7terb\u00b7lich", "frucht\u00b7bar", "We\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "$,", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als er zu Anfangs in die Welt", "tokens": ["Als", "er", "zu", "An\u00b7fangs", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch die Bewegung recht den Geist hineingebracht,", "tokens": ["Durch", "die", "Be\u00b7we\u00b7gung", "recht", "den", "Geist", "hin\u00b7ein\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lie\u00df das darinn, durch welches sie sich r\u00fchret.", "tokens": ["Lie\u00df", "das", "da\u00b7rinn", ",", "durch", "wel\u00b7ches", "sie", "sich", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PAV", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und unterh\u00e4lt sie noch, so wie er sie formiret.", "tokens": ["Und", "un\u00b7ter\u00b7h\u00e4lt", "sie", "noch", ",", "so", "wie", "er", "sie", "for\u00b7mi\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "KOKOM", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}