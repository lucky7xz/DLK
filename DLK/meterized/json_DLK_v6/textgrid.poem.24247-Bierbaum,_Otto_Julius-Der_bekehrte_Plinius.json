{"textgrid.poem.24247": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der bekehrte Plinius", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Plinius der J\u00fcngere spricht:", "tokens": ["Pli\u00b7nius", "der", "J\u00fcn\u00b7ge\u00b7re", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Wenn die Narren tanzen wollen,", "tokens": ["Wenn", "die", "Nar\u00b7ren", "tan\u00b7zen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hindert sie der Weise nicht,", "tokens": ["Hin\u00b7dert", "sie", "der", "Wei\u00b7se", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch entflieht er ihrem Tollen", "tokens": ["Doch", "ent\u00b7flieht", "er", "ih\u00b7rem", "Tol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In ein abgelegnes Haus,", "tokens": ["In", "ein", "ab\u00b7ge\u00b7leg\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieht sich seine Toga aus", "tokens": ["Zieht", "sich", "sei\u00b7ne", "To\u00b7ga", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und verfertigt ein Gedicht,", "tokens": ["Und", "ver\u00b7fer\u00b7tigt", "ein", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das von jenen Pfaden handelt,", "tokens": ["Das", "von", "je\u00b7nen", "Pfa\u00b7den", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Drauf der stille Denker wandelt,", "tokens": ["Drauf", "der", "stil\u00b7le", "Den\u00b7ker", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Hell umstrahlt vom eignen Licht.", "tokens": ["Hell", "um\u00b7strahlt", "vom", "eig\u00b7nen", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ganz so unrecht hat er nicht,", "tokens": ["Ganz", "so", "un\u00b7recht", "hat", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser j\u00fcngre Plinius.", "tokens": ["Die\u00b7ser", "j\u00fcng\u00b7re", "Pli\u00b7nius", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Manchmal ist es kein Genu\u00df,", "tokens": ["Manch\u00b7mal", "ist", "es", "kein", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort zu sein,", "tokens": ["Dort", "zu", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VAINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wo das Bein", "tokens": ["Wo", "das", "Bein"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Tanzt, blo\u00df weil es tanzen mu\u00df.", "tokens": ["Tanzt", ",", "blo\u00df", "weil", "es", "tan\u00b7zen", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zum Beispiel, wo der Leutenant", "tokens": ["Zum", "Bei\u00b7spiel", ",", "wo", "der", "Leu\u00b7ten\u00b7ant"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zum Regimentsmusikget\u00f6se", "tokens": ["Zum", "Re\u00b7gi\u00b7ments\u00b7mu\u00b7sik\u00b7ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Taille der Frau Kommand\u00f6se", "tokens": ["Die", "Tail\u00b7le", "der", "Frau", "Kom\u00b7man\u00b7d\u00f6\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Mit pflichtergebnem Arm umspannt;", "tokens": ["Mit", "pflich\u00b7ter\u00b7geb\u00b7nem", "Arm", "um\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Oder wo,", "tokens": ["O\u00b7der", "wo", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "PWAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Blo\u00df so so,", "tokens": ["Blo\u00df", "so", "so", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Der Staatsanwaltschaftssubstitut", "tokens": ["Der", "Staats\u00b7an\u00b7walt\u00b7schafts\u00b7subs\u00b7ti\u00b7tut"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Fr\u00f6hlich tut,", "tokens": ["Fr\u00f6h\u00b7lich", "tut", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Weil der Anstand es erfordert,", "tokens": ["Weil", "der", "An\u00b7stand", "es", "er\u00b7for\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Da zum Tanzen er beordert,", "tokens": ["Da", "zum", "Tan\u00b7zen", "er", "be\u00b7or\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und die saure Lippe man", "tokens": ["Und", "die", "sau\u00b7re", "Lip\u00b7pe", "man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PIS"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Dabei nicht verwenden kann;", "tokens": ["Da\u00b7bei", "nicht", "ver\u00b7wen\u00b7den", "kann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Oder auch,", "tokens": ["O\u00b7der", "auch", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Wo der Bauch", "tokens": ["Wo", "der", "Bauch"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Des beleibten Handelsmanns", "tokens": ["Des", "be\u00b7leib\u00b7ten", "Han\u00b7dels\u00b7manns"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Sich im Tanz", "tokens": ["Sich", "im", "Tanz"], "token_info": ["word", "word", "word"], "pos": ["PRF", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Widerwillig dreht und schwenkt,", "tokens": ["Wi\u00b7der\u00b7wil\u00b7lig", "dreht", "und", "schwenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Weil ihn die Erw\u00e4gung lenkt,", "tokens": ["Weil", "ihn", "die", "Er\u00b7w\u00e4\u00b7gung", "lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+----+", "measure": "dactylic.init"}, "line.25": {"text": "Da\u00df von eines Kunden Gnaden", "tokens": ["Da\u00df", "von", "ei\u00b7nes", "Kun\u00b7den", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Er zum Hausball ist geladen.", "tokens": ["Er", "zum", "Haus\u00b7ball", "ist", "ge\u00b7la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Bei solcherlei Gelegenheit", "tokens": ["Bei", "sol\u00b7cher\u00b7lei", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Bleibt der Weise lieber weit", "tokens": ["Bleibt", "der", "Wei\u00b7se", "lie\u00b7ber", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Weg vom Schu\u00df nach Plinii", "tokens": ["Weg", "vom", "Schu\u00df", "nach", "Pli\u00b7nii"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "APPR", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Junioris Theorie;", "tokens": ["Ju\u00b7ni\u00b7o\u00b7ris", "The\u00b7o\u00b7rie", ";"], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Denn beim Zeus,", "tokens": ["Denn", "beim", "Zeus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.32": {"text": "Es ist scheu\u00dflich", "tokens": ["Es", "ist", "scheu\u00df\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "und \u00fcbel anzusehn,", "tokens": ["und", "\u00fc\u00b7bel", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Wo sich so", "tokens": ["Wo", "sich", "so"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PRF", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.35": {"text": "Mal apropos", "tokens": ["Mal", "a\u00b7pro\u00b7pos"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.36": {"text": "T\u00e4nzersohlen,", "tokens": ["T\u00e4n\u00b7zer\u00b7soh\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.37": {"text": "Die befohlen,", "tokens": ["Die", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Auf dem Pflichtparkette drehn.", "tokens": ["Auf", "dem", "Pflicht\u00b7par\u00b7ket\u00b7te", "drehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "M\u00fchsal mahlt im M\u00fchlentakte,", "tokens": ["M\u00fch\u00b7sal", "mahlt", "im", "M\u00fch\u00b7len\u00b7tak\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Klipp und klapp,", "tokens": ["Klipp", "und", "klapp", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Das vertrackte", "tokens": ["Das", "ver\u00b7track\u00b7te"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.42": {"text": "Pensum ab;", "tokens": ["Pen\u00b7sum", "ab", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.43": {"text": "Selbst des Walzers holde T\u00f6ne", "tokens": ["Selbst", "des", "Wal\u00b7zers", "hol\u00b7de", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Werden schleppend zum Gest\u00f6hne,", "tokens": ["Wer\u00b7den", "schlep\u00b7pend", "zum", "Ge\u00b7st\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.45": {"text": "Das zum Himmel schluchzt und schreit:", "tokens": ["Das", "zum", "Him\u00b7mel", "schluchzt", "und", "schreit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Welche Niedertr\u00e4chtigkeit!", "tokens": ["Wel\u00b7che", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber nun, o Plinius,", "tokens": ["A\u00b7ber", "nun", ",", "o", "Pli\u00b7nius", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "H\u00f6r, was ich dir sagen mu\u00df:", "tokens": ["H\u00f6r", ",", "was", "ich", "dir", "sa\u00b7gen", "mu\u00df", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steig aus deiner Toga Falten,", "tokens": ["Steig", "aus", "dei\u00b7ner", "To\u00b7ga", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fccke dich mit Lack und Claque,", "tokens": ["Schm\u00fc\u00b7cke", "dich", "mit", "Lack", "und", "Cla\u00b7que", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df dich (Mut, Mann!) mi\u00dfgestalten", "tokens": ["La\u00df", "dich", "(", "Mut", ",", "Mann", "!", ")", "mi\u00df\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word"], "pos": ["VVIMP", "PPER", "$(", "NN", "$,", "NN", "$.", "$(", "VVFIN"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Durch den zwiegeschw\u00e4nzten Frack!", "tokens": ["Durch", "den", "zwie\u00b7ge\u00b7schw\u00e4nz\u00b7ten", "Frack", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Triste scheint zwar die Montur,", "tokens": ["Tris\u00b7te", "scheint", "zwar", "die", "Mon\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Doch das ist von au\u00dfen nur.", "tokens": ["Doch", "das", "ist", "von", "au\u00b7\u00dfen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "APPR", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und nun komm, ich will dich leiten,", "tokens": ["Und", "nun", "komm", ",", "ich", "will", "dich", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie den Dante einst Virgil,", "tokens": ["Wie", "den", "Dan\u00b7te", "einst", "Vir\u00b7gil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber nicht in H\u00f6llenbreiten,", "tokens": ["A\u00b7ber", "nicht", "in",