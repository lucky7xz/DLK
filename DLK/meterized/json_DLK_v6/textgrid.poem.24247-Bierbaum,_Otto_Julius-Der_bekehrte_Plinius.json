: "dactylic.di.plus"}, "line.50": {"text": "Halte doch an!\u00ab", "tokens": ["Hal\u00b7te", "doch", "an", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "PTKVZ", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.51": {"text": "\u2013 \u00bbKeine Idee!", "tokens": ["\u2013", "\u00bb", "Kei\u00b7ne", "I\u00b7dee", "!"], "token_info": ["punct", "punct", "word", "word", "punct"], "pos": ["$(", "$(", "PIAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.52": {"text": "Noch eine Runde rum!", "tokens": ["Noch", "ei\u00b7ne", "Run\u00b7de", "rum", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Links herum, rechts herum!\u00ab \u2013", "tokens": ["Links", "he\u00b7rum", ",", "rechts", "he\u00b7rum", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$.", "$(", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.54": {"text": "Schon ist er fern!", "tokens": ["Schon", "ist", "er", "fern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.55": {"text": "Hat man seine Not mit den Klassikern!", "tokens": ["Hat", "man", "sei\u00b7ne", "Not", "mit", "den", "Klas\u00b7si\u00b7kern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Endlich ist der Tanz vorbei,", "tokens": ["End\u00b7lich", "ist", "der", "Tanz", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter"