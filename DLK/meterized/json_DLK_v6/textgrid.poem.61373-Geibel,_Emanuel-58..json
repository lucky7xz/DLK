{"textgrid.poem.61373": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's nicht schier, um zu verzweifeln,", "tokens": ["Ist's", "nicht", "schier", ",", "um", "zu", "ver\u00b7zwei\u00b7feln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich sehn mu\u00df, wie sie's treiben,", "tokens": ["Wenn", "ich", "sehn", "mu\u00df", ",", "wie", "sie's", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "PWAV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die da singen, die da schreiben", "tokens": ["Die", "da", "sin\u00b7gen", ",", "die", "da", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem weiland Dichterwald!", "tokens": ["In", "dem", "wei\u00b7land", "Dich\u00b7ter\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und du l\u00e4\u00dft es dir gefallen,", "tokens": ["Und", "du", "l\u00e4\u00dft", "es", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutsches Volk, und nimmst von allen,", "tokens": ["Deut\u00b7sches", "Volk", ",", "und", "nimmst", "von", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was sie bringen hei\u00df und kalt:", "tokens": ["Was", "sie", "brin\u00b7gen", "hei\u00df", "und", "kalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Statt des Wahren nur das Reizende,", "tokens": ["Statt", "des", "Wah\u00b7ren", "nur", "das", "Rei\u00b7zen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Statt des Sch\u00f6nen nur das Beizende,", "tokens": ["Statt", "des", "Sch\u00f6\u00b7nen", "nur", "das", "Bei\u00b7zen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Statt des Tiefen Mi\u00dfgestalt.", "tokens": ["Statt", "des", "Tie\u00b7fen", "Mi\u00df\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}