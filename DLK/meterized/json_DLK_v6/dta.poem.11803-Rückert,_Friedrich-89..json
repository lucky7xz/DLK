{"dta.poem.11803": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "89.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel sind der Tugenden, doch jede ist die ganze,", "tokens": ["Viel", "sind", "der", "Tu\u00b7gen\u00b7den", ",", "doch", "je\u00b7de", "ist", "die", "gan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "PIS", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn \u00e4cht, so wie ein Bild vom Fr\u00fchling jede Pflanze.", "tokens": ["Wenn", "\u00e4cht", ",", "so", "wie", "ein", "Bild", "vom", "Fr\u00fch\u00b7ling", "je\u00b7de", "Pflan\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo eine Blume bl\u00fcht, da mu\u00df der Fr\u00fchling seyn,", "tokens": ["Wo", "ei\u00b7ne", "Blu\u00b7me", "bl\u00fcht", ",", "da", "mu\u00df", "der", "Fr\u00fch\u00b7ling", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo der Fr\u00fchling ist, da bl\u00fcht bald gro\u00df und klein.", "tokens": ["Und", "wo", "der", "Fr\u00fch\u00b7ling", "ist", ",", "da", "bl\u00fcht", "bald", "gro\u00df", "und", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "$,", "ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So gleich einander all und jede so verschieden,", "tokens": ["So", "gleich", "ein\u00b7an\u00b7der", "all", "und", "je\u00b7de", "so", "ver\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PIAT", "KON", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wohnen Blumen-gleich die Tugenden in Frieden.", "tokens": ["So", "woh\u00b7nen", "Blu\u00b7men\u00b7gleich", "die", "Tu\u00b7gen\u00b7den", "in", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie wohnen in der Brust, wie Blumen auf der Flur,", "tokens": ["Sie", "woh\u00b7nen", "in", "der", "Brust", ",", "wie", "Blu\u00b7men", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine Himmelslust ist solch ein Anblick nur.", "tokens": ["Und", "ei\u00b7ne", "Him\u00b7mels\u00b7lust", "ist", "solch", "ein", "An\u00b7blick", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIAT", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}