{"textgrid.poem.58663": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kein K\u00f6nig wird allhier so frei und so vergn\u00fcgt gefunden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein K\u00f6nig wird allhier so frei und so vergn\u00fcgt gefunden", "tokens": ["Kein", "K\u00f6\u00b7nig", "wird", "all\u00b7hier", "so", "frei", "und", "so", "ver\u00b7gn\u00fcgt", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "KON", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Als Seelen, die der Liebe Band mit Gott im Geist verbunden,", "tokens": ["Als", "See\u00b7len", ",", "die", "der", "Lie\u00b7be", "Band", "mit", "Gott", "im", "Geist", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "NN", "APPR", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die frei von Lust der Kreatur und gr\u00fcndlich willenlos", "tokens": ["Die", "frei", "von", "Lust", "der", "Kre\u00b7a\u00b7tur", "und", "gr\u00fcnd\u00b7lich", "wil\u00b7len\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "ART", "NN", "KON", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Sich stets und innig lassen Gott und ihm so folgen blo\u00df.", "tokens": ["Sich", "stets", "und", "in\u00b7nig", "las\u00b7sen", "Gott", "und", "ihm", "so", "fol\u00b7gen", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "KON", "ADJD", "VVINF", "NN", "KON", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Herr, bring mich widerspenstig's Kind auch in dies Band des Bundes,", "tokens": ["Herr", ",", "bring", "mich", "wi\u00b7der\u00b7spens\u00b7tig's", "Kind", "auch", "in", "dies", "Band", "des", "Bun\u00b7des", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJA", "NN", "ADV", "APPR", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Mach mich durch deine Zuchtrut' treu den Worten deines Mundes;", "tokens": ["Mach", "mich", "durch", "dei\u00b7ne", "Zuchtrut'", "treu", "den", "Wor\u00b7ten", "dei\u00b7nes", "Mun\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dein Kreuzesstecken t\u00f6te ganz, was sonst im Herzen lebt,", "tokens": ["Dein", "Kreu\u00b7zes\u00b7ste\u00b7cken", "t\u00f6\u00b7te", "ganz", ",", "was", "sonst", "im", "Her\u00b7zen", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Bis alles aus mir ausgefegt, was dir noch widerstrebt!", "tokens": ["Bis", "al\u00b7les", "aus", "mir", "aus\u00b7ge\u00b7fegt", ",", "was", "dir", "noch", "wi\u00b7der\u00b7strebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "PPER", "VVPP", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}