{"textgrid.poem.60219": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du des Gl\u00fcckes Eigensinn ertragen,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du des Gl\u00fcckes Eigensinn ertragen,", "tokens": ["Der", "du", "des", "Gl\u00fc\u00b7ckes", "Ei\u00b7gen\u00b7sinn", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und itzt in seiner Freundes-Schoo\u00df", "tokens": ["Und", "itzt", "in", "sei\u00b7ner", "Freun\u00b7des\u00b7Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf samtnem Sessel wirst getragen,", "tokens": ["Auf", "samt\u00b7nem", "Ses\u00b7sel", "wirst", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Labes, Patriot! verwandle deine Klagen", "tokens": ["O", "La\u00b7bes", ",", "Pat\u00b7ri\u00b7ot", "!", "ver\u00b7wand\u00b7le", "dei\u00b7ne", "Kla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$.", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In Saitenspiel, und la\u00df nicht mehr Gedanken fragen:", "tokens": ["In", "Sai\u00b7ten\u00b7spiel", ",", "und", "la\u00df", "nicht", "mehr", "Ge\u00b7dan\u00b7ken", "fra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "KON", "VVIMP", "PTKNEG", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Warum dein Peter fiel? der strahlenreich und gro\u00df", "tokens": ["Wa\u00b7rum", "dein", "Pe\u00b7ter", "fiel", "?", "der", "strah\u00b7len\u00b7reich", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NE", "VVFIN", "$.", "ART", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der hohen Sonne glich, die Gottes Erde w\u00e4rmen,", "tokens": ["Der", "ho\u00b7hen", "Son\u00b7ne", "glich", ",", "die", "Got\u00b7tes", "Er\u00b7de", "w\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und seine Majest\u00e4t den Menschen zeigen mu\u00df!", "tokens": ["Und", "sei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "den", "Men\u00b7schen", "zei\u00b7gen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihn wecken Seufzer nicht, auch nicht ein Thr\u00e4nengu\u00df", "tokens": ["Ihn", "we\u00b7cken", "Seuf\u00b7zer", "nicht", ",", "auch", "nicht", "ein", "Thr\u00e4\u00b7nen\u00b7gu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "PTKNEG", "$,", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gestr\u00f6mt auf heilige Gebeine;", "tokens": ["Ge\u00b7str\u00f6mt", "auf", "hei\u00b7li\u00b7ge", "Ge\u00b7bei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sein Engel, sonst umcr\u00e4nzt mit Morgensternes Licht,", "tokens": ["Sein", "En\u00b7gel", ",", "sonst", "um\u00b7cr\u00b7\u00e4nzt", "mit", "Mor\u00b7gens\u00b7ter\u00b7nes", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Verh\u00fcllet itzt sein Angesicht", "tokens": ["Ver\u00b7h\u00fcl\u00b7let", "itzt", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und wirft sich nieder, da\u00df er weine,", "tokens": ["Und", "wirft", "sich", "nie\u00b7der", ",", "da\u00df", "er", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und w\u00fcrde, w\u00e4r es ihm erlaubt,", "tokens": ["Und", "w\u00fcr\u00b7de", ",", "w\u00e4r", "es", "ihm", "er\u00b7laubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Den Sch\u00f6pfer aller Wesen fragen:", "tokens": ["Den", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "We\u00b7sen", "fra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "\u00bbwarum der hohen Ceder Haupt", "tokens": ["\u00bb", "wa\u00b7rum", "der", "ho\u00b7hen", "Ce\u00b7der", "Haupt"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Von schnellem Blitze ward zerschlagen?", "tokens": ["Von", "schnel\u00b7lem", "Blit\u00b7ze", "ward", "zer\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Warum der Todes Engel schlug", "tokens": ["Wa\u00b7rum", "der", "To\u00b7des", "En\u00b7gel", "schlug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den Herrscher \u00fcber Nationen,", "tokens": ["Den", "Herr\u00b7scher", "\u00fc\u00b7ber", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der in erhabner Brust so viel Entw\u00fcrfe trug,", "tokens": ["Der", "in", "er\u00b7hab\u00b7ner", "Brust", "so", "viel", "Ent\u00b7w\u00fcr\u00b7fe", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Tugend, das Verdienst, die K\u00fcnste zu belohnen?", "tokens": ["Die", "Tu\u00b7gend", ",", "das", "Ver\u00b7dienst", ",", "die", "K\u00fcns\u00b7te", "zu", "be\u00b7loh\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und den, den schon sein Flei\u00df erhub.", "tokens": ["Und", "den", ",", "den", "schon", "sein", "Flei\u00df", "er\u00b7hub", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Noch gl\u00e4nzender empor zu heben?\u00ab", "tokens": ["Noch", "gl\u00e4n\u00b7zen\u00b7der", "em\u00b7por", "zu", "he\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Er ist nicht mehr! der Staub begrub", "tokens": ["Er", "ist", "nicht", "mehr", "!", "der", "Staub", "be\u00b7grub"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Den, der ein K\u00f6nigreich, ein Land zur\u00fcckzugeben,", "tokens": ["Den", ",", "der", "ein", "K\u00f6\u00b7nig\u00b7reich", ",", "ein", "Land", "zu\u00b7r\u00fcck\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mehr Seeligkeit, mehr Lust genannt,", "tokens": ["Mehr", "See\u00b7lig\u00b7keit", ",", "mehr", "Lust", "ge\u00b7nannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Als wenn er von dem Kayser-Sitze", "tokens": ["Als", "wenn", "er", "von", "dem", "Kay\u00b7ser\u00b7Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Des diamantnen Zepters Spitze", "tokens": ["Des", "di\u00b7a\u00b7mant\u00b7nen", "Zep\u00b7ters", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Zu fremder Bothen Stirn gewandt!", "tokens": ["Zu", "frem\u00b7der", "Bo\u00b7then", "Stirn", "ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Er ist nicht mehr. Ihn segnet Preussen", "tokens": ["Er", "ist", "nicht", "mehr", ".", "Ihn", "seg\u00b7net", "Preus\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Noch in der Ewigkeit; ihm th\u00f6net Lobgedicht,", "tokens": ["Noch", "in", "der", "E\u00b7wig\u00b7keit", ";", "ihm", "th\u00f6\u00b7net", "Lob\u00b7ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn Friedrichs Seufzer von ihm spricht;", "tokens": ["Wenn", "Fried\u00b7richs", "Seuf\u00b7zer", "von", "ihm", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Und Engel horchen zu, und heissen", "tokens": ["Und", "En\u00b7gel", "hor\u00b7chen", "zu", ",", "und", "heis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PTKVZ", "$,", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Ihn g\u00f6ttlich, wie sein Freund ihn nennt!", "tokens": ["Ihn", "g\u00f6tt\u00b7lich", ",", "wie", "sein", "Freund", "ihn", "nennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Er ward der Erde nicht geg\u00f6nnt;", "tokens": ["Er", "ward", "der", "Er\u00b7de", "nicht", "ge\u00b7g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wir sind zu klein, zu dunkelsichtig", "tokens": ["Wir", "sind", "zu", "klein", ",", "zu", "dun\u00b7kel\u00b7sich\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$,", "PTKA", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Den Rathschlu\u00df einzusehn, der alle Dinge richtig,", "tokens": ["Den", "Rath\u00b7schlu\u00df", "ein\u00b7zu\u00b7sehn", ",", "der", "al\u00b7le", "Din\u00b7ge", "rich\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Schon eh er sie gemacht, bestimmt,", "tokens": ["Schon", "eh", "er", "sie", "ge\u00b7macht", ",", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Und Cronen geben kann, so wie er Cronen nimmt.", "tokens": ["Und", "Cro\u00b7nen", "ge\u00b7ben", "kann", ",", "so", "wie", "er", "Cro\u00b7nen", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "$,", "ADV", "KOKOM", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gott ist nicht \u00fcber uns ergrimmt!", "tokens": ["Gott", "ist", "nicht", "\u00fc\u00b7ber", "uns", "er\u00b7grimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ruf deinen Geist mit schnellem Fluge", "tokens": ["Ruf", "dei\u00b7nen", "Geist", "mit", "schnel\u00b7lem", "Flu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zur\u00fcck von Peters Todtenkruge,", "tokens": ["Zu\u00b7r\u00fcck", "von", "Pe\u00b7ters", "Tod\u00b7ten\u00b7kru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und blick' als Vaterlandes-Freund,", "tokens": ["Und", "blick'", "als", "Va\u00b7ter\u00b7lan\u00b7des\u00b7Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf jenen grossen Siegesbogen,", "tokens": ["Auf", "je\u00b7nen", "gros\u00b7sen", "Sie\u00b7ges\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch welchen Friederich in seine Stadt gezogen!", "tokens": ["Durch", "wel\u00b7chen", "Frie\u00b7de\u00b7rich", "in", "sei\u00b7ne", "Stadt", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von hohen H\u00fcgeln sah' der Feind", "tokens": ["Von", "ho\u00b7hen", "H\u00fc\u00b7geln", "sah'", "der", "Feind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit starrem Auge zu, die Schaam auf blasser Wange,", "tokens": ["Mit", "star\u00b7rem", "Au\u00b7ge", "zu", ",", "die", "Schaam", "auf", "blas\u00b7ser", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und seine Krieger, die sich lange", "tokens": ["Und", "sei\u00b7ne", "Krie\u00b7ger", ",", "die", "sich", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herab gewehrt vom hohen Wall,", "tokens": ["Her\u00b7ab", "ge\u00b7wehrt", "vom", "ho\u00b7hen", "Wall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Stehn waffenlos, und taub von starkem Jubelschall", "tokens": ["Stehn", "waf\u00b7fen\u00b7los", ",", "und", "taub", "von", "star\u00b7kem", "Ju\u00b7bel\u00b7schall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "$,", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der B\u00fcrger und des Siegesheeres;", "tokens": ["Der", "B\u00fcr\u00b7ger", "und", "des", "Sie\u00b7ges\u00b7hee\u00b7res", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und Friedrichs grosse Feindin st\u00f6\u00dft", "tokens": ["Und", "Fried\u00b7richs", "gros\u00b7se", "Fein\u00b7din", "st\u00f6\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihrer Brust herauf ein schweres", "tokens": ["Aus", "ih\u00b7rer", "Brust", "her\u00b7auf", "ein", "schwe\u00b7res"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und bittres Ach; So hat, da Griechenland erl\u00f6st", "tokens": ["Und", "bitt\u00b7res", "Ach", ";", "So", "hat", ",", "da", "Grie\u00b7chen\u00b7land", "er\u00b7l\u00f6st"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "ADV", "VAFIN", "$,", "KOUS", "NE", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Durch seine Helden ward, der Perser fortgestossen", "tokens": ["Durch", "sei\u00b7ne", "Hel\u00b7den", "ward", ",", "der", "Per\u00b7ser", "fort\u00b7ges\u00b7tos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bleyschwere Seufzer aus der Brust;", "tokens": ["Bley\u00b7schwe\u00b7re", "Seuf\u00b7zer", "aus", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So klopfte seiner unbewu\u00dft,", "tokens": ["So", "klopf\u00b7te", "sei\u00b7ner", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das Herz erschrocken in dem grossen", "tokens": ["Das", "Herz", "er\u00b7schro\u00b7cken", "in", "dem", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Pompejus, als sein Heer entwich,", "tokens": ["Pom\u00b7pe\u00b7jus", ",", "als", "sein", "Heer", "ent\u00b7wich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Und er von dicker Staubes Wolke", "tokens": ["Und", "er", "von", "di\u00b7cker", "Stau\u00b7bes", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Davon belehrt zur\u00fcck in \u00f6des Lager schlich.", "tokens": ["Da\u00b7von", "be\u00b7lehrt", "zu\u00b7r\u00fcck", "in", "\u00f6\u00b7des", "La\u00b7ger", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Furcht