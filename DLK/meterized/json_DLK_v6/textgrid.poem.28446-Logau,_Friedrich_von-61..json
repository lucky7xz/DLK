{"textgrid.poem.28446": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Kranckheit nicht so sehr als ihren Ursprung heilet,", "tokens": ["Wer", "Kran\u00b7ck\u00b7heit", "nicht", "so", "sehr", "als", "ih\u00b7ren", "Ur\u00b7sprung", "hei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKNEG", "ADV", "ADV", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein solcher Artzt heilt wol und heilet unverweilet;", "tokens": ["Ein", "sol\u00b7cher", "Artzt", "heilt", "wol", "und", "hei\u00b7let", "un\u00b7ver\u00b7wei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADV", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer nicht mit S\u00fcnden k\u00e4mpfft und nur mit Kranckheit k\u00e4mpfft,", "tokens": ["Wer", "nicht", "mit", "S\u00fcn\u00b7den", "k\u00e4mpfft", "und", "nur", "mit", "Kran\u00b7ck\u00b7heit", "k\u00e4mpfft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "NN", "VVFIN", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Der hat sie mehr gest\u00e4rckt und weniger ged\u00e4mpfft.", "tokens": ["Der", "hat", "sie", "mehr", "ge\u00b7st\u00e4rckt", "und", "we\u00b7ni\u00b7ger", "ge\u00b7d\u00e4mpfft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}