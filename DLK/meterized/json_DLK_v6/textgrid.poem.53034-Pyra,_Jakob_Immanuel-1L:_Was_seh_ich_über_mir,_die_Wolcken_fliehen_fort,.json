{"textgrid.poem.53034": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was seh ich \u00fcber mir, die Wolcken fliehen fort,", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was seh ich \u00fcber mir, die Wolcken fliehen fort,", "tokens": ["Was", "seh", "ich", "\u00fc\u00b7ber", "mir", ",", "die", "Wol\u00b7cken", "flie\u00b7hen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Luft wird klar und schnell voll Strahlen, Blitz und Feuer,", "tokens": ["Die", "Luft", "wird", "klar", "und", "schnell", "voll", "Strah\u00b7len", ",", "Blitz", "und", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "ADJD", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es donnert, h\u00f6rt! erstaunt! Der Himmel thut sich auf,", "tokens": ["Es", "don\u00b7nert", ",", "h\u00f6rt", "!", "er\u00b7staunt", "!", "Der", "Him\u00b7mel", "thut", "sich", "auf", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$.", "VVPP", "$.", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Ewige erscheint auf seinem grauen Throne:", "tokens": ["Der", "E\u00b7wi\u00b7ge", "er\u00b7scheint", "auf", "sei\u00b7nem", "grau\u00b7en", "Thro\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein schreckliches Gew\u00f6lck, das brennend \u00fcber ihn,", "tokens": ["Ein", "schreck\u00b7li\u00b7ches", "Ge\u00b7w\u00f6lck", ",", "das", "bren\u00b7nend", "\u00fc\u00b7ber", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein grauses Richthaus w\u00f6lbt, steht um ihn her wie Mauren,", "tokens": ["Sein", "grau\u00b7ses", "Richt\u00b7haus", "w\u00f6lbt", ",", "steht", "um", "ihn", "her", "wie", "Mau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das unz\u00e4hlbare Volck des Reichs der Ewigkeit", "tokens": ["Das", "un\u00b7z\u00e4hl\u00b7ba\u00b7re", "Volck", "des", "Reichs", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Liegt vor dem strengen Stuhl voll Ehrfurcht auf dem Boden,", "tokens": ["Liegt", "vor", "dem", "stren\u00b7gen", "Stuhl", "voll", "Ehr\u00b7furcht", "auf", "dem", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die tiefste Stille herrscht. Des Richters Ausspruch schalt,", "tokens": ["Die", "tiefs\u00b7te", "Stil\u00b7le", "herrscht", ".", "Des", "Rich\u00b7ters", "Aus\u00b7spruch", "schalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der, wenn sein Haupt sich regt, die Feste selbst ersch\u00fcttert,", "tokens": ["Der", ",", "wenn", "sein", "Haupt", "sich", "regt", ",", "die", "Fes\u00b7te", "selbst", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sein Mund verdamt die grund verderbte Welt.", "tokens": ["Sein", "Mund", "ver\u00b7damt", "die", "grund", "ver\u00b7derb\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nunmehr giebt er Befehl den Dienern seiner Rache;", "tokens": ["Nun\u00b7mehr", "giebt", "er", "Be\u00b7fehl", "den", "Die\u00b7nern", "sei\u00b7ner", "Ra\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Ein neuer Schlag best\u00e4rcket den Befehl,", "tokens": ["Ein", "neu\u00b7er", "Schlag", "be\u00b7st\u00e4r\u00b7cket", "den", "Be\u00b7fehl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und dreymal schl\u00e4get er mit seinem eisern Scepter", "tokens": ["Und", "drey\u00b7mal", "schl\u00e4\u00b7get", "er", "mit", "sei\u00b7nem", "ei\u00b7sern", "Scep\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die gantz verworffne Erd, und ihr zerschelter Grund", "tokens": ["Die", "gantz", "ver\u00b7worff\u00b7ne", "Erd", ",", "und", "ihr", "zer\u00b7schel\u00b7ter", "Grund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mu\u00df bis zum heissen Kern ersch\u00fcttern und zerbersten.", "tokens": ["Mu\u00df", "bis", "zum", "heis\u00b7sen", "Kern", "er\u00b7sch\u00fct\u00b7tern", "und", "zer\u00b7bers\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "APPRART", "ADJA", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Des Abgrund Brunnen thun auf sein Gehei\u00df sich auf,", "tokens": ["Des", "Ab\u00b7grund", "Brun\u00b7nen", "thun", "auf", "sein", "Ge\u00b7hei\u00df", "sich", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und stossen \u00fcberall die tief verborgnen Wasser", "tokens": ["Und", "stos\u00b7sen", "\u00fc\u00b7be\u00b7rall", "die", "tief", "ver\u00b7borg\u00b7nen", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aus ihrem Schlund hervor, S\u00fcd, West, Ost, Nord tobt, st\u00fcrmt", "tokens": ["Aus", "ih\u00b7rem", "Schlund", "her\u00b7vor", ",", "S\u00fcd", ",", "West", ",", "Ost", ",", "Nord", "tobt", ",", "st\u00fcrmt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "NE", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Auf das erregte Meer voll Wuth von allen Seiten,", "tokens": ["Auf", "das", "er\u00b7reg\u00b7te", "Meer", "voll", "Wuth", "von", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "R\u00fchrt aus der Tiefe selbst die stillen Wasser vor,", "tokens": ["R\u00fchrt", "aus", "der", "Tie\u00b7fe", "selbst", "die", "stil\u00b7len", "Was\u00b7ser", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.22": {"text": "W\u00e4ltzt \u00fcber allen Strand gantz ungeheure Wellen,", "tokens": ["W\u00e4ltzt", "\u00fc\u00b7ber", "al\u00b7len", "Strand", "gantz", "un\u00b7ge\u00b7heu\u00b7re", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und \u00fcberschwemt die Welt. Sein donnernd starcker Arm", "tokens": ["Und", "\u00fc\u00b7bersc\u00b7hwemt", "die", "Welt", ".", "Sein", "don\u00b7nernd", "star\u00b7cker", "Arm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$.", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "F\u00fchrt durch die weite Luft indessen gantze Meere,", "tokens": ["F\u00fchrt", "durch", "die", "wei\u00b7te", "Luft", "in\u00b7des\u00b7sen", "gant\u00b7ze", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nun st\u00fcrtzt er sie herab mit Feuer untermengt;", "tokens": ["Nun", "st\u00fcrtzt", "er", "sie", "her\u00b7ab", "mit", "Feu\u00b7er", "un\u00b7ter\u00b7mengt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es braust ein steter Gu\u00df durch die pechschwartzen L\u00fcfte,", "tokens": ["Es", "braust", "ein", "ste\u00b7ter", "Gu\u00df", "durch", "die", "pech\u00b7schwart\u00b7zen", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die steigend zornge Fluth durchstr\u00f6met alles Land,", "tokens": ["Die", "stei\u00b7gend", "zorn\u00b7ge", "Fluth", "durch\u00b7str\u00f6\u00b7met", "al\u00b7les", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mord, Zeter, Ach und Weh steigt schon von allen Gr\u00e4ntzen.", "tokens": ["Mord", ",", "Ze\u00b7ter", ",", "Ach", "und", "Weh", "steigt", "schon", "von", "al\u00b7len", "Gr\u00e4nt\u00b7zen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ITJ", "KON", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Das jammervolle Volck, das heulend Rettung sucht,", "tokens": ["Das", "jam\u00b7mer\u00b7vol\u00b7le", "Volck", ",", "das", "heu\u00b7lend", "Ret\u00b7tung", "sucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ringt, schl\u00e4gt die H\u00e4nde nun verzweiflungsvoll zusammen,", "tokens": ["Ringt", ",", "schl\u00e4gt", "die", "H\u00e4n\u00b7de", "nun", "ver\u00b7zwei\u00b7flungs\u00b7voll", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und l\u00e4uft, vergeblich doch, auf steile Berge zu:", "tokens": ["Und", "l\u00e4uft", ",", "ver\u00b7geb\u00b7lich", "doch", ",", "auf", "stei\u00b7le", "Ber\u00b7ge", "zu", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "ADV", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Fische liegen nun auf hohen Ulmen B\u00e4umen", "tokens": ["Die", "Fi\u00b7sche", "lie\u00b7gen", "nun", "auf", "ho\u00b7hen", "Ul\u00b7men", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wo sonst die Taub allein ihr leichtes Nest gebaut.", "tokens": ["Wo", "sonst", "die", "Taub", "al\u00b7lein", "ihr", "leich\u00b7tes", "Nest", "ge\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der scheuen Gemsen Heer, das auf Geb\u00fcrgen klettert,", "tokens": ["Der", "scheu\u00b7en", "Gem\u00b7sen", "Heer", ",", "das", "auf", "Ge\u00b7b\u00fcr\u00b7gen", "klet\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Schwam in der tiefen Fluth. Die pr\u00e4chtig grossen St\u00e4dte", "tokens": ["Schwam", "in", "der", "tie\u00b7fen", "Fluth", ".", "Die", "pr\u00e4ch\u00b7tig", "gros\u00b7sen", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$.", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bedeckte jetzt ein Meer, das ohne Ufer war.", "tokens": ["Be\u00b7deck\u00b7te", "jetzt", "ein", "Meer", ",", "das", "oh\u00b7ne", "U\u00b7fer", "war", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es flogen durch die Welt des H\u00f6chsten Zornes Diener", "tokens": ["Es", "flo\u00b7gen", "durch", "die", "Welt", "des", "H\u00f6chs\u00b7ten", "Zor\u00b7nes", "Die\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und st\u00fcrtzten \u00fcberall die V\u00f6lcker in die See;", "tokens": ["Und", "st\u00fcrtz\u00b7ten", "\u00fc\u00b7be\u00b7rall", "die", "V\u00f6l\u00b7cker", "in", "die", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kein Retten ist nunmehr, was lebet, mu\u00df verderben,", "tokens": ["Kein", "Ret\u00b7ten", "ist", "nun\u00b7mehr", ",", "was", "le\u00b7bet", ",", "mu\u00df", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "$,", "PWS", "VVFIN", "$,", "VMFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Arche nur allein schwimt triumphirend her", "tokens": ["Die", "Ar\u00b7che", "nur", "al\u00b7lein", "schwimt", "tri\u00b7um\u00b7phi\u00b7rend", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Auf schaumerf\u00fcllt