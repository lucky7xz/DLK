{"dta.poem.13770": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Tyroler.  \n Im Jahre 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Euch selbst die Retter, seid ihr ausgezogen,", "tokens": ["Euch", "selbst", "die", "Ret\u00b7ter", ",", "seid", "ihr", "aus\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie helle B\u00e4che brechen aus den Kl\u00fcften;", "tokens": ["Wie", "hel\u00b7le", "B\u00e4\u00b7che", "bre\u00b7chen", "aus", "den", "Kl\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinunter schwindelt T\u00fccke nach den Schl\u00fcften,", "tokens": ["Hin\u00b7un\u00b7ter", "schwin\u00b7delt", "T\u00fc\u00b7cke", "nach", "den", "Schl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Freiheit Burg sind eure Felsenbogen.", "tokens": ["Der", "Frei\u00b7heit", "Burg", "sind", "eu\u00b7re", "Fel\u00b7sen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}