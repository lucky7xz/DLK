{"textgrid.poem.28044": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "59.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann bey der R\u00f6mer Zeit man sah mit Milche triffen", "tokens": ["Wann", "bey", "der", "R\u00f6\u00b7mer", "Zeit", "man", "sah", "mit", "Mil\u00b7che", "trif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "PIS", "VVFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Himmel, sah man auch, wie furchtsam hin sie liffen", "tokens": ["Den", "Him\u00b7mel", ",", "sah", "man", "auch", ",", "wie", "furcht\u00b7sam", "hin", "sie", "lif\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "PIS", "ADV", "$,", "PWAV", "ADJD", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Sybilliner-Buch und stellten \u00fcberall", "tokens": ["Zum", "Sy\u00b7bil\u00b7li\u00b7ner\u00b7Buch", "und", "stell\u00b7ten", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "VVFIN", "ADV"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Viel Wallen und Proce\u00df durch reiner Jungfern Zahl", "tokens": ["Viel", "Wal\u00b7len", "und", "Pro\u00b7ce\u00df", "durch", "rei\u00b7ner", "Jung\u00b7fern", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Au\u00df Vesta Kloster an. Was sollen wir bereiten,", "tokens": ["Au\u00df", "Ves\u00b7ta", "Klos\u00b7ter", "an", ".", "Was", "sol\u00b7len", "wir", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$.", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann von den Jungfern selbst treufft Milch bei unsren Zeiten?", "tokens": ["Wann", "von", "den", "Jung\u00b7fern", "selbst", "treufft", "Milch", "bei", "un\u00b7sren", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}