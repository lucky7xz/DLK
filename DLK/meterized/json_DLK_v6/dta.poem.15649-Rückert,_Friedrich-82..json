{"dta.poem.15649": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O f\u00fcr wieviel der Welt bist du zu Dank verpflichtet,", "tokens": ["O", "f\u00fcr", "wie\u00b7viel", "der", "Welt", "bist", "du", "zu", "Dank", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVFIN", "ART", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie f\u00fcr dich gesetzt, geordnet, eingerichtet.", "tokens": ["Was", "sie", "f\u00fcr", "dich", "ge\u00b7setzt", ",", "ge\u00b7ord\u00b7net", ",", "ein\u00b7ge\u00b7rich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Jahr' und Monate, der Tag' und Stunden Lauf;", "tokens": ["Der", "Jahr'", "und", "Mo\u00b7na\u00b7te", ",", "der", "Tag'", "und", "Stun\u00b7den", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Des Marktes Ma\u00df und Zahl, Gewicht, Vertrag und Kauf.", "tokens": ["Des", "Mark\u00b7tes", "Ma\u00df", "und", "Zahl", ",", "Ge\u00b7wicht", ",", "Ver\u00b7trag", "und", "Kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du brauchst es nicht zu thun, es ist f\u00fcr dich gethan,", "tokens": ["Du", "brauchst", "es", "nicht", "zu", "thun", ",", "es", "ist", "f\u00fcr", "dich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und keinen Augenblick brauchst du zu denken dran.", "tokens": ["Und", "kei\u00b7nen", "Au\u00b7gen\u00b7blick", "brauchst", "du", "zu", "den\u00b7ken", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch denke dran mit Dank in jedem Augenblick,", "tokens": ["Doch", "den\u00b7ke", "dran", "mit", "Dank", "in", "je\u00b7dem", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo der Gewohnheit Druck ber\u00fchret dein Genick.", "tokens": ["Wo", "der", "Ge\u00b7wohn\u00b7heit", "Druck", "be\u00b7r\u00fch\u00b7ret", "dein", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Druck ist \u00e4u\u00dferlich, damit im Innern frei,", "tokens": ["Der", "Druck", "ist", "\u00e4u\u00b7\u00dfer\u00b7lich", ",", "da\u00b7mit", "im", "In\u00b7nern", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Leben unber\u00fchrt, des Geistes Leben sei.", "tokens": ["Vom", "Le\u00b7ben", "un\u00b7be\u00b7r\u00fchrt", ",", "des", "Geis\u00b7tes", "Le\u00b7ben", "sei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}