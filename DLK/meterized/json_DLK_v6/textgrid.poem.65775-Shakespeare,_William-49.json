{"textgrid.poem.65775": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "49", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr jene Zeit, k\u00e4m je die Zeit heran,", "tokens": ["F\u00fcr", "je\u00b7ne", "Zeit", ",", "k\u00e4m", "je", "die", "Zeit", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da ich dich finster s\u00e4h auf meine M\u00e4ngel schmollen;", "tokens": ["Da", "ich", "dich", "fins\u00b7ter", "s\u00e4h", "auf", "mei\u00b7ne", "M\u00e4n\u00b7gel", "schmol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn deine Lieb' ihr h\u00f6chst Gebot getan,", "tokens": ["Wenn", "dei\u00b7ne", "Lieb'", "ihr", "h\u00f6chst", "Ge\u00b7bot", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "R\u00fccksichtlich klug bedacht die Rechnung abzuzollen:", "tokens": ["R\u00fcck\u00b7sicht\u00b7lich", "klug", "be\u00b7dacht", "die", "Rech\u00b7nung", "ab\u00b7zu\u00b7zol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "F\u00fcr jene Zeit, wenn fremd an mir dahin", "tokens": ["F\u00fcr", "je\u00b7ne", "Zeit", ",", "wenn", "fremd", "an", "mir", "da\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "ADJD", "APPR", "PPER", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du wandeln wirst, dein Sonnenauge kaum", "tokens": ["Du", "wan\u00b7deln", "wirst", ",", "dein", "Son\u00b7nen\u00b7au\u00b7ge", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VAFIN", "$,", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch hergewandt, entflohn der Liebe Sinn,", "tokens": ["Noch", "her\u00b7ge\u00b7wandt", ",", "ent\u00b7flohn", "der", "Lie\u00b7be", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gemessne F\u00f6rmlichkeit an ihrem Raum:", "tokens": ["Ge\u00b7mess\u00b7ne", "F\u00f6rm\u00b7lich\u00b7keit", "an", "ih\u00b7rem", "Raum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "F\u00fcr jene Zeit will ich geduldiglich", "tokens": ["F\u00fcr", "je\u00b7ne", "Zeit", "will", "ich", "ge\u00b7dul\u00b7dig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Hier aufs Bewu\u00dftsein meines Werts mich st\u00fctzen;", "tokens": ["Hier", "aufs", "Be\u00b7wu\u00df\u00b7tsein", "mei\u00b7nes", "Werts", "mich", "st\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ja, diese Hand erheb' ich wider mich,", "tokens": ["Ja", ",", "die\u00b7se", "Hand", "er\u00b7heb'", "ich", "wi\u00b7der", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Dein klares Recht an deinem Teil zu sch\u00fctzen.", "tokens": ["Dein", "kla\u00b7res", "Recht", "an", "dei\u00b7nem", "Teil", "zu", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nach des Gesetzes Kraft kannst du mich Armen fliehn;", "tokens": ["Nach", "des", "Ge\u00b7set\u00b7zes", "Kraft", "kannst", "du", "mich", "Ar\u00b7men", "fliehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VMFIN", "PPER", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ich dich lieben darf, ist mir kein Grund verliehn.", "tokens": ["Da\u00df", "ich", "dich", "lie\u00b7ben", "darf", ",", "ist", "mir", "kein", "Grund", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}