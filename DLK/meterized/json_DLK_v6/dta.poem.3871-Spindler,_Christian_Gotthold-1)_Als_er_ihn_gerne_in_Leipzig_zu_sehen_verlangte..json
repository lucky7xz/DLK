{"dta.poem.3871": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "1) Als er ihn gerne in Leipzig zu sehen  \n verlangte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Vater! allerliebster Vater! deine Huld ist un-", "tokens": ["Va\u00b7ter", "!", "al\u00b7ler\u00b7liebs\u00b7ter", "Va\u00b7ter", "!", "dei\u00b7ne", "Huld", "ist", "un"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "gemein,", "tokens": ["ge\u00b7mein", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und der \u00e4ltste deiner S\u00f6hne mu\u00df der Sor-", "tokens": ["Und", "der", "\u00e4lts\u00b7te", "dei\u00b7ner", "S\u00f6h\u00b7ne", "mu\u00df", "der", "Sor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "PPOSAT", "NN", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "gen Vorwurff seyn.", "tokens": ["gen", "Vor\u00b7wurff", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Blut und Adern, Seel und Hertz sind voll sehn-", "tokens": ["Blut", "und", "A\u00b7dern", ",", "Seel", "und", "Hertz", "sind", "voll", "sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "VAFIN", "ADJD", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "lichsten Verlangen,", "tokens": ["lichs\u00b7ten", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dich in unsern Plei\u00df-Athen mit Vergn\u00fcgen zu", "tokens": ["Dich", "in", "un\u00b7sern", "Plei\u00df\u00b7A\u00b7then", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "PTKZU"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "umfangen.", "tokens": ["um\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Eile doch mit deiner Reise, weil das Mehl im Cad", "tokens": ["Ei\u00b7le", "doch", "mit", "dei\u00b7ner", "Rei\u00b7se", ",", "weil", "das", "Mehl", "im", "Cad"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "gebricht,", "tokens": ["ge\u00b7bricht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Denn ich wei\u00df die Vater-Treue l\u00e4st mein sehnend", "tokens": ["Denn", "ich", "wei\u00df", "die", "Va\u00b7ter\u00b7Treue", "l\u00e4st", "mein", "seh\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "PPOSAT", "CARD"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Schmachten nicht.", "tokens": ["Schmach\u00b7ten", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Bleibe ferner deinem Kind mit der alten Huld ge-", "tokens": ["Blei\u00b7be", "fer\u00b7ner", "dei\u00b7nem", "Kind", "mit", "der", "al\u00b7ten", "Huld", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "wogen,", "tokens": ["wo\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Deine Liebe, deine Huld, sey mir ferner unent-", "tokens": ["Dei\u00b7ne", "Lie\u00b7be", ",", "dei\u00b7ne", "Huld", ",", "sey", "mir", "fer\u00b7ner", "un\u00b7ent"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "zogen,", "tokens": ["zo\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Bete wie bi\u00dfher so eifrigst, sorge wie bisher geschehn,", "tokens": ["Be\u00b7te", "wie", "bi\u00df\u00b7her", "so", "eif\u00b7rigst", ",", "sor\u00b7ge", "wie", "bis\u00b7her", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADV", "ADV", "ADJD", "$,", "VVFIN", "KOKOM", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "So wird meiner Wohlfahrt Ancker an den festen", "tokens": ["So", "wird", "mei\u00b7ner", "Wohl\u00b7fahrt", "An\u00b7cker", "an", "den", "fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN", "APPR", "ART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Hafen stehn.", "tokens": ["Ha\u00b7fen", "stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Denn die Fremde, die zwar selbst hier ein Fremd-", "tokens": ["Denn", "die", "Frem\u00b7de", ",", "die", "zwar", "selbst", "hier", "ein", "Frem\u00b7d"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "ART", "TRUNC"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "ling auf der Erden,", "tokens": ["ling", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Wird mir ohne deine Huld nur zum bittern Mara", "tokens": ["Wird", "mir", "oh\u00b7ne", "dei\u00b7ne", "Huld", "nur", "zum", "bit\u00b7tern", "Ma\u00b7ra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "werden.", "tokens": ["wer\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Dem GOtt, so du redlich dienst, der verl\u00e4st uns", "tokens": ["Dem", "Gott", ",", "so", "du", "red\u00b7lich", "dienst", ",", "der", "ver\u00b7l\u00e4st", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADJD", "ADV", "$,", "PRELS", "VVFIN", "PPER"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "beyde nicht,", "tokens": ["bey\u00b7de", "nicht", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.26": {"text": "Ob es donnert, kracht und blitzet, ob so Mast", "tokens": ["Ob", "es", "don\u00b7nert", ",", "kracht", "und", "blit\u00b7zet", ",", "ob", "so", "Mast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "ADV", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.27": {"text": "als Segel bricht.", "tokens": ["als", "Se\u00b7gel", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Frommer Vater! Buchholtz kennt deinen Geist", "tokens": ["From\u00b7mer", "Va\u00b7ter", "!", "Buch\u00b7holtz", "kennt", "dei\u00b7nen", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.29": {"text": "und deine Gaben,", "tokens": ["und", "dei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.30": {"text": "Deine gr\u00f6ste Wei\u00dfheit ist den Erl\u00f6ser lieb zu haben,", "tokens": ["Dei\u00b7ne", "gr\u00f6s\u00b7te", "Wei\u00df\u00b7heit", "ist", "den", "Er\u00b7l\u00f6\u00b7ser", "lieb", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Der ist auch dein Schutz und Helffer, dieser steht", "tokens": ["Der", "ist", "auch", "dein", "Schutz", "und", "Helf\u00b7fer", ",", "die\u00b7ser", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.32": {"text": "dir kr\u00e4fftig bey,", "tokens": ["dir", "kr\u00e4ff\u00b7tig", "bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Der hilfft Amt und B\u00fcrde tragen, der macht dich", "tokens": ["Der", "hilfft", "Amt", "und", "B\u00fcr\u00b7de", "tra\u00b7gen", ",", "der", "macht", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "VVINF", "$,", "PRELS", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.34": {"text": "von Kummer frey.", "tokens": ["von", "Kum\u00b7mer", "frey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.35": {"text": "Und gesetzt, da\u00df seine Hand dich mit Hitz und Angst", "tokens": ["Und", "ge\u00b7setzt", ",", "da\u00df", "sei\u00b7ne", "Hand", "dich", "mit", "Hitz", "und", "Angst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "betr\u00fcbet,", "tokens": ["be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Schl\u00e4gt er doch nicht da\u00df er z\u00fcrnt; nein, er schl\u00e4-", "tokens": ["Schl\u00e4gt", "er", "doch", "nicht", "da\u00df", "er", "z\u00fcrnt", ";", "nein", ",", "er", "schl\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "KOUS", "PPER", "VVFIN", "$.", "PTKANT", "$,", "PPER", "TRUNC"], "meter": "+--+--++-+", "measure": "dactylic.di.plus"}, "line.38": {"text": "get, weil er liebet,", "tokens": ["get", ",", "weil", "er", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Darum, ", "tokens": ["Da\u00b7rum", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.40": {"text": "und Feind,", "tokens": ["und", "Feind", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "La\u00df dirs nicht zu sauer werden, geh, geniesse deinen", "tokens": ["La\u00df", "dirs", "nicht", "zu", "sau\u00b7er", "wer\u00b7den", ",", "geh", ",", "ge\u00b7nies\u00b7se", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "PIS", "PTKNEG", "PTKA", "ADJD", "VAINF", "$,", "VVFIN", "$,", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.42": {"text": "Freund,", "tokens": ["Freund", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Sorge nicht vor deinen Sohn, GOtt wird selb-", "tokens": ["Sor\u00b7ge", "nicht", "vor", "dei\u00b7nen", "Sohn", ",", "Gott", "wird", "selb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,", "NN", "VAFIN", "TRUNC"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.44": {"text": "sten vor ihn sorgen,", "tokens": ["sten", "vor", "ihn", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.45": {"text": "Wer wei\u00df, wo mein Gl\u00fccke bl\u00fcht, auf den bald", "tokens": ["Wer", "wei\u00df", ",", "wo", "mein", "Gl\u00fc\u00b7cke", "bl\u00fcht", ",", "auf", "den", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "ART", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.46": {"text": "und frohen Morgen.", "tokens": ["und", "fro\u00b7hen", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.47": {"text": "Hoffnung machet nicht zu schanden; endlich trifft", "tokens": ["Hoff\u00b7nung", "ma\u00b7chet", "nicht", "zu", "schan\u00b7den", ";", "end\u00b7lich", "trifft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.48": {"text": "dieselbe ein,", "tokens": ["die\u00b7sel\u00b7be", "ein", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.49": {"text": "Wer wei\u00df, welcher Ort im Lande wird mein Eben-", "tokens": ["Wer", "wei\u00df", ",", "wel\u00b7cher", "Ort", "im", "Lan\u00b7de", "wird", "mein", "E\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAT", "NN", "APPRART", "NN", "VAFIN", "PPOSAT", "TRUNC"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.50": {"text": "Ezer seyn.", "tokens": ["E\u00b7zer", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.51": {"text": "Nach der Unlust folget Lust, nach den Dornen kom-", "tokens": ["Nach", "der", "Un\u00b7lust", "fol\u00b7get", "Lust", ",", "nach", "den", "Dor\u00b7nen", "kom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.52": {"text": "men Rosen,", "tokens": ["men", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.53": {"text": "Nach Egyptens Dienstbarkeit, das so l\u00e4ngst er-", "tokens": ["Nach", "E\u00b7gyp\u00b7tens", "Dienst\u00b7bar\u00b7keit", ",", "das", "so", "l\u00e4ngst", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "PRELS", "ADV", "ADV", "TRUNC"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.54": {"text": "w\u00fcnschte Gosen.", "tokens": ["w\u00fcnschte", "Go\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.55": {"text": "Lebe wohl, ", "tokens": ["Le\u00b7be", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.56": {"text": "Thon,", "tokens": ["Thon", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.57": {"text": "Den die schwache Muse singet, und dein dir er-", "tokens": ["Den", "die", "schwa\u00b7che", "Mu\u00b7se", "sin\u00b7get", ",", "und", "dein", "dir", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "PPOSAT", "PPER", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.58": {"text": "gebner Sohn.", "tokens": ["geb\u00b7ner", "Sohn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}