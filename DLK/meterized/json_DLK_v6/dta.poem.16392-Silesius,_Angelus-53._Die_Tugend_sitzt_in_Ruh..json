{"dta.poem.16392": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "53.  Die Tugend sitzt in Ruh.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch wo du Tugend wilst mit Arbeit un\u0304 mit M\u00fch/", "tokens": ["Mensch", "wo", "du", "Tu\u00b7gend", "wilst", "mit", "Ar\u00b7beit", "un\u0304", "mit", "M\u00fch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "PPER", "NN", "VMFIN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "So hastu sie noch nicht/ du kriegest noch umb sie.", "tokens": ["So", "has\u00b7tu", "sie", "noch", "nicht", "/", "du", "krie\u00b7gest", "noch", "umb", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "$(", "PPER", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}