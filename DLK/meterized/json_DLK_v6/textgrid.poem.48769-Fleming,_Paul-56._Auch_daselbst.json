{"textgrid.poem.48769": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "56. Auch daselbst", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du aller Treflichkeit des ganzen Perserlandes", "tokens": ["Du", "al\u00b7ler", "Tref\u00b7lich\u00b7keit", "des", "gan\u00b7zen", "Per\u00b7ser\u00b7lan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in diesen engen Raum zusammenbrachte Zier,", "tokens": ["in", "die\u00b7sen", "en\u00b7gen", "Raum", "zu\u00b7sam\u00b7men\u00b7brach\u00b7te", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gro\u00df, seltsam, herlich, reich; ich neige mich vor dir,", "tokens": ["gro\u00df", ",", "selt\u00b7sam", ",", "her\u00b7lich", ",", "reich", ";", "ich", "nei\u00b7ge", "mich", "vor", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nim diesen tiefen Gru\u00df zum Zeichen eines Pfandes", "tokens": ["nim", "die\u00b7sen", "tie\u00b7fen", "Gru\u00df", "zum", "Zei\u00b7chen", "ei\u00b7nes", "Pfan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PDAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "f\u00fcr deine Gotheit an, die eine gleiches Standes", "tokens": ["f\u00fcr", "dei\u00b7ne", "Got\u00b7heit", "an", ",", "die", "ei\u00b7ne", "glei\u00b7ches", "Stan\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hier nicht hat und nicht wei\u00df; und sei so g\u00fcnstig mir,", "tokens": ["hier", "nicht", "hat", "und", "nicht", "wei\u00df", ";", "und", "sei", "so", "g\u00fcns\u00b7tig", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VAFIN", "KON", "PTKNEG", "VVFIN", "$.", "KON", "VAFIN", "ADV", "ADJD", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df ich mich setze hin an diesem Wasser hier,", "tokens": ["da\u00df", "ich", "mich", "set\u00b7ze", "hin", "an", "die\u00b7sem", "Was\u00b7ser", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "APPR", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das Gold hei\u00dft und Gold f\u00fchrt im Schutze seines Sandes.", "tokens": ["das", "Gold", "hei\u00dft", "und", "Gold", "f\u00fchrt", "im", "Schut\u00b7ze", "sei\u00b7nes", "San\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Indem ich ", "tokens": ["In\u00b7dem", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "und auf mein Vaterland so schleunig bin bedacht,", "tokens": ["und", "auf", "mein", "Va\u00b7ter\u00b7land", "so", "schleu\u00b7nig", "bin", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so mu\u00df ich gleichwol dir die kleine Zeit verg\u00fcnnen", "tokens": ["so", "mu\u00df", "ich", "gleich\u00b7wol", "dir", "die", "klei\u00b7ne", "Zeit", "ver\u00b7g\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "und froh sein \u00fcber dir. So bald der Tag erwacht,", "tokens": ["und", "froh", "sein", "\u00fc\u00b7ber", "dir", ".", "So", "bald", "der", "Tag", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAINF", "APPR", "PPER", "$.", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so bleibest du zwar hier, ich mache mich von hinnen,", "tokens": ["so", "blei\u00b7best", "du", "zwar", "hier", ",", "ich", "ma\u00b7che", "mich", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "VVFIN", "PRF", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "doch werd' ich deiner Gunst mich ewiglich entsinnen.", "tokens": ["doch", "werd'", "ich", "dei\u00b7ner", "Gunst", "mich", "e\u00b7wig\u00b7lich", "ent\u00b7sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}