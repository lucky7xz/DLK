{"textgrid.poem.56537": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du denn, da\u00df ich ganz zu Grunde geh?", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du denn, da\u00df ich ganz zu Grunde geh?", "tokens": ["Willst", "du", "denn", ",", "da\u00df", "ich", "ganz", "zu", "Grun\u00b7de", "geh", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wei\u00dft, du schlugst mich oft schon,", "tokens": ["Du", "wei\u00dft", ",", "du", "schlugst", "mich", "oft", "schon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wenn ich dich bat um einen Strahl der H\u00f6h.", "tokens": ["Wenn", "ich", "dich", "bat", "um", "ei\u00b7nen", "Strahl", "der", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich trug's, denn endlich hofft ich Lohn.", "tokens": ["Ich", "trug's", ",", "denn", "end\u00b7lich", "hofft", "ich", "Lohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Warum von neuem folterst du mich jetzt", "tokens": ["Wa\u00b7rum", "von", "neu\u00b7em", "fol\u00b7terst", "du", "mich", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo ich die ganze Nacht durch mit dir rang?", "tokens": ["Wo", "ich", "die", "gan\u00b7ze", "Nacht", "durch", "mit", "dir", "rang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was hab ich dich denn gar zu sehr verletzt.", "tokens": ["Was", "hab", "ich", "dich", "denn", "gar", "zu", "sehr", "ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "ADV", "ADV", "PTKA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich will Erh\u00f6rung, ich fleh nicht mehr lang!", "tokens": ["Ich", "will", "Er\u00b7h\u00f6\u00b7rung", ",", "ich", "fleh", "nicht", "mehr", "lang", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bist du der Liebe Gott, so gib mir Teil an ihr", "tokens": ["Bist", "du", "der", "Lie\u00b7be", "Gott", ",", "so", "gib", "mir", "Teil", "an", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,", "ADV", "VVIMP", "PPER", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zeig mir nicht blo\u00df Schemen, die entglitten.", "tokens": ["Und", "zeig", "mir", "nicht", "blo\u00df", "Sche\u00b7men", ",", "die", "ent\u00b7glit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hoffe noch: ein Gl\u00fcckstrahl neigt sich mir,", "tokens": ["Ich", "hof\u00b7fe", "noch", ":", "ein", "Gl\u00fccks\u00b7trahl", "neigt", "sich", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "ART", "NN", "VVFIN", "PRF", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch kann ich nicht mehr warten, lange bitten.", "tokens": ["Doch", "kann", "ich", "nicht", "mehr", "war\u00b7ten", ",", "lan\u00b7ge", "bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}