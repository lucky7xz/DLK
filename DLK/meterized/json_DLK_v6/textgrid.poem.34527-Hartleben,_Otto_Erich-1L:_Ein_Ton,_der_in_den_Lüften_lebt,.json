{"textgrid.poem.34527": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Ton, der in den L\u00fcften lebt,", "genre": "verse", "period": "N.A.", "pub_year": 1893, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Ton, der in den L\u00fcften lebt,", "tokens": ["Ein", "Ton", ",", "der", "in", "den", "L\u00fcf\u00b7ten", "lebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hoch \u00fcber allem Dasein schwebt,", "tokens": ["hoch", "\u00fc\u00b7ber", "al\u00b7lem", "Da\u00b7sein", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "den doch des Menschen Ohr erst h\u00f6rt,", "tokens": ["den", "doch", "des", "Men\u00b7schen", "Ohr", "erst", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wenns schon kein fremder Laut mehr st\u00f6rt,", "tokens": ["wenns", "schon", "kein", "frem\u00b7der", "Laut", "mehr", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "ADJA", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "wenn Todesschweigen schon die Welt", "tokens": ["wenn", "To\u00b7des\u00b7schwei\u00b7gen", "schon", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "gebannt in ihren Tiefen h\u00e4lt ...", "tokens": ["ge\u00b7bannt", "in", "ih\u00b7ren", "Tie\u00b7fen", "h\u00e4lt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Ton der Freude, \u00fcberreich", "tokens": ["Ein", "Ton", "der", "Freu\u00b7de", ",", "\u00fc\u00b7berr\u00b7eich"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "bald trotzig froh, bald lieb und weich \u2013", "tokens": ["bald", "trot\u00b7zig", "froh", ",", "bald", "lieb", "und", "weich", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der letzte Ton. Auch du darfst einst ihn h\u00f6ren", "tokens": ["der", "letz\u00b7te", "Ton", ".", "Auch", "du", "darfst", "einst", "ihn", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "PPER", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 dann wird kein Leid, kein Schmerz die Seele mehr beth\u00f6ren!", "tokens": ["\u2013", "dann", "wird", "kein", "Leid", ",", "kein", "Schmerz", "die", "See\u00b7le", "mehr", "be\u00b7th\u00f6\u00b7ren", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PIAT", "NN", "$,", "PIAT", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}