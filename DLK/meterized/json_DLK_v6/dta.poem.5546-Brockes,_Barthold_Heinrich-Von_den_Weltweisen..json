{"dta.poem.5546": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erfordert es demnach nicht unsre Schuldigkeit", "tokens": ["Er\u00b7for\u00b7dert", "es", "dem\u00b7nach", "nicht", "uns\u00b7re", "Schul\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns zu bestreben, das zu kennen,", "tokens": ["Uns", "zu", "be\u00b7stre\u00b7ben", ",", "das", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PRELS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "(so viel uns die Vernunfft dazu will Kr\u00e4ffte g\u00f6nnen)", "tokens": ["(", "so", "viel", "uns", "die", "Ver\u00b7nunfft", "da\u00b7zu", "will", "Kr\u00e4ff\u00b7te", "g\u00f6n\u00b7nen", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPER", "ART", "NN", "PAV", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was diese Welt? Wer der, der alle Dinge macht?", "tokens": ["Was", "die\u00b7se", "Welt", "?", "Wer", "der", ",", "der", "al\u00b7le", "Din\u00b7ge", "macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "$.", "PWS", "ART", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer uns den Athem giebt? Wer uns hervor gebracht?", "tokens": ["Wer", "uns", "den", "A\u00b7them", "giebt", "?", "Wer", "uns", "her\u00b7vor", "ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$.", "PWS", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das, was ein C\u00f6rper sey, und das, was eine Seele;", "tokens": ["Das", ",", "was", "ein", "C\u00f6r\u00b7per", "sey", ",", "und", "das", ",", "was", "ei\u00b7ne", "See\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VAFIN", "$,", "KON", "PDS", "$,", "PRELS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer mit dem ird\u2019schen Stoff ein him\u0303lisch Feur verm\u00e4hle;", "tokens": ["Wer", "mit", "dem", "ird'\u00b7schen", "Stoff", "ein", "him\u0303\u00b7lisch", "Feur", "ver\u00b7m\u00e4h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und was das sey, da\u00df von uns soll vergehen?", "tokens": ["Und", "was", "das", "sey", ",", "da\u00df", "von", "uns", "soll", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "VAFIN", "$,", "KOUS", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auch was das sey, das ewig wird bestehen?", "tokens": ["Auch", "was", "das", "sey", ",", "das", "e\u00b7wig", "wird", "be\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PDS", "VAFIN", "$,", "PRELS", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}