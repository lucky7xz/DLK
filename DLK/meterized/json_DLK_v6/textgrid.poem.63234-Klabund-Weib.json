], "pos": ["ART", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Riefen H\u00fcndin schon.", "tokens": ["Rie\u00b7fen", "H\u00fcn\u00b7din", "schon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Lieg ich dir im Schosse", "tokens": ["Lieg", "ich", "dir", "im", "Schos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gramzerkrampft:", "tokens": ["Gram\u00b7zer\u00b7krampft", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "F\u00fchl ich, wie die grosse", "tokens": ["F\u00fchl", "ich", ",", "wie", "die", "gros\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Welt sich selbst zerstampft.", "tokens": ["Welt", "sich", "selbst", "zer\u00b7stampft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}