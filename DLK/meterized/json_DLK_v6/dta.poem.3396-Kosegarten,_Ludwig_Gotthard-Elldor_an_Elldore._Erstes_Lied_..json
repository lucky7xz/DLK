{"dta.poem.3396": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elldor an Elldore.  \n  Erstes Lied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch eine Nacht! Wie grauenvoll! wie", "tokens": ["Welch", "ei\u00b7ne", "Nacht", "!", "Wie", "grau\u00b7en\u00b7voll", "!", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PIAT", "ART", "NN", "$.", "PWAV", "ADJD", "$.", "PWAV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "dunkel!", "tokens": ["dun\u00b7kel", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Von Sturm und Schlag wie schauerlich!", "tokens": ["Von", "Sturm", "und", "Schlag", "wie", "schau\u00b7er\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber schritt getrost durch ihr erebisch Dunkel;", "tokens": ["Ich", "a\u00b7ber", "schritt", "ge\u00b7trost", "durch", "ihr", "e\u00b7re\u00b7bisch", "Dun\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die starke Liebe schirmte mich.", "tokens": ["Die", "star\u00b7ke", "Lie\u00b7be", "schirm\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich schritt getrost hindurch. Ich h\u00e4tte nicht", "tokens": ["Ich", "schritt", "ge\u00b7trost", "hin\u00b7durch", ".", "Ich", "h\u00e4t\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PAV", "$.", "PPER", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gezittert,", "tokens": ["ge\u00b7zit\u00b7tert", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und h\u00e4tten um mich her die Winde Tod geheult.", "tokens": ["Und", "h\u00e4t\u00b7ten", "um", "mich", "her", "die", "Win\u00b7de", "Tod", "ge\u00b7heult", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PRF", "APZR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und h\u00e4tte Gottes Blitz den Wald um mich zer-", "tokens": ["Und", "h\u00e4t\u00b7te", "Got\u00b7tes", "Blitz", "den", "Wald", "um", "mich", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "NN", "ART", "NN", "APPR", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "splittert \u2014", "tokens": ["split\u00b7tert"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Ich w\u00e4r' getrost hindurch geeilt.", "tokens": ["Ich", "w\u00e4r'", "ge\u00b7trost", "hin\u00b7durch", "ge\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und h\u00e4tte Mord auf mich mit jedem Schritt", "tokens": ["Und", "h\u00e4t\u00b7te", "Mord", "auf", "mich", "mit", "je\u00b7dem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "NN", "APPR", "PRF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gelauert,", "tokens": ["ge\u00b7lau\u00b7ert", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und h\u00e4tte \u00fcber mir der Himmel roth gebrannt,", "tokens": ["Und", "h\u00e4t\u00b7te", "\u00fc\u00b7ber", "mir", "der", "Him\u00b7mel", "roth", "ge\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00e4re unter mir der Abgrund aufgeschauert;", "tokens": ["Und", "w\u00e4\u00b7re", "un\u00b7ter", "mir", "der", "Ab\u00b7grund", "auf\u00b7ge\u00b7schau\u00b7ert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Getrost w\u00e4r' ich hindurch, getrost zu dir gerannt.", "tokens": ["Ge\u00b7trost", "w\u00e4r'", "ich", "hin\u00b7durch", ",", "ge\u00b7trost", "zu", "dir", "ge\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PAV", "$,", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zu dir! zu dir! Dein erstes Grussgeflister,", "tokens": ["Zu", "dir", "!", "zu", "dir", "!", "Dein", "ers\u00b7tes", "Gruss\u00b7ge\u00b7flis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "APPR", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein erster leiser Handdruck, ach!", "tokens": ["Dein", "ers\u00b7ter", "lei\u00b7ser", "Hand\u00b7druck", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein volles feuriges Umfahn im Rabend\u00fcster", "tokens": ["Dein", "vol\u00b7les", "feu\u00b7ri\u00b7ges", "Um\u00b7fahn", "im", "Ra\u00b7ben\u00b7d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mitternacht tilgt' all mein Ungemach.", "tokens": ["Der", "Mit\u00b7ter\u00b7nacht", "tilgt'", "all", "mein", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Zu s\u00fcsse Nacht! Zu rasch verprasste Stunden!", "tokens": ["Zu", "s\u00fcs\u00b7se", "Nacht", "!", "Zu", "rasch", "ver\u00b7prass\u00b7te", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu schnell verrauschte Trunkenheit!", "tokens": ["Zu", "schnell", "ver\u00b7rauschte", "Trun\u00b7ken\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Herz, Herz, wie dass du nicht vom Staube los-", "tokens": ["Herz", ",", "Herz", ",", "wie", "dass", "du", "nicht", "vom", "Stau\u00b7be", "los"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KOKOM", "KOUS", "PPER", "PTKNEG", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gewunden", "tokens": ["ge\u00b7wun\u00b7den"], "token_info": ["word"], "pos": ["VAPP"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Mit ihr empor dich schwangst ins All der Seligkeit!", "tokens": ["Mit", "ihr", "em\u00b7por", "dich", "schwangst", "ins", "All", "der", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Auf ihrem Lager lieblich hingegossen,", "tokens": ["Auf", "ih\u00b7rem", "La\u00b7ger", "lieb\u00b7lich", "hin\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie duftete die junge Rose mir!", "tokens": ["Wie", "duf\u00b7te\u00b7te", "die", "jun\u00b7ge", "Ro\u00b7se", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie gl\u00fchte sie! wie thaute sie! wie flossen", "tokens": ["Wie", "gl\u00fch\u00b7te", "sie", "!", "wie", "thau\u00b7te", "sie", "!", "wie", "flos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rings um sie Frisch' und F\u00fcll' und lechzende Be-", "tokens": ["Rings", "um", "sie", "Frisch'", "und", "F\u00fcll'", "und", "lech\u00b7zen\u00b7de", "Be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "NE", "KON", "NN", "KON", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "gier!", "tokens": ["gier", "!"], "token_info": ["word", "punct"], "pos": ["VVIMP", "$."], "meter": "-", "measure": "single.down"}}, "stanza.7": {"line.1": {"text": "Von ihren Armen sanft hinabgezogen,", "tokens": ["Von", "ih\u00b7ren", "Ar\u00b7men", "sanft", "hin\u00b7ab\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinabgesunken an ihr schlagend Herz,", "tokens": ["Hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", "an", "ihr", "schla\u00b7gend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Itzt steigend, sinkend itzt mit ihres Busens Wogen,", "tokens": ["Itzt", "stei\u00b7gend", ",", "sin\u00b7kend", "itzt", "mit", "ih\u00b7res", "Bu\u00b7sens", "Wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie k\u00e4mpft' ich zwischen Lust und Schmerz!", "tokens": ["Wie", "k\u00e4mpft'", "ich", "zwi\u00b7schen", "Lust", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie strebten meine Kr\u00e4fte, ha! wie dr\u00e4ngten", "tokens": ["Wie", "streb\u00b7ten", "mei\u00b7ne", "Kr\u00e4f\u00b7te", ",", "ha", "!", "wie", "dr\u00e4ng\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "ITJ", "$.", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die M\u00e4chtigen sich hin zu ihr!", "tokens": ["Die", "M\u00e4ch\u00b7ti\u00b7gen", "sich", "hin", "zu", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dass sie nicht der Tugend Riegel sprengten,", "tokens": ["Und", "dass", "sie", "nicht", "der", "Tu\u00b7gend", "Rie\u00b7gel", "spreng\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Elldore, das verdanke dir!", "tokens": ["Ell\u00b7do\u00b7re", ",", "das", "ver\u00b7dan\u00b7ke", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Das danke dieser hellen Morgenr\u00f6the", "tokens": ["Das", "dan\u00b7ke", "die\u00b7ser", "hel\u00b7len", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Unschuld, die dein Angesicht", "tokens": ["Von", "Un\u00b7schuld", ",", "die", "dein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So r\u00fchrend schm\u00fcckt, die mich so flehend flehte:", "tokens": ["So", "r\u00fch\u00b7rend", "schm\u00fcckt", ",", "die", "mich", "so", "fle\u00b7hend", "fleh\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201emein Elldor, ach zerst\u00f6re nicht!\u201c", "tokens": ["\u201e", "mein", "Ell\u00b7dor", ",", "ach", "zer\u00b7st\u00f6\u00b7re", "nicht", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NE", "$,", "ITJ", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Das danke deinem leisen Wimmern:", "tokens": ["Das", "dan\u00b7ke", "dei\u00b7nem", "lei\u00b7sen", "Wim\u00b7mern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201emein Auserw\u00e4hlter, ich bin dein!", "tokens": ["\u201e", "mein", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ter", ",", "ich", "bin", "dein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edoch k\u00f6nntest du dein Heiligthum zertr\u00fcmmern?", "tokens": ["\u201e", "doch", "k\u00f6nn\u00b7test", "du", "dein", "Hei\u00b7lig\u00b7thum", "zer\u00b7tr\u00fcm\u00b7mern", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u201eharr' aus! Einst werd' ich ganz und ewig deine", "tokens": ["\u201e", "harr'", "aus", "!", "Einst", "werd'", "ich", "ganz", "und", "e\u00b7wig", "dei\u00b7ne"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ADV", "KON", "ADJD", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "seyn.\u201c", "tokens": ["seyn", ".", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["VAINF", "$.", "$("], "meter": "+", "measure": "single.up"}}, "stanza.11": {"line.1": {"text": "Ja, du bist mein. Du bist an mich gebunden,", "tokens": ["Ja", ",", "du", "bist", "mein", ".", "Du", "bist", "an", "mich", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPOSAT", "$.", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Banden, die kein Arm zerbricht.", "tokens": ["Mit", "Ban\u00b7den", ",", "die", "kein", "Arm", "zer\u00b7bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm bald, o seligste der Stunden,", "tokens": ["Komm", "bald", ",", "o", "se\u00b7ligs\u00b7te", "der", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "FM", "FM", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Darin Elldore mich mit Gattinarm umflicht.", "tokens": ["Da\u00b7rin", "Ell\u00b7do\u00b7re", "mich", "mit", "Gat\u00b7ti\u00b7narm", "um\u00b7flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "