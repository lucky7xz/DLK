{"textgrid.poem.41997": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dacht' an dich, und sieh, \u00e4therisch schwang", "tokens": ["Ich", "dacht'", "an", "dich", ",", "und", "sieh", ",", "\u00e4t\u00b7he\u00b7risch", "schwang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Petrarch mit seiner Laute sich hernieder,", "tokens": ["Pe\u00b7trarch", "mit", "sei\u00b7ner", "Lau\u00b7te", "sich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sagte: Nimm und singe meine Lieder,", "tokens": ["Und", "sag\u00b7te", ":", "Nimm", "und", "sin\u00b7ge", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und feire die, die ich im Leben sang.", "tokens": ["Und", "fei\u00b7re", "die", ",", "die", "ich", "im", "Le\u00b7ben", "sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, rief ich aus, das dunkle Grab verschlang", "tokens": ["Ach", ",", "rief", "ich", "aus", ",", "das", "dunk\u00b7le", "Grab", "ver\u00b7schlang"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon l\u00e4ngst die Form, den Reiz der zarten Glieder;", "tokens": ["Schon", "l\u00e4ngst", "die", "Form", ",", "den", "Reiz", "der", "zar\u00b7ten", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Himmel hob der sel'ge Geist sich wieder,", "tokens": ["Zum", "Him\u00b7mel", "hob", "der", "sel'\u00b7ge", "Geist", "sich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4\u00dft den Ruhm des Staubes ohne Dank.", "tokens": ["Und", "l\u00e4\u00dft", "den", "Ruhm", "des", "Stau\u00b7bes", "oh\u00b7ne", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Thor! rief der Geist; was starb ist nicht verloren,", "tokens": ["Thor", "!", "rief", "der", "Geist", ";", "was", "starb", "ist", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$.", "PWS", "VVFIN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Einst war sie mir, jetzt ist sie dir geboren;", "tokens": ["Einst", "war", "sie", "mir", ",", "jetzt", "ist", "sie", "dir", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nichts ward an ihr verwandelt als der Name;", "tokens": ["Nichts", "ward", "an", "ihr", "ver\u00b7wan\u00b7delt", "als", "der", "Na\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVPP", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Du sollst mein Lied und meine Lieb' erneun.", "tokens": ["Du", "sollst", "mein", "Lied", "und", "mei\u00b7ne", "Lieb'", "er\u00b7neun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}