{"textgrid.poem.44986": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[freiheitsverse herzubeten]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freiheitsverse herzubeten,", "tokens": ["Frei\u00b7heits\u00b7ver\u00b7se", "her\u00b7zu\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Scheint Gedicht mir im Gedicht,", "tokens": ["Scheint", "Ge\u00b7dicht", "mir", "im", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Freiheit braucht Musketen,", "tokens": ["Denn", "die", "Frei\u00b7heit", "braucht", "Mus\u00b7ke\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme, aber F\u00fc\u00dfe nicht.", "tokens": ["Ar\u00b7me", ",", "a\u00b7ber", "F\u00fc\u00b7\u00dfe", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}