{"dta.poem.14735": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die weinende Braut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl Jahrlang sah ich von den H\u00f6hen", "tokens": ["Wohl", "Jahr\u00b7lang", "sah", "ich", "von", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gr\u00fc\u00dfte Dich viel tausendmal,", "tokens": ["Und", "gr\u00fc\u00df\u00b7te", "Dich", "viel", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unten sah ich Viele gehen,", "tokens": ["Und", "un\u00b7ten", "sah", "ich", "Vie\u00b7le", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch Du erschienst nicht in dem Thal.", "tokens": ["Doch", "Du", "er\u00b7schienst", "nicht", "in", "dem", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mancher Lenz mit bunten Scherzen", "tokens": ["Und", "man\u00b7cher", "Lenz", "mit", "bun\u00b7ten", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kam und verflog im lust'gen Lauf,", "tokens": ["Kam", "und", "ver\u00b7flog", "im", "lust'\u00b7gen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch ach! in dem betrog'nen Herzen", "tokens": ["Doch", "ach", "!", "in", "dem", "be\u00b7tro\u00b7g'\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Geht niemals mehr der Fr\u00fchling auf.", "tokens": ["Geht", "nie\u00b7mals", "mehr", "der", "Fr\u00fch\u00b7ling", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}