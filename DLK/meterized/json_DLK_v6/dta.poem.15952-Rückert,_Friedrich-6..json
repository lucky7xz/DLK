{"dta.poem.15952": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bruchst\u00fcck, welches auf sein Ganzes sich besinnt,", "tokens": ["Ein", "Bruchs\u00b7t\u00fcck", ",", "wel\u00b7ches", "auf", "sein", "Gan\u00b7zes", "sich", "be\u00b7sinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erg\u00e4nzung immer sucht, und nimmer sie gewinnt:", "tokens": ["Er\u00b7g\u00e4n\u00b7zung", "im\u00b7mer", "sucht", ",", "und", "nim\u00b7mer", "sie", "ge\u00b7winnt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$,", "KON", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So findet sich der Mensch, wie er wird sein bewu\u00dft;", "tokens": ["So", "fin\u00b7det", "sich", "der", "Mensch", ",", "wie", "er", "wird", "sein", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an den Menschen kn\u00fcpft den Menschen diese Lust.", "tokens": ["Und", "an", "den", "Men\u00b7schen", "kn\u00fcpft", "den", "Men\u00b7schen", "die\u00b7se", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Ganzes werden nie Bruchst\u00fccke gro\u00df und klein;", "tokens": ["Ein", "Gan\u00b7zes", "wer\u00b7den", "nie", "Bruchs\u00b7t\u00fc\u00b7cke", "gro\u00df", "und", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erg\u00e4nzung findet doch die Welt in Gott allein.", "tokens": ["Er\u00b7g\u00e4n\u00b7zung", "fin\u00b7det", "doch", "die", "Welt", "in", "Gott", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}