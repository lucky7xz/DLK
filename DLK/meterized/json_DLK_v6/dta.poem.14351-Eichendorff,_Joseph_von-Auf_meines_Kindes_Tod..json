{"dta.poem.14351": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf meines Kindes Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie seh'n ich mich auf's neue", "tokens": ["Wie", "seh'n", "ich", "mich", "auf's", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus in Wald und Flur!", "tokens": ["Hin\u00b7aus", "in", "Wald", "und", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob ich mich gr\u00e4m', mich freue,", "tokens": ["Ob", "ich", "mich", "gr\u00e4m'", ",", "mich", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bleibst mir treu, Natur.", "tokens": ["Du", "bleibst", "mir", "treu", ",", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}