{"textgrid.poem.46153": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fort mit euch sorg und leid,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort mit euch sorg und leid,", "tokens": ["Fort", "mit", "euch", "sorg", "und", "leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "als unsrer herzen schlang,", "tokens": ["als", "uns\u00b7rer", "her\u00b7zen", "schlang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und seelenplag!", "tokens": ["und", "see\u00b7len\u00b7plag", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVIMP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "fort mit euch flehen, eid,", "tokens": ["fort", "mit", "euch", "fle\u00b7hen", ",", "eid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKVZ", "APPR", "PPER", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "angst, trauriges gesang", "tokens": ["angst", ",", "trau\u00b7ri\u00b7ges", "ge\u00b7sang"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und lange klag!", "tokens": ["und", "lan\u00b7ge", "klag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "fort schwermut und sorgf\u00e4ltigkeit,", "tokens": ["fort", "schwer\u00b7mut", "und", "sorg\u00b7f\u00e4l\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "VVFIN", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "all ihr feind der leichtm\u00fctigkeit!", "tokens": ["all", "ihr", "feind", "der", "leicht\u00b7m\u00fc\u00b7tig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ihr z\u00e4hren dr\u00fccknet euch,", "tokens": ["Ihr", "z\u00e4h\u00b7ren", "dr\u00fcck\u00b7net", "euch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "weil ich nu euern grund", "tokens": ["weil", "ich", "nu", "eu\u00b7ern", "grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "gedr\u00fccknet find!", "tokens": ["ge\u00b7dr\u00fcck\u00b7net", "find", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "ihr seufzen auch zugleich,", "tokens": ["ihr", "seuf\u00b7zen", "auch", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "dieweil ich nu gesund,", "tokens": ["die\u00b7weil", "ich", "nu", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "legt euern wind!", "tokens": ["legt", "eu\u00b7ern", "wind", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "und ihr gedanken endet euch,", "tokens": ["und", "ihr", "ge\u00b7dan\u00b7ken", "en\u00b7det", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "durch euch sah ich noch niemand reich!", "tokens": ["durch", "euch", "sah", "ich", "noch", "nie\u00b7mand", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "PIS", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Hingegen kommet her", "tokens": ["Hin\u00b7ge\u00b7gen", "kom\u00b7met", "her"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "ihr hofnung, trost und gunst,", "tokens": ["ihr", "hof\u00b7nung", ",", "trost", "und", "gunst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der liebe lust;", "tokens": ["der", "lie\u00b7be", "lust", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "erquicket mehr und mehr", "tokens": ["er\u00b7quic\u00b7ket", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "durch ein neu s\u00fc\u00dfe brunst", "tokens": ["durch", "ein", "neu", "s\u00fc\u00b7\u00dfe", "brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mein schwache brust!", "tokens": ["mein", "schwa\u00b7che", "brust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "und du begird geneu\u00df der weid", "tokens": ["und", "du", "be\u00b7gird", "ge\u00b7neu\u00df", "der", "weid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "der so oft vor gew\u00fcnschten freid.", "tokens": ["der", "so", "oft", "vor", "ge\u00b7w\u00fcnschten", "freid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Der z\u00e4hern tropfenfall", "tokens": ["Der", "z\u00e4\u00b7hern", "tro\u00b7pfen\u00b7fall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "werd ein liebreicher flug", "tokens": ["werd", "ein", "lieb\u00b7rei\u00b7cher", "flug"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "der lieb anblick;", "tokens": ["der", "lieb", "an\u00b7blick", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "ein klarer musikschall", "tokens": ["ein", "kla\u00b7rer", "mu\u00b7sik\u00b7schall"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "werd meiner seufzen zug", "tokens": ["werd", "mei\u00b7ner", "seuf\u00b7zen", "zug"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "und sing mein gl\u00fcck,", "tokens": ["und", "sing", "mein", "gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "und der gedanken gro\u00dfer hauf", "tokens": ["und", "der", "ge\u00b7dan\u00b7ken", "gro\u00b7\u00dfer", "hauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "werd wahrer lust und h\u00f6r nicht auf.", "tokens": ["werd", "wah\u00b7rer", "lust", "und", "h\u00f6r", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}