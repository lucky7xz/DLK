{"textgrid.poem.60988": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: O darum ist der Lenz so sch\u00f6n", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O darum ist der Lenz so sch\u00f6n", "tokens": ["O", "da\u00b7rum", "ist", "der", "Lenz", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PAV", "VAFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Duft und Strahl und Lied,", "tokens": ["Mit", "Duft", "und", "Strahl", "und", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil singend \u00fcber Tal und H\u00f6hn", "tokens": ["Weil", "sin\u00b7gend", "\u00fc\u00b7ber", "Tal", "und", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bald er weiter zieht;", "tokens": ["So", "bald", "er", "wei\u00b7ter", "zieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und darum ist so s\u00fc\u00df der Traum,", "tokens": ["Und", "da\u00b7rum", "ist", "so", "s\u00fc\u00df", "der", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den erste Liebe webt,", "tokens": ["Den", "ers\u00b7te", "Lie\u00b7be", "webt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil schneller wie die Bl\u00fct' am Baum", "tokens": ["Weil", "schnel\u00b7ler", "wie", "die", "Bl\u00fct'", "am", "Baum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er hinwelkt und verschwebt.", "tokens": ["Er", "hin\u00b7welkt", "und", "ver\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und doch! Er l\u00e4\u00dft so still erw\u00e4rmt,", "tokens": ["Und", "doch", "!", "Er", "l\u00e4\u00dft", "so", "still", "er\u00b7w\u00e4rmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So reich das Herz zur\u00fcck;", "tokens": ["So", "reich", "das", "Herz", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' geliebt, ich hab' geschw\u00e4rmt,", "tokens": ["Ich", "hab'", "ge\u00b7liebt", ",", "ich", "hab'", "ge\u00b7schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich preis' auch das ein Gl\u00fcck.", "tokens": ["Ich", "preis'", "auch", "das", "ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gesogen hab' ich Strahl auf Strahl", "tokens": ["Ge\u00b7so\u00b7gen", "hab'", "ich", "Strahl", "auf", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins Herz den kurzen Tag;", "tokens": ["Ins", "Herz", "den", "kur\u00b7zen", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sch\u00f6ne Sonne sinkt zu Tal.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Son\u00b7ne", "sinkt", "zu", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun komme, was kommen mag!", "tokens": ["Nun", "kom\u00b7me", ",", "was", "kom\u00b7men", "mag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sei's bittres Leid, sei's neue Lust,", "tokens": ["Sei's", "bitt\u00b7res", "Leid", ",", "sei's", "neu\u00b7e", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es soll getragen sein:", "tokens": ["Es", "soll", "ge\u00b7tra\u00b7gen", "sein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sichre Schatz in meiner Brust", "tokens": ["Der", "sich\u00b7re", "Schatz", "in", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt dennoch ewig mein.", "tokens": ["Bleibt", "den\u00b7noch", "e\u00b7wig", "mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}