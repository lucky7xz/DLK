{"dta.poem.5282": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lieblichkeiten des Fr\u00fchlings.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser den Winter vertreibenden Lentzen-Zeit", "tokens": ["In", "die\u00b7ser", "den", "Win\u00b7ter", "ver\u00b7trei\u00b7ben\u00b7den", "Lent\u00b7zen\u00b7Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Belebet sich alles im Reiche der Sonnen;", "tokens": ["Be\u00b7le\u00b7bet", "sich", "al\u00b7les", "im", "Rei\u00b7che", "der", "Son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "APPRART", "NE", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Erf\u00fcllet sich alles mit Anmuth und Lieblichkeit:", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "sich", "al\u00b7les", "mit", "An\u00b7muth", "und", "Lieb\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.4": {"text": "Der fr\u00f6liche Weinstock hat Augen gewonnen.", "tokens": ["Der", "fr\u00f6\u00b7li\u00b7che", "Wein\u00b7stock", "hat", "Au\u00b7gen", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es circkelt in B\u00e4umen ein n\u00e4hrender Lebens-Saft.", "tokens": ["Es", "cir\u00b7ckelt", "in", "B\u00e4u\u00b7men", "ein", "n\u00e4h\u00b7ren\u00b7der", "Le\u00b7bens\u00b7Saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Die Knospen erheben sich, schwellen und bersten.", "tokens": ["Die", "Knos\u00b7pen", "er\u00b7he\u00b7ben", "sich", ",", "schwel\u00b7len", "und", "bers\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Es deckt sich der Acker, voll g\u00e4hrender Wunder-Kraft,", "tokens": ["Es", "deckt", "sich", "der", "A\u00b7cker", ",", "voll", "g\u00e4h\u00b7ren\u00b7der", "Wun\u00b7der\u00b7Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.8": {"text": "Mit gr\u00fcnenden Spitzen von Haber und Gersten.", "tokens": ["Mit", "gr\u00fc\u00b7nen\u00b7den", "Spit\u00b7zen", "von", "Ha\u00b7ber", "und", "Gers\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "In W\u00e4ldern erfolget durch wachsender Bl\u00e4tter Pracht,", "tokens": ["In", "W\u00e4l\u00b7dern", "er\u00b7fol\u00b7get", "durch", "wach\u00b7sen\u00b7der", "Bl\u00e4t\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.10": {"text": "Von denen jetzt gleichsam umnebelten Wipfeln,", "tokens": ["Von", "de\u00b7nen", "jetzt", "gleich\u00b7sam", "um\u00b7ne\u00b7bel\u00b7ten", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Auf gr\u00fcnlicher D\u00e4mmerung, die liebliche Schatten-Nacht", "tokens": ["Auf", "gr\u00fcn\u00b7li\u00b7cher", "D\u00e4m\u00b7me\u00b7rung", ",", "die", "lieb\u00b7li\u00b7che", "Schat\u00b7ten\u00b7Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Es spriessen aus scharffen erhabenen Gipfeln", "tokens": ["Es", "spries\u00b7sen", "aus", "scharf\u00b7fen", "er\u00b7ha\u00b7be\u00b7nen", "Gip\u00b7feln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "Bewachsener Berge, die Kr\u00e4uter jetzt \u00fcberall.", "tokens": ["Be\u00b7wach\u00b7se\u00b7ner", "Ber\u00b7ge", ",", "die", "Kr\u00e4u\u00b7ter", "jetzt", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.14": {"text": "Und f\u00fcllen mit duftigem Balsam die L\u00fcfte.", "tokens": ["Und", "f\u00fcl\u00b7len", "mit", "duf\u00b7ti\u00b7gem", "Bal\u00b7sam", "die", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.15": {"text": "Es schwebet der schertzende, schw\u00e4tzige Wiederhall", "tokens": ["Es", "schwe\u00b7bet", "der", "schert\u00b7zen\u00b7de", ",", "schw\u00e4t\u00b7zi\u00b7ge", "Wie\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.16": {"text": "Um ihre bemoste verwachsene Kl\u00fcfte.", "tokens": ["Um", "ih\u00b7re", "be\u00b7mos\u00b7te", "ver\u00b7wach\u00b7se\u00b7ne", "Kl\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.17": {"text": "Das dunckle Geb\u00fcsche, den schattigten Wald erf\u00fcllt", "tokens": ["Das", "dunck\u00b7le", "Ge\u00b7b\u00fc\u00b7sche", ",", "den", "schat\u00b7tig\u00b7ten", "Wald", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVPP"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.18": {"text": "Der schlagenden Nachtigall schmetterndes Schallen.", "tokens": ["Der", "schla\u00b7gen\u00b7den", "Nach\u00b7ti\u00b7gall", "schmet\u00b7tern\u00b7des", "Schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.19": {"text": "Es springet im blumigten Grase das junge Wild,", "tokens": ["Es", "sprin\u00b7get", "im", "blu\u00b7mig\u00b7ten", "Gra\u00b7se", "das", "jun\u00b7ge", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.20": {"text": "Und f\u00fchlet in Adern ein kitzelndes Wallen.", "tokens": ["Und", "f\u00fch\u00b7let", "in", "A\u00b7dern", "ein", "kit\u00b7zeln\u00b7des", "Wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.21": {"text": "Jetzt murmelt und rauschet und rieselt die rege Fluht.", "tokens": ["Jetzt", "mur\u00b7melt", "und", "rau\u00b7schet", "und", "rie\u00b7selt", "die", "re\u00b7ge", "Fluht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Auf wallender Wellen beweglichen Spitzen", "tokens": ["Auf", "wal\u00b7len\u00b7der", "Wel\u00b7len", "be\u00b7weg\u00b7li\u00b7chen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.23": {"text": "Entwirft und formiret der strahlenden Sonnen Glut", "tokens": ["Ent\u00b7wirft", "und", "for\u00b7mi\u00b7ret", "der", "strah\u00b7len\u00b7den", "Son\u00b7nen", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Viel funckelnde Bilder in schimmerden Blitzen;", "tokens": ["Viel", "fun\u00b7ckeln\u00b7de", "Bil\u00b7der", "in", "schim\u00b7mer\u00b7den", "Blit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.25": {"text": "Man sieht, mit Ergetzen, die Blitze verwunderlich", "tokens": ["Man", "sieht", ",", "mit", "Er\u00b7get\u00b7zen", ",", "die", "Blit\u00b7ze", "ver\u00b7wun\u00b7der\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "APPR", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "In tausend beweglichen Spiegeln sich brechen.", "tokens": ["In", "tau\u00b7send", "be\u00b7weg\u00b7li\u00b7chen", "Spie\u00b7geln", "sich", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.27": {"text": "Die Fluth, wie ein lebender Silber-Flu\u00df, schl\u00e4ngelt sich", "tokens": ["Die", "Fluth", ",", "wie", "ein", "le\u00b7ben\u00b7der", "Sil\u00b7ber\u00b7Flu\u00df", ",", "schl\u00e4n\u00b7gelt", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.28": {"text": "Durch gr\u00fcnender Felder smaragdene Fl\u00e4chen.", "tokens": ["Durch", "gr\u00fc\u00b7nen\u00b7der", "Fel\u00b7der", "sma\u00b7rag\u00b7de\u00b7ne", "Fl\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}