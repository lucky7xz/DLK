{"dta.poem.11371": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als Jhro Churf\u00fcrstl. Durchl. von Pfaltz  \n zu Weissenfels bey dem mann-schiessen  \n der b\u00fcrgerschafft den haupt-gewinnst, als  \n sch\u00fctzen-k\u00f6nig,  an.  1704 davon trugen.  \n Jm nahmen der sch\u00fctzen-  \n compagnie.  \n M. S. R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm! so ruffet itzt der Rhein: Komm! so mu\u00df die", "tokens": ["Komm", "!", "so", "ruf\u00b7fet", "itzt", "der", "Rhein", ":", "Komm", "!", "so", "mu\u00df", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "ADV", "ART", "NE", "$.", "VVFIN", "$.", "ADV", "VMFIN", "ART"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Donau sagen:", "tokens": ["Do\u00b7nau", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Theurer Churf\u00fcrst! komm zu uns! Denn die feinde sind", "tokens": ["Theu\u00b7rer", "Chur\u00b7f\u00fcrst", "!", "komm", "zu", "uns", "!", "Denn", "die", "fein\u00b7de", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "VVFIN", "APPR", "PPER", "$.", "KON", "ART", "NN", "VAFIN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "geschlagen.", "tokens": ["ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Jtzo hat zu zweyen mahlen GOtt ein solches werck gethan,", "tokens": ["Jt\u00b7zo", "hat", "zu", "zwe\u00b7yen", "mah\u00b7len", "Gott", "ein", "sol\u00b7ches", "werck", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKZU", "VVINF", "VVINF", "NN", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Da\u00df die ungemeinen siege kaum die nach-welt glauben kan.", "tokens": ["Da\u00df", "die", "un\u00b7ge\u00b7mei\u00b7nen", "sie\u00b7ge", "kaum", "die", "nach\u00b7welt", "glau\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.7": {"text": "Komm! und la\u00df dein hohes haupt mit dem lorbeer nun be-", "tokens": ["Komm", "!", "und", "la\u00df", "dein", "ho\u00b7hes", "haupt", "mit", "dem", "lor\u00b7beer", "nun", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "KON", "VVIMP", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "lohnen,", "tokens": ["loh\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Der dir schon geflochten ist. Denn die ehr und sieges-cronen,", "tokens": ["Der", "dir", "schon", "ge\u00b7floch\u00b7ten", "ist", ".", "Denn", "die", "ehr", "und", "sie\u00b7ges\u00b7cro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VAFIN", "$.", "KON", "ART", "NN", "KON", "ADJA", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Die ein ander Maccab\u00e4us und ein Gideon empf\u00e4ngt,", "tokens": ["Die", "ein", "an\u00b7der", "Mac\u00b7ca\u00b7b\u00e4us", "und", "ein", "Gi\u00b7de\u00b7on", "emp\u00b7f\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "ART", "NE", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Werden an den pyramiden deines nahmens aufgehengt.", "tokens": ["Wer\u00b7den", "an", "den", "py\u00b7ra\u00b7mi\u00b7den", "dei\u00b7nes", "nah\u00b7mens", "auf\u00b7ge\u00b7hengt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Denn du must der Atlas seyn, welcher Leopoldens himmel,", "tokens": ["Denn", "du", "must", "der", "At\u00b7las", "seyn", ",", "wel\u00b7cher", "Leo\u00b7pol\u00b7dens", "him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,", "PRELS", "NE", "NN", "$,"], "meter": "--+-++-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Das ist, unser Teutsches Reich, mitten in dem kriegs-get\u00fcmmel", "tokens": ["Das", "ist", ",", "un\u00b7ser", "Teut\u00b7sches", "Reich", ",", "mit\u00b7ten", "in", "dem", "kriegs\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Durch ein patriotisch hertze tapfferm\u00fcthig unterst\u00fctzt,", "tokens": ["Durch", "ein", "pat\u00b7ri\u00b7o\u00b7tisch", "hert\u00b7ze", "tapf\u00b7fer\u00b7m\u00fct\u00b7hig", "un\u00b7ter\u00b7st\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Und mit volck, mit rath und sorgen die gekr\u00e4nckte freyheit st\u00fctzt.", "tokens": ["Und", "mit", "volck", ",", "mit", "rath", "und", "sor\u00b7gen", "die", "ge\u00b7kr\u00e4nck\u00b7te", "frey\u00b7heit", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}, "line.16": {"text": "Komm! vergn\u00fcge wiederum deiner treuen lande sehnen!", "tokens": ["Komm", "!", "ver\u00b7gn\u00fc\u00b7ge", "wie\u00b7de\u00b7rum", "dei\u00b7ner", "treu\u00b7en", "lan\u00b7de", "seh\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Komm! und la\u00df die ewigkeit dich mit solchem segen cr\u00f6nen,", "tokens": ["Komm", "!", "und", "la\u00df", "die", "e\u00b7wig\u00b7keit", "dich", "mit", "sol\u00b7chem", "se\u00b7gen", "cr\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KON", "VVIMP", "ART", "NN", "PRF", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Welcher, weil der welt-bau stehet, dir wird unverwelcklich seyn!", "tokens": ["Wel\u00b7cher", ",", "weil", "der", "welt\u00b7bau", "ste\u00b7het", ",", "dir", "wird", "un\u00b7ver\u00b7welck\u00b7lich", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Komm! so mu\u00df die Donau sagen: Komm! so ruffet itzt der", "tokens": ["Komm", "!", "so", "mu\u00df", "die", "Do\u00b7nau", "sa\u00b7gen", ":", "Komm", "!", "so", "ruf\u00b7fet", "itzt", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ADV", "VMFIN", "ART", "NE", "VVINF", "$.", "VVFIN", "$.", "ADV", "VVFIN", "ADV", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.20": {"text": "Rhein:", "tokens": ["Rhein", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Bleib! so spricht Angustusburg: Bleib! so mu\u00df die Saale r\u00fcffen:", "tokens": ["Bleib", "!", "so", "spricht", "An\u00b7gus\u00b7tus\u00b7burg", ":", "Bleib", "!", "so", "mu\u00df", "die", "Saa\u00b7le", "r\u00fcf\u00b7fen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VVFIN", "NE", "$.", "NN", "$.", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "---+-+-+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.22": {"text": "Theurer Churf\u00fcrst! bleib noch hier! Denn durch dich sind uns", "tokens": ["Theu\u00b7rer", "Chur\u00b7f\u00fcrst", "!", "bleib", "noch", "hier", "!", "Denn", "durch", "dich", "sind", "uns"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$.", "VVFIN", "ADV", "ADV", "$.", "KON", "APPR", "PPER", "VAFIN", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "die stuffen", "tokens": ["die", "stuf\u00b7fen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Zu gantz ungemeinem ruhme bi\u00df in graue zeit gebaut,", "tokens": ["Zu", "gantz", "un\u00b7ge\u00b7mei\u00b7nem", "ruh\u00b7me", "bi\u00df", "in", "grau\u00b7e", "zeit", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "APPR", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Da man dich, als eine sonne, auf dem weissen felsen schaut.", "tokens": ["Da", "man", "dich", ",", "als", "ei\u00b7ne", "son\u00b7ne", ",", "auf", "dem", "weis\u00b7sen", "fel\u00b7sen", "schaut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$,", "KOUS", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Ach! du kanst doch nimmermehr deine g\u00fcte nicht verneinen:", "tokens": ["Ach", "!", "du", "kanst", "doch", "nim\u00b7mer\u00b7mehr", "dei\u00b7ne", "g\u00fc\u00b7te", "nicht", "ver\u00b7nei\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ADV", "ADV", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Deine holde gegenwart mu\u00df uns noch zur freude scheinen:", "tokens": ["Dei\u00b7ne", "hol\u00b7de", "ge\u00b7gen\u00b7wart", "mu\u00df", "uns", "noch", "zur", "freu\u00b7de", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.28": {"text": "Deine freundschafft, deine liebe, k\u00fcsset unsern Salomon:", "tokens": ["Dei\u00b7ne", "freund\u00b7schafft", ",", "dei\u00b7ne", "lie\u00b7be", ",", "k\u00fcs\u00b7set", "un\u00b7sern", "Sa\u00b7lo\u00b7mon", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.29": {"text": "Und die armen b\u00fcrger tragen auch ein gnaden-theil davon.", "tokens": ["Und", "die", "ar\u00b7men", "b\u00fcr\u00b7ger", "tra\u00b7gen", "auch", "ein", "gna\u00b7den\u00b7theil", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.30": {"text": "Denn da hier, auf alten brauch, nach dem manne ward geschossen,", "tokens": ["Denn", "da", "hier", ",", "auf", "al\u00b7ten", "brauch", ",", "nach", "dem", "man\u00b7ne", "ward", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Hat die sch\u00fctzen-compagnie so ein stoltzes gl\u00fcck genossen,", "tokens": ["Hat", "die", "sch\u00fct\u00b7zen\u00b7com\u00b7pa\u00b7gnie", "so", "ein", "stolt\u00b7zes", "gl\u00fcck", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.32": {"text": "Das vorher noch nie gewesen, schwerlich auch wird wieder", "tokens": ["Das", "vor\u00b7her", "noch", "nie", "ge\u00b7we\u00b7sen", ",", "schwer\u00b7lich", "auch", "wird", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADV", "ADV", "VAPP", "$,", "ADJD", "ADV", "VAFIN", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.33": {"text": "seyn,", "tokens": ["seyn", ","], "token_info": ["word", "punct"], "pos": ["VAINF", "$,"], "meter": "+", "measure": "single.up"}, "line.34": {"text": "Denn du legtest das gesetzte gleich den andern auch mit ein.", "tokens": ["Denn", "du", "leg\u00b7test", "das", "ge\u00b7setz\u00b7te", "gleich", "den", "an\u00b7dern", "auch", "mit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ADV", "ART", "ADJA", "ADV", "APPR", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.35": {"text": "Doch das gl\u00fccke wolte sie noch zu gr\u00f6ssern ehren bringen:", "tokens": ["Doch", "das", "gl\u00fc\u00b7cke", "wol\u00b7te", "sie", "noch", "zu", "gr\u00f6s\u00b7sern", "eh\u00b7ren", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "VMFIN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Da man siebenfach certirt, must es dennoch so gelingen,", "tokens": ["Da", "man", "sie\u00b7ben\u00b7fach", "cer\u00b7tirt", ",", "must", "es", "den\u00b7noch", "so", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.37": {"text": "Da\u00df das loos vom haupt-gewinnste blo\u00df vor dich gefallen", "tokens": ["Da\u00df", "das", "loos", "vom", "haup\u00b7tge\u00b7winns\u00b7te", "blo\u00df", "vor", "dich", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "ADV", "APPR", "PRF", "VVINF"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.38": {"text": "ist,", "tokens": ["ist", ","], "token_info": ["word", "punct"], "pos": ["VAFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.39": {"text": "Und man dich, nach sch\u00fctzen-rechte, nun als ihren k\u00f6nig", "tokens": ["Und", "man", "dich", ",", "nach", "sch\u00fct\u00b7zen\u00b7rech\u00b7te", ",", "nun", "als", "ih\u00b7ren", "k\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "PPER", "$,", "APPR", "ADJA", "$,", "ADV", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.40": {"text": "gr\u00fc\u00dft!", "tokens": ["gr\u00fc\u00dft", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.41": {"text": "Zwar vor dich, Durchlauchtigster! ists ein schlechtes ding zu", "tokens": ["Zwar", "vor", "dich", ",", "Durch\u00b7lauch\u00b7tigs\u00b7ter", "!", "ists", "ein", "schlech\u00b7tes", "ding", "zu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$,", "NN", "$.", "VAFIN", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "nennen:", "tokens": ["nen\u00b7nen", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "--", "measure": "