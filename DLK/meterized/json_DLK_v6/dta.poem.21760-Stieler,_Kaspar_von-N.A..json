{"dta.poem.21760": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe schleifft der Dichter Sinn", "tokens": ["Die", "Lie\u00b7be", "schleifft", "der", "Dich\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und nimt die dunkeln Schlakke\u0303 hin", "tokens": ["und", "nimt", "die", "dun\u00b7keln", "Schlak\u00b7k\u1ebd", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "kaum hat ein Dichter wol geschrieben", "tokens": ["kaum", "hat", "ein", "Dich\u00b7ter", "wol", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verlache/ Filidor/ den Neid", "tokens": ["Ver\u00b7la\u00b7che", "/", "Fi\u00b7li\u00b7dor", "/", "den", "Neid"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$(", "NE", "$(", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dich sch\u00fczzet die gelehrte Zeit", "tokens": ["dich", "sch\u00fcz\u00b7zet", "die", "ge\u00b7lehr\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der alten Liebenden Poeten", "tokens": ["der", "al\u00b7ten", "Lie\u00b7ben\u00b7den", "Po\u00b7et\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die keine Zeit noch Neid wird t\u00f6dten.", "tokens": ["die", "kei\u00b7ne", "Zeit", "noch", "Neid", "wird", "t\u00f6d\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Katull/ Tibull und dem Properz", "tokens": ["Ka\u00b7tull", "/", "Ti\u00b7bull", "und", "dem", "Pro\u00b7perz"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "NE", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "in F\u00f6bus Tempel eingezogen", "tokens": ["in", "F\u00f6\u00b7bus", "Tem\u00b7pel", "ein\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "und \u00fcber das Gestirn geflogen.", "tokens": ["und", "\u00fc\u00b7ber", "das", "Ge\u00b7stirn", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Virgil/ Horaz und den das Land", "tokens": ["Vir\u00b7gil", "/", "Ho\u00b7raz", "und", "den", "das", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "NE", "KON", "ART", "ART", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "der Geten endlich hat verbrandt", "tokens": ["der", "Ge\u00b7ten", "end\u00b7lich", "hat", "ver\u00b7brandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Lieb\u2019 anerst ber\u00fchmet worden.", "tokens": ["der", "Lieb'", "an\u00b7erst", "be\u00b7r\u00fch\u00b7met", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht unsre Deutsche Lichter an/", "tokens": ["Seht", "uns\u00b7re", "Deut\u00b7sche", "Lich\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ob es die Liebe nicht getahn", "tokens": ["ob", "es", "die", "Lie\u00b7be", "nicht", "ge\u00b7tahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df unsre Sprache reine stehet", "tokens": ["Da\u00df", "uns\u00b7re", "Spra\u00b7che", "rei\u00b7ne", "ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und andern zu der Rechten gehet.", "tokens": ["und", "an\u00b7dern", "zu", "der", "Rech\u00b7ten", "ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Lieb\u2019 erhebet unsern Geist", "tokens": ["Die", "Lieb'", "er\u00b7he\u00b7bet", "un\u00b7sern", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df er sich au\u00df dem Staube reist", "tokens": ["da\u00df", "er", "sich", "au\u00df", "dem", 