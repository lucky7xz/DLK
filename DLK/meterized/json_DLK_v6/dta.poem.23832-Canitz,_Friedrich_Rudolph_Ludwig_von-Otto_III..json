{"dta.poem.23832": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Otto III.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hoheit meines Reichs besch\u00fctzt ich durch die Waffen", "tokens": ["Die", "Ho\u00b7heit", "mei\u00b7nes", "Reichs", "be\u00b7sch\u00fctzt", "ich", "durch", "die", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man machte mir zu Rom mit Aufruhr viel zu schaffen/", "tokens": ["Man", "mach\u00b7te", "mir", "zu", "Rom", "mit", "Auf\u00b7ruhr", "viel", "zu", "schaf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "NE", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Weib voll Zorn und List/ die schickte mich ins Grab/", "tokens": ["Ein", "Weib", "voll", "Zorn", "und", "List", "/", "die", "schick\u00b7te", "mich", "ins", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$(", "ART", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als sie mir Gifft und Tod durch Handschuh \u00fcbergab.", "tokens": ["Als", "sie", "mir", "Gifft", "und", "Tod", "durch", "Hand\u00b7schuh", "\u00fc\u00b7berg\u00b7ab", "."], "to