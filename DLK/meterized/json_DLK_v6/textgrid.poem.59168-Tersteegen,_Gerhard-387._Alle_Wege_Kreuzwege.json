{"textgrid.poem.59168": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "387. Alle Wege Kreuzwege", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sag' ich zum Kreuz ja oder nein,", "tokens": ["Sag'", "ich", "zum", "Kreuz", "ja", "o\u00b7der", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "KON", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll ich es lieben oder hassen?", "tokens": ["Soll", "ich", "es", "lie\u00b7ben", "o\u00b7der", "has\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es mischt sich stets in allem ein;", "tokens": ["Es", "mischt", "sich", "stets", "in", "al\u00b7lem", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo ich mich kehr', ich find's auf allen Gassen.", "tokens": ["Wo", "ich", "mich", "kehr'", ",", "ich", "fin\u00b7d's", "auf", "al\u00b7len", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}}}}}