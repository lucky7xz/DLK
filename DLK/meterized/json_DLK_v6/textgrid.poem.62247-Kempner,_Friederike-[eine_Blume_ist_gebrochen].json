{"textgrid.poem.62247": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[eine Blume ist gebrochen]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Blume ist gebrochen,", "tokens": ["Ei\u00b7ne", "Blu\u00b7me", "ist", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier f\u00fcr immerdar, \u2013", "tokens": ["Hier", "f\u00fcr", "im\u00b7mer\u00b7dar", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ADV", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die edle F\u00fcrstin z\u00e4hlte", "tokens": ["Und", "die", "ed\u00b7le", "F\u00fcrs\u00b7tin", "z\u00e4hl\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vierundzwanzig Jahr.", "tokens": ["Vie\u00b7rund\u00b7zwan\u00b7zig", "Jahr", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wer des Lebens Gl\u00fcck gekostet", "tokens": ["Wer", "des", "Le\u00b7bens", "Gl\u00fcck", "ge\u00b7kos\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dann rasch entflieht,", "tokens": ["Und", "dann", "rasch", "ent\u00b7flieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Kennt nicht seine Dissonanzen,", "tokens": ["Kennt", "nicht", "sei\u00b7ne", "Dis\u00b7so\u00b7nan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rte nur sein Lied.", "tokens": ["H\u00f6r\u00b7te", "nur", "sein", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und mich d\u00fcnkt, in solcher Jugend", "tokens": ["Und", "mich", "d\u00fcnkt", ",", "in", "sol\u00b7cher", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Enden leichter sei,", "tokens": ["En\u00b7den", "leich\u00b7ter", "sei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie die T\u00f6ne leicht sich l\u00f6sen", "tokens": ["Wie", "die", "T\u00f6\u00b7ne", "leicht", "sich", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer Melodei.", "tokens": ["Ei\u00b7ner", "Me\u00b7lo\u00b7dei", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}