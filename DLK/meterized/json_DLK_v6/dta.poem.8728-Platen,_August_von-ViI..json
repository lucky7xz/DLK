{"dta.poem.8728": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Steuernd nach dem Land der Pracht,", "tokens": ["Steu\u00b7ernd", "nach", "dem", "Land", "der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Freyheit stolzes Leben", "tokens": ["Wo", "der", "Frey\u00b7heit", "stol\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen Palmen auferwacht.", "tokens": ["Zwi\u00b7schen", "Pal\u00b7men", "au\u00b7fer\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der erhizte Wahn der Jugend,", "tokens": ["Der", "er\u00b7hiz\u00b7te", "Wahn", "der", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der das Gl\u00fcck sich fern verhei\u00dft,", "tokens": ["Der", "das", "Gl\u00fcck", "sich", "fern", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weiche deiner strengern Tugend,", "tokens": ["Wei\u00b7che", "dei\u00b7ner", "stren\u00b7gern", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiche deinem gr\u00f6\u00dfern Geist!", "tokens": ["Wei\u00b7che", "dei\u00b7nem", "gr\u00f6\u00b7\u00dfern", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Soll der lezte Stern erbleichen", "tokens": ["Soll", "der", "lez\u00b7te", "Stern", "er\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An des deutschen Himmels Rand,", "tokens": ["An", "des", "deut\u00b7schen", "Him\u00b7mels", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, so decken uns're Leichen", "tokens": ["O", ",", "so", "de\u00b7cken", "un\u00b7s'\u00b7re", "Lei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Das verlorne Vaterland!", "tokens": ["Das", "ver\u00b7lor\u00b7ne", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}