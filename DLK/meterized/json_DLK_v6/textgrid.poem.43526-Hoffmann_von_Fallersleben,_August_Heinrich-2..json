{"textgrid.poem.43526": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen blinket in silbernem Schein", "tokens": ["Drau\u00b7\u00dfen", "blin\u00b7ket", "in", "sil\u00b7ber\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Schon der Mond mit den Sternelein,", "tokens": ["Schon", "der", "Mond", "mit", "den", "Ster\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und der Tag ist wieder entfloh'n,", "tokens": ["Und", "der", "Tag", "ist", "wie\u00b7der", "ent\u00b7floh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Blumen und V\u00f6gel sie schlafen schon.", "tokens": ["Blu\u00b7men", "und", "V\u00f6\u00b7gel", "sie", "schla\u00b7fen", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Su su su!", "tokens": ["Su", "su", "su", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schlaf auch du!", "tokens": ["Schlaf", "auch", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schlafe, mein Kindchen, in s\u00fc\u00dfer Ruh!", "tokens": ["Schla\u00b7fe", ",", "mein", "Kind\u00b7chen", ",", "in", "s\u00fc\u00b7\u00dfer", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Leid und Freude, Kummer und Qual", "tokens": ["Leid", "und", "Freu\u00b7de", ",", "Kum\u00b7mer", "und", "Qual"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ziehen noch \u00fcber Berg und Thal,", "tokens": ["Zie\u00b7hen", "noch", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Suchen Herberg' im Abendschein,", "tokens": ["Su\u00b7chen", "Her\u00b7ber\u00b7g'", "im", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schleichen in Haus und Herzen ein.", "tokens": ["Schlei\u00b7chen", "in", "Haus", "und", "Her\u00b7zen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Su su su!", "tokens": ["Su", "su", "su", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schlaf auch du!", "tokens": ["Schlaf", "auch", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schlafe, mein Kindchen, in s\u00fc\u00dfer Ruh!", "tokens": ["Schla\u00b7fe", ",", "mein", "Kind\u00b7chen", ",", "in", "s\u00fc\u00b7\u00dfer", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Kommt die Freude, wir lassen sie ein,", "tokens": ["Kommt", "die", "Freu\u00b7de", ",", "wir", "las\u00b7sen", "sie", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Leid und Kummer soll drau\u00dfen sein.", "tokens": ["Leid", "und", "Kum\u00b7mer", "soll", "drau\u00b7\u00dfen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "ADV", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Nur die Freude halte die Wacht", "tokens": ["Nur", "die", "Freu\u00b7de", "hal\u00b7te", "die", "Wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Fr\u00f6hlich bei dir die lange Nacht!", "tokens": ["Fr\u00f6h\u00b7lich", "bei", "dir", "die", "lan\u00b7ge", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Su su su!", "tokens": ["Su", "su", "su", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schlaf auch du!", "tokens": ["Schlaf", "auch", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schlafe, mein Kindchen, in s\u00fc\u00dfer Ruh!", "tokens": ["Schla\u00b7fe", ",", "mein", "Kind\u00b7chen", ",", "in", "s\u00fc\u00b7\u00dfer", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Schleicht das Leid sich dennoch ein,", "tokens": ["Schleicht", "das", "Leid", "sich", "den\u00b7noch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hei\u00dft, mein Kindchen, dich weinen und schrei'n,", "tokens": ["Hei\u00dft", ",", "mein", "Kind\u00b7chen", ",", "dich", "wei\u00b7nen", "und", "schrei'n", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PRF", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Eilt die Mutterliebe herzu,", "tokens": ["Eilt", "die", "Mut\u00b7ter\u00b7lie\u00b7be", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sitzt an der Wieg' und singet im Nu:", "tokens": ["Sitzt", "an", "der", "Wieg'", "und", "sin\u00b7get", "im", "Nu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPRART", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Su su su!", "tokens": ["Su", "su", "su", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Schlaf auch du!", "tokens": ["Schlaf", "auch", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Schlafe, mein Kindchen, in s\u00fc\u00dfer Ruh!", "tokens": ["Schla\u00b7fe", ",", "mein", "Kind\u00b7chen", ",", "in", "s\u00fc\u00b7\u00dfer", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}