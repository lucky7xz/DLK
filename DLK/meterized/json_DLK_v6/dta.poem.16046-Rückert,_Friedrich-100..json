{"dta.poem.16046": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist nicht Weisheit, die nur sich f\u00fcr Weisheit h\u00e4lt,", "tokens": ["Das", "ist", "nicht", "Weis\u00b7heit", ",", "die", "nur", "sich", "f\u00fcr", "Weis\u00b7heit", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "PRELS", "ADV", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich in fremder Bl\u00f6\u00df' Entdeckung wohlgef\u00e4llt.", "tokens": ["Und", "sich", "in", "frem\u00b7der", "Bl\u00f6\u00df'", "Ent\u00b7de\u00b7ckung", "wohl\u00b7ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Einseitig ist und war die Weisheit aller Weisen;", "tokens": ["Ein\u00b7sei\u00b7tig", "ist", "und", "war", "die", "Weis\u00b7heit", "al\u00b7ler", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst Allseitigkeit nicht als dein Vorrecht preisen.", "tokens": ["Du", "wirst", "All\u00b7sei\u00b7tig\u00b7keit", "nicht", "als", "dein", "Vor\u00b7recht", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "PTKNEG", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jedweder Mensch ist doch nur eine von den Seiten", "tokens": ["Jed\u00b7we\u00b7der", "Mensch", "ist", "doch", "nur", "ei\u00b7ne", "von", "den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Menschheit, welche sich erg\u00e4nzen und bestreiten.", "tokens": ["Der", "Menschheit", ",", "wel\u00b7che", "sich", "er\u00b7g\u00e4n\u00b7zen", "und", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVINF", "KON", "VVFIN", "$."], "meter": "-++-+-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Auch eine bist du nur; da\u00df du dich still erg\u00e4nzest", "tokens": ["Auch", "ei\u00b7ne", "bist", "du", "nur", ";", "da\u00df", "du", "dich", "still", "er\u00b7g\u00e4n\u00b7zest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "VAFIN", "PPER", "ADV", "$.", "KOUS", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit andern, n\u00fctzt dir mehr, als da\u00df du streitend gl\u00e4nzest.", "tokens": ["Mit", "an\u00b7dern", ",", "n\u00fctzt", "dir", "mehr", ",", "als", "da\u00df", "du", "strei\u00b7tend", "gl\u00e4n\u00b7zest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "VVFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}