{"dta.poem.11060": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der 148. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lobt, ihr Himmel, Gott den Herrn! lobt ihn in der H\u00f6h und\nFerne,", "tokens": ["Lobt", ",", "ihr", "Him\u00b7mel", ",", "Gott", "den", "Herrn", "!", "lobt", "ihn", "in", "der", "H\u00f6h", "und", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "NN", "ART", "NN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Lobt ihn, alle seine Engel! lobt ihn, alle seine Heere,", "tokens": ["Lobt", "ihn", ",", "al\u00b7le", "sei\u00b7ne", "En\u00b7gel", "!", "lobt", "ihn", ",", "al\u00b7le", "sei\u00b7ne", "Hee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PIAT", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "$,", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Lobt den Sch\u00f6pfer, Sonn und Mond! lobt ihn, alle helle Sterne,", "tokens": ["Lobt", "den", "Sch\u00f6p\u00b7fer", ",", "Sonn", "und", "Mond", "!", "lobt", "ihn", ",", "al\u00b7le", "hel\u00b7le", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "$.", "VVFIN", "PPER", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und ihr Himmel allenthalben! auch des Himmels Wasser", "tokens": ["Und", "ihr", "Him\u00b7mel", "al\u00b7len\u00b7thal\u00b7ben", "!", "auch", "des", "Him\u00b7mels", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$.", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Die daselbsten hoch erhoben,", "tokens": ["Die", "da\u00b7selbs\u00b7ten", "hoch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollen seinen Namen loben!", "tokens": ["Sol\u00b7len", "sei\u00b7nen", "Na\u00b7men", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn, wenn er gebeut, so bald m\u00fcssen alle Dinge werden,", "tokens": ["Denn", ",", "wenn", "er", "ge\u00b7beut", ",", "so", "bald", "m\u00fcs\u00b7sen", "al\u00b7le", "Din\u00b7ge", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADV", "VMFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Er erh\u00e4lt sie, da\u00df dieselben ewiglich und immer stehn,", "tokens": ["Er", "er\u00b7h\u00e4lt", "sie", ",", "da\u00df", "die\u00b7sel\u00b7ben", "e\u00b7wig\u00b7lich", "und", "im\u00b7mer", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PDAT", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Er verordnet, da\u00df sie nimmer anders, als sie gehen, gehn.", "tokens": ["Er", "ver\u00b7ord\u00b7net", ",", "da\u00df", "sie", "nim\u00b7mer", "an\u00b7ders", ",", "als", "sie", "ge\u00b7hen", ",", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Lobt den Herrn, ihr Wallfisch Heere! alle Tiefen auf der Erden,", "tokens": ["Lobt", "den", "Herrn", ",", "ihr", "Wall\u00b7fisch", "Hee\u00b7re", "!", "al\u00b7le", "Tie\u00b7fen", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "NN", "$.", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Feuer, Hagel, Schnee und Dampf, auch die Winde, die sein", "tokens": ["Feu\u00b7er", ",", "Ha\u00b7gel", ",", "Schnee", "und", "Dampf", ",", "auch", "die", "Win\u00b7de", ",", "die", "sein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Auszurichten fertig sind! Berg und H\u00fcgel, jeder Ort!", "tokens": ["Aus\u00b7zu\u00b7rich\u00b7ten", "fer\u00b7tig", "sind", "!", "Berg", "und", "H\u00fc\u00b7gel", ",", "je\u00b7der", "Ort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "$.", "NN", "KON", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Frucht-B\u00e4um und die hohen Cedern: V\u00f6gel, Vieh, Gew\u00fcrm", "tokens": ["Frucht\u00b7B\u00e4\u00b7um", "und", "die", "ho\u00b7hen", "Ce\u00b7dern", ":", "V\u00f6\u00b7gel", ",", "Vieh", ",", "Ge\u00b7w\u00fcrm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$.", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Jhr Monarchen, alle Leute! F\u00fcrsten, alle Richter hier,", "tokens": ["Ihr", "Mon\u00b7ar\u00b7chen", ",", "al\u00b7le", "Leu\u00b7te", "!", "F\u00fcrs\u00b7ten", ",", "al\u00b7le", "Rich\u00b7ter", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PIAT", "NN", "$.", "NN", "$,", "PIAT", "NN", "ADV", "$,"], "meter": "-++-+-+-+-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.15": {"text": "J\u00fcngling und der Jungfraun Schaar, alt und junge sollen loben,", "tokens": ["J\u00fcng\u00b7ling", "und", "der", "Jung\u00b7fraun", "Schaar", ",", "alt", "und", "jun\u00b7ge", "sol\u00b7len", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "NN", "$,", "ADJD", "KON", "ADJA", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Herr Zebaoth, deinen Namen. Sein Nam ist allein erhoben,", "tokens": ["Herr", "Ze\u00b7bao\u00b7th", ",", "dei\u00b7nen", "Na\u00b7men", ".", "Sein", "Nam", "ist", "al\u00b7lein", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+---+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Sein unendlich Lob geht weiter, als der Himmel und die Erde:", "tokens": ["Sein", "un\u00b7end\u00b7lich", "Lob", "geht", "wei\u00b7ter", ",", "als", "der", "Him\u00b7mel", "und", "die", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "PTKVZ", "$,", "KOUS", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Er verschaffet, da\u00df das Horn seines Volks erhaben werde.", "tokens": ["Er", "ver\u00b7schaf\u00b7fet", ",", "da\u00df", "das", "Horn", "sei\u00b7nes", "Volks", "er\u00b7ha\u00b7ben", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Alle seine Heiligen sollen loben fern und nah,", "tokens": ["Al\u00b7le", "sei\u00b7ne", "Hei\u00b7li\u00b7gen", "sol\u00b7len", "lo\u00b7ben", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VMFIN", "VVINF", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Jsraels Geschlecht, das Volk, das ihm dient, Halleluja!", "tokens": ["Js\u00b7raels", "Ge\u00b7schlecht", ",", "das", "Volk", ",", "das", "ihm", "dient", ",", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ITJ", "$."], "meter": "+--+-+---+--+", "measure": "iambic.penta.invert"}}}}}