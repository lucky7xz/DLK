{"textgrid.poem.34082": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lebens-Regel", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer nur den Menschen im Menschen erblickt, der wird mit dem Niedern", "tokens": ["Wer", "nur", "den", "Men\u00b7schen", "im", "Men\u00b7schen", "er\u00b7blickt", ",", "der", "wird", "mit", "dem", "Nie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "APPRART", "NN", "VVPP", "$,", "PRELS", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gern verkehren, als w\u00e4r' Alles auf Erden sich gleich.", "tokens": ["Gern", "ver\u00b7keh\u00b7ren", ",", "als", "w\u00e4r'", "Al\u00b7les", "auf", "Er\u00b7den", "sich", "gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOKOM", "VAFIN", "PIS", "APPR", "NN", "PRF", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber er thu' es nur dann wenn dieser den Niedrigern wieder", "tokens": ["A\u00b7ber", "er", "thu'", "es", "nur", "dann", "wenn", "die\u00b7ser", "den", "Nied\u00b7ri\u00b7gern", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "KOUS", "PDAT", "ART", "NN", "ADV"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Aehnlich behandelt, denn sonst hat's der Gesell nicht verdient.", "tokens": ["A\u00b7ehn\u00b7lich", "be\u00b7han\u00b7delt", ",", "denn", "sonst", "hat's", "der", "Ge\u00b7sell", "nicht", "ver\u00b7dient", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "KON", "ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+--+---+--+--+", "measure": "iambic.penta.relaxed"}}}}}