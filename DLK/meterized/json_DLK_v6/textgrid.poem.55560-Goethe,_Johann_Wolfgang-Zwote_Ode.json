{"textgrid.poem.55560": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Zwote Ode", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du gehst! Ich murre.", "tokens": ["Du", "gehst", "!", "Ich", "mur\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Geh! La\u00df mich murren.", "tokens": ["Geh", "!", "La\u00df", "mich", "mur\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ehrlicher Mann,", "tokens": ["Ehr\u00b7li\u00b7cher", "Mann", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Fliehe dieses Land.", "tokens": ["Flie\u00b7he", "die\u00b7ses", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Tote S\u00fcmpfe,", "tokens": ["To\u00b7te", "S\u00fcmp\u00b7fe", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dampfende Oktobernebel", "tokens": ["Damp\u00b7fen\u00b7de", "Ok\u00b7to\u00b7ber\u00b7ne\u00b7bel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Verweben ihre Ausfl\u00fcsse", "tokens": ["Ver\u00b7we\u00b7ben", "ih\u00b7re", "Aus\u00b7fl\u00fcs\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hier unzertrennlich.", "tokens": ["Hier", "un\u00b7zer\u00b7trenn\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Geb\u00e4rort", "tokens": ["Ge\u00b7b\u00e4r\u00b7ort"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sch\u00e4dlicher Insekten,", "tokens": ["Sch\u00e4d\u00b7li\u00b7cher", "In\u00b7sek\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "M\u00f6rderh\u00fclle", "tokens": ["M\u00f6r\u00b7der\u00b7h\u00fcl\u00b7le"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ihrer Bosheit.", "tokens": ["Ih\u00b7rer", "Bos\u00b7heit", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Am schilfichten Ufer", "tokens": ["Am", "schil\u00b7fich\u00b7ten", "U\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Liegt die woll\u00fcstige,", "tokens": ["Liegt", "die", "wol\u00b7l\u00fcs\u00b7ti\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Flammengez\u00fcngte Schlange,", "tokens": ["Flam\u00b7men\u00b7ge\u00b7z\u00fcng\u00b7te", "Schlan\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gestreichelt vom Sonnenstrahl.", "tokens": ["Ge\u00b7strei\u00b7chelt", "vom", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Fliehe sanfte N\u00e4chtg\u00e4nge", "tokens": ["Flie\u00b7he", "sanf\u00b7te", "N\u00e4cht\u00b7g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "In der Mondend\u00e4mmerung,", "tokens": ["In", "der", "Mon\u00b7den\u00b7d\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Dort halten zuckende Kr\u00f6ten", "tokens": ["Dort", "hal\u00b7ten", "zu\u00b7cken\u00b7de", "Kr\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zusammenk\u00fcnfte auf Kreuzwegen.", "tokens": ["Zu\u00b7sam\u00b7men\u00b7k\u00fcnf\u00b7te", "auf", "Kreuz\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schaden sie nicht,", "tokens": ["Scha\u00b7den", "sie", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Werden sie schrecken.", "tokens": ["Wer\u00b7den", "sie", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ehrlich Mann,", "tokens": ["Ehr\u00b7lich", "Mann", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Fliehe dieses Land!", "tokens": ["Flie\u00b7he", "die\u00b7ses", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}