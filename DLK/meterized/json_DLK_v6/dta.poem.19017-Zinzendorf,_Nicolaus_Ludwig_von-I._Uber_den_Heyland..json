{"dta.poem.19017": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "I.   Uber den Heyland.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du treuer Heyland! allerliebstes Leben!", "tokens": ["Du", "treu\u00b7er", "Hey\u00b7land", "!", "al\u00b7ler\u00b7liebs\u00b7tes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich, dein Gesch\u00f6pff, mu\u00df zittern und erbeben", "tokens": ["Ich", ",", "dein", "Ge\u00b7sch\u00f6pff", ",", "mu\u00df", "zit\u00b7tern", "und", "er\u00b7be\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor deinen schweren Leibs- und Seelen-Plagen,", "tokens": ["Vor", "dei\u00b7nen", "schwe\u00b7ren", "Leibs", "und", "See\u00b7len\u00b7Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dich geschlagen.", "tokens": ["Die", "dich", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich S\u00fcnder solte einst den Frevel b\u00fcssen,", "tokens": ["Ich", "S\u00fcn\u00b7der", "sol\u00b7te", "einst", "den", "Fre\u00b7vel", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den, wider deinen Winck und das Gewissen,", "tokens": ["Den", ",", "wi\u00b7der", "dei\u00b7nen", "Win\u00b7ck", "und", "das", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPOSAT", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Der ersten Eltern Ungehorsam \u00fcbte,", "tokens": ["Der", "ers\u00b7ten", "El\u00b7tern", "Un\u00b7ge\u00b7hor\u00b7sam", "\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dich betr\u00fcbte.", "tokens": ["Und", "dich", "be\u00b7tr\u00fcb\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ach! aber, hochverdienter Seelen-Retter!", "tokens": ["Ach", "!", "a\u00b7ber", ",", "hoch\u00b7ver\u00b7dien\u00b7ter", "See\u00b7len\u00b7Ret\u00b7ter", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es traffen dich die angeflammten Wetter,", "tokens": ["Es", "traf\u00b7fen", "dich", "die", "an\u00b7ge\u00b7flamm\u00b7ten", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich von unsern frevelhafften Thaten", "tokens": ["Die", "sich", "von", "un\u00b7sern", "fre\u00b7vel\u00b7haff\u00b7ten", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entz\u00fcndet hatten.", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "hat\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wir br\u00fcsteten die S\u00fcnden-volle Glieder,", "tokens": ["Wir", "br\u00fcs\u00b7te\u00b7ten", "die", "S\u00fcn\u00b7den\u00b7vol\u00b7le", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir th\u00fcrmeten das stoltze Pfau-Gefieder,", "tokens": ["Wir", "th\u00fcr\u00b7me\u00b7ten", "das", "stolt\u00b7ze", "Pfau\u00b7Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir lebeten in lauter eitlen Freuden,", "tokens": ["Wir", "le\u00b7be\u00b7ten", "in", "lau\u00b7ter", "eit\u00b7len", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und ohne Leiden.", "tokens": ["Und", "oh\u00b7ne", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Drum musten deine theuren Glieder zittern,", "tokens": ["Drum", "mus\u00b7ten", "dei\u00b7ne", "theu\u00b7ren", "Glie\u00b7der", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein edler Leib vor Angst und Grau\u00df ersch\u00fcttern:", "tokens": ["Dein", "ed\u00b7ler", "Leib", "vor", "Angst", "und", "Grau\u00df", "er\u00b7sch\u00fct\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Di\u00df must du blo\u00df allein vor unsre Schulden,", "tokens": ["Di\u00df", "must", "du", "blo\u00df", "al\u00b7lein", "vor", "uns\u00b7re", "Schul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Liebe dulden.", "tokens": ["Aus", "Lie\u00b7be", "dul\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Drum habe Danck, du edler Freund der Seelen!", "tokens": ["Drum", "ha\u00b7be", "Danck", ",", "du", "ed\u00b7ler", "Freund", "der", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "$,", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach! nimm uns ein in deine Seiten-H\u00f6hlen;", "tokens": ["Ach", "!", "nimm", "uns", "ein", "in", "dei\u00b7ne", "Sei\u00b7ten\u00b7H\u00f6h\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ART", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}