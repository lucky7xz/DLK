{"textgrid.poem.50577": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "42. Wehrt des \u00e4usserlichen Zierahts", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als einst ", "tokens": ["Als", "einst"], "token_info": ["word", "word"], "pos": ["KOUS", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "An Eine, die im ", "tokens": ["An", "Ei\u00b7ne", ",", "die", "im"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "$,", "PRELS", "APPRART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So zog sie ihren Handschuh ab", "tokens": ["So", "zog", "sie", "ih\u00b7ren", "Hand\u00b7schuh", "ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der eine ", "tokens": ["Der", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ich halte, sagt' er, mehr ", "tokens": ["Ich", "hal\u00b7te", ",", "sagt'", "er", ",", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ich, sprach sie, ich ziehe hier,", "tokens": ["Und", "ich", ",", "sprach", "sie", ",", "ich", "zie\u00b7he", "hier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ihn fassend bey dem ", "tokens": ["Ihn", "fas\u00b7send", "bey", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}