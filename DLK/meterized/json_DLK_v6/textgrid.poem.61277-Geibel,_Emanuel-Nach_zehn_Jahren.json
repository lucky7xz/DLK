{"textgrid.poem.61277": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Nach zehn Jahren", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der Schwester Haus nach langer Irrfahrt", "tokens": ["In", "der", "Schwes\u00b7ter", "Haus", "nach", "lan\u00b7ger", "Irr\u00b7fahrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Trat ich ein; da h\u00f6rt' ich's drinnen jauchzen", "tokens": ["Trat", "ich", "ein", ";", "da", "h\u00f6rt'", "ich's", "drin\u00b7nen", "jauch\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PIS", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hell von unbekannten Kinderstimmen.", "tokens": ["Hell", "von", "un\u00b7be\u00b7kann\u00b7ten", "Kin\u00b7der\u00b7stim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sieh, und im Gemach, in das der Abend", "tokens": ["Sieh", ",", "und", "im", "Ge\u00b7mach", ",", "in", "das", "der", "A\u00b7bend"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "APPRART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Golden flutete durch schattend Weinlaub,", "tokens": ["Gol\u00b7den", "flu\u00b7te\u00b7te", "durch", "schat\u00b7tend", "Wein\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sah ich wohlgemut die Kleinen spielen,", "tokens": ["Sah", "ich", "wohl\u00b7ge\u00b7mut", "die", "Klei\u00b7nen", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Sieben an der Zahl. Die blonden H\u00e4upter", "tokens": ["Sie\u00b7ben", "an", "der", "Zahl", ".", "Die", "blon\u00b7den", "H\u00e4up\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "APPR", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Tummelten im reichergo\u00dfnen Schimmer", "tokens": ["Tum\u00b7mel\u00b7ten", "im", "rei\u00b7cher\u00b7go\u00df\u00b7nen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Froh umher, und wie die Rosen bl\u00fchten", "tokens": ["Froh", "um\u00b7her", ",", "und", "wie", "die", "Ro\u00b7sen", "bl\u00fch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ihre Wangen von gesunder Frische.", "tokens": ["Ih\u00b7re", "Wan\u00b7gen", "von", "ge\u00b7sun\u00b7der", "Fri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ach, sie alle waren nicht geboren,", "tokens": ["Ach", ",", "sie", "al\u00b7le", "wa\u00b7ren", "nicht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "PIS", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Als ich auszog, durch die Welt zu schweifen,", "tokens": ["Als", "ich", "aus\u00b7zog", ",", "durch", "die", "Welt", "zu", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Selbst die Namen wu\u00dft' ich kaum zu nennen.", "tokens": ["Selbst", "die", "Na\u00b7men", "wu\u00dft'", "ich", "kaum", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Still verwundert drum mit gro\u00dfen Augen", "tokens": ["Still", "ver\u00b7wun\u00b7dert", "drum", "mit", "gro\u00b7\u00dfen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schauten sie mich an, das Spiel verstummte,", "tokens": ["Schau\u00b7ten", "sie", "mich", "an", ",", "das", "Spiel", "ver\u00b7stumm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die \u00c4lteste, mir sch\u00fcchtern nahend,", "tokens": ["Und", "die", "\u00c4l\u00b7tes\u00b7te", ",", "mir", "sch\u00fcch\u00b7tern", "na\u00b7hend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Fragte mit der Mutter Ton: \u00bbWer bist du?\u00ab", "tokens": ["Frag\u00b7te", "mit", "der", "Mut\u00b7ter", "Ton", ":", "\u00bb", "Wer", "bist", "du", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$.", "$(", "PWS", "VAFIN", "PPER", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doch da kam die Schwester. In die Arme", "tokens": ["Doch", "da", "kam", "die", "Schwes\u00b7ter", ".", "In", "die", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sank ich ihr, und dann voll Wonne zeigte", "tokens": ["Sank", "ich", "ihr", ",", "und", "dann", "voll", "Won\u00b7ne", "zeig\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KON", "ADV", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Sie die Kinder mir, den Schatz des Hauses,", "tokens": ["Sie", "die", "Kin\u00b7der", "mir", ",", "den", "Schatz", "des", "Hau\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PPER", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Der so lieblich sich gemehrt, und zeigte", "tokens": ["Der", "so", "lieb\u00b7lich", "sich", "ge\u00b7mehrt", ",", "und", "zeig\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADJD", "PRF", "VVPP", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Dann den heimgekehrten Ohm den Kindern.", "tokens": ["Dann", "den", "heim\u00b7ge\u00b7kehr\u00b7ten", "Ohm", "den", "Kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und nun gab's ein Jubeln, rasch entschlossen", "tokens": ["Und", "nun", "gab's", "ein", "Ju\u00b7beln", ",", "rasch", "ent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Kletterten an mir empor die Buben,", "tokens": ["Klet\u00b7ter\u00b7ten", "an", "mir", "em\u00b7por", "die", "Bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Mich zu k\u00fcssen, und die M\u00e4dchen bogen", "tokens": ["Mich", "zu", "k\u00fcs\u00b7sen", ",", "und", "die", "M\u00e4d\u00b7chen", "bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Mir das Haupt herab, und selbst das Kleinste,", "tokens": ["Mir", "das", "Haupt", "her\u00b7ab", ",", "und", "selbst", "das", "Kleins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$,", "KON", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Das sich erst gescheut vor meinem Barte,", "tokens": ["Das", "sich", "erst", "ge\u00b7scheut", "vor", "mei\u00b7nem", "Bar\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Tastete nach mir mit seinen H\u00e4ndchen.", "tokens": ["Tas\u00b7te\u00b7te", "nach", "mir", "mit", "sei\u00b7nen", "H\u00e4nd\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "O wie ward mir's wohl, so ganz umschlungen,", "tokens": ["O", "wie", "ward", "mir's", "wohl", ",", "so", "ganz", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "NE", "ADV", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ganz umrankt vom jungen, frischen Leben,", "tokens": ["Ganz", "um\u00b7rankt", "vom", "jun\u00b7gen", ",", "fri\u00b7schen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das wie eine Bienentraub' am Stocke", "tokens": ["Das", "wie", "ei\u00b7ne", "Bienen\u00b7traub'", "am", "Sto\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KOKOM", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Um mich hing und tausend Wunder fragte!", "tokens": ["Um", "mich", "hing", "und", "tau\u00b7send", "Wun\u00b7der", "frag\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "KON", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aber leise ging ein Hauch der Wehmut", "tokens": ["A\u00b7ber", "lei\u00b7se", "ging", "ein", "Hauch", "der", "Weh\u00b7mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Durch das Herz mir doch, denn diese K\u00fcsse,", "tokens": ["Durch", "das", "Herz", "mir", "doch", ",", "denn", "die\u00b7se", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "$,", "KON", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Diese Fragen, die mich rings best\u00fcrmten,", "tokens": ["Die\u00b7se", "Fra\u00b7gen", ",", "die", "mich", "rings", "be\u00b7st\u00fcrm\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mahnten sie zugleich nicht: So viel Schritte", "tokens": ["Mahn\u00b7ten", "sie", "zu\u00b7gleich", "nicht", ":", "So", "viel", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$.", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Hast auch du getan dem Tod entgegen,", "tokens": ["Hast", "auch", "du", "ge\u00b7tan", "dem", "Tod", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVPP", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und schon reift in ihnen t\u00e4glich rascher", "tokens": ["Und", "schon", "reift", "in", "ih\u00b7nen", "t\u00e4g\u00b7lich", "ra\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "ADJD", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Das Geschlecht, das \u00fcber deinem Grabe", "tokens": ["Das", "Ge\u00b7schlecht", ",", "das", "\u00fc\u00b7ber", "dei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Wandeln soll und selig sein und weinen.", "tokens": ["Wan\u00b7deln", "soll", "und", "se\u00b7lig", "sein", "und", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KON", "ADJD", "VAINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Und wie segnend legt' ich meine H\u00e4nde", "tokens": ["Und", "wie", "seg\u00b7nend", "legt'", "ich", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Auf ihr Haupt und dachte still die Worte:", "tokens": ["Auf", "ihr", "Haupt", "und", "dach\u00b7te", "still", "die", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Seid gegr\u00fc\u00dft, ihr holden Todesboten!", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ihr", "hol\u00b7den", "To\u00b7des\u00b7bo\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Seid gegr\u00fc\u00dft, ich dank' euch, da\u00df so lieblich", "tokens": ["Seid", "ge\u00b7gr\u00fc\u00dft", ",", "ich", "dank'", "euch", ",", "da\u00df", "so", "lieb\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "VVPP", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Ihr den ernsten Gru\u00df an mich bestellt habt!", "tokens": ["Ihr", "den", "erns\u00b7ten", "Gru\u00df", "an", "mich", "be\u00b7stellt", "habt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Aber ihr - zu vollem Leben freudig", "tokens": ["A\u00b7ber", "ihr", "zu", "vol\u00b7lem", "Le\u00b7ben", "freu\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "$(", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Wachset auf, da\u00df, wenn ich einst dahin bin,", "tokens": ["Wach\u00b7set", "auf", ",", "da\u00df", ",", "wenn", "ich", "einst", "da\u00b7hin", "bin", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "$,", "KOUS", "PPER", "ADV", "PAV", "VAFIN", "$,"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Ihr vollenden m\u00f6gt mit euren Br\u00fcdern,", "tokens": ["Ihr", "voll\u00b7en\u00b7den", "m\u00f6gt", "mit", "eu\u00b7ren", "Br\u00fc\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Was ich selbst und mein Geschlecht nicht konnte!", "tokens": ["Was", "ich", "selbst", "und", "mein", "Ge\u00b7schlecht", "nicht", "konn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "KON", "PPOSAT", "NN", "PTKNEG", "VMFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}