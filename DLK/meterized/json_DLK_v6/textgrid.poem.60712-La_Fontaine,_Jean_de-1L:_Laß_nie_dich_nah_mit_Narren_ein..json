{"textgrid.poem.60712": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df nie dich nah mit Narren ein.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df nie dich nah mit Narren ein.", "tokens": ["La\u00df", "nie", "dich", "nah", "mit", "Nar\u00b7ren", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "ADJD", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist ein Ratschlag ohnegleichen,", "tokens": ["Das", "ist", "ein", "Rat\u00b7schlag", "oh\u00b7ne\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn nichts scheint kl\u00fcger mir zu sein,", "tokens": ["Denn", "nichts", "scheint", "kl\u00fc\u00b7ger", "mir", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "PPER", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als tollen K\u00f6pfen auszuweichen.", "tokens": ["Als", "tol\u00b7len", "K\u00f6p\u00b7fen", "aus\u00b7zu\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bei Hofe stehn sie oft in Gunst,", "tokens": ["Bei", "Ho\u00b7fe", "stehn", "sie", "oft", "in", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der F\u00fcrst erfreut sich ihrer Kunst,", "tokens": ["Der", "F\u00fcrst", "er\u00b7freut", "sich", "ih\u00b7rer", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn f\u00fcr die Schelme, Toren, Gecken", "tokens": ["Denn", "f\u00fcr", "die", "Schel\u00b7me", ",", "To\u00b7ren", ",", "Ge\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sind Narren wahre Vogelschrecken.", "tokens": ["Sind", "Nar\u00b7ren", "wah\u00b7re", "Vo\u00b7gel\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Narr lief um und rief an allen Ecken,", "tokens": ["Ein", "Narr", "lief", "um", "und", "rief", "an", "al\u00b7len", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihm sei Weisheit zu erkaufen.", "tokens": ["Von", "ihm", "sei", "Weis\u00b7heit", "zu", "er\u00b7kau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da kamen massenhaft die Gl\u00e4ubigen gelaufen.", "tokens": ["Da", "ka\u00b7men", "mas\u00b7sen\u00b7haft", "die", "Gl\u00e4u\u00b7bi\u00b7gen", "ge\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Narr schnitt ihnen Fratzen,", "tokens": ["Der", "Narr", "schnitt", "ih\u00b7nen", "Frat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann gab's f\u00fcr gute Batzen", "tokens": ["Dann", "gab's", "f\u00fcr", "gu\u00b7te", "Bat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nebst einem Backenstreich zweiklafterlange Faden.", "tokens": ["Nebst", "ei\u00b7nem", "Ba\u00b7cken\u00b7streich", "zwei\u00b7klaf\u00b7ter\u00b7lan\u00b7ge", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die meisten murrten: was sind das f\u00fcr Sachen!", "tokens": ["Die", "meis\u00b7ten", "murr\u00b7ten", ":", "was", "sind", "das", "f\u00fcr", "Sa\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$.", "PWS", "VAFIN", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch Spott nur folgte ihrem Schaden.", "tokens": ["Doch", "Spott", "nur", "folg\u00b7te", "ih\u00b7rem", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Das beste war, darob zu lachen", "tokens": ["Das", "bes\u00b7te", "war", ",", "da\u00b7rob", "zu", "la\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "$,", "PAV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und sich mit Maulschelle und Faden", "tokens": ["Und", "sich", "mit", "Maul\u00b7schel\u00b7le", "und", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.11": {"text": "Geschwinde aus dem Staub zu machen.", "tokens": ["Ge\u00b7schwin\u00b7de", "aus", "dem", "Staub", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Man sann umsonst der Sache nach,", "tokens": ["Man", "sann", "um\u00b7sonst", "der", "Sa\u00b7che", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Zu finden, was der Narr versprach.", "tokens": ["Zu", "fin\u00b7den", ",", "was", "der", "Narr", "ver\u00b7sprach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Steht die Vernunft denn daf\u00fcr gut,", "tokens": ["Steht", "die", "Ver\u00b7nunft", "denn", "da\u00b7f\u00fcr", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Was solch ein Narrensch\u00e4del tut?", "tokens": ["Was", "solch", "ein", "Nar\u00b7ren\u00b7sch\u00e4\u00b7del", "tut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Zufall einzig spielt die Rollen", "tokens": ["Der", "Zu\u00b7fall", "ein\u00b7zig", "spielt", "die", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Im Hirne eines armen Tollen.", "tokens": ["Im", "Hir\u00b7ne", "ei\u00b7nes", "ar\u00b7men", "Tol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Angef\u00fchrten einer ging jedoch,", "tokens": ["Der", "An\u00b7ge\u00b7f\u00fchr\u00b7ten", "ei\u00b7ner", "ging", "je\u00b7doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Verwirrt von jenen Narrengaben noch,", "tokens": ["Ver\u00b7wirrt", "von", "je\u00b7nen", "Nar\u00b7ren\u00b7ga\u00b7ben", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zu einem Weisen hin, den er befragte,", "tokens": ["Zu", "ei\u00b7nem", "Wei\u00b7sen", "hin", ",", "den", "er", "be\u00b7frag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Worauf ihm dieser ohne Z\u00f6gern sagte:", "tokens": ["Wo\u00b7rauf", "ihm", "die\u00b7ser", "oh\u00b7ne", "Z\u00f6\u00b7gern", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PDAT", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "\u00bbdurchsichtig sind mir diese Hieroglyphen.", "tokens": ["\u00bb", "durch\u00b7sich\u00b7tig", "sind", "mir", "die\u00b7se", "Hie\u00b7ro\u00b7gly\u00b7phen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Vern\u00fcnftige Leute, die ihr Handeln pr\u00fcfen,", "tokens": ["Ver\u00b7n\u00fcnf\u00b7ti\u00b7ge", "Leu\u00b7te", ",", "die", "ihr", "Han\u00b7deln", "pr\u00fc\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Tun gut, wenn allezeit und allerwegen", "tokens": ["Tun", "gut", ",", "wenn", "al\u00b7le\u00b7zeit", "und", "al\u00b7ler\u00b7we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$,", "KOUS", "ADV", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sie zwischen sich und Narren im voraus", "tokens": ["Sie", "zwi\u00b7schen", "sich", "und", "Nar\u00b7ren", "im", "vo\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PRF", "KON", "NN", "APPRART", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Die L\u00e4nge ", "tokens": ["Die", "L\u00e4n\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Sonst bleiben Z\u00e4rtlichkeiten ", "tokens": ["Sonst", "blei\u00b7ben", "Z\u00e4rt\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVINF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Sei ganz zufrieden! Glaube mir,", "tokens": ["Sei", "ganz", "zu\u00b7frie\u00b7den", "!", "Glau\u00b7be", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$.", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Der Narr verkaufte Weisheit dir.\u00ab", "tokens": ["Der", "Narr", "ver\u00b7kauf\u00b7te", "Weis\u00b7heit", "dir", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}