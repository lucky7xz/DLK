{"textgrid.poem.63502": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "De Drom", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Paster Schult vun Snickelsnuckel", "tokens": ["De", "Pas\u00b7ter", "Schult", "vun", "Sni\u00b7ckels\u00b7nu\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "de was grad as sonn Swin so smuck,", "tokens": ["de", "was", "grad", "as", "sonn", "Swin", "so", "smuck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "KON", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "he hadd'n Puckel uppen Puckel", "tokens": ["he", "had\u00b7d'n", "Pu\u00b7ckel", "up\u00b7pen", "Pu\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "un stameln ded he as'n Buck.", "tokens": ["un", "sta\u00b7meln", "ded", "he", "as'n", "Buck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Sin Pr\u00e4digt wu\u00df he as'n Stacker", "tokens": ["Sin", "Pr\u00e4\u00b7digt", "wu\u00df", "he", "as'n", "Sta\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "un sch\u00f6t koppheister d\u00f6rch sin R\u00e4d';", "tokens": ["un", "sch\u00f6t", "kop\u00b7pheis\u00b7ter", "d\u00f6rch", "sin", "R\u00e4d'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "man in Gottswurt was he'n Racker", "tokens": ["man", "in", "Gotts\u00b7wurt", "was", "he'n", "Ra\u00b7cker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "NE", "PWS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "un d\u00fccht sick mihr, as Luther ded.", "tokens": ["un", "d\u00fccht", "sick", "mihr", ",", "as", "Lu\u00b7ther", "ded", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "He was dor heel un gor nah andan,", "tokens": ["He", "was", "dor", "heel", "un", "gor", "nah", "an\u00b7dan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wen em nich ihrt, wen he nich m\u00f6cht,", "tokens": ["wen", "em", "nich", "ihrt", ",", "wen", "he", "nich", "m\u00f6cht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "VVPP", "$,", "PWS", "VVFIN", "PTKNEG", "VMFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "den hadd he girn in Bu\u00df un Bann dan", "tokens": ["den", "hadd", "he", "girn", "in", "Bu\u00df", "un", "Bann", "dan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "FM", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "un em dat Abendmahl verseggt,", "tokens": ["un", "em", "dat", "A\u00b7bend\u00b7mahl", "ver\u00b7seggt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "den hadd he girn turn Kak verdunnert,", "tokens": ["den", "hadd", "he", "girn", "turn", "Kak", "ver\u00b7dun\u00b7nert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "as Hexenmeister glik verbrennt,", "tokens": ["as", "He\u00b7xen\u00b7meis\u00b7ter", "glik", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wir dat man noch dat oll Johrhunnert,", "tokens": ["wir", "dat", "man", "noch", "dat", "oll", "Johr\u00b7hun\u00b7nert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "ART", "ADJD", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "hadd he man d\u00f6rft, hadd he man k\u00fcnnt.", "tokens": ["hadd", "he", "man", "d\u00f6rft", ",", "hadd", "he", "man", "k\u00fcnnt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "PIS", "VVFIN", "$,", "FM", "FM", "PIS", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "(he was sonn rechten Heukendr\u00e4ger,", "tokens": ["(", "he", "was", "sonn", "rech\u00b7ten", "Heu\u00b7ken\u00b7dr\u00e4\u00b7ger", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "KON", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "butwennig witt un blank un glatt,", "tokens": ["but\u00b7wen\u00b7nig", "witt", "un", "blank", "un", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "FM", "FM", "FM", "FM", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "nah \u00fcnnen groff, nah baben s\u00f6t.)", "tokens": ["nah", "\u00fcn\u00b7nen", "groff", ",", "nah", "ba\u00b7ben", "s\u00f6t", ".", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "ADJD", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "He was'n Macker von de Framen,", "tokens": ["He", "was'n", "Ma\u00b7cker", "von", "de", "Fra\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "as Sirup s\u00f6t bi v\u00f6rn\u00e4hm L\u00fcr,", "tokens": ["as", "Si\u00b7rup", "s\u00f6t", "bi", "v\u00f6r\u00b7n\u00e4hm", "L\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "uns' Herrgott un sin'n S\u0153hn sin Namen", "tokens": ["un\u00b7s'", "Herr\u00b7gott", "un", "sin'n", "S\u0153hn", "sin", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "FM", "FM", "FM", "FM", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "de was em dor as Kaff so d\u00fcr.", "tokens": ["de", "was", "em", "dor", "as", "Kaff", "so", "d\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bi v\u00f6rn\u00e4hm Volk dor makt he krumm sick", "tokens": ["Bi", "v\u00f6r\u00b7n\u00e4hm", "Volk", "dor", "makt", "he", "krumm", "sick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "un gaff de Sak ehr rechten Klemm,", "tokens": ["un", "gaff", "de", "Sak", "ehr", "rech\u00b7ten", "Klemm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "\u0153wer mit den D\u00fcwel smet he \u00fcm sick", "tokens": ["\u0153wer", "mit", "den", "D\u00fc\u00b7wel", "smet", "he", "\u00fcm", "sick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "FM", "FM", "FM", "FM"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "bi all de Minschen \u00fcnner em.", "tokens": ["bi", "all", "de", "Min\u00b7schen", "\u00fcn\u00b7ner", "em."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}