{"textgrid.poem.63585": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkst du des Abends noch im Karneval?", "tokens": ["Denkst", "du", "des", "A\u00b7bends", "noch", "im", "Kar\u00b7ne\u00b7val", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Kinder hatten auch ihr Mummensch\u00e4nzchen", "tokens": ["Die", "Kin\u00b7der", "hat\u00b7ten", "auch", "ihr", "Mum\u00b7men\u00b7sch\u00e4nz\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und drehten sich vergn\u00fcgt in Flur und Saal.", "tokens": ["Und", "dreh\u00b7ten", "sich", "ver\u00b7gn\u00fcgt", "in", "Flur", "und", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und unser Nestling, noch ein zartes Pfl\u00e4nzchen,", "tokens": ["Und", "un\u00b7ser", "Nest\u00b7ling", ",", "noch", "ein", "zar\u00b7tes", "Pfl\u00e4nz\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch mit der Lust den Beinchen weit voraus,", "tokens": ["Doch", "mit", "der", "Lust", "den", "Be\u00b7in\u00b7chen", "weit", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Verlangte wie die andern auch sein T\u00e4nzchen.", "tokens": ["Ver\u00b7lang\u00b7te", "wie", "die", "an\u00b7dern", "auch", "sein", "T\u00e4nz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie sah das s\u00fc\u00dfe Dirnchen reizend aus", "tokens": ["Wie", "sah", "das", "s\u00fc\u00b7\u00dfe", "Dirn\u00b7chen", "rei\u00b7zend", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Im Schwabenh\u00e4ubchen, goldgesticktem Mieder,", "tokens": ["Im", "Schwa\u00b7ben\u00b7h\u00e4ub\u00b7chen", ",", "gold\u00b7ge\u00b7stick\u00b7tem", "Mie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Vielfalt'gem Rock mit B\u00e4ndern braun und kraus.", "tokens": ["Viel\u00b7falt'\u00b7gem", "Rock", "mit", "B\u00e4n\u00b7dern", "braun", "und", "kraus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wie sah es sich im Spiegel immer wieder", "tokens": ["Wie", "sah", "es", "sich", "im", "Spie\u00b7gel", "im\u00b7mer", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und lachte selbst sich an, das Evaskind,", "tokens": ["Und", "lach\u00b7te", "selbst", "sich", "an", ",", "das", "E\u00b7vas\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PRF", "PTKVZ", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und regte nach dem Takt die kleinen Glieder.", "tokens": ["Und", "reg\u00b7te", "nach", "dem", "Takt", "die", "klei\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und da ich's auf den Arm nahm und geschwind", "tokens": ["Und", "da", "ich's", "auf", "den", "Arm", "nahm", "und", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Im Kreise schwang, die kleine T\u00e4nzrin wiegend,", "tokens": ["Im", "Krei\u00b7se", "schwang", ",", "die", "klei\u00b7ne", "T\u00e4nz\u00b7rin", "wie\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die H\u00e4rchen flogen ihr im Wirbelwind.", "tokens": ["Die", "H\u00e4r\u00b7chen", "flo\u00b7gen", "ihr", "im", "Wir\u00b7bel\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Sie aber sa\u00df, sich furchtlos an mich schmiegend", "tokens": ["Sie", "a\u00b7ber", "sa\u00df", ",", "sich", "furcht\u00b7los", "an", "mich", "schmie\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PRF", "ADJD", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und sah auf das Get\u00fcmmel stolz herab;", "tokens": ["Und", "sah", "auf", "das", "Ge\u00b7t\u00fcm\u00b7mel", "stolz", "her\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mehr! bat sie, mehr! mit Schmeicheln mich besiegend.", "tokens": ["Mehr", "!", "bat", "sie", ",", "mehr", "!", "mit", "Schmei\u00b7cheln", "mich", "be\u00b7sie\u00b7gend", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "$.", "VVFIN", "PPER", "$,", "ADV", "$.", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "So unerm\u00fcdlich flog sie auf und ab.", "tokens": ["So", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "flog", "sie", "auf", "und", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die wird viel Schuhe brauchen! sagt' ich lachend,", "tokens": ["Die", "wird", "viel", "Schu\u00b7he", "brau\u00b7chen", "!", "sagt'", "ich", "la\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "VVFIN", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Als ich sie endlich ihrer W\u00e4rtrin gab.", "tokens": ["Als", "ich", "sie", "end\u00b7lich", "ih\u00b7rer", "W\u00e4r\u00b7trin", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und wir dann scherzten, stolze Pl\u00e4ne machend,", "tokens": ["Und", "wir", "dann", "scherz\u00b7ten", ",", "stol\u00b7ze", "Pl\u00e4\u00b7ne", "ma\u00b7chend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Wie \u00fcber sechzehn Jahr wir n\u00e4chtelang", "tokens": ["Wie", "\u00fc\u00b7ber", "sech\u00b7zehn", "Jahr", "wir", "n\u00e4ch\u00b7tel\u00b7ang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "CARD", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Dasitzen w\u00fcrden, unsern Schatz bewachend;", "tokens": ["Da\u00b7sit\u00b7zen", "w\u00fcr\u00b7den", ",", "un\u00b7sern", "Schatz", "be\u00b7wa\u00b7chend", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Wenn mit dem Veilchenkranz bei Geigenklang", "tokens": ["Wenn", "mit", "dem", "Veil\u00b7chen\u00b7kranz", "bei", "Gei\u00b7gen\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Das schlanke Kind sich wiegen w\u00fcrd' im Tanze", "tokens": ["Das", "schlan\u00b7ke", "Kind", "sich", "wie\u00b7gen", "w\u00fcrd'", "im", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "VVPP", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "In heller Jugendwonnen \u00dcberschwang,", "tokens": ["In", "hel\u00b7ler", "Ju\u00b7gend\u00b7won\u00b7nen", "\u00dc\u00b7bersc\u00b7hwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und wie mit ihrer Augen dunklem Glanze", "tokens": ["Und", "wie", "mit", "ih\u00b7rer", "Au\u00b7gen", "dunk\u00b7lem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Sie Herzen w\u00fcrde, jung und alt, gewinnen", "tokens": ["Sie", "Her\u00b7zen", "w\u00fcr\u00b7de", ",", "jung", "und", "alt", ",", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPER", "NN", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und uns anl\u00e4cheln unter ihrem Kranze.", "tokens": ["Und", "uns", "an\u00b7l\u00e4\u00b7cheln", "un\u00b7ter", "ih\u00b7rem", "Kran\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Und nun \u2013 nun f\u00fchrt' ein T\u00e4nzer sie von hinnen,", "tokens": ["Und", "nun", "\u2013", "nun", "f\u00fchrt'", "ein", "T\u00e4n\u00b7zer", "sie", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VVFIN", "ART", "NN", "PPER", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Dem sie mit Str\u00e4uben folgte, dessen Reigen", "tokens": ["Dem", "sie", "mit", "Str\u00e4u\u00b7ben", "folg\u00b7te", ",", "des\u00b7sen", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Das Blut ihr in den Adern lie\u00df gerinnen.", "tokens": ["Das", "Blut", "ihr", "in", "den", "A\u00b7dern", "lie\u00df", "ge\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wir h\u00f6ren keinen Ton von muntern Geigen,", "tokens": ["Wir", "h\u00f6\u00b7ren", "kei\u00b7nen", "Ton", "von", "mun\u00b7tern", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Wei\u00df ist der Kranz, die Wangen und das Kleid,", "tokens": ["Wei\u00df", "ist", "der", "Kranz", ",", "die", "Wan\u00b7gen", "und", "das", "Kleid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Und wir \u2013 wir h\u00fcten unser Kind in Schweigen,", "tokens": ["Und", "wir", "\u2013", "wir", "h\u00fc\u00b7ten", "un\u00b7ser", "Kind", "in", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Denn Spiel und Tanz ist aus, lang vor der Zeit.", "tokens": ["Denn", "Spiel", "und", "Tanz", "ist", "aus", ",", "lang", "vor", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PTKVZ", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}