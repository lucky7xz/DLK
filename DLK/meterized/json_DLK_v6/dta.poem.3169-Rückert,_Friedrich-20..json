{"dta.poem.3169": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ungleich gestellt sind Gl\u00fcck und Ungl\u00fcck in dem einen,", "tokens": ["Un\u00b7gleich", "ge\u00b7stellt", "sind", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "in", "dem", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "NN", "KON", "NN", "APPR", "ART", "ART", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da\u00df einen Gipfel jen's wohl hat, doch dieses keinen.", "tokens": ["Da\u00df", "ei\u00b7nen", "Gip\u00b7fel", "jen's", "wohl", "hat", ",", "doch", "die\u00b7ses", "kei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "ADV", "VAFIN", "$,", "ADV", "PDAT", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So gl\u00fccklich kannst du schon geworden seyn, da\u00df nun", "tokens": ["So", "gl\u00fcck\u00b7lich", "kannst", "du", "schon", "ge\u00b7wor\u00b7den", "seyn", ",", "da\u00df", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "ADV", "VAPP", "VAINF", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Zuwachs kein Gewicht kann in die Wage thun.", "tokens": ["Ein", "Zu\u00b7wachs", "kein", "Ge\u00b7wicht", "kann", "in", "die", "Wa\u00b7ge", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch so ungl\u00fccklich nie, da\u00df nicht die Schale schwerer", "tokens": ["Doch", "so", "un\u00b7gl\u00fcck\u00b7lich", "nie", ",", "da\u00df", "nicht", "die", "Scha\u00b7le", "schwe\u00b7rer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "$,", "KOUS", "PTKNEG", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch werden kann, wodurch? vernimm's von deinem Lehrer:", "tokens": ["Noch", "wer\u00b7den", "kann", ",", "wo\u00b7durch", "?", "ver\u00b7nim\u00b7m's", "von", "dei\u00b7nem", "Leh\u00b7rer", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "VMFIN", "$,", "PWAV", "$.", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Dadurch, da\u00df, wenn du schon verlorest jedes Gut,", "tokens": ["Da\u00b7durch", ",", "da\u00df", ",", "wenn", "du", "schon", "ver\u00b7lo\u00b7rest", "je\u00b7des", "Gut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "$,", "KOUS", "PPER", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du obendrein verlierst Fassung und Lebensmuth.", "tokens": ["Du", "o\u00b7ben\u00b7drein", "ver\u00b7lierst", "Fas\u00b7sung", "und", "Le\u00b7bens\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}