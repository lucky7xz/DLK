{"textgrid.poem.26292": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Der Baum am H\u00fcgelrand", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Baum, allein am H\u00fcgelrand,", "tokens": ["Du", "Baum", ",", "al\u00b7lein", "am", "H\u00fc\u00b7gel\u00b7rand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Einsamsein ist mir verwandt.", "tokens": ["Dein", "Ein\u00b7sam\u00b7sein", "ist", "mir", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du siehst wie ich den Tagen nach,", "tokens": ["Du", "siehst", "wie", "ich", "den", "Ta\u00b7gen", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ruhlos rauscht dein Bl\u00e4tterdach.", "tokens": ["Und", "ruh\u00b7los", "rauscht", "dein", "Bl\u00e4t\u00b7ter\u00b7dach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Wolken, Wind, o, Abendland,", "tokens": ["O", "Wol\u00b7ken", ",", "Wind", ",", "o", ",", "A\u00b7bend\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "FM", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie seid ihr Schweigenden mir verwandt!", "tokens": ["Wie", "seid", "ihr", "Schwei\u00b7gen\u00b7den", "mir", "ver\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Blitz springt \u00fcbers dunkle Kraut, \u2013", "tokens": ["Ein", "Blitz", "springt", "\u00fc\u00b7bers", "dunk\u00b7le", "Kraut", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ewigkeit hat uns angeschaut.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "hat", "uns", "an\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Das Leben, \u2013 ein feuriger Augenblick!", "tokens": ["Das", "Le\u00b7ben", ",", "\u2013", "ein", "feu\u00b7ri\u00b7ger", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und Sehnsucht und Sehnsucht ist unser Geschick.", "tokens": ["Und", "Sehn\u00b7sucht", "und", "Sehn\u00b7sucht", "ist", "un\u00b7ser", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}