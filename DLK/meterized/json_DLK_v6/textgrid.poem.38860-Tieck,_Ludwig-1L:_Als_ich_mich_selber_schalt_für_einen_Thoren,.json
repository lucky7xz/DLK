{"textgrid.poem.38860": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich mich selber schalt f\u00fcr einen Thoren,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich mich selber schalt f\u00fcr einen Thoren,", "tokens": ["Als", "ich", "mich", "sel\u00b7ber", "schalt", "f\u00fcr", "ei\u00b7nen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der allerherbste Schmerz mich wollt' erdr\u00fccken,", "tokens": ["Der", "al\u00b7ler\u00b7herbs\u00b7te", "Schmerz", "mich", "wollt'", "er\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor\u00fcber Hoffnung, Zutraun und Erquicken,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "Hoff\u00b7nung", ",", "Zu\u00b7traun", "und", "Er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df ich irrw\u00e4hnend ihn als Freund erkohren.", "tokens": ["Da\u00df", "ich", "irr\u00b7w\u00e4h\u00b7nend", "ihn", "als", "Freund", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da fand ich dich und wurde neu gebohren,", "tokens": ["Da", "fand", "ich", "dich", "und", "wur\u00b7de", "neu", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KON", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ahndung sprach: nein, la\u00df dich nicht ber\u00fccken,", "tokens": ["Die", "Ahn\u00b7dung", "sprach", ":", "nein", ",", "la\u00df", "dich", "nicht", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PTKANT", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es darf dir auch mit diesem Freund nie gl\u00fccken,", "tokens": ["Es", "darf", "dir", "auch", "mit", "die\u00b7sem", "Freund", "nie", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn kaum gefunden, ist er dir verlohren.", "tokens": ["Denn", "kaum", "ge\u00b7fun\u00b7den", ",", "ist", "er", "dir", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein gleiches Liebesband schien uns zu einen,", "tokens": ["Ein", "glei\u00b7ches", "Lie\u00b7bes\u00b7band", "schien", "uns", "zu", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein doppelt Gl\u00fcck entgegen uns zu lachen,", "tokens": ["Ein", "dop\u00b7pelt", "Gl\u00fcck", "ent\u00b7ge\u00b7gen", "uns", "zu", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Morgenschimmer freundlich aufzusteigen;", "tokens": ["Ein", "Mor\u00b7gen\u00b7schim\u00b7mer", "freund\u00b7lich", "auf\u00b7zu\u00b7stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch mu\u00dft ich bald den s\u00fc\u00dfen Trug beweinen,", "tokens": ["Doch", "mu\u00dft", "ich", "bald", "den", "s\u00fc\u00b7\u00dfen", "Trug", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Abendroth schien auf den stillen Nachen,", "tokens": ["Das", "A\u00b7ben\u00b7droth", "schien", "auf", "den", "stil\u00b7len", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Nacht empfing dich und das ewge Schweigen.", "tokens": ["Die", "Nacht", "emp\u00b7fing", "dich", "und", "das", "ew\u00b7ge", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}