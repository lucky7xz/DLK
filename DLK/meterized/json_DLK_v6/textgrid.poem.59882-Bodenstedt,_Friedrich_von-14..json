{"textgrid.poem.59882": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es drehn die Welten sich im Kreise,", "tokens": ["Es", "drehn", "die", "Wel\u00b7ten", "sich", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wandeln stets die alten Gleise.", "tokens": ["Sie", "wan\u00b7deln", "stets", "die", "al\u00b7ten", "Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es geht die Menscheit ihre Bahn", "tokens": ["Es", "geht", "die", "Men\u00b7scheit", "ih\u00b7re", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Grabe, wie sie stets getan.", "tokens": ["Zum", "Gra\u00b7be", ",", "wie", "sie", "stets", "ge\u00b7tan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es bl\u00fcht die Blume wunderbar", "tokens": ["Es", "bl\u00fcht", "die", "Blu\u00b7me", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und welkt wie einst und immerdar.", "tokens": ["Und", "welkt", "wie", "einst", "und", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zerst\u00f6rend ist des Lebens Lauf,", "tokens": ["Zer\u00b7st\u00f6\u00b7rend", "ist", "des", "Le\u00b7bens", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets fri\u00dft ein Tier das andre auf.", "tokens": ["Stets", "fri\u00dft", "ein", "Tier", "das", "and\u00b7re", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es n\u00e4hrt vom Tode sich das Leben,", "tokens": ["Es", "n\u00e4hrt", "vom", "To\u00b7de", "sich", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dies mu\u00df jenem Nahrung geben.", "tokens": ["Und", "dies", "mu\u00df", "je\u00b7nem", "Nah\u00b7rung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein wenig Werden und Vergehn,", "tokens": ["Ein", "we\u00b7nig", "Wer\u00b7den", "und", "Ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich im Kreis die Welten drehn.", "tokens": ["Wie", "sich", "im", "Kreis", "die", "Wel\u00b7ten", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Kreislauf, der zum Wahnsinn triebe,", "tokens": ["Ein", "Kreis\u00b7lauf", ",", "der", "zum", "Wahn\u00b7sinn", "trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "G\u00e4b' ihm nicht Licht und Sinn die Liebe!", "tokens": ["G\u00e4b'", "ihm", "nicht", "Licht", "und", "Sinn", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}