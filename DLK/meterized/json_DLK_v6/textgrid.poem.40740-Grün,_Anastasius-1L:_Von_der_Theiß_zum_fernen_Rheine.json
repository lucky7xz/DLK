{"textgrid.poem.40740": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von der Thei\u00df zum fernen Rheine", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von der Thei\u00df zum fernen Rheine", "tokens": ["Von", "der", "Thei\u00df", "zum", "fer\u00b7nen", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00f6lbt ein Freudenm\u00fcnster sich,", "tokens": ["W\u00f6lbt", "ein", "Freu\u00b7den\u00b7m\u00fcns\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin die ganze Christgemeine", "tokens": ["Drin", "die", "gan\u00b7ze", "Christ\u00b7ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzt: \u00bbHerr Gott, wir loben Dich!\u00ab", "tokens": ["Jauchzt", ":", "\u00bb", "Herr", "Gott", ",", "wir", "lo\u00b7ben", "Dich", "!", "\u00ab"], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$.", "$(", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ungarns Volk k\u00fc\u00dft den Befreiern", "tokens": ["Un\u00b7garns", "Volk", "k\u00fc\u00dft", "den", "Be\u00b7frei\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Kleid und Hand auf ihrer Fahrt,", "tokens": ["Kleid", "und", "Hand", "auf", "ih\u00b7rer", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unberedte Lippen feiern", "tokens": ["Un\u00b7be\u00b7red\u00b7te", "Lip\u00b7pen", "fei\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die\u00df Te-Deum fr\u00f6mmster Art.", "tokens": ["Die\u00df", "Te\u00b7De\u00b7um", "fr\u00f6mms\u00b7ter", "Art", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "In den Thronsaal vor den Kaiser", "tokens": ["In", "den", "Thron\u00b7sa\u00b7al", "vor", "den", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tritt der Prinz zum Kriegsbericht;", "tokens": ["Tritt", "der", "Prinz", "zum", "Kriegs\u00b7be\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist die Majest\u00e4t wohl heiser,", "tokens": ["Ist", "die", "Ma\u00b7jes\u00b7t\u00e4t", "wohl", "hei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie kein Willkommen spricht?", "tokens": ["Da\u00df", "sie", "kein", "Will\u00b7kom\u00b7men", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eugens Worte ziehn geschlossen", "tokens": ["Eu\u00b7gens", "Wor\u00b7te", "ziehn", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVINF", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Kolonnen in die Schlacht,", "tokens": ["Wie", "Ko\u00b7lon\u00b7nen", "in", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Festgegliedert, stahlgegossen,", "tokens": ["Fest\u00b7ge\u00b7glie\u00b7dert", ",", "stahl\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Siegsbewu\u00dft in ihrer Macht.", "tokens": ["Siegs\u00b7be\u00b7wu\u00dft", "in", "ih\u00b7rer", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie Schaum an d\u00fcrrer Klippe", "tokens": ["Doch", "wie", "Schaum", "an", "d\u00fcr\u00b7rer", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schier der Rede Fluth versank,", "tokens": ["Schier", "der", "Re\u00b7de", "Fluth", "ver\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die gr\u00f6\u00dfte Kaiserlippe", "tokens": ["Denn", "die", "gr\u00f6\u00df\u00b7te", "Kai\u00b7ser\u00b7lip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fand kein kleinstes Wort von Dank.", "tokens": ["Fand", "kein", "kleins\u00b7tes", "Wort", "von", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun die h\u00f6chste der Perr\u00fccken", "tokens": ["Nun", "die", "h\u00f6chs\u00b7te", "der", "Per\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steif dir nickt den Abschiedsgru\u00df,", "tokens": ["Steif", "dir", "nickt", "den", "Ab\u00b7schieds\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Siegesheld, magst du dich b\u00fccken,", "tokens": ["Sie\u00b7ge\u00b7sheld", ",", "magst", "du", "dich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn die Zwiesprach ist am Schlu\u00df.", "tokens": ["Denn", "die", "Zwie\u00b7sprach", "ist", "am", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unten an der Treppenpforte", "tokens": ["Un\u00b7ten", "an", "der", "Trep\u00b7pen\u00b7pfor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Trabanten Hauptmann stand,", "tokens": ["Der", "Tra\u00b7ban\u00b7ten", "Haupt\u00b7mann", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der beredtern Flu\u00df der Worte", "tokens": ["Der", "be\u00b7red\u00b7tern", "Flu\u00df", "der", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Kaisers Namen fand:", "tokens": ["In", "des", "Kai\u00b7sers", "Na\u00b7men", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbeuren Degen, stolzer Sieger!", "tokens": ["\u00bb", "eu\u00b7ren", "De\u00b7gen", ",", "stol\u00b7zer", "Sie\u00b7ger", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euer Haus dien' Euch zur Haft;", "tokens": ["Eu\u00b7er", "Haus", "dien'", "Euch", "zur", "Haft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn Gehorsam schm\u00fcckt den Krieger", "tokens": ["Denn", "Ge\u00b7hor\u00b7sam", "schm\u00fcckt", "den", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6her noch, als Gl\u00fcck und Kraft.\u00ab", "tokens": ["H\u00f6\u00b7her", "noch", ",", "als", "Gl\u00fcck", "und", "Kraft", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eugen reicht den Degen artig:", "tokens": ["Eu\u00b7gen", "reicht", "den", "De\u00b7gen", "ar\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbnehmt ihn, der nicht rosten darf!", "tokens": ["\u00bb", "nehmt", "ihn", ",", "der", "nicht", "ros\u00b7ten", "darf", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ward im Dienst des Kaisers schartig;", "tokens": ["Ward", "im", "Dienst", "des", "Kai\u00b7sers", "schar\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nehmt und schleift ihn wieder scharf!\u00ab \u2013", "tokens": ["Nehmt", "und", "schleift", "ihn", "wie\u00b7der", "scharf", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gro\u00df mag dieser Degen scheinen,", "tokens": ["Gro\u00df", "mag", "die\u00b7ser", "De\u00b7gen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als er Heer und Schlacht gelenkt,", "tokens": ["Als", "er", "Heer", "und", "Schlacht", "ge\u00b7lenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gr\u00f6\u00dfer war's, als vor so Kleinen", "tokens": ["Gr\u00f6\u00b7\u00dfer", "wa\u00b7r's", ",", "als", "vor", "so", "Klei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "KOUS", "APPR", "ADV", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Er in Treue sich gesenkt.", "tokens": ["Er", "in", "Treu\u00b7e", "sich", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Als das Volk mit Scham und Staunen", "tokens": ["Als", "das", "Volk", "mit", "Scham", "und", "Stau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah den Feldherrn schwertberaubt,", "tokens": ["Sah", "den", "Feld\u00b7herrn", "schwert\u00b7be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rief der Zorn wie mit Posaunen", "tokens": ["Rief", "der", "Zorn", "wie", "mit", "Po\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All zum Schutz so theurem Haupt;", "tokens": ["All", "zum", "Schutz", "so", "theu\u00b7rem", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch auch dieses Heer der Liebe", "tokens": ["Doch", "auch", "die\u00b7ses", "Heer", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schl\u00e4gt die degenlose Hand,", "tokens": ["Schl\u00e4gt", "die", "de\u00b7gen\u00b7lo\u00b7se", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch sein Blick f\u00fchrt Heldenhiebe,", "tokens": ["Auch", "sein", "Blick", "f\u00fchrt", "Hel\u00b7den\u00b7hie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch sein Wort streckt in den Sand.", "tokens": ["Auch", "sein", "Wort", "streckt", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Zu den ew'gen Sternengleisen", "tokens": ["Zu", "den", "ew'\u00b7gen", "Ster\u00b7nen\u00b7glei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickt der Held aus seiner Haft;", "tokens": ["Blickt", "der", "Held", "aus", "sei\u00b7ner", "Haft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00fcnft'ge Siegessterne kreisen", "tokens": ["K\u00fcnft'\u00b7ge", "Sie\u00b7ges\u00b7ster\u00b7ne", "krei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um das Haupt ihm geisterhaft.", "tokens": ["Um", "das", "Haupt", "ihm", "geis\u00b7ter\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oesterreich, die\u00df Gotteserbe,", "tokens": ["O\u00b7es\u00b7ter\u00b7reich", ",", "die\u00df", "Got\u00b7te\u00b7ser\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcllt die Seele ihm mit Glanz;", "tokens": ["F\u00fcllt", "die", "See\u00b7le", "ihm", "mit", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df kein Feind den Thron verderbe,", "tokens": ["Da\u00df", "kein", "Feind", "den", "Thron", "ver\u00b7der\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der ihn beugt, sei einst sein Kranz!", "tokens": ["Der", "ihn", "beugt", ",", "sei", "einst", "sein", "Kranz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ob den wucht'gen Heldendegen", "tokens": ["Ob", "den", "wucht'\u00b7gen", "Hel\u00b7den\u00b7de\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leopoldus pr\u00fcfend wog?", "tokens": ["Leo\u00b7pol\u00b7dus", "pr\u0