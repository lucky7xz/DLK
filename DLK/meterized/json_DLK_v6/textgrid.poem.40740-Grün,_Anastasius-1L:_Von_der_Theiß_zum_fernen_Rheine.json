aic.tetra"}}, "stanza.9": {"line.1": {"text": "Morgens fr\u00fch an Eugens Pforte", "tokens": ["Mor\u00b7gens", "fr\u00fch", "an", "Eu\u00b7gens", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon der Gardehauptmann stand,", "tokens": ["Schon", "der", "Gar\u00b7de\u00b7haupt\u00b7mann", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der den Strom der Gnadenworte", "tokens": ["Der", "den", "Strom", "der", "Gna\u00b7den\u00b7wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Kaisers Namen fand:", "tokens": ["In", "des", "Kai\u00b7sers", "Na\u00b7men", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbnehmt die\u00df Schwert glorreich wie keines,", "tokens": ["\u00bb", "nehmt", "die\u00df", "Schwert", "glor\u00b7reich", "wie", "kei\u00b7nes", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "NN", "ADJD", "KOKOM", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch Gehorsam schartenrein;", "tokens": ["Durch", "Ge\u00b7hor\u00b7sam", "schar\u00b7ten\u00b7rein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch da\u00df Haupt und Arm nur Eines,", "tokens": ["Doch", "da\u00df", "Haupt", "und", "Arm", "nur", "Ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "KON", "NN", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seid Hofkriegsrath Ihr allein!\u00ab", "tokens": ["Seid", "Hof\u00b7kriegs\u00b7rath", "Ihr", "al\u00b7lein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "NN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Gro\u00dfen Herzen steigt der Tr\u00f6ster", "tokens": ["Gro\u00b7\u00dfen", "Her\u00b7zen", "steigt", "der", "Tr\u00f6s\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtend aus dem eignen Gram;", "tokens": ["Leuch\u00b7tend", "aus", "dem", "eig\u00b7nen", "Gram", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiner gro\u00dfen Siege gr\u00f6\u00dfter", "tokens": ["Sei\u00b7ner", "gro\u00b7\u00dfen", "Sie\u00b7ge", "gr\u00f6\u00df\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War's, als er die\u00df Schwert jetzt ", "tokens": ["Wa\u00b7r's", ",", "als", "er", "die\u00df", "Schwert", "jetzt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PDS", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das er weiht' in Morgenr\u00f6then,", "tokens": ["Das", "er", "weiht'", "in", "Mor\u00b7gen\u00b7r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6