{"textgrid.poem.56630": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "[min zuckers\u00f6t Suschen, wat wullt du di gr\u00e4mn?]", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Min zuckers\u00f6t Suschen, wat wullt du di gr\u00e4mn?", "tokens": ["Min", "zu\u00b7cker\u00b7s\u00f6t", "Su\u00b7schen", ",", "wat", "wullt", "du", "di", "gr\u00e4mn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "PWS", "VVFIN", "PPER", "NE", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kumt Wihnachten wedder, so will ik di n\u0119hmn!", "tokens": ["Kumt", "Wih\u00b7nach\u00b7ten", "wed\u00b7der", ",", "so", "will", "ik", "di", "n\u0119hmn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$,", "ADV", "VMFIN", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "W\u00fcllt P\u0119pern\u0153t kopen un Hasseln\u0153t knacken,", "tokens": ["W\u00fcllt", "P\u0119\u00b7pern\u0153t", "ko\u00b7pen", "un", "Has\u00b7seln\u0153t", "kna\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVINF", "FM", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Un so'n groten Koken ut Deeg w\u00fcllt wi backen!", "tokens": ["Un", "so'n", "gro\u00b7ten", "Ko\u00b7ken", "ut", "Deeg", "w\u00fcllt", "wi", "ba\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "APPR", "NN", "VVFIN", "KOKOM", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Min zuckers\u00f6t Suschen, nu gr\u00e4m di ni sehr!", "tokens": ["Min", "zu\u00b7cker\u00b7s\u00f6t", "Su\u00b7schen", ",", "nu", "gr\u00e4m", "di", "ni", "sehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "ADV", "FM", "FM", "FM", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Denn ward wi mal \u00f6ller, so danzt wi ni mehr!", "tokens": ["Denn", "ward", "wi", "mal", "\u00f6l\u00b7ler", ",", "so", "danzt", "wi", "ni", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "ADV", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "ADV", "ADV", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn schrapt wi den Grapen, denn schrapt wi dat Geld,", "tokens": ["Denn", "schrapt", "wi", "den", "Gra\u00b7pen", ",", "denn", "schrapt", "wi", "dat", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$,", "KON", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn gat wi mit so'n lange N\u00e4sen to Feld!", "tokens": ["Denn", "gat", "wi", "mit", "so'n", "lan\u00b7ge", "N\u00e4\u00b7sen", "to", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "NE", "ADJA", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Min zuckers\u00f6t Suschen, nu gr\u00e4m di man nich!", "tokens": ["Min", "zu\u00b7cker\u00b7s\u00f6t", "Su\u00b7schen", ",", "nu", "gr\u00e4m", "di", "man", "nich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "ADV", "ADJD", "NE", "PIS", "PTKNEG", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ik heff noch dree S\u00fcsselnk, dat weest du man nich!", "tokens": ["Ik", "heff", "noch", "dree", "S\u00fcs\u00b7selnk", ",", "dat", "weest", "du", "man", "nich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "CARD", "NN", "$,", "PAV", "VVFIN", "PPER", "PIS", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Dree S\u00fcsselnk un Dreelnk, un so'n groten Hot!", "tokens": ["Dree", "S\u00fcs\u00b7selnk", "un", "Dre\u00b7elnk", ",", "un", "so'n", "gro\u00b7ten", "Hot", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Un'n grishempen Geldsack, noch eenmal so grot!", "tokens": ["Un'n", "gris\u00b7hem\u00b7pen", "Geld\u00b7sack", ",", "noch", "e\u00b7en\u00b7mal", "so", "grot", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}