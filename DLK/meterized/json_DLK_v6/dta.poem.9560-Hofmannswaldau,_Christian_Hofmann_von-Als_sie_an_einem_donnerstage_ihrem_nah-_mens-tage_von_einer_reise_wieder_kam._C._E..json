{"dta.poem.9560": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie an einem donnerstage/ ihrem nah-  \n mens-tage/ von einer reise wieder kam.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wjllkommen sch\u00f6nster tag! du bringst das rechte gl\u00fccke/", "tokens": ["Wjll\u00b7kom\u00b7men", "sch\u00f6ns\u00b7ter", "tag", "!", "du", "bringst", "das", "rech\u00b7te", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wornach so mancher sich bi\u00dfher zu sehnen pflag/", "tokens": ["Wor\u00b7nach", "so", "man\u00b7cher", "sich", "bi\u00df\u00b7her", "zu", "seh\u00b7nen", "pflag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du wirst uns wohl ein recht verkehrter donnerstag/", "tokens": ["Du", "wirst", "uns", "wohl", "ein", "recht", "ver\u00b7kehr\u00b7ter", "don\u00b7ners\u00b7tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil deinen donner du durch himmlisches geschicke", "tokens": ["Weil", "dei\u00b7nen", "don\u00b7ner", "du", "durch", "himm\u00b7li\u00b7sches", "ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verkehrst in lauter lust und g\u00fcldne freuden-blicke.", "tokens": ["Ver\u00b7kehrst", "in", "lau\u00b7ter", "lust", "und", "g\u00fcld\u00b7ne", "freu\u00b7den\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein ander hasse dich/ so sehr er immer mag/", "tokens": ["Ein", "an\u00b7der", "has\u00b7se", "dich", "/", "so", "sehr", "er", "im\u00b7mer", "mag", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPER", "$(", "ADV", "ADV", "PPER", "ADV", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er tadle deinen blitz und rauhen wetter-schlag;", "tokens": ["Er", "tad\u00b7le", "dei\u00b7nen", "blitz", "und", "rau\u00b7hen", "wet\u00b7ter\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur wisse: da\u00df ich mich sehr hoch an dir erquicke.", "tokens": ["Nur", "wis\u00b7se", ":", "da\u00df", "ich", "mich", "sehr", "hoch", "an", "dir", "er\u00b7qui\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "KOUS", "PPER", "PRF", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn/ da du nicht allein Liborens nahmens-fest/", "tokens": ["Denn", "/", "da", "du", "nicht", "al\u00b7lein", "Li\u00b7bo\u00b7rens", "nah\u00b7mens\u00b7fest", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Besondern auch sie selbst uns wieder schauen l\u00e4st/", "tokens": ["Be\u00b7son\u00b7dern", "auch", "sie", "selbst", "uns", "wie\u00b7der", "schau\u00b7en", "l\u00e4st", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ADV", "PPER", "ADV", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie k\u00f6ntestu uns wol ein gr\u00f6ssres gl\u00fccke weisen?", "tokens": ["Wie", "k\u00f6n\u00b7tes\u00b7tu", "uns", "wol", "ein", "gr\u00f6ss\u00b7res", "gl\u00fc\u00b7cke", "wei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O! stell dich solcher art noch \u00f6ffters bey uns ein/", "tokens": ["O", "!", "stell", "dich", "sol\u00b7cher", "art", "noch", "\u00f6ff\u00b7ters", "bey", "uns", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "PPER", "PIAT", "NN", "ADV", "ADV", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du angenehmer tag! Jedoch/ was w\u00fcnsch ich? nein!", "tokens": ["Du", "an\u00b7ge\u00b7neh\u00b7mer", "tag", "!", "Je\u00b7doch", "/", "was", "w\u00fcnsch", "ich", "?", "nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "$(", "PWS", "VVFIN", "PPER", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sonst m\u00fcste sie zuvor erst wieder von uns reisen.", "tokens": ["Sonst", "m\u00fcs\u00b7te", "sie", "zu\u00b7vor", "erst", "wie\u00b7der", "von", "uns", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}