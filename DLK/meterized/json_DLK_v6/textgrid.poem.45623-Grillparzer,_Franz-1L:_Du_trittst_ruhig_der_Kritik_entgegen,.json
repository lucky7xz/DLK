{"textgrid.poem.45623": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du trittst ruhig der Kritik entgegen,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du trittst ruhig der Kritik entgegen,", "tokens": ["Du", "trittst", "ru\u00b7hig", "der", "Kri\u00b7tik", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So unangreifbar ist noch keiner gewesen:", "tokens": ["So", "un\u00b7an\u00b7greif\u00b7bar", "ist", "noch", "kei\u00b7ner", "ge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "PIS", "VAPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wer dich nicht gelesen, kann dich nicht widerlegen;", "tokens": ["Wer", "dich", "nicht", "ge\u00b7le\u00b7sen", ",", "kann", "dich", "nicht", "wi\u00b7der\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und wer dich widerlegen k\u00f6nnte, kann dich nicht lesen.", "tokens": ["Und", "wer", "dich", "wi\u00b7der\u00b7le\u00b7gen", "k\u00f6nn\u00b7te", ",", "kann", "dich", "nicht", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVINF", "VMFIN", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}}}}}