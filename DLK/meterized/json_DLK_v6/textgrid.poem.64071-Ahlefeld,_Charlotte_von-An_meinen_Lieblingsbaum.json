{"textgrid.poem.64071": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Lieblingsbaum", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tr\u00e4ume, die in stillen Feierstunden,", "tokens": ["Die", "Tr\u00e4u\u00b7me", ",", "die", "in", "stil\u00b7len", "Fei\u00b7er\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die dunkler Schatten mir so oft verlieh,", "tokens": ["Die", "dunk\u00b7ler", "Schat\u00b7ten", "mir", "so", "oft", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die s\u00fcsse Ruh, die ich bei Dir gefunden,", "tokens": ["Die", "s\u00fcs\u00b7se", "Ruh", ",", "die", "ich", "bei", "Dir", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Lieblingsbaum, o die vergess' ich nie!", "tokens": ["Mein", "Lieb\u00b7lings\u00b7baum", ",", "o", "die", "ver\u00b7ge\u00b7ss'", "ich", "nie", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "ART", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Oft sah ich neben Dir die Sonne untergehen,", "tokens": ["Oft", "sah", "ich", "ne\u00b7ben", "Dir", "die", "Son\u00b7ne", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entz\u00fcckt von ihres Anblicks Majest\u00e4t.", "tokens": ["Ent\u00b7z\u00fcckt", "von", "ih\u00b7res", "An\u00b7blicks", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Oft hat des Herbstes lindes, k\u00fchles Wehen", "tokens": ["Oft", "hat", "des", "Herbs\u00b7tes", "lin\u00b7des", ",", "k\u00fch\u00b7les", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Deinem bunten Laub mich \u00fcbers\u00e4et.", "tokens": ["Mit", "Dei\u00b7nem", "bun\u00b7ten", "Laub", "mich", "\u00fc\u00b7bers\u00b7\u00e4et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vor meinen Blicken schwebten holde Bilder,", "tokens": ["Vor", "mei\u00b7nen", "Bli\u00b7cken", "schweb\u00b7ten", "hol\u00b7de", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im lichten Glanz der Jugendfantasie,", "tokens": ["Im", "lich\u00b7ten", "Glanz", "der", "Ju\u00b7gend\u00b7fan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da tr\u00e4umt ich mir des Schicksals H\u00e4rte milder,", "tokens": ["Da", "tr\u00e4umt", "ich", "mir", "des", "Schick\u00b7sals", "H\u00e4r\u00b7te", "mil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Misston wurde Harmonie.", "tokens": ["Und", "je\u00b7der", "Miss\u00b7ton", "wur\u00b7de", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und liebend grub ich einst in Deine Rinde", "tokens": ["Und", "lie\u00b7bend", "grub", "ich", "einst", "in", "Dei\u00b7ne", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Nahmenszug, der in mir brannte, ein,", "tokens": ["Den", "Nah\u00b7mens\u00b7zug", ",", "der", "in", "mir", "brann\u00b7te", ",", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch darum wirst Du mir, Du stille Linde,", "tokens": ["Auch", "da\u00b7rum", "wirst", "Du", "mir", ",", "Du", "stil\u00b7le", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VAFIN", "PPER", "PPER", "$,", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor allen B\u00e4umen ewig theuer seyn.", "tokens": ["Vor", "al\u00b7len", "B\u00e4u\u00b7men", "e\u00b7wig", "theu\u00b7er", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wenn sich in Deinen bl\u00fcthenvollen Zweigen", "tokens": ["Wenn", "sich", "in", "Dei\u00b7nen", "bl\u00fc\u00b7then\u00b7vol\u00b7len", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Westes leiser Odem kaum bewegt,", "tokens": ["Des", "Wes\u00b7tes", "lei\u00b7ser", "O\u00b7dem", "kaum", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fchlt mein Gem\u00fcth sich durch das tiefe Schweigen", "tokens": ["F\u00fchlt", "mein", "Ge\u00b7m\u00fcth", "sich", "durch", "das", "tie\u00b7fe", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der heiligen Natur so ernst erregt.", "tokens": ["Der", "hei\u00b7li\u00b7gen", "Na\u00b7tur", "so", "ernst", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dann denk' ich all' der W\u00fcnsche, die vergebens", "tokens": ["Dann", "denk'", "ich", "all'", "der", "W\u00fcn\u00b7sche", ",", "die", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In meine Seele kamen, und entflohn,", "tokens": ["In", "mei\u00b7ne", "See\u00b7le", "ka\u00b7men", ",", "und", "ent\u00b7flohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seufze: w\u00e4r' der kurze Traum des Lebens", "tokens": ["Und", "seuf\u00b7ze", ":", "w\u00e4r'", "der", "kur\u00b7ze", "Traum", "des", "Le\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor\u00fcber, wie so manche Hoffnung schon.", "tokens": ["Vor\u00b7\u00fc\u00b7ber", ",", "wie", "so", "man\u00b7che", "Hoff\u00b7nung", "schon", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und w\u00e4re einst nach meiner Tage M\u00fchen,", "tokens": ["Und", "w\u00e4\u00b7re", "einst", "nach", "mei\u00b7ner", "Ta\u00b7ge", "M\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Baum, den stets mein Herz mit Liebe nennt,", "tokens": ["O", "Baum", ",", "den", "stets", "mein", "Herz", "mit", "Lie\u00b7be", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein stilles Grab mir ", "tokens": ["Ein", "stil\u00b7les", "Grab", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}}}}}