{"dta.poem.7353": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Donnerstag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Winde rauschen, Flocken tanzen,", "tokens": ["Win\u00b7de", "rau\u00b7schen", ",", "Flo\u00b7cken", "tan\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jede Schwalbe sucht das Haus,", "tokens": ["Je\u00b7de", "Schwal\u00b7be", "sucht", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Pfarrer unerschrocken", "tokens": ["Nur", "der", "Pfar\u00b7rer", "un\u00b7er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Segelt in den Sturm hinaus.", "tokens": ["Se\u00b7gelt", "in", "den", "Sturm", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht zum besten sind die Pfade,", "tokens": ["Nicht", "zum", "bes\u00b7ten", "sind", "die", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber leidlich w\u00fcrd' es seyn,", "tokens": ["A\u00b7ber", "leid\u00b7lich", "w\u00fcrd'", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Tr\u00fcg er unter seinem Mantel", "tokens": ["Tr\u00fcg", "er", "un\u00b7ter", "sei\u00b7nem", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht die Aepfel und den Wein.", "tokens": ["Nicht", "die", "A\u00b7e\u00b7pfel", "und", "den", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ach, ihm ist so wohl zu Muthe,", "tokens": ["Ach", ",", "ihm", "ist", "so", "wohl", "zu", "Mu\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df dem kranken Zimmermann", "tokens": ["Da\u00df", "dem", "kran\u00b7ken", "Zim\u00b7mer\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er die l\u00e4ngst geg\u00f6nnte Gabe", "tokens": ["Er", "die", "l\u00e4ngst", "ge\u00b7g\u00f6nn\u00b7te", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADV", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Endlich einmal bieten kann.", "tokens": ["End\u00b7lich", "ein\u00b7mal", "bie\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer mu\u00df er heimlich lachen,", "tokens": ["Im\u00b7mer", "mu\u00df", "er", "heim\u00b7lich", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Anne Aepfel las,", "tokens": ["Wie", "die", "An\u00b7ne", "A\u00b7e\u00b7pfel", "las", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und wie er den Wein stipitzte,", "tokens": ["Und", "wie", "er", "den", "Wein", "sti\u00b7pitz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "W\u00e4hrend sie im Keller sa\u00df.", "tokens": ["W\u00e4h\u00b7rend", "sie", "im", "Kel\u00b7ler", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00e4ngs des Teiches sieh ihn flattern,", "tokens": ["L\u00e4ngs", "des", "Tei\u00b7ches", "sieh", "ihn", "flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er rudert, wie er streicht,", "tokens": ["Wie", "er", "ru\u00b7dert", ",", "wie", "er", "streicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann den Mantel nimmer zwingen", "tokens": ["Kann", "den", "Man\u00b7tel", "nim\u00b7mer", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit den Fingern starr und feucht.", "tokens": ["Mit", "den", "Fin\u00b7gern", "starr", "und", "feucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oefters aus dem tr\u00fcben Auge", "tokens": ["Oef\u00b7ters", "aus", "dem", "tr\u00fc\u00b7ben", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine kalte Z\u00e4hre bricht,", "tokens": ["Ei\u00b7ne", "kal\u00b7te", "Z\u00e4h\u00b7re", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wehn ihm seine grauen Haare", "tokens": ["Wehn", "ihm", "sei\u00b7ne", "grau\u00b7en", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Spinnenwebig um's Gesicht.", "tokens": ["Spin\u00b7nen\u00b7we\u00b7big", "um's", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch Gottlob! da ist die H\u00fctte,", "tokens": ["Doch", "Gott\u00b7lob", "!", "da", "ist", "die", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und nun \u00f6ffnet sich das Haus,", "tokens": ["Und", "nun", "\u00f6ff\u00b7net", "sich", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun keuchend auf der Tenne", "tokens": ["Und", "nun", "keu\u00b7chend", "auf", "der", "Ten\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttet er die Federn aus.", "tokens": ["Sch\u00fct\u00b7tet", "er", "die", "Fe\u00b7dern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wie freut der gute Pfarrer", "tokens": ["Ach", "wie", "freut", "der", "gu\u00b7te", "Pfar\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich am blanken Feuerschein!", "tokens": ["Sich", "am", "blan\u00b7ken", "Feu\u00b7er\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie gesch\u00e4ftig schenkt dem Kranken", "tokens": ["Wie", "ge\u00b7sch\u00e4f\u00b7tig", "schenkt", "dem", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Er das erste Gl\u00e4schen ein.", "tokens": ["Er", "das", "ers\u00b7te", "Gl\u00e4s\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Setzt sich an des Lagers Ende,", "tokens": ["Setzt", "sich", "an", "des", "La\u00b7gers", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rkt ihm bestens die Geduld,", "tokens": ["St\u00e4rkt", "ihm", "bes\u00b7tens", "die", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von seinen frommen Lippen", "tokens": ["Und", "von", "sei\u00b7nen", "from\u00b7men", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einfach flie\u00dft das Wort der Huld.", "tokens": ["Ein\u00b7fach", "flie\u00dft", "das", "Wort", "der", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die abgezehrten H\u00e4nde", "tokens": ["Wenn", "die", "ab\u00b7ge\u00b7zehr\u00b7ten", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er so fest in seine schlie\u00dft,", "tokens": ["Er", "so", "fest", "in", "sei\u00b7ne", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Anders f\u00fchlt sich dann der Kranke,", "tokens": ["An\u00b7ders", "f\u00fchlt", "sich", "dann", "der", "Kran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meint, da\u00df gar nichts ihn verdrie\u00dft.", "tokens": ["Meint", ",", "da\u00df", "gar", "nichts", "ihn", "ver\u00b7drie\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit der Einfalt, mit der Liebe", "tokens": ["Mit", "der", "Ein\u00b7falt", ",", "mit", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmeichelt er die Seele wach,", "tokens": ["Schmei\u00b7chelt", "er", "die", "See\u00b7le", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann an jedes Herz sich legen,", "tokens": ["Kann", "an", "je\u00b7des", "Herz", "sich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sey es kraftvoll oder schwach.", "tokens": ["Sey", "es", "kraft\u00b7voll", "o\u00b7der", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber drau\u00dfen will es dunkeln,", "tokens": ["A\u00b7ber", "drau\u00b7\u00dfen", "will", "es", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drau\u00dfen tr\u00f6pfelt es vom Dach; \u2014", "tokens": ["Drau\u00b7\u00dfen", "tr\u00f6p\u00b7felt", "es", "vom", "Dach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lange sehn ihm nach die Kinder,", "tokens": ["Lan\u00b7ge", "sehn", "ihm", "nach", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der Kranke seufzt ihm nach.", "tokens": ["Und", "der", "Kran\u00b7ke", "seufzt", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}