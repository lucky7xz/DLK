{"textgrid.poem.25848": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "[ich sitze am Wasser]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sitze am Wasser,", "tokens": ["Ich", "sit\u00b7ze", "am", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bei mir sitzt der Gram,", "tokens": ["Bei", "mir", "sitzt", "der", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wir schauen bleich in die grauen Gr\u00fcnde.", "tokens": ["Wir", "schau\u00b7en", "bleich", "in", "die", "grau\u00b7en", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Unten Felsen,", "tokens": ["Un\u00b7ten", "Fel\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Liegen steif, w\u00e4rmeleer,", "tokens": ["Lie\u00b7gen", "steif", ",", "w\u00e4r\u00b7me\u00b7leer", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Umher gequollen fahles Gras,", "tokens": ["Um\u00b7her", "ge\u00b7quol\u00b7len", "fah\u00b7les", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dr\u00fcber fliegen dunkel die Fische.", "tokens": ["Dr\u00fc\u00b7ber", "flie\u00b7gen", "dun\u00b7kel", "die", "Fi\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Meine gramvollen Augen schweigen,", "tokens": ["Mei\u00b7ne", "gram\u00b7vol\u00b7len", "Au\u00b7gen", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Saugen Fischblut, erfrieren,", "tokens": ["Sau\u00b7gen", "Fischblut", ",", "er\u00b7frie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und stieren gramleer aus der Tiefe.", "tokens": ["Und", "stie\u00b7ren", "gram\u00b7leer", "aus", "der", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}