{"textgrid.poem.43203": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich lie\u00df die Andern dort bei Sang und Tanz,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lie\u00df die Andern dort bei Sang und Tanz,", "tokens": ["Ich", "lie\u00df", "die", "An\u00b7dern", "dort", "bei", "Sang", "und", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auf die Riva eilt ich von dem Feste,", "tokens": ["Und", "auf", "die", "Ri\u00b7va", "eilt", "ich", "von", "dem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In tiefem Schlummer lagen die Pal\u00e4ste,", "tokens": ["In", "tie\u00b7fem", "Schlum\u00b7mer", "la\u00b7gen", "die", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier war ich fern dem bunten Mummenschanz.", "tokens": ["Hier", "war", "ich", "fern", "dem", "bun\u00b7ten", "Mum\u00b7men\u00b7schanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und vor mir wogte sacht das dunkle Meer,", "tokens": ["Und", "vor", "mir", "wog\u00b7te", "sacht", "das", "dunk\u00b7le", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Ufer nahe, schwarze Gondeln schwammen,", "tokens": ["Dem", "U\u00b7fer", "na\u00b7he", ",", "schwar\u00b7ze", "Gon\u00b7deln", "schwam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf einem Boote sch\u00fcrten sie die Flammen,", "tokens": ["Auf", "ei\u00b7nem", "Boo\u00b7te", "sch\u00fcr\u00b7ten", "sie", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Segelschiff glitt geisterhaft daher.", "tokens": ["Ein", "Se\u00b7gel\u00b7schiff", "glitt", "geis\u00b7ter\u00b7haft", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und als ich auf zum n\u00e4chtigen Himmel sah,", "tokens": ["Und", "als", "ich", "auf", "zum", "n\u00e4ch\u00b7ti\u00b7gen", "Him\u00b7mel", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Fiel j\u00e4h ein Stern \u2013 er ist in's Meer versunken,", "tokens": ["Fiel", "j\u00e4h", "ein", "Stern", "\u2013", "er", "ist", "in's", "Meer", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$(", "PPER", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da lallts herauf so weich, so schlummertrunken", "tokens": ["Da", "lallts", "her\u00b7auf", "so", "weich", ",", "so", "schlum\u00b7mer\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJD", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Kinderlippen fragend: ", "tokens": ["Von", "Kin\u00b7der\u00b7lip\u00b7pen", "fra\u00b7gend", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}