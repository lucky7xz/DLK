{"textgrid.poem.52775": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging am k\u00fchligen Strome", "tokens": ["Ich", "ging", "am", "k\u00fch\u00b7li\u00b7gen", "Stro\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor\u00fcber mit meinem sch\u00f6n Lieb,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "mit", "mei\u00b7nem", "sch\u00f6n", "Lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es hobo\u00ebten die Drosseln,", "tokens": ["Es", "ho\u00b7bo\u00ebten", "die", "Dros\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Als sie die Guck\u00e4ug'lein rieb.", "tokens": ["Als", "sie", "die", "Guck\u00b7\u00e4ug'\u00b7lein", "rieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Was zitterst du M\u00e4dchen, was ist dir?", "tokens": ["Was", "zit\u00b7terst", "du", "M\u00e4d\u00b7chen", ",", "was", "ist", "dir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,", "PWS", "VAFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sehe, ich seh', o du Kind,", "tokens": ["Ich", "se\u00b7he", ",", "ich", "seh'", ",", "o", "du", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "FM", "PPER", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich sehe krystallene Tropfen,", "tokens": ["Ich", "se\u00b7he", "krys\u00b7tal\u00b7le\u00b7ne", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie Thr\u00e4nentropfen sind.", "tokens": ["Wie", "Thr\u00e4\u00b7nen\u00b7trop\u00b7fen", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Siehst auch die Wellen Du ziehen?", "tokens": ["Siehst", "auch", "die", "Wel\u00b7len", "Du", "zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Jetzt sind sie da, jetzt dort,", "tokens": ["Jetzt", "sind", "sie", "da", ",", "jetzt", "dort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald seh' ich keine mehr nimmer,", "tokens": ["Bald", "seh'", "ich", "kei\u00b7ne", "mehr", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie da, so wiederum fort.", "tokens": ["Wie", "da", ",", "so", "wie\u00b7de\u00b7rum", "fort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wohl seh' ich die Wellen ja ziehen,", "tokens": ["Wohl", "seh'", "ich", "die", "Wel\u00b7len", "ja", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Was soll das mir und dir?", "tokens": ["Was", "soll", "das", "mir", "und", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir k\u00f6nnen ja, Herzensgeliebte,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "ja", ",", "Her\u00b7zens\u00b7ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Alle beide Nichts daf\u00fcr.", "tokens": ["Al\u00b7le", "bei\u00b7de", "Nichts", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "PIS", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}