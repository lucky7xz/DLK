{"textgrid.poem.49168": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Freund ist mein und ich bin sein,", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund ist mein und ich bin sein,", "tokens": ["Mein", "Freund", "ist", "mein", "und", "ich", "bin", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "KON", "PPER", "VAFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Freund, mein Mond- und Sonnenschein,", "tokens": ["Mein", "Freund", ",", "mein", "Mon\u00b7d", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wenn das bem\u00fchte Taglicht scheidet", "tokens": ["Wenn", "das", "be\u00b7m\u00fch\u00b7te", "Tag\u00b7licht", "schei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die furchtvolle Nacht bricht ein,", "tokens": ["Und", "die", "furcht\u00b7vol\u00b7le", "Nacht", "bricht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Er ach, mein Freund ist mein, ich sein,", "tokens": ["Er", "ach", ",", "mein", "Freund", "ist", "mein", ",", "ich", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$,", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der unter meinen Rosen weidet!", "tokens": ["Der", "un\u00b7ter", "mei\u00b7nen", "Ro\u00b7sen", "wei\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So singet, Jesu, deine Braut,", "tokens": ["So", "sin\u00b7get", ",", "Je\u00b7su", ",", "dei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die du dir vor der Welt vertraut.", "tokens": ["Die", "du", "dir", "vor", "der", "Welt", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Wunderpaar seid, traun, ihr Beid',", "tokens": ["Ein", "Wun\u00b7der\u00b7paar", "seid", ",", "traun", ",", "ihr", "Beid'", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "VVINF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist der F\u00fcrst der Ewigkeit", "tokens": ["Du", "bist", "der", "F\u00fcrst", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sie ein armer Klo\u00df der Erden,", "tokens": ["Und", "sie", "ein", "ar\u00b7mer", "Klo\u00df", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch tr\u00e4gest du zu ihr", "tokens": ["Und", "den\u00b7noch", "tr\u00e4\u00b7gest", "du", "zu", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So unvergleichliche Begier,", "tokens": ["So", "un\u00b7ver\u00b7gleich\u00b7li\u00b7che", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um ihr verm\u00e4hltes Lieb zu werden,", "tokens": ["Um", "ihr", "ver\u00b7m\u00e4hl\u00b7tes", "Lieb", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und f\u00e4llest ihr von Herzen bei,", "tokens": ["Und", "f\u00e4l\u00b7lest", "ihr", "von", "Her\u00b7zen", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ihr kein Freund nicht n\u00e4her sei.", "tokens": ["Da\u00df", "ihr", "kein", "Freund", "nicht", "n\u00e4\u00b7her", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, mehr als Freund, der du f\u00fcr sie", "tokens": ["Ach", ",", "mehr", "als", "Freund", ",", "der", "du", "f\u00fcr", "sie"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "KOUS", "NN", "$,", "PRELS", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So unbeschreiblich' Angst und M\u00fch',", "tokens": ["So", "un\u00b7be\u00b7schreib\u00b7lich'", "Angst", "und", "M\u00fch'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ja selbst den Kreuztod hast erlitten", "tokens": ["Ja", "selbst", "den", "Kreuz\u00b7tod", "hast", "er\u00b7lit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht das harte Grab verschm\u00e4ht;", "tokens": ["Und", "nicht", "das", "har\u00b7te", "Grab", "ver\u00b7schm\u00e4ht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch da du wiederum erh\u00f6ht", "tokens": ["Auch", "da", "du", "wie\u00b7de\u00b7rum", "er\u00b7h\u00f6ht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und deinen Machtthron hast beschritten,", "tokens": ["Und", "dei\u00b7nen", "Macht\u00b7thron", "hast", "be\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Kehrst du doch allzeit bei ihr ein,", "tokens": ["Kehrst", "du", "doch", "all\u00b7zeit", "bei", "ihr", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Um sicher eures Bunds zu sein.", "tokens": ["Um", "si\u00b7cher", "eu\u00b7res", "Bunds", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPOSAT", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Du schm\u00e4hst, ihr holdes Augenlicht,", "tokens": ["Du", "schm\u00e4hst", ",", "ihr", "hol\u00b7des", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die scharfe Rosenweide nicht,", "tokens": ["Die", "schar\u00b7fe", "Ro\u00b7sen\u00b7wei\u00b7de", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Womit sie um und um beschlossen,", "tokens": ["Wo\u00b7mit", "sie", "um", "und", "um", "be\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "KON", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du Rosenfreund, so wei\u00df als roth,", "tokens": ["Du", "Ro\u00b7sen\u00b7freund", ",", "so", "wei\u00df", "als", "roth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Leben wei\u00df, und roth im Tod,", "tokens": ["Im", "Le\u00b7ben", "wei\u00df", ",", "und", "roth", "im", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit deinem Purpurblut beflossen;", "tokens": ["Mit", "dei\u00b7nem", "Pur\u00b7pur\u00b7blut", "be\u00b7flos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Dornenkron', dein' Leidenspracht,", "tokens": ["Die", "Dor\u00b7nen\u00b7kron'", ",", "dein'", "Lei\u00b7den\u00b7spracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat ihre Stacheln stumpf gemacht.", "tokens": ["Hat", "ih\u00b7re", "Sta\u00b7cheln", "stumpf", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach liebstes Heil, vertrauter Freund,", "tokens": ["Ach", "liebs\u00b7tes", "Heil", ",", "ver\u00b7trau\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bruder, der mit mir es meint,", "tokens": ["Mein", "Bru\u00b7der", ",", "der", "mit", "mir", "es", "meint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als nie ein Bruder nicht auf Erden,", "tokens": ["Als", "nie", "ein", "Bru\u00b7der", "nicht", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es bricht mein Abend m\u00e4chtig ein;", "tokens": ["Es", "bricht", "mein", "A\u00b7bend", "m\u00e4ch\u00b7tig", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lass' hier mir deinen Gnadenschein", "tokens": ["Lass'", "hier", "mir", "dei\u00b7nen", "Gna\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein himmelreiches Vorlicht werden,", "tokens": ["Mein", "him\u00b7mel\u00b7rei\u00b7ches", "Vor\u00b7licht", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und gehe, meine Sonne, mir", "tokens": ["Und", "ge\u00b7he", ",", "mei\u00b7ne", "Son\u00b7ne", ",", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch das besorgte Todsthal f\u00fcr.", "tokens": ["Durch", "das", "be\u00b7sorg\u00b7te", "Tods\u00b7thal", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "K\u00fchl' ab die Hitz', die mich entseelt", "tokens": ["K\u00fchl'", "ab", "die", "Hitz'", ",", "die", "mich", "ent\u00b7seelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und diesen langen Tag gequ\u00e4lt,", "tokens": ["Und", "die\u00b7sen", "lan\u00b7gen", "Tag", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nimm weg den unmuthsvollen Schatten,", "tokens": ["Nimm", "weg", "den", "un\u00b7muths\u00b7vol\u00b7len", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der hier um meiner Seele liegt", "tokens": ["Der", "hier", "um", "mei\u00b7ner", "See\u00b7le", "liegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und niemals, wie es sie vergn\u00fcgt,", "tokens": ["Und", "nie\u00b7mals", ",", "wie", "es", "sie", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir meine Pflichtschuld ab l\u00e4\u00dft statten,", "tokens": ["Dir", "mei\u00b7ne", "Pflicht\u00b7schuld", "ab", "l\u00e4\u00dft", "stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Viel wen'ger dich in deinem Licht", "tokens": ["Viel", "wen'\u00b7ger", "dich", "in", "dei\u00b7nem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Recht kommen l\u00e4sset zu Gesicht.", "tokens": ["Recht", "kom\u00b7men", "l\u00e4s\u00b7set", "zu", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Versetz' mich in dein Paradies", "tokens": ["Ver\u00b7setz'", "mich", "in", "dein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lass' hier um dein Lebensflie\u00df", "tokens": ["Und", "lass'", "hier", "um", "dein", "Le\u00b7bens\u00b7flie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns unter deinen Rosen weiden;", "tokens": ["Uns", "un\u00b7ter", "dei\u00b7nen", "Ro\u00b7sen", "wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier stimmt der Engel Chor mit ein,", "tokens": ["Hier", "stimmt", "der", "En\u00b7gel", "Chor", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Freund ist mein und ich bin sein!", "tokens": ["Mein", "Freund", "ist", "mein", "und", "ich", "bin", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "KON", "PPER", "VAFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach heil'ge Liebe voller Freuden,", "tokens": ["Ach", "heil'\u00b7ge", "Lie\u00b7be", "vol\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ach sch\u00f6nes Leben voller Wonn'", "tokens": ["Ach", "sch\u00f6\u00b7nes", "Le\u00b7ben", "vol\u00b7ler", "Wonn'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wann ist der Anfang doch hier von!", "tokens": ["Wann", "ist", "der", "An\u00b7fang", "doch", "hier", "von", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADV", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}