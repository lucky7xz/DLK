{"textgrid.poem.40875": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsbotschaft", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Max steht am Gitterfenster. Weit \u00fcber den Zinnen der Stadt", "tokens": ["Max", "steht", "am", "Git\u00b7ter\u00b7fens\u00b7ter", ".", "Weit", "\u00fc\u00b7ber", "den", "Zin\u00b7nen", "der", "Stadt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$.", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lag wieder im Fr\u00fchlingsprangen die Ebne gr\u00fcn und glatt,", "tokens": ["Lag", "wie\u00b7der", "im", "Fr\u00fch\u00b7lings\u00b7pran\u00b7gen", "die", "Eb\u00b7ne", "gr\u00fcn", "und", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da setzte sich aufs Gitter ein freies V\u00f6gelein", "tokens": ["Da", "setz\u00b7te", "sich", "aufs", "Git\u00b7ter", "ein", "frei\u00b7es", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und sah zum gefangenen K\u00f6nig sorglos und ruhig herein.", "tokens": ["Und", "sah", "zum", "ge\u00b7fan\u00b7ge\u00b7nen", "K\u00f6\u00b7nig", "sorg\u00b7los", "und", "ru\u00b7hig", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+-+--+", "measure": "amphibrach.tetra.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbo Max, siehst du's in der Ferne dort schimmern wei\u00df und licht?", "tokens": ["\u00bb", "o", "Max", ",", "siehst", "du's", "in", "der", "Fer\u00b7ne", "dort", "schim\u00b7mern", "wei\u00df", "und", "licht", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "VVINF", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Das sind des Lenzes Blumen und seine Bl\u00fcthen nicht!", "tokens": ["Das", "sind", "des", "Len\u00b7zes", "Blu\u00b7men", "und", "sei\u00b7ne", "Bl\u00fc\u00b7then", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das sind Paniere und Helme! Dein Vater Friederich", "tokens": ["Das", "sind", "Pa\u00b7nie\u00b7re", "und", "Hel\u00b7me", "!", "Dein", "Va\u00b7ter", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "$.", "PPOSAT", "NN", "NE"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bringt sie von fern f\u00fcr Flandern, als Fr\u00fchlingsgabe, mit sich.", "tokens": ["Bringt", "sie", "von", "fern", "f\u00fcr", "Flan\u00b7dern", ",", "als", "Fr\u00fch\u00b7lings\u00b7ga\u00b7be", ",", "mit", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJD", "APPR", "NN", "$,", "KOUS", "NN", "$,", "APPR", "PRF", "$."], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Was taucht dort aus der Fl\u00e4che wie junge Halme hervor?", "tokens": ["Was", "taucht", "dort", "aus", "der", "Fl\u00e4\u00b7che", "wie", "jun\u00b7ge", "Hal\u00b7me", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das sind nicht schlanke Aehren, die Fr\u00fchling trieb empor!", "tokens": ["Das", "sind", "nicht", "schlan\u00b7ke", "A\u00b7eh\u00b7ren", ",", "die", "Fr\u00fch\u00b7ling", "trieb", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Halme hei\u00dfen Speere, und ihre Bl\u00fcth' ist roth,", "tokens": ["Die", "Hal\u00b7me", "hei\u00b7\u00dfen", "Spee\u00b7re", ",", "und", "ih\u00b7re", "Bl\u00fcth'", "ist", "roth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zur Ernte an jeder Aehre h\u00e4ngt einst als Frucht der Tod.\u00ab", "tokens": ["Zur", "Ern\u00b7te", "an", "je\u00b7der", "A\u00b7eh\u00b7re", "h\u00e4ngt", "einst", "als", "Frucht", "der", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "PIAT", "NN", "VVFIN", "ADV", "KOUS", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Der K\u00f6nig aber errieth nicht, ob so das V\u00f6glein sang,", "tokens": ["Der", "K\u00f6\u00b7nig", "a\u00b7ber", "er\u00b7rieth", "nicht", ",", "ob", "so", "das", "V\u00f6\u00b7glein", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Ob tief im eignen Herzen das s\u00fc\u00dfe Trostlied klang.", "tokens": ["Ob", "tief", "im", "eig\u00b7nen", "Her\u00b7zen", "das", "s\u00fc\u00b7\u00dfe", "Trost\u00b7lied", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch wie ein Kranz von Rosen sinkt auf ein d\u00fcstres Grab,", "tokens": ["Doch", "wie", "ein", "Kranz", "von", "Ro\u00b7sen", "sinkt", "auf", "ein", "d\u00fcst\u00b7res", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So sank jetzt Ruh' und Friede mild auf sein Herz herab.", "tokens": ["So", "sank", "jetzt", "Ruh'", "und", "Frie\u00b7de", "mild", "auf", "sein", "Herz", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "ADJD", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Am Marktplatz standen die Z\u00fcnfte; da st\u00fcrmte vom Thurme nieder", "tokens": ["Am", "Markt\u00b7platz", "stan\u00b7den", "die", "Z\u00fcnf\u00b7te", ";", "da", "st\u00fcrm\u00b7te", "vom", "Thur\u00b7me", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Herr Kopp'noll, bleich im Gesichte, ihm zitterten alle Glieder,", "tokens": ["Herr", "Kopp'\u00b7noll", ",", "bleich", "im", "Ge\u00b7sich\u00b7te", ",", "ihm", "zit\u00b7ter\u00b7ten", "al\u00b7le", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJD", "APPRART", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+--+--+--+--+-+-", "measure": "dactylic.tetra.plus"}, "line.3": {"text": "Nach ihm der Th\u00fcrmer br\u00fcllend: \u00bbLauft, wer noch laufen kann!", "tokens": ["Nach", "ihm", "der", "Th\u00fcr\u00b7mer", "br\u00fcl\u00b7lend", ":", "\u00bb", "Lauft", ",", "wer", "noch", "lau\u00b7fen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVPP", "$.", "$(", "NN", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zahllos wie Fliegen im Sommer r\u00fcckt Deutschlands Heermacht an!", "tokens": ["Zahl\u00b7los", "wie", "Flie\u00b7gen", "im", "Som\u00b7mer", "r\u00fcckt", "Deutschlands", "Heer\u00b7macht", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Kerle sind von Br\u00fcgge kaum eine Meile weit,", "tokens": ["Die", "Ker\u00b7le", "sind", "von", "Br\u00fcg\u00b7ge", "kaum", "ei\u00b7ne", "Mei\u00b7le", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und haben Knochen wie G\u00e4ule, und Schwerter wie Ruder breit,", "tokens": ["Und", "ha\u00b7ben", "Kno\u00b7chen", "wie", "G\u00e4u\u00b7le", ",", "und", "Schwer\u00b7ter", "wie", "Ru\u00b7der", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "KOKOM", "NN", "$,", "KON", "NN", "KOKOM", "NN", "ADJD", "$,"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und B\u00e4rte wie Tannen\u00e4ste. F\u00fcr uns bringt jeder Mann", "tokens": ["Und", "B\u00e4r\u00b7te", "wie", "Tan\u00b7ne\u00b7n\u00e4s\u00b7te", ".", "F\u00fcr", "uns", "bringt", "je\u00b7der", "Mann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KOKOM", "NN", "$.", "APPR", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+--+-+----+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "O weh, einen hohen Galgen! Drum laufe, wer laufen kann!\u00ab", "tokens": ["O", "weh", ",", "ei\u00b7nen", "ho\u00b7hen", "Gal\u00b7gen", "!", "Drum", "lau\u00b7fe", ",", "wer", "lau\u00b7fen", "kann", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "$,", "ART", "ADJA", "NN", "$.", "PAV", "VVFIN", "$,", "PWS", "VVINF", "VMFIN", "$.", "$("], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}}, "stanza.7": {"line.1": {"text": "Da rannten die Zunftgenossen und rannten einander nieder,", "tokens": ["Da", "rann\u00b7ten", "die", "Zunft\u00b7ge\u00b7nos\u00b7sen", "und", "rann\u00b7ten", "ein\u00b7an\u00b7der", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+--+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und liefen nach den Waffen, und standen und liefen wieder,", "tokens": ["Und", "lie\u00b7fen", "nach", "den", "Waf\u00b7fen", ",", "und", "stan\u00b7den", "und", "lie\u00b7fen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und murmelten durch einander, bis endlich laut es scholl", "tokens": ["Und", "mur\u00b7mel\u00b7ten", "durch", "ein\u00b7an\u00b7der", ",", "bis", "end\u00b7lich", "laut", "es", "scholl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$,", "KON", "ADV", "APPR", "PPER", "ADJD"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vom Mund des Volks und der H\u00e4upter: \u00bbLa\u00dft h\u00e4ngen den Koppenoll!\u00ab", "tokens": ["Vom", "Mund", "des", "Volks", "und", "der", "H\u00e4up\u00b7ter", ":", "\u00bb", "La\u00dft", "h\u00e4n\u00b7gen", "den", "Kop\u00b7pe\u00b7noll", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$.", "$(", "VVIMP", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}}}}