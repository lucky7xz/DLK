{"dta.poem.15699": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lange werden um den Unterschied der Zeiten", "tokens": ["Wie", "lan\u00b7ge", "wer\u00b7den", "um", "den", "Un\u00b7ter\u00b7schied", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihren Vorzug noch die Schriftgelehrten streiten?", "tokens": ["Und", "ih\u00b7ren", "Vor\u00b7zug", "noch", "die", "Schrift\u00b7ge\u00b7lehr\u00b7ten", "strei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die alte Zeit war jung, die junge Zeit ist alt;", "tokens": ["Die", "al\u00b7te", "Zeit", "war", "jung", ",", "die", "jun\u00b7ge", "Zeit", "ist", "alt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser siegt der Geist, in jener die Gestalt.", "tokens": ["In", "die\u00b7ser", "siegt", "der", "Geist", ",", "in", "je\u00b7ner", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "NN", "$,", "APPR", "PDAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Alter kannst du nicht der Jugend Schmuck erneuen,", "tokens": ["Im", "Al\u00b7ter", "kannst", "du", "nicht", "der", "Ju\u00b7gend", "Schmuck", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch der Erinnerung, wer wehrt es dir? dich freuen.", "tokens": ["Doch", "der", "E\u00b7rin\u00b7ne\u00b7rung", ",", "wer", "wehrt", "es", "dir", "?", "dich", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWS", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Erinnre dich! so ist die Welt dir neu geboren,", "tokens": ["E\u00b7rinn\u00b7re", "dich", "!", "so", "ist", "die", "Welt", "dir", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Geister der Geschicht' um dich herauf beschworen,", "tokens": ["Die", "Geis\u00b7ter", "der", "Ge\u00b7schicht'", "um", "dich", "her\u00b7auf", "be\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und nichts, was gro\u00df je war und sch\u00f6n, ist dir verloren.", "tokens": ["Und", "nichts", ",", "was", "gro\u00df", "je", "war", "und", "sch\u00f6n", ",", "ist", "dir", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ADJD", "ADV", "VAFIN", "KON", "ADJD", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}