{"textgrid.poem.25760": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Amor mit gelocktem Haar'", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor mit gelocktem Haar'", "tokens": ["A\u00b7mor", "mit", "ge\u00b7lock\u00b7tem", "Haa\u00b7r'"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwebt auf Myrthen hier und Rosen,", "tokens": ["Schwebt", "auf", "Myr\u00b7then", "hier", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er scheint sein T\u00e4ubchenpaar", "tokens": ["Und", "er", "scheint", "sein", "T\u00e4ub\u00b7chen\u00b7paar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voller Unschuld liebzukosen;", "tokens": ["Vol\u00b7ler", "Un\u00b7schuld", "lieb\u00b7zu\u00b7ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch aus seinen Pfeilen spr\u00fchet,", "tokens": ["Doch", "aus", "sei\u00b7nen", "Pfei\u00b7len", "spr\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Eifersucht gegl\u00fchet,", "tokens": ["Von", "der", "Ei\u00b7fer\u00b7sucht", "ge\u00b7gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Funk' auf Funke, hell empor.", "tokens": ["Funk'", "auf", "Fun\u00b7ke", ",", "hell", "em\u00b7por", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denkst du mehr noch als zuvor", "tokens": ["Denkst", "du", "mehr", "noch", "als", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00e4rm' im Herzen zu erregen?", "tokens": ["W\u00e4rm'", "im", "Her\u00b7zen", "zu", "er\u00b7re\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "O du wirst es, kleiner Thor!", "tokens": ["O", "du", "wirst", "es", ",", "klei\u00b7ner", "Thor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ganz dadurch in Asche legen.", "tokens": ["Ganz", "da\u00b7durch", "in", "A\u00b7sche", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Einmal mu\u00df man freilich sterben,", "tokens": ["Ein\u00b7mal", "mu\u00df", "man", "frei\u00b7lich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch man stirbt auch leicht zu fr\u00fch.", "tokens": ["Doch", "man", "stirbt", "auch", "leicht", "zu", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJD", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "La\u00df die Eifersucht doch nie,", "tokens": ["La\u00df", "die", "Ei\u00b7fer\u00b7sucht", "doch", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Amor! solch ein Herz verderben,", "tokens": ["A\u00b7mor", "!", "solch", "ein", "Herz", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIAT", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Das sich nicht f\u00fcr einz'gen Erben", "tokens": ["Das", "sich", "nicht", "f\u00fcr", "einz'\u00b7gen", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Alles Liebensw\u00fcrd'gen h\u00e4lt;", "tokens": ["Al\u00b7les", "Lie\u00b7bens\u00b7w\u00fcrd'\u00b7gen", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Das zwar Eine nur von allen", "tokens": ["Das", "zwar", "Ei\u00b7ne", "nur", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Oben an auf immer stellt,", "tokens": ["O\u00b7ben", "an", "auf", "im\u00b7mer", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Doch der zweiten, die ihr gleichet,", "tokens": ["Doch", "der", "zwei\u00b7ten", ",", "die", "ihr", "glei\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wenn es Sympathie bestimmt,", "tokens": ["Wenn", "es", "Sym\u00b7pa\u00b7thie", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Freundlich auch ein Veilchen reichet,", "tokens": ["Freund\u00b7lich", "auch", "ein", "Veil\u00b7chen", "rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und sich freuet, wenn sie's nimmt;", "tokens": ["Und", "sich", "freu\u00b7et", ",", "wenn", "sie's", "nimmt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Aber selbst im schlimmsten Falle", "tokens": ["A\u00b7ber", "selbst", "im", "schlimms\u00b7ten", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ihrer Rose nie begehrt,", "tokens": ["Ih\u00b7rer", "Ro\u00b7se", "nie", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Und der Einen \u00fcber Alle,", "tokens": ["Und", "der", "Ei\u00b7nen", "\u00fc\u00b7ber", "Al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Eben dieses gern gew\u00e4hrt.", "tokens": ["E\u00b7ben", "die\u00b7ses", "gern", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Amor! Nur in solchen Herzen", "tokens": ["A\u00b7mor", "!", "Nur", "in", "sol\u00b7chen", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Bist du sicher deiner Macht,", "tokens": ["Bist", "du", "si\u00b7cher", "dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Selbst noch dann, wenn Hymens Kerzen", "tokens": ["Selbst", "noch", "dann", ",", "wenn", "Hy\u00b7mens", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "L\u00e4ngstens sie ans Ziel gebracht.", "tokens": ["L\u00e4ngs\u00b7tens", "sie", "ans", "Ziel", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Andre kann in Einer Nacht", "tokens": ["And\u00b7re", "kann", "in", "Ei\u00b7ner", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Dein beth\u00f6rter Wahn verscherzen.", "tokens": ["Dein", "be\u00b7th\u00f6r\u00b7ter", "Wahn", "ver\u00b7scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}