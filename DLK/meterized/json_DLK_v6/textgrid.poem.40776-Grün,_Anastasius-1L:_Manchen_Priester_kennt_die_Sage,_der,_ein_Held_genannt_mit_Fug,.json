{"textgrid.poem.40776": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Manchen Priester kennt die Sage, der, ein Held genannt mit Fug,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchen Priester kennt die Sage, der, ein Held genannt mit Fug,", "tokens": ["Man\u00b7chen", "Pries\u00b7ter", "kennt", "die", "Sa\u00b7ge", ",", "der", ",", "ein", "Held", "ge\u00b7nannt", "mit", "Fug", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "$,", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Durch die Welt das Wort der Wahrheit k\u00fchn und unaufhaltsam trug,", "tokens": ["Durch", "die", "Welt", "das", "Wort", "der", "Wahr\u00b7heit", "k\u00fchn", "und", "un\u00b7auf\u00b7halt\u00b7sam", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Der im K\u00f6nigssaal gerufen: Pfui, ich witt're Kerkerluft!", "tokens": ["Der", "im", "K\u00f6\u00b7nigs\u00b7saal", "ge\u00b7ru\u00b7fen", ":", "Pfui", ",", "ich", "witt'\u00b7re", "Ker\u00b7ker\u00b7luft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "$.", "NN", "$,", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und es manch' besterntem Heuchler laut gesagt: Du bist ein Schuft!", "tokens": ["Und", "es", "man\u00b7ch'", "bes\u00b7tern\u00b7tem", "Heuch\u00b7ler", "laut", "ge\u00b7sagt", ":", "Du", "bist", "ein", "Schuft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "ADJA", "NN", "ADJD", "VVPP", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "----+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "W\u00e4r' ich solch ein Held der Wahrheit, mit dem M\u00f6nchkleid angethan,", "tokens": ["W\u00e4r'", "ich", "solch", "ein", "Held", "der", "Wahr\u00b7heit", ",", "mit", "dem", "M\u00f6nch\u00b7kleid", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Alsbald an des Censors Wohnung trieb' es mich zu pochen an;", "tokens": ["Als\u00b7bald", "an", "des", "Cen\u00b7sors", "Woh\u00b7nung", "trieb'", "es", "mich", "zu", "po\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und ich spr\u00e4che zu dem Manne: \u00bbErzschelm, sink' aufs Knie zur Stell'!", "tokens": ["Und", "ich", "spr\u00e4\u00b7che", "zu", "dem", "Man\u00b7ne", ":", "\u00bb", "Erz\u00b7schelm", ",", "sink'", "aufs", "Knie", "zur", "Stell'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "NN", "$,", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denn du bist ein gro\u00dfer S\u00fcnder, beichte und bekenne schnell!\u00ab", "tokens": ["Denn", "du", "bist", "ein", "gro\u00b7\u00dfer", "S\u00fcn\u00b7der", ",", "beich\u00b7te", "und", "be\u00b7ken\u00b7ne", "schnell", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbund ich h\u00f6r' es schon im Geiste, wie er drauf in Unschuld spricht:", "tokens": ["\u00bb", "und", "ich", "h\u00f6r'", "es", "schon", "im", "Geis\u00b7te", ",", "wie", "er", "drauf", "in", "Un\u00b7schuld", "spricht", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ihr' Ehrw\u00fcrden sind im Irrthum! der Gesuchte bin ich nicht!", "tokens": ["Ih\u00b7r'", "Ehr\u00b7w\u00fcr\u00b7den", "sind", "im", "Irr\u00b7thum", "!", "der", "Ge\u00b7such\u00b7te", "bin", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Ich vers\u00e4ume keine Messe, Amt und Pflicht verseh' ich gut!", "tokens": ["Ich", "ver\u00b7s\u00e4u\u00b7me", "kei\u00b7ne", "Mes\u00b7se", ",", "Amt", "und", "Pflicht", "ver\u00b7seh'", "ich", "gut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Bin kein Hurer, Gottesl\u00e4st'rer, M\u00f6rder, Dieb, ungl\u00e4ub'ger Jud'!", "tokens": ["Bin", "kein", "Hu\u00b7rer", ",", "Got\u00b7tes\u00b7l\u00e4st'\u00b7rer", ",", "M\u00f6r\u00b7der", ",", "Dieb", ",", "un\u00b7gl\u00e4ub'\u00b7ger", "Jud'", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Doch aus mir dann br\u00e4che flammend der Begeist'rung Gluth hervor", "tokens": ["Doch", "aus", "mir", "dann", "br\u00e4\u00b7che", "flam\u00b7mend", "der", "Be\u00b7geist'\u00b7rung", "Gluth", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "ADJD", "ART", "NN", "NN", "PTKVZ"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Wie durch Berg und Kluft der Donner, dr\u00f6hnt' ihm meine Stimm' ans Ohr;", "tokens": ["Wie", "durch", "Berg", "und", "Kluft", "der", "Don\u00b7ner", ",", "dr\u00f6hnt'", "ihm", "mei\u00b7ne", "Stimm'", "ans", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Jeder Blick entfl\u00f6ge t\u00f6dtend ihm als Pfeil ins Herz hinein,", "tokens": ["Je\u00b7der", "Blick", "ent\u00b7fl\u00f6\u00b7ge", "t\u00f6d\u00b7tend", "ihm", "als", "Pfeil", "ins", "Herz", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "PPER", "KOUS", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Jedes Wort, es m\u00fc\u00dft' ein Hammer, der ihn ganz zermalme, sein:", "tokens": ["Je\u00b7des", "Wort", ",", "es", "m\u00fc\u00dft'", "ein", "Ham\u00b7mer", ",", "der", "ihn", "ganz", "zer\u00b7mal\u00b7me", ",", "sein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Ja, du bist ein blinder Jude! denn du hast's noch nicht erkannt,", "tokens": ["Ja", ",", "du", "bist", "ein", "blin\u00b7der", "Ju\u00b7de", "!", "denn", "du", "hast's", "noch", "nicht", "er\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "KON", "PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df des Geistes Freiheit glorreich als Messias uns erstand!", "tokens": ["Da\u00df", "des", "Geis\u00b7tes", "Frei\u00b7heit", "glor\u00b7reich", "als", "Mes\u00b7si\u00b7as", "uns", "er\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADJD", "KOKOM", "NE", "PPER", "VVFIN", "$."], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ja, du bist ein blut'ger M\u00f6rder! doppelt arg und doppelt dreist!", "tokens": ["Ja", ",", "du", "bist", "ein", "blut'\u00b7ger", "M\u00f6r\u00b7der", "!", "dop\u00b7pelt", "arg", "und", "dop\u00b7pelt", "dreist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "ADJD", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nur die Leiber t\u00f6dtet jener, doch du mordest auch den Geist!", "tokens": ["Nur", "die", "Lei\u00b7ber", "t\u00f6d\u00b7tet", "je\u00b7ner", ",", "doch", "du", "mor\u00b7dest", "auch", "den", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PDS", "$,", "KON", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Ja, du bist ein Dieb, ein arger, oder noch viel schlimmer, traun!", "tokens": ["Ja", ",", "du", "bist", "ein", "Dieb", ",", "ein", "ar\u00b7ger", ",", "o\u00b7der", "noch", "viel", "schlim\u00b7mer", ",", "traun", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "KON", "ADV", "ADV", "ADJD", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Obst vom Baum bei Nacht zu stehlen, schwingt sich jener \u00fcbern Zaun;", "tokens": ["Obst", "vom", "Baum", "bei", "Nacht", "zu", "steh\u00b7len", ",", "schwingt", "sich", "je\u00b7ner", "\u00fc\u00b7bern", "Zaun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PRF", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "In des Menschengeistes Garten, schadenfroh mit", "tokens": ["In", "des", "Men\u00b7schen\u00b7geis\u00b7tes", "Gar\u00b7ten", ",", "scha\u00b7den\u00b7froh", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Willst den ganzen Baum du f\u00e4llen, Bl\u00fcthe, Laub und Frucht zugleich!", "tokens": ["Willst", "den", "gan\u00b7zen", "Baum", "du", "f\u00e4l\u00b7len", ",", "Bl\u00fc\u00b7the", ",", "Laub", "und", "Frucht", "zu\u00b7gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,", "NN", "$,", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Ja, du bist ein Ehebrecher! doch an Schande doppelt reich!", "tokens": ["Ja", ",", "du", "bist", "ein", "E\u00b7heb\u00b7re\u00b7cher", "!", "doch", "an", "Schan\u00b7de", "dop\u00b7pelt", "reich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "ADV", "APPR", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Jener gl\u00fcht und flammt f\u00fcrs Sch\u00f6ne, bl\u00fcht's in fremdem Garten gleich;", "tokens": ["Je\u00b7ner", "gl\u00fcht", "und", "flammt", "f\u00fcrs", "Sch\u00f6\u00b7ne", ",", "bl\u00fcht's", "in", "frem\u00b7dem", "Gar\u00b7ten", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "F\u00fcr die sch\u00f6ne, stolze S\u00fcnde ist dein Herz zu klein, zu schmal!", "tokens": ["F\u00fcr", "die", "sch\u00f6\u00b7ne", ",", "stol\u00b7ze", "S\u00fcn\u00b7de", "ist", "dein", "Herz", "zu", "klein", ",", "zu", "schmal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und der Nacht und Nebel Dirne, die nur ist dein Ideal!", "tokens": ["Und", "der", "Nacht", "und", "Ne\u00b7bel", "Dir\u00b7ne", ",", "die", "nur", "ist", "dein", "I\u00b7deal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NN", "NN", "$,", "PRELS", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}, "stanza.8": {"line.1": {"text": "Ja, du bist ein Gottesl\u00e4st'rer, oder \u00e4rger noch, bei Gott!", "tokens": ["Ja", ",", "du", "bist", "ein", "Got\u00b7tes\u00b7l\u00e4st'\u00b7rer", ",", "o\u00b7der", "\u00e4r\u00b7ger", "noch", ",", "bei", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "KON", "ADJD", "ADV", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Todte Holz- und Marmorbilder schl\u00e4gt in Tr\u00fcmmer frech sein Spott!", "tokens": ["Tod\u00b7te", "Holz", "und", "Mar\u00b7mor\u00b7bil\u00b7der", "schl\u00e4gt", "in", "Tr\u00fcm\u00b7mer", "frech", "sein", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "TRUNC", "KON", "NN", "VVFIN", "APPR", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Deine Hand doch ist's, die ruchlos das lebend'ge Bild zerschl\u00e4gt!", "tokens": ["Dei\u00b7ne", "Hand", "doch", "ist's", ",", "die", "ruch\u00b7los", "das", "le\u00b7ben\u00b7d'\u00b7ge", "Bild", "zer\u00b7schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "$,", "PRELS", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Das nach Gottes heil'gem Stempel Menschengeist hat ausgepr\u00e4gt!", "tokens": ["Das", "nach", "Got\u00b7tes", "heil'\u00b7gem", "Stem\u00b7pel", "Men\u00b7schen\u00b7geist", "hat", "aus\u00b7ge\u00b7pr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "ADJA", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Ja, du bist ein gro\u00dfer S\u00fcnder! \u2013 Frei l\u00e4\u00dft irdisch Recht dich geh'n,", "tokens": ["Ja", ",", "du", "bist", "ein", "gro\u00b7\u00dfer", "S\u00fcn\u00b7der", "!", "\u2013", "Frei", "l\u00e4\u00dft", "ir\u00b7disch", "Recht", "dich", "geh'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$(", "NN", "VVFIN", "ADJD", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch