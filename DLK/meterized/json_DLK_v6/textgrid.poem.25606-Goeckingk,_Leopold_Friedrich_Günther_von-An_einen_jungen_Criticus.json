{"textgrid.poem.25606": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "An einen jungen Criticus", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sollst mir dein Patent nicht zeigen;", "tokens": ["Du", "sollst", "mir", "dein", "Pa\u00b7tent", "nicht", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df, zum Criticus kann jeder sich erh\u00f6hn.", "tokens": ["Ich", "wei\u00df", ",", "zum", "Cri\u00b7ti\u00b7cus", "kann", "je\u00b7der", "sich", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "VMFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Darin indessen bin ich eigen:", "tokens": ["Da\u00b7rin", "in\u00b7des\u00b7sen", "bin", "ich", "ei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00fcnschte deinen Bart zu sehn.", "tokens": ["Ich", "w\u00fcnschte", "dei\u00b7nen", "Bart", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}