{"textgrid.poem.41110": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liegt dein Herz gedr\u00fcckt an meines,", "tokens": ["Liegt", "dein", "Herz", "ge\u00b7dr\u00fcckt", "an", "mei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "APPR", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich wahrlich niemals sagen:", "tokens": ["Kann", "ich", "wahr\u00b7lich", "nie\u00b7mals", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind's die Wellen meines, deines,", "tokens": ["Sin\u00b7d's", "die", "Wel\u00b7len", "mei\u00b7nes", ",", "dei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "PPOSAT", "$,", "PPOSAT", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die in solcher Liebe schlagen?", "tokens": ["Die", "in", "sol\u00b7cher", "Lie\u00b7be", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollte nur, ich k\u00f6nnte legen", "tokens": ["Woll\u00b7te", "nur", ",", "ich", "k\u00f6nn\u00b7te", "le\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "$,", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dein Herz mein Herz, zu f\u00fchlen", "tokens": ["In", "dein", "Herz", "mein", "Herz", ",", "zu", "f\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmerz und Lust in gleichen Schl\u00e4gen,", "tokens": ["Schmerz", "und", "Lust", "in", "glei\u00b7chen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiches Lieben, gleiches Zielen,", "tokens": ["Glei\u00b7ches", "Lie\u00b7ben", ",", "glei\u00b7ches", "Zie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df, wenn Frieden meines f\u00e4nde,", "tokens": ["Da\u00df", ",", "wenn", "Frie\u00b7den", "mei\u00b7nes", "f\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Frieden dann auch f\u00e4nde deines,", "tokens": ["Frie\u00b7den", "dann", "auch", "f\u00e4n\u00b7de", "dei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "VVFIN", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df, wenn deins im Tode st\u00e4nde,", "tokens": ["Da\u00df", ",", "wenn", "deins", "im", "To\u00b7de", "st\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dann auch st\u00e4nd' im Tode meines.", "tokens": ["Dann", "auch", "st\u00e4nd'", "im", "To\u00b7de", "mei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}