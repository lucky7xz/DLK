{"textgrid.poem.47369": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Naturpoesie", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Sch\u00f6nste ward gedichtet", "tokens": ["Das", "Sch\u00f6ns\u00b7te", "ward", "ge\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von keines Dichters Mund,", "tokens": ["Von", "kei\u00b7nes", "Dich\u00b7ters", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Denkmal ist errichtet,", "tokens": ["Kein", "Denk\u00b7mal", "ist", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Marmor thut es kund.", "tokens": ["Kein", "Mar\u00b7mor", "thut", "es", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hat sich selbst geboren,", "tokens": ["Es", "hat", "sich", "selbst", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie eine Blume sprie\u00dft", "tokens": ["Wie", "ei\u00b7ne", "Blu\u00b7me", "sprie\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wie aus Felsenthoren", "tokens": ["Und", "wie", "aus", "Fel\u00b7sen\u00b7tho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Brunnquell sich ergie\u00dft.", "tokens": ["Ein", "Brunn\u00b7quell", "sich", "er\u00b7gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}