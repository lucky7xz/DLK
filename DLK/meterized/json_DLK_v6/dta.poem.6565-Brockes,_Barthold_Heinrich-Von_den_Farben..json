{"dta.poem.6565": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Schwartze nun besteht aus Netzen, voller Ecken,", "tokens": ["Das", "Schwart\u00b7ze", "nun", "be\u00b7steht", "aus", "Net\u00b7zen", ",", "vol\u00b7ler", "E\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worinn die Strahlen sich vertieffen und verstecken,", "tokens": ["Wo\u00b7rinn", "die", "Strah\u00b7len", "sich", "ver\u00b7tief\u00b7fen", "und", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deren kleine Falten", "tokens": ["Und", "de\u00b7ren", "klei\u00b7ne", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dieselbigen verbergen und behalten,", "tokens": ["Die\u00b7sel\u00b7bi\u00b7gen", "ver\u00b7ber\u00b7gen", "und", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Worinn sie sich vertheilen und verlieren,", "tokens": ["Wo\u00b7rinn", "sie", "sich", "ver\u00b7thei\u00b7len", "und", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So, da\u00df sie unser Aug\u2019 nicht wieder r\u00fchren.", "tokens": ["So", ",", "da\u00df", "sie", "un\u00b7ser", "Aug'", "nicht", "wie\u00b7der", "r\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ja k\u00f6nnen wir davon nicht klar die Probe sp\u00fcren?", "tokens": ["Ja", "k\u00f6n\u00b7nen", "wir", "da\u00b7von", "nicht", "klar", "die", "Pro\u00b7be", "sp\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PPER", "PAV", "PTKNEG", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die dunckle Nacht, der Wolcken tr\u00fcber Dufft,", "tokens": ["Die", "dunck\u00b7le", "Nacht", ",", "der", "Wol\u00b7cken", "tr\u00fc\u00b7ber", "Dufft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die tieffen Oerter, worinn nimmer", "tokens": ["Die", "tief\u00b7fen", "O\u00b7er\u00b7ter", ",", "wo\u00b7rinn", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADV"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Ein Licht gelangt, noch sonst ein Schimmer,", "tokens": ["Ein", "Licht", "ge\u00b7langt", ",", "noch", "sonst", "ein", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sind in der Finsterni\u00df, worinn sie stehn,", "tokens": ["Sind", "in", "der", "Fins\u00b7ter\u00b7ni\u00df", ",", "wo\u00b7rinn", "sie", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nichts anders als nur schwartz und dunckel, anzusehn.", "tokens": ["Nichts", "an\u00b7ders", "als", "nur", "schwartz", "und", "dun\u00b7ckel", ",", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "ADV", "KOKOM", "ADV", "ADJD", "KON", "ADJD", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}