{"dta.poem.12673": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nach-reue.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.71", "cy:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich soll es ewig reun,", "tokens": ["Mich", "soll", "es", "e\u00b7wig", "reun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich nicht bin ein affocate worden;", "tokens": ["Da\u00df", "ich", "nicht", "bin", "ein", "af\u00b7fo\u00b7ca\u00b7te", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So k\u00f6nt ich schon bef\u00f6dert seyn,", "tokens": ["So", "k\u00f6nt", "ich", "schon", "be\u00b7f\u00f6\u00b7dert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00e4\u00dfe wo in einem h\u00f6hern orden.", "tokens": ["Und", "s\u00e4\u00b7\u00dfe", "wo", "in", "ei\u00b7nem", "h\u00f6\u00b7hern", "or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich wolt\u2019 es nichts nicht achten,", "tokens": ["Ich", "wolt'", "es", "nichts", "nicht", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn gleich die leute mich zum b\u00f6sen Christen machten,", "tokens": ["Wenn", "gleich", "die", "leu\u00b7te", "mich", "zum", "b\u00f6\u00b7sen", "Chris\u00b7ten", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es kommt schon eine zeit,", "tokens": ["Es", "kommt", "schon", "ei\u00b7ne", "zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df man sie wiederum davor geheit.", "tokens": ["Da\u00df", "man", "sie", "wie\u00b7de\u00b7rum", "da\u00b7vor", "ge\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "PAV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Jedoch ich gebe mich nun drein,", "tokens": ["Je\u00b7doch", "ich", "ge\u00b7be", "mich", "nun", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und will nur, was ich bin, auch ferner seyn,", "tokens": ["Und", "will", "nur", ",", "was", "ich", "bin", ",", "auch", "fer\u00b7ner", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,", "ADV", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So kan ich jedermann recht unter augen treten,", "tokens": ["So", "kan", "ich", "je\u00b7der\u00b7mann", "recht", "un\u00b7ter", "au\u00b7gen", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Unds siebende gebot mit froherm hertzen beten.", "tokens": ["Unds", "sie\u00b7ben\u00b7de", "ge\u00b7bot", "mit", "fro\u00b7herm", "hert\u00b7zen", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}}}}