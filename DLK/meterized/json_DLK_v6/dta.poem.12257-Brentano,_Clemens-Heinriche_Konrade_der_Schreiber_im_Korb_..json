{"dta.poem.12257": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Heinriche Konrade der Schreiber  \n im Korb .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ging ein Schreiber spatzieren aus               ", "tokens": ["Es", "ging", "ein", "Schrei\u00b7ber", "spat\u00b7zie\u00b7ren", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wohl an dem Markt da steht ein Haus,", "tokens": ["Wohl", "an", "dem", "Markt", "da", "steht", "ein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heinriche Konrade der Schreiber im Korb.", "tokens": ["Hein\u00b7ri\u00b7che", "Kon\u00b7ra\u00b7de", "der", "Schrei\u00b7ber", "im", "Korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Er sprach: \u201eGott gr\u00fc\u00df euch Jungfrau fein,", "tokens": ["Er", "sprach", ":", "\u201e", "Gott", "gr\u00fc\u00df", "euch", "Jung\u00b7frau", "fein", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "VVFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201enun wollt ihr heut mein Schlafbuhl sein?\u201c", "tokens": ["\u201e", "nun", "wollt", "ihr", "heut", "mein", "Schlaf\u00b7buhl", "sein", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heinriche Konrade der Schreiber im Korb.", "tokens": ["Hein\u00b7ri\u00b7che", "Kon\u00b7ra\u00b7de", "der", "Schrei\u00b7ber", "im", "Korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Sie sprach: \u201eKommt schier her wiedere,", "tokens": ["Sie", "sprach", ":", "\u201e", "Kommt", "schier", "her", "wie\u00b7de\u00b7re", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVIMP", "ADV", "APZR", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201ewann sich mein Herr legt niedere.\u201c", "tokens": ["\u201e", "wann", "sich", "mein", "Herr", "legt", "nie\u00b7de\u00b7re", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "PRF", "PPOSAT", "NN", "VVFIN", "PIS", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heinriche Konrade der Schreiber im Korb.", "tokens": ["Hein\u00b7ri\u00b7che", "Kon\u00b7ra\u00b7de", "der", "Schrei\u00b7ber", "im", "Korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Wohlhin, wohlhin gen Mitternacht,", "tokens": ["Wohl\u00b7hin", ",", "wohl\u00b7hin", "gen", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schreiber kam gegangen dar.", "tokens": ["Der", "Schrei\u00b7ber", "kam", "ge\u00b7gan\u00b7gen", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heinriche Konrade der Schreiber im Korb.", "tokens": ["Hein\u00b7ri\u00b7che", "Kon\u00b7ra\u00b7de", "der", "Schrei\u00b7ber", "im", "Korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Sie sprach: \u201eMein Schlafbuhl sollst nicht sein,", "tokens": ["Sie", "sprach", ":", "\u201e", "Mein", "Schlaf\u00b7buhl", "sollst", "nicht", "sein", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPOSAT", "NN", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edu setz'st dich dann ins K\u00f6rbelein.\u201c", "tokens": ["\u201e", "du", "setz'st", "dich", "dann", "ins", "K\u00f6r\u00b7be\u00b7lein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heinriche Konrade der Schreiber im Korb.", "tokens": ["Hein\u00b7ri\u00b7che", "Kon\u00b7ra\u00b7de", "der", "Schrei\u00b7ber", "im", "Korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Dem Schreiber gefiel der Korb nicht wohl,", "tokens": ["Dem", "Schrei\u00b7ber", "ge\u00b7fiel", "der", "Korb", "nicht", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er durft ihm nicht getrauen wohl.", "tokens": ["Er", "durft", "ihm", "nicht", "ge\u00b7trau\u00b7en", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVPP", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Heinriche Konrade der Schreiber im Korb.", "tokens": ["Hein\u00b7ri\u00b7che", "Kon\u00b7ra\u00b7de", "der", "Schrei\u00b7ber", "im", "Korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Der Schreiber wollt gen Himmel fahren,", "tokens": ["Der", "Schrei\u00b7ber", "wollt", "gen", "Him\u00b7mel", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da hatt' er weder Ro\u00df noch Wagen.", "tokens": ["Da", "hatt'", "er", "we\u00b7der", "Ro\u00df", "noch", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Heinriche Konrade der Schreiber im Korb.", "tokens": ["Hein\u00b7ri\u00b7che", "Kon\u00b7ra\u00b7de", "der", "Schrei\u00b7ber", "im", "Korb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"t