{"textgrid.poem.26510": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freilich, freilich, Alles eitel,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freilich, freilich, Alles eitel,", "tokens": ["Frei\u00b7lich", ",", "frei\u00b7lich", ",", "Al\u00b7les", "ei\u00b7tel", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "PIS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles Trug und Schein \u2013", "tokens": ["Al\u00b7les", "Trug", "und", "Schein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, wie bald ergraut der Scheitel,", "tokens": ["Ach", ",", "wie", "bald", "er\u00b7graut", "der", "Schei\u00b7tel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und du stehst allein!", "tokens": ["Und", "du", "stehst", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Deine Hoffnungen und Thaten", "tokens": ["Dei\u00b7ne", "Hoff\u00b7nun\u00b7gen", "und", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat die Zeit gef\u00e4llt,", "tokens": ["Hat", "die", "Zeit", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und du siehest neue Saaten", "tokens": ["Und", "du", "sie\u00b7hest", "neu\u00b7e", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ohne dich bestellt.", "tokens": ["Oh\u00b7ne", "dich", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und du fragst zuletzt mit Grollen:", "tokens": ["Und", "du", "fragst", "zu\u00b7letzt", "mit", "Grol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich nur gelebt,", "tokens": ["Hab'", "ich", "nur", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um der rauhen Hand zu zollen,", "tokens": ["Um", "der", "rau\u00b7hen", "Hand", "zu", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die die Gr\u00e4ber gr\u00e4bt?", "tokens": ["Die", "die", "Gr\u00e4\u00b7ber", "gr\u00e4bt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}