{"dta.poem.290": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet sie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lieb dich sch\u00f6nstes Kind/", "tokens": ["Ich", "lieb", "dich", "sch\u00f6ns\u00b7tes", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Anblick deiner zarten Wangen", "tokens": ["Der", "An\u00b7blick", "dei\u00b7ner", "zar\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4\u00dft mir ein solch Gefilde sehn/", "tokens": ["L\u00e4\u00dft", "mir", "ein", "solch", "Ge\u00b7fil\u00b7de", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf welchem ", "tokens": ["Auf", "wel\u00b7chem"], "token_info": ["word", "word"], "pos": ["APPR", "PWAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Die in der sch\u00f6nsten Bl\u00fchte prangen.", "tokens": ["Die", "in", "der", "sch\u00f6ns\u00b7ten", "Bl\u00fch\u00b7te", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein Schnecken-Blut ist zuvergleichen", "tokens": ["Kein", "Schne\u00b7cken\u00b7Blut", "ist", "zu\u00b7ver\u00b