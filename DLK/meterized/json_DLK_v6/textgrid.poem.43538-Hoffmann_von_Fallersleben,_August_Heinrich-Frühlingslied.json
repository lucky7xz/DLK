{"textgrid.poem.43538": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslied", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blauer Himmel, milde Luft,", "tokens": ["Blau\u00b7er", "Him\u00b7mel", ",", "mil\u00b7de", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vogelsang und Bl\u00fcthenduft,", "tokens": ["Vo\u00b7gel\u00b7sang", "und", "Bl\u00fc\u00b7then\u00b7duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberall Sang und Schall,", "tokens": ["Ue\u00b7be\u00b7rall", "Sang", "und", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Freud' und Leben \u00fcberall:", "tokens": ["Freud'", "und", "Le\u00b7ben", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und in diesen sch\u00f6nen Tagen,", "tokens": ["Und", "in", "die\u00b7sen", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herz, und du nur wolltest klagen?", "tokens": ["Herz", ",", "und", "du", "nur", "woll\u00b7test", "kla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie die Ros' in ihrer Pracht", "tokens": ["Wie", "die", "Ros'", "in", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Froh der Sonn' entgegen lacht,", "tokens": ["Froh", "der", "Sonn'", "ent\u00b7ge\u00b7gen", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chle du voller Ruh", "tokens": ["L\u00e4ch\u00b7le", "du", "vol\u00b7ler", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gottes lieber Sonne zu!", "tokens": ["Got\u00b7tes", "lie\u00b7ber", "Son\u00b7ne", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcrchte keine Nacht auf Erden!", "tokens": ["F\u00fcrch\u00b7te", "kei\u00b7ne", "Nacht", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer mu\u00df es Morgen werden.", "tokens": ["Im\u00b7mer", "mu\u00df", "es", "Mor\u00b7gen", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}