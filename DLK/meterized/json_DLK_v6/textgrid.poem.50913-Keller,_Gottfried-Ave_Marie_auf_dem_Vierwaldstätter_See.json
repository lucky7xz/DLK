{"textgrid.poem.50913": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Ave Marie auf dem Vierwaldst\u00e4tter See", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fuhr ein Schifflein gegen Fl\u00fcelen,", "tokens": ["Fuhr", "ein", "Schif\u00b7flein", "ge\u00b7gen", "Fl\u00fce\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drin ich sa\u00df, zur Abendzeit,", "tokens": ["Drin", "ich", "sa\u00df", ",", "zur", "A\u00b7bend\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die finsteren Wasser sp\u00fclen", "tokens": ["Wo", "die", "fins\u00b7te\u00b7ren", "Was\u00b7ser", "sp\u00fc\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und den Bergen die F\u00fc\u00dfe k\u00fchlen", "tokens": ["Und", "den", "Ber\u00b7gen", "die", "F\u00fc\u00b7\u00dfe", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Schon seit einer Ewigkeit.", "tokens": ["Schon", "seit", "ei\u00b7ner", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus den finstern Felseng\u00e4ngen", "tokens": ["Aus", "den", "fins\u00b7tern", "Fel\u00b7sen\u00b7g\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bang ein Hauch des F\u00f6hnes strich,", "tokens": ["Bang", "ein", "Hauch", "des", "F\u00f6h\u00b7nes", "strich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Gewebe von Abendkl\u00e4ngen", "tokens": ["Ein", "Ge\u00b7we\u00b7be", "von", "A\u00b7bend\u00b7kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zitterte an den Alpenh\u00e4ngen,", "tokens": ["Zit\u00b7ter\u00b7te", "an", "den", "Al\u00b7pen\u00b7h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und der Ferg bekreuzte sich.", "tokens": ["Und", "der", "Ferg", "be\u00b7kreuz\u00b7te", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dunkel lauschten die Kapellen", "tokens": ["Dun\u00b7kel", "lauschten", "die", "Ka\u00b7pel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Alter Freiheit aus dem See;", "tokens": ["Al\u00b7ter", "Frei\u00b7heit", "aus", "dem", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo einst fuhren die frommen Tellen,", "tokens": ["Wo", "einst", "fuh\u00b7ren", "die", "from\u00b7men", "Tel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Tauchte jetzo aus den Wellen", "tokens": ["Tauch\u00b7te", "jet\u00b7zo", "aus", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dieses Wassers schlimme Fee.", "tokens": ["Die\u00b7ses", "Was\u00b7sers", "schlim\u00b7me", "Fee", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, ich sah sie steigen, winken", "tokens": ["Ja", ",", "ich", "sah", "sie", "stei\u00b7gen", ",", "win\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der schw\u00e4rzlichgr\u00fcnen Flut!", "tokens": ["Aus", "der", "schw\u00e4rz\u00b7lich\u00b7gr\u00fc\u00b7nen", "Flut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lie\u00df der Krone goldene Zinken", "tokens": ["Lie\u00df", "der", "Kro\u00b7ne", "gol\u00b7de\u00b7ne", "Zin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "T\u00fcckisch in der Sonne blinken,", "tokens": ["T\u00fc\u00b7ckisch", "in", "der", "Son\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der sterbenden Sonne Glut.", "tokens": ["In", "der", "ster\u00b7ben\u00b7den", "Son\u00b7ne", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Fabelhaft und heidnisch bl\u00fchte", "tokens": ["Fa\u00b7bel\u00b7haft", "und", "heid\u00b7nisch", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihrer Sch\u00f6nheit arger Flor;", "tokens": ["Ih\u00b7rer", "Sch\u00f6n\u00b7heit", "ar\u00b7ger", "Flor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wilde Schadenfreude gl\u00fchte", "tokens": ["Wil\u00b7de", "Scha\u00b7den\u00b7freu\u00b7de", "gl\u00fch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein buhlerisch Feuer spr\u00fchte", "tokens": ["Und", "ein", "buh\u00b7le\u00b7risch", "Feu\u00b7er", "spr\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aus den seidenen Wimpern vor.", "tokens": ["Aus", "den", "sei\u00b7de\u00b7nen", "Wim\u00b7pern", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Haar und Schleier, ungebunden,", "tokens": ["Haar", "und", "Schlei\u00b7er", ",", "un\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wehten in dem hei\u00dfen Wind;", "tokens": ["Weh\u00b7ten", "in", "dem", "hei\u00b7\u00dfen", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie hielt im wei\u00dfen, runden", "tokens": ["Und", "sie", "hielt", "im", "wei\u00b7\u00dfen", ",", "run\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arm ein Kind mit sieben Wunden,", "tokens": ["Arm", "ein", "Kind", "mit", "sie\u00b7ben", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein ersterbendes, welkes Kind.", "tokens": ["Ein", "ers\u00b7ter\u00b7ben\u00b7des", ",", "wel\u00b7kes", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "An den staffellosen W\u00e4nden", "tokens": ["An", "den", "staf\u00b7fel\u00b7lo\u00b7sen", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glitt die grauliche Nix hinan;", "tokens": ["Glitt", "die", "grau\u00b7li\u00b7che", "Nix", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Purpurzinnen und R\u00e4nden", "tokens": ["Von", "den", "Pur\u00b7pur\u00b7zin\u00b7nen", "und", "R\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hielt sie das Kind in erhobenen H\u00e4nden", "tokens": ["Hielt", "sie", "das", "Kind", "in", "er\u00b7ho\u00b7be\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "\u00dcber der L\u00e4nder tiefen Plan.", "tokens": ["\u00dc\u00b7ber", "der", "L\u00e4n\u00b7der", "tie\u00b7fen", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Sieben Tropfen aus sieben Wunden", "tokens": ["Sie\u00b7ben", "Trop\u00b7fen", "aus", "sie\u00b7ben", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "CARD", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Pre\u00dfte sie dem armen Wurm;", "tokens": ["Pre\u00df\u00b7te", "sie", "dem", "ar\u00b7men", "Wurm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die rot hinabgeschwunden,", "tokens": ["Wo", "die", "rot", "hin\u00b7ab\u00b7ge\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sich die Flut emporgewunden,", "tokens": ["Hat", "sich", "die", "Flut", "em\u00b7por\u00b7ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schreiend in Wut und Weh und Sturm!", "tokens": ["Schrei\u00b7end", "in", "Wut", "und", "Weh", "und", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wut und Wahn die Herzen fa\u00dfte", "tokens": ["Wut", "und", "Wahn", "die", "Her\u00b7zen", "fa\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Borden rings am See,", "tokens": ["An", "den", "Bor\u00b7den", "rings", "am", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Da\u00df der Priester im Blute pra\u00dfte", "tokens": ["Da\u00df", "der", "Pries\u00b7ter", "im", "Blu\u00b7te", "pra\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und der Bruder den Bruder ha\u00dfte,", "tokens": ["Und", "der", "Bru\u00b7der", "den", "Bru\u00b7der", "ha\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ihm zum eigenen Gift und Weh!", "tokens": ["Ihm", "zum", "ei\u00b7ge\u00b7nen", "Gift", "und", "Weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Als das Ave Marie verklungen,", "tokens": ["Als", "das", "A\u00b7ve", "Ma\u00b7rie", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NE", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "War der arge Spuk entflohn. \u2013", "tokens": ["War", "der", "ar\u00b7ge", "Spuk", "ent\u00b7flohn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch ein Alphorn hat gesungen", "tokens": ["Noch", "ein", "Al\u00b7phorn", "hat", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der H\u00f6h, und leis bezwungen", "tokens": ["Aus", "der", "H\u00f6h", ",", "und", "leis", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ADJD", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Hat mein Herz sein s\u00fc\u00dfer Ton.", "tokens": ["Hat", "mein", "Herz", "sein", "s\u00fc\u00b7\u00dfer", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}