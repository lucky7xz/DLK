{"textgrid.poem.67796": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das s\u00fc\u00dfeste Genie\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das s\u00fc\u00dfeste Genie\u00dfen", "tokens": ["Das", "s\u00fc\u00b7\u00dfes\u00b7te", "Ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist, nichts von Stolze wissen,", "tokens": ["Ist", ",", "nichts", "von", "Stol\u00b7ze", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PIS", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich seiner Demuth freun.", "tokens": ["Sich", "sei\u00b7ner", "De\u00b7muth", "freun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wer seiner Pflicht sich freuet", "tokens": ["Wer", "sei\u00b7ner", "Pflicht", "sich", "freu\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und jede Hoffart scheuet,", "tokens": ["Und", "je\u00b7de", "Hof\u00b7fart", "scheu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der f\u00fchlt das Gl\u00fcck, er selbst zu sein.", "tokens": ["Der", "f\u00fchlt", "das", "Gl\u00fcck", ",", "er", "selbst", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PPER", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was n\u00fctzen uns die Gaben,", "tokens": ["Was", "n\u00fct\u00b7zen", "uns", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wenn wir nicht brauchen sie?", "tokens": ["Wenn", "wir", "nicht", "brau\u00b7chen", "sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was st\u00f6ren uns die Gaben,", "tokens": ["Was", "st\u00f6\u00b7ren", "uns", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Andre f\u00fcr uns haben?", "tokens": ["Die", "And\u00b7re", "f\u00fcr", "uns", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Mit Dir hast Du verloren", "tokens": ["Mit", "Dir", "hast", "Du", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich selbst, Dich selbst, den Thoren,", "tokens": ["Dich", "selbst", ",", "Dich", "selbst", ",", "den", "Tho\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "ADV", "$,", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Alles \u00fcbernahm.", "tokens": ["Der", "Al\u00b7les", "\u00fc\u00b7bern\u00b7ahm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was hast Du als Dein Leben?", "tokens": ["Was", "hast", "Du", "als", "Dein", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ward es Dir gegeben,", "tokens": ["Und", "ward", "es", "Dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Du's verschwendetest im Gram?", "tokens": ["Da\u00df", "Du's", "ver\u00b7schwen\u00b7de\u00b7test", "im", "Gram", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Herr, la\u00df uns unser Fehlen", "tokens": ["Herr", ",", "la\u00df", "uns", "un\u00b7ser", "Feh\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und unsre Tage z\u00e4hlen,", "tokens": ["Und", "uns\u00b7re", "Ta\u00b7ge", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht Eitelkeit uns freun!", "tokens": ["Nicht", "Ei\u00b7tel\u00b7keit", "uns", "freun", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "La\u00df uns selbststehend werden", "tokens": ["La\u00df", "uns", "selbst\u00b7ste\u00b7hend", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "VAINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Und vor Dir, hier auf Erden", "tokens": ["Und", "vor", "Dir", ",", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "$,", "ADV", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wie dort im Himmel, Kinder sein!", "tokens": ["Wie", "dort", "im", "Him\u00b7mel", ",", "Kin\u00b7der", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "$,", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was n\u00fctzen uns Gesch\u00e4fte,", "tokens": ["Was", "n\u00fct\u00b7zen", "uns", "Ge\u00b7sch\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An die wir unsre Kr\u00e4fte", "tokens": ["An", "die", "wir", "uns\u00b7re", "Kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verschwenden ohne Pflicht?", "tokens": ["Ver\u00b7schwen\u00b7den", "oh\u00b7ne", "Pflicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Uns durch uns selbst zu lohnen:", "tokens": ["Uns", "durch", "uns", "selbst", "zu", "loh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Demuth gieb uns, Hochmuth nicht!", "tokens": ["Die", "De\u00b7muth", "gieb", "uns", ",", "Hoch\u00b7muth", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "PPER", "$,", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In Wolken schwinden D\u00fcnste,", "tokens": ["In", "Wol\u00b7ken", "schwin\u00b7den", "D\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach Wolken zielen K\u00fcnste,", "tokens": ["Nach", "Wol\u00b7ken", "zie\u00b7len", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sich des Leeren freun.", "tokens": ["Die", "sich", "des", "Lee\u00b7ren", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In wem sich Menschheit reget,", "tokens": ["In", "wem", "sich", "Menschheit", "re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In wem sich Kraft beweget,", "tokens": ["In", "wem", "sich", "Kraft", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der f\u00fchl' das Gl\u00fcck, ", "tokens": ["Der", "f\u00fchl'", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}}}}