{"dta.poem.19254": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du ruhst nicht, bis den Strom, der breit durch L\u00e4nder schwillt,", "tokens": ["Du", "ruhst", "nicht", ",", "bis", "den", "Strom", ",", "der", "breit", "durch", "L\u00e4n\u00b7der", "schwillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du schwach und schmal entdeckst, wie er dem Sand entquillt.", "tokens": ["Du", "schwach", "und", "schmal", "ent\u00b7deckst", ",", "wie", "er", "dem", "Sand", "ent\u00b7quillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und meinst du nun, der Strom sei diesem Quell entsprossen,", "tokens": ["Und", "meinst", "du", "nun", ",", "der", "Strom", "sei", "die\u00b7sem", "Quell", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da soviel tausend B\u00e4ch' in ihm zusammenflossen?", "tokens": ["Da", "so\u00b7viel", "tau\u00b7send", "B\u00e4ch'", "in", "ihm", "zu\u00b7sam\u00b7men\u00b7flos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "CARD", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du legst nur, damit klein des Gro\u00dfen Ursprung sei,", "tokens": ["Du", "legst", "nur", ",", "da\u00b7mit", "klein", "des", "Gro\u00b7\u00dfen", "Ur\u00b7sprung", "sei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Namen eines Stroms dem winz'gen Rinsal bei.", "tokens": ["Den", "Na\u00b7men", "ei\u00b7nes", "Stroms", "dem", "winz'\u00b7gen", "Rin\u00b7sal", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}