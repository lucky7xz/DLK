{"textgrid.poem.40912": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00dcberraschung im Garten", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwer spr\u00fczt mer alli Fr\u00fceih mi Rosmeri?", "tokens": ["\u00bb", "wer", "spr\u00fczt", "mer", "al\u00b7li", "Fr\u00fc\u00b7eih", "mi", "Ros\u00b7me\u00b7ri", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ADV", "PIAT", "NN", "NE", "NE", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Es cha doch nit der Tau vom Himmel si;", "tokens": ["Es", "cha", "doch", "nit", "der", "Tau", "vom", "Him\u00b7mel", "si", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADV", "PTKNEG", "ART", "NN", "APPRART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sust h\u00e4tt der Mangeld au si Sach,", "tokens": ["sust", "h\u00e4tt", "der", "Man\u00b7geld", "au", "si", "Sach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er stoht doch au nit unterm Dach.", "tokens": ["er", "stoht", "doch", "au", "nit", "un\u00b7term", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer spr\u00fczt mer alle Fr\u00fceih mi Rosmeri?", "tokens": ["Wer", "spr\u00fczt", "mer", "al\u00b7le", "Fr\u00fc\u00b7eih", "mi", "Ros\u00b7me\u00b7ri", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PIAT", "NN", "NE", "NE", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und wenn i no so fr\u00fceih ins G\u00e4rtli spring,", "tokens": ["Und", "wenn", "i", "no", "so", "fr\u00fc\u00b7eih", "ins", "G\u00e4rt\u00b7li", "spring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "NE", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "und unterwegs mi Morgeliedli sing,", "tokens": ["und", "un\u00b7ter\u00b7wegs", "mi", "Mor\u00b7ge\u00b7lied\u00b7li", "sing", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "isch n\u00e4umis gschafft. Wie st\u00f6hn jez reihewis", "tokens": ["isch", "n\u00e4u\u00b7mis", "gschafft", ".", "Wie", "st\u00f6hn", "jez", "rei\u00b7he\u00b7wis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "$.", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "die Erbse wieder do am schlanke Ris", "tokens": ["die", "Erb\u00b7se", "wie\u00b7der", "do", "am", "schlan\u00b7ke", "Ris"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "in ihrem Bluest! I chumm nit us dem Ding.", "tokens": ["in", "ih\u00b7rem", "Blu\u00b7est", "!", "I", "chumm", "nit", "us", "dem", "Ding", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "XY", "XY", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Was gilt's, es sin die Jumpferen usem See!", "tokens": ["Was", "gilt's", ",", "es", "sin", "die", "Jump\u00b7fe\u00b7ren", "u\u00b7sem", "See", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Me meint zwar, 's ch\u00f6m, wie lang scho, keini meh.", "tokens": ["Me", "meint", "zwar", ",", "'s", "ch\u00f6m", ",", "wie", "lang", "scho", ",", "kei\u00b7ni", "meh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PPER", "NE", "$,", "PWAV", "ADJD", "NE", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Sust sin sie in der Mitternacht,", "tokens": ["Sust", "sin", "sie", "in", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "wenn niemes meh as d'Sterne wacht,", "tokens": ["wenn", "nie\u00b7mes", "meh", "as", "d'\u00b7S\u00b7ter\u00b7ne", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "in d'Felder use gwandelt usem See.", "tokens": ["in", "d'\u00b7Fel\u00b7der", "u\u00b7se", "gwan\u00b7delt", "u\u00b7sem", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.16": {"text": "Sie hen im Feld, sie hen mit frummer Hand", "tokens": ["Sie", "hen", "im", "Feld", ",", "sie", "hen", "mit", "frum\u00b7mer", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "de brave L\u00fcte gschafft im Garteland,", "tokens": ["de", "bra\u00b7ve", "L\u00fc\u00b7te", "gschafft", "im", "Gar\u00b7te\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "und isch me fr\u00fceih im Morgeschimmer cho,", "tokens": ["und", "isch", "me", "fr\u00fc\u00b7eih", "im", "Mor\u00b7ge\u00b7schim\u00b7mer", "cho", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADJD", "APPRART", "NN", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "und het jez wellen an si Arbet go,", "tokens": ["und", "het", "jez", "wel\u00b7len", "an", "si", "Ar\u00b7bet", "go", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "APPR", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "isch alles fertig gsi \u2013 und wie scharmant!", "tokens": ["isch", "al\u00b7les", "fer\u00b7tig", "gsi", "\u2013", "und", "wie", "schar\u00b7mant", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "ADJD", "NE", "$(", "KON", "PWAV", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Du Schalk d\u00f6rt hinte, meinsch i seh di nit?", "tokens": ["Du", "Schalk", "d\u00f6rt", "hin\u00b7te", ",", "meinsch", "i", "seh", "di", "nit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "VVFIN", "$,", "FM", "FM", "FM", "FM", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Jo, duck di numme nieder, wie de witt!", "tokens": ["Jo", ",", "duck", "di", "num\u00b7me", "nie\u00b7der", ",", "wie", "de", "witt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "FM", "FM", "PTKVZ", "$,", "PWAV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "I ha mer's vorgstellt, du w\u00fcrsch's si.", "tokens": ["I", "ha", "mer's", "vorgs\u00b7tellt", ",", "du", "w\u00fcr\u00b7sch's", "si", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVPP", "$,", "PPER", "VAFIN", "NE", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.24": {"text": "Was falleder f\u00fcr Jesten i? \u2013", "tokens": ["Was", "fal\u00b7le\u00b7der", "f\u00fcr", "Jes\u00b7ten", "i", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADV", "APPR", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "O lueg, vertritt mer mini Sezlig nit!\u00ab \u2013", "tokens": ["O", "lu\u00b7eg", ",", "ver\u00b7tritt", "mer", "mi\u00b7ni", "Sez\u00b7lig", "nit", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "ADV", "NE", "NE", "PTKNEG", "$.", "$(", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.26": {"text": "\u00bbo K\u00e4tterli, de hesch's nit solle seh!", "tokens": ["\u00bb", "o", "K\u00e4t\u00b7ter\u00b7li", ",", "de", "he\u00b7sch's", "nit", "sol\u00b7le", "seh", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "NE", "NE", "PTKNEG", "VMFIN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Jo, dine Blume hani z'trinke ge,", "tokens": ["Jo", ",", "di\u00b7ne", "Blu\u00b7me", "ha\u00b7ni", "z'\u00b7trin\u00b7ke", "ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "und wenn de wotsch, i gieng f\u00fcr di dur's F\u00fc\u00fcr", "tokens": ["und", "wenn", "de", "wot\u00b7sch", ",", "i", "gieng", "f\u00fcr", "di", "dur's", "F\u00fc\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NE", "ADJD", "$,", "NE", "VVFIN", "APPR", "NE", "APPRART", "NN"], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "und um mi Lebe w\u00e4r mer dis nit z't\u00fc\u00fcr", "tokens": ["und", "um", "mi", "Le\u00b7be", "w\u00e4r", "mer", "dis", "nit", "z'\u00b7t\u00fc\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN", "VAFIN", "ADV", "PDS", "PTKNEG", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "und 's isch mer, o gar s\u00f6lli wohl und weh.\u00ab", "tokens": ["und", "'s", "isch", "mer", ",", "o", "gar", "s\u00f6l\u00b7li", "wohl", "und", "weh", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "ADJD", "ADV", "$,", "FM", "FM", "FM", "ADV", "KON", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.31": {"text": "So het zum K\u00e4tterli der Friedli gseit;", "tokens": ["So", "het", "zum", "K\u00e4t\u00b7ter\u00b7li", "der", "Fried\u00b7li", "gseit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NE", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.32": {"text": "er het e schweri Lieb im Herze treit,", "tokens": ["er", "het", "e", "schwe\u00b7ri", "Lieb", "im", "Her\u00b7ze", "treit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "FM", "FM", "NN", "APPRART", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "und het's nit ch\u00f6nne sage just,", "tokens": ["und", "het's", "nit", "ch\u00f6n\u00b7ne", "sa\u00b7ge", "just", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PTKNEG", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "und es het au in siner Brust", "tokens": ["und", "es", "het", "au", "in", "si\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "e sch\u00fc\u00fcchi zarti Lieb zum Friedli treit.", "tokens": ["e", "sch\u00fc\u00fc\u00b7chi", "zar\u00b7ti", "Lieb", "zum", "Fried\u00b7li", "treit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "\u00bblueg, Friedli, mini sch\u00f6ne Bl\u00fcemli a!", "tokens": ["\u00bb", "lu\u00b7eg", ",", "Fried\u00b7li", ",", "mi\u00b7ni", "sch\u00f6\u00b7ne", "Bl\u00fcem\u00b7li", "a", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,"