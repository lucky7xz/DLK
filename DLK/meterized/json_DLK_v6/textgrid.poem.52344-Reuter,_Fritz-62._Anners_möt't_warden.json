{"textgrid.poem.52344": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "62. Anners m\u00f6t't warden", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbne, Schr\u00f6der, 't geiht nich!\u00ab seggt de Schult,", "tokens": ["\u00bb", "ne", ",", "Schr\u00f6\u00b7der", ",", "'t", "geiht", "nich", "!", "\u00ab", "seggt", "de", "Schult", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NN", "$,", "NE", "VVFIN", "PTKNEG", "$.", "$(", "VVFIN", "NE", "NE", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "\u00bbne, Vadder, gl\u00f6w du mi,", "tokens": ["\u00bb", "ne", ",", "Vad\u00b7der", ",", "gl\u00f6w", "du", "mi", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$,", "VVFIN", "PPER", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn so en jeder reden wullt,", "tokens": ["Wenn", "so", "en", "je\u00b7der", "re\u00b7den", "wullt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVFIN", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn wir't all l\u00e4ngst v\u00f6rbi.", "tokens": ["Denn", "wir't", "all", "l\u00e4ngst", "v\u00f6r\u00b7bi", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADV", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "De Obrig- un de Geistlichkeit,", "tokens": ["De", "Ob\u00b7rig", "un", "de", "Geist\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "FM", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De m\u00f6st du stets spektieren,", "tokens": ["De", "m\u00f6st", "du", "stets", "spek\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Un mit din oll Rebelligkeit", "tokens": ["Un", "mit", "din", "oll", "Re\u00b7bel\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "ADJD", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Deihst di blot rungenieren.", "tokens": ["Deihst", "di", "blot", "run\u00b7ge\u00b7nie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "\u203aschult\u2039, s\u00e4d uns' Herr Pastur tau mi,", "tokens": ["\u203a", "schult", "\u2039", ",", "s\u00e4d", "un\u00b7s'", "Herr", "Pas\u00b7tur", "tau", "mi", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "\u203awir m\u00fcssen's auferhalten", "tokens": ["\u203a", "wir", "m\u00fcs\u00b7sen's", "au\u00b7fer\u00b7hal\u00b7ten"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Un gegen Demokrateri", "tokens": ["Un", "ge\u00b7gen", "De\u00b7mo\u00b7kra\u00b7te\u00b7ri"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NE"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.4": {"text": "Aufstehen for dem Alten;", "tokens": ["Auf\u00b7ste\u00b7hen", "for", "dem", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "'ne St\u00e4rkung for Regierung sein", "tokens": ["'ne", "St\u00e4r\u00b7kung", "for", "Re\u00b7gie\u00b7rung", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un for den hohen Adel:", "tokens": ["Un", "for", "den", "ho\u00b7hen", "A\u00b7del", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Un nie un nie nich fall uns ein,", "tokens": ["Un", "nie", "un", "nie", "nich", "fall", "uns", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "FM", "ADV", "PTKNEG", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Zu pr\u00e4tendieren Tadel.\u2039", "tokens": ["Zu", "pr\u00e4\u00b7ten\u00b7die\u00b7ren", "Ta\u00b7del", ".", "\u2039"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u203aherr Paster\u2039, s\u00e4d ick, \u203aHerr Pastur,", "tokens": ["\u203a", "herr", "Pas\u00b7ter", "\u2039", ",", "s\u00e4d", "ick", ",", "\u203a", "Herr", "Pas\u00b7tur", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$(", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ick mit min Fru un Kinner", "tokens": ["Ick", "mit", "min", "Fru", "un", "Kin\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "FM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Un mit min ganzes Infentur,", "tokens": ["Un", "mit", "min", "gan\u00b7zes", "In\u00b7fen\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wi meinen't so nich minner.\u2039", "tokens": ["Wi", "mei\u00b7nen't", "so", "nich", "min\u00b7ner", ".", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PTKNEG", "ADJA", "$.", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "So m\u00f6t dat bliwen, m\u00f6t dat sin,", "tokens": ["So", "m\u00f6t", "dat", "bli\u00b7wen", ",", "m\u00f6t", "dat", "sin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "NN", "$,", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "S\u00fcs kann't nich assistieren!\u00ab", "tokens": ["S\u00fcs", "kann't", "nich", "as\u00b7sis\u00b7tie\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Un bost sick in 'ne Wut herin", "tokens": ["Un", "bost", "sick", "in", "'ne", "Wut", "he\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Un spuckt un deiht handtieren.", "tokens": ["Un", "spuckt", "un", "deiht", "hand\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Un kikt oll Schr\u00f6dern gnittig an,", "tokens": ["Un", "kikt", "oll", "Schr\u00f6\u00b7dern", "gnit\u00b7tig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "As k\u00fcnn hei'n gliksten w\u00f6rgen,", "tokens": ["As", "k\u00fcnn", "hei'n", "gliks\u00b7ten", "w\u00f6r\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "As wir all rip sin Vaddermann", "tokens": ["As", "wir", "all", "rip", "sin", "Vad\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "PIAT", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00f6r D\u00e4ms un f\u00f6r Dreibergen.", "tokens": ["F\u00f6r", "D\u00e4ms", "un", "f\u00f6r", "Drei\u00b7ber\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Dunn k\u00fcmmt herin sin Ossenknecht:", "tokens": ["Dunn", "k\u00fcmmt", "he\u00b7rin", "sin", "Os\u00b7sen\u00b7knecht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbschult, unsen sch\u00f6nen Weiten,", "tokens": ["\u00bb", "schult", ",", "un\u00b7sen", "sch\u00f6\u00b7nen", "Wei\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den'n heww'n de wilden Swin mal recht", "tokens": ["Den'n", "hew\u00b7w'n", "de", "wil\u00b7den", "Swin", "mal", "recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ADJA", "NE", "ADV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dalrangt \u2013 'ne Schan'n tau heiten!\u00ab", "tokens": ["Dal\u00b7rangt", "\u2013", "'ne", "Schan'n", "tau", "hei\u00b7ten", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$(", "ART", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbwat?\u00ab r\u00f6ppt de Schult, \u00bbde wilden Swin?", "tokens": ["\u00bb", "wat", "?", "\u00ab", "r\u00f6ppt", "de", "Schult", ",", "\u00bb", "de", "wil\u00b7den", "Swin", "?"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "$.", "$(", "VVFIN", "NE", "NE", "$,", "$(", "NE", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dat m\u00f6t de D\u00fcwel halen!", "tokens": ["Dat", "m\u00f6t", "de", "D\u00fc\u00b7wel", "ha\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Un dat will 'ne Regierung sin?", "tokens": ["Un", "dat", "will", "'ne", "Re\u00b7gie\u00b7rung", "sin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dorf\u00f6r s\u00e4l'n wi betahlen?", "tokens": ["Dor\u00b7f\u00f6r", "s\u00e4l'n", "wi", "be\u00b7tah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ick will doch glik taum Preister hen", "tokens": ["Ick", "will", "doch", "glik", "taum", "Preis\u00b7ter", "hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "ADV", "NN", "VVINF"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Un will den Preister fragen \u2013", "tokens": ["Un", "will", "den", "Preis\u00b7ter", "fra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Min sch\u00f6ne Weiten as 'ne Denn!", "tokens": ["Min", "sch\u00f6\u00b7ne", "Wei\u00b7ten", "as", "'ne", "Denn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "FM", "FM", "KON", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ick will dat Amt verklagen!\u00ab", "tokens": ["Ick", "will", "dat", "Amt", "ver\u00b7kla\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "De Schult, de l\u00f6ppt, bald is hei t'r\u00fcgg", "tokens": ["De", "Schult", ",", "de", "l\u00f6ppt", ",", "bald", "is", "hei", "t'\u00b7r\u00fcgg"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "$,", "ADV", "FM", "FM", "FM"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Un kratzt sick achter't Uhr,", "tokens": ["Un", "kratzt", "sick", "ach\u00b7ter't", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "CARD", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Oll Schr\u00f6der grint: \u00bbGevadder, segg,", "tokens": ["Oll", "Schr\u00f6\u00b7der", "grint", ":", "\u00bb", "Ge\u00b7vad\u00b7der", ",", "segg", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "$(", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wat s\u00e4d de Herr Pastur?\u00ab", "tokens": ["Wat", "s\u00e4d", "de", "Herr", "Pas\u00b7tur", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "\u00bbgevadder\u00ab, seggt de Schult un kratzt", "tokens": ["\u00bb", "ge\u00b7vad\u00b7der", "\u00ab", ",", "seggt", "de", "Schult", "un", "kratzt"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "$(", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch d\u00fcller as v\u00f6rher,", "tokens": ["Noch", "d\u00fcl\u00b7ler", "as", "v\u00f6r\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbbi den'n is ok wat rinner platzt,", "tokens": ["\u00bb", "bi", "den'n", "is", "ok", "wat", "rin\u00b7ner", "platzt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kamm ok wat in de Quer:", "tokens": ["Kamm", "ok", "wat", "in", "de", "Quer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "De Eddelmann m\u00f6t alle Johr", "tokens": ["De", "Ed\u00b7del\u00b7mann", "m\u00f6t", "al\u00b7le", "Johr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein fettes Swin em bringen,", "tokens": ["Ein", "fet\u00b7tes", "Swin", "em", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So'n richtig Hauptswin, grot un swor", "tokens": ["So'n", "rich\u00b7tig", "Haupts\u00b7win", ",", "grot", "un", "swor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$,", "ADJD", "FM", "FM"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Un fett v\u00f6r allen Dingen.", "tokens": ["Un", "fett", "v\u00f6r", "al\u00b7len", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wat deiht nu uns' gaud Eddelmann?", "tokens": ["Wat", "deiht", "nu", "un\u00b7s'", "gaud", "Ed\u00b7del\u00b7mann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hei schickt 'ne olle S\u00e4g,", "tokens": ["Hei", "schickt", "'ne", "ol\u00b7le", "S\u00e4g", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "De't Fauder nich 