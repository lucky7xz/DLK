{"dta.poem.18151": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ratur und Schrifft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Natur und GOttes Wort, sind zwo", "tokens": ["Die", "Na\u00b7tur", "und", "Got\u00b7tes", "Wort", ",", "sind", "zwo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,", "VAFIN", "CARD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "reiche Labsahls Quellen,", "tokens": ["rei\u00b7che", "Lab\u00b7sahls", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wo mit fetten Ueberflus, lauter Le-", "tokens": ["Wo", "mit", "fet\u00b7ten", "Ue\u00b7ber\u00b7flus", ",", "lau\u00b7ter", "Le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,", "PIAT", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "benstropfen schwellen:", "tokens": ["ben\u00b7strop\u00b7fen", "schwel\u00b7len", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Jene gleicht den Jacobs Brunn \u2020 der", "tokens": ["Je\u00b7ne", "gleicht", "den", "Ja\u00b7cobs", "Brunn", "\u2020", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NE", "NE", "$(", "ART"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "den Durst nicht immer stillt,", "tokens": ["den", "Durst", "nicht", "im\u00b7mer", "stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Diese s\u00e4ttigt weil ihr Safft, aus dem Baum des", "tokens": ["Die\u00b7se", "s\u00e4t\u00b7tigt", "weil", "ihr", "Safft", ",", "aus", "dem", "Baum", "des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOUS", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "ART"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Lebens quillt;", "tokens": ["Le\u00b7bens", "quillt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Brauche beides Sterblicher! irdisch himm-", "tokens": ["Brau\u00b7che", "bei\u00b7des", "Sterb\u00b7li\u00b7cher", "!", "ir\u00b7disch", "him\u00b7m"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PIAT", "NN", "$.", "ADJD", "TRUNC"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.10": {"text": "lisches Vergn\u00fcgen,", "tokens": ["li\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Jenes siehst du in der Welt, dieses in der\nBibel liegen.", "tokens": ["Je\u00b7nes", "siehst", "du", "in", "der", "Welt", ",", "die\u00b7ses", "in", "der", "Bi\u00b7bel", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PDAT", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}}}}}