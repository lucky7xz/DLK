{"textgrid.poem.53517": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Kolonne", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hochr\u00e4drige, \u00fcberdachte Wagen,", "tokens": ["Hoch\u00b7r\u00e4d\u00b7ri\u00b7ge", ",", "\u00fc\u00b7berd\u00b7ach\u00b7te", "Wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Immer einer hinter dem andern.", "tokens": ["Im\u00b7mer", "ei\u00b7ner", "hin\u00b7ter", "dem", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "ART", "ADJA", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der Regen rieselt. Sie fahren seit Tagen,", "tokens": ["Der", "Re\u00b7gen", "rie\u00b7selt", ".", "Sie", "fah\u00b7ren", "seit", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Seit Wochen, im Schritt, ein endloses Wandern.", "tokens": ["Seit", "Wo\u00b7chen", ",", "im", "Schritt", ",", "ein", "end\u00b7lo\u00b7ses", "Wan\u00b7dern", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Fahrer dr\u00f6seln auf ihren B\u00f6cken,", "tokens": ["Die", "Fah\u00b7rer", "dr\u00f6\u00b7seln", "auf", "ih\u00b7ren", "B\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vorne im Halbschlaf der Herr Sergeant;", "tokens": ["Vor\u00b7ne", "im", "Halb\u00b7schlaf", "der", "Herr", "Ser\u00b7ge\u00b7ant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "NE", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Das Wasser rinnt an den schweren R\u00f6cken", "tokens": ["Das", "Was\u00b7ser", "rinnt", "an", "den", "schwe\u00b7ren", "R\u00f6\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Herunter \u2013 grau und glatt liegt das Land \u2013 \u2013", "tokens": ["Her\u00b7un\u00b7ter", "\u2013", "grau", "und", "glatt", "liegt", "das", "Land", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$(", "ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$(", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Der Fahrer tr\u00e4umt auf seinem Sitze,", "tokens": ["Der", "Fah\u00b7rer", "tr\u00e4umt", "auf", "sei\u00b7nem", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nur manchmal schreckt ein Rufen den Mann.", "tokens": ["Nur", "manch\u00b7mal", "schreckt", "ein", "Ru\u00b7fen", "den", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Ein Ruf pflanzt sich fort von hinten zur Spitze:", "tokens": ["Ein", "Ruf", "pflanzt", "sich", "fort", "von", "hin\u00b7ten", "zur", "Spit\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKVZ", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "\u00bbrechts ran!\u00ab", "tokens": ["\u00bb", "rechts", "ran", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Ein Auto braust. Vorbei. Sie sinnen", "tokens": ["Ein", "Au\u00b7to", "braust", ".", "Vor\u00b7bei", ".", "Sie", "sin\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und tr\u00e4umen wieder im gleichen Trott.", "tokens": ["Und", "tr\u00e4u\u00b7men", "wie\u00b7der", "im", "glei\u00b7chen", "Trott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie wird das drau\u00dfen? Wie wird das drinnen?", "tokens": ["Wie", "wird", "das", "drau\u00b7\u00dfen", "?", "Wie", "wird", "das", "drin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PDS", "ADV", "$.", "PWAV", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Friede? Wandlung? Du lieber Gott!", "tokens": ["Frie\u00b7de", "?", "Wand\u00b7lung", "?", "Du", "lie\u00b7ber", "Gott", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "ADV", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "So lange geschm\u00e4ht \u2013 jetzt steht es kritisch \u2013", "tokens": ["So", "lan\u00b7ge", "ge\u00b7schm\u00e4ht", "\u2013", "jetzt", "steht", "es", "kri\u00b7tisch", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$(", "ADV", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Rote war stets ein schwarzer Mann \u2013", "tokens": ["Der", "Ro\u00b7te", "war", "stets", "ein", "schwar\u00b7zer", "Mann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "J\u00e4h f\u00e4hrt er auf. Wie klingt das politisch:", "tokens": ["J\u00e4h", "f\u00e4hrt", "er", "auf", ".", "Wie", "klingt", "das", "po\u00b7li\u00b7tisch", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VVFIN", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbrechts ran!\u00ab", "tokens": ["\u00bb", "rechts", "ran", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "PTKVZ", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Wird sich das \u00e4ndern? In neuen Bahnen?", "tokens": ["Wird", "sich", "das", "\u00e4n\u00b7dern", "?", "In", "neu\u00b7en", "Bah\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PDS", "VVINF", "$.", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Es w\u00e4re die allerh\u00f6chste Zeit.", "tokens": ["Es", "w\u00e4\u00b7re", "die", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nicht mehr: Obrigkeit, Untertanen,", "tokens": ["Nicht", "mehr", ":", "Ob\u00b7rig\u00b7keit", ",", "Un\u00b7ter\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "NN", "$,", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nur noch Deutsche \u2013 im gleichen Leid.", "tokens": ["Nur", "noch", "Deut\u00b7sche", "\u2013", "im", "glei\u00b7chen", "Leid", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$(", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Die Pferde poltern ein wenig geschwinder,", "tokens": ["Die", "Pfer\u00b7de", "pol\u00b7tern", "ein", "we\u00b7nig", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIAT", "ADJA", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sein nasses Gesicht zieht sich lustig in Falten:", "tokens": ["Sein", "nas\u00b7ses", "Ge\u00b7sicht", "zieht", "sich", "lus\u00b7tig", "in", "Fal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Nur noch Landsleute \u2013 und die Kinder", "tokens": ["Nur", "noch", "Lands\u00b7leu\u00b7te", "\u2013", "und", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Habens besser als die Alten.", "tokens": ["Ha\u00b7bens", "bes\u00b7ser", "als", "die", "Al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Neue Zeilen und neue Besen \u2013", "tokens": ["Neu\u00b7e", "Zei\u00b7len", "und", "neu\u00b7e", "Be\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Besser, als er es je haben kann . . .", "tokens": ["Bes\u00b7ser", ",", "als", "er", "es", "je", "ha\u00b7ben", "kann", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "$,", "KOUS", "PPER", "PPER", "ADV", "VAINF", "VMFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "So ist es denn nicht umsonst gewesen:", "tokens": ["So", "ist", "es", "denn", "nicht", "um\u00b7sonst", "ge\u00b7we\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VAPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}