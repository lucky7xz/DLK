{"textgrid.poem.65792": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "66", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcde von alle ", "tokens": ["M\u00fc\u00b7de", "von", "al\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PIAT"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verdienst zum Bettler sehn geboren werden,", "tokens": ["Ver\u00b7dienst", "zum", "Bett\u00b7ler", "sehn", "ge\u00b7bo\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hohle D\u00fcrftigkeit in Gr\u00fcn und Rot,", "tokens": ["Und", "hoh\u00b7le", "D\u00fcrf\u00b7tig\u00b7keit", "in", "Gr\u00fcn", "und", "Rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie sich reinste Treu entf\u00e4rbt auf Erden,", "tokens": ["Und", "wie", "sich", "reins\u00b7te", "Treu", "ent\u00b7f\u00e4rbt", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und goldnen Ehrenschmuck auf Knechteshaupt,", "tokens": ["Und", "gold\u00b7nen", "Eh\u00b7ren\u00b7schmuck", "auf", "Knech\u00b7tes\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und jungfr\u00e4uliche Tugend frech gesch\u00e4ndet,", "tokens": ["Und", "jung\u00b7fr\u00e4u\u00b7li\u00b7che", "Tu\u00b7gend", "frech", "ge\u00b7sch\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und Hoheit ihres Herrschertums beraubt,", "tokens": ["Und", "Ho\u00b7heit", "ih\u00b7res", "Herr\u00b7scher\u00b7tums", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Kraft an lahmes Regiment verschwendet,", "tokens": ["Und", "Kraft", "an", "lah\u00b7mes", "Re\u00b7gi\u00b7ment", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und Kunst im Zungenbande der Gewalt,", "tokens": ["Und", "Kunst", "im", "Zun\u00b7gen\u00b7ban\u00b7de", "der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und Schulenunsinn, der Vernunft entgeistert,", "tokens": ["Und", "Schu\u00b7len\u00b7un\u00b7sinn", ",", "der", "Ver\u00b7nunft", "ent\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und schlichte Wahrheit, die man Einfalt schalt,", "tokens": ["Und", "schlich\u00b7te", "Wahr\u00b7heit", ",", "die", "man", "Ein\u00b7falt", "schalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und wie vom B\u00f6sen Gutes wird gemeistert:", "tokens": ["Und", "wie", "vom", "B\u00f6\u00b7sen", "Gu\u00b7tes", "wird", "ge\u00b7meis\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "M\u00fcde von alle dem, w\u00e4r Tod mir s\u00fc\u00df;", "tokens": ["M\u00fc\u00b7de", "von", "al\u00b7le", "dem", ",", "w\u00e4r", "Tod", "mir", "s\u00fc\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "ART", "$,", "VAFIN", "NN", "PPER", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Nur, da\u00df ich sterbend den Geliebten lie\u00df!", "tokens": ["Nur", ",", "da\u00df", "ich", "ster\u00b7bend", "den", "Ge\u00b7lieb\u00b7ten", "lie\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}