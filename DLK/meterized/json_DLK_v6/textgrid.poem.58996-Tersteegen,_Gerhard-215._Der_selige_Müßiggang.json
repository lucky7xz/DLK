{"textgrid.poem.58996": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "215. Der selige M\u00fc\u00dfiggang", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer voraus viel \u00fcberdenket,", "tokens": ["Wer", "vo\u00b7raus", "viel", "\u00fc\u00b7ber\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der hat immer viel zu tun;", "tokens": ["Der", "hat", "im\u00b7mer", "viel", "zu", "tun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ins stille Nun sich senket,", "tokens": ["Wer", "ins", "stil\u00b7le", "Nun", "sich", "sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann in Gott stets m\u00fc\u00dfig ruhn.", "tokens": ["Kann", "in", "Gott", "stets", "m\u00fc\u00b7\u00dfig", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}