{"dta.poem.18556": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "A ls sie seine  V erpflichtungen  \n Flaterien nannte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Die Englische Gestalt der sch\u00f6nen Liesimenen/", "tokens": ["Die", "Eng\u00b7li\u00b7sche", "Ge\u00b7stalt", "der", "sch\u00f6\u00b7nen", "Lie\u00b7si\u00b7me\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht auch ein Felsen-Hertz den leichten Zunder gleich.", "tokens": ["Macht", "auch", "ein", "Fel\u00b7sen\u00b7Hertz", "den", "leich\u00b7ten", "Zun\u00b7der", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Tugenden/ die sie mit tausend Lorbern kr\u00f6nen/", "tokens": ["Die", "Tu\u00b7gen\u00b7den", "/", "die", "sie", "mit", "tau\u00b7send", "Lor\u00b7bern", "kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind fast bey aller Welt an Ehrerbiethung reich.", "tokens": ["Sind", "fast", "bey", "al\u00b7ler", "Welt", "an", "Ehr\u00b7er\u00b7bie\u00b7thung", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und dennoch soll mein Hertz bey ihren Augen-Strahlen/", "tokens": ["Und", "den\u00b7noch", "soll", "mein", "Hertz", "bey", "ih\u00b7ren", "Au\u00b7gen\u00b7Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie Salamander kalt in heissen Flammen seyn.", "tokens": ["Wie", "Sa\u00b7la\u00b7man\u00b7der", "kalt", "in", "heis\u00b7sen", "Flam\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Will mein verliebter Kiel der Sch\u00f6nheit W\u00fcrckung mahlen/", "tokens": ["Will", "mein", "ver\u00b7lieb\u00b7ter", "Kiel", "der", "Sch\u00f6n\u00b7heit", "W\u00fcr\u00b7ckung", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So spricht sie/ dieses ist nur leerer Worte Schein.", "tokens": ["So", "spricht", "sie", "/", "die\u00b7ses", "ist", "nur", "lee\u00b7rer", "Wor\u00b7te", "Schein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PDS", "VAFIN", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O nein du art'ger Freund ich kenne hier dein Schertzen/", "tokens": ["O", "nein", "du", "art'\u00b7ger", "Freund", "ich", "ken\u00b7ne", "hier", "dein", "Schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "PPER", "ADJA", "NN", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und da\u00df du weit entfernt mit deinen Hertzen bist.", "tokens": ["Und", "da\u00df", "du", "weit", "ent\u00b7fernt", "mit", "dei\u00b7nen", "Hert\u00b7zen", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich sage/ Sch\u00f6nste/ ja/ ich schreibe sonder Hertzen/", "tokens": ["Ich", "sa\u00b7ge", "/", "Sch\u00f6ns\u00b7te", "/", "ja", "/", "ich", "schrei\u00b7be", "son\u00b7der", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NN", "$(", "PTKANT", "$(", "PPER", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Indem dasselbige bey Liesiemenen ist.", "tokens": ["In\u00b7dem", "das\u00b7sel\u00b7bi\u00b7ge", "bey", "Lie\u00b7sie\u00b7me\u00b7nen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}