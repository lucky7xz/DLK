{"dta.poem.12960": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "U bersetzungen  \n Aus des   Thom\u00e6 a Kempis   buche  \n Von der nachfolge Christi.  \n Des ersten buches  \n Erstes capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl diesem, der mir folgt, und mein gof\u00fchrtes leben", "tokens": ["Wohl", "die\u00b7sem", ",", "der", "mir", "folgt", ",", "und", "mein", "go\u00b7f\u00fchr\u00b7tes", "le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor eine regel h\u00e4lt, nach der man wandeln mu\u00df:", "tokens": ["Vor", "ei\u00b7ne", "re\u00b7gel", "h\u00e4lt", ",", "nach", "der", "man", "wan\u00b7deln", "mu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "PIS", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer licht und himmel sucht, der fasse den entschlu\u00df,", "tokens": ["Wer", "licht", "und", "him\u00b7mel", "sucht", ",", "der", "fas\u00b7se", "den", "ent\u00b7schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sich auf die dunckle bahn des creutzes zu begeben.", "tokens": ["Sich", "auf", "die", "dunck\u00b7le", "bahn", "des", "creut\u00b7zes", "zu", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sprach des Vaters Wort, das Wort, das weder flei\u00df,", "tokens": ["So", "sprach", "des", "Va\u00b7ters", "Wort", ",", "das", "Wort", ",", "das", "we\u00b7der", "flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "PRELS", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch englischer verstand recht auszudr\u00fccken wei\u00df.", "tokens": ["Noch", "eng\u00b7li\u00b7scher", "ver\u00b7stand", "recht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADJD", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was sind der heiligen geheimni\u00df-volle lehren,", "tokens": ["Was", "sind", "der", "hei\u00b7li\u00b7gen", "ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7le", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als tropffen aus der see, die JEsus von sich geu\u00dft?", "tokens": ["Als", "tropf\u00b7fen", "aus", "der", "see", ",", "die", "Je\u00b7sus", "von", "sich", "geu\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$,", "PRELS", "NE", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er ist der reine brunn, aus dem das leben fleu\u00dft.", "tokens": ["Er", "ist", "der", "rei\u00b7ne", "brunn", ",", "aus", "dem", "das", "le\u00b7ben", "fleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PDS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer lautre wahrheit sucht, der darff nur JEsum h\u00f6ren,", "tokens": ["Wer", "laut\u00b7re", "wahr\u00b7heit", "sucht", ",", "der", "darff", "nur", "Je\u00b7sum", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$,", "PRELS", "VMFIN", "ADV", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und auf die wege sehn, die er gegangen ist,", "tokens": ["Und", "auf", "die", "we\u00b7ge", "sehn", ",", "die", "er", "ge\u00b7gan\u00b7gen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So hat er ihm gewi\u00df das rechte licht erkiest.", "tokens": ["So", "hat", "er", "ihm", "ge\u00b7wi\u00df", "das", "rech\u00b7te", "licht", "er\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch must du seinen Geist dich stets regieren lassen;", "tokens": ["Doch", "must", "du", "sei\u00b7nen", "Geist", "dich", "stets", "re\u00b7gie\u00b7ren", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst wird das himmel-brodt vor dir verborgen seyn.", "tokens": ["Sonst", "wird", "das", "him\u00b7mel\u00b7brodt", "vor", "dir", "ver\u00b7bor\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn mancher l\u00e4st das wort zu beyden ohren ein,", "tokens": ["Denn", "man\u00b7cher", "l\u00e4st", "das", "wort", "zu", "bey\u00b7den", "oh\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wei\u00df doch seine krafft nicht von sich selbst zu fassen.", "tokens": ["Und", "wei\u00df", "doch", "sei\u00b7ne", "krafft", "nicht", "von", "sich", "selbst", "zu", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "APPR", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum wer die wahrheit liebt und nach erleuchtung strebt,", "tokens": ["Drum", "wer", "die", "wahr\u00b7heit", "liebt", "und", "nach", "er\u00b7leuch\u00b7tung", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "ART", "NN", "VVFIN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der lebe, wie der HErr, das Leben selbst, gelebt.", "tokens": ["Der", "le\u00b7be", ",", "wie", "der", "Herr", ",", "das", "Le\u00b7ben", "selbst", ",", "ge\u00b7lebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "ART", "NN", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was hilfft es, wenn wir gleich best\u00e4ndig disputiren,", "tokens": ["Was", "hilfft", "es", ",", "wenn", "wir", "gleich", "be\u00b7st\u00e4n\u00b7dig", "dis\u00b7pu\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob, warum, und wie GOtt dreyeinig ist und heist?", "tokens": ["Ob", ",", "wa\u00b7rum", ",", "und", "wie", "Gott", "drey\u00b7ei\u00b7nig", "ist", "und", "heist", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "$,", "KON", "PWAV", "NN", "ADJD", "VAFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn unser hertze sich der demuth nicht beflei\u00dft,", "tokens": ["Wenn", "un\u00b7ser", "hert\u00b7ze", "sich", "der", "de\u00b7muth", "nicht", "be\u00b7flei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ohn welche wir umsonst den Christen-namen f\u00fchren.", "tokens": ["Ohn", "wel\u00b7che", "wir", "um\u00b7sonst", "den", "Chris\u00b7ten\u00b7na\u00b7men", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn Christus lehret uns: da\u00df zanck und stoltz, der welt,", "tokens": ["Denn", "Chris\u00b7tus", "leh\u00b7ret", "uns", ":", "da\u00df", "zanck", "und", "stoltz", ",", "der", "welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "$.", "KOUS", "NN", "KON", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dem Drey-Einigen nur demuth wohl gef\u00e4llt.", "tokens": ["Und", "dem", "Drey\u00b7Ei\u00b7ni\u00b7gen", "nur", "de\u00b7muth", "wohl", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Der ausdruck hoher wort und spitziger gedancken", "tokens": ["Der", "aus\u00b7druck", "ho\u00b7her", "wort", "und", "spit\u00b7zi\u00b7ger", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht keinen Heiligen und keinen Christen nicht.", "tokens": ["Macht", "kei\u00b7nen", "Hei\u00b7li\u00b7gen", "und", "kei\u00b7nen", "Chris\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gott sieht mehr, was man thut, als was die lippe spricht.", "tokens": ["Gott", "sieht", "mehr", ",", "was", "man", "thut", ",", "als", "was", "die", "lip\u00b7pe", "spricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "PRELS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer mit der s\u00fcnde k\u00e4mpfft, und in den tugend-schrancken", "tokens": ["Wer", "mit", "der", "s\u00fcn\u00b7de", "k\u00e4mpfft", ",", "und", "in", "den", "tu\u00b7gen\u00b7dschran\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "VVFIN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den fieges-prei\u00df gewinnt, und GOtt die ehre giebt,", "tokens": ["Den", "fie\u00b7ge\u00b7sprei\u00df", "ge\u00b7winnt", ",", "und", "Gott", "die", "eh\u00b7re", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ist allein der mann, den JEsus ehrt und liebt.", "tokens": ["Der", "ist", "al\u00b7lein", "der", "mann", ",", "den", "Je\u00b7sus", "ehrt", "und", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,", "ART", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wei\u00df ich die bu\u00dfe gleich nicht k\u00fcnstlich zu beschreiben;", "tokens": ["Wei\u00df", "ich", "die", "bu\u00b7\u00dfe", "gleich", "nicht", "k\u00fcnst\u00b7lich", "zu", "be\u00b7schrei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Genug: da\u00df bu\u00df und reu mein hertze kr\u00e4fftig r\u00fchrt.", "tokens": ["Ge\u00b7nug", ":", "da\u00df", "bu\u00df", "und", "reu", "mein", "hert\u00b7ze", "kr\u00e4ff\u00b7tig", "r\u00fchrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "NN", "KON", "ADJD", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hast du die bibel schon bis auf den band studirt,", "tokens": ["Hast", "du", "die", "bi\u00b7bel", "schon", "bis", "auf", "den", "band", "stu\u00b7dirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alle wissenschafft, die ketzer einzutreiben;", "tokens": ["Und", "al\u00b7le", "wis\u00b7sen\u00b7schafft", ",", "die", "ket\u00b7zer", "ein\u00b7zu\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRELS", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ist es gleichwol nichts, so viel du auch gefa\u00dft,", "tokens": ["So", "ist", "es", "gleich\u00b7wol", "nichts", ",", "so", "viel", "du", "auch", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "$,", "ADV", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo du nicht GOttes gnad, und lieb, und glauben hast.", "tokens": ["Wo", "du", "nicht", "Got\u00b7tes", "gnad", ",", "und", "lieb", ",", "und", "glau\u00b7ben", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "NN", "NN", "$,", "KON", "ADJD", "$,", "KON", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die h\u00f6chste weisheit ist GOtt lieben und ihm dienen,", "tokens": ["Die", "h\u00f6chs\u00b7te", "weis\u00b7heit", "ist", "Gott", "lie\u00b7ben", "und", "ihm", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "VVINF", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor das himmelreich das reich der welt verschm\u00e4hn.", "tokens": ["Und", "vor", "das", "him\u00b7mel\u00b7reich", "das", "reich", "der", "welt", "ver\u00b7schm\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "ART", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn die auf eitle sch\u00e4tz und ehr\u2019 und wohllust sehn,", "tokens": ["Denn", "die", "auf", "eit\u00b7le", "sch\u00e4tz", "und", "ehr'", "und", "wohl\u00b7lust", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "KON", "