{"textgrid.poem.64031": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Tatkraft", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nicht traurig zu sein, nicht hilflos zu tragen.", "tokens": ["Ich", "ha\u00b7be", "nicht", "trau\u00b7rig", "zu", "sein", ",", "nicht", "hil\u00b7flos", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VAINF", "$,", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Stark heiter. Das hab ich zu sein. Was lasse ich", "tokens": ["Stark", "hei\u00b7ter", ".", "Das", "hab", "ich", "zu", "sein", ".", "Was", "las\u00b7se", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "PDS", "VAFIN", "PPER", "PTKZU", "VAINF", "$.", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir von jedem Wind die bl\u00f6den Kohlen wieder", "tokens": ["Mir", "von", "je\u00b7dem", "Wind", "die", "bl\u00f6\u00b7den", "Koh\u00b7len", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PIAT", "NN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Anfachen, die mir in den Lenden stecken.", "tokens": ["An\u00b7fa\u00b7chen", ",", "die", "mir", "in", "den", "Len\u00b7den", "ste\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich verbiete mir, mich aufzubringen, traurig zu sein.", "tokens": ["Ich", "ver\u00b7bie\u00b7te", "mir", ",", "mich", "auf\u00b7zu\u00b7brin\u00b7gen", ",", "trau\u00b7rig", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVIZU", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "Ich verbiete mir, mich zu hassen.", "tokens": ["Ich", "ver\u00b7bie\u00b7te", "mir", ",", "mich", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Wann meine Lippen was tragen, was suchen sie:", "tokens": ["Wann", "mei\u00b7ne", "Lip\u00b7pen", "was", "tra\u00b7gen", ",", "was", "su\u00b7chen", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PIS", "VVINF", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Staub.", "tokens": ["Staub", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Ich verbiete mir von meiner Seele zu nehmen und nicht ihr zu geben.", "tokens": ["Ich", "ver\u00b7bie\u00b7te", "mir", "von", "mei\u00b7ner", "See\u00b7le", "zu", "neh\u00b7men", "und", "nicht", "ihr", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "KON", "PTKNEG", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "In der Einsamkeit, eine wehe Sonne, da will ich leuchten lernen.", "tokens": ["In", "der", "Ein\u00b7sam\u00b7keit", ",", "ei\u00b7ne", "we\u00b7he", "Son\u00b7ne", ",", "da", "will", "ich", "leuch\u00b7ten", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJD", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.11": {"text": "Erst Fertige geh\u00f6ren hinaus.", "tokens": ["Erst", "Fer\u00b7ti\u00b7ge", "ge\u00b7h\u00f6\u00b7ren", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Die Flammen des Geschlechts sind dann eine Sonne geworden;", "tokens": ["Die", "Flam\u00b7men", "des", "Ge\u00b7schlechts", "sind", "dann", "ei\u00b7ne", "Son\u00b7ne", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Eine weise Sonne f\u00fcr alle.", "tokens": ["Ei\u00b7ne", "wei\u00b7se", "Son\u00b7ne", "f\u00fcr", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIS", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Dann werbt um mich.", "tokens": ["Dann", "werbt", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Alle.", "tokens": ["Al\u00b7le", "."], "token_info": ["word", "punct"], "pos": ["PIAT", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Mann und Weib.", "tokens": ["Mann", "und", "Weib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Gleich.", "tokens": ["Gleich", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Die hohe Leidenschaft, die Liebe zu allen Geschwistern", "tokens": ["Die", "ho\u00b7he", "Lei\u00b7den\u00b7schaft", ",", "die", "Lie\u00b7be", "zu", "al\u00b7len", "Ge\u00b7schwis\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "des Odems brennt gl\u00fchender, inniger.", "tokens": ["des", "O\u00b7dems", "brennt", "gl\u00fc\u00b7hen\u00b7der", ",", "in\u00b7ni\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Mehr als Rauschvergehen ist Rauschtun.", "tokens": ["Mehr", "als", "Rauschver\u00b7ge\u00b7hen", "ist", "Rauschtun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Allen.", "tokens": ["Al\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Da\u00df sie ihn mitnehmen.", "tokens": ["Da\u00df", "sie", "ihn", "mit\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Als Weisheit entfalten.", "tokens": ["Als", "Weis\u00b7heit", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "Alle sollt ihr dann werben um mich.", "tokens": ["Al\u00b7le", "sollt", "ihr", "dann", "wer\u00b7ben", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "VAFIN", "APPR", "PPER", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.25": {"text": "Nun, altes M\u00fctterchen,", "tokens": ["Nun", ",", "al\u00b7tes", "M\u00fct\u00b7ter\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.26": {"text": "Keinem versage ich mich.", "tokens": ["Kei\u00b7nem", "ver\u00b7sa\u00b7ge", "ich", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PRF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Deine welken Wehen, ich verstehe.", "tokens": ["Dei\u00b7ne", "wel\u00b7ken", "We\u00b7hen", ",", "ich", "ver\u00b7ste\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Stille sie.", "tokens": ["Stil\u00b7le", "sie", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.29": {"text": "Und euch ihr Barden, wie werde ich dann euch erst l\u00fcften.", "tokens": ["Und", "euch", "ihr", "Bar\u00b7den", ",", "wie", "wer\u00b7de", "ich", "dann", "euch", "erst", "l\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Ihr Unfertigen, die ihr euch an Unfertige kauert.", "tokens": ["Ihr", "Un\u00b7fer\u00b7ti\u00b7gen", ",", "die", "ihr", "euch", "an", "Un\u00b7fer\u00b7ti\u00b7ge", "kau\u00b7ert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+---+----+--+-", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Wollt ihr auseinander.", "tokens": ["Wollt", "ihr", "aus\u00b7ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "Auseinander sage ich.", "tokens": ["Aus\u00b7ein\u00b7an\u00b7der", "sa\u00b7ge", "ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Ich will euch helfen, eure Bl\u00f6\u00dfe zu verdecken!", "tokens": ["Ich", "will", "euch", "hel\u00b7fen", ",", "eu\u00b7re", "Bl\u00f6\u00b7\u00dfe", "zu", "ver\u00b7de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ihr m\u00fcden, m\u00fcrrischen Felsen, die ihr die N\u00e4sse liebt!", "tokens": ["Ihr", "m\u00fc\u00b7den", ",", "m\u00fcr\u00b7ri\u00b7schen", "Fel\u00b7sen", ",", "die", "ihr", "die", "N\u00e4s\u00b7se", "liebt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Und dann meint: Ihr seid G\u00e4rten.", "tokens": ["Und", "dann", "meint", ":", "Ihr", "seid", "G\u00e4r\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.36": {"text": "Es gibt nur einen Stern f\u00fcr uns.", "tokens": ["Es", "gibt", "nur", "ei\u00b7nen", "Stern", "f\u00fcr", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Den Mannesstern.", "tokens": ["Den", "Man\u00b7ness\u00b7tern", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Den grauen Stern der Tatkraft.", "tokens": ["Den", "grau\u00b7en", "Stern", "der", "Tat\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das will der Weltvater von allen.", "tokens": ["Das", "will", "der", "Welt\u00b7va\u00b7ter", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "APPR", "PIAT", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ach ihr Schelme!", "tokens": ["Ach", "ihr", "Schel\u00b7me", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ihr Tr\u00e4umenden!", "tokens": ["Ihr", "Tr\u00e4u\u00b7men\u00b7den", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Ihr leichtgewandigen, zierlichen Flammen!", "tokens": ["Ihr", "leicht\u00b7ge\u00b7wan\u00b7di\u00b7gen", ",", "zier\u00b7li\u00b7chen", "Flam\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie so schelmisch ihr tanzt \u2013 Barden auf Kugeln.", "tokens": ["Wie", "so", "schel\u00b7misch", "ihr", "tanzt", "\u2013", "Bar\u00b7den", "auf", "Ku\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPER", "VVFIN", "$(", "NN", "APPR", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}