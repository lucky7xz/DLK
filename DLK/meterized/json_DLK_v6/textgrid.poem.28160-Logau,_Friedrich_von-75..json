{"textgrid.poem.28160": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nivula ist wie ein Schnee,", "tokens": ["Ni\u00b7vu\u00b7la", "ist", "wie", "ein", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der kaum ietzt fiel au\u00df der H\u00f6h.", "tokens": ["Der", "kaum", "ietzt", "fiel", "au\u00df", "der", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie auch ihre Redligkeit", "tokens": ["Wie", "auch", "ih\u00b7re", "Red\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist wie Schnee zu Mertzens-Zeit,", "tokens": ["Ist", "wie", "Schnee", "zu", "Mert\u00b7zens\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, wie neu er ist geacht,", "tokens": ["Der", ",", "wie", "neu", "er", "ist", "ge\u00b7acht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer tr\u00fcbes Wasser macht.", "tokens": ["Im\u00b7mer", "tr\u00fc\u00b7bes", "Was\u00b7ser", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}