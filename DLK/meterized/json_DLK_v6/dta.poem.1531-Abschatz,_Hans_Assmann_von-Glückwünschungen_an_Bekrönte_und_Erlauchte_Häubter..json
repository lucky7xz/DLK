{"dta.poem.1531": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungen  \n an  \n Bekr\u00f6nte  \n und  \n Erlauchte H\u00e4ubter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Zum dritten Bande spricht das H\u00f6chste Drey den Segen/", "tokens": ["Zum", "drit\u00b7ten", "Ban\u00b7de", "spricht", "das", "H\u00f6chs\u00b7te", "Drey", "den", "Se\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihm dreyfach/ was bi\u00dfher ermangelt/ beyzulegen.", "tokens": ["Ihm", "drey\u00b7fach", "/", "was", "bi\u00df\u00b7her", "er\u00b7man\u00b7gelt", "/", "bey\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "ADV", "VVFIN", "$(", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}