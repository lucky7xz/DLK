{"textgrid.poem.32262": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "1L: T\u00f6ne, frohe Leier,", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6ne, frohe Leier,", "tokens": ["T\u00f6\u00b7ne", ",", "fro\u00b7he", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "T\u00f6ne Lust und Wein!", "tokens": ["T\u00f6\u00b7ne", "Lust", "und", "Wein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "T\u00f6ne, sanfte Leier,", "tokens": ["T\u00f6\u00b7ne", ",", "sanf\u00b7te", "Lei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "T\u00f6ne Liebe drein!", "tokens": ["T\u00f6\u00b7ne", "Lie\u00b7be", "drein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wilde Krieger singen,", "tokens": ["Wil\u00b7de", "Krie\u00b7ger", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ha\u00df und Rach' und Blut", "tokens": ["Ha\u00df", "und", "Rach'", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In die Laute singen,", "tokens": ["In", "die", "Lau\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist nicht Lust, ist Wut.", "tokens": ["Ist", "nicht", "Lust", ",", "ist", "Wut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Zwar der Heldens\u00e4nger", "tokens": ["Zwar", "der", "Hel\u00b7den\u00b7s\u00e4n\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sammelt Lorbeern ein;", "tokens": ["Sam\u00b7melt", "Lor\u00b7beern", "ein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihn verehrt man l\u00e4nger.", "tokens": ["Ihn", "ver\u00b7ehrt", "man", "l\u00e4n\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lebt er l\u00e4nger? Nein.", "tokens": ["Lebt", "er", "l\u00e4n\u00b7ger", "?", "Nein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Er vergr\u00e4bt im Leben", "tokens": ["Er", "ver\u00b7gr\u00e4bt", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sich in Tiefsinn ein:", "tokens": ["Sich", "in", "Tief\u00b7sinn", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um erst dann zu leben,", "tokens": ["Um", "erst", "dann", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wann er Staub wird sein.", "tokens": ["Wann", "er", "Staub", "wird", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VAFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Lobt sein g\u00f6ttlich Feuer,", "tokens": ["Lobt", "sein", "g\u00f6tt\u00b7lich", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Zeit und Afterzeit!", "tokens": ["Zeit", "und", "Af\u00b7ter\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und an meiner Leier", "tokens": ["Und", "an", "mei\u00b7ner", "Lei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Lobt die Fr\u00f6hlichkeit.", "tokens": ["Lobt", "die", "Fr\u00f6h\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}