{"textgrid.poem.49597": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mondnacht am Chiemsee", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gespensterhaft die Berge ragen.", "tokens": ["Ge\u00b7spens\u00b7ter\u00b7haft", "die", "Ber\u00b7ge", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weit \u00fcber sie mit bleichem Schein,", "tokens": ["Weit", "\u00fc\u00b7ber", "sie", "mit", "blei\u00b7chem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von raschen Wolken tief umzogen,", "tokens": ["Von", "ra\u00b7schen", "Wol\u00b7ken", "tief", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schaut silberhell der Mond herein.", "tokens": ["Schaut", "sil\u00b7ber\u00b7hell", "der", "Mond", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der See ergl\u00e4nzt von seinen Strahlen,", "tokens": ["Der", "See", "er\u00b7gl\u00e4nzt", "von", "sei\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die spielen gl\u00e4nzend dr\u00fcber her,", "tokens": ["Die", "spie\u00b7len", "gl\u00e4n\u00b7zend", "dr\u00fc\u00b7ber", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PAV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als tanzten Nixen ihren Reigen", "tokens": ["Als", "tanz\u00b7ten", "Ni\u00b7xen", "ih\u00b7ren", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf leichtbewegtem Wellenmeer.", "tokens": ["Auf", "leicht\u00b7be\u00b7weg\u00b7tem", "Wel\u00b7len\u00b7meer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Ufer durch die hohe Buche", "tokens": ["Am", "U\u00b7fer", "durch", "die", "ho\u00b7he", "Bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit leisem Hauch der Nachtwind zieht,", "tokens": ["Mit", "lei\u00b7sem", "Hauch", "der", "Nacht\u00b7wind", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in den Zweigen t\u00f6nt ein Fl\u00fcstern", "tokens": ["Und", "in", "den", "Zwei\u00b7gen", "t\u00f6nt", "ein", "Fl\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geheimnisvoll, fast wie ein Lied.", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", ",", "fast", "wie", "ein", "Lied", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin allein. Und wonnetrunken", "tokens": ["Ich", "bin", "al\u00b7lein", ".", "Und", "won\u00b7ne\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ergeb ich mich der stillen Pracht", "tokens": ["Er\u00b7geb", "ich", "mich", "der", "stil\u00b7len", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine Brust durchbebt der Zauber", "tokens": ["Und", "mei\u00b7ne", "Brust", "durch\u00b7bebt", "der", "Zau\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der feierlichen Sommernacht.", "tokens": ["Der", "fei\u00b7er\u00b7li\u00b7chen", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}