{"textgrid.poem.64441": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "63. Von Herrn M.M", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein neuer Dionys rief von der Seine Strande", "tokens": ["Ein", "neu\u00b7er", "Di\u00b7o\u00b7nys", "rief", "von", "der", "Sei\u00b7ne", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sophistenschw\u00e4rme her f\u00fcr seinen Unterricht.", "tokens": ["So\u00b7phis\u00b7ten\u00b7schw\u00e4r\u00b7me", "her", "f\u00fcr", "sei\u00b7nen", "Un\u00b7ter\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Plato lebt' in seinem Lande,", "tokens": ["Ein", "Pla\u00b7to", "lebt'", "in", "sei\u00b7nem", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und diesen kannt' er nicht.", "tokens": ["Und", "die\u00b7sen", "kannt'", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}