{"dta.poem.3076": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "122.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sprich, wie der Muselman im Ungl\u00fcck und im Gl\u00fcck", "tokens": ["Sprich", ",", "wie", "der", "Mu\u00b7sel\u00b7man", "im", "Un\u00b7gl\u00fcck", "und", "im", "Gl\u00fcck"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spricht: Wir sind Gottes und kehren zu ihm zur\u00fcck.", "tokens": ["Spricht", ":", "Wir", "sind", "Got\u00b7tes", "und", "keh\u00b7ren", "zu", "ihm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "NN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Was ihn erfreut, ergetzt, was ihn betr\u00fcbt, verletzt,", "tokens": ["Was", "ihn", "er\u00b7freut", ",", "er\u00b7getzt", ",", "was", "ihn", "be\u00b7tr\u00fcbt", ",", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihn bedroht, erschreckt, verwundert und entsetzt;", "tokens": ["Was", "ihn", "be\u00b7droht", ",", "er\u00b7schreckt", ",", "ver\u00b7wun\u00b7dert", "und", "ent\u00b7setzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ihn ergreift, entz\u00fcckt, was ihn beth\u00f6rt, ber\u00fcckt,", "tokens": ["Was", "ihn", "er\u00b7greift", ",", "ent\u00b7z\u00fcckt", ",", "was", "ihn", "be\u00b7th\u00f6rt", ",", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihn zum Himmel hebt und ihn zu Boden dr\u00fcckt;", "tokens": ["Was", "ihn", "zum", "Him\u00b7mel", "hebt", "und", "ihn", "zu", "Bo\u00b7den", "dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er sprach und spricht noch jetzt sein Bannwort, und zuletzt", "tokens": ["Er", "sprach", "und", "spricht", "noch", "jetzt", "sein", "Bann\u00b7wort", ",", "und", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat alles dieses Wort ins Gleichgewicht gesetzt.", "tokens": ["Hat", "al\u00b7les", "die\u00b7ses", "Wort", "ins", "Gleich\u00b7ge\u00b7wicht", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PDAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum, wie mit Gleichmut er im Ungl\u00fcck und im Gl\u00fcck,", "tokens": ["Drum", ",", "wie", "mit", "Gleich\u00b7mut", "er", "im", "Un\u00b7gl\u00fcck", "und", "im", "Gl\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "APPR", "NN", "PPER", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprich: Wir sind Gottes und kehren zu ihm zur\u00fcck.", "tokens": ["Sprich", ":", "Wir", "sind", "Got\u00b7tes", "und", "keh\u00b7ren", "zu", "ihm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VAFIN", "NN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}