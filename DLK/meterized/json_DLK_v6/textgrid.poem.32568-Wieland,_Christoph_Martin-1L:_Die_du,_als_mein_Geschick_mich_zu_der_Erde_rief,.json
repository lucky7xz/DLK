{"textgrid.poem.32568": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die du, als mein Geschick mich zu der Erde rief,", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die du, als mein Geschick mich zu der Erde rief,", "tokens": ["Die", "du", ",", "als", "mein", "Ge\u00b7schick", "mich", "zu", "der", "Er\u00b7de", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich mit segnendem Mund k\u00fc\u00dftest und weihetest,", "tokens": ["Mich", "mit", "seg\u00b7nen\u00b7dem", "Mund", "k\u00fc\u00df\u00b7test", "und", "wei\u00b7he\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Hier dein S\u00e4nger zu sein, Weisheit, begeistre mich,", "tokens": ["Hier", "dein", "S\u00e4n\u00b7ger", "zu", "sein", ",", "Weis\u00b7heit", ",", "be\u00b7geist\u00b7re", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VAINF", "$,", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Da\u00df ich von deiner Sch\u00f6nheit sing.", "tokens": ["Da\u00df", "ich", "von", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "sing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach wie wenige sind's, G\u00f6ttin, wie wenige,", "tokens": ["Ach", "wie", "we\u00b7ni\u00b7ge", "sin\u00b7d's", ",", "G\u00f6t\u00b7tin", ",", "wie", "we\u00b7ni\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PIAT", "NN", "$,", "NN", "$,", "PWAV", "PIS", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Denen Du dich vertraut? welche den Sonnenglanz", "tokens": ["De\u00b7nen", "Du", "dich", "ver\u00b7traut", "?", "wel\u00b7che", "den", "Son\u00b7nen\u00b7glanz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "PRF", "ADJD", "$.", "PRELS", "ART", "NN"], "meter": "+--+-+---+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deiner Sch\u00f6nheit gesehn und den entz\u00fcckenden", "tokens": ["Dei\u00b7ner", "Sch\u00f6n\u00b7heit", "ge\u00b7sehn", "und", "den", "ent\u00b7z\u00fc\u00b7cken\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "S\u00fc\u00df harmonischen Mund geh\u00f6rt!", "tokens": ["S\u00fc\u00df", "har\u00b7mo\u00b7ni\u00b7schen", "Mund", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wie sollten sie dich finden? wo sucht man dich?", "tokens": ["Und", "wie", "soll\u00b7ten", "sie", "dich", "fin\u00b7den", "?", "wo", "sucht", "man", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$.", "PWAV", "VVFIN", "PIS", "PRF", "$."], "meter": "-++---+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ist der Zugang zu dir mit unersteiglichen", "tokens": ["Ist", "der", "Zu\u00b7gang", "zu", "dir", "mit", "un\u00b7er\u00b7steig\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Furchtbarn Alpen verwehrt? oder verbirgst du dich", "tokens": ["Furcht\u00b7barn", "Al\u00b7pen", "ver\u00b7wehrt", "?", "o\u00b7der", "ver\u00b7birgst", "du", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVPP", "$.", "KON", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In cimmerische Finsternis?", "tokens": ["In", "cim\u00b7me\u00b7ri\u00b7sche", "Fins\u00b7ter\u00b7nis", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist's ein blumenlos Land \u00f6d und von Raben nur", "tokens": ["Ist's", "ein", "blu\u00b7men\u00b7los", "Land", "\u00f6d", "und", "von", "Ra\u00b7ben", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJD", "NN", "ADJD", "KON", "APPR", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und von Eulen bewohnt? Sind es cecropische", "tokens": ["Und", "von", "Eu\u00b7len", "be\u00b7wohnt", "?", "Sind", "es", "ce\u00b7cro\u00b7pi\u00b7sche"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVPP", "$.", "VAFIN", "PPER", "ADJA"], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Labyrinthe, wodurch man zu den H\u00f6hen irrt,", "tokens": ["La\u00b7by\u00b7rin\u00b7the", ",", "wo\u00b7durch", "man", "zu", "den", "H\u00f6\u00b7hen", "irrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Die dein ewiger Tempel kr\u00f6nt?", "tokens": ["Die", "dein", "e\u00b7wi\u00b7ger", "Tem\u00b7pel", "kr\u00f6nt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ist's der runzlichte Duns oder Caritides,", "tokens": ["Ist's", "der", "runz\u00b7lich\u00b7te", "Duns", "o\u00b7der", "Ca\u00b7ri\u00b7ti\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der den dornichten Weg, G\u00f6ttin, uns f\u00fchren soll?", "tokens": ["Der", "den", "dor\u00b7nich\u00b7ten", "Weg", ",", "G\u00f6t\u00b7tin", ",", "uns", "f\u00fch\u00b7ren", "soll", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "NN", "$,", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ist dein Heiligtum denn staubichten Tr\u00e4umern nur,", "tokens": ["Ist", "dein", "Hei\u00b7lig\u00b7tum", "denn", "stau\u00b7bich\u00b7ten", "Tr\u00e4u\u00b7mern", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Aquinaten nur aufgetan?", "tokens": ["A\u00b7qui\u00b7na\u00b7ten", "nur", "auf\u00b7ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Ach! so suchen sie dich! Dich, die mein Sokrates", "tokens": ["Ach", "!", "so", "su\u00b7chen", "sie", "dich", "!", "Dich", ",", "die", "mein", "Sok\u00b7ra\u00b7tes"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "PRF", "$.", "PPER", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Bei der holden Natur unter den Grazien,", "tokens": ["Bei", "der", "hol\u00b7den", "Na\u00b7tur", "un\u00b7ter", "den", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "(ein entz\u00fcckend Gesicht!) schwesterlich sitzen find,", "tokens": ["(", "ein", "ent\u00b7z\u00fc\u00b7ckend", "Ge\u00b7sicht", "!", ")", "schwes\u00b7ter\u00b7lich", "sit\u00b7zen", "find", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "NN", "$.", "$(", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Wie Diana bei Nymphen sitzt.", "tokens": ["Wie", "Di\u00b7a\u00b7na", "bei", "Nym\u00b7phen", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn der stolze Sophist \u00fcber die Sterne bald", "tokens": ["Wenn", "der", "stol\u00b7ze", "So\u00b7phist", "\u00fc\u00b7ber", "die", "Ster\u00b7ne", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Deine Larve verfolgt, bald dich im Abgrund sp\u00e4ht,", "tokens": ["Dei\u00b7ne", "Lar\u00b7ve", "ver\u00b7folgt", ",", "bald", "dich", "im", "Ab\u00b7grund", "sp\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "ADV", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wenn ein schw\u00e4rmender Kopf, fiebrischer Flammen voll,", "tokens": ["Wenn", "ein", "schw\u00e4r\u00b7men\u00b7der", "Kopf", ",", "fie\u00b7bri\u00b7scher", "Flam\u00b7men", "voll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dich in Wolken zu k\u00fcssen w\u00e4hnt,", "tokens": ["Dich", "in", "Wol\u00b7ken", "zu", "k\u00fcs\u00b7sen", "w\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "So begegnetest du, sch\u00f6n wie Unsterbliche,", "tokens": ["So", "be\u00b7ge\u00b7gne\u00b7test", "du", ",", "sch\u00f6n", "wie", "U\u00b7nsterb\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und mit offenem Arm suchenden Tullien,", "tokens": ["Und", "mit", "of\u00b7fe\u00b7nem", "Arm", "su\u00b7chen\u00b7den", "Tul\u00b7li\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Epikur sah dich so, unter hymettischen", "tokens": ["E\u00b7pi\u00b7kur", "sah", "dich", "so", ",", "un\u00b7ter", "hy\u00b7met\u00b7ti\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Rosen k\u00fc\u00dfte dich Platon oft.", "tokens": ["Ro\u00b7sen", "k\u00fc\u00df\u00b7te", "dich", "Pla\u00b7ton", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "NE", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Mit wohlredendem Mund, wie ihn Diotima", "tokens": ["Mit", "wohl\u00b7re\u00b7den\u00b7dem", "Mund", ",", "wie", "ihn", "Dio\u00b7ti\u00b7ma"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PWAV", "PPER", "NE"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mich Doris gelehrt, hast du den Weisesten,", "tokens": ["Und", "mich", "Do\u00b7ris", "ge\u00b7lehrt", ",", "hast", "du", "den", "Wei\u00b7ses\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "VVPP", "$,", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Was kein Z\u00e4nker gewu\u00dft, die verge\u00dfne Kunst!", "tokens": ["Was", "kein", "Z\u00e4n\u00b7ker", "ge\u00b7wu\u00dft", ",", "die", "ver\u00b7ge\u00df\u00b7ne", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Leben gelehrt und ein Mensch zu sein.", "tokens": ["Le\u00b7ben", "ge\u00b7lehrt", "und", "ein", "Mensch", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "H\u00f6re, Weisheit, auch mich, wenn je mein junger Fu\u00df", "tokens": ["H\u00f6\u00b7re", ",", "Weis\u00b7heit", ",", "auch", "mich", ",", "wenn", "je", "mein", "jun\u00b7ger", "Fu\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "ADV", "PPER", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Deine Pfade gesucht, und mich Aurora oft", "tokens": ["Dei\u00b7ne", "Pfa\u00b7de", "ge\u00b7sucht", ",", "und", "mich", "Au\u00b7ro\u00b7ra", "oft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "KON", "PPER", "NE", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Wundernd ansah und dann einen zufriednen Glanz", "tokens": ["Wun\u00b7dernd", "an\u00b7sah", "und", "dann", "ei\u00b7nen", "zu\u00b7fried\u00b7nen", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KON", "ADV", "ART", "ADJA", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Um mein forschendes Auge go\u00df,", "tokens": ["Um", "mein", "for\u00b7schen\u00b7des", "Au\u00b7ge", "go\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "O so zeige dich mir, wie du dich Bodmern zeigst,", "tokens": ["O", "so", "zei\u00b7ge", "dich", "mir", ",", "wie", "du", "dich", "Bod\u00b7mern", "zeigst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "PRF", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dich zu sehen gewohnt, voll des olympischen", "tokens": ["Dich", "zu", "se\u00b7hen", "ge\u00b7wohnt", ",", "voll", "des", "o\u00b7lym\u00b7pi\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "VVPP", "$,", "ADJD", "ART", "NN"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sanften Lichts, das dein Aug unersch\u00f6pft um sich gie\u00dft,", "tokens": ["Sanf\u00b7ten", "Lichts", ",", "das", "dein", "Aug", "un\u00b7er\u00b7sch\u00f6pft", "um", "sich", "gie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mi\u00dft Er leicht deine Gegenwart,", "tokens": ["Mi\u00dft", "Er", "leicht", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Lehr auch mich, wie du Ihn gelehrtest, die edle Kunst,", "tokens": ["Lehr", "auch", "mich", ",", "wie", "du", "Ihn", "ge\u00b7lehr\u00b7test", ",", "die", "ed\u00b7le", "Kunst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "---+-+-+-