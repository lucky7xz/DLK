 "ge\u00b7st\u00e4rkt", ",", "will", "ich", "der", "Las\u00b7ter", "Brut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "VVPP", "$,", "VMFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und den G\u00f6tzen des Wahns und dem vielk\u00f6pfigen", "tokens": ["Und", "den", "G\u00f6t\u00b7zen", "des", "Wahns", "und", "dem", "viel\u00b7k\u00f6p\u00b7fi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "KON", "ART", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Irrtum Widerstand tun, stets ein erkl\u00e4rter Feind", "tokens": ["Irr\u00b7tum", "Wi\u00b7der\u00b7stand", "tun", ",", "stets", "ein", "er\u00b7kl\u00e4r\u00b7ter", "Feind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Allem, was dich, o Menschheit sch\u00e4ndt.", "tokens": ["Al\u00b7lem", ",", "was", "dich", ",", "o", "Menschheit", "sch\u00e4ndt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "$,", "FM", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "In bezauberndem Reiz, jugendlich sch\u00f6n und frei", "tokens": ["In", "be\u00b7zau\u00b7bern\u00b7dem", "Reiz", ",", "ju\u00b7gend\u00b7lich", "sch\u00f6n", "und", "frei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "ADJD", "KON", "ADJD"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "Will ich die Wahrheit alsdann zeigen, in nackender", "tokens": ["Will", "ich", "die", "Wahr\u00b7heit", "als\u00b7dann", "zei\u00b7gen", ",", "in", "na\u00b7cken\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,", "APPR", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Liebenswerter Gestalt, so trat Elise dort,", "tokens": ["Lie\u00b7bens\u00b7wer\u00b7ter", "Ge\u00b7stalt", ",", "so", "trat", "E\u00b7li\u00b7se", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "NE", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein lebendiger Marmor, her.", "tokens": ["Ein", "le\u00b7ben\u00b7di\u00b7ger", "Mar\u00b7mor", ",", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.23": {"line.1": {"text": "Freunde, h\u00f6ret mir zu, und euer edles Herz", "tokens": ["Freun\u00b7de", ",", "h\u00f6\u00b7ret", "mir", "zu", ",", "und", "eu\u00b7er", "ed\u00b7les", "Herz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schlage st\u00e4rker in euch, wenn ihr mich singen h\u00f6rt,", "tokens": ["Schla\u00b7ge", "st\u00e4r\u00b7ker", "in", "euch", ",", "wenn", "ihr", "mich", "sin\u00b7gen", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dann erinnert mich oft: Freund, la\u00df dein Leben stets", "tokens": ["Dann", "e\u00b7rin\u00b7nert", "mich", "oft", ":", "Freund", ",", "la\u00df", "dein", "Le\u00b7ben", "stets"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$.", "NN", "$,", "VVIMP", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lehrend wie deine Lieder sein.", "tokens": ["Leh\u00b7rend", "wie", "dei\u00b7ne", "Lie\u00b7der", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "VAINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}