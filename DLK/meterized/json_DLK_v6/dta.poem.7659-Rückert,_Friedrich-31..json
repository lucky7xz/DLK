{"dta.poem.7659": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte von der Zeit mich nebenaus gerettet,", "tokens": ["Ich", "hat\u00b7te", "von", "der", "Zeit", "mich", "ne\u00b7be\u00b7naus", "ge\u00b7ret\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Vor ihren St\u00fcrmen in ein Ruhthal mich gebettet.", "tokens": ["Vor", "ih\u00b7ren", "St\u00fcr\u00b7men", "in", "ein", "Ruh\u00b7thal", "mich", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da richtet' ich mich ein, bequem f\u00fcr mich zu hausen,", "tokens": ["Da", "rich\u00b7tet'", "ich", "mich", "ein", ",", "be\u00b7quem", "f\u00fcr", "mich", "zu", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lie\u00df die tolle Zeit indessen weiter brausen.", "tokens": ["Und", "lie\u00df", "die", "tol\u00b7le", "Zeit", "in\u00b7des\u00b7sen", "wei\u00b7ter", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich dacht' ich sei zur\u00fcck, und weit sei mir die Zeit", "tokens": ["Ich", "dacht'", "ich", "sei", "zu\u00b7r\u00fcck", ",", "und", "weit", "sei", "mir", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voraus, da sah ich da\u00df sie selbst zur\u00fcck sei weit.", "tokens": ["Vo\u00b7raus", ",", "da", "sah", "ich", "da\u00df", "sie", "selbst", "zu\u00b7r\u00fcck", "sei", "weit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "KOUS", "PPER", "ADV", "PTKVZ", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was ist das hinter ihr, vor dem sie nimmt die Flucht;", "tokens": ["Was", "ist", "das", "hin\u00b7ter", "ihr", ",", "vor", "dem", "sie", "nimmt", "die", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "APPR", "PPER", "$,", "APPR", "PRELS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und was das au\u00dfer ihr, nach dem sie ewig sucht?", "tokens": ["Und", "was", "das", "au\u00b7\u00dfer", "ihr", ",", "nach", "dem", "sie", "e\u00b7wig", "sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PDS", "APPR", "PPER", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}