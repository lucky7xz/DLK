{"dta.poem.9278": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Wjr armen gelehrten wir haben die ehre/", "tokens": ["Wjr", "ar\u00b7men", "ge\u00b7lehr\u00b7ten", "wir", "ha\u00b7ben", "die", "eh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "PPER", "VAFIN", "ART", "VVFIN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die andern indessen besitzen das geld:", "tokens": ["Die", "an\u00b7dern", "in\u00b7des\u00b7sen", "be\u00b7sit\u00b7zen", "das", "geld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Drum gibet die witzig und spitzige welt", "tokens": ["Drum", "gi\u00b7bet", "die", "wit\u00b7zig", "und", "spit\u00b7zi\u00b7ge", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJD", "KON", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den k\u00fcnsten und sprachen gar selten geh\u00f6re.", "tokens": ["Den", "k\u00fcns\u00b7ten", "und", "spra\u00b7chen", "gar", "sel\u00b7ten", "ge\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Sie schertzen mit dieser ver\u00e4chtigen lehre/", "tokens": ["Sie", "schert\u00b7zen", "mit", "die\u00b7ser", "ver\u00b7\u00e4ch\u00b7ti\u00b7gen", "leh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Weil solche nicht grosse ducaten erh\u00e4lt:", "tokens": ["Weil", "sol\u00b7che", "nicht", "gros\u00b7se", "du\u00b7ca\u00b7ten", "er\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Hingegen der reiche wird \u00f6ffter bestellt/", "tokens": ["Hin\u00b7ge\u00b7gen", "der", "rei\u00b7che", "wird", "\u00f6ff\u00b7ter", "be\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "ADJD", "VVPP", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Als wenn er in allen viel ehrlicher w\u00e4re.", "tokens": ["Als", "wenn", "er", "in", "al\u00b7len", "viel", "ehr\u00b7li\u00b7cher", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "PIAT", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Doch bleiben wir fleissig und achten es nicht/", "tokens": ["Doch", "blei\u00b7ben", "wir", "fleis\u00b7sig", "und", "ach\u00b7ten", "es", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wenn irgend ein esel die rosen verspricht/", "tokens": ["Wenn", "ir\u00b7gend", "ein", "e\u00b7sel", "die", "ro\u00b7sen", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der selten was anders als disteln gefressen.", "tokens": ["Der", "sel\u00b7ten", "was", "an\u00b7ders", "als", "dis\u00b7teln", "ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PWS", "ADV", "KOUS", "NN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Wir suchen die sch\u00e4tze die nimmer vergehn/", "tokens": ["Wir", "su\u00b7chen", "die", "sch\u00e4t\u00b7ze", "die", "nim\u00b7mer", "ver\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "VVFIN", "ART", "ADV", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und mitten in unsern gedancken bestehn/", "tokens": ["Und", "mit\u00b7ten", "in", "un\u00b7sern", "ge\u00b7dan\u00b7cken", "be\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So k\u00f6nnen wir leichtlich das ander vergessen.", "tokens": ["So", "k\u00f6n\u00b7nen", "wir", "leicht\u00b7lich", "das", "an\u00b7der", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}