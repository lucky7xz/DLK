{"textgrid.poem.59232": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer auf den Wogen schliefe,", "tokens": ["Wer", "auf", "den", "Wo\u00b7gen", "schlie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein sanft gewiegtes Kind,", "tokens": ["Ein", "sanft", "ge\u00b7wieg\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kennt nicht des Lebens Tiefe,", "tokens": ["Kennt", "nicht", "des", "Le\u00b7bens", "Tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor s\u00fc\u00dfem Tr\u00e4umen blind.", "tokens": ["Vor", "s\u00fc\u00b7\u00dfem", "Tr\u00e4u\u00b7men", "blind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wen die St\u00fcrme fassen", "tokens": ["Doch", "wen", "die", "St\u00fcr\u00b7me", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu wildem Tanz und Fest,", "tokens": ["Zu", "wil\u00b7dem", "Tanz", "und", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wen hoch auf dunklen Stra\u00dfen", "tokens": ["Wen", "hoch", "auf", "dunk\u00b7len", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die falsche Welt verl\u00e4\u00dft:", "tokens": ["Die", "fal\u00b7sche", "Welt", "ver\u00b7l\u00e4\u00dft", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der lernt sich wacker r\u00fchren,", "tokens": ["Der", "lernt", "sich", "wa\u00b7cker", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Nacht und Klippen hin", "tokens": ["Durch", "Nacht", "und", "Klip\u00b7pen", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Lernt der das Steuer f\u00fchren", "tokens": ["Lernt", "der", "das", "Steu\u00b7er", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit sichrem, ernstem Sinn.", "tokens": ["Mit", "sich\u00b7rem", ",", "erns\u00b7tem", "Sinn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der ist vom echten Kerne,", "tokens": ["Der", "ist", "vom", "ech\u00b7ten", "Ker\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erprobt zu Lust und Pein,", "tokens": ["Er\u00b7probt", "zu", "Lust", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der glaubt an Gott und Sterne,", "tokens": ["Der", "glaubt", "an", "Gott", "und", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der soll mein Schiffmann sein!", "tokens": ["Der", "soll", "mein", "Schiff\u00b7mann", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}