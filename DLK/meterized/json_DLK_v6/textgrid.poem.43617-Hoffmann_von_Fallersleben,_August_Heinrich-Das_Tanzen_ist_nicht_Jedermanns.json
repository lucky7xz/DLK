{"textgrid.poem.43617": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Tanzen ist nicht Jedermanns", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Kibitz und die Kibitzin,", "tokens": ["Der", "Ki\u00b7bitz", "und", "die", "Ki\u00b7bit\u00b7zin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hatten beide Einen Sinn:", "tokens": ["Die", "hat\u00b7ten", "bei\u00b7de", "Ei\u00b7nen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie fingen an zu tanzen.", "tokens": ["Sie", "fin\u00b7gen", "an", "zu", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie hoben hurtig Bein um Bein", "tokens": ["Sie", "ho\u00b7ben", "hur\u00b7tig", "Bein", "um", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sprangen hoch im Sonnenschein:", "tokens": ["Und", "spran\u00b7gen", "hoch", "im", "Son\u00b7nen\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das war ein lustig Tanzen!", "tokens": ["Das", "war", "ein", "lus\u00b7tig", "Tan\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Jungfer Gans, die sah es an:", "tokens": ["Die", "Jung\u00b7fer", "Gans", ",", "die", "sah", "es", "an", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Andre k\u00f6nnen, ich auch kann!", "tokens": ["Was", "And\u00b7re", "k\u00f6n\u00b7nen", ",", "ich", "auch", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "$,", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und fing auch an zu tanzen.", "tokens": ["Und", "fing", "auch", "an", "zu", "tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ei, Jungfer Gans, was f\u00e4llt dir ein?", "tokens": ["Ei", ",", "Jung\u00b7fer", "Gans", ",", "was", "f\u00e4llt", "dir", "ein", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NE", "$,", "PWS", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du brichst am Ende noch ein Bein", "tokens": ["Du", "brichst", "am", "En\u00b7de", "noch", "ein", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit deinem fetten Ranzen!", "tokens": ["Mit", "dei\u00b7nem", "fet\u00b7ten", "Ran\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Gans, die stolpert \u00fcber'n Stein", "tokens": ["Die", "Gans", ",", "die", "stol\u00b7pert", "\u00fc\u00b7ber'n", "Stein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und purzelt hin und bricht ein Bein,", "tokens": ["Und", "pur\u00b7zelt", "hin", "und", "bricht", "ein", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da war vorbei das Tanzen.", "tokens": ["Da", "war", "vor\u00b7bei", "das", "Tan\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mu\u00dft' es kommen, Jungfer Gans!", "tokens": ["So", "mu\u00dft'", "es", "kom\u00b7men", ",", "Jung\u00b7fer", "Gans", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Tanzen ist nicht Jedermanns,", "tokens": ["Das", "Tan\u00b7zen", "ist", "nicht", "Je\u00b7der\u00b7manns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df k\u00fcnftig Andre tanzen!", "tokens": ["La\u00df", "k\u00fcnf\u00b7tig", "And\u00b7re", "tan\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}