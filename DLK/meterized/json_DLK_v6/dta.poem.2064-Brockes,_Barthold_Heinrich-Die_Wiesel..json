{"dta.poem.2064": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wiesel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Scheint gleich dieses kleine Thier uns zum Schaden", "tokens": ["Scheint", "gleich", "die\u00b7ses", "klei\u00b7ne", "Thier", "uns", "zum", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PDAT", "ADJA", "NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "mehr gegeben,", "tokens": ["mehr", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Als uns Nutzen zu verschaffen, da es junge H\u00fcner, Tau-", "tokens": ["Als", "uns", "Nut\u00b7zen", "zu", "ver\u00b7schaf\u00b7fen", ",", "da", "es", "jun\u00b7ge", "H\u00fc\u00b7ner", ",", "Tau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "NN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADJA", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "ben", "tokens": ["ben"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und noch ander Hausgefl\u00fcgel pflegt zu w\u00fcrgen und zu", "tokens": ["Und", "noch", "an\u00b7der", "Haus\u00b7ge\u00b7fl\u00fc\u00b7gel", "pflegt", "zu", "w\u00fcr\u00b7gen", "und", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "NN", "VVFIN", "PTKZU", "VVINF", "KON", "APPR"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "rauben,", "tokens": ["rau\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Auch so vieler V\u00f6gel Bruten, die in Wald und Feldern", "tokens": ["Auch", "so", "vie\u00b7ler", "V\u00f6\u00b7gel", "Bru\u00b7ten", ",", "die", "in", "Wald", "und", "Fel\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "leben,", "tokens": ["le\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Fri\u00dft, und ihre Eyer s\u00e4uft: dienen sie doch, wie die", "tokens": ["Fri\u00dft", ",", "und", "ih\u00b7re", "Ey\u00b7er", "s\u00e4uft", ":", "die\u00b7nen", "sie", "doch", ",", "wie", "die"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ART"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Katzen,", "tokens": ["Kat\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Uns zu nicht geringem Nutzen \u00f6fters gegen M\u00e4us\u2019 und", "tokens": ["Uns", "zu", "nicht", "ge\u00b7rin\u00b7gem", "Nut\u00b7zen", "\u00f6f\u00b7ters", "ge\u00b7gen", "M\u00e4us'", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PTKNEG", "ADJA", "NN", "ADV", "APPR", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Ratzen:", "tokens": ["Rat\u00b7zen", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Sie sind auch der Schlangen Feinde, als womit sie stetig", "tokens": ["Sie", "sind", "auch", "der", "Schlan\u00b7gen", "Fein\u00b7de", ",", "als", "wo\u00b7mit", "sie", "ste\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "NN", "$,", "KOUS", "PWAV", "PPER", "ADJD"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "k\u00e4mpfen:", "tokens": ["k\u00e4mp\u00b7fen", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Jhre B\u00e4lge n\u00fctzen uns, nicht nur Schmerz und Gift zu", "tokens": ["Ih\u00b7re", "B\u00e4l\u00b7ge", "n\u00fct\u00b7zen", "uns", ",", "nicht", "nur", "Schmerz", "und", "Gift", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "ADV", "NN", "KON", "NN", "PTKZU"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "d\u00e4mpfen,", "tokens": ["d\u00e4mp\u00b7fen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Und uns, f\u00fcr den Frost zu sch\u00fctzen; sondern auch in Arze-", "tokens": ["Und", "uns", ",", "f\u00fcr", "den", "Frost", "zu", "sch\u00fct\u00b7zen", ";", "son\u00b7dern", "auch", "in", "Ar\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$.", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "neyen", "tokens": ["ne\u00b7yen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "Hat man dieser Thiere Glieder sich auf manche Art zu", "tokens": ["Hat", "man", "die\u00b7ser", "Thie\u00b7re", "Glie\u00b7der", "sich", "auf", "man\u00b7che", "Art", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PDAT", "NN", "NN", "PRF", "APPR", "PIAT", "NN", "APPR"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "freuen.", "tokens": ["freu\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}