{"textgrid.poem.64836": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erinnerung soll ich dir schenken,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erinnerung soll ich dir schenken,", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "soll", "ich", "dir", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn du in weite Ferne fort,", "tokens": ["Wenn", "du", "in", "wei\u00b7te", "Fer\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mildes Freundesangedenken \u2013", "tokens": ["Und", "mil\u00b7des", "Freun\u00b7de\u00b7san\u00b7ge\u00b7den\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So fleht dein letztes Abschiedswort.", "tokens": ["So", "fleht", "dein", "letz\u00b7tes", "Ab\u00b7schieds\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Erinn'rung? H\u00e4tte denn das Leben", "tokens": ["Er\u00b7inn'\u00b7rung", "?", "H\u00e4t\u00b7te", "denn", "das", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch einen Reiz, noch Werth f\u00fcr mich,", "tokens": ["Noch", "ei\u00b7nen", "Reiz", ",", "noch", "Werth", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt' das Ged\u00e4chtni\u00df draus entschweben,", "tokens": ["Sollt'", "das", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "draus", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "An deine Freundschaft und an dich?", "tokens": ["An", "dei\u00b7ne", "Freund\u00b7schaft", "und", "an", "dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gewi\u00df! nach manchem langen Jahre,", "tokens": ["Ge\u00b7wi\u00df", "!", "nach", "man\u00b7chem", "lan\u00b7gen", "Jah\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von keinem Freudenthau benetzt,", "tokens": ["Von", "kei\u00b7nem", "Freu\u00b7dent\u00b7hau", "be\u00b7netzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird auf der Seele Weihaltare", "tokens": ["Wird", "auf", "der", "See\u00b7le", "Wei\u00b7hal\u00b7ta\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Bildni\u00df prangen so wie jetzt.", "tokens": ["Dein", "Bild\u00b7ni\u00df", "pran\u00b7gen", "so", "wie", "jetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O wer, wer malt die dunkle Wonne,", "tokens": ["O", "wer", ",", "wer", "malt", "die", "dunk\u00b7le", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "$,", "PWS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mich durchflammt mit Allgewalt,", "tokens": ["Die", "mich", "durch\u00b7flammt", "mit", "All\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Blick ich in deines Auges Sonne,", "tokens": ["Blick", "ich", "in", "dei\u00b7nes", "Au\u00b7ges", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Draus Kunde aus dem Jenseits strahlt?", "tokens": ["Draus", "Kun\u00b7de", "aus", "dem", "Jen\u00b7seits", "strahlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer nennt mein schmerzlich s\u00fc\u00dfes Lauschen,", "tokens": ["Wer", "nennt", "mein", "schmerz\u00b7lich", "s\u00fc\u00b7\u00dfes", "Lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seligkeit, die mich durchdringt,", "tokens": ["Die", "Se\u00b7lig\u00b7keit", ",", "die", "mich", "durch\u00b7dringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn mild, wie Aeolsharfenrauschen,", "tokens": ["Wenn", "mild", ",", "wie", "A\u00b7e\u00b7ols\u00b7har\u00b7fen\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An's Ohr mir Deine Stimme klingt?", "tokens": ["An's", "Ohr", "mir", "Dei\u00b7ne", "Stim\u00b7me", "klingt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wenn, wie lauer Weste F\u00e4cheln,", "tokens": ["Und", "wenn", ",", "wie", "lau\u00b7er", "Wes\u00b7te", "F\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PWAV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Sang durch Abendluft get\u00f6nt,", "tokens": ["Wie", "Sang", "durch", "A\u00b7ben\u00b7dluft", "ge\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein zaubrisch wunderbares L\u00e4cheln,", "tokens": ["Dein", "zaub\u00b7risch", "wun\u00b7der\u00b7ba\u00b7res", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sch\u00f6nen Z\u00fcge noch versch\u00f6nt!", "tokens": ["Die", "sch\u00f6\u00b7nen", "Z\u00fc\u00b7ge", "noch", "ver\u00b7sch\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wenn dir einst diese Reize schwinden,", "tokens": ["Wenn", "dir", "einst", "die\u00b7se", "Rei\u00b7ze", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geraubt vom neidischen Geschick,", "tokens": ["Ge\u00b7raubt", "vom", "nei\u00b7di\u00b7schen", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn bleich sich deine Locken winden,", "tokens": ["Wenn", "bleich", "sich", "dei\u00b7ne", "Lo\u00b7cken", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dann blick in meine Brust zur\u00fcck!", "tokens": ["Dann", "blick", "in", "mei\u00b7ne", "Brust", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da lebt dein Bild f\u00fcr Ewigkeiten", "tokens": ["Da", "lebt", "dein", "Bild", "f\u00fcr", "E\u00b7wig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner jetz'gen Sch\u00f6nheit fort,", "tokens": ["In", "sei\u00b7ner", "jetz'\u00b7gen", "Sch\u00f6n\u00b7heit", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erreicht nicht von dem Sturm der Zeiten,", "tokens": ["Er\u00b7reicht", "nicht", "von", "dem", "Sturm", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn mein Empfinden ist sein Hort.", "tokens": ["Denn", "mein", "Emp\u00b7fin\u00b7den", "ist", "sein", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn Wonnen mein Gem\u00fcth durchsch\u00fcttern", "tokens": ["Wenn", "Won\u00b7nen", "mein", "Ge\u00b7m\u00fcth", "durch\u00b7sch\u00fct\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat dieses Bild sie angefacht;", "tokens": ["Hat", "die\u00b7ses", "Bild", "sie", "an\u00b7ge\u00b7facht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Thr\u00e4nen mir im Auge zittern,", "tokens": ["Wenn", "Thr\u00e4\u00b7nen", "mir", "im", "Au\u00b7ge", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So sind sie von dem Bild erwacht!", "tokens": ["So", "sind", "sie", "von", "dem", "Bild", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}