{"textgrid.poem.48630": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "43. Als die F\u00fcrstl. Holstein. Gesandten mit dero Komitate von Moskaw nacher Persien aufbrachen, 1636. im Junio", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steh' auf, steh' auf aus Thetis feuchten Armen,", "tokens": ["Steh'", "auf", ",", "steh'", "auf", "aus", "The\u00b7tis", "feuch\u00b7ten", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "o g\u00fcldner Phaeton,", "tokens": ["o", "g\u00fcld\u00b7ner", "Phae\u00b7ton", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "steh' auf und la\u00df von deiner Glut erwarmen", "tokens": ["steh'", "auf", "und", "la\u00df", "von", "dei\u00b7ner", "Glut", "er\u00b7war\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVIMP", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Olympens ganzen Thron!", "tokens": ["O\u00b7lym\u00b7pens", "gan\u00b7zen", "Thron", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verhalt uns nicht", "tokens": ["Ver\u00b7halt", "uns", "nicht"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "dein Lebens-Licht!", "tokens": ["dein", "Le\u00b7bens\u00b7Licht", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "La\u00df Sturm und Wind und Regen f\u00fcr dir fallen,", "tokens": ["La\u00df", "Sturm", "und", "Wind", "und", "Re\u00b7gen", "f\u00fcr", "dir", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "indem wir dir,", "tokens": ["in\u00b7dem", "wir", "dir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "o unsre Zier,", "tokens": ["o", "uns\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "ein Ehrenlied zu Lobe lassen schallen!", "tokens": ["ein", "Eh\u00b7ren\u00b7lied", "zu", "Lo\u00b7be", "las\u00b7sen", "schal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Di\u00df ist der Tag nach so viel hundert Tagen,", "tokens": ["Di\u00df", "ist", "der", "Tag", "nach", "so", "viel", "hun\u00b7dert", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die uns beschwerlich sind,", "tokens": ["die", "uns", "be\u00b7schwer\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "zu dem wir einst mit Jauchzen k\u00f6nnen sagen:", "tokens": ["zu", "dem", "wir", "einst", "mit", "Jauch\u00b7zen", "k\u00f6n\u00b7nen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Willkommen, edles Kind!", "tokens": ["Will\u00b7kom\u00b7men", ",", "ed\u00b7les", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein s\u00fc\u00dfer Glanz", "tokens": ["Dein", "s\u00fc\u00b7\u00dfer", "Glanz"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "erfreut uns ganz.", "tokens": ["er\u00b7freut", "uns", "ganz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Durch dich steht uns der Himmel offen;", "tokens": ["Durch", "dich", "steht", "uns", "der", "Him\u00b7mel", "of\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "das sch\u00f6ne Tun", "tokens": ["das", "sch\u00f6\u00b7ne", "Tun"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "gew\u00e4hrst du nun,", "tokens": ["ge\u00b7w\u00e4hrst", "du", "nun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "auf welches wir so manche Jahre hoffen.", "tokens": ["auf", "wel\u00b7ches", "wir", "so", "man\u00b7che", "Jah\u00b7re", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Apollo, la\u00df dein gn\u00e4digs Antlitz schauen", "tokens": ["A\u00b7pol\u00b7lo", ",", "la\u00df", "dein", "gn\u00e4\u00b7digs", "Ant\u00b7litz", "schau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVIMP", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf uns und unsern Pfad!", "tokens": ["auf", "uns", "und", "un\u00b7sern", "Pfad", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erw\u00e4ge wol, was f\u00fcr ein hoch Vertrauen", "tokens": ["Er\u00b7w\u00e4\u00b7ge", "wol", ",", "was", "f\u00fcr", "ein", "hoch", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PRELS", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ganz Holstein in dich hat!", "tokens": ["ganz", "Hol\u00b7stein", "in", "dich", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Di\u00df edle Paar", "tokens": ["Di\u00df", "ed\u00b7le", "Paar"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "und seine Schaar", "tokens": ["und", "sei\u00b7ne", "Schaar"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "hat deiner Gunst, o Kunst, sich ganz ergeben.", "tokens": ["hat", "dei\u00b7ner", "Gunst", ",", "o", "Kunst", ",", "sich", "ganz", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "FM", "NN", "$,", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kein Fall verr\u00fcckts.", "tokens": ["Kein", "Fall", "ver\u00b7r\u00fcckts", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wilst du, so gl\u00fcckts,", "tokens": ["Wilst", "du", ",", "so", "gl\u00fcckts", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADV", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "so hats nicht Not um unser k\u00fchnes Leben.", "tokens": ["so", "hats", "nicht", "Not", "um", "un\u00b7ser", "k\u00fch\u00b7nes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lauf, Moskaw, lauf und sag' es deiner Wolgen", "tokens": ["Lauf", ",", "Mos\u00b7kaw", ",", "lauf", "und", "sag'", "es", "dei\u00b7ner", "Wol\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "NN", "KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit schnellen Wellen an,", "tokens": ["mit", "schnel\u00b7len", "Wel\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da\u00df wir alsbald ihr sind bereit zu folgen", "tokens": ["da\u00df", "wir", "als\u00b7bald", "ihr", "sind", "be\u00b7reit", "zu", "fol\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VAFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "bis \u00fcber den Hyrkan!", "tokens": ["bis", "\u00fc\u00b7ber", "den", "Hyr\u00b7kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Indessen sprich,", "tokens": ["In\u00b7des\u00b7sen", "sprich", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "da\u00df alles sich,", "tokens": ["da\u00df", "al\u00b7les", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "was Schaden bringt, von ihren Ufern mache!", "tokens": ["was", "Scha\u00b7den", "bringt", ",", "von", "ih\u00b7ren", "U\u00b7fern", "ma\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "La\u00df unsren Lauf", "tokens": ["La\u00df", "un\u00b7sren", "Lauf"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "nichts halten auf!", "tokens": ["nichts", "hal\u00b7ten", "auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Der alles sieht, h\u00e4lt \u00fcber uns selbst Wache.", "tokens": ["Der", "al\u00b7les", "sieht", ",", "h\u00e4lt", "\u00fc\u00b7ber", "uns", "selbst", "Wa\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "VVFIN", "APPR", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und nun Ade, ihr Reu\u00dfischen Najaden,", "tokens": ["Und", "nun", "A\u00b7de", ",", "ihr", "Reu\u00b7\u00dfi\u00b7schen", "Na\u00b7ja\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "du wolbekante Schaar!", "tokens": ["du", "wol\u00b7be\u00b7kan\u00b7te", "Schaar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein tr\u00fcber Quell soll euren Fl\u00fcssen schaden", "tokens": ["Kein", "tr\u00fc\u00b7ber", "Quell", "soll", "eu\u00b7ren", "Fl\u00fcs\u00b7sen", "scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "durch dieses lange Jahr!", "tokens": ["durch", "die\u00b7ses", "lan\u00b7ge", "Jahr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bleibt ihr uns Freund,", "tokens": ["Bleibt", "ihr", "uns", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "so wagts kein Feind.", "tokens": ["so", "wagts", "kein", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und du, o Stadt, o gro\u00dfe, bleib bei Gl\u00fccke!", "tokens": ["Und", "du", ",", "o", "Stadt", ",", "o", "gro\u00b7\u00dfe", ",", "bleib", "bei", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "FM", "ADJA", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nun, gute Nacht!", "tokens": ["Nun", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Hats Gott bedacht,", "tokens": ["Hats", "Gott", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "so scheiden wir und kommen wol zur\u00fccke.", "tokens": ["so", "schei\u00b7den", "wir", "und", "kom\u00b7men", "wol", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}