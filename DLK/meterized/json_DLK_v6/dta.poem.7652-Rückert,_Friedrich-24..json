{"dta.poem.7652": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum gehst in der Welt du aus dir selbst hinaus?", "tokens": ["Wa\u00b7rum", "gehst", "in", "der", "Welt", "du", "aus", "dir", "selbst", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ART", "NN", "PPER", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Um still in dich zur\u00fcck zu kehren aus dem Braus.", "tokens": ["Um", "still", "in", "dich", "zu\u00b7r\u00fcck", "zu", "keh\u00b7ren", "aus", "dem", "Braus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und warum aus dem Braus gehst du in dich zur\u00fcck?", "tokens": ["Und", "wa\u00b7rum", "aus", "dem", "Braus", "gehst", "du", "in", "dich", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu sinnen f\u00fcr die Welt im Stillen Lust und Gl\u00fcck.", "tokens": ["Zu", "sin\u00b7nen", "f\u00fcr", "die", "Welt", "im", "Stil\u00b7len", "Lust", "und", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Begl\u00fcckt, wenn dir die Welt gibt, was du brauchen kannst,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wenn", "dir", "die", "Welt", "gibt", ",", "was", "du", "brau\u00b7chen", "kannst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und brauchen will die Welt, was du f\u00fcr sie ersannst.", "tokens": ["Und", "brau\u00b7chen", "will", "die", "Welt", ",", "was", "du", "f\u00fcr", "sie", "er\u00b7sannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ART", "NN", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}