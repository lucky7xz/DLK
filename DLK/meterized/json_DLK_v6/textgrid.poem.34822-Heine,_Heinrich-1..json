{"textgrid.poem.34822": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entflieh mit mir und sei mein Weib,", "tokens": ["Ent\u00b7flieh", "mit", "mir", "und", "sei", "mein", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ruh an meinem Herzen aus;", "tokens": ["Und", "ruh", "an", "mei\u00b7nem", "Her\u00b7zen", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fern in der Fremde sei mein Herz", "tokens": ["Fern", "in", "der", "Frem\u00b7de", "sei", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dein Vaterland und Vaterhaus.", "tokens": ["Dein", "Va\u00b7ter\u00b7land", "und", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gehst du nicht mit, so sterb ich hier", "tokens": ["Gehst", "du", "nicht", "mit", ",", "so", "sterb", "ich", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und du bist einsam und allein;", "tokens": ["Und", "du", "bist", "ein\u00b7sam", "und", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bleibst du auch im Vaterhaus,", "tokens": ["Und", "bleibst", "du", "auch", "im", "Va\u00b7ter\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wirst doch wie in der Fremde sein.", "tokens": ["Wirst", "doch", "wie", "in", "der", "Frem\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}