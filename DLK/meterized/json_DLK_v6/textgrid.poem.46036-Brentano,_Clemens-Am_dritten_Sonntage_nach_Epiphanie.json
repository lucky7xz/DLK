{"textgrid.poem.46036": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Am dritten Sonntage nach Epiphanie", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steig, o Herr, vom Berg hernieder,", "tokens": ["Steig", ",", "o", "Herr", ",", "vom", "Berg", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streck die Hand aus, mach mich rein.", "tokens": ["Streck", "die", "Hand", "aus", ",", "mach", "mich", "rein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr! ich glaub', heil' meine Glieder,", "tokens": ["Herr", "!", "ich", "glaub'", ",", "heil'", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df des Hauptmanns Knecht mich sein.", "tokens": ["La\u00df", "des", "Haupt\u00b7manns", "Knecht", "mich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "NN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein nicht w\u00fcrdig ist die Decke", "tokens": ["Dein", "nicht", "w\u00fcr\u00b7dig", "ist", "die", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "PTKNEG", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Hauses, ist zu schlecht,", "tokens": ["Mei\u00b7nes", "Hau\u00b7ses", ",", "ist", "zu", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprich ein Wort nur, und erwecke", "tokens": ["Sprich", "ein", "Wort", "nur", ",", "und", "er\u00b7we\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADV", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zur Gesundheit deinen Knecht.", "tokens": ["Zur", "Ge\u00b7sund\u00b7heit", "dei\u00b7nen", "Knecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u00bbnach dem Glauben dir geschehe!\u00ab \u2013", "tokens": ["\u00bb", "nach", "dem", "Glau\u00b7ben", "dir", "ge\u00b7sche\u00b7he", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "PPER", "VVFIN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Glauben, den Er selten fand, \u2013", "tokens": ["Glau\u00b7ben", ",", "den", "Er", "sel\u00b7ten", "fand", ",", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herr, den Glauben mir erh\u00f6he,", "tokens": ["Herr", ",", "den", "Glau\u00b7ben", "mir", "er\u00b7h\u00f6\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sto\u00df mich nicht ins finstre Land!", "tokens": ["Sto\u00df", "mich", "nicht", "ins", "finst\u00b7re", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}