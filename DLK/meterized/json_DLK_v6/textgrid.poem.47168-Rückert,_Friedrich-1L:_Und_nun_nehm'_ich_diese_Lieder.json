{"textgrid.poem.47168": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und nun nehm' ich diese Lieder", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun nehm' ich diese Lieder", "tokens": ["Und", "nun", "nehm'", "ich", "die\u00b7se", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die Hand zum letztenmal,", "tokens": ["In", "die", "Hand", "zum", "letz\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im klaren Spiegel wieder", "tokens": ["Und", "im", "kla\u00b7ren", "Spie\u00b7gel", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' ich meiner Jugend Strahl,", "tokens": ["Seh'", "ich", "mei\u00b7ner", "Ju\u00b7gend", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Blumen meines Liebefr\u00fchlings ohne Zahl.", "tokens": ["Die", "Blu\u00b7men", "mei\u00b7nes", "Lie\u00b7be\u00b7fr\u00fch\u00b7lings", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Aller Glanz darin vereinigt,", "tokens": ["Al\u00b7ler", "Glanz", "da\u00b7rin", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PAV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch die Schatten fehlen nicht;", "tokens": ["Auch", "die", "Schat\u00b7ten", "feh\u00b7len", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch die \u00e4u\u00dfern Tr\u00fcben reinigt", "tokens": ["Doch", "die", "\u00e4u\u00b7\u00dfern", "Tr\u00fc\u00b7ben", "rei\u00b7nigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein im Innern wirksam Licht,", "tokens": ["Ein", "im", "In\u00b7nern", "wirk\u00b7sam", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Wirkung \u00fcberlass' ich Leben und Gedicht.", "tokens": ["Der", "Wir\u00b7kung", "\u00fc\u00b7ber\u00b7lass'", "ich", "Le\u00b7ben", "und", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Vollendetes hienieden", "tokens": ["Ein", "Voll\u00b7en\u00b7de\u00b7tes", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wird nie dem Vollendungsdrang,", "tokens": ["Wird", "nie", "dem", "Vol\u00b7len\u00b7dungs\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch die Seel' ist nur zufrieden,", "tokens": ["Doch", "die", "Seel'", "ist", "nur", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sie nach Vollendung rang;", "tokens": ["Wenn", "sie", "nach", "Vol\u00b7len\u00b7dung", "rang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Ich bin mit dem zufrieden, was ich lebt' und sang.", "tokens": ["Ich", "bin", "mit", "dem", "zu\u00b7frie\u00b7den", ",", "was", "ich", "lebt'", "und", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJD", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}