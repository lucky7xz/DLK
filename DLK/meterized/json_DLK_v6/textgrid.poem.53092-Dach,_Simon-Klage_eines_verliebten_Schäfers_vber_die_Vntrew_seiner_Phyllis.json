text": "Was wil ich Armer aber machen?", "tokens": ["Was", "wil", "ich", "Ar\u00b7mer", "a\u00b7ber", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lass' es jetzt also geschehn;", "tokens": ["Ich", "lass'", "es", "jetzt", "al\u00b7so", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, wie sie vor-steh' jhren Sachen,", "tokens": ["Doch", ",", "wie", "sie", "vor\u00b7steh'", "jhren", "Sa\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das wird Sie selbst in kurtzen sehn:", "tokens": ["Das", "wird", "Sie", "selbst", "in", "kurt\u00b7zen", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Frefel so vntrewer Thaten", "tokens": ["Der", "Fre\u00b7fel", "so", "vntre\u00b7wer", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Pflegt selten gl\u00fccklich zu gerahten.", "tokens": ["Pflegt", "sel\u00b7ten", "gl\u00fcck\u00b7lich", "zu", "ge\u00b7rah\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Nicht, da\u00df Ihr Vngl\u00f