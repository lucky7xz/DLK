{"textgrid.poem.53092": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Klage eines verliebten Sch\u00e4fers vber die Vntrew seiner Phyllis", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fieng ein Sch\u00e4fer an zu klagen:", "tokens": ["Es", "fi\u00b7eng", "ein", "Sch\u00e4\u00b7fer", "an", "zu", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie seine liebste Phillis Ihn", "tokens": ["Wie", "sei\u00b7ne", "liebs\u00b7te", "Phil\u00b7lis", "Ihn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NE", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch liebgehabt vor wenig Tagen,", "tokens": ["Noch", "lieb\u00b7ge\u00b7habt", "vor", "we\u00b7nig", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd nun geschlossen aus dem Sinn,", "tokens": ["Vnd", "nun", "ge\u00b7schlos\u00b7sen", "aus", "dem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch jhren sch\u00f6nen Krantz von Myrthen", "tokens": ["Auch", "jhren", "sch\u00f6\u00b7nen", "Krantz", "von", "Myr\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gegeben einem andern Hirten.", "tokens": ["Ge\u00b7ge\u00b7ben", "ei\u00b7nem", "an\u00b7dern", "Hir\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er senckt' in\u00df Gra\u00df die matten Glieder", "tokens": ["Er", "sen\u00b7ckt'", "in\u00df", "Gra\u00df", "die", "mat\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bey einer Silber-klahren Bach,", "tokens": ["Bey", "ei\u00b7ner", "Sil\u00b7ber\u00b7klah\u00b7ren", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd warff da Stab vnd Tasche nieder,", "tokens": ["Vnd", "warff", "da", "Stab", "vnd", "Ta\u00b7sche", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Irden Trinck-geschirr zerbrach,", "tokens": ["Sein", "Ir\u00b7den", "Trin\u00b7ck\u00b7ge\u00b7schirr", "zer\u00b7brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Verflucht' auch seine Wald-Schalmeyen", "tokens": ["Ver\u00b7flucht'", "auch", "sei\u00b7ne", "Wald\u00b7Schal\u00b7meyen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd fieng erb\u00e4rmblich an zu schreyen:", "tokens": ["Vnd", "fi\u00b7eng", "er\u00b7b\u00e4rmb\u00b7lich", "an", "zu", "schre\u00b7yen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Numehr werd' ich doch nimmer singen,", "tokens": ["Nu\u00b7mehr", "werd'", "ich", "doch", "nim\u00b7mer", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich dancke meinem Leben ab!", "tokens": ["Ich", "dan\u00b7cke", "mei\u00b7nem", "Le\u00b7ben", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dich, O s\u00fcsses Seiten-zwingen,", "tokens": ["Vor", "dich", ",", "O", "s\u00fcs\u00b7ses", "Sei\u00b7ten\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00e4hl' ich mir ein Todten-Grab,", "tokens": ["Er\u00b7w\u00e4hl'", "ich", "mir", "ein", "Tod\u00b7ten\u00b7Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In welchem ich auch noch vor Morgen", "tokens": ["In", "wel\u00b7chem", "ich", "auch", "noch", "vor", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wil schliessen alle Pein vnd Sorgen.", "tokens": ["Wil", "schlies\u00b7sen", "al\u00b7le", "Pein", "vnd", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr Sternen, ewig' Himmels Liechter,", "tokens": ["Ihr", "Ster\u00b7nen", ",", "e\u00b7wig'", "Him\u00b7mels", "Liech\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jhr den Krei\u00df der Welt durchrennt,", "tokens": ["Die", "jhr", "den", "Krei\u00df", "der", "Welt", "durch\u00b7rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seyd zwischen mir vnd Phyllis Richter!", "tokens": ["Seyd", "zwi\u00b7schen", "mir", "vnd", "Phyl\u00b7lis", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "APPR", "PPER", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwegt es bey euch vnd erkennt:", "tokens": ["Er\u00b7wegt", "es", "bey", "euch", "vnd", "er\u00b7kennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob ich mit Recht mein JungesLeben", "tokens": ["Ob", "ich", "mit", "Recht", "mein", "Jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vmb jhrentwillen auff-soll geben?", "tokens": ["Vmb", "jhrent\u00b7wil\u00b7len", "auf\u00b7fsoll", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hab' ich Sie nicht, wie meine Seele,", "tokens": ["Hab'", "ich", "Sie", "nicht", ",", "wie", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja noch viel hertzlicher geliebt,", "tokens": ["Ja", "noch", "viel", "hertz\u00b7li\u00b7cher", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sey auch in des Grabes H\u00f6le", "tokens": ["So", "sey", "auch", "in", "des", "Gra\u00b7bes", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was mich viel mehr als jezt betr\u00fcebt!", "tokens": ["Was", "mich", "viel", "mehr", "als", "jezt", "be\u00b7tr\u00fcebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PIS", "KOKOM", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hab' ich Ihr nicht mein Hertz verpf\u00e4ndet,", "tokens": ["Hab'", "ich", "Ihr", "nicht", "mein", "Hertz", "ver\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So werde nie mein Leid geendet!", "tokens": ["So", "wer\u00b7de", "nie", "mein", "Leid", "ge\u00b7en\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr Hirten, die jhr ferne weidet,", "tokens": ["Ihr", "Hir\u00b7ten", ",", "die", "jhr", "fer\u00b7ne", "wei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Ficht-vnd Linden-Ba\u00fcm' allhie,", "tokens": ["Ihr", "Ficht\u00b7\u00b7vnd", "Lin\u00b7den\u00b7Ba\u00fcm'", "all\u00b7hie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Quellen, die jhr vnterscheidet", "tokens": ["Ihr", "Quel\u00b7len", ",", "die", "jhr", "vn\u00b7ter\u00b7schei\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier zwischen mein-vnd jenem Vieh',", "tokens": ["Hier", "zwi\u00b7schen", "mein\u00b7\u00b7vnd", "je\u00b7nem", "Vieh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr K\u00f6nnet, wollet jhr nur, zeugen,", "tokens": ["Ihr", "K\u00f6n\u00b7net", ",", "wol\u00b7let", "jhr", "nur", ",", "zeu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADV", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ich gewesen jhr Leib-eigen!", "tokens": ["Wie", "ich", "ge\u00b7we\u00b7sen", "jhr", "Leib\u00b7ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich habe meiner selbst vergessen,", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7ner", "selbst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd einig nur auff Sie geschawt,", "tokens": ["Vnd", "ei\u00b7nig", "nur", "auff", "Sie", "ge\u00b7schawt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bin Schlaff-lohs manche Nacht gesessen,", "tokens": ["Bin", "Schlaff\u00b7lohs", "man\u00b7che", "Nacht", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erfrohren vnd gar nass bethawt,", "tokens": ["Er\u00b7froh\u00b7ren", "vnd", "gar", "nass", "be\u00b7thawt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd nur gespielt, da\u00df ich f\u00fcr allen", "tokens": ["Vnd", "nur", "ge\u00b7spielt", ",", "da\u00df", "ich", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "KOUS", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr m\u00f6cht' im Hertzen wolgefallen.", "tokens": ["Ihr", "m\u00f6cht'", "im", "Hert\u00b7zen", "wol\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was ich ie gutes hab' erzwungen,", "tokens": ["Was", "ich", "ie", "gu\u00b7tes", "hab'", "er\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJA", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward Ihr zu Ehren angewandt,", "tokens": ["Ward", "Ihr", "zu", "Eh\u00b7ren", "an\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr hab' ich es zu gut gesungen,", "tokens": ["Ihr", "hab'", "ich", "es", "zu", "gut", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was von mir in der Welt bekant;", "tokens": ["Was", "von", "mir", "in", "der", "Welt", "be\u00b7kant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich liebten andre Sch\u00e4ferinnen,", "tokens": ["Mich", "lieb\u00b7ten", "and\u00b7re", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie aber war nicht zu gewinnen.", "tokens": ["Sie", "a\u00b7ber", "war", "nicht", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Man schaw' auff mein' vnd ihre Heerde,", "tokens": ["Man", "scha\u00b7w'", "auff", "mein'", "vnd", "ih\u00b7re", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was f\u00fcr ein gro\u00dfer Vnterscheid", "tokens": ["Was", "f\u00fcr", "ein", "gro\u00b7\u00dfer", "Vn\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darunder doch begriffen werde!", "tokens": ["Da\u00b7run\u00b7der", "doch", "be\u00b7grif\u00b7fen", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd dieses in gar kurtzer Zeit!", "tokens": ["Vnd", "die\u00b7ses", "in", "gar", "kurt\u00b7zer", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil ich nur Ihr Vieh' au\u00dfgetrieben,", "tokens": ["Weil", "ich", "nur", "Ihr", "Vieh'", "au\u00df\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Mein Eigenthumb nie k\u00f6nnen lieben.", "tokens": ["Mein", "Ei\u00b7gen\u00b7thumb", "nie", "k\u00f6n\u00b7nen", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was hab' ich mir nicht vnverdrossen", "tokens": ["Was", "hab'", "ich", "mir", "nicht", "vn\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entzogen, vnd Ihr zu-gesteckt?", "tokens": ["Ent\u00b7zo\u00b7gen", ",", "vnd", "Ihr", "zu\u00b7ge\u00b7steckt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat stets meiner Milch genossen,", "tokens": ["Sie", "hat", "stets", "mei\u00b7ner", "Milch", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meiner Wolle sich gedeckt,", "tokens": ["Mit", "mei\u00b7ner", "Wol\u00b7le", "sich", "ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Sie nur m\u00f6chte zierlich prangen,", "tokens": ["Da\u00df", "Sie", "nur", "m\u00f6ch\u00b7te", "zier\u00b7lich", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bin ich offt nackt vnd blo\u00df gegangen.", "tokens": ["Bin", "ich", "offt", "nackt", "vnd", "blo\u00df", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stan