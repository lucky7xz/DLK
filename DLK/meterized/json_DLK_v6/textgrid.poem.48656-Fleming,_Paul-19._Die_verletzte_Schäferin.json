"trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Regen, St\u00fcrme, Schnee und Schein", "tokens": ["Re\u00b7gen", ",", "St\u00fcr\u00b7me", ",", "Schnee", "und", "Schein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sagen, da\u00df sie fl\u00fcchtig sein;", "tokens": ["sa\u00b7gen", ",", "da\u00df", "sie", "fl\u00fcch\u00b7tig", "sein", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "KOUS", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glut und Luft und Flut und Erden", "tokens": ["Glut", "und", "Luft", "und", "Flut", "und", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sind stets nichts, da\u00df sie was werden.", "tokens": ["sind", "stets", "nichts", ",", "da\u00df", "sie", "was", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$,", "KOUS", "PPER", "PIS", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Bittre Freude, s\u00fc\u00dfes Leid,", "tokens": ["Bitt\u00b7re", "Freu\u00b7de", ",", "s\u00fc\u00b7\u00dfes", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "was ists, das bleibt allezeit?", "tokens": ["was", "ists", ",", "das", "bleibt", "al\u00b7le\u00b7zeit", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PDS", "VVFIN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri