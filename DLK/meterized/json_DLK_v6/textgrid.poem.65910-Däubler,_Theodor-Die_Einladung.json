{"textgrid.poem.65910": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die Einladung", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wolken glitzern um die h\u00f6chste Stunde,", "tokens": ["Die", "Wol\u00b7ken", "glit\u00b7zern", "um", "die", "h\u00f6chs\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein j\u00fcngster Wind versilbert sacht den Wald;", "tokens": ["Ein", "j\u00fcngs\u00b7ter", "Wind", "ver\u00b7sil\u00b7bert", "sacht", "den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die \u00d6lb\u00e4ume bekennen sich zur Gottgewalt", "tokens": ["Die", "\u00d6l\u00b7b\u00e4u\u00b7me", "be\u00b7ken\u00b7nen", "sich", "zur", "Gott\u00b7ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zittern von Athenas Nahen Kunde.", "tokens": ["Und", "zit\u00b7tern", "von", "A\u00b7the\u00b7nas", "Na\u00b7hen", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ihr Silberwispern frommt aus hehrem Munde;", "tokens": ["Ihr", "Sil\u00b7ber\u00b7wis\u00b7pern", "frommt", "aus", "heh\u00b7rem", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch \u2013 Demeter vernehmbar blo\u00df \u2013 erschallt", "tokens": ["Doch", "\u2013", "De\u00b7me\u00b7ter", "ver\u00b7nehm\u00b7bar", "blo\u00df", "\u2013", "er\u00b7schallt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$(", "NN", "ADJD", "ADV", "$(", "VVPP"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ein Ruf der Zeus-Entragten, als Gestalt:", "tokens": ["Ein", "Ruf", "der", "Zeus\u00b7En\u00b7trag\u00b7ten", ",", "als", "Ge\u00b7stalt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Begl\u00fcck Athen, Erkorene zum Bunde!", "tokens": ["Be\u00b7gl\u00fcck", "A\u00b7then", ",", "Er\u00b7ko\u00b7re\u00b7ne", "zum", "Bun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Da l\u00f6st von Kore sich die gro\u00dfe Mutter,", "tokens": ["Da", "l\u00f6st", "von", "Ko\u00b7re", "sich", "die", "gro\u00b7\u00dfe", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehorcht der Tochter des Kroniden schlicht;", "tokens": ["Ge\u00b7horcht", "der", "Toch\u00b7ter", "des", "Kro\u00b7ni\u00b7den", "schlicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Fuhren folgt ihr hochgestuftes Futter.", "tokens": ["In", "Fuh\u00b7ren", "folgt", "ihr", "hoch\u00b7ge\u00b7stuf\u00b7tes", "Fut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Viel Korn kommt an, auch \u00dcberschwang an Butter:", "tokens": ["Viel", "Korn", "kommt", "an", ",", "auch", "\u00dc\u00b7bersc\u00b7hwang", "an", "But\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Stadt enth\u00fcllt sich Demeters Gesicht,", "tokens": ["Der", "Stadt", "ent\u00b7h\u00fcllt", "sich", "De\u00b7me\u00b7ters", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "F\u00fcr Opfer nickt sie \u2013 tiefumfleht \u2013 Verzicht.", "tokens": ["F\u00fcr", "Op\u00b7fer", "nickt", "sie", "\u2013", "tie\u00b7fumf\u00b7leht", "\u2013", "Ver\u00b7zicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$(", "VVFIN", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}