{"textgrid.poem.58903": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "122. Eitelkeit der Eitelkeiten", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eitel ist doch alles nicht,", "tokens": ["Wie", "ei\u00b7tel", "ist", "doch", "al\u00b7les", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Gott nicht ist und zu Gott f\u00fchret!", "tokens": ["Was", "Gott", "nicht", "ist", "und", "zu", "Gott", "f\u00fch\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PTKNEG", "VAFIN", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist nicht wert, da\u00df man's ansicht", "tokens": ["Es", "ist", "nicht", "wert", ",", "da\u00df", "man's", "an\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Zeit damit verlieret.", "tokens": ["Und", "sei\u00b7ne", "Zeit", "da\u00b7mit", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}