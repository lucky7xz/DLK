{"dta.poem.11649": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Charis und Penia.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Seht doch den Schl\u00e4fer dort in's Gras gestreckt!", "tokens": ["Seht", "doch", "den", "Schl\u00e4\u00b7fer", "dort", "in's", "Gras", "ge\u00b7streckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es ist des Gauklers Sohn, der sch\u00f6ne Knabe,", "tokens": ["Es", "ist", "des", "Gauk\u00b7lers", "Sohn", ",", "der", "sch\u00f6\u00b7ne", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den gestern wir so lieblich tanzen sahn.", "tokens": ["Den", "ge\u00b7stern", "wir", "so", "lieb\u00b7lich", "tan\u00b7zen", "sahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "F\u00fcr jezt das bunte J\u00e4ckchen abgeworfen,", "tokens": ["F\u00fcr", "jezt", "das", "bun\u00b7te", "J\u00e4ck\u00b7chen", "ab\u00b7ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Schatten suchend vor der Mittagsschw\u00fcle,", "tokens": ["Den", "Schat\u00b7ten", "su\u00b7chend", "vor", "der", "Mit\u00b7tags\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Warf er sich in des Wirthes Garten, faul.", "tokens": ["Warf", "er", "sich", "in", "des", "Wirt\u00b7hes", "Gar\u00b7ten", ",", "faul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.7": {"text": "Hier unter den Syringenbusch.", "tokens": ["Hier", "un\u00b7ter", "den", "Sy\u00b7rin\u00b7gen\u00b7busch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}