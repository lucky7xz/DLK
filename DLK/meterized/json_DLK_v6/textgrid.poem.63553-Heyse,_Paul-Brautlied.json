{"textgrid.poem.63553": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Brautlied", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Scheiden ist seliger,", "tokens": ["Welch", "ein", "Schei\u00b7den", "ist", "se\u00b7li\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Als zu scheiden von M\u00e4dchentagen?", "tokens": ["Als", "zu", "schei\u00b7den", "von", "M\u00e4d\u00b7chen\u00b7ta\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKA", "ADJD", "APPR", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Welch ein Klagen ist fr\u00f6hlicher,", "tokens": ["Welch", "ein", "Kla\u00b7gen", "ist", "fr\u00f6h\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Als in Myrten um Veilchen klagen?", "tokens": ["Als", "in", "Myr\u00b7ten", "um", "Veil\u00b7chen", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Als dein Schifflein im Hafen lag,", "tokens": ["Als", "dein", "Schif\u00b7flein", "im", "Ha\u00b7fen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Meerw\u00e4rts oft sich die Wimpel regten,", "tokens": ["Meer\u00b7w\u00e4rts", "oft", "sich", "die", "Wim\u00b7pel", "reg\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Ob auch heimischer Wellenschlag,", "tokens": ["Ob", "auch", "hei\u00b7mi\u00b7scher", "Wel\u00b7len\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Land und Himmel es treulich hegten.", "tokens": ["Land", "und", "Him\u00b7mel", "es", "treu\u00b7lich", "heg\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun die Anker gelichtet sind,", "tokens": ["Nun", "die", "An\u00b7ker", "ge\u00b7lich\u00b7tet", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "O wie k\u00f6stlich die Fahrt ins Weite!", "tokens": ["O", "wie", "k\u00f6st\u00b7lich", "die", "Fahrt", "ins", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "D\u00fcfte schwimmen im Fr\u00fchlingswind,", "tokens": ["D\u00fcf\u00b7te", "schwim\u00b7men", "im", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Und du l\u00e4chelst an ", "tokens": ["Und", "du", "l\u00e4\u00b7chelst", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Manch ein segnender Seufzer schwingt", "tokens": ["Manch", "ein", "seg\u00b7nen\u00b7der", "Seuf\u00b7zer", "schwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Sich ins Segel, es lind zu schwellen.", "tokens": ["Sich", "ins", "Se\u00b7gel", ",", "es", "lind", "zu", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "La\u00df dies Lied, das die Liebe singt,", "tokens": ["La\u00df", "dies", "Lied", ",", "das", "die", "Lie\u00b7be", "singt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDS", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sich als g\u00fcnstigen Hauch gesellen!", "tokens": ["Sich", "als", "g\u00fcns\u00b7ti\u00b7gen", "Hauch", "ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOUS", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}