{"dta.poem.13271": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201evermeinst du, K\u00f6niginn! die Tugenden verm\u00f6gen", "tokens": ["\u201e", "ver\u00b7meinst", "du", ",", "K\u00f6\u00b7ni\u00b7ginn", "!", "die", "Tu\u00b7gen\u00b7den", "ver\u00b7m\u00f6\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edem P\u00f6bel einen Zaum an seinen Wahn zu legen?", "tokens": ["\u201e", "dem", "P\u00f6\u00b7bel", "ei\u00b7nen", "Zaum", "an", "sei\u00b7nen", "Wahn", "zu", "le\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ees fliegt die Fledermaus mit Ha\u00df im Tag herum;", "tokens": ["\u201e", "es", "fliegt", "die", "Fle\u00b7der\u00b7maus", "mit", "Ha\u00df", "im", "Tag", "he\u00b7rum", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie Spinne l\u00e4\u00dft ihr Gift auch auf die sch\u00f6nste Bluhm;", "tokens": ["\u201e", "die", "Spin\u00b7ne", "l\u00e4\u00dft", "ihr", "Gift", "auch", "auf", "die", "sch\u00f6ns\u00b7te", "Bluhm", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "515\u201dKurz: er verschm\u00e4ht und ha\u00dft, sch\u00e4zt und verlangt die Sachen,", "tokens": ["\"", "Kurz", ":", "er", "ver\u00b7schm\u00e4ht", "und", "ha\u00dft", ",", "sch\u00e4zt", "und", "ver\u00b7langt", "die", "Sa\u00b7chen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "PPER", "VVPP", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201enicht wie sie selber seynd; wie er sie pflegt zu machen.", "tokens": ["\u201e", "nicht", "wie", "sie", "sel\u00b7ber", "seynd", ";", "wie", "er", "sie", "pflegt", "zu", "ma\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PWAV", "PPER", "ADV", "VAFIN", "$.", "PWAV", "PPER", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ees kommt zu Zeiten ihm ein Ferngla\u00df in die Hand,", "tokens": ["\u201e", "es", "kommt", "zu", "Zei\u00b7ten", "ihm", "ein", "Fern\u00b7gla\u00df", "in", "die", "Hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201emit diesem schauet er auf seinen Gegenstand;", "tokens": ["\u201e", "mit", "die\u00b7sem", "schau\u00b7et", "er", "auf", "sei\u00b7nen", "Ge\u00b7gen\u00b7stand", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eweil aber sein Gesicht die Seh-Kunst nicht verstehet,", "tokens": ["\u201e", "weil", "a\u00b7ber", "sein", "Ge\u00b7sicht", "die", "Seh\u00b7Kunst", "nicht", "ver\u00b7ste\u00b7het", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "520\u201dDas Augen-Rohr verkehrt nach seiner Absicht drehet;", "tokens": ["\"", "Das", "Au\u00b7gen\u00b7Rohr", "ver\u00b7kehrt", "nach", "sei\u00b7ner", "Ab\u00b7sicht", "dre\u00b7het", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eso scheint der gr\u00f6\u00dfte Thurn in seinem Auge klein,", "tokens": ["\u201e", "so", "scheint", "der", "gr\u00f6\u00df\u00b7te", "Thurn", "in", "sei\u00b7nem", "Au\u00b7ge", "klein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eund da\u00df, was nahe steht, entfernet m\u00fcsse seyn.", "tokens": ["\u201e", "und", "da\u00df", ",", "was", "na\u00b7he", "steht", ",", "ent\u00b7fer\u00b7net", "m\u00fcs\u00b7se", "seyn", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VVFIN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eder P\u00f6bel ist ein Rath, der stets mit Blindheit w\u00e4hlet;", "tokens": ["\u201e", "der", "P\u00f6\u00b7bel", "ist", "ein", "Rath", ",", "der", "stets", "mit", "Blind\u00b7heit", "w\u00e4h\u00b7let", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ejm w\u00e4hlen fehlt, doch w\u00e4hlt; im w\u00e4hlen wieder fehlet.", "tokens": ["\u201e", "jm", "w\u00e4h\u00b7len", "fehlt", ",", "doch", "w\u00e4hlt", ";", "im", "w\u00e4h\u00b7len", "wie\u00b7der", "feh\u00b7let", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVINF", "VVFIN", "$,", "ADV", "VVFIN", "$.", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "525\u201dEr liebt die Neuigkeit wie sie auch immer sey;", "tokens": ["\"", "Er", "liebt", "die", "Neu\u00b7ig\u00b7keit", "wie", "sie", "auch", "im\u00b7mer", "sey", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "KOKOM", "PPER", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201efast jeder Fall erweckt Verwundrung und Geschrey.", "tokens": ["\u201e", "fast", "je\u00b7der", "Fall", "er\u00b7weckt", "Ver\u00b7wund\u00b7rung", "und", "Ge\u00b7schrey", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIAT", "NN", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eso rufft er, da\u00df der Schwarm der Adler was bedeute,", "tokens": ["\u201e", "so", "rufft", "er", ",", "da\u00df", "der", "Schwarm", "der", "Ad\u00b7ler", "was", "be\u00b7deu\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ART", "NN", "PWS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eund diesem Kronen-Haus, wer sagt mir, was? bereite.", "tokens": ["\u201e", "und", "die\u00b7sem", "Kro\u00b7nen\u00b7Haus", ",", "wer", "sagt", "mir", ",", "was", "?", "be\u00b7rei\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "$,", "PWS", "VVFIN", "PPER", "$,", "PWS", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201enicht drey\u00dfig Jahre seynd, da\u00df durch dergleichen Flug", "tokens": ["\u201e", "nicht", "drey\u00b7\u00dfig", "Jah\u00b7re", "seynd", ",", "da\u00df", "durch", "derg\u00b7lei\u00b7chen", "Flug"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKNEG", "CARD", "NN", "VAFIN", "$,", "KOUS", "APPR", "PIS", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "530\u201dSich eine V\u00f6gel-Schaar in diese W\u00e4lder trug;", "tokens": ["\"", "Sich", "ei\u00b7ne", "V\u00f6\u00b7gel\u00b7Schaar", "in", "die\u00b7se", "W\u00e4l\u00b7der", "trug", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRF", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "\u201ewas hatte dazumahl der Flug nach sich gezogen?", "tokens": ["\u201e", "was", "hat\u00b7te", "da\u00b7zu\u00b7mahl", "der", "Flug", "nach", "sich", "ge\u00b7zo\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ADV", "ART", "NN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201enichts: ob der P\u00f6bel gleich sich sehr darum bewogen;", "tokens": ["\u201e", "nichts", ":", "ob", "der", "P\u00f6\u00b7bel", "gleich", "sich", "sehr", "da\u00b7rum", "be\u00b7wo\u00b7gen", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "$.", "KOUS", "ART", "NN", "ADV", "PRF", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "\u201enichts ist daraus erfolgt. So mach ich den Beschlu\u00df:", "tokens": ["\u201e", "nichts", "ist", "da\u00b7raus", "er\u00b7folgt", ".", "So", "mach", "ich", "den", "Be\u00b7schlu\u00df", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "PAV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eda\u00df man des P\u00f6bels Wahn vorsichtig folgen mu\u00df.", "tokens": ["\u201e", "da\u00df", "man", "des", "P\u00f6\u00b7bels", "Wahn", "vor\u00b7sich\u00b7tig", "fol\u00b7gen", "mu\u00df", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIS", "ART", "NN", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "535\u201dEr ist des Ungeheurs, der Seltsamkeit Verfechter,", "tokens": ["\"", "Er", "ist", "des", "Un\u00b7ge\u00b7heurs", ",", "der", "Selt\u00b7sam\u00b7keit", "Ver\u00b7fech\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u201eund wird gepr\u00fcftem Sinn zum Spiel und zum Gel\u00e4chter.", "tokens": ["\u201e", "und", "wird", "ge\u00b7pr\u00fcf\u00b7tem", "Sinn", "zum", "Spiel", "und", "zum", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "ADJA", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}