{"textgrid.poem.38370": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Kinderpredigt", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Huhn und ein Hahn,", "tokens": ["Ein", "Huhn", "und", "ein", "Hahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die Predigt geht an,", "tokens": ["Die", "Pre\u00b7digt", "geht", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein Kuh und ein Kalb,", "tokens": ["Ein", "Kuh", "und", "ein", "Kalb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die Predigt ist halb,", "tokens": ["Die", "Pre\u00b7digt", "ist", "halb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ein Katz und ein Maus,", "tokens": ["Ein", "Katz", "und", "ein", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Die Predigt ist aus,", "tokens": ["Die", "Pre\u00b7digt", "ist", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Geht alle nach Haus,", "tokens": ["Geht", "al\u00b7le", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Und haltet ein Schmaus.", "tokens": ["Und", "hal\u00b7tet", "ein", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Habt ihr was, so e\u00dft es,", "tokens": ["Habt", "ihr", "was", ",", "so", "e\u00dft", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Habt ihr nichts, verge\u00dft es,", "tokens": ["Habt", "ihr", "nichts", ",", "ver\u00b7ge\u00dft", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Habt ihr ein St\u00fcckchen Brod,", "tokens": ["Habt", "ihr", "ein", "St\u00fcck\u00b7chen", "Brod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So theilt es mit der Noth,", "tokens": ["So", "theilt", "es", "mit", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Und habt ihr noch ein Bros\u00e4mlein,", "tokens": ["Und", "habt", "ihr", "noch", "ein", "Bro\u00b7s\u00e4m\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So streuet es den V\u00f6gelein.", "tokens": ["So", "streu\u00b7et", "es", "den", "V\u00f6\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}