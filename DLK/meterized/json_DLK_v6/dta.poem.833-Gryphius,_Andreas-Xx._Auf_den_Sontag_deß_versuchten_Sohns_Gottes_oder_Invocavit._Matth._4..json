{"dta.poem.833": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  Auf den Sontag de\u00df versuchten Sohns  \n Gottes/ oder  Invocavit.  Matth. 4.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Weg! weg! hinweg du stoltzer Geist! dafern mir schon die ", "tokens": ["Weg", "!", "weg", "!", "hin\u00b7weg", "du", "stolt\u00b7zer", "Geist", "!", "da\u00b7fern", "mir", "schon", "die"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PTKVZ", "$.", "ADV", "PPER", "ADJA", "NN", "$.", "KOUS", "PPER", "ADV", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "rawe W\u00fcsten ", "tokens": ["ra\u00b7we", "W\u00fcs\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "In welcher GOtt mich pr\u00fcfen wil/ nichts al\u00df nur harte", "tokens": ["In", "wel\u00b7cher", "Gott", "mich", "pr\u00fc\u00b7fen", "wil", "/", "nichts", "al\u00df", "nur", "har\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVINF", "VMFIN", "$(", "PIS", "KOKOM", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Steine weist; ", "tokens": ["Stei\u00b7ne", "weist", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wird meine matte Seele doch durch dessen kr\u00e4fftigs Wort", "tokens": ["Wird", "mei\u00b7ne", "mat\u00b7te", "See\u00b7le", "doch", "durch", "des\u00b7sen", "kr\u00e4ff\u00b7tigs", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gespei\u00dft ", "tokens": ["ge\u00b7spei\u00dft"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Der alles Brott vnd Speise schafft. dafern du gleich mit", "tokens": ["Der", "al\u00b7les", "Brott", "vnd", "Spei\u00b7se", "schafft", ".", "da\u00b7fern", "du", "gleich", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "VVFIN", "$.", "KOUS", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "schlimmen L\u00fcsten ", "tokens": ["schlim\u00b7men", "L\u00fcs\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Mich in den Abgrund st\u00fcrtzen wilt/ wird mich doch dessen", "tokens": ["Mich", "in", "den", "Ab\u00b7grund", "st\u00fcrt\u00b7zen", "wilt", "/", "wird", "mich", "doch", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "VMFIN", "$(", "VAFIN", "PPER", "ADV", "PDS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allmacht fristen ", "tokens": ["All\u00b7macht", "fris\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der f\u00fcr die seinen trewlich sorgt/ der in dem weg vns blei-", "tokens": ["Der", "f\u00fcr", "die", "sei\u00b7nen", "trew\u00b7lich", "sorgt", "/", "der", "in", "dem", "weg", "vns", "blei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "PPOSAT", "ADJD", "VVFIN", "$(", "ART", "APPR", "ART", "ADV", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "ben heist/ ", "tokens": ["ben", "heist", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.5": {"text": "Der durch der Engel starcken Schutz/ den seinen festen", "tokens": ["Der", "durch", "der", "En\u00b7gel", "star\u00b7cken", "Schutz", "/", "den", "sei\u00b7nen", "fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$(", "ART", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "beystand leist. ", "tokens": ["beys\u00b7tand", "leist", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.7": {"text": "Vnd nicht von vn\u00df versucht wil seyn! du wirst doch (glaub", "tokens": ["Vnd", "nicht", "von", "vn\u00df", "ver\u00b7sucht", "wil", "seyn", "!", "du", "wirst", "doch", "(", "glaub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PTKNEG", "APPR", "NE", "VVFIN", "VMFIN", "VAINF", "$.", "PPER", "VAFIN", "ADV", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ich/) keinen Ehristen ", "tokens": ["ich", "/", ")", "kei\u00b7nen", "E\u00b7hris\u00b7ten"], "token_info": ["word", "punct", "punct", "word", "word"], "pos": ["PPER", "$(", "$(", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Der seinen ", "tokens": ["Der", "sei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "der Welt ", "tokens": ["der", "Welt"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Durch pr\u00e4chtig auffgeschm\u00fccktes nichts/ durch wollust", "tokens": ["Durch", "pr\u00e4ch\u00b7tig", "auff\u00b7ge\u00b7schm\u00fcck\u00b7tes", "nichts", "/", "durch", "wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "PIS", "$(", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "vnd verg\u00e4nglich Geldt ", "tokens": ["vnd", "ver\u00b7g\u00e4ng\u00b7lich", "Geldt"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Bewegen da\u00df er Knie vnd Hertz/ ohn m\u00e4chtig wunder! vor", "tokens": ["Be\u00b7we\u00b7gen", "da\u00df", "er", "Knie", "vnd", "Hertz", "/", "ohn", "m\u00e4ch\u00b7tig", "wun\u00b7der", "!", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "KOUS", "PPER", "NN", "KON", "NN", "$(", "APPR", "ADJD", "NN", "$.", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "dir neige? ", "tokens": ["dir", "nei\u00b7ge", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Kom an! versuche wie du wilt! Ich wil/ weil ", "tokens": ["Kom", "an", "!", "ver\u00b7su\u00b7che", "wie", "du", "wilt", "!", "Ich", "wil", "/", "weil"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKVZ", "$.", "VVFIN", "KOKOM", "PPER", "VMFIN", "$.", "PPER", "VMFIN", "$(", "KOUS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mich batt. ", "tokens": ["mich", "batt", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der deine gantze macht zust\u00f6rt/ vnd dir den Kopff zutret-", "tokens": ["Der", "dei\u00b7ne", "gant\u00b7ze", "macht", "zu\u00b7st\u00f6rt", "/", "vnd", "dir", "den", "Kopff", "zu\u00b7tret"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "VVFIN", "VVPP", "$(", "KON", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "ten hatt ", "tokens": ["ten", "hatt"], "token_info": ["word", "word"], "pos": ["FM", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Dir Erbfeind widerstehn/ bi\u00df Er die Ehren-Kron mir endtlich", "tokens": ["Dir", "Erb\u00b7feind", "wi\u00b7der\u00b7stehn", "/", "bi\u00df", "Er", "die", "Eh\u00b7ren\u00b7Kron", "mir", "endt\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVINF", "$(", "KOUS", "PPER", "ART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "zeige. ", "tokens": ["zei\u00b7ge", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}