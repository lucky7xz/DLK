{"textgrid.poem.62456": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Die Rosenknospen an ihre K\u00f6nigin", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die St\u00fcrme durchw\u00fcthen", "tokens": ["Die", "St\u00fcr\u00b7me", "durch\u00b7w\u00fct\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Winter den Baum;", "tokens": ["Im", "Win\u00b7ter", "den", "Baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch schlummern wir Bl\u00fcten", "tokens": ["Doch", "schlum\u00b7mern", "wir", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im seligen Traum.", "tokens": ["Im", "se\u00b7li\u00b7gen", "Traum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Von Bl\u00e4ttern umgeben,", "tokens": ["Von", "Bl\u00e4t\u00b7tern", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von G\u00f6ttern bewacht,", "tokens": ["Von", "G\u00f6t\u00b7tern", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gedeiht unser Leben", "tokens": ["Ge\u00b7deiht", "un\u00b7ser", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In Winter und Nacht.", "tokens": ["In", "Win\u00b7ter", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wollst G\u00f6ttin uns pflegen", "tokens": ["Wollst", "G\u00f6t\u00b7tin", "uns", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "NN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit sonnigem Blick", "tokens": ["Mit", "son\u00b7ni\u00b7gem", "Blick"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und spenden uns Segen", "tokens": ["Und", "spen\u00b7den", "uns", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Als unser Geschick.", "tokens": ["Als", "un\u00b7ser", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Bald naht, uns entfaltend", "tokens": ["Bald", "naht", ",", "uns", "ent\u00b7fal\u00b7tend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der Lenz, unser Freund,", "tokens": ["Der", "Lenz", ",", "un\u00b7ser", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein Leben gestaltend,", "tokens": ["Ein", "Le\u00b7ben", "ge\u00b7stal\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das selten erscheint.", "tokens": ["Das", "sel\u00b7ten", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "O Wesen, gesendet", "tokens": ["O", "We\u00b7sen", ",", "ge\u00b7sen\u00b7det"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "VVPP"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Von himmlischer Au,", "tokens": ["Von", "himm\u00b7li\u00b7scher", "Au", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dein Vaterland spendet", "tokens": ["Dein", "Va\u00b7ter\u00b7land", "spen\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dir Sonne, dir Thau.", "tokens": ["Dir", "Son\u00b7ne", ",", "dir", "Thau", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ob wir auch vergehen,", "tokens": ["Ob", "wir", "auch", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "So schnell als der Mai,", "tokens": ["So", "schnell", "als", "der", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wir duften, wir wehen", "tokens": ["Wir", "duf\u00b7ten", ",", "wir", "we\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Von Lieb' und von Treu.", "tokens": ["Von", "Lieb'", "und", "von", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}