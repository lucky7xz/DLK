", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "in den wachen Morgentraum", "tokens": ["in", "den", "wa\u00b7chen", "Mor\u00b7gen\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "spr\u00fchen tausend Silbersterne.", "tokens": ["spr\u00fc\u00b7hen", "tau\u00b7send", "Sil\u00b7bers\u00b7ter\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Milder Lichtschein, g\u00fct'ge Ruhe,", "tokens": ["Mil\u00b7der", "Licht\u00b7schein", ",", "g\u00fct'\u00b7ge", "Ru\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "k\u00fchl und heilig-still die L\u00fcfte:", "tokens": ["k\u00fchl", "und", "hei\u00b7lig\u00b7still", "die", "L\u00fcf\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die dunkelklaren Welten", "tokens": ["Durch", "die", "dun\u00b7kel\u00b7kla\u00b7ren", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "hallen lange Glockent\u00f6ne", "tokens": ["hal\u00b7len", "lan\u00b7ge", "Glo\u00b7cken\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "in den wachen Morgentraum.", "tokens": ["in", "den", "wa\u00b7chen", "Mor\u00b7gen\u00b7traum", "."], "token_info