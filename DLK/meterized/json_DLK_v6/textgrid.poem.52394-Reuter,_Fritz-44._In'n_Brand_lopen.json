{"textgrid.poem.52394": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "44. In'n Brand lopen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De oll Herr Plett m\u00fcggt girn de Trummel h\u00fcren,", "tokens": ["De", "oll", "Herr", "Plett", "m\u00fcggt", "girn", "de", "Trum\u00b7mel", "h\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "De Trummel was sin Insterment,", "tokens": ["De", "Trum\u00b7mel", "was", "sin", "Ins\u00b7ter\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un wenn de Tamburs bi dat \u00c4uwen wiren,", "tokens": ["Un", "wenn", "de", "Tam\u00b7burs", "bi", "dat", "\u00c4u\u00b7wen", "wi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "NE", "NE", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Denn was Herr Plett dorbi, un wenn't", "tokens": ["Denn", "was", "Herr", "Plett", "dor\u00b7bi", ",", "un", "wenn't"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "NN", "NE", "NE", "$,", "FM", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bummsk\u00fclen runne regen ded,", "tokens": ["Bumms\u00b7k\u00fc\u00b7len", "run\u00b7ne", "re\u00b7gen", "ded", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hei schugt nich Regen oder Storm", "tokens": ["Hei", "schugt", "nich", "Re\u00b7gen", "o\u00b7der", "Storm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Un g\u00fcng nah'n Takt denn hen un her", "tokens": ["Un", "g\u00fcng", "nah'n", "Takt", "denn", "hen", "un", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJA", "NN", "KON", "FM", "FM", "APZR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In'n Slo\u00dfgorn bi den Pulvertorm.", "tokens": ["In'n", "Slo\u00df\u00b7gorn", "bi", "den", "Pul\u00b7ver\u00b7torm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Na, de Tambur-Majur, denn de Tamburen", "tokens": ["Na", ",", "de", "Tam\u00b7bur\u00b7Ma\u00b7jur", ",", "denn", "de", "Tam\u00b7bu\u00b7ren"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "NE", "$,", "KON", "NE", "NE"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Heww'n ehren Separat-Majuren,", "tokens": ["Hew\u00b7w'n", "eh\u00b7ren", "Se\u00b7pa\u00b7rat\u00b7Ma\u00b7ju\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "De hadd dat oftmals seihn, dat Plett", "tokens": ["De", "hadd", "dat", "oft\u00b7mals", "seihn", ",", "dat", "Plett"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Takt glik richtig namen hett", "tokens": ["Den", "Takt", "glik", "rich\u00b7tig", "na\u00b7men", "hett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Un dat \u00fcm keinen Pris der Welt", "tokens": ["Un", "dat", "\u00fcm", "kei\u00b7nen", "Pris", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hei ut den Takt den rute f\u00f6llt.", "tokens": ["Hei", "ut", "den", "Takt", "den", "ru\u00b7te", "f\u00f6llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "De seggt sin Tamburs nu Bescheid,", "tokens": ["De", "seggt", "sin", "Tam\u00b7burs", "nu", "Be\u00b7scheid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.8": {"text": "Un as nu in de negsten Dagen", "tokens": ["Un", "as", "nu", "in", "de", "negs\u00b7ten", "Da\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "APPR", "NE", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Herr Plett dor ruhig r\u00fcmmer geiht", "tokens": ["Herr", "Plett", "dor", "ru\u00b7hig", "r\u00fcm\u00b7mer", "geiht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "ADJD", "ADJA", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Un nah den Takt deiht r\u00fcmmer bummeln,", "tokens": ["Un", "nah", "den", "Takt", "deiht", "r\u00fcm\u00b7mer", "bum\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Dunn ward'n de Kirls dor greller slagen", "tokens": ["Dunn", "ward'n", "de", "Kirls", "dor", "grel\u00b7ler", "sla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Un fang'n nu fixer an tau trummeln.", "tokens": ["Un", "fang'n", "nu", "fi\u00b7xer", "an", "tau", "trum\u00b7meln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Na, uns' Herr Plett marschiert noch mit,", "tokens": ["Na", ",", "un\u00b7s'", "Herr", "Plett", "mar\u00b7schiert", "noch", "mit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Noch h\u00f6llt hei \u00fcmmer richtig Tritt.", "tokens": ["Noch", "h\u00f6llt", "hei", "\u00fcm\u00b7mer", "rich\u00b7tig", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch de Majur, dat was en Racker,", "tokens": ["Doch", "de", "Ma\u00b7jur", ",", "dat", "was", "en", "Ra\u00b7cker", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "PRELS", "PWS", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Hei winkt de Kirls: \u00bbMan tau! Noch d\u00fcller!\u00ab", "tokens": ["Hei", "winkt", "de", "Kirls", ":", "\u00bb", "Man", "tau", "!", "Noch", "d\u00fcl\u00b7ler", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "$.", "$(", "PIS", "PTKVZ", "$.", "ADV", "ADJD", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Noch h\u00f6llt uns' oll Herr Plett sick wacker,", "tokens": ["Noch", "h\u00f6llt", "un\u00b7s'", "oll", "Herr", "Plett", "sick", "wa\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "NN", "ADJD", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Hei smitt de Bein un r\u00f6nnt, as s\u00fcll'e", "tokens": ["Hei", "smitt", "de", "Bein", "un", "r\u00f6nnt", ",", "as", "s\u00fcll'e"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVFIN", "$,", "FM.la", "FM.la"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Fiw Milen lopen in 'ne Stun'n.", "tokens": ["Fiw", "Mi\u00b7len", "lo\u00b7pen", "in", "'", "ne", "Stun", "'", "n."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "abbreviation"], "pos": ["NE", "NN", "VVFIN", "APPR", "$(", "FM", "FM", "$(", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Doch de Majur, de winkt man blo\u00df,", "tokens": ["Doch", "de", "Ma\u00b7jur", ",", "de", "winkt", "man", "blo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "NE", "VVFIN", "PIS", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Dunn geiht dat in den Stormschritt los:", "tokens": ["Dunn", "geiht", "dat", "in", "den", "Storm\u00b7schritt", "los", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.22": {"text": "\u00bbplumpsack! Plumpsack! Plumpsack! Plumpsack!\u00ab", "tokens": ["\u00bb", "plum\u00b7psack", "!", "Plum\u00b7psack", "!", "Plum\u00b7psack", "!", "Plum\u00b7psack", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Dunn steiht hei still: \u00bbEn dummen Snack!\u00ab", "tokens": ["Dunn", "steiht", "hei", "still", ":", "\u00bb", "En", "dum\u00b7men", "Snack", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "NE", "PTKVZ", "$.", "$(", "NE", "ADJA", "NN", "$.", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.24": {"text": "Un wischt den Sweit sick af un pust", "tokens": ["Un", "wischt", "den", "Sweit", "sick", "af", "un", "pust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "FM", "FM", "FM", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Un drauht de Rackers mit de Fust.", "tokens": ["Un", "drauht", "de", "Ra\u00b7ckers", "mit", "de", "Fust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "APPR", "NE", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.26": {"text": "\u00bbschapsk\u00f6pper s\u00fcnd ji all tauhopen,", "tokens": ["\u00bb", "schaps\u00b7k\u00f6p\u00b7per", "s\u00fcnd", "ji", "all", "tau\u00b7ho\u00b7pen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Meint ji, ick sall in'n Brand mi lopen?", "tokens": ["Meint", "ji", ",", "ick", "sall", "in'n", "Brand", "mi", "lo\u00b7pen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "PPER", "ADV", "APPRART", "NN", "NE", "VVINF", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Ut Rand un Band?", "tokens": ["Ut", "Rand", "un", "Band", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "FM", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "In'n Brand?\u00ab", "tokens": ["In'n", "Brand", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}}}}