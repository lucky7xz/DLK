{"textgrid.poem.61156": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn nach des Tags Verbluten weit und breit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn nach des Tags Verbluten weit und breit", "tokens": ["Wenn", "nach", "des", "Tags", "Ver\u00b7blu\u00b7ten", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Finsternis sich schauervoll ergie\u00dfet,", "tokens": ["Die", "Fins\u00b7ter\u00b7nis", "sich", "schau\u00b7er\u00b7voll", "er\u00b7gie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df Berg und Tal in w\u00fcstes Schwarz zerflie\u00dfet,", "tokens": ["Da\u00df", "Berg", "und", "Tal", "in", "w\u00fcs\u00b7tes", "Schwarz", "zer\u00b7flie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da tritt hervor der Sterne Heiterkeit.", "tokens": ["Da", "tritt", "her\u00b7vor", "der", "Ster\u00b7ne", "Hei\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wenn ein Volk in trotz'gem Widerstreit", "tokens": ["Und", "wenn", "ein", "Volk", "in", "trotz'\u00b7gem", "Wi\u00b7der\u00b7streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem gottgesandten Strahl das Herz verschlie\u00dfet,", "tokens": ["Dem", "gott\u00b7ge\u00b7sand\u00b7ten", "Strahl", "das", "Herz", "ver\u00b7schlie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um H\u00fctt' und Schlo\u00df der L\u00fcgen Unkraut sprie\u00dfet,", "tokens": ["Um", "H\u00fctt'", "und", "Schlo\u00df", "der", "L\u00fc\u00b7gen", "Un\u00b7kraut", "sprie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist der Seher, der Propheten Zeit.", "tokens": ["Das", "ist", "der", "Se\u00b7her", ",", "der", "Pro\u00b7phe\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Herr, sieh gen Himmel uns die Arme strecken!", "tokens": ["Herr", ",", "sieh", "gen", "Him\u00b7mel", "uns", "die", "Ar\u00b7me", "stre\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6r' unser hei\u00dfes Flehen fr\u00fch und spat,", "tokens": ["H\u00f6r'", "un\u00b7ser", "hei\u00b7\u00dfes", "Fle\u00b7hen", "fr\u00fch", "und", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wollest einen Retter uns erwecken!", "tokens": ["Du", "wol\u00b7lest", "ei\u00b7nen", "Ret\u00b7ter", "uns", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dies Volk ist irr, und irr der hohe Rat -", "tokens": ["Dies", "Volk", "ist", "irr", ",", "und", "irr", "der", "ho\u00b7he", "Rat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "ADJD", "$,", "KON", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O la\u00df ihn nahn im Donner deiner Schrecken,", "tokens": ["O", "la\u00df", "ihn", "nahn", "im", "Don\u00b7ner", "dei\u00b7ner", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJA", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Spreu zu scheiden von der guten Saat!", "tokens": ["Die", "Spreu", "zu", "schei\u00b7den", "von", "der", "gu\u00b7ten", "Saat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}