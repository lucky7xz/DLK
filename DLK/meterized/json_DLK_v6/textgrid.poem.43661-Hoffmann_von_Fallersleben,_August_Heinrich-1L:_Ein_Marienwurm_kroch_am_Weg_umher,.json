{"textgrid.poem.43661": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Marienwurm kroch am Weg umher,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Marienwurm kroch am Weg umher,", "tokens": ["Ein", "Ma\u00b7ri\u00b7en\u00b7wurm", "kroch", "am", "Weg", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und es war das Herz ihm so bang und schwer.", "tokens": ["Und", "es", "war", "das", "Herz", "ihm", "so", "bang", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Und er klagte still vor sich hin und sang,", "tokens": ["Und", "er", "klag\u00b7te", "still", "vor", "sich", "hin", "und", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "PRF", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df es leise nur durch die L\u00fcfte drang.", "tokens": ["Da\u00df", "es", "lei\u00b7se", "nur", "durch", "die", "L\u00fcf\u00b7te", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbo du b\u00f6ser Wind, o wie hass' ich dich!", "tokens": ["\u00bb", "o", "du", "b\u00f6\u00b7ser", "Wind", ",", "o", "wie", "hass'", "ich", "dich", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "ADJA", "NN", "$,", "FM", "KOKOM", "VAFIN", "PPER", "PRF", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hast mein Haus zerst\u00f6rt, hast vertrieben mich.", "tokens": ["Hast", "mein", "Haus", "zer\u00b7st\u00f6rt", ",", "hast", "ver\u00b7trie\u00b7ben", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,", "VAFIN", "VVPP", "PPER", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Tulpe war mein Haus, und ich wohnte drin,", "tokens": ["Tul\u00b7pe", "war", "mein", "Haus", ",", "und", "ich", "wohn\u00b7te", "drin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und es ging mir da recht nach meinem Sinn.", "tokens": ["Und", "es", "ging", "mir", "da", "recht", "nach", "mei\u00b7nem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Abends ward das Thor immer zugemacht:", "tokens": ["A\u00b7bends", "ward", "das", "Thor", "im\u00b7mer", "zu\u00b7ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sicher schlief ich so manche Mondscheinnacht.", "tokens": ["Si\u00b7cher", "schlief", "ich", "so", "man\u00b7che", "Mond\u00b7schein\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Morgens ging von selbst wieder auf das Thor,", "tokens": ["Mor\u00b7gens", "ging", "von", "selbst", "wie\u00b7der", "auf", "das", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit dem Morgenroth kam ich dann hervor.", "tokens": ["Mit", "dem", "Mor\u00b7gen\u00b7roth", "kam", "ich", "dann", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "O du b\u00f6ser Wind, o wie hass' ich dich!", "tokens": ["O", "du", "b\u00f6\u00b7ser", "Wind", ",", "o", "wie", "hass'", "ich", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "FM", "KOKOM", "VAFIN", "PPER", "PRF", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hast mein Haus zerst\u00f6rt, hast vertrieben mich.\u00ab", "tokens": ["Hast", "mein", "Haus", "zer\u00b7st\u00f6rt", ",", "hast", "ver\u00b7trie\u00b7ben", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$,", "VAFIN", "VVPP", "PPER", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}