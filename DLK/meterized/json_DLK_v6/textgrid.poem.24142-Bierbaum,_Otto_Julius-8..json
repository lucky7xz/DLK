{"textgrid.poem.24142": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist viel tot in mir. Ich wei\u00df nun, jene Qual", "tokens": ["Nun", "ist", "viel", "tot", "in", "mir", ".", "Ich", "wei\u00df", "nun", ",", "je\u00b7ne", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "$.", "PPER", "VVFIN", "ADV", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mich ins Fremde trieb und immer r\u00fcckw\u00e4rts doch", "tokens": ["Die", "mich", "ins", "Frem\u00b7de", "trieb", "und", "im\u00b7mer", "r\u00fcck\u00b7w\u00e4rts", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "KON", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Blick der Sehnsucht wandte, war nicht mehr", "tokens": ["Den", "Blick", "der", "Sehn\u00b7sucht", "wand\u00b7te", ",", "war", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als einer Krankheit letzter Ueberfall.", "tokens": ["Als", "ei\u00b7ner", "Krank\u00b7heit", "letz\u00b7ter", "Ue\u00b7ber\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh, auf dem Schnee hier steht ein Sarg, \u2013 hinein", "tokens": ["Sieh", ",", "auf", "dem", "Schnee", "hier", "steht", "ein", "Sarg", ",", "\u2013", "hin\u00b7ein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["NE", "$,", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$,", "$(", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die leere Puppe jenes faulen Grams!", "tokens": ["Die", "lee\u00b7re", "Pup\u00b7pe", "je\u00b7nes", "fau\u00b7len", "Grams", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lemuren, kommt, und schaufelt mir ein Grab", "tokens": ["Le\u00b7mu\u00b7ren", ",", "kommt", ",", "und", "schau\u00b7felt", "mir", "ein", "Grab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr diese b\u00f6se Puppe, \u2013 Schnee, Schnee, Schnee", "tokens": ["F\u00fcr", "die\u00b7se", "b\u00f6\u00b7se", "Pup\u00b7pe", ",", "\u2013", "Schnee", ",", "Schnee", ",", "Schnee"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "$(", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Darauf und schwere Bl\u00f6cke Eis. Macht schnell!", "tokens": ["Da\u00b7rauf", "und", "schwe\u00b7re", "Bl\u00f6\u00b7cke", "Eis", ".", "Macht", "schnell", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "KON", "ADJA", "NN", "NN", "$.", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Tief, tief das Grab, in Eis und Schnee tief, tief!", "tokens": ["Tief", ",", "tief", "das", "Grab", ",", "in", "Eis", "und", "Schnee", "tief", ",", "tief", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich will nicht wissen, wo der Popanz liegt!", "tokens": ["Ich", "will", "nicht", "wis\u00b7sen", ",", "wo", "der", "Po\u00b7panz", "liegt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ah, da\u00df ich frei bin! Wintersonne, sieh,", "tokens": ["Ah", ",", "da\u00df", "ich", "frei", "bin", "!", "Win\u00b7ter\u00b7son\u00b7ne", ",", "sieh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$.", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hier steh ich fr\u00f6hlich zwischen Eis und Schnee,", "tokens": ["Hier", "steh", "ich", "fr\u00f6h\u00b7lich", "zwi\u00b7schen", "Eis", "und", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und niemals wu\u00dft ich mehr, was Fr\u00fchling ist.", "tokens": ["Und", "nie\u00b7mals", "wu\u00dft", "ich", "mehr", ",", "was", "Fr\u00fch\u00b7ling", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich war ins Grau, ins Neblige verrannt.", "tokens": ["Ich", "war", "ins", "Grau", ",", "ins", "Neb\u00b7li\u00b7ge", "ver\u00b7rannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich hing am Gram wie in der Spinne Netz", "tokens": ["Ich", "hing", "am", "Gram", "wie", "in", "der", "Spin\u00b7ne", "Netz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "KOKOM", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die arme Fliege, und schon fuhr auf mich", "tokens": ["Die", "ar\u00b7me", "Flie\u00b7ge", ",", "und", "schon", "fuhr", "auf", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADV", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die gro\u00dfe Spinne los, die alles fri\u00dft,", "tokens": ["Die", "gro\u00b7\u00dfe", "Spin\u00b7ne", "los", ",", "die", "al\u00b7les", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da sprach was \u00fcber meinem Leben wacht:", "tokens": ["Da", "sprach", "was", "\u00fc\u00b7ber", "mei\u00b7nem", "Le\u00b7ben", "wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Noch nicht, noch nicht! Und wie im M\u00e4rchen wars:", "tokens": ["Noch", "nicht", ",", "noch", "nicht", "!", "Und", "wie", "im", "M\u00e4r\u00b7chen", "wars", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "ADV", "PTKNEG", "$.", "KON", "PWAV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich stand verwandelt und erl\u00f6st und frei", "tokens": ["Ich", "stand", "ver\u00b7wan\u00b7delt", "und", "er\u00b7l\u00f6st", "und", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "KON", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Im allersch\u00f6nsten Schlosse von Kristall.", "tokens": ["Im", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Schlos\u00b7se", "von", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Oh sch\u00f6ner Winter, kalt und sonnenklar,", "tokens": ["Oh", "sch\u00f6\u00b7ner", "Win\u00b7ter", ",", "kalt", "und", "son\u00b7nen\u00b7klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Frost hat mich gesund gemacht und hart.", "tokens": ["Dein", "Frost", "hat", "mich", "ge\u00b7sund", "ge\u00b7macht", "und", "hart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir ist, als ruhte jetzt in meiner Hand", "tokens": ["Mir", "ist", ",", "als", "ruh\u00b7te", "jetzt", "in", "mei\u00b7ner", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein wohlgeh\u00e4mmert Schwert. Und ich bin stark,", "tokens": ["Ein", "wohl\u00b7ge\u00b7h\u00e4m\u00b7mert", "Schwert", ".", "Und", "ich", "bin", "stark", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$.", "KON", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir alle Wege frei damit zu haun.", "tokens": ["Mir", "al\u00b7le", "We\u00b7ge", "frei", "da\u00b7mit", "zu", "haun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ADJD", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In Niederungen geh ich nun nicht mehr.", "tokens": ["In", "Nie\u00b7de\u00b7run\u00b7gen", "geh", "ich", "nun", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}