{"textgrid.poem.41529": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am f\u00fcnften Sonntage in der Fasten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Propheten sind begraben!", "tokens": ["Die", "Pro\u00b7phe\u00b7ten", "sind", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abraham ist tot!", "tokens": ["Ab\u00b7ra\u00b7ham", "ist", "tot", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Millionen, Greis' und Knaben,", "tokens": ["Mil\u00b7lion\u00b7en", ",", "Greis'", "und", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und der M\u00e4gdlein rot,", "tokens": ["Und", "der", "M\u00e4gd\u00b7lein", "rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Viele, die mir Liebe gaben,", "tokens": ["Vie\u00b7le", ",", "die", "mir", "Lie\u00b7be", "ga\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denen ich sie bot,", "tokens": ["De\u00b7nen", "ich", "sie", "bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Alle, alle sind begraben!", "tokens": ["Al\u00b7le", ",", "al\u00b7le", "sind", "be\u00b7gra\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle sind sie tot!", "tokens": ["Al\u00b7le", "sind", "sie", "tot", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Herr, du hast es mir verk\u00fcndet,", "tokens": ["Herr", ",", "du", "hast", "es", "mir", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dein Wort steht fest,", "tokens": ["Und", "dein", "Wort", "steht", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df nur der das Leben findet,", "tokens": ["Da\u00df", "nur", "der", "das", "Le\u00b7ben", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der das Leben l\u00e4\u00dft.", "tokens": ["Der", "das", "Le\u00b7ben", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ach, in meiner Seele windet", "tokens": ["Ach", ",", "in", "mei\u00b7ner", "See\u00b7le", "win\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Es sich dumpf gepre\u00dft;", "tokens": ["Es", "sich", "dumpf", "ge\u00b7pre\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Doch, du hast es mir verk\u00fcndet,", "tokens": ["Doch", ",", "du", "hast", "es", "mir", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dein Wort steht fest!", "tokens": ["Und", "dein", "Wort", "steht", "fest", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Aber von mir selbst bereitet,", "tokens": ["A\u00b7ber", "von", "mir", "selbst", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leb' ich oft der Pein,", "tokens": ["Leb'", "ich", "oft", "der", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alles scheint mir wohl geleitet,", "tokens": ["Al\u00b7les", "scheint", "mir", "wohl", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Mensch allein,", "tokens": ["Und", "der", "Mensch", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der dein Ebenbild bedeutet,", "tokens": ["Der", "dein", "E\u00b7ben\u00b7bild", "be\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jammervoll zu sein:", "tokens": ["Jam\u00b7mer\u00b7voll", "zu", "sein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sieh, so hab' ich mir bereitet", "tokens": ["Sieh", ",", "so", "hab'", "ich", "mir", "be\u00b7rei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Namenlose Pein.", "tokens": ["Na\u00b7men\u00b7lo\u00b7se", "Pein", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Hab' ich grausend es empfunden,", "tokens": ["Hab'", "ich", "grau\u00b7send", "es", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie in der Natur", "tokens": ["Wie", "in", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN"], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "An ein F\u00e4serchen gebunden,", "tokens": ["An", "ein", "F\u00e4\u00b7ser\u00b7chen", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Nerve nur,", "tokens": ["Ei\u00b7ne", "Ner\u00b7ve", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Oft dein Ebenbild verschwunden", "tokens": ["Oft", "dein", "E\u00b7ben\u00b7bild", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf die letzte Spur:", "tokens": ["Auf", "die", "letz\u00b7te", "Spur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Hab' ich keinen Geist gefunden,", "tokens": ["Hab'", "ich", "kei\u00b7nen", "Geist", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen K\u00f6rper nur!", "tokens": ["Ei\u00b7nen", "K\u00f6r\u00b7per", "nur", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Seh ich dann zu Staub zerfallen,", "tokens": ["Seh", "ich", "dann", "zu", "Staub", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was so warm gelebt,", "tokens": ["Was", "so", "warm", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ohne da\u00df die Muskeln wallen,", "tokens": ["Oh\u00b7ne", "da\u00df", "die", "Mus\u00b7keln", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Nerve bebt,", "tokens": ["Ei\u00b7ne", "Ner\u00b7ve", "bebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Da die Seele doch an allen", "tokens": ["Da", "die", "See\u00b7le", "doch", "an", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Innig fest geklebt,", "tokens": ["In\u00b7nig", "fest", "ge\u00b7klebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "M\u00f6cht' ich selbst zu Staub zerfallen,", "tokens": ["M\u00f6cht'", "ich", "selbst", "zu", "Staub", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich nie gelebt!", "tokens": ["Da\u00df", "ich", "nie", "ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Schrecklich \u00fcber alles Denken", "tokens": ["Schreck\u00b7lich", "\u00fc\u00b7ber", "al\u00b7les", "Den\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die dumpfe Nacht,", "tokens": ["Ist", "die", "dump\u00b7fe", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Drin sich kann ein Geist versenken,", "tokens": ["Drin", "sich", "kann", "ein", "Geist", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der allein gedacht,", "tokens": ["Der", "al\u00b7lein", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Der sich nicht von dir lie\u00df lenken,", "tokens": ["Der", "sich", "nicht", "von", "dir", "lie\u00df", "len\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "APPR", "PPER", "VVFIN", "VVINF", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Helle Glaubensmacht!", "tokens": ["Hel\u00b7le", "Glau\u00b7bens\u00b7macht", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ach, was mag der Finstre denken", "tokens": ["Ach", ",", "was", "mag", "der", "Finst\u00b7re", "den\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWS", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als die finstre Nacht!", "tokens": ["Als", "die", "finst\u00b7re", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Meine Lieder werden leben,", "tokens": ["Mei\u00b7ne", "Lie\u00b7der", "wer\u00b7den", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich l\u00e4ngst entschwand,", "tokens": ["Wenn", "ich", "l\u00e4ngst", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mancher wird vor ihnen beben,", "tokens": ["Man\u00b7cher", "wird", "vor", "ih\u00b7nen", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der gleich mir empfand.", "tokens": ["Der", "gleich", "mir", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ob ein andrer sie gegeben,", "tokens": ["Ob", "ein", "an\u00b7drer", "sie", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder meine Hand!", "tokens": ["O\u00b7der", "mei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sieh, die Lieder durften leben,", "tokens": ["Sieh", ",", "die", "Lie\u00b7der", "durf\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber ich entschwand!", "tokens": ["A\u00b7ber", "ich", "ent\u00b7schwand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Bruder mein, so la\u00df uns sehen", "tokens": ["Bru\u00b7der", "mein", ",", "so", "la\u00df", "uns", "se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "$,", "ADV", "VVIMP", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fest auf Gottes Wort,", "tokens": ["Fest", "auf", "Got\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die Verwirrung wird vergehen,", "tokens": ["Die", "Ver\u00b7wir\u00b7rung", "wird", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies lebt ewig fort!", "tokens": ["Dies", "lebt", "e\u00b7wig", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wei\u00dft du wie sie mag entstehen", "tokens": ["Wei\u00dft", "du", "wie", "sie", "mag", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Im Gehirne dort?", "tokens": ["Im", "Ge\u00b7hir\u00b7ne", "dort", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ob wir einst nicht l\u00e4chelnd sehen", "tokens": ["Ob", "wir", "einst", "nicht", "l\u00e4\u00b7chelnd", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Verst\u00f6rung Wort,", "tokens": ["Der", "Ver\u00b7st\u00f6\u00b7rung", "Wort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Wie es hing an einem Faden,", "tokens": ["Wie", "es", "hing", "an", "ei\u00b7nem", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der zu hart gespannt,", "tokens": ["Der", "zu", "hart", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit entflammten Blut beladen", "tokens": ["Mit", "ent\u00b7flamm\u00b7ten", "Blut", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich der Stirn entwand?", "tokens": ["Sich", "der", "Stirn", "ent\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Flehen wir zu Gottes Gnaden!", "tokens": ["Fle\u00b7hen", "wir", "zu", "Got\u00b7tes", "Gna\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flehn zu seiner Hand,", "tokens": ["Flehn", "zu", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die die F\u00e4dchen und die Faden", "tokens": ["Die", "die", "F\u00e4d\u00b7chen", "und", "die", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Liebreich ausgespannt.", "tokens": ["Lieb\u00b7reich", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}