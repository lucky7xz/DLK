{"textgrid.poem.48862": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "87. Zu ihrem Geburtstage", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist der siebzigste nach f\u00fcnfmal hundert Tagen", "tokens": ["Du", "bist", "der", "sieb\u00b7zigs\u00b7te", "nach", "f\u00fcnf\u00b7mal", "hun\u00b7dert", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "APPR", "ADV", "CARD", "NN"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und sechsmal tausenden, da\u00df meines Lichtes Licht", "tokens": ["und", "sechs\u00b7mal", "tau\u00b7sen\u00b7den", ",", "da\u00df", "mei\u00b7nes", "Lich\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das Licht der gro\u00dfen Welt nahm in ihr Angesicht,", "tokens": ["das", "Licht", "der", "gro\u00b7\u00dfen", "Welt", "nahm", "in", "ihr", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und h\u00f6rt' ihr frohes Haus von junger Freude sagen.", "tokens": ["und", "h\u00f6rt'", "ihr", "fro\u00b7hes", "Haus", "von", "jun\u00b7ger", "Freu\u00b7de", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bis mir willkommen itzt, du Ende meiner Klagen,", "tokens": ["Bis", "mir", "will\u00b7kom\u00b7men", "itzt", ",", "du", "En\u00b7de", "mei\u00b7ner", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "ADV", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "du Anfang meiner Lust, von dem mein Herze spricht:", "tokens": ["du", "An\u00b7fang", "mei\u00b7ner", "Lust", ",", "von", "dem", "mein", "Her\u00b7ze", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein angenehmer Tag ist mir erschienen nicht,", "tokens": ["ein", "an\u00b7ge\u00b7neh\u00b7mer", "Tag", "ist", "mir", "er\u00b7schie\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so lange Ph\u00f6bus hat sein Rad herumgetragen.", "tokens": ["so", "lan\u00b7ge", "Ph\u00f6\u00b7bus", "hat", "sein", "Rad", "her\u00b7um\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Blume, welche mir von s\u00fc\u00dfer Liebe wegen", "tokens": ["Die", "Blu\u00b7me", ",", "wel\u00b7che", "mir", "von", "s\u00fc\u00b7\u00dfer", "Lie\u00b7be", "we\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Liebste selbst gesandt, die send' ich dir entgegen,", "tokens": ["die", "Liebs\u00b7te", "selbst", "ge\u00b7sandt", ",", "die", "send'", "ich", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,", "PRELS", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um da\u00df du sp\u00fcren magst, wie lieb du mir brichst an.", "tokens": ["um", "da\u00df", "du", "sp\u00fc\u00b7ren", "magst", ",", "wie", "lieb", "du", "mir", "brichst", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was k\u00fcnt' ich Liebers dir, als etwas Solches senden,", "tokens": ["Was", "k\u00fcnt'", "ich", "Lie\u00b7bers", "dir", ",", "als", "et\u00b7was", "Sol\u00b7ches", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "PPER", "$,", "KOUS", "PIS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so her gekommen war von der Geliebten H\u00e4nden,", "tokens": ["so", "her", "ge\u00b7kom\u00b7men", "war", "von", "der", "Ge\u00b7lieb\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APZR", "VVPP", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ohn' die mir Nichts ist lieb, was lieblich hei\u00dfen kan!", "tokens": ["ohn'", "die", "mir", "Nichts", "ist", "lieb", ",", "was", "lieb\u00b7lich", "hei\u00b7\u00dfen", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VAFIN", "ADJD", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}