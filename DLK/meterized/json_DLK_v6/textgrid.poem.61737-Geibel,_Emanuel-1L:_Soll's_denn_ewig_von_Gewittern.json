{"textgrid.poem.61737": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll's denn ewig von Gewittern", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll's denn ewig von Gewittern", "tokens": ["Soll's", "denn", "e\u00b7wig", "von", "Ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Am umw\u00f6lkten Himmel braun?", "tokens": ["Am", "um\u00b7w\u00f6lk\u00b7ten", "Him\u00b7mel", "braun", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll denn stets der Boden zittern,", "tokens": ["Soll", "denn", "stets", "der", "Bo\u00b7den", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drauf wir unsre H\u00fctten baun?", "tokens": ["Drauf", "wir", "uns\u00b7re", "H\u00fct\u00b7ten", "baun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder wollt ihr mit den Waffen", "tokens": ["O\u00b7der", "wollt", "ihr", "mit", "den", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich Rast und Frieden schaffen?", "tokens": ["End\u00b7lich", "Rast", "und", "Frie\u00b7den", "schaf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df die Welt nicht mehr, in Sorgen", "tokens": ["Da\u00df", "die", "Welt", "nicht", "mehr", ",", "in", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um ihr leichtersch\u00fcttert Gl\u00fcck,", "tokens": ["Um", "ihr", "leich\u00b7ter\u00b7sch\u00fct\u00b7tert", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4glich bebe vor dem Morgen,", "tokens": ["T\u00e4g\u00b7lich", "be\u00b7be", "vor", "dem", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebt ihr ihren Kern zur\u00fcck!", "tokens": ["Gebt", "ihr", "ih\u00b7ren", "Kern", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht Europas Herz gesunden,", "tokens": ["Macht", "Eu\u00b7ro\u00b7pas", "Herz", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Heil ist euch gefunden.", "tokens": ["Und", "das", "Heil", "ist", "euch", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einen Hort geht aufzurichten,", "tokens": ["Ei\u00b7nen", "Hort", "geht", "auf\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Hort im deutschen Land!", "tokens": ["Ei\u00b7nen", "Hort", "im", "deut\u00b7schen", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht zum Lenken und zum Schlichten", "tokens": ["Sucht", "zum", "Len\u00b7ken", "und", "zum", "Schlich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine schwerterprobte Hand,", "tokens": ["Ei\u00b7ne", "schwer\u00b7ter\u00b7prob\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die den g\u00fcldnen Apfel halte", "tokens": ["Die", "den", "g\u00fcld\u00b7nen", "Ap\u00b7fel", "hal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Reichs in Treuen walte.", "tokens": ["Und", "des", "Reichs", "in", "Treu\u00b7en", "wal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sein gef\u00fcrstet Banner trage", "tokens": ["Sein", "ge\u00b7f\u00fcrs\u00b7tet", "Ban\u00b7ner", "tra\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVPP", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Stamm, wie er's erkor,", "tokens": ["Je\u00b7der", "Stamm", ",", "wie", "er's", "er\u00b7kor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber \u00fcber alle rage", "tokens": ["A\u00b7ber", "\u00fc\u00b7ber", "al\u00b7le", "ra\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stolzentfaltet eins empor,", "tokens": ["Stol\u00b7zent\u00b7fal\u00b7tet", "eins", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoch, im Schmuck der Eichenreiser,", "tokens": ["Hoch", ",", "im", "Schmuck", "der", "Ei\u00b7chen\u00b7rei\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wall' es vor dem deutschen Kaiser.", "tokens": ["Wall'", "es", "vor", "dem", "deut\u00b7schen", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn die heil'ge Krone wieder", "tokens": ["Wenn", "die", "heil'\u00b7ge", "Kro\u00b7ne", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine hohe Scheitel schm\u00fcckt,", "tokens": ["Ei\u00b7ne", "ho\u00b7he", "Schei\u00b7tel", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Haupt durch alle Glieder", "tokens": ["Aus", "dem", "Haupt", "durch", "al\u00b7le", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stark ein ein'ger Wille z\u00fcckt,", "tokens": ["Stark", "ein", "ein'\u00b7ger", "Wil\u00b7le", "z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird im V\u00f6lkerrat vor allen", "tokens": ["Wird", "im", "V\u00f6l\u00b7ker\u00b7rat", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutscher Spruch aufs neu' erschallen.", "tokens": ["Deut\u00b7scher", "Spruch", "aufs", "neu'", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann nicht mehr zum Weltgesetze", "tokens": ["Dann", "nicht", "mehr", "zum", "Welt\u00b7ge\u00b7set\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird die Laun' am Seinestrom,", "tokens": ["Wird", "die", "Laun'", "am", "Sei\u00b7nes\u00b7trom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann vergeblich seine Netze", "tokens": ["Dann", "ver\u00b7geb\u00b7lich", "sei\u00b7ne", "Net\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirft der Fischer aus in Rom,", "tokens": ["Wirft", "der", "Fi\u00b7scher", "aus", "in", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00e4nger nicht mit seinen Horden", "tokens": ["L\u00e4n\u00b7ger", "nicht", "mit", "sei\u00b7nen", "Hor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreckt uns der Kolo\u00df im Norden.", "tokens": ["Schreckt", "uns", "der", "Ko\u00b7lo\u00df", "im", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Macht und Freiheit, Recht und Sitte,", "tokens": ["Macht", "und", "Frei\u00b7heit", ",", "Recht", "und", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klarer Geist und scharfer Hieb,", "tokens": ["Kla\u00b7rer", "Geist", "und", "schar\u00b7fer", "Hieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00fcgeln dann aus starker Mitte", "tokens": ["Z\u00fc\u00b7geln", "dann", "aus", "star\u00b7ker", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jeder Selbstsucht wilden Trieb,", "tokens": ["Je\u00b7der", "Selbst\u00b7sucht", "wil\u00b7den", "Trieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es mag am deutschen Wesen", "tokens": ["Und", "es", "mag", "am", "deut\u00b7schen", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einmal noch die Welt genesen.", "tokens": ["Ein\u00b7mal", "noch", "die", "Welt", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}