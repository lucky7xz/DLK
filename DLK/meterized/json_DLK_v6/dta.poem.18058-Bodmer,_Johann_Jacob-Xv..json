{"dta.poem.18058": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Xv.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Wasser ward einmahl so gro\u00df,", "tokens": ["Ein", "Was\u00b7ser", "ward", "ein\u00b7mahl", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df es aus seinem Bete flo\u00df,", "tokens": ["Da\u00df", "es", "aus", "sei\u00b7nem", "Be\u00b7te", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weit und breit im Land umschweift;", "tokens": ["Und", "weit", "und", "breit", "im", "Land", "um\u00b7schweift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schleppt dahin, was es ergreift;", "tokens": ["Es", "schleppt", "da\u00b7hin", ",", "was", "es", "er\u00b7greift", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit andern Dingen schleifts davon", "tokens": ["Mit", "an\u00b7dern", "Din\u00b7gen", "schleifts", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zween Hafen, einer war von Thon:", "tokens": ["Zween", "Ha\u00b7fen", ",", "ei\u00b7ner", "war", "von", "Thon", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PIS", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der andre war aus Ertz gegossen.", "tokens": ["Der", "and\u00b7re", "war", "aus", "Ertz", "ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die beyde kamen so geflossen,", "tokens": ["Die", "bey\u00b7de", "ka\u00b7men", "so", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und weil der erste leichter war, ", "tokens": ["Und", "weil", "der", "ers\u00b7te", "leich\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gelang sein Weg ihm desto besser", "tokens": ["Ge\u00b7lang", "sein", "Weg", "ihm", "des\u00b7to", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Auf dem starckstr\u00f6hmenden Gew\u00e4sser.", "tokens": ["Auf", "dem", "starck\u00b7str\u00f6h\u00b7men\u00b7den", "Ge\u00b7w\u00e4s\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Er fuhr voran, der andre nach.", "tokens": ["Er", "fuhr", "vo\u00b7ran", ",", "der", "and\u00b7re", "nach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PRELS", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der rief ihm zu: Ist dir so gach, ", "tokens": ["Der", "rief", "ihm", "zu", ":", "Ist", "dir", "so", "gach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$.", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mein Freund, und wartest du nicht mein?", "tokens": ["Mein", "Freund", ",", "und", "war\u00b7test", "du", "nicht", "mein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wir wollen Spie\u00dfgesellen seyn.", "tokens": ["Wir", "wol\u00b7len", "Spie\u00df\u00b7ge\u00b7sel\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Wart mein, ich will mit dir hinfahren;", "tokens": ["Wart", "mein", ",", "ich", "will", "mit", "dir", "hin\u00b7fah\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Gott wird uns beyde wohl bewahren.", "tokens": ["Gott", "wird", "uns", "bey\u00b7de", "wohl", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Topf von Thone sprach: Ich bin f\u00fcr dich zu kranck ", "tokens": ["Der", "Topf", "von", "Tho\u00b7ne", "sprach", ":", "Ich", "bin", "f\u00fcr", "dich", "zu", "kranck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Gew\u00fcnnest du mir an nur einen Wanck,", "tokens": ["Ge\u00b7w\u00fcn\u00b7nest", "du", "mir", "an", "nur", "ei\u00b7nen", "Wanck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Th\u00e4t ich von ungefehr nur einen Sto\u00df an dich,", "tokens": ["Th\u00e4t", "ich", "von", "un\u00b7ge\u00b7fehr", "nur", "ei\u00b7nen", "Sto\u00df", "an", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJD", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und stiessest du zur\u00fcck an mich,", "tokens": ["Und", "sties\u00b7sest", "du", "zu\u00b7r\u00fcck", "an", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So w\u00e4r ich erstes Streiches todt.", "tokens": ["So", "w\u00e4r", "ich", "ers\u00b7tes", "Strei\u00b7ches", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ich h\u00fcte mich vor dieser Noth.", "tokens": ["Ich", "h\u00fc\u00b7te", "mich", "vor", "die\u00b7ser", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Du \u00fcbertriffst an St\u00e4rcke mich,", "tokens": ["Du", "\u00fc\u00b7bert\u00b7riffst", "an", "St\u00e4r\u00b7cke", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Drum bin ich kein Gesell f\u00fcr dich.", "tokens": ["Drum", "bin", "ich", "kein", "Ge\u00b7sell", "f\u00fcr", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Die St\u00f6sse w\u00e4ren mein und dein,", "tokens": ["Die", "St\u00f6s\u00b7se", "w\u00e4\u00b7ren", "mein", "und", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "KON", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Schade w\u00e4re immer mein.", "tokens": ["Der", "Scha\u00b7de", "w\u00e4\u00b7re", "im\u00b7mer", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}