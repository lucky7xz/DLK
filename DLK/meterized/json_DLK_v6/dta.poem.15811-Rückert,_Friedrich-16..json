{"dta.poem.15811": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du unbeschriebnes Blatt, nun komm' und sei beschrieben", "tokens": ["Du", "un\u00b7be\u00b7schrieb\u00b7nes", "Blatt", ",", "nun", "komm'", "und", "sei", "be\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "ADV", "VVFIN", "KON", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Tochter meines Freunds, ich darf es nicht verschieben.", "tokens": ["Der", "Toch\u00b7ter", "mei\u00b7nes", "Freunds", ",", "ich", "darf", "es", "nicht", "ver\u00b7schie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein unbeschriebnes Blatt ist jugendlicher Sinn;", "tokens": ["Ein", "un\u00b7be\u00b7schrieb\u00b7nes", "Blatt", "ist", "ju\u00b7gend\u00b7li\u00b7cher", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Viel Sch\u00f6nes, Gutes drauf zu schreiben ist Gewinn.", "tokens": ["Viel", "Sch\u00f6\u00b7nes", ",", "Gu\u00b7tes", "drauf", "zu", "schrei\u00b7ben", "ist", "Ge\u00b7winn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "NN", "PAV", "PTKZU", "VVINF", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein fleckenloses Blatt ist jungfr\u00e4uliches Herz;", "tokens": ["Ein", "fle\u00b7cken\u00b7lo\u00b7ses", "Blatt", "ist", "jung\u00b7fr\u00e4u\u00b7li\u00b7ches", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nie furche drein die Schrift von Leidenschaft und Schmerz!", "tokens": ["Nie", "fur\u00b7che", "drein", "die", "Schrift", "von", "Lei\u00b7den\u00b7schaft", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Schreib fein bed\u00e4chtig so da\u00df nichts sei auszustreichen;", "tokens": ["Schreib", "fein", "be\u00b7d\u00e4ch\u00b7tig", "so", "da\u00df", "nichts", "sei", "aus\u00b7zu\u00b7strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ADV", "KOUS", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ausgestrichnes Wort ist ein entstellend Zeichen.", "tokens": ["Ein", "aus\u00b7ge\u00b7strich\u00b7nes", "Wort", "ist", "ein", "ent\u00b7stel\u00b7lend", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Zug, der bla\u00df erlischt, wird leichter angefrischt,", "tokens": ["Ein", "Zug", ",", "der", "bla\u00df", "er\u00b7lischt", ",", "wird", "leich\u00b7ter", "an\u00b7ge\u00b7frischt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein fehlgeschriebner wird nie gr\u00fcndlich weggewischt.", "tokens": ["Ein", "fehl\u00b7ge\u00b7schrieb\u00b7ner", "wird", "nie", "gr\u00fcnd\u00b7lich", "weg\u00b7ge\u00b7wischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vom Messerchen, wie fein es kratzte, bleibt die Spur,", "tokens": ["Vom", "Mes\u00b7ser\u00b7chen", ",", "wie", "fein", "es", "kratz\u00b7te", ",", "bleibt", "die", "Spur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nie wirds glatt, ob man mit Bimsstein dr\u00fcber fuhr.", "tokens": ["Und", "nie", "wirds", "glatt", ",", "ob", "man", "mit", "Bims\u00b7stein", "dr\u00fc\u00b7ber", "fuhr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "$,", "KOUS", "PIS", "APPR", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}