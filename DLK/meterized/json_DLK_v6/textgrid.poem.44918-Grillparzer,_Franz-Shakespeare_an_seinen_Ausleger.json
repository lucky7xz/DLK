{"textgrid.poem.44918": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Shakespeare an seinen Ausleger", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie alles sich dir zur Absicht eint,", "tokens": ["Wie", "al\u00b7les", "sich", "dir", "zur", "Ab\u00b7sicht", "eint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du scheinst in meiner Brust zu lesen!", "tokens": ["Du", "scheinst", "in", "mei\u00b7ner", "Brust", "zu", "le\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So h\u00e4tt ichs allerdings gemeint,", "tokens": ["So", "h\u00e4tt", "ichs", "al\u00b7ler\u00b7dings", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hei\u00dft: wenn ich Ludwig Tieck gewesen.", "tokens": ["Hei\u00dft", ":", "wenn", "ich", "Lud\u00b7wig", "Tieck", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOUS", "PPER", "NE", "NE", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}