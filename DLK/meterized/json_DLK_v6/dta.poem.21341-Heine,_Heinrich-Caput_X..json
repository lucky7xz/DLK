{"dta.poem.21341": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der lieben Westphalen womit ich so oft", "tokens": ["Der", "lie\u00b7ben", "West\u00b7pha\u00b7len", "wo\u00b7mit", "ich", "so", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In G\u00f6ttingen getrunken,", "tokens": ["In", "G\u00f6t\u00b7tin\u00b7gen", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis wir ger\u00fchrt einander an\u2019s Herz", "tokens": ["Bis", "wir", "ge\u00b7r\u00fchrt", "ein\u00b7an\u00b7der", "an's", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "PRF", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und unter die Tische gesunken!", "tokens": ["Und", "un\u00b7ter", "die", "Ti\u00b7sche", "ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ich habe sie immer so lieb gehabt,", "tokens": ["Ich", "ha\u00b7be", "sie", "im\u00b7mer", "so", "lieb", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die lieben, guten Westphalen,", "tokens": ["Die", "lie\u00b7ben", ",", "gu\u00b7ten", "West\u00b7pha\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ein Volk so fest, so sicher, so treu,", "tokens": ["Ein", "Volk", "so", "fest", ",", "so", "si\u00b7cher", ",", "so", "treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ganz ohne Glei\u00dfen und Prahlen.", "tokens": ["Ganz", "oh\u00b7ne", "Glei\u00b7\u00dfen", "und", "Prah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie standen sie pr\u00e4chtig auf der Mensur,", "tokens": ["Wie", "stan\u00b7den", "sie", "pr\u00e4ch\u00b7tig", "auf", "der", "Men\u00b7sur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit ihren L\u00f6wenherzen!", "tokens": ["Mit", "ih\u00b7ren", "L\u00f6\u00b7wen\u00b7her\u00b7zen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es fielen so grade, so ehrlich gemeint,", "tokens": ["Es", "fie\u00b7len", "so", "gra\u00b7de", ",", "so", "ehr\u00b7lich", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Quarten und die Terzen.", "tokens": ["Die", "Quar\u00b7ten", "und", "die", "Ter\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie fechten gut, sie trinken gut,", "tokens": ["Sie", "fech\u00b7ten", "gut", ",", "sie", "trin\u00b7ken", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn sie die Hand dir reichen,", "tokens": ["Und", "wenn", "sie", "die", "Hand", "dir", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zum Freundschaftsb\u00fcndni\u00df, dann weinen sie;", "tokens": ["Zum", "Freund\u00b7schafts\u00b7b\u00fcn\u00b7dni\u00df", ",", "dann", "wei\u00b7nen", "sie", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VVINF", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sind sentimentale Eichen.", "tokens": ["Sind", "sen\u00b7ti\u00b7men\u00b7ta\u00b7le", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}