{"textgrid.poem.50573": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "38. Milton mit Blindheit gestrafft", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blinde ", "tokens": ["Der", "blin\u00b7de"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und als hierauf ein Freund von seinem K\u00f6nig", "tokens": ["Und", "als", "hier\u00b7auf", "ein", "Freund", "von", "sei\u00b7nem", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Weil seinen ", "tokens": ["Weil", "sei\u00b7nen"], "token_info": ["word", "word"], "pos": ["KOUS", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und dass ich nunmehr ", "tokens": ["Und", "dass", "ich", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sprach ", "tokens": ["Sprach"], "token_info": ["word"], "pos": ["NN"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Denn die sind, ", "tokens": ["Denn", "die", "sind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}}}}}