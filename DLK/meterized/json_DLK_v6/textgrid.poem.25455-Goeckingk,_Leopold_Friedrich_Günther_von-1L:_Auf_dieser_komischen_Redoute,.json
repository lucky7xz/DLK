", "tokens": ["Doch", "mi\u00b7schen", "wir", "bei", "fei\u00b7ne\u00b7rem", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.84": {"text": "Uns selten nur in das Gew\u00fchle", "tokens": ["Uns", "sel\u00b7ten", "nur", "in", "das", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.85": {"text": "Des gro\u00dfen Contretanzes ein.", "tokens": ["Des", "gro\u00b7\u00dfen", "Con\u00b7tre\u00b7tan\u00b7zes", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.86": {"text": "In ihm tanzt der Betrug mit der Verf\u00fchrung vor;", "tokens": ["In", "ihm", "tanzt", "der", "Be\u00b7trug", "mit", "der", "Ver\u00b7f\u00fch\u00b7rung", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.87": {"text": "Die gro\u00dfe Welt folgt unter Scherz und Lachen,", "tokens": ["Die", "gro\u00b7\u00dfe", "Welt", "folgt", "un\u00b7ter", "Scherz", "und", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.88": {"text": "Die krummen Touren mit zu machen,", "tokens": ["Die", "krum\u00b7men", "Tou\u00b7ren", "mit", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.89": {"text": "Worin sich selbst die Arglist wohl verlor.", "tokens": ["Wo\u00b7rin", "sich", "selbst", "die", "Arg\u00b7list", "wohl", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.90": {"text": "Ja! auf des Bodens Spiegelgl\u00e4tte", "tokens": ["Ja", "!", "auf", "des", "Bo\u00b7dens", "Spie\u00b7gel\u00b7gl\u00e4t\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.91": {"text": "F\u00e4llt oft der Weise, wie der Thor;", "tokens": ["F\u00e4llt", "oft", "der", "Wei\u00b7se", ",", "wie", "der", "Thor", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.92": {"text": "Doch warum flicht er sich auch mit in diese Kette?", "tokens": ["Doch", "wa\u00b7rum", "flicht", "er", "sich", "auch", "mit", "in", "die\u00b7se", "Ket\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.93": {"text": "Wir aber h\u00fcpfen, fern von aller gro\u00dfen Welt,", "tokens": ["Wir", "a\u00b7ber", "h\u00fcp\u00b7fen", ",", "fern", "von", "al\u00b7ler", "gro\u00b7\u00dfen", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "ADJD", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.94": {"text": "Zwar