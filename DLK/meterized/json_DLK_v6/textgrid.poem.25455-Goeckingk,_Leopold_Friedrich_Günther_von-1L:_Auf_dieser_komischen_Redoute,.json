{"textgrid.poem.25455": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf dieser komischen Redoute,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dieser komischen Redoute,", "tokens": ["Auf", "die\u00b7ser", "ko\u00b7mi\u00b7schen", "Re\u00b7dou\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man nennet sie mit einer Sylbe: Welt!", "tokens": ["Man", "nen\u00b7net", "sie", "mit", "ei\u00b7ner", "Syl\u00b7be", ":", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hatt' ich mich unbemerkt im Winkel hingestellt,", "tokens": ["Hatt'", "ich", "mich", "un\u00b7be\u00b7merkt", "im", "Win\u00b7kel", "hin\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Tanze zugesehn, und hinter meinem Hute", "tokens": ["Dem", "Tan\u00b7ze", "zu\u00b7ge\u00b7sehn", ",", "und", "hin\u00b7ter", "mei\u00b7nem", "Hu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach Wunsch mich satt geweint, nach Herzenslust gelacht.", "tokens": ["Nach", "Wunsch", "mich", "satt", "ge\u00b7weint", ",", "nach", "Her\u00b7zens\u00b7lust", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die\u00df steckte nun nicht blo\u00df in ", "tokens": ["Die\u00df", "steck\u00b7te", "nun", "nicht", "blo\u00df", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ADV", "APPR"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nein! jedem ist die\u00df Fieber zugedacht.", "tokens": ["Nein", "!", "je\u00b7dem", "ist", "die\u00df", "Fie\u00b7ber", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PIS", "VAFIN", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ja! wenn ein Knabe, kaum der Ruthe", "tokens": ["Ja", "!", "wenn", "ein", "Kna\u00b7be", ",", "kaum", "der", "Ru\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$.", "KOUS", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Entwachsen, fertig schon mit seiner Rolle w\u00e4re,", "tokens": ["Ent\u00b7wach\u00b7sen", ",", "fer\u00b7tig", "schon", "mit", "sei\u00b7ner", "Rol\u00b7le", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So h\u00e4tt' er doch den ", "tokens": ["So", "h\u00e4tt'", "er", "doch", "den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So gut, als ein Methusalem, gemacht.", "tokens": ["So", "gut", ",", "als", "ein", "Me\u00b7thu\u00b7sa\u00b7lem", ",", "ge\u00b7macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der wunderliche Weise Griechenlandes,", "tokens": ["Der", "wun\u00b7der\u00b7li\u00b7che", "Wei\u00b7se", "Grie\u00b7chen\u00b7lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der nie aus seinem Gransen kam,", "tokens": ["Der", "nie", "aus", "sei\u00b7nem", "Gran\u00b7sen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "War doch noch immer unsers Standes,", "tokens": ["War", "doch", "noch", "im\u00b7mer", "un\u00b7sers", "Stan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und wenn er sonst sich je die M\u00fche nahm,", "tokens": ["Und", "wenn", "er", "sonst", "sich", "je", "die", "M\u00fc\u00b7he", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Betrachtungen auch \u00fcber sich zu machen,", "tokens": ["Be\u00b7trach\u00b7tun\u00b7gen", "auch", "\u00fc\u00b7ber", "sich", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "So mu\u00dft' er ja nothwendig \u2013 lachen.", "tokens": ["So", "mu\u00dft'", "er", "ja", "noth\u00b7wen\u00b7dig", "\u2013", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "$(", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der andre Herr, der sich ins Grab gelacht,", "tokens": ["Der", "and\u00b7re", "Herr", ",", "der", "sich", "ins", "Grab", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "War sicher doch mit Seufzern auch bekannt.", "tokens": ["War", "si\u00b7cher", "doch", "mit", "Seuf\u00b7zern", "auch", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "So oft, gepeitscht von eines W\u00fcthrichs Macht,", "tokens": ["So", "oft", ",", "ge\u00b7peitscht", "von", "ei\u00b7nes", "W\u00fcth\u00b7richs", "Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Die Unschuld sich in Blut und Thr\u00e4nen wand,", "tokens": ["Die", "Un\u00b7schuld", "sich", "in", "Blut", "und", "Thr\u00e4\u00b7nen", "wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "So mocht' er gleich von au\u00dfen fr\u00f6hlich scheinen,", "tokens": ["So", "mocht'", "er", "gleich", "von", "au\u00b7\u00dfen", "fr\u00f6h\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Er mu\u00dfte doch im Innern weinen.", "tokens": ["Er", "mu\u00df\u00b7te", "doch", "im", "In\u00b7nern", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Die Schauer hat der Weise wie der Thor!", "tokens": ["Die", "Schau\u00b7er", "hat", "der", "Wei\u00b7se", "wie", "der", "Thor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Nur mit dem gro\u00dfen Unterschiede,", "tokens": ["Nur", "mit", "dem", "gro\u00b7\u00dfen", "Un\u00b7ter\u00b7schie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der eine ", "tokens": ["Der", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Der \u00fcber viel, und kurz; dann hat er wieder Friede,", "tokens": ["Der", "\u00fc\u00b7ber", "viel", ",", "und", "kurz", ";", "dann", "hat", "er", "wie\u00b7der", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "$,", "KON", "ADJD", "$.", "ADV", "VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der andre, h\u00e4ngt sein Steckenpferd das Ohr,", "tokens": ["Der", "and\u00b7re", ",", "h\u00e4ngt", "sein", "Ste\u00b7cken\u00b7pferd", "das", "Ohr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Wird seines Schreiens nimmer m\u00fcde.", "tokens": ["Wird", "sei\u00b7nes", "Schrei\u00b7ens", "nim\u00b7mer", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "So sah ich's auf dem Maskenball' auch immer;", "tokens": ["So", "sah", "ich's", "auf", "dem", "Mas\u00b7ken\u00b7ball'", "auch", "im\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Der eine nur verbarg mit k\u00fcnstlichem Gesicht'", "tokens": ["Der", "ei\u00b7ne", "nur", "ver\u00b7barg", "mit", "k\u00fcnst\u00b7li\u00b7chem", "Ge\u00b7sicht'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Sein Lachen oder sein Gewimmer;", "tokens": ["Sein", "La\u00b7chen", "o\u00b7der", "sein", "Ge\u00b7wim\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Der andre lacht' und weint' im gro\u00dfen Tanzsaal' nicht,", "tokens": ["Der", "and\u00b7re", "lacht'", "und", "weint'", "im", "gro\u00b7\u00dfen", "Tanz\u00b7saal'", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wohl aber in dem Nebenzimmer.", "tokens": ["Wohl", "a\u00b7ber", "in", "dem", "Ne\u00b7ben\u00b7zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Der eine l\u00e4chelt nur, so oft der muntre Knabe,", "tokens": ["Der", "ei\u00b7ne", "l\u00e4\u00b7chelt", "nur", ",", "so", "oft", "der", "mun\u00b7tre", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der Witz, im Schoo\u00df' der Mutter Weisheit spielt,", "tokens": ["Der", "Witz", ",", "im", "Schoo\u00df'", "der", "Mut\u00b7ter", "Weis\u00b7heit", "spielt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Und, da\u00df des Weisen Herz sich labe,", "tokens": ["Und", ",", "da\u00df", "des", "Wei\u00b7sen", "Herz", "sich", "la\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Sein M\u00fcthlein an dem Thoren k\u00fchlt.", "tokens": ["Sein", "M\u00fcth\u00b7lein", "an", "dem", "Tho\u00b7ren", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Dann ist die\u00df L\u00e4cheln ein Gefl\u00fcster,", "tokens": ["Dann", "ist", "die\u00df", "L\u00e4\u00b7cheln", "ein", "Ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "S\u00fc\u00df, wie der ", "tokens": ["S\u00fc\u00df", ",", "wie", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.41": {"text": "Doch wenn bei seinem Stiefgeschwister,", "tokens": ["Doch", "wenn", "bei", "sei\u00b7nem", "Stief\u00b7ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Dem Aberwitz', des Meister Unsinns Sohn,", "tokens": ["Dem", "A\u00b7ber\u00b7witz'", ",", "des", "Meis\u00b7ter", "Un\u00b7sinns", "Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Der grobe Bootsknecht aus Natur,", "tokens": ["Der", "gro\u00b7be", "Boots\u00b7knecht", "aus", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Der Kammerherr aus Eigennutzen lacht:", "tokens": ["Der", "Kam\u00b7mer\u00b7herr", "aus", "Ei\u00b7gen\u00b7nut\u00b7zen", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Wohl dir, wenn dann die\u00df Lachen nur", "tokens": ["Wohl", "dir", ",", "wenn", "dann", "die\u00df", "La\u00b7chen", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "KOUS", "ADV", "PDS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Dem Ohr', und nicht zugleich dem Herzen, Ekel macht!", "tokens": ["Dem", "Ohr'", ",", "und", "nicht", "zu\u00b7gleich", "dem", "Her\u00b7zen", ",", "E\u00b7kel", "macht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PTKNEG", "ADV", "ART", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wohin ich sah, war alles Mummerei,", "tokens": ["Wo\u00b7hin", "ich", "sah", ",", "war", "al\u00b7les", "Mum\u00b7me\u00b7rei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Voll Thoren war's, die hier nach \u00e4u\u00dfern Trachten,", "tokens": ["Voll", "Tho\u00b7ren", "wa\u00b7r's", ",", "die", "hier", "nach", "\u00e4u\u00b7\u00dfern", "Trach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.49": {"text": "Ein jeder zwar nach seiner Phantasei,", "tokens": ["Ein", "je\u00b7der", "zwar", "nach", "sei\u00b7ner", "Phan\u00b7ta\u00b7sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["