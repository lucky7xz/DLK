{"dta.poem.309": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er preiset sein Gl\u00fcck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die s\u00fcsse Stunde bricht schon an/", "tokens": ["Die", "s\u00fcs\u00b7se", "Stun\u00b7de", "bricht", "schon", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinen Geist vergn\u00fcgen kan/", "tokens": ["Die", "mei\u00b7nen", "Geist", "ver\u00b7gn\u00fc\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit l\u00e4ngst begehrter Liebes-Lust.", "tokens": ["Mit", "l\u00e4ngst", "be\u00b7gehr\u00b7ter", "Lie\u00b7bes\u00b7Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es fehlet nur daran", "tokens": ["Es", "feh\u00b7let", "nur", "da\u00b7ran"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein bald verschwundner Augen-Blick", "tokens": ["Ein", "bald", "ver\u00b7schwund\u00b7ner", "Au\u00b7gen\u00b7Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So bl\u00fcht im Pracht mein h\u00f6chstes Gl\u00fcck", "tokens": ["So", "bl\u00fcht", "im", "Pracht", "mein", "h\u00f6chs\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das mich mit ", "tokens": ["Das", "mich", "mit"], "token_info": ["word", "word", "word"], "pos": ["PDS", "PRF", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Und ein in ", "tokens": ["Und", "ein", "in"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "In Kurtzem ists gethan/", "tokens": ["In", "Kurt\u00b7zem", "ists", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df ich mein Engel gantz gewi\u00df", "tokens": ["Da\u00df", "ich", "mein", "En\u00b7gel", "gantz", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In deinen engen Paradies", "tokens": ["In", "dei\u00b7nen", "en\u00b7gen", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Rosen brechen kan.", "tokens": ["Die", "Ro\u00b7sen", "bre\u00b7chen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein sch\u00f6ner Mund der spricht das Ja/", "tokens": ["Dein", "sch\u00f6\u00b7ner", "Mund", "der", "spricht", "das", "Ja", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ein gew\u00fcnschter Schlu\u00df ist da/", "tokens": ["Und", "ein", "ge\u00b7w\u00fcnschter", "Schlu\u00df", "ist", "da", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df ich dich bald/ Annehmlichste.", "tokens": ["Da\u00df", "ich", "dich", "bald", "/", "An\u00b7nehm\u00b7lichs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "$(", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In h\u00f6chster Lust umpfah", "tokens": ["In", "h\u00f6chs\u00b7ter", "Lust", "um\u00b7pfah"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie freut sich doch darob mein Geist", "tokens": ["Wie", "freut", "sich", "doch", "da\u00b7rob", "mein", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sich dein Paradies auffschleu\u00dft", "tokens": ["Da\u00df", "sich", "dein", "Pa\u00b7ra\u00b7dies", "auff\u00b7schleu\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mir die Anmuth nicht verwehrt", "tokens": ["Und", "mir", "die", "An\u00b7muth", "nicht", "ver\u00b7wehrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Bey dir ", "tokens": ["Bey", "dir"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Du theilest seine Sch\u00e4tze schon/", "tokens": ["Du", "thei\u00b7lest", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", "schon", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und reichest mir der Liebe-Lohn", "tokens": ["Und", "rei\u00b7chest", "mir", "der", "Lie\u00b7be\u00b7Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Durch ein vers\u00fc\u00dftes Ja.", "tokens": ["Durch", "ein", "ver\u00b7s\u00fc\u00df\u00b7tes", "Ja", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}