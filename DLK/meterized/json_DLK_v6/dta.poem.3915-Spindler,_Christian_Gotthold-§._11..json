{"dta.poem.3915": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "\u00a7. 11.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wan\u0303 die Verlobung nun von beyden ist geschehen,", "tokens": ["Wa\u00f1", "die", "Ver\u00b7lo\u00b7bung", "nun", "von", "bey\u00b7den", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "PIS", "VAFIN", "VVPP", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "So mag der Br\u00e4utigam bey Leibe nichts versehen.", "tokens": ["So", "mag", "der", "Br\u00e4u\u00b7ti\u00b7gam", "bey", "Lei\u00b7be", "nichts", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NE", "APPR", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er care\u00dfiret sie, er hat dabey die Wahl,", "tokens": ["Er", "ca\u00b7re\u00b7\u00dfi\u00b7ret", "sie", ",", "er", "hat", "da\u00b7bey", "die", "Wahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df, wenn er k\u00fcssen will, so thut ers tausendmahl.", "tokens": ["Da\u00df", ",", "wenn", "er", "k\u00fcs\u00b7sen", "will", ",", "so", "thut", "ers", "tau\u00b7send\u00b7mahl", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}