{"textgrid.poem.38867": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl gilt es Sturm und Krieg ist vorgeschrieben", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl gilt es Sturm und Krieg ist vorgeschrieben", "tokens": ["Wohl", "gilt", "es", "Sturm", "und", "Krieg", "ist", "vor\u00b7ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Manne der gewappnet steht zum Streite,", "tokens": ["Dem", "Man\u00b7ne", "der", "ge\u00b7wapp\u00b7net", "steht", "zum", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wer des Friedens wegen K\u00e4mpfe scheute,", "tokens": ["Doch", "wer", "des", "Frie\u00b7dens", "we\u00b7gen", "K\u00e4mp\u00b7fe", "scheu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist niemals noch dem Himmel treu verblieben.", "tokens": ["Ist", "nie\u00b7mals", "noch", "dem", "Him\u00b7mel", "treu", "ver\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des einen Leben ist ein ewig Lieben,", "tokens": ["Des", "ei\u00b7nen", "Le\u00b7ben", "ist", "ein", "e\u00b7wig", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VAFIN", "ART", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihm giebt die Kunst freiwillig sich zur Beute,", "tokens": ["Ihm", "giebt", "die", "Kunst", "frei\u00b7wil\u00b7lig", "sich", "zur", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der andre schweift durch Land- und Meeres-Weite,", "tokens": ["Der", "and\u00b7re", "schweift", "durch", "Lan\u00b7d", "und", "Mee\u00b7res\u00b7Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vom Schicksal ohne Rast umher getrieben.", "tokens": ["Vom", "Schick\u00b7sal", "oh\u00b7ne", "Rast", "um\u00b7her", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die goldne Frucht vom Wunderbaum zu brechen,", "tokens": ["Die", "gold\u00b7ne", "Frucht", "vom", "Wun\u00b7der\u00b7baum", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geht Herkules rasch nach den Hesperiden,", "tokens": ["Geht", "Her\u00b7ku\u00b7les", "rasch", "nach", "den", "Hes\u00b7pe\u00b7ri\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Nach m\u00fchevollem Kampf gekr\u00f6nt zum Gotte.", "tokens": ["Nach", "m\u00fc\u00b7he\u00b7vol\u00b7lem", "Kampf", "ge\u00b7kr\u00f6nt", "zum", "Got\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er zwang vorher manch wild' unb\u00e4nd'ge Rotte;", "tokens": ["Er", "zwang", "vor\u00b7her", "manch", "wild'", "un\u00b7b\u00e4n\u00b7d'\u00b7ge", "Rot\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bevor er einging zu dem ewgen Frieden,", "tokens": ["Be\u00b7vor", "er", "ein\u00b7ging", "zu", "dem", "ew\u00b7gen", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durft' Fried' und Freude nicht die St\u00e4rke schw\u00e4chen.", "tokens": ["Durft'", "Fried'", "und", "Freu\u00b7de", "nicht", "die", "St\u00e4r\u00b7ke", "schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}