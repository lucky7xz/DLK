{"dta.poem.8148": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtslied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Ein milder Stern hernieder lacht;", "tokens": ["Ein", "mil\u00b7der", "Stern", "her\u00b7nie\u00b7der", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein weihrauchs\u00fc\u00dfes Harzged\u00fcfte", "tokens": ["Ein", "weih\u00b7rauch\u00b7s\u00fc\u00b7\u00dfes", "Harz\u00b7ge\u00b7d\u00fcf\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durchschwimmet tr\u00e4umerisch die L\u00fcfte,", "tokens": ["Durch\u00b7schwim\u00b7met", "tr\u00e4u\u00b7me\u00b7risch", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kerzenhelle wird die Nacht.", "tokens": ["Und", "ker\u00b7zen\u00b7hel\u00b7le", "wird", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir ist das Herz so froh erschrocken,", "tokens": ["Mir", "ist", "das", "Herz", "so", "froh", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die liebe Weihnachtszeit!", "tokens": ["Das", "ist", "die", "lie\u00b7be", "Weih\u00b7nachts\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6re fernher Kirchenglocken", "tokens": ["Ich", "h\u00f6\u00b7re", "fern\u00b7her", "Kir\u00b7chen\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich lieblich heimathlich verlocken", "tokens": ["Mich", "lieb\u00b7lich", "hei\u00b7math\u00b7lich", "ver\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In m\u00e4rchenstille Herrlichkeit.", "tokens": ["In", "m\u00e4r\u00b7chen\u00b7stil\u00b7le", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein frommer Zauber h\u00e4lt mich wieder", "tokens": ["Ein", "from\u00b7mer", "Zau\u00b7ber", "h\u00e4lt", "mich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Anbetend, staunend mu\u00df ich stehn;", "tokens": ["An\u00b7be\u00b7tend", ",", "stau\u00b7nend", "mu\u00df", "ich", "stehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Es sinkt auf meine Augenlider", "tokens": ["Es", "sinkt", "auf", "mei\u00b7ne", "Au\u00b7gen\u00b7li\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein goldner Kindertraum hernieder,", "tokens": ["Ein", "gold\u00b7ner", "Kin\u00b7der\u00b7traum", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich f\u00fchl's, ein Wunder ist geschehn.", "tokens": ["Ich", "f\u00fchl's", ",", "ein", "Wun\u00b7der", "ist", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}