{"textgrid.poem.67773": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Weihnachtsgesang", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewig aus des Vaters Herzen", "tokens": ["E\u00b7wig", "aus", "des", "Va\u00b7ters", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns geborner Gottessohn!", "tokens": ["Uns", "ge\u00b7bor\u00b7ner", "Got\u00b7tes\u00b7sohn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Sch\u00f6pfung Licht und Leben,", "tokens": ["Al\u00b7ler", "Sch\u00f6p\u00b7fung", "Licht", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller S\u00fcnder Gnadenthron!", "tokens": ["Al\u00b7ler", "S\u00fcn\u00b7der", "Gna\u00b7dent\u00b7hron", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den Gefangnen ein Erretter", "tokens": ["Den", "Ge\u00b7fang\u00b7nen", "ein", "Er\u00b7ret\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Streitern ew'ger Lohn!", "tokens": ["Und", "den", "Strei\u00b7tern", "ew'\u00b7ger", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kommst Du endlich auf die Erde,", "tokens": ["Kommst", "Du", "end\u00b7lich", "auf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den so lang' der V\u00e4ter Herz", "tokens": ["Den", "so", "lang'", "der", "V\u00e4\u00b7ter", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah und freute sich, erhob sich", "tokens": ["Sah", "und", "freu\u00b7te", "sich", ",", "er\u00b7hob", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "$,", "VVFIN", "PRF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch im Tode himmelw\u00e4rts,", "tokens": ["Noch", "im", "To\u00b7de", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dich zu fassen, Dich zu ziehen", "tokens": ["Dich", "zu", "fas\u00b7sen", ",", "Dich", "zu", "zie\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fcher schon in unsern Schmerz?", "tokens": ["Fr\u00fc\u00b7her", "schon", "in", "un\u00b7sern", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kommst Du endlich? Und es schlummert", "tokens": ["Kommst", "Du", "end\u00b7lich", "?", "Und", "es", "schlum\u00b7mert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$.", "KON", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings um Dich Dein Volk und Land!", "tokens": ["Rings", "um", "Dich", "Dein", "Volk", "und", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nacht ist weit umher, und Hirten,", "tokens": ["Nacht", "ist", "weit", "um\u00b7her", ",", "und", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "PTKVZ", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Armen Hirten wird bekannt,", "tokens": ["Ar\u00b7men", "Hir\u00b7ten", "wird", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer Du bist! Und hoch in L\u00fcften", "tokens": ["Wer", "Du", "bist", "!", "Und", "hoch", "in", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VAFIN", "$.", "KON", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird Dein himmlisch Reich genannt:", "tokens": ["Wird", "Dein", "himm\u00b7lisch", "Reich", "ge\u00b7nannt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbehre sei Gott in den H\u00f6hen,", "tokens": ["\u00bb", "eh\u00b7re", "sei", "Gott", "in", "den", "H\u00f6\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Fried' auf Erden! Aller Welt", "tokens": ["Fried'", "auf", "Er\u00b7den", "!", "Al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$.", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heil und Wohlgefallen!\u00ab Segnend", "tokens": ["Heil", "und", "Wohl\u00b7ge\u00b7fal\u00b7len", "!", "\u00ab", "Seg\u00b7nend"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["NN", "KON", "NN", "$.", "$(", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ruhe rings des Himmels Zelt", "tokens": ["Ru\u00b7he", "rings", "des", "Him\u00b7mels", "Zelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den V\u00f6lkern, und die V\u00f6lker", "tokens": ["Auf", "den", "V\u00f6l\u00b7kern", ",", "und", "die", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Preisen Gott und ihren Held!", "tokens": ["Prei\u00b7sen", "Gott", "und", "ih\u00b7ren", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Also h\u00f6r' ich Himmelsch\u00f6re,", "tokens": ["Al\u00b7so", "h\u00f6r'", "ich", "Him\u00b7mel\u00b7sch\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also seh' ich \u00fcber Dir", "tokens": ["Al\u00b7so", "seh'", "ich", "\u00fc\u00b7ber", "Dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenen neuen Stern der Ehre;", "tokens": ["Je\u00b7nen", "neu\u00b7en", "Stern", "der", "Eh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er ruft V\u00f6lkern, er ruft mir:", "tokens": ["Er", "ruft", "V\u00f6l\u00b7kern", ",", "er", "ruft", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbh\u00f6ret's, V\u00f6lker, h\u00f6r es, Armer!", "tokens": ["\u00bb", "h\u00f6\u00b7ret's", ",", "V\u00f6l\u00b7ker", ",", "h\u00f6r", "es", ",", "Ar\u00b7mer", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Sohn bestrahl' ich hier!\u00ab", "tokens": ["Got\u00b7tes", "Sohn", "be\u00b7strahl'", "ich", "hier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}