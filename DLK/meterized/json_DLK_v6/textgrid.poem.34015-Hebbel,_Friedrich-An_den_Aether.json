{"textgrid.poem.34015": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An den Aether", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Allewiger und unbegr\u00e4nzter Aether!", "tokens": ["Al\u00b7le\u00b7wi\u00b7ger", "und", "un\u00b7be\u00b7gr\u00e4nz\u00b7ter", "A\u00b7e\u00b7ther", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "NN", "$."], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.2": {"text": "Durch's Engste, wie durch's Weiteste Ergo\u00dfner!", "tokens": ["Durch's", "Engs\u00b7te", ",", "wie", "durch's", "Wei\u00b7tes\u00b7te", "Er\u00b7go\u00df\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von keinem Ring des Daseins Ausgeschlo\u00dfner!", "tokens": ["Von", "kei\u00b7nem", "Ring", "des", "Da\u00b7seins", "Aus\u00b7ge\u00b7schlo\u00df\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von jedem Hauch des Lebens still Durchwehter!", "tokens": ["Von", "je\u00b7dem", "Hauch", "des", "Le\u00b7bens", "still", "Durch\u00b7weh\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des Unerforschten einziger Vertreter!", "tokens": ["Des", "Un\u00b7er\u00b7forschten", "ein\u00b7zi\u00b7ger", "Ver\u00b7tre\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein erster und sein w\u00fcrdigster Entspro\u00dfner!", "tokens": ["Sein", "ers\u00b7ter", "und", "sein", "w\u00fcr\u00b7digs\u00b7ter", "Ent\u00b7spro\u00df\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von ihm allein in tiefster Ruh' Umflo\u00dfner!", "tokens": ["Von", "ihm", "al\u00b7lein", "in", "tiefs\u00b7ter", "Ruh'", "Um\u00b7flo\u00df\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir gegen\u00fcber werd' auch ich ein Beter!", "tokens": ["Dir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "werd'", "auch", "ich", "ein", "Be\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein schweifend Auge, das dich gern umspannte,", "tokens": ["Mein", "schwei\u00b7fend", "Au\u00b7ge", ",", "das", "dich", "gern", "um\u00b7spann\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Schlie\u00dft sich vor dir in Ehrfurcht, eh' es scheitert,", "tokens": ["Schlie\u00dft", "sich", "vor", "dir", "in", "Ehr\u00b7furcht", ",", "eh'", "es", "schei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "APPR", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn Nichts ermi\u00dft der Blick, als seine Schranken.", "tokens": ["Denn", "Nichts", "er\u00b7mi\u00dft", "der", "Blick", ",", "als", "sei\u00b7ne", "Schran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So auch mein Geist vor Gott, denn er erkannte,", "tokens": ["So", "auch", "mein", "Geist", "vor", "Gott", ",", "denn", "er", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "APPR", "NN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er, umfa\u00dft, sich nie so sehr erweitert,", "tokens": ["Da\u00df", "er", ",", "um\u00b7fa\u00dft", ",", "sich", "nie", "so", "sehr", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "$,", "PRF", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Allumfasser wieder zu umranken.", "tokens": ["Den", "Al\u00b7lum\u00b7fas\u00b7ser", "wie\u00b7der", "zu", "um\u00b7ran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}