{"textgrid.poem.47008": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herein von drau\u00dfen in verworrnem Schwalle", "tokens": ["Her\u00b7ein", "von", "drau\u00b7\u00dfen", "in", "ver\u00b7worr\u00b7nem", "Schwal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verletzt mein Ohr ein Schwirren und ein Summen,", "tokens": ["Ver\u00b7letzt", "mein", "Ohr", "ein", "Schwir\u00b7ren", "und", "ein", "Sum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Flattern, Schnattern, Kr\u00e4chzen, Bl\u00f6ken, Brummen,", "tokens": ["Ein", "Flat\u00b7tern", ",", "Schnat\u00b7tern", ",", "Kr\u00e4ch\u00b7zen", ",", "Bl\u00f6\u00b7ken", ",", "Brum\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gefl\u00fcgel in dem Hofe, Vieh im Stalle.", "tokens": ["Ge\u00b7fl\u00fc\u00b7gel", "in", "dem", "Ho\u00b7fe", ",", "Vieh", "im", "Stal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und innen hier die Tisch' und B\u00e4nke alle", "tokens": ["Und", "in\u00b7nen", "hier", "die", "Tisch'", "und", "B\u00e4n\u00b7ke", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "KON", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besetzt mit viel Gesichtern, matten, dummen,", "tokens": ["Be\u00b7setzt", "mit", "viel", "Ge\u00b7sich\u00b7tern", ",", "mat\u00b7ten", ",", "dum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bepflanzt mit viel Gestalten, tr\u00e4gen, krummen;", "tokens": ["Be\u00b7pflanzt", "mit", "viel", "Ge\u00b7stal\u00b7ten", ",", "tr\u00e4\u00b7gen", ",", "krum\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "PIAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Aug' ist mit dem Ohr im gleichen Falle.", "tokens": ["Das", "Aug'", "ist", "mit", "dem", "Ohr", "im", "glei\u00b7chen", "Fal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da tritt herein im schlankgeschn\u00fcrten Mieder", "tokens": ["Da", "tritt", "her\u00b7ein", "im", "schlank\u00b7ge\u00b7schn\u00fcr\u00b7ten", "Mie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein M\u00e4dchen, das mit einem Gru\u00df mich kirret,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "mit", "ei\u00b7nem", "Gru\u00df", "mich", "kir\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von allen Sinnen f\u00e4llt es mir wie Schuppen.", "tokens": ["Von", "al\u00b7len", "Sin\u00b7nen", "f\u00e4llt", "es", "mir", "wie", "Schup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Wirtschaft Mi\u00dflaut schmilzt in sanfte Lieder,", "tokens": ["Der", "Wirt\u00b7schaft", "Mi\u00df\u00b7laut", "schmilzt", "in", "sanf\u00b7te", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sowie sie spricht; und wie sie blickt, entwirret", "tokens": ["So\u00b7wie", "sie", "spricht", ";", "und", "wie", "sie", "blickt", ",", "ent\u00b7wir\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "KON", "PWAV", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich rings der Kn\u00e4u'l in wohlgef\u00e4llige Gruppen.", "tokens": ["Sich", "rings", "der", "Kn\u00e4u'l", "in", "wohl\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Grup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}