"text": "Als den Klugen du gefielst", "tokens": ["Als", "den", "Klu\u00b7gen", "du", "ge\u00b7fielst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mi\u00dffielst den Dummen.", "tokens": ["Und", "mi\u00df\u00b7fielst", "den", "Dum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}