{"textgrid.poem.61253": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lied vom Wein", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun gr\u00fc\u00df' dich Gott, du Himmelstau,", "tokens": ["Nun", "gr\u00fc\u00df'", "dich", "Gott", ",", "du", "Him\u00b7mel\u00b7stau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Ehrenpreis der Rebenau,", "tokens": ["Du", "Eh\u00b7ren\u00b7preis", "der", "Re\u00b7be\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Wein, du Kind der Sonnen!", "tokens": ["O", "Wein", ",", "du", "Kind", "der", "Son\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie blinkst du mich so wohlgetan", "tokens": ["Wie", "blinkst", "du", "mich", "so", "wohl\u00b7ge\u00b7tan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus hellgeschliffnem Becher an", "tokens": ["Aus", "hell\u00b7ge\u00b7schliff\u00b7nem", "Be\u00b7cher", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ein g\u00fcldner Bronnen!", "tokens": ["Als", "wie", "ein", "g\u00fcld\u00b7ner", "Bron\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O komm empor an meinen Mund", "tokens": ["O", "komm", "em\u00b7por", "an", "mei\u00b7nen", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00fclle mir das Herz zur Stund'", "tokens": ["Und", "f\u00fcl\u00b7le", "mir", "das", "Herz", "zur", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bis auf den Grund", "tokens": ["Bis", "auf", "den", "Grund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Mit allen deinen Wonnen!", "tokens": ["Mit", "al\u00b7len", "dei\u00b7nen", "Won\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So wie das Licht den Edelstein", "tokens": ["So", "wie", "das", "Licht", "den", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchstr\u00f6mt mit seinem klaren Schein,", "tokens": ["Durch\u00b7str\u00f6mt", "mit", "sei\u00b7nem", "kla\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollst du den Sinn mir kl\u00e4ren;", "tokens": ["Sollst", "du", "den", "Sinn", "mir", "kl\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was noch tr\u00fcb in meinem Mut,", "tokens": ["Und", "was", "noch", "tr\u00fcb", "in", "mei\u00b7nem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das soll hinweg die heil'ge Glut", "tokens": ["Das", "soll", "hin\u00b7weg", "die", "heil'\u00b7ge", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der feuchten Flamme zehren.", "tokens": ["Der", "feuch\u00b7ten", "Flam\u00b7me", "zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich stimme dir daf\u00fcr zum Zoll", "tokens": ["Ich", "stim\u00b7me", "dir", "da\u00b7f\u00fcr", "zum", "Zoll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Lied an aller Freuden voll,", "tokens": ["Ein", "Lied", "an", "al\u00b7ler", "Freu\u00b7den", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das l\u00e4ngst mir schwoll", "tokens": ["Das", "l\u00e4ngst", "mir", "schwoll"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Im Busen dir zu Ehren.", "tokens": ["Im", "Bu\u00b7sen", "dir", "zu", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ja, gro\u00df ist deiner Wunder Kraft", "tokens": ["Ja", ",", "gro\u00df", "ist", "dei\u00b7ner", "Wun\u00b7der", "Kraft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Freud' und wo in Kummers Haft", "tokens": ["In", "Freud'", "und", "wo", "in", "Kum\u00b7mers", "Haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "PWAV", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Einsam ein Mann mag trinken;", "tokens": ["Ein\u00b7sam", "ein", "Mann", "mag", "trin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Du b\u00e4ndigst mild den dumpfen Gram,", "tokens": ["Du", "b\u00e4n\u00b7digst", "mild", "den", "dum\u00b7pfen", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4\u00dft ihn, zu Tr\u00e4nen wundersam", "tokens": ["L\u00e4\u00dft", "ihn", ",", "zu", "Tr\u00e4\u00b7nen", "wun\u00b7der\u00b7sam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gel\u00f6st, im Kelch versinken.", "tokens": ["Ge\u00b7l\u00f6st", ",", "im", "Kelch", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O k\u00f6stlich wird der Becher da,", "tokens": ["O", "k\u00f6st\u00b7lich", "wird", "der", "Be\u00b7cher", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie jener, drin Kleopatra", "tokens": ["Wie", "je\u00b7ner", ",", "drin", "Kleo\u00b7pat\u00b7ra"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "PDS", "$,", "ADV", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Perle sah", "tokens": ["Die", "Per\u00b7le", "sah"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Zergehn mit klarem Blinken.", "tokens": ["Zer\u00b7gehn", "mit", "kla\u00b7rem", "Blin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es schl\u00e4ft in dir die alte Zeit,", "tokens": ["Es", "schl\u00e4ft", "in", "dir", "die", "al\u00b7te", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hohe Lust, das s\u00fc\u00dfe Leid,", "tokens": ["Die", "ho\u00b7he", "Lust", ",", "das", "s\u00fc\u00b7\u00dfe", "Leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Minne zartes Kosen;", "tokens": ["Der", "Min\u00b7ne", "zar\u00b7tes", "Ko\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es schl\u00e4ft in dir das Lied versch\u00e4mt,", "tokens": ["Es", "schl\u00e4ft", "in", "dir", "das", "Lied", "ver\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Lied, das fromm den Sturm bez\u00e4hmt,", "tokens": ["Das", "Lied", ",", "das", "fromm", "den", "Sturm", "be\u00b7z\u00e4hmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Flut und Leben tosen.", "tokens": ["Wenn", "Flut", "und", "Le\u00b7ben", "to\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Jugend hebt sich wunderbar", "tokens": ["Die", "Ju\u00b7gend", "hebt", "sich", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus dir empor und kr\u00e4nzet klar", "tokens": ["Aus", "dir", "em\u00b7por", "und", "kr\u00e4n\u00b7zet", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Silberhaar", "tokens": ["Das", "Sil\u00b7ber\u00b7haar"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Mit frischen Maienrosen.", "tokens": ["Mit", "fri\u00b7schen", "Mai\u00b7en\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und was der Mensch, vom Gott bewegt,", "tokens": ["Und", "was", "der", "Mensch", ",", "vom", "Gott", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tiefgeheim im Busen tr\u00e4gt,", "tokens": ["So", "tief\u00b7ge\u00b7heim", "im", "Bu\u00b7sen", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als sei's der Welt versunken,", "tokens": ["Als", "sei's", "der", "Welt", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du pochst mit goldnem Finger dran,", "tokens": ["Du", "pochst", "mit", "gold\u00b7nem", "Fin\u00b7ger", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis da\u00df der Schrein sich aufgetan,", "tokens": ["Bis", "da\u00df", "der", "Schrein", "sich", "auf\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Sch\u00e4tze prunken.", "tokens": ["Und", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", "prun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da klingt herauf der Weisheit Wort,", "tokens": ["Da", "klingt", "her\u00b7auf", "der", "Weis\u00b7heit", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da taucht empor der Liebe Hort,", "tokens": ["Da", "taucht", "em\u00b7por", "der", "Lie\u00b7be", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Um fort und fort", "tokens": ["Um", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Zu gl\u00fchn in hellen Funken.", "tokens": ["Zu", "gl\u00fchn", "in", "hel\u00b7len", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und bist du selber nicht, o Wein,", "tokens": ["Und", "bist", "du", "sel\u00b7ber", "nicht", ",", "o", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Spiegel nur und Widerschein", "tokens": ["Ein", "Spie\u00b7gel", "nur", "und", "Wi\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Wandel unsrer Tage?", "tokens": ["Vom", "Wan\u00b7del", "uns\u00b7rer", "Ta\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gebrochen, bis zum Kern versehrt,", "tokens": ["Ge\u00b7bro\u00b7chen", ",", "bis", "zum", "Kern", "ver\u00b7sehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wirst du zu Glut und Geist verkl\u00e4rt", "tokens": ["Wirst", "du", "zu", "Glut", "und", "Geist", "ver\u00b7kl\u00e4rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und selbst ein Bann der Plage.", "tokens": ["Und", "selbst", "ein", "Bann", "der", "Pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dein Feuer s\u00fc\u00df, das siegreich loht,", "tokens": ["Dein", "Feu\u00b7er", "s\u00fc\u00df", ",", "das", "sieg\u00b7reich", "loht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Spricht dann von Glorien nach der Not,", "tokens": ["Spricht", "dann", "von", "Glo\u00b7ri\u00b7en", "nach", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Und da\u00df aus Tod", "tokens": ["Und", "da\u00df", "aus", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Der Jugend Flamme 