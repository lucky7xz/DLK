{"textgrid.poem.31442": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sey still / gib Gott die Ehr! Er wei\u00df die zeit zu finden:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey still / gib Gott die Ehr! Er wei\u00df die zeit zu finden:", "tokens": ["Sey", "still", "/", "gib", "Gott", "die", "Ehr", "!", "Er", "wei\u00df", "die", "zeit", "zu", "fin\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$(", "VVIMP", "NN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in seiner vorsicht schatz ist sie schon ausgetheilt.", "tokens": ["in", "sei\u00b7ner", "vor\u00b7sicht", "schatz", "ist", "sie", "schon", "aus\u00b7ge\u00b7theilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein Ziel wird durch gedult / durch murren nicht / ereilt.", "tokens": ["Sein", "Ziel", "wird", "durch", "ge\u00b7dult", "/", "durch", "mur\u00b7ren", "nicht", "/", "er\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NN", "$(", "APPR", "VVFIN", "PTKNEG", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Ball pflegt / schl\u00e4gt man stark / aufs neue zu verschwinden.", "tokens": ["Der", "Ball", "pflegt", "/", "schl\u00e4gt", "man", "stark", "/", "aufs", "neu\u00b7e", "zu", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "PIS", "ADJD", "$(", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Schiff laufft Hafen ein / bey sanfft gelinden winden:", "tokens": ["Ein", "Schiff", "laufft", "Ha\u00b7fen", "ein", "/", "bey", "sanfft", "ge\u00b7lin\u00b7den", "win\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "ART", "$(", "APPR", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der ungedultes-Brau\u00df die Anfurt nur verweilt.", "tokens": ["der", "un\u00b7ge\u00b7dul\u00b7tes\u00b7Brau\u00df", "die", "An\u00b7furt", "nur", "ver\u00b7weilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein still-verwundter wird viel ehender geheilt.", "tokens": ["Ein", "still\u00b7ver\u00b7wund\u00b7ter", "wird", "viel", "e\u00b7hen\u00b7der", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von freyheit unbekriegt / l\u00e4sst Er sich eher binden.", "tokens": ["Von", "frey\u00b7heit", "un\u00b7be\u00b7kriegt", "/", "l\u00e4sst", "Er", "sich", "e\u00b7her", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$(", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sich blinden / Gott vertraut / ist Haupt-vorsichtigkeit.", "tokens": ["Sich", "blin\u00b7den", "/", "Gott", "ver\u00b7traut", "/", "ist", "Haup\u00b7tvor\u00b7sich\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$(", "NN", "ADJD", "$(", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So h\u00f6her ist sein Ziel / so minder es zu greiffen:", "tokens": ["So", "h\u00f6\u00b7her", "ist", "sein", "Ziel", "/", "so", "min\u00b7der", "es", "zu", "greif\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "es wunder-herrlicht sich / mit l\u00e4ngerung der zeit.", "tokens": ["es", "wun\u00b7der\u00b7herr\u00b7licht", "sich", "/", "mit", "l\u00e4n\u00b7ge\u00b7rung", "der", "zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$(", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die kostbarn Geister S\u00e4fft / gemach / gemach her tr\u00e4uffen.", "tokens": ["Die", "kost\u00b7barn", "Geis\u00b7ter", "S\u00e4fft", "/", "ge\u00b7mach", "/", "ge\u00b7mach", "her", "tr\u00e4uf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ADV", "$(", "ADV", "APZR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "so wenig als die Sonn versiht ihr untergehn /", "tokens": ["so", "we\u00b7nig", "als", "die", "Sonn", "ver\u00b7siht", "ihr", "un\u00b7ter\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "kan Gott die rechte Zeit der H\u00fclff' auch \u00fcbersehn.", "tokens": ["kan", "Gott", "die", "rech\u00b7te", "Zeit", "der", "H\u00fclff'", "auch", "\u00fc\u00b7ber\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}