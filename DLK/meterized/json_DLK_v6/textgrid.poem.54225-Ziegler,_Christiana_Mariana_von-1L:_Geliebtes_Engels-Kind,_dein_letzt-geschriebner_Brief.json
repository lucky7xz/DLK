{"textgrid.poem.54225": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geliebtes Engels-Kind, dein letzt-geschriebner Brief", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Engels-Kind, dein letzt-geschriebner Brief", "tokens": ["Ge\u00b7lieb\u00b7tes", "En\u00b7gels\u00b7Kind", ",", "dein", "letz\u00b7tge\u00b7schrieb\u00b7ner", "Brief"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kam um dieselbe Zeit, als man zur Tafel rief;", "tokens": ["Kam", "um", "die\u00b7sel\u00b7be", "Zeit", ",", "als", "man", "zur", "Ta\u00b7fel", "rief", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "NN", "$,", "KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lie\u00df das Essen stehn, zu dem ich mich schon schickte,", "tokens": ["Ich", "lie\u00df", "das", "Es\u00b7sen", "stehn", ",", "zu", "dem", "ich", "mich", "schon", "schick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,", "APPR", "PRELS", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dieweil mein Auge was in deinem Blatt erblickte,", "tokens": ["Die\u00b7weil", "mein", "Au\u00b7ge", "was", "in", "dei\u00b7nem", "Blatt", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das meine Seel und Geist weit besser s\u00e4ttgen kan,", "tokens": ["Das", "mei\u00b7ne", "Seel", "und", "Geist", "weit", "bes\u00b7ser", "s\u00e4tt\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "KON", "NN", "ADJD", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als das so schmackbare und s\u00fc\u00df-beschriebne Man;", "tokens": ["Als", "das", "so", "schmack\u00b7ba\u00b7re", "und", "s\u00fc\u00df\u00b7be\u00b7schrieb\u00b7ne", "Man", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADJA", "KON", "ADJA", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn die Versicherung, die mir dein Kiel gegeben,", "tokens": ["Denn", "die", "Ver\u00b7si\u00b7che\u00b7rung", ",", "die", "mir", "dein", "Kiel", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Heist eine Kost, von der ich gantz allein kan leben.", "tokens": ["Heist", "ei\u00b7ne", "Kost", ",", "von", "der", "ich", "gantz", "al\u00b7lein", "kan", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nur dieses schmertzet mich und mehret meine Noth,", "tokens": ["Nur", "die\u00b7ses", "schmert\u00b7zet", "mich", "und", "meh\u00b7ret", "mei\u00b7ne", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVFIN", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wormit auch ausserdem mir dein Entfernen droht,", "tokens": ["Wor\u00b7mit", "auch", "aus\u00b7ser\u00b7dem", "mir", "dein", "Ent\u00b7fer\u00b7nen", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df dich der Zweifel will auf die Gedancken treiben,", "tokens": ["Da\u00df", "dich", "der", "Zwei\u00b7fel", "will", "auf", "die", "Ge\u00b7dan\u00b7cken", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ob ich in frembder Lufft getreu dir w\u00fcrde bleiben.", "tokens": ["Ob", "ich", "in", "fremb\u00b7der", "Lufft", "ge\u00b7treu", "dir", "w\u00fcr\u00b7de", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJD", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ists m\u00f6glich? da\u00df dir auch hiervon nur tr\u00e4umen mag,", "tokens": ["Ists", "m\u00f6g\u00b7lich", "?", "da\u00df", "dir", "auch", "hier\u00b7von", "nur", "tr\u00e4u\u00b7men", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "KOUS", "PPER", "ADV", "PAV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dergleichen Argwohn schreckt mehr als ein Donnerschlag,", "tokens": ["Derg\u00b7lei\u00b7chen", "Arg\u00b7wohn", "schreckt", "mehr", "als", "ein", "Don\u00b7ner\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Engel, meine Treu wird nun und nimmer wancken,", "tokens": ["Mein", "En\u00b7gel", ",", "mei\u00b7ne", "Treu", "wird", "nun", "und", "nim\u00b7mer", "wan\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich, ich, ich opffre dir Seel, Sinnen und Gedancken.", "tokens": ["Ich", ",", "ich", ",", "ich", "opff\u00b7re", "dir", "Seel", ",", "Sin\u00b7nen", "und", "Ge\u00b7dan\u00b7cken", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.17": {"text": "Du bist mein G\u00f6tzen-Bild, mein Weyrauch brennt vor dich,", "tokens": ["Du", "bist", "mein", "G\u00f6t\u00b7zen\u00b7Bild", ",", "mein", "Wey\u00b7rauch", "brennt", "vor", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein Knecht ist h\u00f6chst-best\u00fcrtzt, er kr\u00e4nckt und gr\u00e4met sich,", "tokens": ["Dein", "Knecht", "ist", "h\u00f6chst\u00b7be\u00b7st\u00fcrtzt", ",", "er", "kr\u00e4nckt", "und", "gr\u00e4\u00b7met", "sich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich mu\u00df den Tauben gleich nach dir, Entfernte, girren,", "tokens": ["Ich", "mu\u00df", "den", "Tau\u00b7ben", "gleich", "nach", "dir", ",", "Ent\u00b7fern\u00b7te", ",", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "APPR", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn Einsamkeit mich hei\u00dft bald da bald dorthin irren.", "tokens": ["Wenn", "Ein\u00b7sam\u00b7keit", "mich", "hei\u00dft", "bald", "da", "bald", "dor\u00b7thin", "ir\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Sehnsucht ruffet dich, und trifft dich doch nicht an;", "tokens": ["Die", "Sehn\u00b7sucht", "ruf\u00b7fet", "dich", ",", "und", "trifft", "dich", "doch", "nicht", "an", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wer ist, der meine Pein und Schmertzen schiltern kan?", "tokens": ["Wer", "ist", ",", "der", "mei\u00b7ne", "Pein", "und", "Schmert\u00b7zen", "schil\u00b7tern", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn sich das Auge schlie\u00dft, so k\u00fc\u00df ich dich in Schlummer,", "tokens": ["Wenn", "sich", "das", "Au\u00b7ge", "schlie\u00dft", ",", "so", "k\u00fc\u00df", "ich", "dich", "in", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Erwach ich von der Lust, o was vor Schmertz und Kummer", "tokens": ["Er\u00b7wach", "ich", "von", "der", "Lust", ",", "o", "was", "vor", "Schmertz", "und", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,", "FM", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bef\u00e4llt mein armes Hertz! weil di\u00df kein Labsal ist,", "tokens": ["Be\u00b7f\u00e4llt", "mein", "ar\u00b7mes", "Hertz", "!", "weil", "di\u00df", "kein", "Lab\u00b7sal", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$.", "KOUS", "PDS", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn man den Schatten fa\u00dft, den C\u00f6rper aber mi\u00dft.", "tokens": ["Wenn", "man", "den", "Schat\u00b7ten", "fa\u00dft", ",", "den", "C\u00f6r\u00b7per", "a\u00b7ber", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Weckt mich Aurorens Schein, so bitt ich um Erbarmen", "tokens": ["Weckt", "mich", "Au\u00b7ro\u00b7rens", "Schein", ",", "so", "bitt", "ich", "um", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Den Himmel! da\u00df ich dich, ach, balde m\u00f6cht umarmen.", "tokens": ["Den", "Him\u00b7mel", "!", "da\u00df", "ich", "dich", ",", "ach", ",", "bal\u00b7de", "m\u00f6cht", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KOUS", "PPER", "PRF", "$,", "ITJ", "$,", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und r\u00fcckt des Hespers Glantz zur Abend-Zeit heran,", "tokens": ["Und", "r\u00fcckt", "des", "Hes\u00b7pers", "Glantz", "zur", "A\u00b7ben\u00b7dZeit", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So streb ich noch darnach, ob ich dich haben kan.", "tokens": ["So", "streb", "ich", "noch", "dar\u00b7nach", ",", "ob", "ich", "dich", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$,", "KOUS", "PPER", "PRF", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hieraus so schliesse nun mein sehnliches Verlangen,", "tokens": ["Hier\u00b7aus", "so", "schlies\u00b7se", "nun", "mein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mich reitzt kein sch\u00f6n Gesicht, kein Mund wo Rosen prangen,", "tokens": ["Mich", "reitzt", "kein", "sch\u00f6n", "Ge\u00b7sicht", ",", "kein", "Mund", "wo", "Ro\u00b7sen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJD", "NN", "$,", "PIAT", "NN", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kein Auge, das mit Gluth und Flammen um sich blitzt,", "tokens": ["Kein", "Au\u00b7ge", ",", "das", "mit", "Gluth", "und", "Flam\u00b7men", "um", "sich", "blitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Weil mir dein Contrefey in Seel und Hertzen sitzt,", "tokens": ["Weil", "mir", "dein", "Con\u00b7tre\u00b7fey", "in", "Seel", "und", "Hert\u00b7zen", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und deine Seltenheit, die man bewundernd siehet,", "tokens": ["Und", "dei\u00b7ne", "Sel\u00b7ten\u00b7heit", ",", "die", "man", "be\u00b7wun\u00b7dernd", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Von andern Frauenvolck mein eckles Auge ziehet.", "tokens": ["Von", "an\u00b7dern", "Frau\u00b7en\u00b7volck", "mein", "ec\u00b7kles", "Au\u00b7ge", "zie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mein Wort, das ich dir gab, war nicht von Porcellan,", "tokens": ["Mein", "Wort", ",", "das", "ich", "dir", "gab", ",", "war", "nicht", "von", "Por\u00b7cel\u00b7lan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und zarten Spiegel-Gla\u00df, ich dencke noch daran,", "tokens": ["Und", "zar\u00b7ten", "Spie\u00b7gel\u00b7Gla\u00df", ",", "ich", "den\u00b7cke", "noch", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und werde nicht so leicht mit Schwur und Eyden spielen,", "tokens": ["Und", "wer\u00b7de", "nicht", "so", "leicht", "mit", "Schwur", "und", "Ey\u00b7den", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "ADJD", "APPR", "VVFIN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der Himmel m\u00f6chte sonst an mir die Rache k\u00fchlen.", "tokens": ["Der", "Him\u00b7mel", "m\u00f6ch\u00b7te", "sonst", "an", "mir", "die", "Ra\u00b7che", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Verla\u00df dich sicher drauf, und zweiffle gar nicht mehr,", "tokens": ["Ver\u00b7la\u00df", "dich", "si\u00b7cher", "drauf", ",", "und", "zweiff\u00b7le", "gar", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Als ob nicht Seladon bereits der deine w\u00e4r.", "tokens": ["Als", "ob", "nicht", "Se\u00b7la\u00b7don", "be\u00b7reits", "der", "dei\u00b7ne", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PTKNEG", "NN", "ADV", "ART", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich werd inde\u00df so viel, als immer m\u00f6glich, eilen,", "tokens": ["Ich", "werd", "in\u00b7de\u00df", "so", "viel", ",", "als", "im\u00b7mer", "m\u00f6g\u00b7lich", ",", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "ADV", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ku\u00df, Schertze, Blicke, Hand, und Hertz mit dir zu theilen.", "tokens": ["Ku\u00df", ",", "Schert\u00b7ze", ",", "Bli\u00b7cke", ",", "Hand", ",", "und", "Hertz", "mit", "dir", "zu", "thei\u00b7len", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ich ziehe mit Triumph im Geist bey dir schon ein,", "tokens": ["Ich", "zie\u00b7he", "mit", "Tri\u00b7umph", "im", "Geist", "bey", "dir", "schon", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.46": {"text": "O k\u00f6nt es heute noch, Geliebte, m\u00f6glich seyn!", "tokens": ["O", "k\u00f6nt", "es", "heu\u00b7te", "noch", ",", "Ge\u00b7lieb\u00b7te", ",", "m\u00f6g\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "$,", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Wer wei\u00df, wie balde mir die Hofnung l\u00e4st versprechen,", "tokens": ["Wer", "wei\u00df", ",", "wie", "bal\u00b7de", "mir", "die", "Hof\u00b7nung", "l\u00e4st", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Da\u00df ich, mein Engel, kan Vergn\u00fcgungs-Rosen brechen,", "tokens": ["Da\u00df", "ich", ",", "mein", "En\u00b7gel", ",", "kan", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7Ro\u00b7sen", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wenn sich von ohngefehr ein frembder G\u00e4rtner find,", "tokens": ["Wenn", "sich", "von", "ohn\u00b7ge\u00b7fehr", "ein", "fremb\u00b7der", "G\u00e4rt\u00b7ner", "find", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So sage, da\u00df vor ihn die D\u00f6rner \u00fcbrig sind;", "tokens": ["So", "sa\u00b7ge", ",", "da\u00df", "vor", "ihn", "die", "D\u00f6r\u00b7ner", "\u00fcb\u00b7rig", "sind", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wiewohl kein Argwohn mich auf Sucht und Eifer treibet,", "tokens": ["Wie\u00b7wohl", "kein", "Arg\u00b7wohn", "mich", "auf", "Sucht", "und", "Ei\u00b7fer", "trei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Weil deiner Tugend Strahl mir stets vor Augen bleibet.", "tokens": ["Weil", "dei\u00b7ner", "Tu\u00b7gend", "Strahl", "mir", "stets", "vor", "Au\u00b7gen", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}