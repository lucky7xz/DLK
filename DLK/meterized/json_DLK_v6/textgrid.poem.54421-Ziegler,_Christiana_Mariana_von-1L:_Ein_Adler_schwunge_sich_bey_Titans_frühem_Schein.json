 "line.59": {"text": "Sag, ob ein Stammhaus wohl im ganzen Kreis der Erden,", "tokens": ["Sag", ",", "ob", "ein", "Stamm\u00b7haus", "wohl", "im", "gan\u00b7zen", "Kreis", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Als wie das meinige, kann angetroffen werden?", "tokens": ["Als", "wie", "das", "mei\u00b7ni\u00b7ge", ",", "kann", "an\u00b7ge\u00b7trof\u00b7fen", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "PPOSS", "$,", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die hat durch ihren Strahl, der sich so weit erstrecket,", "tokens": ["Die", "hat", "durch", "ih\u00b7ren", "Strahl", ",", "der", "sich", "so", "weit", "er\u00b7stre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Und alle Welt erhitzt, mich wirklich ausgehecket.", "tokens": ["Und", "al\u00b7le", "Welt", "er\u00b7hitzt", ",", "mich", "wirk\u00b7lich", "aus\u00b7ge\u00b7he\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Und dennoch will das Volk bey so gestalten Sachen", "tokens": ["Und", "den\u00b7noch", "will", "das", "Volk", "bey", "so", "ge\u00b7stal\u00b7ten", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Aus mir, als ihrem Sohn, auch nicht das mindste machen.", "tokens": ["Aus", "mir", ",", "als", "ih\u00b7rem", "Sohn", ",", "auch", "nicht", "das", "minds\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADV", "PTKNEG", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ich bleibe, wer ich bin, das ist, gering und schlecht,", "tokens": ["Ich", "blei\u00b7be", ",", "wer", "ich", "bin", ",", "das", "ist", ",", "ge\u00b7ring", "und", "schlecht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "PDS", "VAFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Mein Adel und mein Haus ertheilet mir kein Recht,", "tokens": ["Mein", "A\u00b7del", "und", "mein", "Haus", "er\u00b7thei\u00b7let", "mir", "kein", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Und hat mir nie Gewalt und Vollmacht zugesprochen,", "tokens": ["Und", "hat", "mir", "nie", "Ge\u00b7walt", "und", "Voll\u00b7macht", "zu\u00b7ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Auf mein ererbtes Blut, wie du, vor Stolz zu pochen.", "tokens": ["Auf", "mein", "er\u00b7erb\u00b7tes", "Blut", ",", "wie", "du", ",", "vor", "Stolz", "zu", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Merkt, Kinder! dieses wohl, die ihr auf solches trutzt,", "tokens": ["Merkt", ",", "Kin\u00b7der", "!", "die\u00b7ses", "wohl", ",", "die", "ihr", "auf", "sol\u00b7ches", "trutzt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "PDAT", "ADV", "$,", "PRELS", "PPER", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Und mit erborgtem Glanz von euren V\u00e4tern stutzt,", "tokens": ["Und", "mit", "er\u00b7borg\u00b7tem", "Glanz", "von", "eu\u00b7ren", "V\u00e4\u00b7tern", "stutzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Der Eltern Trefflichkeit auf eure Rechnung schreibet,", "tokens": ["Der", "El\u00b7tern", "Treff\u00b7lich\u00b7keit", "auf", "eu\u00b7re", "Rech\u00b7nung", "schrei\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Und bey der Ahnen Zahl allein nur stehen bleibet.", "tokens": ["Und", "bey", "der", "Ah\u00b7nen", "Zahl", "al\u00b7lein", "nur", "ste\u00b7hen", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADV", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Berufet euch ja nicht auf eurer H\u00e4user Glanz,", "tokens": ["Be\u00b7ru\u00b7fet", "euch", "ja", "nicht", "auf", "eu\u00b7rer", "H\u00e4u\u00b7ser", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.74": {"text": "Und ihre Herrlichkeit; der V\u00e4ter Ehrenkranz,", "tokens": ["Und", "ih\u00b7re", "Herr\u00b7lich\u00b7keit", ";", "der", "V\u00e4\u00b7ter", "Eh\u00b7ren\u00b7kranz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$.", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Ziert nur ihr edles Haupt, nicht aber eure Scheitel,", "tokens": ["Ziert", "nur", "ihr", "ed\u00b7les", "Haupt", ",", "nicht", "a\u00b7ber", "eu\u00b7re", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,", "PTKNEG", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.76": {"text": "Ihr Ruhm geht euch nicht an, und eurer ist ganz eitel.", "tokens": ["Ihr", "Ruhm", "geht", "euch", "nicht", "an", ",", "und", "eu\u00b7rer", "ist", "ganz", "ei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "KON", "PPOSAT", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Dies nutzt euch alles nichts, wofern ihr nicht den Pfad,", "tokens": ["Dies", "nutzt", "euch", "al\u00b7les", "nichts", ",", "wo\u00b7fern", "ihr", "nicht", "den", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "PIS", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.78": {"text": "Den ihr entflammter Fu\u00df von Jugend auf betrat,", "tokens": ["Den", "ihr", "ent\u00b7flamm\u00b7ter", "Fu\u00df", "von", "Ju\u00b7gend", "auf", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.79": {"text": "Auch edelm\u00fcthig sucht, die Tugend mit ererbet,", "tokens": ["Auch", "e\u00b7del\u00b7m\u00fct\u00b7hig", "sucht", ",", "die", "Tu\u00b7gend", "mit", "er\u00b7er\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "ART", "NN", "APPR", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.80": {"text": "Und durch Verdienste Ruhm und eignes Lob erwerbet.", "tokens": ["Und", "durch", "Ver\u00b7diens\u00b7te", "Ruhm", "und", "eig\u00b7nes", "Lob", "er\u00b7wer\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.81": {"text": "Der blosse Name schenkt euch wenig Schmuck und Licht,", "tokens": ["Der", "blos\u00b7se", "Na\u00b7me", "schenkt", "euch", "we\u00b7nig", "Schmuck", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.82": {"text": "Wofern die W\u00fcrde nicht euch Lorberreiser flicht,", "tokens": ["Wo\u00b7fern", "die", "W\u00fcr\u00b7de", "nicht", "euch", "Lor\u00b7berr\u00b7ei\u00b7ser", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.83": {"text": "Und ihr mit eigner Faust, von Gro\u00dfmuth angefeuret,", "tokens": ["Und", "ihr", "mit", "eig\u00b7ner", "Faust", ",", "von", "Gro\u00df\u00b7muth", "an\u00b7ge\u00b7feu\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.84": {"text": "Der V\u00e4ter Helm poliert, das alte Schild verneuret.", "tokens": ["Der", "V\u00e4\u00b7ter", "Helm", "po\u00b7liert", ",", "das", "al\u00b7te", "Schild", "ver\u00b7neu\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}