{"textgrid.poem.54421": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Adler schwunge sich bey Titans fr\u00fchem Schein", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Adler schwunge sich bey Titans fr\u00fchem Schein", "tokens": ["Ein", "Ad\u00b7ler", "schwun\u00b7ge", "sich", "bey", "Ti\u00b7tans", "fr\u00fc\u00b7hem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von seinem Nest herab, das er sich insgemein", "tokens": ["Von", "sei\u00b7nem", "Nest", "her\u00b7ab", ",", "das", "er", "sich", "ins\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "PRELS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf den erhabensten der Felsen pflegt zu bauen,", "tokens": ["Auf", "den", "er\u00b7ha\u00b7bens\u00b7ten", "der", "Fel\u00b7sen", "pflegt", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Um desto f\u00fcglicher der Sonne zu zuschauen.", "tokens": ["Um", "des\u00b7to", "f\u00fcg\u00b7li\u00b7cher", "der", "Son\u00b7ne", "zu", "zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die grosse Hungersnoth trieb ihn aus seinem Sitz;", "tokens": ["Die", "gros\u00b7se", "Hun\u00b7gers\u00b7noth", "trieb", "ihn", "aus", "sei\u00b7nem", "Sitz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drum trug ihn auch sein Flug so schnell als wie der Blitz", "tokens": ["Drum", "trug", "ihn", "auch", "sein", "Flug", "so", "schnell", "als", "wie", "der", "Blitz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nach einer H\u00f6le zu; weil ihm die Spur endeckte,", "tokens": ["Nach", "ei\u00b7ner", "H\u00f6\u00b7le", "zu", ";", "weil", "ihm", "die", "Spur", "en\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df ein verrecktes Aas unfehlbar drinnen steckte.", "tokens": ["Da\u00df", "ein", "ver\u00b7reck\u00b7tes", "Aas", "un\u00b7fehl\u00b7bar", "drin\u00b7nen", "steck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dies fand er wirklich auch. Drum fiel er es gleich an;", "tokens": ["Dies", "fand", "er", "wirk\u00b7lich", "auch", ".", "Drum", "fiel", "er", "es", "gleich", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADV", "$.", "PAV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kaum aber, da\u00df er nur den ersten Bi\u00df gethan,", "tokens": ["Kaum", "a\u00b7ber", ",", "da\u00df", "er", "nur", "den", "ers\u00b7ten", "Bi\u00df", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So kroch ein Molch hervor, der sich zu diesem machte,", "tokens": ["So", "kroch", "ein", "Molch", "her\u00b7vor", ",", "der", "sich", "zu", "die\u00b7sem", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "PRF", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und ebenfals vor sich was anzutreffen dachte.", "tokens": ["Und", "e\u00b7ben\u00b7fals", "vor", "sich", "was", "an\u00b7zu\u00b7tref\u00b7fen", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PRF", "PIS", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch was f\u00fcr ein Gelerm und \u00e4ngstlich Mordgeschrey", "tokens": ["Doch", "was", "f\u00fcr", "ein", "Ge\u00b7lerm", "und", "\u00e4ngst\u00b7lich", "Mord\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ART", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Erhob sich nicht so gleich bey der Schmarutzerey;", "tokens": ["Er\u00b7hob", "sich", "nicht", "so", "gleich", "bey", "der", "Schma\u00b7rut\u00b7ze\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie? fuhr der Adler auf mit grimmigen Geberden,", "tokens": ["Wie", "?", "fuhr", "der", "Ad\u00b7ler", "auf", "mit", "grim\u00b7mi\u00b7gen", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mu\u00df meine Tafel hier durch dich entweyhet werden?", "tokens": ["Mu\u00df", "mei\u00b7ne", "Ta\u00b7fel", "hier", "durch", "dich", "ent\u00b7wey\u00b7het", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Durch dich, du greuelhaft und recht verw\u00fcnschtes Thier?", "tokens": ["Durch", "dich", ",", "du", "greu\u00b7el\u00b7haft", "und", "recht", "ver\u00b7w\u00fcnschtes", "Thier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJD", "KON", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Scheut deine Niedrigkeit den Odem nicht von mir?", "tokens": ["Scheut", "dei\u00b7ne", "Nied\u00b7rig\u00b7keit", "den", "O\u00b7dem", "nicht", "von", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "F\u00fcrchtst du den Adler nicht, den K\u00f6nig der Gefl\u00fcgel,", "tokens": ["F\u00fcrchtst", "du", "den", "Ad\u00b7ler", "nicht", ",", "den", "K\u00f6\u00b7nig", "der", "Ge\u00b7fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dem Recht und die Natur der Hoheit wahres Siegel", "tokens": ["Dem", "Recht", "und", "die", "Na\u00b7tur", "der", "Ho\u00b7heit", "wah\u00b7res", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Auf jeden Kiel gedr\u00fcckt, dem nichts am Adel fehlt", "tokens": ["Auf", "je\u00b7den", "Kiel", "ge\u00b7dr\u00fcckt", ",", "dem", "nichts", "am", "A\u00b7del", "fehlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Weil ihn selbst Jupiter zu seinem Tr\u00e4ger wehlt?", "tokens": ["Weil", "ihn", "selbst", "Ju\u00b7pi\u00b7ter", "zu", "sei\u00b7nem", "Tr\u00e4\u00b7ger", "wehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und du verworfner Wurm, wagst dich zu meinen F\u00fcssen?", "tokens": ["Und", "du", "ver\u00b7worf\u00b7ner", "Wurm", ",", "wagst", "dich", "zu", "mei\u00b7nen", "F\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "O solltst du nicht mit Recht dergleichen Frevel b\u00fcssen?", "tokens": ["O", "solltst", "du", "nicht", "mit", "Recht", "derg\u00b7lei\u00b7chen", "Fre\u00b7vel", "b\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Vom f\u00fcnften Tag der Welt geht schon mein Adel an,", "tokens": ["Vom", "f\u00fcnf\u00b7ten", "Tag", "der", "Welt", "geht", "schon", "mein", "A\u00b7del", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Erwege nun, wie viel ich Ahnen zehlen kann,", "tokens": ["Er\u00b7we\u00b7ge", "nun", ",", "wie", "viel", "ich", "Ah\u00b7nen", "zeh\u00b7len", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PIS", "PPER", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was mir Geburt und Blut hier in der Welt zu leben,", "tokens": ["Was", "mir", "Ge\u00b7burt", "und", "Blut", "hier", "in", "der", "Welt", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "F\u00fcr einen Rang geg\u00f6nnt, f\u00fcr Vorzug hat gegeben.", "tokens": ["F\u00fcr", "ei\u00b7nen", "Rang", "ge\u00b7g\u00f6nnt", ",", "f\u00fcr", "Vor\u00b7zug", "hat", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mein Stamm und mein Geschlecht, das nicht kann edler seyn,", "tokens": ["Mein", "Stamm", "und", "mein", "Ge\u00b7schlecht", ",", "das", "nicht", "kann", "ed\u00b7ler", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "PRELS", "PTKNEG", "VMFIN", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Macht mit Gesch\u00f6pfen sich, wie du bist, nicht gemein;", "tokens": ["Macht", "mit", "Ge\u00b7sch\u00f6p\u00b7fen", "sich", ",", "wie", "du", "bist", ",", "nicht", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PRF", "$,", "PWAV", "PPER", "VAFIN", "$,", "PTKNEG", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "Wir halten Nachbarschaft nur mit den hohen Sternen;", "tokens": ["Wir", "hal\u00b7ten", "Nach\u00b7bar\u00b7schaft", "nur", "mit", "den", "ho\u00b7hen", "Ster\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Von deren Glanz du dich must in der Tief entfernen.", "tokens": ["Von", "de\u00b7ren", "Glanz", "du", "dich", "must", "in", "der", "Tief", "ent\u00b7fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "PPER", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kreuch Scheusal! in den Sumpf, den dir mit Vorbedacht", "tokens": ["Kreuch", "Scheu\u00b7sal", "!", "in", "den", "Sumpf", ",", "den", "dir", "mit", "Vor\u00b7be\u00b7dacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der Wille der Natur zum Aufenthalt gemacht.", "tokens": ["Der", "Wil\u00b7le", "der", "Na\u00b7tur", "zum", "Auf\u00b7ent\u00b7halt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Es d\u00fcrfte sonst mein Volk, mein edles Volk wohl sagen;", "tokens": ["Es", "d\u00fcrf\u00b7te", "sonst", "mein", "Volk", ",", "mein", "ed\u00b7les", "Volk", "wohl", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich h\u00e4tte, weil ich nur den Hauch von dir ertragen,", "tokens": ["Ich", "h\u00e4t\u00b7te", ",", "weil", "ich", "nur", "den", "Hauch", "von", "dir", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "In unserm hohem Haus den gr\u00f6sten Sturm erregt,", "tokens": ["In", "un\u00b7serm", "ho\u00b7hem", "Haus", "den", "gr\u00f6s\u00b7ten", "Sturm", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und selbigem dadurch ein Schandmal eingepr\u00e4gt;", "tokens": ["Und", "sel\u00b7bi\u00b7gem", "da\u00b7durch", "ein", "Schand\u00b7mal", "ein\u00b7ge\u00b7pr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Gesch\u00f6pfen, welche nur aus schlechtem Holz geschnitzet,", "tokens": ["Ge\u00b7sch\u00f6p\u00b7fen", ",", "wel\u00b7che", "nur", "aus", "schlech\u00b7tem", "Holz", "ge\u00b7schnit\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "G\u00f6nnt man das Auge nicht, wenn man erhaben sitzet.", "tokens": ["G\u00f6nnt", "man", "das", "Au\u00b7ge", "nicht", ",", "wenn", "man", "er\u00b7ha\u00b7ben", "sit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKNEG", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er h\u00e4tte weiter noch von Stolz und Zorn ger\u00fchrt,", "tokens": ["Er", "h\u00e4t\u00b7te", "wei\u00b7ter", "noch", "von", "Stolz", "und", "Zorn", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Des hohen Hauses Ruhm in dem Gespr\u00e4ch vollf\u00fchrt,", "tokens": ["Des", "ho\u00b7hen", "Hau\u00b7ses", "Ruhm", "in", "dem", "Ge\u00b7spr\u00e4ch", "voll\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wenn solches nicht der Molch noch endlich unterbrochen,", "tokens": ["Wenn", "sol\u00b7ches", "nicht", "der", "Molch", "noch", "end\u00b7lich", "un\u00b7ter\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und bey erlittner Schmach dies gegen ihn gesprochen:", "tokens": ["Und", "bey", "er\u00b7litt\u00b7ner", "Schmach", "dies", "ge\u00b7gen", "ihn", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PDS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Ists m\u00f6glich, da\u00df du dich, du hochgebohrnes Thier!", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "du", "dich", ",", "du", "hoch\u00b7ge\u00b7bohr\u00b7nes", "Thier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Durch deinen Stamm erhebst, hingegen aber mir", "tokens": ["Durch", "dei\u00b7nen", "Stamm", "er\u00b7hebst", ",", "hin\u00b7ge\u00b7gen", "a\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die Tiefe, die mich lie\u00df das Licht der Welt erblicken,", "tokens": ["Die", "Tie\u00b7fe", ",", "die", "mich", "lie\u00df", "das", "Licht", "der", "Welt", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Und meine Niedrigkeit so frech suchst vorzur\u00fccken?", "tokens": ["Und", "mei\u00b7ne", "Nied\u00b7rig\u00b7keit", "so", "frech", "suchst", "vor\u00b7zu\u00b7r\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wie w\u00e4r es, wenn ich dir ohn da\u00df es prahlhaft lie\u00df,", "tokens": ["Wie", "w\u00e4r", "es", ",", "wenn", "ich", "dir", "ohn", "da\u00df", "es", "prahl\u00b7haft", "lie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "APPR", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die Hoheit meines Stamms weit herrlicher erwies?", "tokens": ["Die", "Ho\u00b7heit", "mei\u00b7nes", "Stamms", "weit", "herr\u00b7li\u00b7cher", "er\u00b7wies", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Du machst so viel Geplerr, und r\u00fchmest dein Geschlechte,", "tokens": ["Du", "machst", "so", "viel", "Ge\u00b7plerr", ",", "und", "r\u00fch\u00b7mest", "dein", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Da\u00df jeder, der es h\u00f6rt, gewi\u00df dabey ged\u00e4chte,", "tokens": ["Da\u00df", "je\u00b7der", ",", "der", "es", "h\u00f6rt", ",", "ge\u00b7wi\u00df", "da\u00b7bey", "ge\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Ob h\u00e4tten dich wohl gar die G\u00f6tter dieser Welt", "tokens": ["Ob", "h\u00e4t\u00b7ten", "dich", "wohl", "gar", "die", "G\u00f6t\u00b7ter", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Auf ihrem Schoo\u00df gezeugt und \u00fcber uns gestellt;", "tokens": ["Auf", "ih\u00b7rem", "Schoo\u00df", "ge\u00b7zeugt", "und", "\u00fc\u00