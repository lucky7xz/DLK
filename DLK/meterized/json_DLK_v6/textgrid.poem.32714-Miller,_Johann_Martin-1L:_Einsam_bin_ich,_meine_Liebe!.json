{"textgrid.poem.32714": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam bin ich, meine Liebe!", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam bin ich, meine Liebe!", "tokens": ["Ein\u00b7sam", "bin", "ich", ",", "mei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denke dein, und h\u00e4rme mich.", "tokens": ["Den\u00b7ke", "dein", ",", "und", "h\u00e4r\u00b7me", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ist mir die Welt so tr\u00fcbe,", "tokens": ["Wie", "ist", "mir", "die", "Welt", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie so traurig ohne dich!", "tokens": ["Wie", "so", "trau\u00b7rig", "oh\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier, wo keine Seele lauschet,", "tokens": ["Hier", ",", "wo", "kei\u00b7ne", "See\u00b7le", "lau\u00b7schet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klag' ich mit der Nachtigall,", "tokens": ["Klag'", "ich", "mit", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in meine Klage rauschet", "tokens": ["Und", "in", "mei\u00b7ne", "Kla\u00b7ge", "rau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Trauriger der Wasserfall.", "tokens": ["Trau\u00b7ri\u00b7ger", "der", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus verschr\u00e4nkten Buchenreisen", "tokens": ["Aus", "ver\u00b7schr\u00e4nk\u00b7ten", "Bu\u00b7chen\u00b7rei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Girrt der Tauber seine Pein;", "tokens": ["Girrt", "der", "Tau\u00b7ber", "sei\u00b7ne", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Amseln singen ihre Weisen", "tokens": ["Am\u00b7seln", "sin\u00b7gen", "ih\u00b7re", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Tannenbaum darein;", "tokens": ["Auf", "dem", "Tan\u00b7nen\u00b7baum", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Grillen zirpen aus dem Moose,", "tokens": ["Gril\u00b7len", "zir\u00b7pen", "aus", "dem", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das mir keine Blum' erzieht,", "tokens": ["Das", "mir", "kei\u00b7ne", "Blum'", "er\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Als die kleine, stille Rose,", "tokens": ["Als", "die", "klei\u00b7ne", ",", "stil\u00b7le", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die in blasser R\u00f6te bl\u00fcht.", "tokens": ["Die", "in", "blas\u00b7ser", "R\u00f6\u00b7te", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, Elisens s\u00fc\u00dfes Bildnis,", "tokens": ["Ach", ",", "E\u00b7li\u00b7sens", "s\u00fc\u00b7\u00dfes", "Bild\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Diese Thr\u00e4ne wein' ich dir!", "tokens": ["Die\u00b7se", "Thr\u00e4\u00b7ne", "wein'", "ich", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei, in dieser \u00f6den Wildnis,", "tokens": ["Sei", ",", "in", "die\u00b7ser", "\u00f6\u00b7den", "Wild\u00b7nis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei ein Bild Elisens mir!", "tokens": ["Sei", "ein", "Bild", "E\u00b7li\u00b7sens", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NE", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So, auf ferner Flur, verlassen,", "tokens": ["So", ",", "auf", "fer\u00b7ner", "Flur", ",", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4rmt im stillen sie sich ab;", "tokens": ["H\u00e4rmt", "im", "stil\u00b7len", "sie", "sich", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "So, wie deine Bl\u00e4tter, blassen", "tokens": ["So", ",", "wie", "dei\u00b7ne", "Bl\u00e4t\u00b7ter", ",", "blas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PWAV", "PPOSAT", "NN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihre zarten Wangen ab.", "tokens": ["Ih\u00b7re", "zar\u00b7ten", "Wan\u00b7gen", "ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O! ich sehe sie, und d\u00fcster", "tokens": ["O", "!", "ich", "se\u00b7he", "sie", ",", "und", "d\u00fcs\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "PPER", "$,", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ihr holdes Angesicht;", "tokens": ["Ist", "ihr", "hol\u00b7des", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In das leise Haingefl\u00fcster", "tokens": ["In", "das", "lei\u00b7se", "Hain\u00b7ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mischt sich ihre Stimme nicht;", "tokens": ["Mischt", "sich", "ih\u00b7re", "Stim\u00b7me", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles mahnet sie des Gl\u00fcckes,", "tokens": ["Al\u00b7les", "mah\u00b7net", "sie", "des", "Gl\u00fc\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das uns hier vor\u00fcberschwand;", "tokens": ["Das", "uns", "hier", "vor\u00b7\u00fc\u00b7bersc\u00b7hwand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles sie des Mi\u00dfgeschickes,", "tokens": ["Al\u00b7les", "sie", "des", "Mi\u00df\u00b7ge\u00b7schi\u00b7ckes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das von ihr mich weggebannt.", "tokens": ["Das", "von", "ihr", "mich", "weg\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weine nicht! Ein Tag vereinet", "tokens": ["Wei\u00b7ne", "nicht", "!", "Ein", "Tag", "ver\u00b7ei\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald auf ewig mich mit dir.", "tokens": ["Bald", "auf", "e\u00b7wig", "mich", "mit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann, o meine Liebe, weinet", "tokens": ["Dann", ",", "o", "mei\u00b7ne", "Lie\u00b7be", ",", "wei\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "FM", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur der Liebe Gl\u00fcck aus mir!", "tokens": ["Nur", "der", "Lie\u00b7be", "Gl\u00fcck", "aus", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6ner, wie, nach Maienregen,", "tokens": ["Sch\u00f6\u00b7ner", ",", "wie", ",", "nach", "Mai\u00b7en\u00b7re\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Blumenwiese lacht,", "tokens": ["Ei\u00b7ne", "Blu\u00b7men\u00b7wie\u00b7se", "lacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blickt uns dann der Tag entgegen,", "tokens": ["Blickt", "uns", "dann", "der", "Tag", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der uns ewig gl\u00fccklich macht!", "tokens": ["Der", "uns", "e\u00b7wig", "gl\u00fcck\u00b7lich", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}