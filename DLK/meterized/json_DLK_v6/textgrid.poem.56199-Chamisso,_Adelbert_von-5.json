{"textgrid.poem.56199": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie trag ich's doch zu leben", "tokens": ["Wie", "trag", "ich's", "doch", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur mir und meiner Pein?", "tokens": ["Nur", "mir", "und", "mei\u00b7ner", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem Liebsten sollt ich dienen,", "tokens": ["Dem", "Liebs\u00b7ten", "sollt", "ich", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da wollt ich selig sein!", "tokens": ["Da", "wollt", "ich", "se\u00b7lig", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich wollt ein treuer Page", "tokens": ["Ich", "wollt", "ein", "treu\u00b7er", "Pa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Um den Gebieter stehn,", "tokens": ["Um", "den", "Ge\u00b7bie\u00b7ter", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVINF", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bereit zu jeder Botschaft", "tokens": ["Be\u00b7reit", "zu", "je\u00b7der", "Bot\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und jeden Gang zu gehn.", "tokens": ["Und", "je\u00b7den", "Gang", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich kenne jede Windung", "tokens": ["Ich", "ken\u00b7ne", "je\u00b7de", "Win\u00b7dung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Stra\u00dfen, jedes Haus,", "tokens": ["Der", "Stra\u00b7\u00dfen", ",", "je\u00b7des", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeden Stein am Wege,", "tokens": ["Und", "je\u00b7den", "Stein", "am", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weiche jedem aus.", "tokens": ["Und", "wei\u00b7che", "je\u00b7dem", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie freudig zitternd tr\u00fcg ich", "tokens": ["Wie", "freu\u00b7dig", "zit\u00b7ternd", "tr\u00fcg", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm nachts die Fackel vor,", "tokens": ["Ihm", "nachts", "die", "Fa\u00b7ckel", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die freud'ge Luft ihm spendend,", "tokens": ["Die", "freu\u00b7d'\u00b7ge", "Luft", "ihm", "spen\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die selber ich verlor!", "tokens": ["Die", "sel\u00b7ber", "ich", "ver\u00b7lor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Oh, traurig ist's im Dunkeln,", "tokens": ["Oh", ",", "trau\u00b7rig", "ist's", "im", "Dun\u00b7keln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich wei\u00df es nur zu sehr!", "tokens": ["Ich", "wei\u00df", "es", "nur", "zu", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Licht wollt ich, Licht verbreiten", "tokens": ["Licht", "wollt", "ich", ",", "Licht", "ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "NN", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Um seine Schritte her.", "tokens": ["Um", "sei\u00b7ne", "Schrit\u00b7te", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ihn sollte stets erfreuen", "tokens": ["Ihn", "soll\u00b7te", "stets", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das allerfreu'nde Licht,", "tokens": ["Das", "al\u00b7ler\u00b7freu'n\u00b7de", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Anblick sollte jeden", "tokens": ["Sein", "An\u00b7blick", "soll\u00b7te", "je\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erfreuen, mich nur nicht.", "tokens": ["Er\u00b7freu\u00b7en", ",", "mich", "nur", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und sollte da mich treffen", "tokens": ["Und", "soll\u00b7te", "da", "mich", "tref\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Menschen Spott und Hohn,", "tokens": ["Der", "Men\u00b7schen", "Spott", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh es nicht, und h\u00f6rt ich's,", "tokens": ["Ich", "seh", "es", "nicht", ",", "und", "h\u00f6rt", "ich's", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach das ertr\u00fcg ich schon.", "tokens": ["Ach", "das", "er\u00b7tr\u00fcg", "ich", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}