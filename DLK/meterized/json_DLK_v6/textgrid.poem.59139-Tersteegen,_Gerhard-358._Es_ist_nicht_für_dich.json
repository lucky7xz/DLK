{"textgrid.poem.59139": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "358. Es ist nicht f\u00fcr dich", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Himmelskind, la\u00df fahr'n die Erd',", "tokens": ["Du", "Him\u00b7mel\u00b7skind", ",", "la\u00df", "fahr'n", "die", "Erd'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVIMP", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gro\u00dfer Reis'pack nur beschwert!", "tokens": ["Ein", "gro\u00b7\u00dfer", "Reis'\u00b7pack", "nur", "be\u00b7schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer Gott hat, der bedarf sehr wenig", "tokens": ["Wer", "Gott", "hat", ",", "der", "be\u00b7darf", "sehr", "we\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "ADV", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist doch reicher als ein K\u00f6nig.", "tokens": ["Und", "ist", "doch", "rei\u00b7cher", "als", "ein", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}