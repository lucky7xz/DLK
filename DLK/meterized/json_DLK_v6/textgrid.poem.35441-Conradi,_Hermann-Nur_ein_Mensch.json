{"textgrid.poem.35441": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Nur ein Mensch", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stand auf sturmbestrichnem, granitnem Bergeshaupt,", "tokens": ["Ich", "stand", "auf", "sturm\u00b7bes\u00b7trich\u00b7nem", ",", "gra\u00b7nit\u00b7nem", "Ber\u00b7ges\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Umbr\u00fcllt vom Eisorkane, von stechendem Schnee umstaubt \u2013", "tokens": ["Um\u00b7br\u00fcllt", "vom", "Ei\u00b7sor\u00b7ka\u00b7ne", ",", "von", "ste\u00b7chen\u00b7dem", "Schnee", "um\u00b7staubt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Tief unter mir, umschlungen vom Nebelgewande der Nacht,", "tokens": ["Tief", "un\u00b7ter", "mir", ",", "um\u00b7schlun\u00b7gen", "vom", "Ne\u00b7bel\u00b7ge\u00b7wan\u00b7de", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Lag Wahn und Menschenschicksal, lag Elend und Kronenpracht ...", "tokens": ["Lag", "Wahn", "und", "Men\u00b7schen\u00b7schick\u00b7sal", ",", "lag", "E\u00b7lend", "und", "Kro\u00b7nen\u00b7pracht", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Lag all das wirre Suchen: die Pilgerfahrt zum Licht \u2013", "tokens": ["Lag", "all", "das", "wir\u00b7re", "Su\u00b7chen", ":", "die", "Pil\u00b7ger\u00b7fahrt", "zum", "Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "ADJA", "NN", "$.", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lag all das ewige Irren: ein w\u00fcstes H\u00f6llengedicht!", "tokens": ["Lag", "all", "das", "e\u00b7wi\u00b7ge", "Ir\u00b7ren", ":", "ein", "w\u00fcs\u00b7tes", "H\u00f6l\u00b7len\u00b7ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Lag glei\u00dfender Glanz und Entsagung \u2013 Gethsemane und Rom:", "tokens": ["Lag", "glei\u00b7\u00dfen\u00b7der", "Glanz", "und", "Ent\u00b7sa\u00b7gung", "\u2013", "Geth\u00b7se\u00b7ma\u00b7ne", "und", "Rom", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$(", "NN", "KON", "NE", "$."], "meter": "-+--+--+-+-+--+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dort wurmt sich ein armer Schw\u00e4rmer \u2013 hier schwillt der L\u00fcste Strom!", "tokens": ["Dort", "wurmt", "sich", "ein", "ar\u00b7mer", "Schw\u00e4r\u00b7mer", "\u2013", "hier", "schwillt", "der", "L\u00fcs\u00b7te", "Strom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Lag all die bl\u00f6de Verblendung, die vor den G\u00f6tzen kniet \u2013", "tokens": ["Lag", "all", "die", "bl\u00f6\u00b7de", "Ver\u00b7blen\u00b7dung", ",", "die", "vor", "den", "G\u00f6t\u00b7zen", "kniet", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Lag all die feige Knechtschaft, die sich im Staube m\u00fcht,", "tokens": ["Lag", "all", "die", "fei\u00b7ge", "Knecht\u00b7schaft", ",", "die", "sich", "im", "Stau\u00b7be", "m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Faulende Fr\u00fcchte zu sammeln, lohender Br\u00fcnste voll \u2013", "tokens": ["Fau\u00b7len\u00b7de", "Fr\u00fcch\u00b7te", "zu", "sam\u00b7meln", ",", "lo\u00b7hen\u00b7der", "Br\u00fcns\u00b7te", "voll", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,", "ADJA", "NN", "ADJD", "$("], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lag all die j\u00e4he Verzweiflung \u2013 der heilige R\u00e4chergroll! ...", "tokens": ["Lag", "all", "die", "j\u00e4\u00b7he", "Ver\u00b7zwei\u00b7flung", "\u2013", "der", "hei\u00b7li\u00b7ge", "R\u00e4\u00b7cher\u00b7groll", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PIAT", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Sklavenkette klirrte \u2013 ihr schneidender Ton verklang;", "tokens": ["Die", "Skla\u00b7ven\u00b7ket\u00b7te", "klirr\u00b7te", "\u2013", "ihr", "schnei\u00b7den\u00b7der", "Ton", "ver\u00b7klang", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Schellenkappe t\u00f6nte \u2013 ihr lockend Gel\u00e4ut versank \u2013", "tokens": ["Die", "Schel\u00b7len\u00b7kap\u00b7pe", "t\u00f6n\u00b7te", "\u2013", "ihr", "lo\u00b7ckend", "Ge\u00b7l\u00e4ut", "ver\u00b7sank", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVPP", "NN", "VVFIN", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von bleichen M\u00e4rtyrerlippen verwehte der letzte Schwur \u2013", "tokens": ["Von", "blei\u00b7chen", "M\u00e4r\u00b7ty\u00b7rer\u00b7lip\u00b7pen", "ver\u00b7weh\u00b7te", "der", "letz\u00b7te", "Schwur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im Schweigen der Bergesw\u00fcste verstummt die Kreatur ...", "tokens": ["Im", "Schwei\u00b7gen", "der", "Ber\u00b7ges\u00b7w\u00fcs\u00b7te", "ver\u00b7stummt", "die", "Kre\u00b7a\u00b7tur", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Die einst mit flammenden Schwertern \u00fcber den Erdball gebraust,", "tokens": ["Die", "einst", "mit", "flam\u00b7men\u00b7den", "Schwer\u00b7tern", "\u00fc\u00b7ber", "den", "Erd\u00b7ball", "ge\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die Babel-Dome gefestet mit blut'ger Despotenfaust \u2013", "tokens": ["Die", "Babel\u00b7Do\u00b7me", "ge\u00b7fes\u00b7tet", "mit", "blut'\u00b7ger", "Des\u00b7po\u00b7ten\u00b7faust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die ihre C\u00e4sarenspuren mit ehernem Mei\u00dfel gehauen,", "tokens": ["Die", "ih\u00b7re", "C\u00e4\u00b7sa\u00b7ren\u00b7spu\u00b7ren", "mit", "e\u00b7her\u00b7nem", "Mei\u00b7\u00dfel", "ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hier an den Felsenbr\u00fcsten zerf\u00e4llt das irdische Grauen,", "tokens": ["Hier", "an", "den", "Fel\u00b7sen\u00b7br\u00fcs\u00b7ten", "zer\u00b7f\u00e4llt", "das", "ir\u00b7di\u00b7sche", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Das sie heraufbeschworen im bangenden Menschenhirn \u2013", "tokens": ["Das", "sie", "her\u00b7auf\u00b7be\u00b7schwo\u00b7ren", "im", "ban\u00b7gen\u00b7den", "Men\u00b7schen\u00b7hirn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ihre Kronenzepter zersplittern an der steinernen Bergesstirn \u2013", "tokens": ["Ih\u00b7re", "Kro\u00b7nen\u00b7zep\u00b7ter", "zer\u00b7split\u00b7tern", "an", "der", "stei\u00b7ner\u00b7nen", "Ber\u00b7ges\u00b7stirn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+--+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Und ihrer Allmacht Male zerbr\u00f6ckeln wie m\u00fcrbe Spreu:", "tokens": ["Und", "ih\u00b7rer", "All\u00b7macht", "Ma\u00b7le", "zer\u00b7br\u00f6\u00b7ckeln", "wie", "m\u00fcr\u00b7be", "Spreu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Das Schweigen der Felsen\u00f6de verschlingt den Siegerschrei ...", "tokens": ["Das", "Schwei\u00b7gen", "der", "Fel\u00b7sen\u00b7\u00f6\u00b7de", "ver\u00b7schlingt", "den", "Sie\u00b7ger\u00b7schrei", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Im Schweigen der Bergesw\u00fcste verstummt die Kreatur \u2013", "tokens": ["Im", "Schwei\u00b7gen", "der", "Ber\u00b7ges\u00b7w\u00fcs\u00b7te", "ver\u00b7stummt", "die", "Kre\u00b7a\u00b7tur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hier lebt und atmet nur eines: die unbefleckte Natur ...", "tokens": ["Hier", "lebt", "und", "at\u00b7met", "nur", "ei\u00b7nes", ":", "die", "un\u00b7be\u00b7fleck\u00b7te", "Na\u00b7tur", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADV", "PIS", "$.", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und mich durchdrang die Wollust, an dieser Felsenbrust", "tokens": ["Und", "mich", "durch\u00b7drang", "die", "Wol\u00b7lust", ",", "an", "die\u00b7ser", "Fel\u00b7sen\u00b7brust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mein S\u00fcnderhaupt zu zerschmettern \u2013 all meine Erdenlust \u2013", "tokens": ["Mein", "S\u00fcn\u00b7der\u00b7haupt", "zu", "zer\u00b7schmet\u00b7tern", "\u2013", "all", "mei\u00b7ne", "Er\u00b7den\u00b7lust", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$(", "PIAT", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "All meine Erdenduldung, von dieser Gr\u00f6\u00dfe zerdr\u00fcckt \u2013", "tokens": ["All", "mei\u00b7ne", "Er\u00b7den\u00b7dul\u00b7dung", ",", "von", "die\u00b7ser", "Gr\u00f6\u00b7\u00dfe", "zer\u00b7dr\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "All meine Gramverschuldung, wiedergeburtsbegl\u00fcckt \u2013", "tokens": ["All", "mei\u00b7ne", "Gram\u00b7ver\u00b7schul\u00b7dung", ",", "wie\u00b7der\u00b7ge\u00b7burts\u00b7be\u00b7gl\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "VVFIN", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wiedergeboren und enden: zum erstenmal ein Held!", "tokens": ["Wie\u00b7der\u00b7ge\u00b7bo\u00b7ren", "und", "en\u00b7den", ":", "zum", "ers\u00b7ten\u00b7mal", "ein", "Held", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$.", "APPRART", "ADV", "ART", "NN", "$."], "meter": "---+--+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ausatmen in diese Wildnis meine kleine, d\u00fcrftige Welt!", "tokens": ["Aus\u00b7at\u00b7men", "in", "die\u00b7se", "Wild\u00b7nis", "mei\u00b7ne", "klei\u00b7ne", ",", "d\u00fcrf\u00b7ti\u00b7ge", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+--+", "measure": "iambic.septa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da kroch es heran, das Entsetzen, belastete mich wie Erz \u2013", "tokens": ["Da", "kroch", "es", "he\u00b7ran", ",", "das", "Ent\u00b7set\u00b7zen", ",", "be\u00b7las\u00b7te\u00b7te", "mich", "wie", "Erz", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "KOKOM", "NN", "$("], "meter": "-+--+--+--+--+-+", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Und h\u00e4mmern sp\u00fcrt' ich mein armes, todbangendes Menschenherz:", "tokens": ["Und", "h\u00e4m\u00b7mern", "sp\u00fcrt'", "ich", "mein", "ar\u00b7mes", ",", "tod\u00b7ban\u00b7gen\u00b7des", "Men\u00b7schen\u00b7herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-++--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Gemach kehrt' ich zu Tal mich, nach Menschenspur hinab \u2013", "tokens": ["Ge\u00b7mach", "kehrt'", "ich", "zu", "Tal", "mich", ",", "nach", "Men\u00b7schen\u00b7spur", "hin\u00b7ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PPER", "$,", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bei Alltagsm\u00fchen zu suchen nach meinem Alltagsgrab.", "tokens": ["Bei", "All\u00b7tags\u00b7m\u00fc\u00b7hen", "zu", "su\u00b7chen", "nach", "mei\u00b7nem", "All\u00b7tags\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}