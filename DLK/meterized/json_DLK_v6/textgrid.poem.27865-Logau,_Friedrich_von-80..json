{"textgrid.poem.27865": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Heliconisch Volck, euch ist zu viel geschehen,", "tokens": ["Ihr", "He\u00b7li\u00b7co\u00b7nisch", "Volck", ",", "euch", "ist", "zu", "viel", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "PPER", "VAFIN", "PTKA", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil man nie geglaubt, drum da\u00df mans nie gesehen,", "tokens": ["Die\u00b7weil", "man", "nie", "ge\u00b7glaubt", ",", "drum", "da\u00df", "mans", "nie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$,", "PAV", "KOUS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ihr uns habt gesagt, wie Lycus armes Land", "tokens": ["Was", "ihr", "uns", "habt", "ge\u00b7sagt", ",", "wie", "Ly\u00b7cus", "ar\u00b7mes", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VVPP", "$,", "PWAV", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Chim\u00e6ra hat erschreckt, verw\u00fcstet und verbrant.", "tokens": ["Chim\u00e6ra", "hat", "er\u00b7schreckt", ",", "ver\u00b7w\u00fcs\u00b7tet", "und", "ver\u00b7brant", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von vornen war sie L\u00f6w, war Zieg am Bauch und R\u00fccken;", "tokens": ["Von", "vor\u00b7nen", "war", "sie", "L\u00f6w", ",", "war", "Zieg", "am", "Bauch", "und", "R\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "PPER", "NE", "$,", "VAFIN", "NN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr letztes muste sich zu einem Drachen schicken;", "tokens": ["Ihr", "letz\u00b7tes", "mus\u00b7te", "sich", "zu", "ei\u00b7nem", "Dra\u00b7chen", "schi\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr Maul war voller Glut; ihr Leib war voller Gifft,", "tokens": ["Ihr", "Maul", "war", "vol\u00b7ler", "Glut", ";", "ihr", "Leib", "war", "vol\u00b7ler", "Gifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bi\u00df da\u00df Alcides Keul auff ihr Gehirne trifft;", "tokens": ["Bi\u00df", "da\u00df", "Al\u00b7ci\u00b7des", "Keul", "auff", "ihr", "Ge\u00b7hir\u00b7ne", "trifft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "NE", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Trifft aber nur so weit, da\u00df damals sie entschlaffen", "tokens": ["Trifft", "a\u00b7ber", "nur", "so", "weit", ",", "da\u00df", "da\u00b7mals", "sie", "ent\u00b7schlaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ADJD", "$,", "KOUS", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und, ietzund nun erweckt durch unsre Deutsche Waffen,", "tokens": ["Und", ",", "ie\u00b7tzund", "nun", "er\u00b7weckt", "durch", "uns\u00b7re", "Deut\u00b7sche", "Waf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Tobt mitten unter uns, an Form und Namen alt,", "tokens": ["Tobt", "mit\u00b7ten", "un\u00b7ter", "uns", ",", "an", "Form", "und", "Na\u00b7men", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An Kr\u00e4fften aber neu und \u00e4rger an Gewalt.", "tokens": ["An", "Kr\u00e4ff\u00b7ten", "a\u00b7ber", "neu", "und", "\u00e4r\u00b7ger", "an", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es ist der tolle Krieg; der seinselbst eigne Wiege", "tokens": ["Es", "ist", "der", "tol\u00b7le", "Krieg", ";", "der", "sein\u00b7selbst", "eig\u00b7ne", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Hat um und um gest\u00fcrtzt, da\u00df unten oben liege;", "tokens": ["Hat", "um", "und", "um", "ge\u00b7st\u00fcrtzt", ",", "da\u00df", "un\u00b7ten", "o\u00b7ben", "lie\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "KON", "APPR", "VVPP", "$,", "KOUS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es ist der t\u00f6rchte Krieg, der sonsten nichts ersiegt,", "tokens": ["Es", "ist", "der", "t\u00f6rch\u00b7te", "Krieg", ",", "der", "sons\u00b7ten", "nichts", "er\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn da\u00df er sagen mag: noch haben wir gekriegt!", "tokens": ["Denn", "da\u00df", "er", "sa\u00b7gen", "mag", ":", "noch", "ha\u00b7ben", "wir", "ge\u00b7kriegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zum ersten war er L\u00f6w, ver\u00fcbte k\u00fchne Thaten,", "tokens": ["Zum", "ers\u00b7ten", "war", "er", "L\u00f6w", ",", "ver\u00b7\u00fcb\u00b7te", "k\u00fch\u00b7ne", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "NE", "$,", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Hilt h\u00f6her auff die Faust, als t\u00fcckisches verrathen,", "tokens": ["Hilt", "h\u00f6\u00b7her", "auff", "die", "Faust", ",", "als", "t\u00fc\u00b7cki\u00b7sches", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Und Deutschland war noch Deutsch; man schlug noch ernstlich drauff,", "tokens": ["Und", "Deutschland", "war", "noch", "Deutsch", ";", "man", "schlug", "noch", "ernst\u00b7lich", "drauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "NN", "$.", "PIS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Sah auff de\u00df Krieges End und nicht auff fernern Lauff.", "tokens": ["Sah", "auff", "de\u00df", "Krie\u00b7ges", "End", "und", "nicht", "auff", "fer\u00b7nern", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "KON", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da nun der s\u00fcsse Brauch, zu machen fette Beute", "tokens": ["Da", "nun", "der", "s\u00fcs\u00b7se", "Brauch", ",", "zu", "ma\u00b7chen", "fet\u00b7te", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Au\u00df allem, was Gott selbst gehabt und alle Leute,", "tokens": ["Au\u00df", "al\u00b7lem", ",", "was", "Gott", "selbst", "ge\u00b7habt", "und", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "NN", "ADV", "VAPP", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "An stat de\u00df Soldes kam, so wuchs dem Krieg ein Bauch,", "tokens": ["An", "stat", "de\u00df", "Sol\u00b7des", "kam", ",", "so", "wuchs", "dem", "Krieg", "ein", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Drau\u00df wie von einer Zieg ein sch\u00e4dlich d\u00fcrrer Rauch", "tokens": ["Drau\u00df", "wie", "von", "ei\u00b7ner", "Zieg", "ein", "sch\u00e4d\u00b7lich", "d\u00fcr\u00b7rer", "Rauch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "F\u00fcr Kraut und B\u00e4ume fuhr. Die Nahrung ward vertrieben;", "tokens": ["F\u00fcr", "Kraut", "und", "B\u00e4u\u00b7me", "fuhr", ".", "Die", "Nah\u00b7rung", "ward", "ver\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der Ochsen saure M\u00fch ist unvergolten blieben;", "tokens": ["Der", "Och\u00b7sen", "sau\u00b7re", "M\u00fch", "ist", "un\u00b7ver\u00b7gol\u00b7ten", "blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein andrer nam es weg; es hie\u00df, der Wirth ins Haus", "tokens": ["Ein", "an\u00b7drer", "nam", "es", "weg", ";", "es", "hie\u00df", ",", "der", "Wirth", "ins", "Haus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "La\u00df alles, was er hat, und geh auff ewig rau\u00df.", "tokens": ["La\u00df", "al\u00b7les", ",", "was", "er", "hat", ",", "und", "geh", "auff", "e\u00b7wig", "rau\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "APPR", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drauff ward man nun bedacht, den Krieg weit hin zu spielen,", "tokens": ["Drauff", "ward", "man", "nun", "be\u00b7dacht", ",", "den", "Krieg", "weit", "hin", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PIS", "ADV", "VVPP", "$,", "ART", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Nicht auff den Feind so wol, als auff den Freund zu zielen,", "tokens": ["Nicht", "auff", "den", "Feind", "so", "wol", ",", "als", "auff", "den", "Freund", "zu", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der noch in gutem Land in seinem Schaten sa\u00df", "tokens": ["Der", "noch", "in", "gu\u00b7tem", "Land", "in", "sei\u00b7nem", "Scha\u00b7ten", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und sein gen\u00fcglich Brot mit s\u00fc\u00dfem Frieden a\u00df.", "tokens": ["Und", "sein", "ge\u00b7n\u00fcg\u00b7lich", "Brot", "mit", "s\u00fc\u00b7\u00dfem", "Frie\u00b7den", "a\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zu diesem drang man ein, wann Titan gleich noch stunde,", "tokens": ["Zu", "die\u00b7sem", "drang", "man", "ein", ",", "wann", "Ti\u00b7tan", "gleich", "noch", "stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PIS", "PTKVZ", "$,", "PWAV", "NN", "ADV", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wo sonst der heisse L\u00f6w bl\u00e4st Flammen au\u00df dem Munde;", "tokens": ["Wo", "sonst", "der", "heis\u00b7se", "L\u00f6w", "bl\u00e4st", "Flam\u00b7men", "au\u00df", "dem", "Mun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Noch must es Winter seyn; noch nam man da quartier,", "tokens": ["Noch", "must", "es", "Win\u00b7ter", "seyn", ";", "noch", "nam", "man", "da", "quar\u00b7tier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VAINF", "$.", "ADV", "VVFIN", "PIS", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und alles, was man fand, war schuldige Geb\u00fcr.", "tokens": ["Und", "al\u00b7les", ",", "was", "man", "fand", ",", "war", "schul\u00b7di\u00b7ge", "Ge\u00b7b\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Als wie der scharffe Zahn der Ziegen auch die Rinden,", "tokens": ["Als", "wie", "der", "scharf\u00b7fe", "Zahn", "der", "Zie\u00b7gen", "auch", "die", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "An Bl\u00e4ttern nicht vergn\u00fcgt, von B\u00e4umen pflegt zu schinden:", "tokens": ["An", "Bl\u00e4t\u00b7tern", "nicht", "ver\u00b7gn\u00fcgt", ",", "von", "B\u00e4u\u00b7men", "pflegt", "zu", "schin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "$,", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So war es nicht genug zu fressen unser Gut;", "tokens": ["So", "war", "es", "nicht", "ge\u00b7nug", "zu", "fres\u00b7sen", "un\u00b7ser", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Man gunt uns in dem Leib auch kaum das letzte Blut.", "tokens": ["Man", "gunt", "uns", "in", "dem", "Leib", "auch", "kaum", "das", "letz\u00b7te", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Feind blieb, wo er war, und wolt er wo nicht bleiben,", "tokens": ["Der", "Feind", "blieb", ",", "wo", "er", "war", ",", "und", "wolt", "er", "wo", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$,", "KON", "VMFIN", "PPER", "PWAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Bi\u00df da\u00df man vom Quartier kunt \u00fcberall vertreiben", "tokens": ["Bi\u00df", "da\u00df", "man", "vom", "Quar\u00b7tier", "kunt", "\u00fc\u00b7be\u00b7rall", "ver\u00b7trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PIS", "APPRART", "NN", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Das viergef\u00fcste Volck, so mocht er immer hin", "tokens": ["Das", "vier\u00b7ge\u00b7f\u00fcs\u00b7te", "Volck", ",", "so", "mocht", "er", "im\u00b7mer", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "In sein, in unser Land nach gutem d\u00fcncken ziehn.", "tokens": ["In", "sein", ",", "in", "un\u00b7ser", "Land", "nach", "gu\u00b7tem", "d\u00fcn\u00b7cken", "ziehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Weil er ein Cavallier, so stund es zu verf\u00fchren,", "tokens": ["Weil", "er", "ein", "Ca\u00b7val\u00b7lier", ",", "so", "stund", "es", "zu", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "$,", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So ihm man liesse zu auch was zu prosperiren;", "tokens": ["So", "ihm", "man", "lies\u00b7se", "zu", "auch", "was", "zu", "pros\u00b7pe\u00b7ri\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PIS", "VVFIN", "APPR", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Er mochte pl\u00fcndern dort, wir pl\u00fcnderten allhier,", "tokens": ["Er", "moch\u00b7te", "pl\u00fcn\u00b7dern", "dort", ",", "wir", "pl\u00fcn\u00b7der\u00b7ten", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.48": {"text": "Da\u00df gleichwol der Soldat h\u00e4tt immer etwas f\u00fcr.", "tokens": ["Da\u00df", "gleich\u00b7wol", "der", "Sol\u00b7dat", "h\u00e4tt", "im\u00b7mer", "et\u00b7was", "f\u00fcr", "."], "token_inf