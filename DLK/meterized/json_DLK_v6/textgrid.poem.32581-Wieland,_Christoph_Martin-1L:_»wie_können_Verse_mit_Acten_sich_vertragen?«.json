{"textgrid.poem.32581": {"metadata": {"author": {"name": "Wieland, Christoph Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwie k\u00f6nnen Verse mit Acten sich vertragen?\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwie k\u00f6nnen Verse mit Acten sich vertragen?\u00ab", "tokens": ["\u00bb", "wie", "k\u00f6n\u00b7nen", "Ver\u00b7se", "mit", "Ac\u00b7ten", "sich", "ver\u00b7tra\u00b7gen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VMFIN", "NN", "APPR", "NN", "PRF", "VVFIN", "$.", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So h\u00f6rten am Fu\u00dfe des grinsenden Marsyas schon", "tokens": ["So", "h\u00f6r\u00b7ten", "am", "Fu\u00b7\u00dfe", "des", "grin\u00b7sen\u00b7den", "Mar\u00b7syas", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "ADV"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "die Musen ihren Lieblingssohn,", "tokens": ["die", "Mu\u00b7sen", "ih\u00b7ren", "Lieb\u00b7lings\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "den Lehrer der sch\u00f6nen Kunst, die M\u00e4dchen zu fangen, klagen;", "tokens": ["den", "Leh\u00b7rer", "der", "sch\u00f6\u00b7nen", "Kunst", ",", "die", "M\u00e4d\u00b7chen", "zu", "fan\u00b7gen", ",", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "so klagte am Arno der S\u00e4nger der sch\u00f6nen Angelica schon,", "tokens": ["so", "klag\u00b7te", "am", "Ar\u00b7no", "der", "S\u00e4n\u00b7ger", "der", "sch\u00f6\u00b7nen", "An\u00b7ge\u00b7li\u00b7ca", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NE", "ART", "NN", "ART", "ADJA", "NE", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "so klagten Marino und Tasso \u2013 und, nichts von andern zu sagen,", "tokens": ["so", "klag\u00b7ten", "Ma\u00b7ri\u00b7no", "und", "Tas\u00b7so", "\u2013", "und", ",", "nichts", "von", "an\u00b7dern", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "$(", "KON", "$,", "PIS", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "ich selbst, in meinen bessern Tagen,", "tokens": ["ich", "selbst", ",", "in", "mei\u00b7nen", "bes\u00b7sern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "wie oft, wenn ich, wie sanft! in Musenarmen schlief,", "tokens": ["wie", "oft", ",", "wenn", "ich", ",", "wie", "sanft", "!", "in", "Mu\u00b7sen\u00b7ar\u00b7men", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "$,", "PWAV", "ADJD", "$.", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und pl\u00f6tzlich aus den lieblichsten Gesichten", "tokens": ["und", "pl\u00f6tz\u00b7lich", "aus", "den", "lieb\u00b7lichs\u00b7ten", "Ge\u00b7sich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "das heisre Gl\u00f6ckchen mich zur Ratsversammlung rief,", "tokens": ["das", "heis\u00b7re", "Gl\u00f6ck\u00b7chen", "mich", "zur", "Rats\u00b7ver\u00b7samm\u00b7lung", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "um zwischen Trax und Stax zu schlichten und zu richten,", "tokens": ["um", "zwi\u00b7schen", "Trax", "und", "Stax", "zu", "schlich\u00b7ten", "und", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NE", "KON", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "wie oft, wenn ich bis an die Ohren tief", "tokens": ["wie", "oft", ",", "wenn", "ich", "bis", "an", "die", "Oh\u00b7ren", "tief"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "in muffigten Papieren steckte", "tokens": ["in", "muf\u00b7fig\u00b7ten", "Pa\u00b7pie\u00b7ren", "steck\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.14": {"text": "und was ich suchte, mich oft halbe Tage neckte", "tokens": ["und", "was", "ich", "such\u00b7te", ",", "mich", "oft", "hal\u00b7be", "Ta\u00b7ge", "neck\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PPER", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "und wenn es mir schon in die H\u00e4nde lief,", "tokens": ["und", "wenn", "es", "mir", "schon", "in", "die", "H\u00e4n\u00b7de", "lief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "wie Maros Galathee, flugs wieder sich versteckte:", "tokens": ["wie", "Ma\u00b7ros", "Ga\u00b7la\u00b7thee", ",", "flugs", "wie\u00b7der", "sich", "ver\u00b7steck\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "$,", "ADV", "ADV", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "wie oft bet\u00e4ubt ich da mit diesem Klageton", "tokens": ["wie", "oft", "be\u00b7t\u00e4ubt", "ich", "da", "mit", "die\u00b7sem", "Kla\u00b7ge\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "der holden Musen zarte Ohren?", "tokens": ["der", "hol\u00b7den", "Mu\u00b7sen", "zar\u00b7te", "Oh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Und dennoch wurden mir, dem Bartelus zum Hohn,", "tokens": ["Und", "den\u00b7noch", "wur\u00b7den", "mir", ",", "dem", "Bar\u00b7te\u00b7lus", "zum", "Hohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "in jenen Tagen Agathon,", "tokens": ["in", "je\u00b7nen", "Ta\u00b7gen", "A\u00b7gat\u00b7hon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Musarion, Endymion,", "tokens": ["Mu\u00b7sa\u00b7ri\u00b7on", ",", "En\u00b7dy\u00b7mi\u00b7on", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "der neue Amadis und Idris selbst geboren;", "tokens": ["der", "neu\u00b7e", "A\u00b7ma\u00b7dis", "und", "Id\u00b7ris", "selbst", "ge\u00b7bo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "KON", "NE", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "verstohlner Weis erzeugt, und falls mich etwa nicht", "tokens": ["ver\u00b7stohl\u00b7ner", "Weis", "er\u00b7zeugt", ",", "und", "falls", "mich", "et\u00b7wa", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$,", "KON", "KOUS", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "ein z\u00e4rtlich Vorurteil zu ihrer Gunst besticht,", "tokens": ["ein", "z\u00e4rt\u00b7lich", "Vor\u00b7ur\u00b7teil", "zu", "ih\u00b7rer", "Gunst", "be\u00b7sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "nur desto gl\u00fccklicher geboren!", "tokens": ["nur", "des\u00b7to", "gl\u00fcck\u00b7li\u00b7cher", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.26": {"text": "und eben weil Mama Natur", "tokens": ["und", "e\u00b7ben", "weil", "Ma\u00b7ma", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "zu ihrer s\u00fc\u00dfen Zeugung nur", "tokens": ["zu", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Zeu\u00b7gung", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "die Augenblicke bald Auroren,", "tokens": ["die", "Au\u00b7gen\u00b7bli\u00b7cke", "bald", "Au\u00b7ro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "bald sp\u00e4ter Nacht, und o! wie manchesmal", "tokens": ["bald", "sp\u00e4\u00b7ter", "Nacht", ",", "und", "o", "!", "wie", "man\u00b7ches\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "NN", "$,", "KON", "FM", "$.", "PWAV", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "den lieben goldenen ", "tokens": ["den", "lie\u00b7ben", "gol\u00b7de\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.31": {"text": "um so viel gl\u00fccklicher geboren!", "tokens": ["um", "so", "viel", "gl\u00fcck\u00b7li\u00b7cher", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "O Du, den gleich, wie Du den Tag erblickt,", "tokens": ["O", "Du", ",", "den", "gleich", ",", "wie", "Du", "den", "Tag", "er\u00b7blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "der Musen lieblichste an ihre Brust gedr\u00fcckt,", "tokens": ["der", "Mu\u00b7sen", "lieb\u00b7lichs\u00b7te", "an", "ih\u00b7re", "Brust", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "la\u00df, edler Freund und Z\u00f6gling der Cam\u00f6nen,", "tokens": ["la\u00df", ",", "ed\u00b7ler", "Freund", "und", "Z\u00f6g\u00b7ling", "der", "Ca\u00b7m\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "mein Beispiel mit den Acten Dich vers\u00f6hnen!", "tokens": ["mein", "Bei\u00b7spiel", "mit", "den", "Ac\u00b7ten", "Dich", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "la\u00df ungehascht der Stunden Keine fliehn,", "tokens": ["la\u00df", "un\u00b7ge\u00b7hascht", "der", "Stun\u00b7den", "Kei\u00b7ne", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJD", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "wenn Themis \u2013 wie bei uns sich Frau Schicane nennet \u2013", "tokens": ["wenn", "The\u00b7mis", "\u2013", "wie", "bei", "uns", "sich", "Frau", "Schi\u00b7ca\u00b7ne", "nen\u00b7net", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$(", "KOKOM", "APPR", "PPER", "PRF", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dir freie Atemz\u00fcge g\u00f6nnet,", "tokens": ["Dir", "frei\u00b7e", "A\u00b7tem\u00b7z\u00fc\u00b7ge", "g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "und wenn ein Genius, Faun oder Amorin,", "tokens": ["und", "wenn", "ein", "Ge\u00b7nius", ",", "Faun", "o\u00b7der", "A\u00b7mo\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.40": {"text": "Dich in die heilgen Schatten ladet,", "tokens": ["Dich", "in", "die", "heil\u00b7gen", "Schat\u00b7ten", "la\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "wo sich die Grazie im Aganippe badet.", "tokens": ["wo", "sich", "die", "Gra\u00b7zie", "im", "A\u00b7gan\u00b7ip\u00b7pe", "ba\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}